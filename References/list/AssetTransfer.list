AssetTransferList is a List
    is primary
    title is "AssetTransfers"
    initially display search
    implements FixedSizeList
        show 7 lines
    no data message is "There_are_no_Asset_Transfers"
        icon is icon.icon-empty-no-orders
    Display Fields
        Company
            label is "FromCompany"
        Asset
        AssetTransferTo.ToAssetCompany
            label is "ToCompany"
        button of "<AssetTransferTo.ToAsset>"
            visible when (PartialTransfer)
            link is TransferToAssetLink
            label is "ToAsset"
            display as link
        TransferDate
        PostingDate
        FullOrPartial
        Description
        CreatedBy
        TransferStatus
        TotalItemQuantity
            label is "OriginalQuantity"
        TotalTransferQuantity
            label is "TransferQuantity"
        TotalTransferTransactionCost
            label is "TransferAmount"
    restrict action Create
    restrict action Update
    Detail Sections
        Main
            form is NewAssetTransferMain
        Items
            visible when (PartialTransfer)
            list is AssetItemTransfer set.AssetItemTransfers
        Books
            visible when (AssetBookTransferExists)
            list is AssetBookTransfersRel.AssetBookTransfers
