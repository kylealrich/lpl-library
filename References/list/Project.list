AssetInterfaceProjectSelect is a List
    title is "<FinanceEnterpriseGroup.ProjectLabel>"
    Display Fields
        DisplayProject
            label is "<FinanceEnterpriseGroup.ProjectLabel>"
        Description
            label is "Description"
            raise blue alert when (IsASystemProject)
                mouse over text is "System<FinanceEnterpriseGroup.ProjectLabel>"
        ProjectDateRange.BeginDate
        ProjectDateRange.EndDate
        ProjectType
    Instance Selection
        where (IsSummaryOrPostingProject
        and   FinanceEnterpriseGroup = actor.context.FinanceEnterpriseGroup)
    restrict action Create
    restrict action Update
    restrict action Delete
    restrict action Move
    restrict action ChangeDateRange
    restrict action CreateChildWithin
    restrict action RemoveFromStructure
    restrict action RemoveSummaryFromStructure
    restrict action AddPostingProjectToStructure
    restrict action AddProjectToStructures
    restrict action CreateComments
    restrict action LoadProjectPeriodOfPerformanceFromTemplate
    restrict action AddPostingToSummary
    restrict action CopyPostingProject
    restrict action CopySummaryProject
    restrict action CreateProjectContractPercentComplete
    restrict action UpdateAssignmentFringe
    restrict action UpdateAssignmentLabor
    restrict action RequestNewPostingProject
    restrict action RequestProjectStatusChange
    restrict action RequestEmployeeForProject
    restrict action CopyProjectPersonnel
    restrict action CreateProjectPersonnel
    restrict action CopyProjectBillingOverrides
    restrict action IncludeSelectedProject
    restrict action IncludeFundingSourceProject
    restrict action EndAllAssignments
    restrict action CreateProjectAsset
