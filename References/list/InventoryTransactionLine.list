ColumnarList is a List
    title is "ColumnarReleasedTransactionLineHistory"
    always display search
    search form is ColumnarSearch
    implements ColumnarView
    Display Fields
        Company
        InventoryLocation
        TransactionSystemCode
        InventoryTransaction.InventoryDocumentType
        InventoryTransaction
            link is InventoryTransactionNav
        InventoryTransactionLine.LineNumber
            label is "Line"
            left align
            link is TransactionLine
        group label is "Originating"
            OriginatingTransactionDocument
                label is "Document"
                left align
            OriginatingTransactionLine
                label is "Line"
                left align
        TransactionDate
            label is "TransactionDate"
        UpdateDate
        Item
            link is ItemInformation
        group label is "Transaction"
            DerivedEnteredQuantity
                raise blue alert when (HasSupplementalUOM)
                    mouse over text is "SupplementalQuantitiesExist"
            TransactionUOM
        group label is "Stock"
            Quantity
            StockUOM
                label is "UOM"
        InventoryReasonCode
            label is "ReasonCode"
        Bin
        Status
        IntransitReceivingStatus
            label is "ReceivingStatus"
    Instance Selection
        where (IsReleasedGLUpdatedOrIntermediate
        and   not InventoryTransaction.IsInventoryIntransitReceiving)
    restrict action Unreleased.Create
    restrict action Unreleased.Update
    restrict action Unreleased.Delete
    Charts
        TotalsByTransactionSystemCode is an AggregationTable
            title is "TotalsByTransactionSystemCode"
            chart list
            show totals first
            Dimensions
                TransactionSystemCode
                    ascending
                Company
                    ascending
                InventoryLocation
                    ascending
                Item
                    ascending
            Measures
                sum Quantity
                    display blank when zero
            Subtotal By
                TransactionSystemCode
                    page break
                Company
                    page break
                InventoryLocation
                    page break
