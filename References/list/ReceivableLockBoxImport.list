Payments is a List
    implements InlineCreate
    no data message is "No_Lockbox_Interface_Payment_Records_Exist"
    Display Fields
        ReceivableLockBoxImport.LockSeq
            label is "PaymentSequence"
            allow update
        CheckNumber
            label is "CheckNumber"
            allow update
        PaymentAmount
            label is "PaymentAmount"
            allow update
        BankIDNumber
            label is "BankNumber"
            allow update
        AccountID
            label is "AccountID"
            allow update
        group label is "FranchiseManagementFields"
            Required
                allow update
            ShipToAddress
                label is "ShipToAddress"
                allow update
            Contract
                allow update
            BillingDate
                allow update
            SalesAmount
                label is "SalesAmount"
                allow update
    Instance Selection
        where (ReceivableLockBoxImport.LockType.PaymentDetail)
    create action is PaymentCreate
    restrict action Create
    restrict action RemittanceCreate
