BillingInvoiceImportList is a List
    is primary
    title is "BillingInvoiceImport"
    initially display search
    no data message is "Add_a_new_invoice_import"
        icon is icon.icon-empty-new-project-new
        button of "Create"
            action is Create
    Display Fields
        RunGroup
        Company
        InvcPrefix
            label is "InvoicePrefix"
        TransientConditionalInvoiceNumber
        ErrorMessage
        ErrorFile
    Actions
        action is InterfaceInvoices
            action icon is transfer
            hide when invalid
        action is Create
            action icon is create
            label is "_"
            hide when invalid
        action is open
            label is "_"
        action is Delete
            label is "_"
        action is search
            label is "_"
        action is refresh
            label is "_"
