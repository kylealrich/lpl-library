ApprovedCashLedgerBatchesList is a List
    title is "ApprovedBatches"
    initially display search
    show in descending order
    no data message is "There_are_currently_no_Approved_Batches"
        icon is icon.icon-empty-no-tasks
    Display Fields
        BatchApprovalType
        CashLedgerBatchApproval
            label is "BatchNumber"
        BatchDate
        ApprovalCode
    Instance Selection
        where (Status.Approved)
    Actions
        action is Open.Create
            action icon is create
            label is "_"
            mouse over text is "Create"
            hide when invalid
        action is save
            label is "_"
            mouse over text is "Save"
        action is search
            label is "_"
        action is refresh
            label is "_"
