ConfigurationParameterKeyList is a List
    is primary
    title is "ConfigurationParameterKeys"
    Display Fields
        ConfigurationParameterCategory
            label is "Component"
            sort order is primary
                is default
        ConfigurationParameterKey
            label is "Key"
            raise yellow alert when (Deprecated)
                mouse over text is "ConfigurationParameterIsDeprecated"
        Residence
        Usage
        PreferredCategory
            sort order is ByPreferredCategory
        PreferredDataType
        ParameterCount
            link is DefinedParameters
    Actions
        action is open
        action is CreateParameter
        action is search
