AllProjectIndirectBurdenTransactionList is a List
    is report for Projects
        category is "TransactionsAndBalances"
    title is "IndirectBurdenTransactions"
    initially display search
    no data message is "No_indirect_burden_transactions_exist."
        icon is icon.icon-empty-no-data
    Display Fields
        RunGroup
        AccountingEntity
            label is "From<FinanceEnterpriseGroup.AccountingEntityLabel>"
        Project
        ProjectIndirectBurdenCode
        Status
            raise red alert when (ErrorMessageEntered)
                mouse over text is "ErrorExists"
        Amount
            label is "Burden"
            raise red alert when (PartialStatus.Limited)
                mouse over text is "BurdenAmountLimitedByLackOfAvailableFunds"
            raise yellow alert when (OverrideFD2Entered)
                mouse over text is "<FinanceEnterpriseGroup.FinanceDimension2Label>OverriddenTo<OverrideFD2>"
            raise green alert when (PartialStatus.Reallocated)
                mouse over text is "PartialBurdenAmountReallocatedToOverride<FinanceEnterpriseGroup.FinanceDimension2Label>"
        group label is "Expense"
            OriginalGLTransactionDetail.TransactionDate
                label is "TransactionDate"
            OriginalGLTransactionDetail.PostingDate
                label is "PostingDate"
            OriginalGLTransactionDetail.ReportCurrencyAmount.ProjectAmount.EnteredCurrencyAmount
                label is "Amount"
            OriginalGLTransactionDetail.FinanceCodeBlock
                label is "FinanceStructure"
        group label is "Burden"
            GeneratedGLTransactionDetail.JournalizeGroup
                label is "JournalizeGroup"
                link is GlobalLedgerJournalControl
            GeneratedGLTransactionDetail.PostingDate
                label is "PostingDate"
            GeneratedGLTransactionDetail.RelatedJournalTransaction(GeneralLedgerTransaction).Status
                label is "PostingStatus"
            GeneratedGLTransactionDetail.FinanceCodeBlock
                label is "FinanceStructure"
        group label is "A\\PHold"
            OriginalAPHold
                label is "Expense"
            GeneratedAPHold
                label is "Burden"
        PartialStatus
            column visible when (actor.context.FinanceEnterpriseGroup.UseFundedAmounts)
        OverrideFD2
            label is "Override<FinanceEnterpriseGroup.FinanceDimension2Label>"
            column visible when (actor.context.FinanceEnterpriseGroup.UseFundedAmounts)
        ErrorMessage
    open action is restricted
    Actions
        action is Reverse
            action icon is undo
        action is Delete
            action icon is delete
            label is "_"
