AssetItemDisposalWizard is a List
    title is "AssetItemDisposals"
    no data message is "Select_Items_to_Dispose"
        icon is icon.icon-empty-no-orders
    Display Fields
        AssetItem.ItemNumber
        AssetItem.Description
        AssetItem.PurchaseDate
        TransientDisposalDate
            label is "DisposalDate"
        AssetItem.TransactionItemCost
            label is "Cost"
        AssetItem.ItemQuantity
        TransactionItemCost
            label is "DisposeItemCost"
        DisposalQuantity
            label is "DisposeQuantity"
            raise red alert when (DisposalQuantityError)
                mouse over text is "TheAssetDisposalQuantityIsNotEqualToItemDisposalQuantity(s)"
    restrict action Create
    restrict action Delete
    Actions
        action is helper list
            label is "SelectItemsToDispose"
        action is search
        action is refresh
