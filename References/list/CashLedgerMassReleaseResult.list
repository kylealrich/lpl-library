BankFundTransfersResultList is a List
    title is "MassReleaseResultsForBankFundTransfers"
    initially display search
    no data message is "There_are_currently_no_Mass_Release_Results_For_Bank_Fund_Transfers"
        icon is icon.icon-empty-no-tasks
    Display Fields
        CashManagementGroup
        CashLedgerMassReleaseResult
        CreateDate
            sort order is ByCreateDate
                is default
        Status
            raise yellow alert when (BankFundTransferErrorsExist)
                mouse over text is "NotAllBankFundTransfersReleased;UnresolvedErrorsExist"
        ReleaseCount
            label is "RecordsReleased"
        ErrorCount
            label is "RecordsWithErrors"
    Instance Selection
        where (ReleaseTypeIsBankFundTransfers)
    form is BankFundTransfersResultsForm
    Actions
        action is open
            action icon is open
            label is "_"
            mouse over text is "Open"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is search
            label is "_"
        action is refresh
            label is "_"
