ContractLineAttributeUpdateList is a List
    title is "AttributesToUpdate"
    initially display search
    no data message is "Doesn't_look_like_there's_any"
        icon is icon.icon-empty-no-orders-new
    Display Fields
        Contract
            link is ContractNav
        ContractLine
            link is ContractLineNav
        InternalItemNumber
            link is ItemNav
            raise blue alert when (GTINForItemMatchingUOM)
                mouse over text is "GTINWillBeUpdatedForEnteredUOM;ExistingGTINWillBeInactivated"
        ManufacturerCode
        ManufacturerDivision
        ManufacturerPartNumberIdentifier
        VendorID
        VendorItemIdentifier
        ErrorsExist
        Status
    Instance Selection
        where (AvailableToProcess
        and   not ConsolidatedItemMasterEntered)
    Actions
        action is UpdateAttributes
        action is RecheckErrors
            action icon is validate
            hide when invalid
        action is UpdateAttributesInstance
            hide when invalid
        action is SetToNotProcessed
        action is open
            label is "_"
        action is refresh
            label is "_"
        navigation is ViewFactSheet
    Detail Sections
        Errors
            visible when (ErrorsExist)
            list is ContractLineAttributeError set.ContractLineAttributeErrorList
