GLTransactionInterfaceFESList is a List
    title is "GlobalLedgerTransactionInterfaceList"
    initially display search
    no data message is "No_Transaction_Interface_been_created"
        description is "Create_interface_transactions_from_an_external_source._Journal_entries_are_created_with_a_status_of_Released."
    Display Fields
        FinanceEnterpriseGroup
        GLTransactionInterface.RunGroup
            label is "RunGroup"
        GLTransactionInterface.SequenceNumber
            label is "SequenceNumber"
            raise red alert when (ErrorTransaction)
                mouse over text is "<ErrorMessage>"
        Project.ParentDisplayContract
            label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Contract"
            link is ProjectContractNav
        Project
            label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
        FinanceDimension2
            label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>"
        Status
        AccountingEntity
            label is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
        OrganizationCode
        ToAccountingEntity
            label is "To<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
        AccountingUnit
            label is "<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>"
            column visible when (actor.context.FinanceEnterpriseGroup.AccountingUnitLabelExists)
        AccountCode
        PostingDate
            label is "PostDate"
        ReportCurrencyAmount.AssignToCurrency
            column visible when (false)
        TransactionAmount
        CreatedByFES
            column visible when (FrontEndSplitsEnabled)
        FESManuallyUpdated
            column visible when (FrontEndSplitsEnabled)
    Instance Selection
        where (PendingFESGLInterfaceTransactions)
