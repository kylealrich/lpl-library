BankServiceCube is a List
    title is "FeeServiceSummary"
    implements CubeView
        suppress blank rows
        show total node
            label is "Total"
        show 2 levels
        Row Dimensions
            CashManagementAccount.FinancialInstitution
            BankServiceCode.BankServiceCategory
            BankServiceCode
    no data message is "No_Fee_Service_Analysis"
        icon is icon.icon-empty-no-tasks
    Display Fields
        BankChargedAmount
        Quantity
    open action is restricted
    Charts
        BankServicesChargedByAccountChart is a BarChart
            title is "TotalServicesChargedByAccount"
            chart list
            show legend
            element name is "<CashManagementAccount>"
            element value is BankChargedAmount
                title is "ServicesChargedByAccounts"
        ServicesByInstitutionChart is a BarChart
            title is "ServicesByInstitution"
            chart list
            show legend
            element name is "<CashManagementAccount.FinancialInstitution>"
            element value is BankChargedAmount
                title is "ServicesByInstitutions"
        ServicesByLocationChart is a PieChart
            title is "ServicesByLocation"
            chart list
            show legend
            show labels
            element name is "<CashManagementAccount.CashManagementLocation>"
            element value is BankChargedAmount
                title is "ServicesByLocation"
        ServicesByLegalEntityChart is a PieChart
            title is "ServicesByLegalEntity"
            chart list
            show legend
            show labels
            element name is "<CashManagementAccount.LegalEntity>"
            element value is BankChargedAmount
                title is "ServicesByLegalEntity"
        ServicesChargedByCategory is a PieChart
            title is "ServicesChargedByCategory"
            chart list
            show legend
            show labels
            element name is "<BankServiceCode.BankServiceCategory>"
            element value is BankChargedAmount
