CashTransactionProcessingRuleGroups is a List
    is primary
    title is "ProcessingRuleGroups"
    initially display search
    no data message is "Add_a_new_Processing_Rule_Group"
        icon is icon.icon-empty-new-project
        button of "Create"
            action is Create
    Display Fields
        CashTransactionProcessingRuleGroup
            raise yellow alert when (NotOnACashManagementAccount)
                mouse over text is "ProcessingRuleGroupIsNotAssignedToACashManagementAccount"
            sort order is primary
        Description
            allow update
        Active
            allow update
