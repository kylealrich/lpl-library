CashDetailApplicationList is a List
    is primary
    title is "AppliedCashDetails"
    no data message is "No_Cash_Detail_Applications_Exist"
    Display Fields
        group label is "Application"
            TransientAppliedAmount
                label is "Amount"
            ApplicationSource
                label is "Source"
        group label is "Transaction"
            CashDetailApplicationGroup.Company
            CashDetailApplicationGroup.ReceivableApplication.TransType
                label is "Type"
            CashDetailApplicationGroup.ReceivableApplication.Invoice
                label is "Invoice"
            CashDetailApplicationGroup.ReceivableApplication.PaymentSeq
                label is "Sequence"
