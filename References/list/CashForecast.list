CashForecastCardList is a List
    title is "CashForecasts"
    single select
    keyword search field is CashForecast
        label is "SearchCashForecasts"
    search form is CashForecastCardSearch
        use as filter
    no data message is "No_Cash_Forecasts_to_view"
        icon is icon.icon-empty-no-tasks
    card view is CashForecastCard
        row separator is horizontal rule
        sort order is ByStatus
            is default
            label is "CashForecasts"
    Display Fields
    Actions
        action is New.Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is New.Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
            hide when invalid
        action is Closed.Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
            hide when invalid
    Detail Sections
        ForecastByAccount
            visible when (HaveBuiltForecast)
            quad width
            list is CashForecastAccount set.CashForecastAccountMatrixList
        PublishedForecastByWeek
            visible when (HasWeeklyPublishedDates)
            quad width
            list is WeeklyCashForecastPublishDateRel.WeeklyCashForecastPublishDates
        PublishedForecastByMonth
            visible when (HasMonthlyPublishedDates)
            quad width
            list is MonthlyCashForecastPublishDateRel.MonthlyCashForecastPublishDates
