AttachedRebateQualifierContracts is a List
    Display Fields
        ContractRel.WorkingContractID
            label is "WorkingContractID"
        ContractRel.Name
            label is "ContractName"
        OriginalContractNumber
            label is "Contract"
        ContractRel.Supplier.SupplierName
            label is "SupplierName"
        NumberOfEligibleItemsToSelect
            label is "EligibleItems"
            column visible when (ContractRebateQualifier.NotAllInvoicesForSupplier)
        NumberOfItemsSelectedForQualificationPurposes
            label is "SelectedQualificationItems"
            column visible when (ContractRebateQualifier.NotAllInvoicesForSupplier)
    Actions
        action is SelectAllItemsForRebateQualification
            hide when invalid
            valid when (NotAllItemsSelected)
        action is helper list
            label is "SelectAContractForRebate"
        action is open
            label is "_"
