ContractDetailList is a List
    title is "Details"
    Display Fields
        button of "<ChargebackYesOrNo>"
            action is SetChargeback
            label is "CB"
            display as link
        PayablesInvoiceDetail.ItemDescription
        ContractLine
            label is "Line"
            sort order is primary
        ItemType
            label is "Type"
            sort order is ByItemType
        EnteredQuantity
            label is "Quantity"
            sort order is ByMatchedQuantity
        EnteredUnitCost
            label is "Cost"
            sort order is ByMatchUnitCost
        DerivedExtendedMatchAmount
            label is "InvoiceExtendedAmount"
        DerivedContractCost
            label is "ContractCost"
        DerivedContractLineExtended
            label is "ContractLineExtendedAmount"
        DerivedContractLineDiscrepancy
            label is "Discrepancy"
    open action is restricted
    restrict action Create
    restrict action Update
    restrict action Delete
    Actions
        action is refresh
        action is search
