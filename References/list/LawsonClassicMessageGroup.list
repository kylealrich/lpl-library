LCMGList is a List
    is primary
    title is "ClassicMessageGroupList"
    Display Fields
        LawsonClassicMessageGroup
            label is "ID"
        create stamp
            label is "Created"
            sort order is ByStamp
                is default descending
        EffectiveDate
        ExecuteTime
        Status
            raise red alert when (IsFailed)
            raise yellow alert when (IsHold)
            raise green alert when (IsComplete)
        create stamp.actor
            label is "Actor"
