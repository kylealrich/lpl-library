AllLineErrors is a List
    is report for ContractManager,ApplicationAdministrator,SupplyManagementProcessor,Contract,SupplyManagementViewer
        category is "Interface"
        module is contract
    title is "InterfaceLineErrors"
    single select
    initially display search
    Display Fields
        ContractImport
            label is "ContractInterface"
            link is ContractImport
        ContractLineImport
            label is "LineNumber"
        ItemNumber
            link is ItemLink
        VendorItem
            link is VendorItemLink
            raise yellow alert when (SameItemDifferentUOM)
                mouse over text is "Line<first NonMatchingUOMExistsRel.ContractLineImport>HasTheSameVendorItemWithADifferentUOM"
        UOM
        UOMConversion
        ItemMasterUOMConversion
            label is "ItemMasterUnitOfUseConversion"
        ItemDescription
            label is "Description"
        ManufacturerInformation.Manufacturer
            label is "ManufacturerCode"
            allow update
        ManufacturerInformation.ManufacturerNumber
            label is "ManufacturerNumber"
        UNSPSCCode
            allow update
        UpdateFunction
            label is "UpdateFunction"
            allow update
        UOMErrorsExist
        LineWithDuplicateUOM
            label is "ChangedUOM"
    Instance Selection
        where (HasLineErrors
        and   not GPOItemDoesNotExist)
    restrict action Create
    restrict action Select
    restrict action Unselect
    Actions
        action is open
            label is "_"
        action is save
            label is "_"
    Detail Sections
        Details
            list is ContractLineImportError set.ContractLineImportErrorList
        ItemUnitsOfMeasure
            visible when (ItemExists)
            list is ItemUOMsRel.UOMsForItem
