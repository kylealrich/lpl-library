BudgetEditBatchList is a List
    is primary
    title is "BudgetEditBatches"
    single select
    initially display search
    no data message is "Nothing_to_show"
        description is "There_are_currently_no_transactions_processed_or_queued"
        icon is icon.icon-empty-no-orders-new
    Display Fields
        DocumentID
            label is "DocumentID"
            raise red alert when (HasError)
                mouse over text is "<ErrorMessage>"
        BusinessClassName
        EditContext
        BudgetEditCallBackWrap.FEG
            label is "FinanceEnterpriseGroup"
        BudgetEditMode
        BudgetEditTotalsProcessing
            label is "TotalsProcessing"
        PActionCode
            label is "ActionCode"
        CreateTime
            sort order is ByCompletionTime
                is default
        StartTime
        CompletionTime
        ErrorMessage
    restrict action Create
    restrict action Update
    restrict action Delete
    Actions
        action is ProcessBatchEdits
        action is PruneCompletedEdits
        action is open
            action icon is open
            label is "_"
            mouse over text is "Open"
        action is search
            label is "_"
            mouse over text is "Search"
        action is refresh
            label is "_"
            mouse over text is "Refresh"
