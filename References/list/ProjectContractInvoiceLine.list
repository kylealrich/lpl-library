AllProjectBillableLaborInvoiceLinesDetail is a List
    is report for Projects
        category is "InvoicingAndRevenue"
    title is "BilledLabor"
    single select
    initially display search
    implements HorizontalScrolling
        freeze first 3 columns
    no data message is "No_billed_labor_exits."
        icon is icon.icon-empty-no-data
    Display Fields
        ProjectContract.DisplayContract
            label is "Contract"
        Project
            label is "<FinanceEnterpriseGroup.ProjectLabel>"
        GLTransactionDetail.FinanceCodeBlock.GeneralLedgerChartAccount
            label is "Account"
        GLTransactionDetail.Resource
            label is "Resource"
        GLTransactionDetail.Resource.FirstLastName
            label is "Name"
        group label is "Expense"
            GLTransactionDetail.TransactionDate
                label is "TransactionDate"
            GLTransactionDetail.PostingDate
                label is "PostingDate"
        GLTransactionDetail.System
            label is "System"
        GLTransactionDetail.GeneralLedgerEvent
            label is "Event"
        BillableRate
        UnitsAmount
            label is "Units"
        Amount
            label is "InvoiceAmount"
        ProjectContract.Currency
            label is "Currency"
        BillableAmount
            label is "BasisAmount"
        ProjectContractInvoice.Status
            label is "InvoiceStatus"
    Instance Selection
        where (ProjectContractInvoice.InvoiceType.Bill
        and   GLTransactionDetail.Resource entered
        and   LineType.Standard)
    open action is restricted
    restrict action RemoveTransaction
    restrict action RemoveMultipleTransactions
    restrict action RemoveAllowance
    restrict action RemoveAdvanceBillingLine
    restrict action RemoveAppliedAdvanceBilling
    restrict action RemoveRetainage
    restrict action RemoveRetainer
    restrict action RemoveRevenueTransaction
    restrict action RemoveRevenueAllowance
    restrict action ReapplyRetainer
    restrict action CreateAdvanceBilling
    restrict action EditAdvanceBillingLine
