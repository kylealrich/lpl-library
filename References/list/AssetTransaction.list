AssetBalanceListTransactions is a List
    title is "AssetTransactions"
    search form is AssetTransactionFilter
        always display
        use as filter
    suppress sub-totals
    no data message is "No_Transactions_to_show"
        icon is icon.icon-empty-no-data
    Display Fields
        Company
        Book
        Asset
        DerivedAssetAccountType
            label is "AccountType"
        AssetProcess
            sort order is Set2
        TransactionDate
        PostingDate
        Description
        TransactionAmount
            label is "TransactionAmount"
            total
        TransientBookCurrency
            label is "TransactionCurrency"
        AssetTransactionAmount.BaseAmount.EnteredCurrencyAmount
            label is "BookAmount"
            total
        Book.Currency
            label is "BookCurrency"
    Instance Selection
        where (IsIncludedInSearchWithHistory)
    open action is restricted
    restrict action Create
    restrict action WizardCreate
    restrict action Update
    restrict action Delete
    restrict action RefreshData
