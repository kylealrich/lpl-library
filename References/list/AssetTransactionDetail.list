AllReleasedAssets is a List
    title is "ReleasedAssetList"
    single select
    Display Fields
        AssetCompany
            sort order is ForSumAmount
                is default
                Subtotal By
                    AssetCompany
        Asset
            link is AssetLink
            sort order is ForSumAmount
        Asset.Description
        Asset.TagNumber
        AssetProcess
        OriginatingAsset
            label is "CreatedFromAsset"
    Instance Selection
        where (not Asset.InterfaceInProgress
        and   not Asset.Status.Unreleased)
