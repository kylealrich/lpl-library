ActiveDiversityResponses is a List
    Display Fields
        PayablesDiversityCode
            label is "DiversityCode"
            raise red alert when (ExpirationDateRedAlert)
                mouse over text is "DiversityHasExpiredOrWillExpireWithin<Alerts.RedAlert>Days"
            raise yellow alert when (ExpirationDateYellowAlert)
                mouse over text is "DiversityWillExpireWithin<Alerts.YellowAlert>Days"
        PayablesDiversityCode.Description
            label is "DiversityDescription"
        EffectiveDate
            allow update
        ExpirationDate
            allow update
        ReviewDate
            allow update
    Instance Selection
        where (IsActive)
    form is BuyerDiversity
    restrict action AttachDiversityToContract
