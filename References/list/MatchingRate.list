MatchingRateDescendingList is a List
    title is "MatchingRates:___LastRunDate_<actor.context.FinanceEnterpriseGroup.LastRunDateMatchingRateReport>"
    no data message is "Matching_Rate_Listing_Does_Not_Exist"
        description is "Run_Report_to_Generate."
        icon is icon.icon-empty-no-analytics
        button of "CreateMatchingRateListing"
            action is RunReport
    Display Fields
        Year
            sort order is YearMonthDescending
                is default
        Month
        TotalMatchCount
            label is "TotalCount"
            raise green alert when (Status.InProcess)
                mouse over text is "ReportStillInProcess.RefreshForUpdatedCounts."
        group label is "AutoMatch"
            AutoMatchCount
                label is "Count"
            AutoMatchCountPercent
                label is "Percent"
        group label is "OnlineMatch"
            OnlineMatchCount
                label is "Count"
            OnlineMatchCountPercent
                label is "Percent"
        group label is "ReconciliationMatch"
            ReconMatchCount
                label is "Count"
            ReconMatchCountPercent
                label is "Percent"
    open action is restricted
    restrict action Create
    restrict action Update
    restrict action Delete
    restrict action Purge
    Actions
        action is RunReport
        action is search
            label is "_"
        action is refresh
            label is "_"
