AssetCommentImportList is a List
    title is "AssetCommentInterfaceList"
    initially display search
    no data message is "click_below_to_start_importing_Comments"
        icon is icon.icon-empty-new-project
        button of "Create"
            action is Create
    Display Fields
        RunGroup
        Company
            label is "Company"
        ConversionNumber
            label is "AssetImportGroup"
        Asset.TagNumber
            label is "TagNumber"
        Type
            label is "CommentType"
            show up to 25 characters
            sort order is ByType
        button of "<Name>"
            sub form is AssetCommentSubform
            label is "Title"
            display as link
        Comment
        button of "<Attachment.Title>"
            view Attachment.File
            label is "Attachment"
            left align
            display as link
        RecordInError
        ErrorMessage
    form is AssetCommentImportForm
