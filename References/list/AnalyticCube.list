AnalyticCubeList is a List
    is primary
    title is "Cubes"
    initially display search
    Display Fields
        BusinessClass
            label is "Name"
            bold
            link is AnalyticCubeLogs
        LastReloadDate
            label is "LastReloadDate"
            display as tag
                color of blue when (ReloadedInLastDay)
        LastRefreshDate
            label is "LastRefreshDate"
            display as tag
                color of blue when (RefreshedInLastDay)
        RecoveryAction
            column visible when (EnableRestrictedActions)
        RecoveryDate
            column visible when (EnableRestrictedActions)
        Checkpoint
            allow update
            column visible when (EnableRestrictedActions)
        CheckpointDACName
            allow update
            column visible when (EnableRestrictedActions)
        Pause
            allow update
        Mode
            raise red alert when (ShowModeRedAlert)
                mouse over text is "<ModeStatusMessage>"
            raise yellow alert when (ShowModeYellowAlert)
                mouse over text is "<ModeStatusMessage>"
            column visible when (not EnableRestrictedActions)
        Mode
            allow update
            raise red alert when (ShowModeRedAlert)
                mouse over text is "<ModeStatusMessage>"
            raise yellow alert when (ShowModeYellowAlert)
                mouse over text is "<ModeStatusMessage>"
            column visible when (EnableRestrictedActions)
        IsCubeLocked
            label is "Locked"
            raise red alert when (ShowIsCubeLockedRedAlert)
                mouse over text is "CubeLockIsOrphaned.VerifyThatARunningProcessOwnsTheLock."
        HasUserCubeDeployment
            raise yellow alert when (ShowDeployedYellowAlert)
                mouse over text is "<UserCubeDeploymentMessage>"
            raise green alert when (ActiveDeployedConfiguration)
                mouse over text is "<UserCubeDeploymentMessage>"
        HasUserCube
    restrict action CreateDACubes
    restrict action DeleteDACubes
    restrict action ExportDACubes
    restrict action ExportDACubesFromBackup
    restrict action ImportDACubes
    restrict action CreateDefinitionReport
    restrict action CreateDefinitionReportInForeground
    restrict action DeleteDefinitionReports
    restrict action CreateInvalidFactRowsReport
    restrict action CreateInvalidFactRowsReportInForeground
    restrict action DeleteInvalidFactRowReports
    restrict action CreateInvalidDimensionsReport
    restrict action CreateInvalidDimensionsReportInForeground
    restrict action DeleteInvalidDimensionReports
    restrict action CreateRefreshReport
    restrict action CreateRefreshReportInForeground
    restrict action DeleteRefreshReports
    restrict action CreateDimensionSurrogates
    restrict action CreateDimensionSurrogatesInForeground
    restrict action DeleteDimensionSurrogates
    restrict action DeleteMDXQueries
    restrict action CreateMDXQuery
    Actions
        action is Refresh
            label is "RefreshCube"
        action is Reload
            label is "ReloadCube"
        action is save
        action is refresh
