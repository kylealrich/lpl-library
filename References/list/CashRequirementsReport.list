CashRequirementsReports is a List
    title is "SelectedVendors"
    Display Fields
        VendorName
            sort order is ByVendorName
                is default
            column visible when (not CashRequirementsResult.DisplaySearchName)
        VendorSearchName
            sort order is ByVendorSearchName
            column visible when (CashRequirementsResult.DisplaySearchName)
        CashRequirementsReport.PayVendor
            sort order is ByCashRequirementsResult
        CashRequirementsReport.RemitToCode
        CashRequirementsReport.CashCode
        CashRequirementsReport.BankTransactionCode
        Address
        CashRequirementsReport.PayVendor.VendorClass
        TotalPaymentAmount
        TotalDiscountAmount
        TotalNetPaymentAmount
            raise red alert when (TotalNetPaymentAmountIsNegative)
                mouse over text is "<NegativeNetPaymentAmountMessage>"
        CashRequirementsReport.InvoiceCurrency
        TotalBankCheckAmount
            label is "TotalBankPaymentAmount"
        CheckCurrency
            label is "PaymentCurrency"
    open action is restricted
    Detail Sections
        SelectedInvoices
            title is "SelectedInvoices_(<NumSelectedPayablesInvoicePaymentRel>)"
            list is SelectedPayablesInvoicePaymentRel.CashRequirementsVendorReportList
