AllProjectAssignmentLaborList is a List
    title is "LaborDistribution"
    single select
    initially display search
    implements HorizontalScrolling
        freeze first column
    no data message is "No_labor_distribution_exists."
        icon is icon.icon-empty-no-data
    Display Fields
        Project
            label is "<FinanceEnterpriseGroup.ProjectLabel>"
            sort order is primary
        Project.Description
            raise blue alert when (HasProjectAssignmentLaborComments)
                mouse over text is "CommentsExists"
        FinanceCodeBlock
        Employee
            raise red alert when (not Employee.ProjectEmployee.Active)
                mouse over text is "<FinanceEnterpriseGroup.ProjectLabel>EmployeeIsInactive"
            sort order is ByEmployeeAndSchedulePeriod
        Employee.Employee.PresentationNameSnapshot
            label is "Name"
        LaborPeriod.Period
        DateRange.Begin
        DateRange.End
        Status
            display as tag
                color of blue when (IsBlueStatusTag)
                color of yellow when (Status.PendingApproval)
                color of green when (IsGreenStatusTag)
            raise yellow alert when (Adjusted)
                mouse over text is "LaborDistributionsHaveBeenAdjusted;_RedistributionRequired"
        RedistributionApprovalStatus
            display as tag
                color of yellow when (RedistributionApprovalStatus.PendingApproval)
                color of green when (IsGreenRedistributionStatusTag)
            column visible when (LaborDistributionApprovalTrue)
        Adjusted
        FullyDistributed
        PlannedPercent
        ActualPercent
        ProjectSchedulePeriod
            column visible when (false)
        Capped
        TransactionAmount
        OverageAmount
        DistributedOverageAmount
    Instance Selection
        where (NotExcluded)
    restrict action ApproveLabor
    restrict action SubmitForApproval
    restrict action UnapproveLabor
    restrict action CopyLabor
    restrict action PreviewAdjustments
    restrict action RedistributeLabor
    restrict action ReverseLaborDistributions
    restrict action DistributeIndividualLabor
    restrict action ValidateIndividualLaborJournalEntries
    restrict action Update
    restrict action CreateEncumbranceIndividual
    restrict action DeleteEncumbranceIndividual
    restrict action SubmitRedistributionForApproval
    restrict action ResetLaborRedistributionApprovals
    restrict action ResetLaborDistributionApprovals
    restrict action PopulateActualPercent
