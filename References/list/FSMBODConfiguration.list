ActiveInboundsList is a List
    title is "ActiveInbound"
    no data message is "No_Active_Inbound_BO\\Ds"
        icon is icon.icon-empty-no-data
        button of "ViewAllInbounds"
            link is ActiveInboundsNav
    card view is ActiveBODsCardView
    Display Fields
    Instance Selection
        where (FSMBODConfiguration.Direction.Inbound
        and   Enable)
    restrict action Create
    restrict action Update
    restrict action EnableIntegration
    restrict action SwitchFromIPAToLPLBODs
