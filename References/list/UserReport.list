BTAUserReportList is a List
    title is "Reports"
    Display Fields
        Name
            sort order is ByName
        MimeType
            label is "DocumentType"
            sort order is ByMimeType
        create stamp
        button of "View"
            visible when (ReportDocument.HasDocument)
            view ReportDocument.Document
    restrict action PurgeMyReports
    restrict action PurgeActorReports
    restrict action PurgeReports
    restrict action PurgeFolderReports
