DailyTransactionChartView is a List
    search form is ChartSearchForm
        use as filter
    no data message is "No_Daily_Transaction_ChartToView"
        description is "Run_Recommendations_EngineAnd_Generate_Daily_DataToSeeTheChart"
        icon is icon.icon-empty-no-analytics
    Display Fields
        ReorderPoint
        ReferenceMaxOrder
        StockOnHandAtEndOfDay
        NetUnitsOut
        IIHDailyData
            label is "TransactionDate"
    Instance Selection
        where (WithinDateRange)
    open action is restricted
    Charts
        StockOnHandActivity is a LineChart
            title is "SOHDailyTrend"
            chart list
            is default
            show legend
            element name is "<IIHDailyData>"
            element value is ReorderPoint
            element value is ReferenceMaxOrder
            element value is NetUnitsOut
            element value is StockOnHandAtEndOfDay
                title is "StockOnHandAtEndOfDay(<DefaultTransactionUOMDerived>)"
