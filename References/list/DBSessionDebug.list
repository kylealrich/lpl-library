DBSessionDebugList is a List
    is primary
    title is "DatabaseTierSessionDebug"
    Display Fields
        Start
            sort order is ByStartDescending
                is default
        End
        DurationSecs
            label is "Duration_(secs)"
        UnitCount
            label is "Units_of_Work"
            raise red alert when (HasUnitsOfWorkPendingTempFileLoad)
                mouse over text is "Load_of_TemporaryDebugFile(s)Incomplete."
            display zero
        Actor
            show up to 20 characters
            sort order is ByActor
        SessionDescription
            allow update
            show up to 30 characters
            sort order is BySessionDescription
        BackgroundActions
            raise red alert when (HasIncompleteBackgroundActions)
                mouse over text is "<IncompleteBackgroundActions>Incomplete."
            display zero
        button of "Share"
            action is Share
        button of "CodeCoverage"
            action is CodeCoverage
    Detail Sections
        UnitsOfWork
            title is "Units_ofWork"
            list is DBSessionDebugUnit set.DBSessionDebugUnitList
