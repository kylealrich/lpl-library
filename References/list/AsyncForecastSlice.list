AsyncForecastSliceList is a List
    is primary
    title is "ForecastSlices"
    Display Fields
        MemberCount
        SliceDescription
    Actions
        action is open
        action is charts
        navigation is ViewMembers
    Charts
        ForecastHorizontalBarChart is a BarChart
            chart list
            show bars horizontally
            element name is untranslatable:"<SliceDescription>"
            element value is MemberCount
                title is untranslatable:"<AsyncSchedulingForecast.DurationMessage>"
        ForecastVerticalBarChart is a BarChart
            chart list
            is default
            show bars vertically
            element name is untranslatable:"<SliceDescription>"
            element value is MemberCount
                title is untranslatable:"<AsyncSchedulingForecast.DurationMessage>"
        ForecastLineChart is a LineChart
            chart list
            element name is untranslatable:"<SliceDescription>"
            element value is MemberCount
                title is untranslatable:"<AsyncSchedulingForecast.DurationMessage>"
