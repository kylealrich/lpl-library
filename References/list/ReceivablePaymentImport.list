InterfacePaymentsList is a List
    title is "InterfacePayments"
    initially display search
    no data message is "No_Payment_Interface_Records_Exist"
        icon is icon.icon-empty-no-notes
        button of "CreateInterfaceRecord"
            action is Create
    Display Fields
        RunGroup
            sort order is ByRunGroup
                is default
        Company
        Customer
            select is CustomerSelect
        InterfacedCustomer
        BatchNumber
        group label is "Transaction"
            TransNbr
                label is "Number"
            TranAmt
                label is "Amount"
        group label is "Remit"
            RemitType
                label is "Type"
            TransRemNbr
                label is "Number"
        ErrorMessage
    Instance Selection
        where (IsValidForActorContext)
    Actions
        action is InterfacePayments
            action icon is transfer
            hide when invalid
        action is ResetErrorMessage
            action icon is undo
            hide when invalid
        action is Create
            label is "_"
        action is open
            label is "_"
        action is Delete
            label is "_"
        action is search
            label is "_"
        action is refresh
            label is "_"
