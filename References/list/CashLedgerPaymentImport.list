CashLedgerPaymentImportList is a List
    is primary
    title is "PaymentInterface"
    initially display search
    no data message is "There_are_currently_no_Payment_Interface_records"
        icon is icon.icon-empty-no-tasks
    Display Fields
        CashManagementGroup
        CashLedgerPaymentImport.RunGroup
        CashLedgerPaymentImport.CashCode
        CashLedgerPaymentImport.BankTransactionCode
        CashLedgerPaymentImport.ReferenceNumber
        CashLedgerPaymentImport.SequenceNumber
        TransactionDate
        DistributionAmount
        CreatedByFES
            column visible when (FrontEndSplitsEnabled)
        FESManuallyUpdated
            column visible when (FrontEndSplitsEnabled)
        ErrorMessage
    form is PaymentLoadMaintenance
    Actions
        action is InterfaceCashPayments
            label is "InterfaceCashPayments"
            hide when invalid
        action is DeleteAllTransactionsForRunGroup
            label is "DeleteAllTransactionsForRunGroup"
            hide when invalid
        action is DeletePartiallyImportedRecord
            label is "DeletePartiallyImportedRecord"
            hide when invalid
        action is Error.ResetErrorsByRunGroup
            label is "ResetErrorsByRunGroup"
            hide when invalid
        action is ReadyToImport.Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is open
            action icon is open
            label is "_"
            mouse over text is "Open"
        action is ReadyToImport.Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
            hide when invalid
        action is Error.Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
            hide when invalid
        action is search
            label is "_"
        action is refresh
            label is "_"
