AgentWithActorList is a List
    title is "Agents"
    initially display search
    Display Fields
        KeyText
            label is "AgentSymbolicKey"
            sort order is ByKeyText
                is default
        BusinessObjectRef.BusinessClassName
            label is "AgentType"
            sort order is BusClassSet
        BusinessObjectRef.BusinessObjectKey
            label is "BusinessObjectKey"
        ParentAgent.Actor
            label is "LinkedActor"
            link is ViewLinkedActor
            raise yellow alert when (not HasParentAgentActor)
                mouse over text is "NoActorFoundForAgent"
        ParentAgent.Actor.PersonName.GivenName
            label is "ActorGivenName"
        ParentAgent.Actor.PersonName.FamilyName
            label is "ActorFamilyName"
    form is PrimaryForm
