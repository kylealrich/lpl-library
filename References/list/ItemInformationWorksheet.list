CreateItemWorksheetList is a List
    is primary
    title is "AllWorksheetItems"
    single select
    initially display search
    no data message is "No_WorksheetItems_To_Display"
        icon is icon.icon-empty-no-data
    Display Fields
        RunGroup
            allow update
            raise blue alert when (FromNotice)
                mouse over text is "CreatedFromNewItemProposal"
            sort order is ByRunGroup
                is default
        InterfacedItem
            allow update
            column visible when (ManualItemNumbering)
        Description
            allow update
        StockUOM
            label is "UOM"
            allow update
        group label is "Manufacturer"
            Manufacturer
                label is "Code"
                allow update
            ManufacturerNumber
                label is "Number"
        FinalItemNumber
            label is "ItemNumber"
        Status
    restrict action CreateItemWorksheetFromCSV
    Actions
        action is CreateItemInformationForThisRunGroup
            action icon is create
            label is "CreateItemInformation"
            hide when invalid
        action is CreateItemLocationRecords
            action icon is create
        action is MassDelete
            action icon is delete
        action is RecheckErrors
            action icon is validate
            hide when invalid
        navigation is ViewUploadSpreadsheets
            hide when invalid
        navigation is ViewFactSheet
            hide when invalid
        action is CreateItemWorksheet
            action icon is create
            label is "_"
            mouse over text is "Create_Item_Worksheet"
        action is open
            label is "_"
        action is save
            label is "_"
            hide when invalid
        action is refresh
            label is "_"
