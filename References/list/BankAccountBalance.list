BankAccountPeriodBalances is a List
    is primary
    initially display search
    no data message is "No_Bank_Account_Period_Balances_to_view"
        icon is icon.icon-empty-no-tasks
    Display Fields
        CashCode
        CashCode.Description
            label is "Description"
        CashCode.Currency
            label is "Currency"
        BankAccountBalance
            label is "EndingDate"
            sort order is Set2
                is default
        BeginningBalance
        TransactionBalance
            label is "PeriodTransactionAmount"
        EndingBalance
    Actions
        action is BankPeriodReopen
            label is "Reopen"
            hide when invalid
        action is open
            action icon is open
            label is "_"
            mouse over text is "Open"
        action is search
            label is "_"
        action is refresh
            label is "_"
