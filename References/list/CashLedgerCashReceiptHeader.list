AllCashLedgerCashReceiptHeaderList is a List
    is primary
    title is "CashReceipts"
    initially display search
    no data message is "No_Cash_Receipts_to_view"
        icon is icon.icon-empty-no-tasks
    Display Fields
        CashCode
        CashLedgerCashReceiptHeader.BankTransactionCode
            label is "TransactionCode"
        CashLedgerCashReceiptHeader.TransactionNumber
            label is "DepositNumber"
            raise red alert when (ImportInProcess)
                mouse over text is "TransactionIsPartiallyAddedFromRunGroup:<OriginatingImportRun.RunGroup>RunTime:<OriginatingImportRun.RunTime>"
        Status
        TransactionType
            label is "ReceiptType"
        DepositDate
        DepositAmount
            raise red alert when (OutOfBalance)
                mouse over text is "ReceiptOutOfBalanceBy:<BalanceAmount>"
    Actions
        action is Release
            label is "Release"
            hide when invalid
        action is SubmitForApproval
            label is "SubmitForApproval"
            hide when invalid
        action is ApplyAdditionalCashAllocations
            label is "ApplyAdditionalCashAllocations"
            hide when invalid
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is open
            action icon is open
            label is "_"
            mouse over text is "Open"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is search
            label is "_"
        action is refresh
            label is "_"
