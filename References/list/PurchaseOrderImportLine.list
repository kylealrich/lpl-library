PurchaseOrderImportLineList is a List
    title is "InterfacedPurchaseOrderLines"
    no data message is "Add_a_new_Purchase_Order_Line"
        icon is icon.icon-empty-new-project-new
        button of "Create"
            action is Create
    Display Fields
        LineNumber
        RecordType
        Item
            allow update
        Description
            allow update
        VendorItem
            allow update
        ItemType
            allow update
        HSNSACCode
            allow update
            column visible when (IsHSNSACCodeEnabled)
        ServiceCode
            allow update
        Quantity
            allow update
        EnteredBuyUOM
            label is "U\\O\\M"
            allow update
        EnteredUnitCost
            label is "UnitCost"
            allow update
        EarlyDeliveryDate
            label is "DeliveryDate"
            allow update
        ShipToLocation
            label is "ShipTo"
            allow update
