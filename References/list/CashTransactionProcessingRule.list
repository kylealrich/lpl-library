CashTransactionProcessingRules is a List
    is primary
    title is "ProcessingRules"
    initially display search
    no data message is "Add_a_new_Processing_Rule"
        icon is icon.icon-empty-new-project
        button of "Create"
            action is Create
    Display Fields
        CashTransactionProcessingRule
            raise red alert when (RuleReferencesCashManagementAccount)
                mouse over text is "CustomGroupReferencesCashManagementAccount;MayCauseUnexpectedResultsIfProcessingRuleGroupIsAssignedToADifferentAccount"
            raise yellow alert when (NotInProcessingRuleGroup)
                mouse over text is "ProcessingRuleIsNotAssignedToAProcessingRuleGroup"
            sort order is primary
        Description
            allow update
        AssignmentRule
            allow update
        Active
            allow update
    restrict action AddProcessingRule
