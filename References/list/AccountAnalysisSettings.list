AccountAnalysisSettingsForActorList is a List
    propagate as a drill
    title is "AccountAnalysisSettings"
    single select
    initially display search
    no data message is "Create_an_Account_Analysis"
        description is "Account_analysis_settings_are_a_set_of_reusable_reporting_parameters_that_are_saved_for_quick_analysis."
        icon is icon.icon-empty-no-analytics
    Display Fields
        AccountAnalysisSettings
        Description
            raise red alert when (CubeOffline)
                mouse over text is "CubeIs_Unavailable"
        ReportingBasisDisplay
            label is "ReportingBasis"
            raise red alert when (ReportingBasis.BasisIsIncomplete)
                mouse over text is "BasisDefinitionIsIncomplete"
        PreservedBasisRepresentativeText
        OverridesExist
            label is "StructureOverridesExist"
        button of "AnalyzeAccounts"
            link is ChartAnalysisNav
            display as link
        button of "AnalyzeBudget"
            visible when (IsBudgetScenario)
            link is ChartAnalysisBudgetNav
            display as link
        button of "TrialBalance"
            link is TrialBalanceNav
            display as link
    Instance Selection
        where (Actor = actor)
