PhysicalInventoryImportList is a List
    is primary
    title is "PhysicalInventoryInterface"
    initially display search
    no data message is "No_Interface_Record_Available"
        icon is icon.icon-empty-no-tasks
    Display Fields
        RunGroup
            allow update
        Company
        PhysicalInventoryImport.SelectID
        PhysicalInventoryImport.InventoryLocation
        PhysicalInventoryImport.Item
        CountQuantity
            allow update
        CountUnitOfMeasure
            label is "CountUOM"
            allow update
        InventoryReasonCode
            allow update
        ErrorMessage
    Actions
        action is InterfacePhysicalInventoryCounts
        action is MassDelete
        action is Update
        action is Create
            action icon is create
            label is "_"
        action is save
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
        action is search
            label is "_"
        action is refresh
            label is "_"
