CashApplicationResults is a List
    title is "AutomaticCashApplicationResults"
    initially display search
    no data message is "No_Automatic_Cash_Application_Results_Exist"
    Display Fields
        PrmCompany
            label is "Company"
            sort order is ByCompany
        PrmCompanyGroup
            label is "CompanyGroup"
        RunTime
            sort order is ByRunTime
                is default
        Status
            display as tag
                color of green when (Status.Complete)
                color of gray when (Status.InProcess)
    Instance Selection
        where (IsValidForActorContext)
    Actions
        action is Run
            action icon is transfer
            label is "AutomaticCashApplication"
        action is DeleteAllResults
        action is open
            action icon is open
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is search
            label is "_"
        action is refresh
            label is "_"
