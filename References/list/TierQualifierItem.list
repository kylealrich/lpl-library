AllTierQualifierItems is a List
    is primary
    initially display search
    Display Fields
        Manufacturer
        ManufacturerNumber
            sort order is ByManufacturerInformation
                is default
        Vendor
            raise blue alert when (VendorEntered)
                mouse over text is "<Vendor.VendorName>"
            column visible when (TierQualifierVendorItemsExist)
        VendorItem
            column visible when (TierQualifierVendorItemsExist)
        ItemNumber
            sort order is ByItemDetails
        Description
        OriginalContractAndLine.OriginalContract
            label is "QualifierContract"
            link is QualifierContractNav
        OriginalContractAndLine.OriginalContractLine
            label is "ContractLine"
            link is QualifierContractLineNav
        ContractTier
            label is "Tier"
            sort order is ByTier
            column visible when (ContractHasTierQualifierItems)
        ContractTierQualifier
            label is "Qualifier"
            column visible when (ContractHasTierQualifierItems)
        QualifiedOrCompetitiveItem
    open action is restricted
    restrict action ResetQualifiedCompetitiveFlags
    restrict action CreateVendorItemQualifierItems
    Actions
        action is Create
            label is "CreateANonContractRelatedItem"
        action is AttachQualifierToItem
            hide when invalid
        action is Purge
        action is search
            label is "_"
        navigation is PurchaseOrderLinesForContractLine
            label is "POLinesForContract"
            hide when invalid
        navigation is PurchaseOrderLinesForItem
            label is "POLinesForItem"
            hide when invalid
        navigation is InvoiceDetailLinesForContractLine
            label is "InvoiceLinesForContract"
            hide when invalid
        navigation is SplitByVendorItem
            hide when invalid
