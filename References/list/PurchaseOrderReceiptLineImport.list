InvalidLines is a List
    Display Fields
        PurchaseOrderReceiptLineImport.LineNumber
            label is "LineNumber"
            allow update
            raise red alert when (HasLineAddOnCostsWithInvalidRunGroup)
                mouse over text is "<HasLineAddOnCostsWithInvalidRunGroupMsg>"
        ItemType
            allow update
        Item
            allow update
        VendorItem
            allow update
        Description
            allow update
        EnteredReceivedQuantity
            allow update
        ReceivedUOM
            allow update
            select is ValidUnitsOfMeasure
        RecordInError
        RunGroup
            allow update
        button of "Detail"
            sub form is Detail
            display as link
    restrict action Create
    restrict action CreateNoRules
