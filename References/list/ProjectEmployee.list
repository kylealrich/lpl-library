ActiveProjectEmployeeList is a List
    title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Employees"
    Display Fields
        Employee
            raise red alert when (EmployeeIsInactive)
                mouse over text is "EmployeeIsInactive"
        Employee.Employee.PresentationNameSnapshot
            label is "Name"
            show up to 30 characters
        group label is "Schedule"
            ProjectLaborSchedule
                label is "Labor"
            ProjectEffortSchedule
                label is "Effort"
        Certifier
            label is "Certifier"
        Certifier.PresentationNameSnapshot
            label is "Name"
            show up to 30 characters
        BurdenCode.Rate
            label is "FringeBurden"
        group label is "Encumbrance"
            SalaryEncumbrance
                label is "Salary"
            FringeEncumbrance
                label is "Fringe"
        Employee.Employee.RelationshipToOrganization
    Instance Selection
        where (Active)
    restrict action UpdateEmployeeImages
