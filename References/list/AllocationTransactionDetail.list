AllocationRunBudgetDetailList is a List
    title is "AllocationTransactions"
    single select
    initially display search
    no data message is "No_Transactions_Generated"
        icon is icon.icon-empty-no-data
    Display Fields
        AllocationTransactionDetail
            label is "Transaction"
            raise red alert when (HasError)
                mouse over text is "<ErrorMessage>"
            sort order is ByDetail
                is default
        StepNumber
            label is "Step"
        Direction
        Allocation
        AllocationLine
            label is "Line"
        FinanceCodeBlock
            label is "FinanceStructure"
        UnitsAmount
            label is "Units"
        TransactionAmount
            label is "Amount"
        CurrencyCode
            label is "Currency"
    open action is restricted
    Actions
        action is open
            label is "_"
        action is search
            label is "_"
