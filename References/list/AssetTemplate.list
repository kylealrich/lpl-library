AssetTemplate is a List
    title is "AssetTemplates"
    is default select
    no data message is "No_Asset_Templates_to_show"
        icon is icon.icon-empty-no-data
    Display Fields
        AssetTemplate
            label is "Template"
        Description
        Company
            sort order is primary
        AssetType
        AssetGuidelineClass
            allow update
            column visible when (AseetGuidelineClassesExists)
        AssetAccountingUnitGroup
        AssetAccountGroup
        AssetDivision
        AssetLocation
    Instance Selection
        where (IsValidForActorContext)
