TrueCostUnutilizedCapacityInterfaceList is a List
    is primary
    title is "UnutilizedCapacities"
    initially display search
    Display Fields
        TrueCostUnutilizedCapacityInterface.RunGroup
            raise red alert when (HasError)
                mouse over text is "<ErrorDescription>"
        TrueCostUnutilizedCapacityInterface.Sequence
        TrueCostCapacity
        TrueCostCapacityUnutilized
        UnutilizedCapacityDimension
        ErrorDescription
    Actions
        action is InterfaceUnutilizedCapacity
            label is "Interface"
            mouse over text is "InterfaceUnutilizedCapacities"
        action is CopyRunGroup
            mouse over text is "CopyRunGroup"
        action is DeleteAllTransactionsForRunGroup
            label is "DeleteTransactions"
            mouse over text is "DeleteAllTransactionsForRunGroup"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is open
            label is "_"
            mouse over text is "Open"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is search
            label is "_"
            mouse over text is "Search"
