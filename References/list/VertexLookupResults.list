VertexLookupResultsList is a List
    is primary
    title is "LookupTaxAreaResults"
    Display Fields
        VertexConfig
        RunDate
        LogSearchName
        LookupVendorGroup
            label is "VendorGroup"
            visible when (VendorEntered)
        button of "Vendor:<LookupVendor.VendorName>"
            visible when (VendorEntered)
            link is NavToVendor
            display as link
        button of "Vendor:<LookupVendor.VendorName>,Location:<LookupVendorLocation>"
            visible when (VendorAndLocationEntered)
            link is NavToVendorLocation
            display as link
        button of "Company:<LookupARCompany>,Customer:<LookupCustomer.Name>,ShipTo:<LookupCustomerShipToLoc>"
            visible when (CustomerAndShipToEntered)
            link is NavToCustomerShipTo
            display as link
        LookupCustomerGroup
            label is "CustomerGroup"
            visible when (CustomerOrShipToEntered)
        button of "Company:<LookupARCompany>,Customer:<LookupCustomer.Name>,ShipTo:<LookupCustomerShipToLoc>"
            visible when (CustomerEntered)
            link is NavToCustomerShipTo
            display as link
        button of "InventoryCompany:<LookupInventoryCompany>,InventoryLocation:<LookupInventoryLocation>"
            visible when (InventoryLocationEntered)
            link is NavToInventoryLocation
            display as link
        button of "InventoryCompany:<LookupInventoryCompany>,RequestingLocation:<LookupRequestingLocation>"
            visible when (RequestingLocationEntered)
            link is NavToRequestingLocation
            display as link
        button of "BillingCompany:<LookupBillingCompany>,ProcessLevel:<LookupBillingProcessLevel>"
            visible when (BillingProcessLevelEntered)
            link is NavToBillingProcessLevel
            display as link
        button of "PayablesCompany:<LookupPayablesCompany>,ProcessLevel:<LookupPayablesProcessLevel>"
            visible when (PayablesProcessLevelEntered)
            link is NavToPayablesProcessLevel
            display as link
        LookupPostalAddress
            label is "PostalAddress"
        DerivedMessage
            no label
        Status
            allow update
    restrict action Create
