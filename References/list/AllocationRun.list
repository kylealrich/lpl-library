AllocationControlList is a List
    title is "AllocationRuns"
    initially display search
    no data message is "No_Allocation_Runs"
        icon is icon.icon-empty-no-data
    Display Fields
        AllocationRun
            sort order is ByRunDescending
                is default
        Status
            raise red alert when (IsStatusFailedOrHasError)
                mouse over text is "<HoverErrorMessage>"
        CurrentStepNumber
            label is "Step"
        PostingDate
        AllocationPeriod.DisplayPeriod
            label is "Period"
        Allocation
        AllocationGroup
        create stamp
            label is "RunDate_/Time"
        RunEndTimeStamp
