DemandForecastSummary is a List
    title is "UnreservedProcedureDemandForecastSummary"
    initially display search
    called out actions only
    Display Fields
        Company
        InventoryLocation
        Item
        ForecastDate
        group label is "ProcedureDemandForecast"
            DemandForecastQuantity.QuantityPerDay[1]
                label is "Today"
            DemandForecastQuantity.QuantityPerDay[2]
                label is "1-Day"
            DemandForecastQuantity.QuantityPerDay[3]
                label is "2-Day"
            DemandForecastQuantity.QuantityPerDay[4]
                label is "3-Day"
            DemandForecastQuantity.QuantityPerDay[5]
                label is "4-Day"
            DemandForecastQuantity.QuantityPerDay[6]
                label is "5-Day"
            DemandForecastQuantity.QuantityPerDay[7]
                label is "Beyond5-Day"
        group label is "InventoryBalances"
            DerivedAvailableQuantity
                label is "AvailableToAllocate"
            DerivedReservedQuantity
                label is "Reserved"
            DerivedUnreservedQuantity
                label is "Unreserved"
    open action is restricted
    Actions
        action is DemandForecasting
            label is "RunDemandForecast"
        action is ReserveForScheduledProcedures
        action is refresh
            label is "_"
