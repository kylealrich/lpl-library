ColumnarList is a List
    title is "Commitments"
    single select
    search form is ColumnarSearchForm
    implements ColumnarView
    no data message is "Use_search_to_find_your_data."
        icon is icon.icon-empty-no-data
    Display Fields
        System
            label is "System"
        DerivedDocumentNumber
            label is "DocumentNumber"
        IsContractManagement
            label is "Contracts"
        TransactionDate
        Status
        DerivedDisplayPeriod
            label is "PeriodDate"
        FinanceCodeBlock.Ledger
            label is "Ledger"
        FinanceCodeBlock.ToAccountingEntity
            label is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
        FinanceCodeBlock.GeneralLedgerChartAccount
            label is "<actor.context.FinanceEnterpriseGroup.AccountLabel>"
        FinanceCodeBlock.Project
            label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
        FinanceCodeBlock.FinanceDimension2
            label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>"
        TransactionAmount
            total
    Instance Selection
        where (FinanceEnterpriseGroup = actor.context.FinanceEnterpriseGroup)
    Charts
        GLCommitATByAccountingEntity is an AggregationTable
            title is "TotalsBy<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
            chart list
            show totals first
            Dimensions
                FinanceCodeBlock.ToAccountingEntity
                    label is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
                    ascending
                System
                    label is "System"
                    ascending
                DerivedDocumentNumber
                    label is "DocumentNumber"
                    ascending
                IsContractManagement
                    label is "Contracts"
                    ascending
                TransactionDate
                    ascending
                Status
                    ascending
                DerivedDisplayPeriod
                    label is "PeriodDate"
                    ascending
                FinanceCodeBlock.Ledger
                    label is "Ledger"
                    ascending
                FinanceCodeBlock.GeneralLedgerChartAccount
                    label is "<actor.context.FinanceEnterpriseGroup.AccountLabel>"
                    ascending
                FinanceCodeBlock.Project
                    label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
                    ascending
                FinanceCodeBlock.FinanceDimension2
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>"
                    ascending
            Measures
                sum TransactionAmount
            Subtotal By
                FinanceCodeBlock.ToAccountingEntity
                    page break
