InterfacePaymentsList is a List
    title is "InterfaceNonARPayments"
    initially display search
    no data message is "No_Non_AR_Payment_Interface_Records_Exist"
        icon is icon.icon-empty-no-notes
        button of "CreateInterfaceRecord"
            action is Create
    Display Fields
        RunGroup
            sort order is ByRunGroup
                is default
        Company
        BatchNumber
        RecordType
        TransNbr
            label is "PaymentNumber"
        TranAmt
            label is "TransactionAmount"
        SumOfDistributions
        SumOfDistributionsDifference
            label is "Difference"
        ErrorMessage
    Instance Selection
        where (IsValidForActorContext)
    Actions
        action is InterfaceNonARPayments
            action icon is transfer
        action is ResetErrorMessage
            action icon is undo
            hide when invalid
        action is Create
            label is "_"
        action is open
            label is "_"
        action is Delete
            label is "_"
        action is search
            label is "_"
        action is refresh
            label is "_"
