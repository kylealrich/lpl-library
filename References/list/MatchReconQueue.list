MatchPrepayments is a List
    title is "MatchPrepayments"
    initially display search
    no data message is "No_Prepaid_Purchase_Order_Invoices"
        icon is icon.icon-empty-no-orders-new
    Display Fields
        Company
            sort order is primary
        Vendor
            sort order is ByVendor
        VendorSearchName
            sort order is ByVendorSearchName
        Invoice
            sort order is ByInvoice
        DueDate
            sort order is ByDueDate
        PurchaseOrder
            sort order is ByPurchaseOrder
        PurchaseOrder.ShipToLocation
        PayablesInvoice.AuthorityCode
        Buyer
        Buyer.Employee.PresentationNameSnapshot
            label is "BuyerName"
        InvoiceAmount
        button of "Comments"
            link is SmartReconMatchInvoiceMessageCommentNavigation
            display as link
    Instance Selection
        where (PayablesInvoice.AvailableForReconciliationQueue
        and   PayablesInvoice.MatchPrepayment
        and   (PayablesInvoice.MatchStatus.Unmatched
            or    PayablesInvoice.MatchStatus.POCostMessagesExist))
    form is SmartReconDetailMatchComposite
        when (not SmartReconUIV2)
    form is SmartReconDetailMatchCompositeV2
        when (SmartReconUIV2)
    restrict action SetChargeback
    restrict action AddToMyWorkList
    Actions
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is search
            label is "_"
        action is refresh
            label is "_"
