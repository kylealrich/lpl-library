OLAPConnections is a List
    is primary
    title is "OLAPConnections"
    Display Fields
        OLAPConnection
            label is "Connection"
            left align
        Status
            left align
            link is OLAPConnection
            raise red alert when (IsErrorStatus)
                mouse over text is "CouldNotConnectToTheOLAPServer"
            raise yellow alert when (Status.Unknown)
                mouse over text is "CouldNotDetermineTheConnectionStatus"
            raise green alert when (Status.Healthy)
                mouse over text is "ConnectionToOLAPServerWasSuccessful"
    Detail Sections
        Entries
            title is "<OLAPConnection>Cubes"
            list is AnalyticCubesRel.AnalyticCubeStatusDetailList
