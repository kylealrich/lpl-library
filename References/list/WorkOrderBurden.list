Burdens is a List
    is primary
    title is "Burdens"
    no data message is "Doesn't_look_like_there's_any"
        icon is icon.icon-empty-no-budget-new
        button of "Create"
            valid when (not WorkOrder.IsReleasedOrUnreleased)
            action is Create
    Display Fields
        WorkOrderBurden
            label is "Sequence"
        BurdenCode
            label is "Burden"
        BurdenCode.Description
        EstimatedTotal
        ConfirmationMessage
            label is "Message"
