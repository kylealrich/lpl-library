HeaderAddOnCosts is a List
    implements InlineCreate
    no data message is "Start_a_new_Add_On_Cost"
        icon is icon.icon-empty-new-project-new
        button of "Create"
            action is Create
    Display Fields
        PurchaseOrderReceiptLineAOCImport.AddOnCharge
            label is "AddOnCharge"
            allow update
        Vendor
            allow update
            select is ActiveVendorsByName
        TotalAddOnChargeAmount
            allow update
