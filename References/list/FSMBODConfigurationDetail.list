ActiveAlertsList is a List
    title is "ActiveAlerts"
    no data message is "No_Active_Alerts"
        icon is icon.icon-empty-no-data
    card view is ActiveAlertsCardView
    Display Fields
    Instance Selection
        where (FSMBODConfiguration.Direction.Outbound
        and   FSMBODConfiguration.Noun = "PulseAlert"
        and   FSMBODConfiguration.Enable
        and   Enable)
    restrict action Create
