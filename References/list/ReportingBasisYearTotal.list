PrimaryReportingBasisYearTotalList is a List
    is primary
    propagate as a drill
    title is "BeginingBalance"
    implements HorizontalScrolling
        freeze first 5 columns
    Display Fields
        ReportingBasisYearTotal.AccountingEntity
            label is "<FinanceEnterpriseGroup.AccountingEntityLabel>"
        ReportingBasisYearTotal.GeneralLedgerChartAccount
            label is "<FinanceEnterpriseGroup.AccountLabel>"
        ReportingBasisYearTotal.EntityYear
            label is "Period"
            select is BegBalAndEndDateList
        ReportingBasisYearTotal.System
            label is "System"
        ReportingBasisYearTotal.Currency
            label is "Currency"
        ReportingBasisYearTotal.PrimaryLedger
            label is "PrimaryLedger"
        ReportingBasisYearTotal.AccountingUnit
            label is "<FinanceEnterpriseGroup.AccountingUnitLabel>"
            column visible when (AccountingUnitUsed)
        TransactionAmount
            label is "TransactionAmount"
        FunctionalAmount
            label is "FunctionalAmount"
        AlternateAmount
            label is "AlternateAmount"
        AlternateAmount2
            label is "Alternate2Amount"
        AlternateAmount3
            label is "Alternate3Amount"
        ProjectAmount
            label is "ProjectAmount"
            column visible when (ProjectExists)
        ReportAmount1
            label is "ReportingCurrency<FinanceEnterpriseGroup.ReportCurrencyOne>"
            column visible when (ReportCurrencyOneExists)
        ReportAmount2
            label is "ReportingCurrency<FinanceEnterpriseGroup.ReportCurrencyTwo>"
            column visible when (ReportCurrencyTwoExists)
        ReportAmount3
            label is "ReportingCurrency<FinanceEnterpriseGroup.ReportCurrencyThree>"
            column visible when (ReportCurrencyThreeExists)
        ReportAmount4
            label is "ReportingCurrency<FinanceEnterpriseGroup.ReportCurrencyFour>"
            column visible when (ReportCurrencyFourExists)
        ReportAmount5
            label is "ReportingCurrency<FinanceEnterpriseGroup.ReportCurrencyFive>"
            column visible when (ReportCurrencyFiveExists)
