AllCashLedgerElectronicFundsTransferTransactionList is a List
    is primary
    title is "AllElectronicFundsTransfers"
    no data message is "No_Electronic_Funds_Transfers_to_view"
        icon is icon.icon-empty-no-tasks
    Display Fields
        CashCode
        CashLedgerElectronicFundsTransferTransaction.BankTransactionCode
            label is "TransactionCode"
        Reference
            label is "ReferenceNumber"
            raise red alert when (MultiRedAlertErrorsExist)
                mouse over text is "<DerivedFirstErrorMessage>"
        Status
        EFTProcessingHold
        ReceiptAmount
            label is "Amount"
            raise red alert when (IsUnreleasedOutOfbalance)
                mouse over text is "TransactionOutOfBalanceBy:<BalanceAmount>"
        ConfirmationNumber
        TransactionDate
        ElectronicFundsTransferDepositDate
            label is "DepositDate"
        VoidDate
            label is "RejectDate"
    form is EFTCCExtractEntryForm
    restrict action EFTRejectUpdate
    Actions
        action is Release
            label is "Release"
            hide when invalid
        action is SubmitForApproval
            label is "SubmitForApproval"
            hide when invalid
        action is CancelUnreleasedEFT
            label is "Cancel"
            hide when invalid
        action is CancelReleasedEFT
            label is "Cancel"
            hide when invalid
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is open
            action icon is open
            label is "_"
            mouse over text is "Open"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is search
            label is "_"
        action is refresh
            label is "_"
