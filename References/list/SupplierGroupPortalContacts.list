AccountsPayableContactList is a List
    title is "AccountsPayableContacts"
    Display Fields
        ContactName
        ContactPhone
        button of "<ContactEmail>"
            visible when (EmailAddressExists)
            link is "mailto:<ContactEmail>"
            label is "EmailAddress"
            display as link
    Instance Selection
        where (DisplayOnPortal)
    form is PortalSupplierContacts
    restrict action Create
    restrict action Update
    restrict action Delete
