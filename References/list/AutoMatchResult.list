AutoMatchResults is a List
    is primary
    title is "AutoMatchResults"
    initially display search
    no data message is "NoCurrentAutoMatchResults"
        description is "To_\\Start_\\Auto_\\Match,ClickRunAutoMatchInTheTopRight"
        icon is icon.icon-empty-no-budget
        button of "PerformAutoMatch"
            action is RunAutoMatch
    Display Fields
        AutoMatchResult
            label is "ResultID"
        PrmCompanyGroup
            label is "CompanyGroup"
        PrmCompany
            label is "Company"
        RunTime
            sort order is ByRunTime
                is default
        SelectionComplete
        DoneProcessing
            raise yellow alert when (TerminatedInvoicesExist)
                mouse over text is "<ProcessingErrorsMsg>"
        PrmInterfaceRun
            label is "InterfaceRunID"
        MarkedComplete
    Actions
        action is RunAutoMatch
            label is "RunAutoMatch"
        action is PurgeResults
            label is "PurgeResults"
        navigation is SummaryNavigation
            label is "SummaryDashboard"
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is open
            label is "_"
        action is search
            label is "_"
        action is refresh
            label is "_"
