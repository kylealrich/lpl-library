DeliveryTicketAndBackorderList is a List
    title is "DeliveryTicketsAndBackorders"
    single select
    initially display search
    no data message is "Patiently_awaiting_the_next_Delivery_Tickets_And_Backorders"
        icon is icon.icon-empty-no-tasks-new
    Display Fields
        PurchaseOrderResult
            label is "DocumentID"
        ParametersPrintDeliveryAndPutaway.Company
            label is "Company"
        Status
        FinishedTimeStamp
            label is "PrintedTimestamp"
        button of "ViewPDF"
            link is DeliverAndBackOrderNav
            label is "_"
            display as link
    Instance Selection
        where (ReportType.DeliveryTicketAndBackorderReport)
    form is DeliveryAndPutawayResultForm
    open action is restricted
    restrict action CreateReceivedNotInvoicedReport
    restrict action ResubmitReceivedNotInvoicedReport
    restrict action Delete
