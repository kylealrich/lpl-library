CashManagementLocations is a List
    is primary
    title is "CompanyLocations"
    initially display search
    implements TreeView
        has children when (HasChild)
    no data message is "Add_a_new_Company_Location"
        icon is icon.icon-empty-new-project
        button of "Create"
            action is Create
    Display Fields
        CashManagementLocation
        Description
            allow update
        Currency
        Active
            allow update
