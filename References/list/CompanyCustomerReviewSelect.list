AnalystCustomerReviewList is a List
    title is "CustomerReviews"
    initially display search
    no data message is "No_Customer_Review_Selections_Exist"
    Display Fields
        CompanyCustomerReviewSelect.Customer
        CompanyCustomerReviewSelect.Customer.Name
        CompanyCustomerReviewSelect.Date
            label is "Selected"
        Type
            label is "SelectReason"
        CustomerOrNationalAccountBaseBalance
            label is "Balance"
        DisplayTransactionCurrencyCode
            label is "Currency"
        CustomerOrNationalAccountHoldCode
            label is "HoldCode"
        button of "Customer"
            visible when (IsCompanyCustomerLevel)
            link is CompanyCustomerReviewNav
            display as link
        button of "NationalAccount"
            visible when (IsNationalAccountCustomerLevel)
            link is NationalAccountReviewNav
            display as link
    Instance Selection
        where (IsOpen)
    form is CompanyCustomerAnalystReview
