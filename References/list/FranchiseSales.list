ActualSales is a List
    show 10 lines
    initially display search
    no data message is "No_Actual_Sales_records_to_view"
        icon is icon.icon-empty-no-tasks
        button of "Create"
            action is Create
    Display Fields
        Company
        Customer
            select is CustomerSelectList
        CustomerShipTo
        FranchiseContract
            label is "Contract"
        FranchiseSales.Date
        FranchiseSales.AdjustmentNumber
        Status
    Instance Selection
        where (FranchiseSales.SalesType.Actualinvoice
        and   FranchiseContract.IsValidForActorContext)
    Detail Sections
        CycleCharges
            list is FranchiseCycleChargesRel.CycleCharges
