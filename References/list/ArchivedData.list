ArchivedDataList is a List
    is primary
    title is "DetailResults"
    single select
    Display Fields
        ArchivedData.PurgedBusinessClass
            sort order is primary
                is default
        ParentBusinessClass
        ArchivedData.Sequence
        RecordCount
        SentToArchive
            raise red alert when (RaiseRedAlert)
                mouse over text is untranslatable:"<RedAlertMessage>"
            raise blue alert when (RaiseBlueAlert)
                mouse over text is "DataRestoredOn<LastRestoredStamp>"
        update stamp
            sort order is ByUpdateStamp
        button of "View"
            visible when (HasDocument)
            view DecodedJSONData
