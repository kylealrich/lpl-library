Comments is a List
    implements InlineCreate
    no data message is "No_Conversion_Or_Interface_Comments_Exist"
    Display Fields
        SeqNbr
            label is "SequenceNumber"
            allow update
        Comment
            allow update
        BeginDate
            allow update
        EndDate
            allow update
        TransientPrintCode
            allow update
        CommentText
            allow update
    Actions
        action is ConvertComments
            action icon is transfer
            hide when invalid
        action is Create
            label is "_"
        action is open
            label is "_"
        action is save and new
            label is "_"
        action is Delete
            label is "_"
        action is search
            label is "_"
