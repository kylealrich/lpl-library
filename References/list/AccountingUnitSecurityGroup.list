AccountingUnitSecurityGroups is a List
    is primary
    title is "AccountingUnitSecurityGroups_(FEG_Dimension_'\\<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>')"
    initially display search
    Display Fields
        AccountingUnitSecurityGroup
            raise yellow alert when (NoActorsAssigned)
                mouse over text is "NoActorsAssigned"
        Description
        RequiresRebuild
            raise red alert when (RequiresRebuild)
                mouse over text is "GroupMustBeRebuilt,RunThe_Rebuild_GroupsAction"
        MatrixSecurityGroup
            column visible when (MatrixSecurityEnabled)
    Actions
        action is DeleteSecurityGroup
        action is RebuildGroups
        action is CopyMembers
        navigation is SecurityGroupFactSheet
        action is Create
            action icon is create
            label is "_"
        action is Update
            action icon is open
            label is "_"
        action is search
            action icon is filter
            label is "_"
        action is refresh
            label is "_"
    Detail Sections
        Members
            list is AccountingUnitSecurityGroupMember set.primary
        AccessibleAccountingUnits
            list is AccountingUnitSecurityGroupDetail set.primary
        ActorsAssignedToThisGroup
            list is ActorContextRel.primary
