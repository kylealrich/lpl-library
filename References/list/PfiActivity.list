ActivityList is a List
    title is "ActivityNodes"
    initially display search
    Display Fields
        PfiActivity
            sort order is primary
                is default
        ActivityName
            sort order is ByWorkunitActivityNameActivity
        ActivityType
            sort order is ByWorkunitActivityTypeActivity
        ActStatus
            raise red alert when (IsFailed)
                mouse over text is "Failed"
            sort order is ByWorkunitActStatusActivity
        StartDate
            sort order is ByWorkunitStartDateActivity
        EndDate
            sort order is ByWorkunitEndDateActivity
        ElapsedTimeAsString
            label is "ExecutionTime"
    form is ActivityComposite
    restrict action CreateComparisonActivity
