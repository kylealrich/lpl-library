CheckPointActorList is a List
    title is "Checkpoint"
    Display Fields
        CheckPoint.JobName
            label is "JobName"
            sort order is primary
                is default descending
        CheckPoint.JobKeysHash
            label is "JobKeysHash"
        Actor
            sort order is ByActor
        create stamp
            label is "Created"
            sort order is ByCreateStamp
        update stamp
            label is "LastUpdated"
            sort order is ByUpdateStamp
        PendingRestart
        StatusMessage
    Instance Selection
        where (Actor = actor)
