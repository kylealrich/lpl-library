ChangePasswordList is a List
    title is "ResetCandidatePassword"
    Display Fields
        Identity
        Service
        first IdentityActor set.Actor.PersonName.GivenName
            label is "FirstName"
        first IdentityActor set.Actor.PersonName.FamilyName
            label is "LastName"
    Instance Selection
        where (Service = config.ResetCandidatePassword)
    form is ResetPasswordForm
    restrict action Create
    restrict action Update
    restrict action Delete
    restrict action Purge
    restrict action ClearSecurityAnswers
    restrict action CreateAS400Identity
    restrict action CreateDatabaseIdentity
    restrict action CreateFormBasedIdentity
    restrict action CreateIdentity
    restrict action CreateMISocketIdentity
    restrict action CreateUnixIdentity
    restrict action CreateWebServiceIdentity
    restrict action CreateWindowsIdentity
    restrict action ForcePasswordReset
    restrict action MarkForPasswordReset
    restrict action ChangePassword
    restrict action AdminChangePassword
    restrict action ViewFullSummary
    restrict action UpdateSecurityAnswers
    Actions
        action is search
        action is ResetPassword
        action is GenerateAndSendPassword
            label is "GenerateAndSendPassword"
