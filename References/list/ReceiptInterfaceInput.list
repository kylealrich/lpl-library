ReceiptInterfaceInputList is a List
    is primary
    title is "ReceiptInterfaceInput"
    always display search
    no data message is "No_new_Receipt_Interface_Input"
        icon is icon.icon-empty-no-orders-new
    Display Fields
        RunGroup
        RecordType
            raise red alert when (ReferenceNotEntered)
                mouse over text is "ReferenceNumberRequired"
        Company
            raise red alert when (HasDuplicateReceiptImport)
                mouse over text is "DuplicateReceiptExistsInReceiptInterfaceMaintenance"
        ReferenceNumber
            raise red alert when (HasDuplicateReceipt)
                mouse over text is "DuplicateReceiptExistsInReceiptInterface"
            sort order is ByRunGroupUniqueID
                is default
        LineNumber
            raise red alert when (DuplicateLineExists)
                mouse over text is "DuplicateLineRecordExists"
        PurchaseOrder
        InterfacedPurchaseOrder
        PoCode
        Item
        VendorItem
        HSNSACCode
            column visible when (IsHSNSACCodeEnabled)
        Description
        AddOnCharge
            raise red alert when (DuplicateAOCExists)
                mouse over text is "DuplicateAddOnChargeRecordExists"
        Location
    Actions
        action is InterfaceReceipts
            action icon is process
        action is DeleteAllTransactionsForRunGroup
            action icon is reject
        action is open
            label is "_"
        action is search
            label is "_"
        action is refresh
            label is "_"
