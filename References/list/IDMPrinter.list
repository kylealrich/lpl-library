ActiveIDMPrinter is a List
    title is "ActivePrinters"
    single select
    initially display search
    no data message is "Add_a_New_IDM_Printer"
        icon is icon.icon-empty-no-tasks
        button of "Create"
            action is Create
    Display Fields
        IDMPrinter
            raise blue alert when (ActorDefaultPrinter)
                mouse over text is "DefaultPrinter"
        LocationName
            label is "Location"
        PrinterID
        ExistsInIDM
        Type
    Instance Selection
        where (Active)
    Actions
        action is SyncIDMPrinters
            action icon is process
        action is PrintTestPage
            action icon is print
        action is SetAsDefaultPrinter
            action icon is apply
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is open
            action icon is open
            label is "_"
            mouse over text is "Open"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is search
            label is "_"
            mouse over text is "Search"
        action is refresh
            label is "_"
            mouse over text is "Refresh"
