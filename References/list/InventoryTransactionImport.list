ImportTransactionDocument is a List
    title is "InventoryTransactionInterfaceDocuments"
    initially display search
    no data message is "No_TransactionInterfaceDocuments_Record_Exist"
        icon is icon.icon-empty-no-data
    Display Fields
        RunGroup
            allow update
        Company
        InventoryLocation
            label is "Location"
        InventoryTransactionImport
            label is "ImportedDocument"
        InventoryTransactionNumber
            link is PartialInventoryTransaction
        InventoryDocumentType
            label is "Type"
        ErrorLevel
        ErrorMessage
    Actions
        action is InterfaceInventoryTransactions
        action is MassDelete
        action is Create
            action icon is create
            label is "_"
        action is Update
            action icon is open
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
        action is search
            label is "_"
        action is refresh
            label is "_"
