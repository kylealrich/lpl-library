InterfaceInvoices is a List
    title is "PayablesInterfaceInvoices"
    initially display search
    no data message is "There_are_currently_no_Interface_Invoices"
        icon is icon.icon-empty-no-tasks
        button of "Create"
            action is Create
    Display Fields
        RunGroup
        Company
        Vendor
        Invoice
            label is "Invoice"
            raise yellow alert when (IsPartiallyImported)
                mouse over text is "InvoiceIsPartiallyInterfaced"
            sort order is ByRunGroup
                is default
        Suffix
        ProcessLevel
        InvoiceDate
        DueDate
        InvoiceAmount
        RecordInError
        ErrorMessage
    open action is Update
    Actions
        action is MultiThreadInterfaceInvoices
            action icon is process
            hide when invalid
        action is InterfaceInvoices
            action icon is process
            hide when invalid
        action is DeleteAllTransactionsForRunGroup
            action icon is delete
            hide when invalid
        action is ResetErrorMessage
            action icon is finalize
            hide when invalid
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is open
            label is "_"
        action is save
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
