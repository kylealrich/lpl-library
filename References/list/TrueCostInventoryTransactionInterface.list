TrueCostInventoryTransactionIntList is a List
    is primary
    title is "InventoryTransactions"
    Display Fields
        TrueCostInventoryTransactionInterface.RunGroup
            raise red alert when (HasError)
                mouse over text is "<ErrorDescription>"
        TrueCostInventoryTransactionInterface.Sequence
        Company
        Transaction
        TransactionDate
        Item
        Quantity
        UnitCost
        AccountingEntity
        AccountingUnit
        ErrorDescription
    Actions
        action is GenerateResourceCapacityRates
            mouse over text is "GenerateResourceCapacityRates"
        action is CopyRunGroup
            mouse over text is "CopyRunGroup"
        action is DeleteAllTransactionForRunGroup
            label is "DeleteTransactions"
            mouse over text is "DeleteAllTransactionForRunGroup"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is open
            label is "_"
            mouse over text is "Open"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is search
            label is "_"
            mouse over text is "Search"
