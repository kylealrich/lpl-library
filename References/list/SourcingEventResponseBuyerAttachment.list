BuyerAttachmentList is a List
    is primary
    title is "BuyerAttachments"
    Display Fields
        SeqNumber
            label is "AttachmentNumber"
        Attachment.Title
            label is "AttachmentTitle"
            allow update
        button of "<Attachment.Title>"
            view Attachment.File
            label is "Attachment"
            left align
            display as link
