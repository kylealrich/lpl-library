AssetAccountBalanceByTypeList is a List
    title is "AssetAccounts"
    suppress sub-totals
    Display Fields
        StudentActivityAccount
            label is "Activity"
        StudentActivityAccount.Description
        BeginningBalanceByType
            display when blank
            display zero
            total
        PeriodReceiptsByType
            label is "Receipts"
            display when blank
            display zero
            total
        PeriodDisbursementsByType
            label is "Disbursements"
            display when blank
            display zero
            total
        PeriodTransfersByType
            label is "Transfers"
            display when blank
            display zero
            total
        PeriodEndingBalanceByType
            label is "EndingBalance"
            display when blank
            display zero
            total
    Instance Selection
        where (HasBalancesByType
        and   StudentActivityAccount.StudentActivityType.Asset)
    form is StudentActivityBalanceByTypeForm
