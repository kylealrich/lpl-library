AssetTemplateBookList is a List
    is primary
    title is "AssetTemplateBooks"
    single select
    initially display search
    Display Fields
        AssetTemplate
        Book
        TransientBookRegulation
            label is "BookRegulation"
            column visible when (AllowsBookUpdateWithRegulation)
        Convention
            allow update
        Active
            allow update
        Method
            allow update
            select is DepreciationMethods
        Life
            allow update
        LifeInYears
            allow update
        BusinessPercent
            label is "Business%"
            allow update
        SalvagePercentage
            allow update
        group label is "CalculationMethod"
            InsuranceCalculationMethod
                label is "Insurance"
                allow update
            ReplacementCalculationMethod
                label is "Replacement"
                allow update
