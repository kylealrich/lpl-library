BankServiceCodeLatestList is a List
    title is "FeeServiceCodes"
    no data message is "No_Fee_Service_Codes_to_view"
        icon is icon.icon-empty-no-tasks
    Display Fields
        BankServiceCode
            label is "Code"
            sort order is primary
                is default
        Description
            allow update
        BankServiceCategory
            label is "Category"
            allow update
        AsOfDate
            sort order is ByAsOfDate
        UnitPrice
            allow update
        ZeroDollarAmount
            allow update
    Instance Selection
        where (IsLatestVersion)
    restrict action Create
