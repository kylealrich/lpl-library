BuyerInvoiceLineFactConsolidateItemForBuyer is a List
    title is "ItemsForBuyer-<ContractLabel>InDescendingOrderByAmount-<ProcurementGroup.first BuyerSpendInvoiceLineFactConsolidateControlsRel.ConsolidationDateRange.Begin>-<ProcurementGroup.first BuyerSpendInvoiceLineFactConsolidateControlsRel.ConsolidationDateRange.End>"
    suppress sub-totals
    Display Fields
        DerivedItemValue
            label is "Item"
            raise red alert when (ConsolidatePercentRedAlert)
                mouse over text is "ItemOffContractPurchasesAreGreaterThan20%"
            raise yellow alert when (ConsolidatePercentYellowAlert)
                mouse over text is "ItemOffContractPurchasesAreBetween10And20%"
            sort order is ByItemLevel3
        ItemDescription
            label is "ItemDescription"
        DerivedContractItemType
            label is "ItemType"
        ShipToLocation
            label is "Location"
        TotalExtendedAmount
            label is "OffContractAmount"
            sort order is ByAmount
            total
            column visible when (OffContract)
        OnOffPercent
            label is "OffContractPercent"
            sort order is ByOffPercent
            column visible when (OffContract)
        TotalExtendedAmount
            label is "OnContractAmount"
            sort order is ByAmount
            total
            column visible when (OnContract)
        OnOffPercent
            label is "OnContractPercent"
            sort order is ByOffPercent
            column visible when (OnContract)
        TotalExtendedAmount
            label is "NoContractAmount"
            sort order is ByAmount
            total
            column visible when (NoContract)
        OnOffPercent
            label is "NoContractPercent"
            sort order is ByOffPercent
            column visible when (NoContract)
    Instance Selection
        where (InvoiceLineFactConsolidate.Level2.Type.ShipToLocation
        and   HasInvoiceLinesForLocationItem)
    form is BuyerSpendInvoiceLinesForLocationItem
    restrict action Create
    restrict action Update
    restrict action CreateEvent
