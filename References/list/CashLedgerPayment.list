AllCashLedgerPayments is a List
    is primary
    title is "CashLedgerPayments"
    no data message is "No_Cash_Ledger_Payments_to_view"
        icon is icon.icon-empty-no-tasks
    Display Fields
        CashCode
        CashLedgerPayment.BankTransactionCode
        Reference
            label is "ReferenceNumber"
            raise red alert when (MultiRedAlertErrorsExist)
                mouse over text is "<DerivedFirstErrorMessage>"
        PaymentProcessingHold
        TransactionNumber
            label is "PaymentNumber"
            raise red alert when (ImportInProcess)
                mouse over text is "TransactionIsPartiallyAddedFromRunGroup:<OriginatingImportRun.RunGroup>RunTime:<OriginatingImportRun.RunTime>"
        Status
        PaymentStatus
        PaymentDate
        BankCheckAmount
            raise red alert when (OutOfBalance)
                mouse over text is "PaymentOutOfBalanceBy:<BalanceAmount>"
    form is PaymentEntryForm
    restrict action CreateBatchApproval
    Actions
        action is Unreleased.Release
            label is "Release"
            hide when invalid
        action is Unreleased.SubmitForApproval
            label is "SubmitForApproval"
            hide when invalid
        action is Released.ChangePaymentProcessingHold
            label is "ChangePaymentProcessingHold"
            hide when invalid
        action is PendingApproval.ChangePaymentProcessingHold
            label is "ChangePaymentProcessingHold"
            hide when invalid
        action is Unreleased.Cancel
            label is "Cancel"
            hide when invalid
        action is Released.Cancel
            label is "Cancel"
            hide when invalid
        action is Unreleased.Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is open
            action icon is open
            label is "_"
            mouse over text is "Open"
        action is Unreleased.Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is search
            label is "_"
        action is refresh
            label is "_"
