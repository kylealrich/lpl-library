CashLedgerReceiptImportList is a List
    is primary
    title is "ReceiptInterface"
    initially display search
    no data message is "There_are_currently_no_Receipt_Interface_records"
        icon is icon.icon-empty-no-tasks
    Display Fields
        CashManagementGroup
        CashLedgerReceiptImport.RunGroup
        CashLedgerReceiptImport.CashCode
        CashLedgerReceiptImport.BankTransactionCode
        CashLedgerReceiptImport.TransactionNumber
        CashLedgerReceiptImport.SequenceNumber
        Company
        TransactionType
        TransactionDate
        ReceiptAmount
            label is "DistributionAmount"
        ErrorMessage
    Actions
        action is InterfaceCashReceipts
            label is "InterfaceCashReceipts"
            hide when invalid
        action is DeleteAllTransactionsForRunGroup
            label is "DeleteAllTransactionsForRunGroup"
            hide when invalid
        action is DeletePartiallyImportedTransaction
            label is "DeletePartiallyImportedTransaction"
            hide when invalid
        action is Error.ResetErrorsByRunGroup
            label is "ResetErrorsByRunGroup"
            hide when invalid
        action is ReadyToImport.Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is open
            action icon is open
            label is "_"
            mouse over text is "Open"
        action is ReadyToImport.Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
            hide when invalid
        action is Error.Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
            hide when invalid
        action is search
            label is "_"
        action is refresh
            label is "_"
