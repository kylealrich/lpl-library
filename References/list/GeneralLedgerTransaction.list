ColumnarList is a List
    title is "PostedTransactions"
    single select
    search form is ColumnarSearchForm
    implements ColumnarView
    no data message is "Use_search_to_find_your_data."
        icon is icon.icon-empty-no-data
    Display Fields
        AccountingEntity
            label is "<FinanceEnterpriseGroup.AccountingEntityLabel>"
        PostingDate
        GeneralLedgerClosePeriod.GeneralLedgerCalendarPeriod
            label is "JournalPeriod"
        GeneralLedgerJournalControl
            label is "Journal"
        FinanceCodeBlock.Ledger
            label is "Ledger"
        FinanceCodeBlock.ToAccountingEntity
            label is "PostTo<FinanceEnterpriseGroup.AccountingEntityLabel>"
        FinanceCodeBlock.GeneralLedgerChartAccount
            label is "<FinanceEnterpriseGroup.AccountLabel>"
        FinanceCodeBlock.Project
            label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
            column visible when (actor.context.FinanceEnterpriseGroup.ProjectEntry)
        FinanceCodeBlock.FinanceDimension2
            label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>"
            column visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension2Entry)
        Status
        System
        ColumnarAmount
            label is "Functional"
            total
    Instance Selection
        where (PeriodWithinSearchPeriodRange
        and   MatchesColumnarSearchFinanceDimensions
        and   MatchesColumnarSearchProject
        and   MatchesColumnarSearchAccountingUnit
        and   MatchesColumnarSearchAccount)
    restrict action Unreleased.Create
    restrict action Unreleased.Update
    restrict action Unreleased.Delete
    restrict action CopyTransaction
    restrict action ManualEncumbrance.MaintainEncumbrance
    restrict action CreateReconcilingItem
    restrict action CreateDistribution
    restrict action CreateAmortization
    Charts
        GLTranATByAccountingEntity is an AggregationTable
            title is "TotalsBy<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
            chart list
            show totals first
            Dimensions
                AccountingEntity
                    label is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
                    ascending
                FinanceCodeBlock.Ledger
                    label is "Ledger"
                    ascending
                PostingDate
                    ascending
                GeneralLedgerClosePeriod.GeneralLedgerCalendarPeriod
                    label is "JournalPeriod"
                    ascending
                GeneralLedgerJournalControl
                    label is "Journal"
                    ascending
                FinanceCodeBlock.ToAccountingEntity
                    label is "PostTo<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
                    ascending
                FinanceCodeBlock.GeneralLedgerChartAccount
                    label is "<actor.context.FinanceEnterpriseGroup.AccountLabel>"
                    ascending
                FinanceCodeBlock.Project
                    label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
                    ascending
                    dimension valid when (actor.context.FinanceEnterpriseGroup.ProjectEntry)
                FinanceCodeBlock.FinanceDimension2
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>"
                    ascending
                    dimension valid when (actor.context.FinanceEnterpriseGroup.FinanceDimension2Entry)
            Measures
                sum ColumnarAmount
                    label is "FunctionalAmount"
            Subtotal By
                AccountingEntity
                    page break
