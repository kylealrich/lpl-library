AllActiveWarehouseDockList is a List
    title is "Docks"
    no data message is "ThereAreNo_DockRecordsYet"
        icon is icon.icon-empty-no-tasks-new
        button of "Create"
            action is Create
    Display Fields
        Company
        InventoryLocation
        WarehouseDock
        Description
        DockType
        Active
    Instance Selection
        where (Active
        and   DockType.LoadingDock
        or    DockType.OpenDock)
