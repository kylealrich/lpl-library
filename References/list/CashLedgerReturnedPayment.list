AllTransactions is a List
    no data message is "No_Transactions_to_view"
        icon is icon.icon-empty-no-tasks
    Display Fields
        TransactionNumber
            label is "TransactionNumber"
            raise red alert when (BudgetEditErrorExists)
                mouse over text is "<DerivedFirstErrorMessage>"
        ReturnDate
            label is "Date"
        Status
        Description
        BankAccountReturnAmount
            label is "Amount"
            raise red alert when (OutOfBalance)
                mouse over text is "ReturnedPaymentOutOfBalanceBy:<BalanceAmount>"
    Actions
        action is Release
            label is "Release"
            hide when invalid
        action is SubmitForApproval
            label is "SubmitForApproval"
            hide when invalid
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is open
            action icon is open
            label is "_"
            mouse over text is "Open"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is search
            label is "_"
        action is refresh
            label is "_"
