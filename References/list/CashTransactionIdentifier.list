CashTransactionIdentifiers is a List
    is primary
    title is "CashTransactionIdentifiers"
    initially display search
    no data message is "Add_a_new_Identifier"
        icon is icon.icon-empty-new-project
        button of "Create"
            action is Create
    Display Fields
        CashTransactionIdentifierType
            sort order is primary
                is default
        CashTransactionIdentifier
            sort order is ByIdentifier
        Description
            allow update
        CashTransactionCategory
            allow update
            select is CashTransactionCategorySelect
        IdentifierAccount
            allow update
        CashAllocationCode
            select is ActiveCashAllocationCodes
        Active
            allow update
