CGFinalEFTNotificationPrint is a List
    title is "FinalNotePrintedList"
    single select
    implements HorizontalScrolling
        freeze first 4 columns
    no data message is "No_Electronic_Funds_Transfer_Transactions_Exist"
    Display Fields
        Company
            sort order is Set1
                Subtotal By
                    Company
        ReceivableInvoiceDetail.Customer
            label is "Customer"
            sort order is Set2
                is default
                Subtotal By
                    CrCustomer
        ReceivableInvoiceDetail.ReceivableInvoiceDetailType
            label is "Type"
        ReceivableInvoiceDetail.Invoice
            label is "Transaction"
        ReceivableInvoiceDetail.TransactionDate
        EFTNPrintDiscountDate
            label is "DiscountDate"
        ReceivableInvoiceDetail.DueDate
        ReceivableInvoiceDetail.CustomerPurchaseOrder
        ReceivableInvoiceDetail.TransientSignedBaseAmount
            label is "BaseOpenAmount"
        Company.Currency
            label is "BaseCurrency"
        group label is "ProcessingCompanyCurrency_(<EFTNotificationPrintResult.ProcessingCompany.Currency>)"
            EFTNPrintInvoiceAmount
                label is "Amount"
                total
            EFTNPrintDiscountAmount
                label is "DiscountAmount"
                total
            EFTNPrintNetAmount
                label is "NetAmount"
                total
