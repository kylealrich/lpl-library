PurchaseOrderImportList is a List
    is primary
    title is "PurchaseOrderInterfaceMaintenance"
    initially display search
    no data message is "Add_a_new_Purchase_Order_Interface"
        icon is icon.icon-empty-new-project-new
        button of "Create"
            action is Create
    Display Fields
        RunGroup
            allow update
            sort order is ByRunGroup
                is default
        Company
            allow update
        PurchaseOrderImport
            label is "InterfacedPurchaseOrder"
        PurchaseOrder
            link is PartialPurchaseOrder
        POCode
        PurchaseOrderImportRecordType
        Vendor
            allow update
        Buyer
            allow update
        group label is "Error"
            ErrorLevel
                label is "Level"
            ErrorMessage
                label is "Message"
    Actions
        action is InterfacePurchaseOrders
            action icon is submit
        action is MassDelete
            action icon is delete
            mouse over text is "MassDelete"
        navigation is ViewFactSheet
            action icon is preview
        navigation is ViewUploadSpreadsheets
            action icon is preview
            label is "ViewUploadSpreadsheets"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is Update
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is search
            label is "_"
