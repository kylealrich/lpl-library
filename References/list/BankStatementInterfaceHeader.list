BankStatementInterfaceHeaderList is a List
    is primary
    title is "BankStatementInterface"
    initially display search
    no data message is "There_has_been_no_Bank_Statement_Interface_activity"
        icon is icon.icon-empty-no-tasks
    Display Fields
        BankStatementInterfaceHeader
            raise yellow alert when (StatementType.Mixed)
                mouse over text is "Mixed_-_\\PreviousDay/\\Intraday"
            raise blue alert when (StatementType.Intraday)
                mouse over text is "Intraday"
            sort order is primary
                is default descending
        FileFormat
        Filename
        ImportDate
        ExpectedNumberOfRecords
        ActualNumberOfRecords
        Status
            link is ValidationErrors
            raise red alert when (ImportFailed)
                mouse over text is "ImportProcessFailed;SeeErrorTabForDetails"
            raise yellow alert when (HasSkippedTransactions)
                mouse over text is "SomeInterfaceTransactionsWereSkipped"
