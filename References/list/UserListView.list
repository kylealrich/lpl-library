AdminUserListView is a List
    title is "ManageSaveSearches"
    initially display search
    Display Fields
        Actor
            sort order is ActorFolderSet
                is default
        BusinessView
            label is "BusinessView"
            sort order is ByViewListActor
        BusinessList
            label is "List"
            sort order is ByListViewActor
        Status
            raise red alert when (Status.Invalid)
        IsDefault
        update stamp
            label is "LastChanged"
        UserFolder.Name
            label is "UserFolder"
    restrict action MoveToTop
    restrict action MoveToBottom
    restrict action MoveUp
    restrict action MoveDown
    Actions
        action is Delete
        action is Purge
        action is DeleteUserListViewsForActor
            label is "DeleteAllForActor"
        action is PurgeUserListViewsForActor
            label is "PurgeAllForActor"
        action is PurgeInvalidUserListViews
            label is "PurgeInvalid"
        action is ReassignOwner
    Detail Fields On Side
        TName
        update stamp.actor
            label is "LastChangedBy"
        create stamp.actor
            label is "CreatedBy"
        create stamp
            label is "Created"
        TDescription
