AttachedTierQualifierContracts is a List
    title is "TierQualifierContracts"
    initially display search
    Display Fields
        SelectedContractNumber
            link is QualifierContractLink
            raise yellow alert when (ContractHasNoQualifierItems)
                mouse over text is "ContractHasNoQualifierItems"
        ContractRel.Name
        ContractRel.Supplier.SupplierName
            label is "SupplierName"
        QualifiedOrCompetitiveContract
            allow update
        ContractRel.IsManufacturerContract
    Actions
        action is Create
            label is "CreateQualifierContracts"
        navigation is QualifierItemsForAContract
            label is "ViewQualifierItems"
            hide when invalid
        action is SelectAllRemainingItemsForTierQualifier
            hide when invalid
        action is open
            label is "_"
