ProcurementCardStatementInputList is a List
    is primary
    title is "ProcurementCardStatementInput"
    initially display search
    no data message is "Nothing_to_see_here"
        icon is icon.icon-empty-no-data-new
    Display Fields
        ProcurementGroup
        RecordType
        ProcurementCardProgram
        ProcurementCardStatementImport
            label is "Statement"
        Company
        ProcurementCardNumber
        TransactionNumber
            raise red alert when (DuplicateTransactionInput)
                mouse over text is "DuplicateTransactionExistsInStatementInput"
        TransactionDate
            raise red alert when (DuplicateTransactionInterface)
                mouse over text is "DuplicateTransactionExistsInStatementInterface"
        ItemCharge
        DistributionAmount
    Actions
        action is InterfaceProcurementCardStatements
        action is open
            label is "_"
        action is search
            action icon is filter
            label is "_"
        action is refresh
            label is "_"
