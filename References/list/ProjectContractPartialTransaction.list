AllProjectContractPartialBilledTransactions is a List
    is report for Projects
        category is "InvoicingAndRevenue"
    title is "PartiallyBilledTransactions"
    initially display search
    implements HorizontalScrolling
        freeze first 2 columns
    no data message is "No_partially_billed_transactions_exist_for_invoicing."
        icon is icon.icon-empty-no-data
    Display Fields
        GLTransactionDetail.FinanceCodeBlock.Project
            label is "<FinanceEnterpriseGroup.ProjectLabel>"
        GLTransactionDetail.TransactionDate
            label is "ExpenseTransactionDate"
        GLTransactionDetail.ReportCurrencyAmount.ProjectAmount.EnteredCurrencyAmount
            label is "ExpenseAmount"
            total
        TotalBilledAmount
            total
        TotalFees
            total
        GLTransactionDetail.FinanceCodeBlock.GeneralLedgerChartAccount
            label is "Expense<FinanceEnterpriseGroup.AccountLabel>"
        GLTransactionDetail.FinanceCodeBlock.ToAccountingEntity
            label is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
        GLTransactionDetail.FinanceCodeBlock.AccountingUnit
            label is "<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>"
        GLTransactionDetail.PostingDate
            label is "ExpensePostingDate"
        Hold
    Instance Selection
        where (RecordType.Bill)
    open action is restricted
    Actions
        action is RemoveHold
            hide when invalid
        action is PutOnHold
            hide when invalid
