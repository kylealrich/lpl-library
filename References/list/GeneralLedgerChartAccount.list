AccountAnalysisBudgetList is a List
    title is "<ReportingChart>_ChartIn<AccountActualCurrency>"
    implements TreeView
        has children when (HasChildren)
    implements HorizontalScrolling
        freeze first 2 columns
    first page header is AccountAnalysisPDFDisplay
    Display Fields
        DisplayAccount
            label is "Display<FinanceEnterpriseGroup.AccountLabel>"
        AccountDescription
            label is "Description"
            raise yellow alert when (DerivedReportingChartFutureChanges)
                mouse over text is "FutureChangesExistForReportingChart"
            raise blue alert when (IsASystemAccount)
                mouse over text is "IsASystem<FinanceEnterpriseGroup.AccountLabel>"
        DisplayChartSection
            label is "ChartSection"
        DerivedEndingBudgetBalance
            label is "<DerivedPeriodBudgetAmountLabel>"
            link is TotalsForCubeAmount
            column visible when (DisplayTotalLink)
        DerivedEndingBudgetBalance
            label is "<DerivedPeriodBudgetAmountLTDLabel>"
            column visible when (HideTotalLink)
        DerivedEndingActualBalance
            label is "<DerivedPeriodActualLabel>"
            link is TotalsForCubeAmountTwoNav
            column visible when (DisplayTotalLink)
        DerivedEndingActualBalance
            label is "<DerivedPeriodActualLTDLabel>"
            column visible when (HideTotalLink)
        DerivedEndingBalanceEncumbrance
            label is "<DerivedPeriodEncumbranceAmountLabel>"
            link is TotalsForCubeEncumbranceNav
            column visible when (DisplayTotalLink)
        DerivedEndingBalanceEncumbrance
            label is "<DerivedPeriodEncumbranceAmountLTDLabel>"
            column visible when (HideTotalLink)
        DerivedEndingBalanceCommitment
            label is "<DerivedPeriodCommitmentAmountLabel>"
            link is TotalsForCubeCommitmentNav
            column visible when (DisplayTotalLink)
        DerivedEndingBalanceCommitment
            label is "<DerivedPeriodCommitmentAmountLTDLabel>"
            column visible when (HideTotalLink)
        AccountBudgetVariance
            label is "VarianceAmount"
        AccountBudgetVariancePercent
            label is "RemainingPercent"
        AccountAnalysisSettings
            column visible when (false)
        DerivedStartDateJulian
            column visible when (false)
        DerivedEndDateJulian
            column visible when (false)
    Instance Selection
        where (not SkipAnalysisBudgetList)
    form is SummaryChartAccountComposite
    restrict action Create
    restrict action Update
    restrict action Delete
    restrict action Move
    restrict action CreateChildWithin
    restrict action RemoveFromChart
    restrict action AddPostingAccountToChart
    restrict action AddAccountToCharts
    restrict action ResequencePeers
    restrict action RemoveBranch
    restrict action AddPostingToSummary
    restrict action SetIncludeBudgetEditOnPostingAccounts
    restrict action RebuildDisplayAccount
    restrict action ResequenceAllDisplayOrders
