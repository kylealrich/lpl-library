AssetInterfaceList is a List
    is primary
    title is "AssetInterfaceRecords"
    search form is AssetInterfaceRecordSearch
        use as filter
    no data message is "No_Interface_Records_Exists"
    Display Fields
        Company
        AssetTemplate
            allow update
            select is ValidTemplatesRel.AssetTemplates
        Combine
            allow update
            sort order is ByTemplateCombine
        InterfaceSystem
        HoldAsset
            allow update
        ItemQuantity
            label is "Quantity"
        TransactionItemCost
            label is "Cost"
            raise red alert when (RebalanceError)
                mouse over text is "RebalanceTotal<TransactionItemCostSum>DoesNotEqualOriginalCost<OriginalCost>"
        TransactionItemTax
            label is "Tax"
    restrict action Create
    restrict action SplitAndRebalance
    restrict action TemplateSplitAndRebalance
