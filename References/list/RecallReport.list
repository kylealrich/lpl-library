RecallReportList is a List
    is primary
    is report for RecallCoordinator,RecallAdministrator,ApplicationAdministrator,ItemLifecycleManagement,SupplyManagementProcessor,SupplyManagementViewer
        category is "Analysis"
    title is "ItemLifecycleManagementReports"
    single select
    no data message is "No_Recall_Reports_have_been_created:_Select_Create_to_start_a_Report"
        icon is icon.icon-empty-no-tasks
    Display Fields
        Name
            sort order is ByName
        Frequency
            allow update
        NumberOfPeriods
            allow update
        IncludeCurrent
            allow update
        EndDate
            allow update
        ReportFromDate
            label is "FromDate"
        ReportThruDate
            label is "ThruDate"
    Actions
        action is open
            label is "ViewReport"
        action is Create
        action is search
            label is "_"
        action is Delete
    Detail Sections
        Periods
            list is RecallReportPeriod set.primary
