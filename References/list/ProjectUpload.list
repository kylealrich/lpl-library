ProjectUploadForStructure is a List
    title is "Uploads"
    initially display search
    implements HorizontalScrolling
        freeze first 4 columns
    no data message is "No_records_exist_for_upload."
        icon is icon.icon-empty-no-data
    Display Fields
        FinanceEnterpriseGroup
        ProjectStructure
            label is "<FinanceEnterpriseGroup.ProjectLabel>Structure"
        ProjectUpload.RunGroup
            label is "RunGroup"
            sort order is primary
        ProjectUpload.Sequence
            label is "Sequence"
            raise red alert when (ErrorTransaction)
                mouse over text is "<ErrorMessage>"
        ParentProject
            label is "Parent<FinanceEnterpriseGroup.ProjectLabel>"
        Project
            label is "<FinanceEnterpriseGroup.ProjectLabel>"
            sort order is ByDimension
        Description
        RecordType
        Currency
        ProjectStatus
        ProjectStatus.Description
            label is "Description"
        ContractID
        ContractType
        Contract
            column visible when (InEnterpriseStructure)
        ContractAmount
            column visible when (InEnterpriseStructure)
        ContractCurrency
            label is "Currency"
            column visible when (InEnterpriseStructure)
        ContractBillingMethod
            label is "BillingMethod"
            column visible when (InEnterpriseStructure)
        ContractBillingRateTable
            label is "BillingRateTable"
            column visible when (InEnterpriseStructure)
        ContractMarkupPercent
            label is "MarkupPercent"
            column visible when (InEnterpriseStructure)
        ContractRevenueRecognition
            label is "RevenueRecognition"
            column visible when (InEnterpriseStructure)
        ContractRevenueMethod
            label is "RevenueMethod"
        group label is "DateRange"
            ProjectDateRange.BeginDate
                no label
                column visible when (InEnterpriseStructure)
            ProjectDateRange.EndDate
                no label
                column visible when (InEnterpriseStructure)
        AddressCode
        AddressCode.Description
            label is "Description"
        PersonResponsible.PresentationNameSnapshot
            label is "PersonResponsible"
            show up to 30 characters
            column visible when (InEnterpriseStructure)
        ProjectTeam
            column visible when (FinanceEnterpriseGroup.ProjectInvoicing)
        ErrorMessage
