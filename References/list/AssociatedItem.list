AssociatedItemDashboard is a List
    title is "AssociatedItems"
    no data message is "Doesn't_look_like_there's_any"
        icon is icon.icon-empty-no-orders-new
    Display Fields
        AssociatedItem.AssociatedItemSubType
        AssociatedItem.RelatedItem
        Contract
        ContractLine
            raise red alert when (ContractLineClosed)
                mouse over text is "<ContractLineClosedMessage>"
            raise yellow alert when (ContractLineNotAvailable)
                mouse over text is "<ContractNotAvailableMessage>"
        group label is "Location"
            AssociatedItem.RelatedCompany
                label is "Company"
            AssociatedItem.RelatedLocation
                label is "InventoryLocation"
        EffectiveDate
            raise yellow alert when (InvalidSpecialItemEffectiveDate)
                mouse over text is "<EffectiveDateMessage>"
        ExpirationDate
            raise yellow alert when (InvalidSpecialItemExpirationDate)
                mouse over text is "<ExpirationDateMessage>"
        Priority
            sort order is AllTypesByPriority
                is default
    restrict action CreateRequisitionLine
    restrict action Create
    restrict action CreateSubstituteSpecialItem
