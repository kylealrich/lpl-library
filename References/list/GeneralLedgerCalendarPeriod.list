AccountAnalysisAmountsList is a List
    title is "<GeneralLedgerChartAccountRel.GeneralLedgerChartAccount>-PeriodBalances"
    called out actions only
    Display Fields
        DisplayPeriod
        SummaryAmount
    Actions
        action is charts
            action icon is icon.chart
    Charts
        SummaryAmountBarChart is a BarChart
            title is "BarChart"
            chart elements
            is default
            show legend
            element name is "<DisplayPeriod>"
            element value is SummaryAmount
                title is "Activity"
        SummaryAmountPieChart is a PieChart
            title is "PieChart"
            chart elements
            show legend
            show as donut
            element name is "<DisplayPeriod>"
            element value is SummaryAmount
                title is "Activity"
