ADBTransactionList is a List
    is primary
    title is "DailyTransaction"
    Display Fields
        AccountingEntity
            label is "<FinanceEnterpriseGroup.AccountingEntityLabel>"
            sort order is ByStatusEntityAccount
                is default
        AccountingUnit
            label is "<FinanceEnterpriseGroup.AccountingUnitLabel>"
            column visible when (FinanceEnterpriseGroup.ADBSelection.AccountAndAccountingUnit)
        FinanceDimension1
            label is "<FinanceEnterpriseGroup.FinanceDimension1Label>"
            column visible when (FinanceEnterpriseGroup.ADBSelection.AccountAndDimension1)
        ADBTransaction.GeneralLedgerChartAccount
            label is "<FinanceEnterpriseGroup.AccountLabel>"
        ADBTransaction.PostingDate
            label is "PostDate"
        ADBTransaction.TransactionDate
            label is "TransactionDate"
        ADBTransaction.RunDate
            label is "RunDate"
        Status
        FunctionalAmount
