ActivePurchaseFromLocations is a List
    title is "PurchaseFromLocations"
    is default select
    Display Fields
        PurchaseFromLocation
            label is "LocationCode"
        VendorLocationRel.VendorName
            label is "Name"
            disable translation filtering
        VendorLocationRel.VendorCity
            label is "City"
        VendorLocationRel.VendorStateProvince
            label is "State\\,\\Province"
        VendorLocationRel.VendorPostalCode
            label is "PostalCode"
    Instance Selection
        where (Active)
