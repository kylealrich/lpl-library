InlineRemits is a List
    title is "InterfaceRemittances"
    implements InlineCreate
    no data message is "No_Payment_Remittances_Exist"
    Display Fields
        group label is "Remit"
            ReceivablePaymentRemitImport.LastRemittanceSequence
                label is "Sequence"
                allow update
            TransType
                label is "Type"
                allow update
            TransRemNbr
                label is "Number"
                allow update
        AppldAmt
            label is "AppliedAmount"
            allow update
