ApplicationCloseList is a List
    title is "Distributions"
    initially display search
    suppress sub-totals
    no data message is "No_Distributions_Exist"
    Display Fields
        ReceivableCompanyDataGroup.TransactionCompany
        ReceivableCompanyGroup.GlCompany
            label is "GlobalLedgerCompany"
        ReceivableCompanyDataGroup.Customer
        TransType
            label is "TransactionType"
        Invoice
        GeneralLedgerAccount
        Description
        DistributionAmount.AssignToCurrency
            label is "_"
            column visible when (false)
        DistributionAmount.CurrencyAmount
            total
        DistributionAmount.FunctionalAmount.EnteredCurrencyAmount
            label is "FunctionalAmount"
            total
        Origin
        AuditSummaryIndicator
        ProgramSequence
    restrict action Create
    restrict action CreateInvoiceDistribution
    restrict action CreatePaymentDistribution
    restrict action CreateManualInvoiceDistribution
    restrict action Update
    restrict action Delete
    restrict action JournalizeDistributions
    restrict action NewJournalizeDistributions
    restrict action ReturnToMakerCreate
    Actions
        action is open
            label is "_"
        action is search
            label is "_"
        action is refresh
            label is "_"
