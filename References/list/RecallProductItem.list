ItemsList is a List
    title is "Items"
    Display Fields
        Item
            raise yellow alert when (ItemOnAnotherNotice)
                mouse over text is "<DuplicateItemNoticeMessage>"
        VendorItem
        ManufacturerInformation.Manufacturer
        ManufacturerInformation.ManufacturerNumber
        NDC
        GTIN
        HibccUpn
        DisplayDescription
            label is "Description"
        DisplayUom
            label is "StockUom"
            column visible when (RecallProduct.HasItemMaster)
