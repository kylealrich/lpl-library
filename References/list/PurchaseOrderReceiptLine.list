InterfaceListing is a List
    title is "ReceiptLines"
    Display Fields
        PurchaseOrderReceipt
            label is "Receiver"
            link is ViewReceiver
        PurchaseOrderLine.LineNumber
            label is "Line"
        PurchaseOrderReceipt.PurchaseOrder
            label is "PurchaseOrder"
            link is ViewPO
        Item
        Description
            disable translation filtering
        ShipToLocation
            label is "Location"
        EnteredReceivedQuantity
            label is "Quantity"
        VendorBuyUOM
            label is "U\\O\\M"
        PurchaseOrderReceipt.ReceivedDate
            label is "DateReceived"
        Status
    restrict action PopulateMissingUOM
    restrict action MobileUpdate
    restrict action MobileUpdateReceiptLine
