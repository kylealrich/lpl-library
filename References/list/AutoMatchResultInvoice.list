AllInvoices is a List
    title is "Invoices"
    Display Fields
        Invoice
            link is PayablesInvoicePrimary
            sort order is ByCompanyInvoiceOnly
                is default
                Subtotal By
                    Company
        MatchStatus
            label is "ReportedMatchStatus"
            raise blue alert when (MatchStatusHasChanged)
                mouse over text is "MatchStatusHasBeenUpdatedTo<PayablesInvoice.MatchStatus>"
        PayablesInvoice.MatchStatus
            label is "CurrentMatchStatus"
        PurchaseOrder
            link is PODashboard
        ServiceContract
        Buyer
            sort order is ByBuyer
                Subtotal By
                    Buyer
        Operator
            sort order is ByOperator
                Subtotal By
                    Operator
        Vendor
            sort order is ByVendor
                Subtotal By
                    Vendor
        VendorName
            sort order is ByVendorName
                Subtotal By
                    VendorName
        DueDate
        InvoiceAmount
            total
        ErrorMessage
    open action is restricted
    Actions
        action is search
            label is "_"
        action is refresh
            label is "_"
