AllFederalFinancialReportsListList is a List
    title is "FederalFinancialReports"
    initially display search
    implements HorizontalScrolling
        freeze first column
    no data message is "No_federal_financial_reports_exist."
        icon is icon.icon-empty-no-data
    Display Fields
        FinanceDimension2
            label is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
            link is ViewFinanceDimension2
        FinanceDimension2.Description
            label is "Description"
        SequenceNumber
            label is "ReportSequence"
        Description
            label is "ReportDescription"
            allow update
        ReportType
        ReportingThruDate
        FinalReport
            label is "ReportStatus"
        ProjectContract.DisplayContract
            label is "<FinanceEnterpriseGroup.ProjectLabel>Contract"
        ProjectContract.Project.Description
            label is "Description"
        button of "ViewReport"
            visible when (GeneratedReportExists)
            view GeneratedReport.File
        button of "ViewReport"
            visible when (GeneratedIDMReportExists)
            link is external "<DerivedIDMReportLink>"
            display as link
        FinanceDimension2.TransientContextDimension
            column visible when (false)
        FinanceDimension2.FinanceDimension2Structure
            column visible when (false)
