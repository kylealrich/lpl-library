CommitmentEncumbranceVarianceResults is a List
    is primary
    title is "CommitmentsAndEncumbrancesVarianceReport"
    single select
    initially display search
    implements HorizontalScrolling
        freeze first 5 columns
    orientation is landscape
    font offset is -4
    no data message is "There_Are_No_Commitment_Or_Encumbrance_Variances"
        icon is icon.icon-empty-new-project
        button of "RunReport"
            action is ExecuteVarianceReport
    Display Fields
        Scenario
            column width is 14
        AccountingEntity
            label is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
        System
            column width is 16
        GeneralLedgerChartAccount
            label is "<actor.context.FinanceEnterpriseGroup.AccountLabel>"
            column width is 16
        AccountingUnit
            label is "<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>"
            column visible when (AccountingUnitUsed)
            column width is 16
        Project
            label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
            column visible when (ProjectUsed)
            column width is 16
        FinanceDimension1
            label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension1Label>"
            column visible when (Dimension1Used)
            column width is 16
        CurrencyCode
            column width is 16
        EntityYearPeriod.Date
            label is "PeriodDate"
            column width is 20
        GLTTransactionAmount
        GLCommitTransactionAmount
        GLCommitTransactionVariance
            label is "Variance"
    open action is restricted
    Actions
        action is ExecuteVarianceReport
            label is "RunReport"
        action is search
            label is "_"
        action is refresh
            label is "_"
    Detail Sections
        DetailSection
            title is "GLCommitDetails"
            list is GLCommitRel.GLTotalGLCommitList
