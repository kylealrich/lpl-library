InvoicedNotReceivedLines is a List
    title is "<InvoicedNotReceivedResult.DerivedFormTitle>"
    initially display search
    search form is InvoicedNotReceivedSearchForm
        use as filter
    Display Fields
        Company
            sort order is ByCompanyInvoiceOnly
        Vendor
            sort order is ByVendor
                is default
        Invoice
            link is PayablesInvoiceNav
            sort order is ByInvoice
        Buyer
            sort order is ByBuyer
        DueDate
        InvoiceDate
        InvoiceType
        PurchaseOrder
            link is PurchaseOrderInformation
            sort order is ByCompanyPurchaseOrder
        PurchaseOrderLine.LineNumber
            label is "Line"
            link is PurchaseOrderLineInformation
        Item
            label is "Item"
            sort order is ByCompanyItemTypeItemSearch
        PayablesInvoiceDetail.ItemDescription
        ItemType
            sort order is ByCompanyItemTypeItemSearch
        PayablesInvoiceDetail.DerivedInrUom
            label is "UOM"
        PurchaseOrderLine.ItemGTIN
        INRQuantity
            label is "INRQuantity"
        PayablesInvoiceDetail.MatchUnitCost
            label is "UnitCost"
        DerivedExtendedINRAmount
            label is "ExtendedAmount"
        PayablesInvoiceDetail.DerivedAccount
            label is "DistributionAccount"
        PayablesInvoiceDetail.Location
            label is "ShipTo"
    Instance Selection
        where (PayablesInvoice.IsValidForActorContext
        and   IsSelectedInvoiceType
        and   WithinDueDateRange)
    open action is restricted
    restrict action Create
    restrict action Update
    restrict action Delete
    Detail Sections
        InvoiceNotRecievedDetails
            visible when (not PayablesInvoiceDetail.InventoryTypeExists)
            list is PayablesInvoiceDetail.DetailInvoiceDistributionsRel.InvoiceNotRecievedDistributionsDetailList
