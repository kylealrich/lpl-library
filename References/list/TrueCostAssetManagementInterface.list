TrueCostAssetManagementInterfaceList is a List
    is primary
    title is "AssetTransactions"
    Display Fields
        TrueCostAssetManagementInterface.RunGroup
            raise red alert when (HasError)
                mouse over text is "<ErrorDescription>"
        TrueCostAssetManagementInterface.Sequence
            label is "SequenceNumber"
        TrueCostConfiguration
            label is "True\\CostConfiguration"
        AccountingEntity
        AssetAccountingUnitGroup
            label is "AssetDimensionGroup"
        Asset
        CurrentPeriodDepreciation
        YearEndDate
        Period
        ErrorDescription
    Actions
        action is GenerateResourceCapacityRates
            mouse over text is "GenerateResourceCapacityRates"
        action is CopyRunGroup
            mouse over text is "CopyRunGroup"
        action is DeleteAllTransactionsForRunGroup
            label is "DeleteTransactions"
            mouse over text is "DeleteAllTransactionsForRunGroup"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is open
            label is "_"
            mouse over text is "Open"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is search
            label is "_"
            mouse over text is "Search"
