AccountLevelCreatedFromBankStatementLine is a List
    title is "UnreleasedCashLedgerTransactionCreatedFromBankStatements"
    initially display search
    no data message is "There_are_currently_no_Unreleased_Cash_Ledger_Transaction_Created_From_Bank_Statements"
        icon is icon.icon-empty-no-tasks
    Display Fields
        CashManagementAccount
            sort order is ByCashManagementAccount
                is default
        CreatedFromBankStatement
            label is "StatementNumber"
        CreatedFromBankStatementLine.LineNumber
            label is "StatementLine"
        BankTransactionCode
        TransactionNumber
            label is "Number"
            raise red alert when (BudgetEditErrorExists)
                mouse over text is "<DerivedFirstErrorMessage>"
        Status
        Company
        IssuedBankAmount
            label is "IssueAmount"
            raise red alert when (IsUnreleasedOutOfbalance)
                mouse over text is "TransactionOutOfBalanceBy:<BalanceAmount>"
            raise yellow alert when (DetailsOutOfBalance)
                mouse over text is "DetailsAmountDoesNotMatchWithIssuedBankAmount"
            raise green alert when (HasTransactionDetails)
                mouse over text is "HasTransactionDetails"
        IssueDate
            label is "TransactionDate"
        Description
    Instance Selection
        where (CreatedFromBankStatement entered
        and   (Status.Unreleased
            or    Status.PendingApproval))
    restrict action Unreleased.Create
    restrict action Open.Reconcile
    restrict action Open.StopPayment
    restrict action Reconciled.Reopen
    restrict action StopPayment.Reopen
    restrict action StaleDated.Reopen
    restrict action VoidStopPayUpdate
    restrict action MassUpdate
    Actions
        action is Unreleased.Release
            label is "Release"
            hide when invalid
        action is Unreleased.SubmitForApproval
            label is "SubmitForApproval"
            hide when invalid
        action is ReleaseAmendedDistributions
            label is "ReleaseAmendedDistributions"
            hide when invalid
        action is search
            label is "_"
        action is refresh
            label is "_"
