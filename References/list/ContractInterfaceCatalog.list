ContractInterfaceCatalogList is a List
    is primary
    propagate as a drill
    title is "ContractInterfaceCatalogs"
    single select
    no data message is "Start_a_new_Contract_Interface_Catalog"
        icon is icon.icon-empty-new-project-new
        button of "Create"
            action is Create
    Display Fields
        ContractGroup
        WorkingContractID
        Name
        RecordType
            sort order is ByRecordType
                is default
        Supplier
        VendorItem
        ContractImportTier
    form is ContractInterfaceCatalogForm
    Actions
        action is CreateImportRecords
        action is refresh
            label is "_"
        action is search
            label is "_"
