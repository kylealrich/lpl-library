MyReportDataList is a List
    title is "ReportData"
    Display Fields
        Name
            sort order is ByActorName
        MimeType
            sort order is ByActorMimeType
        create stamp
            sort order is ByActorTime
                is default
        button of "Data"
            visible when (HasGroupFieldData)
            view GroupFieldDataBinary
    Instance Selection
        where (create stamp.actor = actor)
    Detail Sections
        ReportDocuments
            title is "Reports"
            list is ReportDocument set.MyReportDocumentsList
