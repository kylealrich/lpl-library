AccountantFundingSourceDeliverableList is a List
    is report for Projects
        category is "InvoicingAndRevenue"
    propagate as a drill
    title is "Deliverables"
    initially display search
    implements InlineCreate
    no data message is "No_Deliverables_exist."
        icon is icon.icon-empty-no-data
    Display Fields
        FinanceDimension2
            label is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
            link is ViewFinanceDimension2
        FinanceDimension2.FundingType
            label is "Type"
        Deliverable
            raise red alert when (DueDateHasPast)
                mouse over text is "DeliverableDueDateHasPast"
            raise yellow alert when (DueWithin30Days)
                mouse over text is "DeliverableDueDateIsApproaching"
            sort order is ByDueDate
                is default
        Description
            allow update
            raise blue alert when (HasDeliverableComments)
                mouse over text is "CommentsExists"
        PersonResponsible
            allow update
        PersonResponsible.FirstLastName
            label is "Name"
        InvoiceDate
            label is "DueDate"
            allow update
            raise blue alert when (DueWithin30Days)
                mouse over text is "DueWithin30Days"
        DeliveredDate
            allow update
    Actions
        action is SendDueDateNotification
        navigation is ProjectFundingSourceDeliverableComment
            action icon is preview
            label is "ViewComments"
            hide when invalid
            valid when (HasDeliverableComments)
        action is CreateComments
            action icon is create
        action is Create
            action icon is create
        action is open
            action icon is open
        action is save
            action icon is save
        action is Delete
            action icon is delete
