ConversionInvoices is a List
    title is "ConversionInvoices"
    initially display search
    no data message is "No_Conversion_Invoices_Exist"
    Display Fields
        RunGroup
        Company
        Customer
        ConversionInvoiceType
            label is "Type"
        Invoice
        InvoiceAmount.CurrencyAmount
            label is "Amount"
        ErrorFile
        ErrorMessage
    Instance Selection
        where (IsValidForActorContext
        and   ConversionInvoice)
    form is ConversionObligation
    create action is CreateConversionInterface
    restrict action DeleteAllInterfaceInvoicesForRunGroup
    restrict action InterfaceInvoices
    Actions
        action is ConvertInvoices
            action icon is transfer
            hide when invalid
        action is ResetErrorMessage
            action icon is undo
            hide when invalid
        action is CreateConversionInterface
            action icon is create
            label is "_"
        action is open
            label is "_"
        action is Delete
            label is "_"
        action is search
            label is "_"
        action is refresh
            label is "_"
