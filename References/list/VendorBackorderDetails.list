BackorderList is a List
    is primary
    title is "VendorBackorderDetails"
    initially display search
    Display Fields
        RecordType
            sort order is ByRecordType
                is default
        ProcurementGroup
            sort order is ByProcurementGroup
        PurchasingCompany
        PurchaseOrder
        PurchaseOrderLine
            link is PurchaseOrderLineNav
        Contract
            raise red alert when (IsContractExpired)
                mouse over text is "<ContractExpiredMessage>"
        ContractLine
            link is ContractLineNav
        VendorItem
            link is VendorItemNav
            sort order is ByVendorItem
        Item
            sort order is ByItem
        Vendor
        BackorderType
            allow update
        BackorderQuantity
        StartDate
            allow update
        EstimatedAvailabilityDate
            allow update
        Status
        AcknowledgementType
        AcknowledgementDate
    open action is restricted
    Actions
        action is CreateVendorItemBackorder
            action icon is create
        action is CreateSpecialItemBackorder
            action icon is create
        action is CloseBackorder
            action icon is finalize
            label is "Close"
        action is save
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
        action is search
            label is "_"
        action is refresh
            label is "_"
