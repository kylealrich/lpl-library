EDIInvoiceMatchInterfaceInputList is a List
    is primary
    title is "EDIInvoiceMatchInterfaceInput"
    always display search
    no data message is "No_EDI_interface_invoices_to_display"
        icon is icon.icon-empty-no-orders-new
    Display Fields
        RunGroup
            sort order is ByRunGroupUniqueID
                is default
        RecordType
        Company
        DerivedVendor
            label is "Vendor"
            raise red alert when (HasDuplicateEDIInvoice)
                mouse over text is "DuplicateInvoiceExistsInEDIInterface"
        Invoice
            raise red alert when (HasDuplicateMatchInterfaceInvoice)
                mouse over text is "DuplicateInvoiceExistsInMatchInvoiceInterface"
        LineNumber
            raise red alert when (DuplicateLineExists)
                mouse over text is "DuplicateLineRecordExists"
        SeqNbr
        Item
        VendorItem
        AddOnCharge
            raise red alert when (DuplicateAOCExists)
                mouse over text is "DuplicateAddOnChargeRecordExists"
        DerivedPurchaseOrder
            label is "PurchaseOrder"
        HSNSACCode
            allow update
            column visible when (IsHSNSACCodeEnabled)
    Actions
        action is InterfaceInvoices
            action icon is process
        action is DeleteAllTransactionsForRunGroup
            action icon is reject
        action is open
            label is "_"
        action is search
            label is "_"
        action is refresh
            label is "_"
