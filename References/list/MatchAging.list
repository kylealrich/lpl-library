MatchAgingCardList is a List
    title is "MatchAgingSummary"
    show 1 lines
    single select
    no data message is "Match_Aging_Summary_Does_Not_Exist"
        description is "Run_'Create_Summary'_toDefineAgingPeriodsAndGenerateSummaryData."
        icon is icon.icon-empty-no-analytics
        button of "CreateSummary"
            valid when (not MatchAgingExists)
            action is CreateMatchAgingSummary
    card view is MatchAgingCard
    Display Fields
    Detail Sections
        YearMonthAgingWithPercents
            title is "ByMonth"
            visible when (Status.Complete)
            list is YearMonthSummaryRel.YearMonthAgingWithPercent
        VendorAgingWithPercents
            title is "ByVendor"
            visible when (Status.Complete)
            list is VendorSummaryRel.VendorAgingWithPercent
        UnmatchedCard
            title is "Unmatched"
            visible when (Status.Complete)
            allow nested detail sections
            list is VendorUnmatchedSummaryRel.VendorUnmatchedCardList
