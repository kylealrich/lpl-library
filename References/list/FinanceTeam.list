BuildPayablesTeamList is a List
    title is "FinanceTeams"
    single select
    keyword search field is FinanceTeam
        label is "SearchTeams"
    search form is TeamSearch
        use as filter
    no data message is "Add_a_New_Finance_Team"
        icon is icon.icon-empty-no-tasks
        button of "Create"
            action is Create
    card view is TeamCardView
        row separator is horizontal rule
    Display Fields
    form is PayablesFinanceTeamForm
    restrict action Purge
    restrict action AddMember
    Actions
        action is Create
            action icon is create
            label is "_"
    Detail Sections
        Membership
            list is FinanceTeamMemberBySearchNameRel.BuildPayablesTeamList
                helper list is NotOnTeamRel.AddToTeamHelperList
                    action is AddToTeam
                        invoked.PrmFinanceTeam = FinanceTeam
