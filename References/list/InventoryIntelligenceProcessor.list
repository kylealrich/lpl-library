AccessRightsDashboard is a List
    title is "Processors"
    single select
    keyword search field is InventoryIntelligenceProcessor.Employee.Name.PreferredFirstAndLastName
        label is "Search_byName"
    search form is ProcessorSearchForm
        use as filter
    implements DashBoardView
    no data message is "ClickBelowToAdd_Access_Rights"
        icon is icon.icon-empty-new-project
        button of "Create"
            action is Create
    card view is ProcessorCardViewBasic
        row separator is horizontal rule
    Display Fields
    form is ProcessorCompositeView
    Actions
        action is Create
            action icon is create
            label is "Create"
    Detail Sections
        Profile
            triple width
            form is DetailSectionForm
            overview navigation is IIPNav
                label is "ViewProcessor"
        Locations
            triple width
            list is InventoryIntelligenceProcessorLocation set.ProcessorLocationList
