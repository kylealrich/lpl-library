BanksList is a List
    title is "Banks"
    no data message is "No_EFT_Banks_Exist"
        description is "To_\\CreateAn_\\EFT_\\Bank,ClickThe_'plus'_inTheTopRightOr_\\Create_\\EFT_\\BankBelow."
        icon is icon.icon-empty-no-data
        button of "CreateEFTBank"
            action is Create
    Display Fields
        BankType
            label is "Type"
        CustomerDraftBank.BOEEFTBank
            label is "Bank"
        CustomerDraftBank.BOEEFTBankAccount
            label is "Account"
        DefaultBank
            label is "Default"
            display as tag
                color of green when (DefaultBank)
                color of gray when (not DefaultBank)
    form is GroupBank
