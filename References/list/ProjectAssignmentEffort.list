AllProjectAssignmentEffortList is a List
    title is "EffortCertification"
    single select
    initially display search
    implements HorizontalScrolling
        freeze first column
    no data message is "No_projects_with_effort_certification."
        icon is icon.icon-empty-no-data
    Display Fields
        Project
            label is "<FinanceEnterpriseGroup.ProjectLabel>"
            sort order is primary
        Project.Description
            label is "Description"
            raise blue alert when (HasProjectAssignmentEffortComments)
                mouse over text is "CommentsExists"
        EffortEmployee
            raise red alert when (not Employee.ProjectEmployee.Active)
                mouse over text is "<FinanceEnterpriseGroup.ProjectLabel>EmployeeIsInactive"
            sort order is ByEmployeeAndPeriod
        Employee.Employee.PresentationNameSnapshot
            label is "Name"
        ProjectEffortPeriod
            label is "EffortPeriod"
        DateRange.Begin
        DateRange.End
        Project.CertificationRequired
        Certified
        CertifiedCommittedEffort
        group label is "AveragePercent"
            AveragePlannedPercent
                label is "Planned"
            AverageActualPercent
                label is "Actual"
        EffortPercent
    restrict action Update
    restrict action CertifyEffort
    restrict action UncertifyEffort
    restrict action PopulateEffortPercent
    restrict action MobilePopulateEffortPercent
