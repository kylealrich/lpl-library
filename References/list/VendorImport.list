VendorImportList is a List
    is primary
    title is "VendorInterfaceMaintenance"
    initially display search
    no data message is "There_are_currently_no_Vendor_Interface_Records"
        icon is icon.icon-empty-no-tasks
        button of "Create"
            action is CreateVendorImport
    Display Fields
        RunGroup
        VendorGroup
        VendorMappingTableInterfaceRel.Vendor
            label is "VendorMappingVendor"
        VendorImport.OldVendor
        NewVendor
        VendorImport.VendorLocation
        VendorName
        RecordInError
        ErrorMessage
    Actions
        action is VendorInterface
            action icon is process
        action is CreateVendorImport
            action icon is create
        action is CreateVendorLocationImport
            action icon is create
        action is DeleteAllTransactionsForRunGroup
            action icon is delete
        action is open
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
        action is search
            label is "_"
