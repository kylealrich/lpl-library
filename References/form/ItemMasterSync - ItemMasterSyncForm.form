ItemMasterSyncForm is a CompositeForm
    is primary
    title is "ItemMasterSynchronization"
    context form is ItemMasterContext
    Actions
        action is Create
            label is "_"
        action is Delete
            label is "_"
            hide when invalid
        action is save
            label is "_"
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is refresh
            label is "_"
    ItemInformation is a Panel
        form is ItemInformation
    ItemLocations is a Panel
        list is ItemLocationRel.ItemLocationsForAnItem
    VendorItems is a Panel
        list is VendorItemRel.VendorItemsForItem
    ContractLines is a Panel
        visible when (ContractsAreFound)
        list is ContractLinesRel.ItemMasterSyncContractLines
    ContractInterfaceLines is a Panel
        visible when (ContractInterfacesAreFound)
        list is ContractInterfaceLinesRel.ItemMasterSyncLines
    GlobalTradeItemNumbers is a Panel
        visible when (GTINsAreFound)
        list is GTINRel.GTINNumbersForAnItem
