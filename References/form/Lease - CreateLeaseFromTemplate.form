CreateLeaseFromTemplate is an ActionForm
    action is Unreleased.CreateLeaseFromTemplate
    Layout
        FinanceEnterpriseGroup
            label is "FinanceEnterpriseGroup"
        LeaseTemplate
            label is "LeaseTemplate"
        PrmLeaseName
            label is "Lease"
        PrmLeaseDescription
            label is "Description"
        PrmBeginDate
            label is "BeginDate"
        PrmEndDate
            label is "EndDate"
        PrmLeaseTerm
            label is "Term"
        visible when (LeaseTemplate.Lessor not entered)
            PrmLessor
                label is "Lessor"
                display as required
        visible when (LeaseTemplate.InterestRate not entered)
            PrmInterestRate
                label is "InterestRate"
                display as required
        visible when (LeaseTemplate.ExerciseTermination)
            PrmEarlyTerminationDate
                label is "EarlyTerminationDate"
