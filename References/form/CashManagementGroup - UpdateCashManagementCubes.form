UpdateCashManagementCubes is an ActionForm
    action is UpdateCashManagementCubes
    title is "UpdateCashManagementCubes"
    Layout
        single column
            text of "SelectTheAnalyticCubesYouWouldLikeToUpdate:"
                header2
                bold
        blank line
        three column
            column1
                CashPosition
                Forecasting
                BankServices
            column2
                visible when (CashPosition)
                    CashPositionOption
                        no label
                        display as horizontal radio buttons
                visible when (not CashPosition)
                    BlankField
                        no label
                        display as text
                visible when (Forecasting)
                    ForecastingOption
                        no label
                        display as horizontal radio buttons
                visible when (not Forecasting)
                    BlankField
                        no label
                        display as text
                visible when (BankServices)
                    BankServicesOption
                        no label
                        display as horizontal radio buttons
                visible when (not BankServices)
                    BlankField
                        no label
                        display as text
            column3
                paragraph
                    BankStatementTotalCubeRel.LastRefreshDate
                        display as text
                    BankStatementTotalCubeRel.LastReloadDate
                        display as text
                paragraph
                    ForecastingAnalyticCubeRel.LastRefreshDate
                        display as text
                    ForecastingAnalyticCubeRel.LastReloadDate
                        display as text
                paragraph
                    BankServicesAnalyticCubeRel.LastRefreshDate
                        display as text
                    BankServicesAnalyticCubeRel.LastReloadDate
                        display as text
        blank line
        single column
            text of "SelectRefreshInOrderToUpdateExistingCubeData.SelectReloadToDeleteExistingCubeDataAndRebuildData."
                italics
