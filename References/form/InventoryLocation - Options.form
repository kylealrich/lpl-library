Options is a Form
    Layout
        two column distributed
            column1
                header2 of "Accounts"
                VendorReturnSuspenseAccount
                    label is "ReturnsSuspense"
                    compact format
                InspectionHoldAccount
                    label is "InspectionHold"
                    compact format
                FreightAccount
                    label is "Freight"
                    compact format
                HandlingAccount
                    label is "Handling"
                    compact format
                ScrapAccount
                    label is "Scrap"
                    compact format
            column2
                header2 of "ExpirationsOfLots"
                InventoryLocationLotTracking
                visible when (not Company.ItemGroup.HideHealthcareInformation)
                    header2 of "Healthcare"
                    paragraph
                        HealthIndustryNumber
                        GlobalLocationNumber
                            display as text
        header2 of "Defaults"
        two column distributed
            column1
                paragraph
                    DisableOvershipmentMessages
                ProcessLevel
                    label is "POProcessLevel"
                POCode
                paragraph
                    DefaultDropshipPurchaseOrder
                        label is "DropshipPurchaseOrder"
                TaxCode
                    label is "SellTaxCode"
                PurchaseOrderTaxCode
                    label is "PurchasingTaxCode"
                ReceivingDays
                PricingGroup
                    select is ActivePricingGroupsForLocations
                paragraph
                    ERSCapable
                PurchaseOrderApprovalType
                UseGLCategoryIssueAccountingUnitAsDefault
                    label is "UseAccountGroupIssueAccountingUnit_as_default"
            column2
                TwoStepPurchaseOrderReceiving
                AllocationPurpose
                ReceivingAllocation
                PrintLabels
                    label is "PrintChargeLabels"
                OneSourceDocumentToOnePO
                    label is "SingleDocument_to_aPurchaseOrder"
                    show up to 30 characters
                paragraph
                    EAMLocation
                        label is "LocationType"
                        show up to 10 characters
                    RetailLocationType
                        show up to 10 characters
                paragraph
                    MSCMDelivery
                paragraph
                    PreferredBin
                        label is "DefaultPreferredBin"
                        show up to 10 characters
                    PreferredHoldBin
                        label is "DefaultPreferredHoldBin"
                        show up to 10 characters
                DirectPayCertificate
                TrackStockUOMAtItemLocation
                OrderUOMRounding
        two column distributed
            column1
                header2 of "Roles"
                Vendor
                PurchaseFromLocation
                    label is "PurchaseFrom"
                Customer
                CustomerShipTo
                    label is "ShipTo"
                ReplenishmentRequester
                    label is "Requester"
            column2
                header2 of "RequisitionOptions"
                paragraph
                    WarehouseProcessType
                        label is "WarehouseProcessType"
                    WarehouseProcessType.Description
                        no label
                        display as text
                ProcessStatus
                    label is "RunStatus"
                    display as text
        header2 of "ItemMasterSync"
        single column
            ItemSyncOption
