MPCardChargesSearch is a SearchForm
    title is "PCardChargesSearch"
    Layout
        single column
            ProcurementCardStatementTransaction.Company
                label is "Company"
            Vendor
            ProcurementCardUser
                label is "CardHolder"
            ProcurementCardStatementTransaction.TransactionNumber
                label is "Transaction"
            Status
            ItemCharge
                label is "ChargeAmount"
        two column distributed
            SearchDateRange.Begin
                label is "TransactionDateBegin"
            SearchDateRange.End
                label is "TransactionDateEnd"
