PayablesInvoiceHistoryForm is a CompositeForm
    is primary
    title is "ArchivedPayablesInvoice"
    show panel control on left
    Main is a Panel
        form is MainFormWithTemplate4
    Options is a Panel
        form is PrimaryOptionsForm
    Payment is a Panel
        form is PrimaryPaymentForm
    Distributions is a Panel
        title is "Distributions"
        visible when (not IsAPurchaseOrderInvoice)
        list is PayablesInvoiceDistributionHistory set.PayablesInvoiceDistributionHistoryList
    SpreadAddOnCharges is a Panel
        title is "SpreadAddOnCharges"
        visible when (IsAPurchaseOrderInvoice)
        list is PayablesInvoiceSpreadAddOnChargeHistoryRel.primary
    PaymentSchedule is a Panel
        title is "PaySchedule"
        list is PayablesInvoicePaymentHistoryRel.primary
