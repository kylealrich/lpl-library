NonCashLedgerTransactionForm is a CompositeForm
    is primary
    title is "NonCashLedgerTransaction"
    context form is NonCashLedgerTransactionContextForm
        background is theme color
    Actions
        action is ReleaseDistributions
            label is "ReleaseDistributions"
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is save
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    Detail is a Panel
        form is NonCashLedgerTransactionDetail
    ReconciliationVarianceDistributions is a Panel
        list is NonCashLedgerDistribution set.NonCashLedgerDistributionList
    BudgetErrors is a Panel
        title is "BudgetErrors<DerivedHeaderBudgetErrorCount>"
        visible when (HeaderBudgetEditErrorExists)
        list is HeaderBudgetEditErrorRel.primary
    WarningBudgetErrors is a Panel
        title is "WarningBudgetErrors<DerivedWarningBudgetErrorCount>"
        visible when (DistributionBudgetEditErrorExists)
        list is DistributionBudgetEditErrorRel.primary
