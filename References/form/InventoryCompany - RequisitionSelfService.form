RequisitionSelfService is a Form
    Layout
        valid when (AllowRSSOptionsUpdates)
            three column
                column1
                    button of "SetThisCompanyToBasicUserSettings"
                        action is SetRSSOptionsToBasicUser
                    button of "SetAllRequestersToBasicUserSettings"
                        action is SetAllRequestersToBasicUser
                column2
                    button of "SetThisCompanyToPowerUserSettings"
                        action is SetRSSOptionsToPowerUser
                    button of "SetAllRequestersToPowerUserSettings"
                        action is SetAllRequestersToPowerUser
                column3
                    blank line
                    button of "SetAllRequestersToUseCompanySettings"
                        action is InitializeAllRequestersRSSOptions
            blank line
            header2 of "RequisitionSelf-\\ServiceSettings"
            blank line
            three column
                column1
                    InternalOnly
                    ContractsOnly
                    AllowFilterOptionInCatalogSearch
                        label is "AllowFilterOptionInCatalogSearch"
                    AllowContractProposals
                    RSSHomePageTitle
                        label is "RSSHomePageTitle"
                    VendorSearchOption
                        label is "VendorSearchOption"
                column2
                    SpecialItemValidation
                    PreventRequesterRSSOptionsUpdates
                    AllowQuantityMultiplierOnShoppingList
                        label is "AllowQuantityMultiplierOnShoppingList"
                    AllowRequestNewItem
                    AllowRequestNewVendor
                column3
                    AllowAllCommentTypes
                    DisplayOnlyTransactionUOM
                    EnableFavorites
                    DuplicateItemValidation
            blank line
            header2 of "RequisitionSelf-\\ServiceDisplayOptions"
            three column
                column1
                    AOCDisplayOption
