ViewCurrentChanges is a CompositeForm
    title is "Term_andConditionUpdates"
    Actions
        action is refresh
    TermAndCondition is a Panel
        title is "Term_andCondition"
        form is CurrentChangesMainForm
    ExistingAttachments is a Panel
        visible when (ContractTermAndConditionAttachmentExists)
        list is ContractTermAndConditionAttachmentRel.ExistingTermAndConditionAttachments
    AttachmentsToBeAdded is a Panel
        visible when (AttachmentsToBeAddedExist)
        list is ContractTermAndConditionAddAttachmentRel.AddAttachmentUpdates
    AttachmentsToBeDeleted is a Panel
        visible when (AttachmentsToBeDeletedExist)
        list is ContractTermAndConditionDeleteAttachmentRel.DeleteAttachmentUpdates
