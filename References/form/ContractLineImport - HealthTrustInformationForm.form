HealthTrustInformationForm is a Form
    title is "HealthTrustInformation"
    Layout
        display as text
            single column
                first ContractGPOItemFormRel.UniversalItemNumber
                first ContractGPOItemFormRel.UOMTypeID
                first ContractGPOItemFormRel.ItemShortDescription
                first ContractGPOItemFormRel.ItemFullDescription
                first ContractGPOItemFormRel.ContainsHazardousMaterials
                first ContractGPOItemFormRel.ContainsLatex
                first ContractGPOItemFormRel.LatexFree
                first ContractGPOItemFormRel.PVCDEHPFree
                first ContractGPOItemFormRel.MercuryFree
                first ContractGPOItemFormRel.ThimerosalFree
