SupplierParticipationContext is a Form
    Actions
        action is refresh
    Layout
        paragraph
            NotifiedSupplier.SupplierGroup
                display as text
        paragraph
            NotifiedSupplier.Supplier
                display as text
        paragraph
            NotifiedSupplier.Supplier.DiversityCodesExist
                display as text
        header2 of "_"
        paragraph
            NotifiedSupplier.SupplierSourceId
                label is "NotificationDetailForSuppplierContact"
                display as text
        paragraph
            NotifiedSupplier.SupplierSourceId.MainContact.FirstAndLastName
                display as text
        header2 of "_"
        paragraph
            text of "TotalsForSourceI\\D_<NotifiedSupplier.SupplierSourceId>"
                bold
        blank line
        three column
            column1
                paragraph
                    SupplierParticipationEventNoBid
                        label is "TotalEventsBid"
                        display as text
                paragraph
                    SupplierParticipationEventAward
                        label is "TotalEventAward"
                        display as text
                paragraph
                    SupplierParticipationEventSuccess
                        label is "EventBidSuccess"
                        display as text
            column2
                paragraph
                    SupplierParticipationEventLineNoBid
                        label is "TotalLinesBid"
                        display as text
                paragraph
                    SupplierParticipationEventLineAwarded
                        label is "TotalAwardedLines"
                        display as text
                paragraph
                    SupplierParticipationEventLineSuccess
                        label is "LineBidSuccess"
                        display as text
            column3
                paragraph
                    SupplierParticipationBidTotal
                        label is "BidTotal"
                        display as text
                paragraph
                    SupplierParticipationAwardTotal
                        label is "AwardedTotal"
                        display as text
                paragraph
                    SupplierParticipationAmountBidSuccess
                        label is "AmountBidSuccess"
                        display as text
        header2 of "_"
        paragraph
            text of "ResponseDetailForSupplierContact\\:_<NotifiedSupplier.Supplier.SupplierName>"
                bold
        blank line
        four column
            column1
                paragraph
                    SupplierParticipationResponseBySupplierTermsAccepted
                        label is "EventsWithTermAccepted"
                        display as text
                paragraph
                    SupplierParticipationResponseBySupplierTermsNotAccepted
                        label is "TermsNotAccepted"
                        display as text
            column2
                paragraph
                    SupplierParticipationEventResponseNoBidBySupplier
                        label is "TotalEventsNoBid"
                        display as text
                paragraph
                    SupplierParticipationEventResponseBidBySupplier
                        label is "TotalEventsBid"
                        display as text
                paragraph
                    SupplierParticipationEventAwardBySupplier
                        label is "TotalEventsAwarded"
                        display as text
                paragraph
                    SupplierParticipationEventtBidSuccessBySupplier
                        label is "EventBidSuccess"
                        display as text
            column3
                paragraph
                    SupplierParticipationEventLineResponseBySupplierNoBid
                        label is "TotalEventLinesNoBid"
                        display as text
                paragraph
                    SupplierParticipationEventLineResponseBySupplierBid
                        label is "TotalEventLinesBid"
                        display as text
                paragraph
                    SupplierParticipationAwardedResponseLineBySupplier
                        label is "TotalEventLinesAwarded"
                        display as text
                paragraph
                    SupplierParticipationEventLineBidSuccessBySupplier
                        label is "TotalEventLinesAwarded"
                        display as text
            column4
                paragraph
                    SupplierParticipationTotalEventBidBySupplier
                        label is "BidTotal"
                        display as text
                paragraph
                    SupplierParticipationTotalEventAwardBySupplier
                        label is "TotalAwardedAmount"
                        display as text
                paragraph
                    SupplierParticipationAmountBidSuccessBySupplier
                        label is "AmountBidSuccess"
                        display as text
        header2 of "_"
        paragraph
            text of "DiversityAwardRates"
                bold
        blank line
        three column
            column1
                paragraph
                    SupplierParticipationEventResponseNoBidByDiversity
                        label is "DiversityEventResponses"
                        display as text
                paragraph
                    SupplierParticipationEventAwardedResponseByDiversity
                        label is "DiversityAwards"
                        display as text
                paragraph
                    SupplierParticipationEventTotalAwardResponseByDiversity
                        label is "TotalEventAwards"
                        display as text
                paragraph
                    SupplierParticipationTotalEventAwardRateByDiversity
                        label is "DiversityAwardRate"
                        display as text
            column2
                paragraph
                    SupplierParticipationEventLineNoBidResponseByDiversity
                        label is "DiversityLinesResponsed"
                        display as text
                paragraph
                    SupplierParticipationEventLineAwardedResponseByDiversity
                        label is "DiversityLinesAwarded"
                        display as text
                paragraph
                    SupplierParticipationEventLineTotalAwardResponseByDiversity
                        label is "TotalAwardedLines"
                        display as text
                paragraph
                    SupplierParticipationLineTotalEventAwardRateByDiversity
                        label is "LineAwardRate"
                        display as text
            column3
                paragraph
                    SupplierParticipationDiversityAmountBid
                        label is "DiversityAmountBid"
                        display as text
                paragraph
                    SupplierParticipationDiversityAwardAmount
                        label is "DiversityAwardAmount"
                        display as text
                paragraph
                    SupplierParticipationAwardedTotal
                        label is "AwardedTotal"
                        display as text
                paragraph
                    SupplierParticipationAmountAwardRate
                        label is "AmountAwardRate"
                        display as text
        blank line
