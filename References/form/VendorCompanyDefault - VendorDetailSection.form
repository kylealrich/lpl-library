VendorDetailSection is a Form
    title is "VendorCompanyDefault"
    Layout
        display as text
            header2 of "Payment"
            three column distributed
                paragraph
                    TermsCode
                    TermsCode.Desc1
                        no label
                paragraph
                    CashCode
                    CashCode.Description
                        no label
                paragraph
                    BankTransactionCode
                    BankTransactionCode.Description
                        no label
            header2 of "Invoice"
            three column distributed
                column1
                    paragraph
                        ProcessLevel
                        ProcessLevel.Name
                            no label
                    paragraph
                        AccrualCode
                        AccrualCode.Description
                            no label
                    paragraph
                        DiscountCode
                        DiscountCode.Description
                            no label
                    paragraph
                        AuthorityCode
                        AuthorityCode.Description
                            no label
                    SecondaryWithholdingCode1
                column2
                    paragraph
                        RetainageAccrualCode
                        RetainageAccrualCode.Description
                            no label
                    paragraph
                        DistributionCode
                        DistributionCode.Description
                            no label
                    paragraph
                        IncomeCode
                        IncomeCode.Description
                            no label
                    paragraph
                        TaxUsageCode
                        TaxUsageCode.Description
                            no label
                    SecondaryWithholdingCode2
                column3
                    paragraph
                        TaxCode
                            select is TaxCodeFilteredList
                        TaxCode.Description
                            no label
                    paragraph
                        DiversityCode
                        DiversityCode.Description
                            no label
                    paragraph
                        InvoiceRoutingCategory
                        InvoiceRoutingCategory.Description
                            no label
                    paragraph
                        Vendor.ForeignEconomicRegulatoryCode
                        Vendor.ForeignEconomicRegulatoryCode.Description
                            no label
                    SecondaryWithholdingCode3
            header2 of "E\\D\\IOrderNumber"
            three column distributed
                PoEdiNumber
                RevisedEDINumber
