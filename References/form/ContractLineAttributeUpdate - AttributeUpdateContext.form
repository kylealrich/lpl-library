AttributeUpdateContext is a Form
    Layout
        visible when (not RecordExists)
            three column distributed
                paragraph
                    ContractGroup
                    ContractGroup.BusinessGroup.Description
                        no label
                        display as text
                paragraph
                    visible when (Contract not entered)
                        Contract
                    visible when (Contract entered)
                        Contract
                            display as text
                    visible when (ContractLine not entered)
                        ContractLine
                    visible when (ContractLine entered)
                        ContractLine
                            display as text
                    ContractLine.ItemDescription
                        no label
                        display as text
                paragraph
                    InternalItemNumber
                    InternalItemNumber.Description
                        no label
                        display as text
            three column distributed
                InternalSupplier
                VendorItemIdentifier
                visible when (ContractLine entered)
                    ContractLine.UOM.UnitOfMeasure
                        label is "ContractLineUOM"
            three column distributed
                ManufacturerCode
                ManufacturerDivision
                ManufacturerPartNumberIdentifier
            visible when (not DataFromSymmetric)
                three column distributed
                    ExternalContractNumber
                    ManufacturerID
                    VendorID
        visible when (RecordExists)
            display as text
                three column distributed
                    paragraph
                        ContractGroup
                        ContractGroup.BusinessGroup.Description
                            no label
                    paragraph
                        Contract
                        ContractLine
                        ContractLine.ItemDescription
                            no label
                    paragraph
                        InternalItemNumber
                        InternalItemNumber.Description
                            no label
                three column distributed
                    Status
                    UpdateDate
                three column distributed
                    InternalSupplier
                    VendorItemIdentifier
                    visible when (ContractLine entered)
                        ContractLine.UOM.UnitOfMeasure
                            label is "ContractLineUOM"
                three column distributed
                    ManufacturerCode
                    ManufacturerDivision
                    ManufacturerPartNumberIdentifier
                visible when (DataFromGHXOrOther)
                    three column distributed
                        ExternalContractNumber
                        ManufacturerID
                        VendorID
