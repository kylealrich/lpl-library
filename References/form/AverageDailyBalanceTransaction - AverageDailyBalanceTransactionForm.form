AverageDailyBalanceTransactionForm is a Form
    is primary
    title is "DailyTransaction"
    Layout
        two column
            FinanceEnterpriseGroup
            FinanceEnterpriseGroup.FinanceEnterpriseGroupDescription
                no label
        visible when (FinanceEnterpriseGroup.IncludeLedgerInAverageDailyBalance)
            single column
                TransactionLedger
        two column
            AccountingEntity
                label is "<FinanceEnterpriseGroup.AccountingEntityLabel>"
            AccountingEntity.Name
                no label
        two column
            AverageDailyBalanceTransaction.GeneralLedgerChartAccount
                label is "<FinanceEnterpriseGroup.AccountLabel>"
            AverageDailyBalanceTransaction.GeneralLedgerChartAccount.AccountDescription
                no label
        visible when (FinanceEnterpriseGroup.ADBAccountWithDimension)
            single column
                ADBDimension1
                    label is "<FinanceEnterpriseGroup.ADBDimensions.Dimension1.Label>"
        visible when (FinanceEnterpriseGroup.ADBAccountWithTwoDimensions)
            single column
                ADBDimension2
                    label is "<FinanceEnterpriseGroup.ADBDimensions.Dimension2.Label>"
        visible when (FinanceEnterpriseGroup.ADBSelection.AccountAndThreeDimensions)
            single column
                ADBDimension3
                    label is "<FinanceEnterpriseGroup.ADBDimensions.Dimension3.Label>"
        blank line
        single column
            AverageDailyBalanceTransaction.PostingDate
                label is "PostDate"
            AverageDailyBalanceTransaction.TransactionDate
                label is "TransactionDate"
            AverageDailyBalanceTransaction.RunDate
                label is "RunDate"
            Status
            FunctionalAmount
        visible when (FinanceEnterpriseGroup.CalculateADBOption.AllCurrencyAmounts)
            single column
                TransactionCurrency
                TransactionAmount
                AlternateAmount1
                AlternateAmount2
                AlternateAmount3
                ReportAmount1
                ReportAmount2
                ReportAmount3
                ReportAmount4
                ReportAmount5
