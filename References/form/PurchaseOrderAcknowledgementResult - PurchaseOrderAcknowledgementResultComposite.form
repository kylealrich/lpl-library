PurchaseOrderAcknowledgementResultComposite is a CompositeForm
    is primary
    title is "PurchaseOrderAcknowledgementResult"
    context form is NewPurchaseOrderAcknowledgementResultContextForm
        background is theme color
    Actions
        action is ProcessAcknowledgements
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is Delete
            label is "_"
        action is refresh
            label is "_"
    Acknowledgements is a MultiListPanel
        list is PurchaseOrderRevisionByResultRel.PurchaseOrdersAcknowledgedNoVendorNoCompany
            visible when (VendorExists)
        list is PurchaseOrderRevisionByResultRel.PurchaseOrdersAcknowledgedNoCompany
            visible when (CompanyOnly)
        list is PurchaseOrderRevisionByResultRel.PurchaseOrdersAcknowledged
            visible when (NoVendorNoCompany)
    Messages is a MultiListPanel
        list is PurchaseOrderMessageRel.NewPurchaseOrderAcknowledgementMessageListNoVendorNoCompany
            visible when (VendorExists)
        list is PurchaseOrderMessageRel.NewPurchaseOrderAcknowledgementMessageListNoCompany
            visible when (CompanyOnly)
        list is PurchaseOrderMessageRel.NewPurchaseOrderAcknowledgementMessageList
            visible when (NoVendorNoCompany)
