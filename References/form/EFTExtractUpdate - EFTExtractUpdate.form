EFTExtractUpdate is an ActionForm
    action is EFTExtractUpdate
    title is "EFTExtractFileCreation"
    Layout
        single column
            CashManagementGroup
        blank line
        two column
            Company
                select is Company
            Company.Name
                no label
                display as text
        single column
            text of "Or"
        two column
            CompanyGroup
            CompanyGroup.Description
                no label
                display as text
        blank line
        two column
            CashCode
            CashCode.Description
                no label
                display as text
        two column
            CashCodeGroup
                label is "CashCodeGroup"
                select is CashCodeGroup
            CashCodeGroup.Description
                no label
                display as text
        single column
            CashLedgerProcessGroup
            FileName
        blank line
        single column
            AchCompanyId
                label is "A\\C\\HCompanyI\\D"
            DepositDate
            PostDate
            ProcessThroughDate
            TransType
                label is "TransactionType"
            PrintDetail
            FillLastBlock
                label is "FillA\\C\\HLastBlock"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                EFTExtractUpdateReport_RDG
                CreditCardTransactionReport_RDG
                EFTOrCreditCardTransactionErrorReport_RDG
            column2 of 25%
                EFTExtractUpdateReport_XPT
                CreditCardTransactionReport_XPT
                EFTOrCreditCardTransactionErrorReport_XPT
