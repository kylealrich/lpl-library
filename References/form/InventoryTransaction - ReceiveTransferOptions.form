ReceiveTransferOptions is a Form
    Layout
        paragraph
            Company
                display as text
            Company.Name
                no label
                display as text
        paragraph
            InventoryLocation
                label is "Location"
                display as text
            InventoryLocation.Name
                no label
                display as text
        single column
            TransactionDate
                label is "ReceivedDate"
            visible when (not IsUnreleased)
                GeneralLedgerDate
                    label is "PostDate"
                    display only
            visible when (IsUnreleased)
                GeneralLedgerDate
                    label is "PostDate"
            ReceiveAll
