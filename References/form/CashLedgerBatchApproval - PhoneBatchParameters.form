PhoneBatchParameters is a Form
    title is "BatchParameters"
    Layout
        display as text
            paragraph
                visible when (BatchApprovalType.CashLedgerPayment)
                    CashLedgerPaymentGroup
                visible when (BatchApprovalType.CashLedgerReceipt)
                    CashLedgerReceiptGroup
                visible when (BatchApprovalType.ElectronicFundsTransaction)
                    CashLedgerEFTTransactionGroup
            paragraph
                GeneralLedgerSourceCodeName
                    label is "SourceCode"
            visible when (not BatchApprovalType.CashLedgerReceipt)
                two column table
                    column1
                        CashManagementAccount
                    column2
                        CashManagementAccount.Title
                            label is "AccountTitle"
            two column table
                column1
                    visible when (CashCode entered)
                        CashCode.CashCodeAndDescription
                            label is "CashCode"
                    ApprovalCostCenter
                    DateRange.Begin
                        label is "FromPostDate"
                    SearchAmountRange.Begin
                        label is "FromAmount"
                column2
                    visible when (BankTransactionCode entered)
                        BankTransactionCode.BankTransactionCodeName
                            label is "TransactionCode"
                    ApprovalCategory
                    DateRange.End
                        label is "ToPostDate"
                    SearchAmountRange.End
                        label is "ToAmount"
            single column
                CommentText
                    label is "Comment"
