DistributionImport is a Form
    is primary
    Layout
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        paragraph
            DistributionAccount
                label is "GlobalLedgerAccount"
                compact format
        paragraph
            TransactionAmount
        paragraph
            TransactionTaxableAmount
                label is "TaxableAmount"
        paragraph
            Description
        paragraph
            TaxCode
                select is TaxCodes
            Units
        visible when (ShowTaxOriginCrossReference)
            TaxOriginCrossReference
        visible when (TaxEntityUsesOneSource)
            two column
                UNSPSCCode
                    compact format
                UNSPSCCode.ShortDescription
                    no label
                    display as text
