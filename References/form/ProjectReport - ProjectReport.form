ProjectReport is an ActionForm
    action is ProjectReport
    Layout
        header2 of "Project"
        three column
            Project.Project[1]
                label is "Projects"
            Project.Project[2]
                no label
            Project.Project[3]
                no label
        three column
            Project.Project[4]
                no label
            Project.Project[5]
                no label
            Project.Project[6]
                no label
        three column
            Project.Project[7]
                no label
            Project.Project[8]
                no label
            Project.Project[9]
                no label
        three column
            Project.Project[10]
                no label
            Project.Project[11]
                no label
            Project.Project[12]
                no label
        header2 of "Transaction"
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        two column distributed
            column1
                FrPostDate
                    label is "FromPostDate"
            column2
                ToPostDate
                    label is "ToPostDate"
        two column distributed
            column1
                paragraph
                    FrProcessLevel
                        label is "FromProcessLevel"
                        select is ProcessLevels
                    FrProcessLevel.Name
                        no label
                        display as text
            column2
                paragraph
                    ToProcessLevel
                        label is "ToProcessLevel"
                        select is ProcessLevels
                    ToProcessLevel.Name
                        no label
                        display as text
        single column
            TransType
                label is "TransactionType"
        paragraph
            Customer
                select is GroupSearchName
            Customer.Name
                no label
                display as text
        blank line
        paragraph
            CurrencyCd
                label is "TransactionCurrency"
            CurrencyCd.Description
                no label
                display as text
        single column
            AcoCurrDisplay
                label is "CurrencyDisplay"
        two column distributed
            column1
                PrintDetail
            column2
                Subtotals
                    label is "IncludeSubtotals"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                ProjectReportReport_RDG
            column2 of 25%
                ProjectReportReport_XPT
