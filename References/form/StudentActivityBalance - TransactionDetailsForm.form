TransactionDetailsForm is a CompositeForm
    title is "TransactionDetails"
    context form is TransactionDetailsContextForm
    Receipts is a Panel
        visible when (HasReceiptDetails)
        list is StudentActivityReceiptDetailRel.ReceiptDetailBySchoolList
    BankReceipts is a Panel
        visible when (HasBankReceiptDetails)
        list is StudentActivityBankReceiptDetailRel.BankReceiptDetailBySchoolList
    Disbursements is a Panel
        visible when (HasDisbursementDetails)
        list is StudentActivityDisbursementDetailRel.DisbursementDetailBySchoolList
    Transfers is a Panel
        visible when (HasTransferDetails)
        list is StudentActivityTransferDetailRel.TransferDetailBySchoolList
    BankTransfers is a Panel
        visible when (HasBankTransferDetails)
        list is StudentActivityBankTransferDetailRel.BankTransferDetailBySchoolList
