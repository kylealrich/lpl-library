MobileInventoryTransactionLine is a Form
    title is "Main"
    Layout
        single column
            visible when (not InventoryTransaction.IsInventoryIntransitReceiving)
                paragraph
                    Item
                        select is MobileItemFilterSelection
                    Item.Description
                        label is "Description"
            visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                paragraph
                    Item
                        display as text
                    Item.Description
                        label is "Description"
                        display as text
            visible when (not InventoryTransaction.IsInventoryIntransitReceiving)
                GlobalTradeItemNumber
                    label is "GTIN"
            visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                GlobalTradeItemNumber
                    label is "GTIN"
                    display as text
            visible when (false)
                ItemLocation.BinTracked
            visible when (not InventoryTransaction.IsInventoryBinTransfer)
                visible when (ItemLocation.BinTracked)
                    paragraph
                        Bin
                        text of "Or"
                        MultipleBins
        single column
            TransientQuantityValue
                label is "Quantity"
            visible when (not InventoryTransaction.IsInventoryIntransitReceiving)
                visible when (not InventoryTransaction.IsInventoryBinTransfer)
                    TransactionUOM
                        label is "TransactionUOM"
                        select is ValidTransactionUnitsOfMeasure
            visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                TransactionUOM
                    label is "TransactionUOM"
                    display as text
        visible when (InventoryTransaction.IsInventoryBinTransfer)
            paragraph
                visible when (ItemLocation.BinTracked)
                    Bin
                        label is "FromBin"
                        select is BinWithStockOnHandRel.BinList
                visible when (not ItemLocation.IsSerialTracked)
                    TransactionUOM
                        label is "FromUOM"
            paragraph
                visible when (ItemLocation.BinTracked)
                    FromToCompanyLocationBin.FromToBin
                        label is "ToBin"
                visible when (not ItemLocation.IsSerialTracked)
                    visible when (ItemLocation.TrackedInAlternateUnitsOfMeasure)
                        ToUOM
                            label is "ToUOM"
        single column
            visible when (not InventoryTransaction.IsInventoryTransferOrIntransitReceiving)
                visible when (InventoryTransaction.Status.Unreleased)
                    TransientPreciseUnitCost
                visible when (InventoryTransaction.Status.Released)
                    TransientUnitCostValue
                        label is "UnitCost"
            visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                TransientPreciseUnitCost
                    display as text
            visible when (InventoryTransaction.IsInventoryTransfer)
                visible when (InventoryTransaction.Status.Released)
                    TransientPreciseUnitCost
                        display as text
            visible when (InventoryTransaction.IsIssuesOrAdjustments)
                text of "Or"
                UseLastIssueCost
                    label is "UseLastIssueCost"
        visible when (InventoryTransaction.IsCostAdjustment)
            paragraph
                BaseCost
            visible when (not Status.Released)
                CurrentCost
        paragraph
            InventoryReasonCode
                label is "ReasonCode"
            InventoryReasonCode.Description
                no label
                display as text
            visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                paragraph
                    OpenQuantity
                        display as text
                    OriginalQuantity
                        display as text
        paragraph
            visible when (not InventoryTransaction.IsInventoryTransferBinTransferAndIsInventoryIntransitReceiving)
                OffsetAccount
                    label is "<InventoryTransaction.OffsetAccountLabel>"
                    compact format
            visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                paragraph
                    AddOnChargeUnitCost
                    text of "Per"
        visible when (InventoryTransaction.IsInventoryIntransitReceiving)
            single column
                AddOnCostAccount
                    label is "AddOnCostAccount"
                    compact format
        visible when (InventoryTransaction.IsIssuesOrAdjustments)
            single column
                EAMWorkOrderNumber
        visible when (IsInventoryReturns)
            single column
                EAMWorkOrder
                EAMWorkOrderActivity
        visible when (InventoryTransaction.IsInventoryTransfer)
            paragraph
                FromToCompanyLocationBin.FromToCompany
                    label is "ToCompany"
                    display as text
                FromToCompanyLocationBin.FromToLocation
                    label is "ToLocation"
                    display as text
                FromToCompanyLocationBin.FromToBin
                    label is "ToBin"
        paragraph
            Reference
                label is "Reference"
        paragraph
            LineComment
                label is "LineComments"
            visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                blank line
            visible when (InventoryTransaction.IsCostAdjustment)
                LastUpdateDate
                LastUpdateBy
        visible when (LineExists)
            visible when (not InventoryTransaction.IsInventoryIntransitReceiving)
                visible when (not InventoryTransaction.IsInventoryBinTransfer)
                    visible when (not InventoryTransaction.IsCostAdjustment)
                        header4 of "StockOnHandQuantityIn_'<DerivedUOM>'"
                        visible when (HasUnequalTransactionAndStockUOM)
                            two column table
                                column1
                                    DerivedBeforeQuantityInTransactionUOM
                                        label is "TransactionUOM-Before"
                                        display as text
                                column2
                                    paragraph
                                        DerivedAfterQuantityInTransactionUOM
                                            label is "After"
                                            display as text
                                        TransactionUOM
                                            no label
                                            display as text
                        two column table
                            column1
                                visible when (IsTrackedInAndAlternateStockUOM)
                                    DerivedBeforeQuantity
                                        label is "TrackedUOM_-_Before"
                                        display as text
                                visible when (not IsTrackedInAndAlternateStockUOM)
                                    DerivedBeforeQuantity
                                        label is "StockUOM_-_Before"
                                        display as text
                            column2
                                visible when (IsTrackedInAndAlternateStockUOM)
                                    DerivedAfterQuantity
                                        label is "TrackedUOM_-_After"
                                        display as text
                                visible when (not IsTrackedInAndAlternateStockUOM)
                                    DerivedAfterQuantity
                                        label is "StockUOM_-_After"
                                        display as text
