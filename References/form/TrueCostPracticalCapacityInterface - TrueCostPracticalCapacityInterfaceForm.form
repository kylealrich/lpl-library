TrueCostPracticalCapacityInterfaceForm is a Form
    is primary
    title is "PracticalCapacityVolume"
    Layout
        single column
            TrueCostConfiguration
                label is "Configuration"
                display as text
            visible when (TrueCostPracticalCapacityInterface.RunGroup entered)
                TrueCostPracticalCapacityInterface.RunGroup
                display as text
            visible when (TrueCostPracticalCapacityInterface.RunGroup not entered)
                TrueCostPracticalCapacityInterface.RunGroup
            TrueCostCapacity
        paragraph
            AccountingEntity
            visible when (TrueCostConfiguration.AccountingUnitEntry)
                AccountingUnit
                    show up to 15 characters
            visible when (TrueCostConfiguration.FinanceEnterpriseGroup.ProjectEntry)
                Project
                    show up to 15 characters
            visible when (TrueCostConfiguration.FinanceEnterpriseGroup.FinanceDimension1Entry)
                FinanceDimension1
            visible when (TrueCostConfiguration.FinanceEnterpriseGroup.FinanceDimension2Entry)
                FinanceDimension2
            visible when (TrueCostConfiguration.FinanceEnterpriseGroup.FinanceDimension3Entry)
                FinanceDimension3
            visible when (TrueCostConfiguration.FinanceEnterpriseGroup.FinanceDimension4Entry)
                FinanceDimension4
            visible when (TrueCostConfiguration.FinanceEnterpriseGroup.FinanceDimension5Entry)
                FinanceDimension5
            visible when (TrueCostConfiguration.FinanceEnterpriseGroup.FinanceDimension6Entry)
                FinanceDimension6
            visible when (TrueCostConfiguration.FinanceEnterpriseGroup.FinanceDimension7Entry)
                FinanceDimension7
            visible when (TrueCostConfiguration.FinanceEnterpriseGroup.FinanceDimension8Entry)
                FinanceDimension8
            visible when (TrueCostConfiguration.FinanceEnterpriseGroup.FinanceDimension9Entry)
                FinanceDimension9
            visible when (TrueCostConfiguration.FinanceEnterpriseGroup.FinanceDimension10Entry)
                FinanceDimension10
        single column
            TrueCostResourceIntensityFactor
            TrueCostResourceUtilizationDriver
            CapacityVolume
            EffectiveDate
            visible when (HasError)
                ErrorDescription
                    display as text
