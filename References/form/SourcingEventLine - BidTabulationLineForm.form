BidTabulationLineForm is a SummaryForm
    Layout
        header2 of "Item:_<ItemNumber>"
            show border color of black
        visible when (Status.Cancelled)
            single column
                text of "**ThisLineHasBeenCancelled**"
                    header2
                    bold
                blank line
        paragraph
            ItemDescription
                label is "Description"
            ItemDescription2
                label is "Description2"
        visible when (HasThirdDescription)
            single column
                ItemDescription3
                    label is "LongItemDescription"
        four column
            UOM.UnitOfMeasure
            Quantity
            PerUnitEstimate
                label is "UnitEst"
            DerivedExtendedEstimate
                label is "ExtendedEst"
        visible when (SourcingEvent.HasPerUnitEstimateLineAOCs)
            four column
                column1
                    blank line
                column2
                    blank line
                column3
                    DerivedPerUnitEstimateAOC
                        label is "UnitAOCEst"
                        display when blank
                column4
                    DerivedExtendedAOCEstimate
                        label is "TotalAOCEst"
                        display when blank
            four column
                column1
                    blank line
                column2
                    blank line
                column3
                    blank line
                column4
                    DerivedTotalEstimate
                        label is "TotalEst"
                        display when blank
        header2 of "_"
        list is SubmittedResponsesForBidTabulation.BidTabulationResponseList
        visible when (SubmittedAlternateResponsesExist)
            header2 of "**AlternateResponses**"
            list is SubmittedAlternateResponses.BidTabulationResponseList
