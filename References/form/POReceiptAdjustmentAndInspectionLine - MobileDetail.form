MobileDetail is a Form
    title is "Detail"
    Layout
        visible when (LineExists)
            single column
                PurchaseOrderReceiptLine.PurchaseOrderLine.Description
                    label is "ItemDescription"
                    display as text
                PurchaseOrderReceiptLine.ItemType
                    display as text
        visible when (not LineExists)
            single column
                ReceiptLinesForTemplateRel.PurchaseOrderLine.Description
                    label is "ItemDescription"
                    display as text
                ReceiptLinesForTemplateRel.ItemType
                    display as text
        paragraph
            TransientRejectedQuantity
                label is "RejectedQuantity"
            TransientAcceptReject
        visible when (LineExists)
            single column
                PurchaseOrderReceiptLine.Bin
                    label is "OriginalBin"
                    display as text
                Bin
                    label is "RejectedFromBin"
                    select is MobileBinsForALocationSelect
                WriteOff
                    label is "Write\\-\\Off"
            two column table
                column1
                    PurchaseOrderLine.Quantity
                        label is "QuantityOrdered"
                        display as text
                    PurchaseOrderReceiptLine.OriginalReceivedQuantity
                        label is "OriginalQuantityReceived"
                        display as text
                    PurchaseOrderReceiptLine.OriginalUnitCost
                        label is "UnitCost"
                        display as text
                column2
                    PurchaseOrderReceiptLine.ReceivedUOM
                        label is "UOM"
                        display as text
                    PurchaseOrderLine.EnteredBuyUOM
                        label is "UOM"
                        display as text
                    PurchaseOrderReceiptLine.ReceivedUOM
                        label is "UOM"
                        display as text
            single column
                paragraph
                    PurchaseOrderReceiptLine.ShipToLocation
                        label is "ReceivedLocation"
                        display as text
                    PurchaseOrderReceiptLine.ShipToLocation.Name
                        no label
                        display as text
            single column
                PurchaseOrderReceiptLine.ReceivedDate
                    display as text
            two column table
                column1
                    AcceptedQuantity
                        display as text
                column2
                    OvershipReturnQuantity
                        display as text
            single column
                Inspected
                    label is "InspectionStatus"
                    display as text
                VendorReturn
                    label is "ReturnNumber"
                    display as text
                GlobalLineType
                    label is "LineType"
                    select is MobileLineTypeCodesSelect
        visible when (not LineExists)
            single column
                ReceiptLinesForTemplateRel.Bin
                    label is "OriginalBin"
                    display as text
                Bin
                    label is "RejectedFromBin"
                    select is MobileBinsForALocationSelect
                WriteOff
                    label is "Write\\-\\Off"
            two column table
                column1
                    ReceiptLinesForTemplateRel.PurchaseOrderLine.Quantity
                        label is "QuantityOrdered"
                        display as text
                    ReceiptLinesForTemplateRel.OriginalReceivedQuantity
                        label is "OriginalQuantityReceived"
                        display as text
                    ReceiptLinesForTemplateRel.OriginalUnitCost
                        label is "UnitCost"
                        display as text
                column2
                    ReceiptLinesForTemplateRel.PurchaseOrderLine.EnteredBuyUOM
                        label is "UOM"
                        display as text
                    ReceiptLinesForTemplateRel.ReceivedUOM
                        label is "UOM"
                        display as text
                    ReceiptLinesForTemplateRel.ReceivedUOM
                        label is "UOM"
                        display as text
            single column
                paragraph
                    ReceiptLinesForTemplateRel.ShipToLocation
                        label is "ReceivedLocation"
                        display as text
                    ReceiptLinesForTemplateRel.ShipToLocation.Name
                        no label
                        display as text
            single column
                ReceiptLinesForTemplateRel.ReceivedDate
                    display as text
            two column table
                column1
                    AcceptedQuantity
                        display as text
                column2
                    OvershipReturnQuantity
                        display as text
            single column
                Inspected
                    label is "InspectionStatus"
                    display as text
                VendorReturn
                    label is "ReturnNumber"
                    display as text
                GlobalLineType
                    label is "LineType"
                    select is MobileLineTypeCodesSelect
