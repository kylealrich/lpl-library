CreateCashOffsetsForm is an ActionForm
    action is CreateCashOffsets
    title is "CreateCashOffsets"
    Layout
        single column
            PrmEnterpriseGroup
                label is "FinanceEnterpriseGroup"
                display only
            PrmRunGroup
                label is "RunGroup"
            PrmCashOffsetAccount
                label is "CashOffsetAccount"
            PrmMoveCreateOffsetsErrorsToErrorGroup
            PrmInterfaceAfterOffset
                label is "InterfaceAfterOffset"
            visible when (PrmInterfaceAfterOffset)
                PrmAccountingEntity
                    label is "<PrmEnterpriseGroup.AccountingEntityLabel>"
                PrmEditOnly
                    label is "EditOnlyNoUpdate"
                visible when (not PrmEditOnly)
                    PrmEditAndInterface
                        label is "EditAndInterfaceIfNoErrors"
                visible when (PrmPartialUpdate)
                    PrmMoveErrorsToNewRunGroup
                        label is "MoveErrorsToNewRunGroup"
                visible when (PrmMoveErrorsToNewRunGroup)
                    PrmErrorRunGroupPrefix
                        label is "ErrorRunGroupPrefix"
                visible when (not PrmEditOnly)
                    PrmJournalizeByEntity
                        label is "JournalizeBy<PrmEnterpriseGroup.AccountingEntityLabel>"
                    PrmJournalByJournalCode
                        label is "SplitJournalsByJournalCode"
                PrmByPassOrganizationCode
                    label is "BypassOrganizationCode"
                PrmByPassAccountCode
                    label is "BypassAccountCode"
                PrmBypassStructureRelationEdit
                    label is "BypassStructureRelationValidation"
                visible when (not PrmEditOnly)
                    PrmInterfaceInDetail
                        label is "InterfaceInDetail"
                PrmCurrencyTable
                    label is "CurrencyTable"
                PrmBypassNegativeRateEdit
                    label is "BypassNegativeRateEdit"
