RSSLPLRequisitionLineDetail is a CompositeForm
    title is "<DerivedFormTitle>"
    context form is RSSLPLRequisitionLineContext
        background is theme color
    restrict action Processed.UpdateLineReturn
    restrict action MobileCopyRequisitionLine
    restrict action ModalCopyMultipleRequisitionLines
    restrict action NonModalCopyRequisitionLine
    restrict action Unreleased.RSSLPLUpdateRequisitionLine
    restrict action RSSLPLAddNewSpecialServiceToNewRequisition
    restrict action RSSLPLAddNewSpecialServiceToRequisition
    restrict action ModalCopyMultipleRequisitionLinesForSpecialAndService
    restrict action NonModalCopyRequisitionLineForSpecialAndService
    restrict action ModalCopyRequisitionLineForSpecialAndService
    restrict action MobileCopyRequisitionLineForSpecialAndService
    restrict action Unreleased.RSSLPLUpdateRequisitionLineQTY
    restrict action RSSLPLModalCopyLineOptionPromptForRecentlyRequestedItems
    restrict action RSSLPLModalCopyLineOptionPromptForSpecialAndService
    Actions
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is save and close
            action icon is save
    Main is a Panel
        form is RSSLPLRequisitionLineMain
    PurchasingOptions is a Panel
        form is RSSLPLRequisitionLinePurchasingOptions
    ClassesAndCodes is a Panel
        title is "Classes_andCodes"
        form is RSSLPLRequisitionLineClassesAndCodes
    SpecialHandling is a Panel
        title is "Special_Handling"
        visible when (DisplaySpecialHandlingCode)
        form is RSSLPLRequisitionLineSpecialHandling
    Miscellaneous is a Panel
        form is RSSLPLRequisitionLineMiscellaneous
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
    ProcedureInformation is a Panel
        visible when (DisplayPatientProcedure)
        form is RSSLPLProcedureInformationForm
    WarehouseOrder is a Panel
        visible when (not IsCreatePurchaseOrder)
        form is RSSLPLRequisitionLineWarehouseOrder
    Distributions is a Panel
        title is "Distributions<DistributionCount>"
        visible when (DisplayAccounting)
        list is RequisitionLineDistributionsRel.RSSLPLDistributionsForARequisitionLine
    Comments is a Panel
        title is "Comments<CommentCount>"
        list is RequisitionLineCommentRel.RSSLPLRequisitionLineComments
    Components is a Panel
        visible when (IsKitItem)
        list is KitItemFromLocationRel.KitComponentsRel.RSSLPLComponents
    AddOnCharges is a Panel
        title is "AddOnCharges<AddOnChargeCount>"
        visible when (DisplayAOCOption)
        list is RequisitionLineAddOnCharge set.RSSLPLRequisitionLineAddOnChargeList
    UserFields is a Panel
        visible when (actor.agent(Employee).Employee.Requester.DisplayUserFields)
        form is RSSLPLUserFields
    Errors is a Panel
        visible when (LineErrorsExist)
        list is RequisitionLineError set.RequisitionLineErrorList
