HeaderCurrencyAmountsForm is a Form
    title is "HeaderCurrencyAmounts"
    Layout
        visible when (false)
            HeaderAmountData.AssignToCurrency
        blank line
        four column table
            column headers
                "_"
                "Amount"
                "Currency"
                "Rate"
            column1
                text of "Functional"
                visible when (DetailCompany.OrigCompany.AccountingEntity.AlternateCurrencyExists)
                    text of "Alternate"
                visible when (DetailCompany.OrigCompany.AccountingEntity.AlternateCurrency2Exists)
                    text of "Alternate2"
                visible when (DetailCompany.OrigCompany.AccountingEntity.AlternateCurrency3Exists)
