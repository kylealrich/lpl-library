CashTransactionProcessingRuleGroup is a CompositeForm
    is primary
    title is "ProcessingRuleGroup"
    context form is CashTransactionProcessingRuleGroupContext
        background is theme color
    ProcessingRules is a Panel
        list is CashTransactionProcessingRuleGroupDetail set.CashTransactionProcessingRuleGroupDetails
            helper list is CashTransactionProcessingRuleRel.CashTransactionProcessingRulesHelper
                action is AddProcessingRule
                    invoked.PrmCashManagementGroup = CashManagementGroup
                    invoked.PrmCashTransactionProcessingRuleGroup = CashTransactionProcessingRuleGroup
