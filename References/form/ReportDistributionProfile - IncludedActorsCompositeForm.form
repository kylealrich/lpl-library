IncludedActorsCompositeForm is a CompositeForm
    title is "IncludedActors"
    context form is ReportDistributionProfileContextForm
    restrict action AddProfileToDistributionGroup
    ActorsInSpecifiedActorGroup is a Panel
        title is "ActorsIn<ReportDistributionProfile.ActorGroup>_<GroupOnlyTargetedText>"
        visible when (ActorGroup entered)
        list is ActorGroup.GroupMemberRel.ActorsForActorGroupList
            helper list is ActorGroup.ActorsNotInGroupRel.ActorList
                action is AddActorToActorGroup
    ActorsInExpandedActorGroup is a Panel
        title is "ActorsIn<ReportDistributionProfile.ActorGroup>AndDescendents_<DescendentsTargetedText>"
        visible when (ShowDescendentActorGroups)
        list is ActorGroup.ActorGroupMemberShadow set.ActorGroupMemberShadowResidenceGroupActorList
    RoleMembership is a Panel
        title is "MembersOf<Role>Role"
        visible when (Role entered)
        list is Role.ActorRoleRel.ActorRoleRoleList
    RoleGroupMembership is a Panel
        title is "MembersOf<RoleGroup>RoleGroup"
        visible when (RoleGroup entered)
        list is RoleGroupActorsRel.ActorRoleRoleList
