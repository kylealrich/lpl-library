MobileReceiverLineSummary is a Form
    title is "ReceivingLineDetail"
    Layout
        single column
            ShipToLocation
                label is "ReceivingLocation"
                display as text
            ShipToLocation.Name
                label is "LocationName"
                display as text
            visible when (CreatedBy entered)
                CreatedBy
                    display as text
            blank line
            PurchaseOrderReceipt.ReceivedDate
                label is "DateAndTimeReceived"
                display as text
        single column
            visible when (HasUnreleasedBuyerMessage)
                BuyerMessageDisplay
                    no label
                    display as text
            paragraph
                visible when (IsCatchWeightQuantityOutOfTolerance)
                    CatchWeightOutOfToleranceMsg
                        no label
                        display as tag
                            color of yellow
            paragraph
                visible when (IsOutofBalance)
                    IsOutofBalancemsg
                        no label
                        display as tag
                            color of yellow
            paragraph
                visible when (not IsCatchWeightItemFulfilled)
                    StockQuantityAlertMsg
                        no label
                        display as tag
                            color of yellow
        single column
            Item
                display as text
            PurchaseOrderLine.Description
                label is "ItemDetail"
                display as text
        two column table
            column1
                visible when (IsHSNSACCodeEnabled)
                    HSNSACCode
                        display as text
                EnteredReceivedQuantity
                    label is "QuantityCurrentlyReceived"
                    display as text
                OriginalReceivedQuantity
                    label is "QuantityReceivedOriginally"
                    display as text
                MatchedQuantity
                    display as text
                OpenToMatchQuantity
                    display as text
                MatchedNotReceivedQuantity
                    label is "MatchedNotReceivedQuantity"
                    display as text
                WriteOffQuantity
                    label is "ReceiverWriteOffQuantity"
                    display as text
                ArchivedQuantity
                    label is "ReceiverArchivedQuantity"
                    display as text
                DerivedRejectedQuantityDisplay
                    display as text
                ChargebackQuantity
                    label is "P\\-\\CardChargeQuantity"
                    display as text
            column2
                visible when (IsHSNSACCodeEnabled)
                    HSNSACCode.Description
                        label is "CodeDescription"
                        display as text
                ReceivedUOM
                    label is "_"
                    display as text
                ReceivedUOM
                    label is "_"
                    display as text
                PurchaseOrderLine.EnteredBuyUOM
                    label is "_"
                    display as text
                PurchaseOrderLine.EnteredBuyUOM
                    label is "_"
                    display as text
                PurchaseOrderLine.EnteredBuyUOM
                    label is "_"
                    display as text
                PurchaseOrderLine.EnteredBuyUOM
                    label is "_"
                    display as text
                PurchaseOrderLine.EnteredBuyUOM
                    label is "_"
                    display as text
                ReceivedUOM
                    label is "_"
                    display as text
                ReceivedUOM
                    label is "_"
                    display as text
        single column
            CatchWeightQuantity
                display as text
        paragraph
            visible when (MultipleBins)
                MultipleBins
                    display as text
            visible when (not MultipleBins)
                Bin
                    display as text
                Bin.BinType
                    label is "Type"
                    display as text
                Bin.StorageCode
                    label is "Storage"
                    display as text
        paragraph
            CancelBackorder
                label is "BackOrderCancelled"
            InspectionRequired
        paragraph
            MatchUnitCost
                label is "UnitCostToMatch"
                display as text
        paragraph
            DerivedOriginalUnitCostInReceivedUOM
                display as text
        paragraph
            VendorReturn
                display as text
            VendorReturnLine
                display as text
            VendorReturnLine.ReturnQuantity
                display as text
