Receiver is a Form
    Layout
        display as text
            paragraph
                PurchaseOrderReceipt.Vendor
                    label is "Vendor"
                PurchaseOrderReceipt.Vendor.VendorName
                    no label
            paragraph
                PurchaseOrderReceipt.PurchaseFromLocation
                    label is "PurchaseFrom"
                PurchaseOrderReceipt.PurchaseFromLocation.VendorName
                    no label
            paragraph
                PurchaseOrderReceipt.Buyer
                    label is "Buyer"
                PurchaseOrderReceipt.Buyer.Employee.PresentationNameSnapshot
                    no label
                PurchaseOrderReceipt.Buyer.Employee.EmployeeWorkTelephone
                    label is "WorkPhone"
            paragraph
                PurchaseOrderReceipt.PurchaseOrder
                    label is "PurchaseOrderNumber"
                PurchaseOrderReceipt.PurchaseOrder.Company
                    no label
            paragraph
                PurchaseOrderReceipt.ReceivedDate
                    label is "Date\\,TimeReceived"
            paragraph
                PurchaseOrderReceipt.BillOfLading
                    label is "Bill_ofLading"
            paragraph
                PurchaseOrderReceipt.Carrier
                    label is "Carrier"
            paragraph
                PurchaseOrderReceipt.PurchaseOrder.FreightTerm.Description
                    label is "FreightTerm"
        paragraph
            GlobalDocumentType
                label is "DocumentTypeCode"
                select is DocumentTypeCodes
