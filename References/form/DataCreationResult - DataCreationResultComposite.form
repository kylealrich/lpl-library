DataCreationResultComposite is a CompositeForm
    is primary
    title is "DataCreationResult"
    context form is DataCreationResultContext
    Errors is a Panel
        visible when (ErrorsExist)
        list is DataCreationResultDetailErrorRel.DataCreationResultDetailErrors
    AllRepSetResults is a Panel
        title is "All"
        visible when (Type.ReplicationSet)
        list is DataCreationResultDetailRel.primary
    AllBirstModelResults is a Panel
        title is "All"
        visible when (Type.GLReportDesignerModel)
        list is DataCreationResultDetail set.primary
    ValidateSetupResults is a Panel
        visible when (Type.ValidateBirstIntegration)
        list is DataCreationResultDetailRel.ValidateSetupResultDetails
    AllActionEvents is a Panel
        title is "All"
        visible when (Type.ActionEvent)
        list is DataCreationResultDetailRel.ActionEventResultDetails
    AllPurgeCatalogResults is a Panel
        title is "All"
        visible when (Type.PurgeCatalog)
        list is DataCreationResultDetailRel.PurgeCatalogResultDetails
    ColumnarQueryResults is a Panel
        title is "All"
        visible when (Type.ColumnarQuery)
        list is DataCreationResultDetailRel.ColumnarQueryResultDetails
