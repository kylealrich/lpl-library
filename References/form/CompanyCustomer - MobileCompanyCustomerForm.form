MobileCompanyCustomerForm is a CompositeForm
    title is "CompanyCustomer"
    context form is MobileCustomerFormContext
    show panel control on left
    Main is a Panel
        form is Main
    Address is a Panel
        form is Address
    Billing is a Panel
        form is MobileBilling
    OrderEntry is a Panel
        visible when (HasBillingCompany)
        form is MobileOrderEntry
    ProcessingRules is a Panel
        form is ProcessingRules
    CashApplication is a Panel
        form is MobileCashApplication
    Dunning is a Panel
        form is Dunning
    CustomerComments is a Panel
        list is ReceivableCommentsRel.MobileCustomerCommentsList
    TransactionComments is a Panel
        list is ReceivableCommentsRel.MobileTransactionCommentsList
    NationalAccountChildren is a Panel
        visible when (not NationalAccountIndicator.Child)
        list is NationalAccountCustomerRel.CompanyCustomerNationalAccountSpeedEntry
    ProcessLevelInclusion is a Panel
        list is CustomerProcessLevelsRel.MobileCustomerProcessLevelInclusions
    Contacts is a Panel
        list is CompanyCustomerContactRel.MobileCustomerContacts
    Banks is a Panel
        visible when (ElectronicFundsOrBOEProcessing)
        list is CustomerDraftBankRel.MobileBanksList
    BankAccounts is a Panel
        list is CompanyCustomerBankAccountRel.MobileCompanyCustomerBankAccountInlineList
    FundsTransfer is a Panel
        visible when (Company.CustomerGroupField.CustomerGroup.CompanyEFTProcessing)
        list is EFTNotificationRel.MobileEFTNotificationList
    FundsTransferCustomers is a Panel
        visible when (Company.CustomerGroupField.CustomerGroup.CompanyEFTProcessing)
        list is ElectronicFundsTransferCustomerGroupRel.MobileGroupEFTCustomerList
    Attachments is a Panel
        list is CompanyCustomerAttachment set.MobileAttachments
    InvoiceTypes is a Panel
        list is CustomerInvoiceTypeCodesRel.MobileInvoiceTypeCodeList
    AgePeriodBalance is a Panel
        visible when (ShowAgePeriodBalance)
        list is AgePeriodBalanceHeaderRel.primary
    Certifications is a Panel
        list is CustomerCertificationsRel.MobileCustomerCertificationPanelList
