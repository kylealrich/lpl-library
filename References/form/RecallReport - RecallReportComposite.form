        title is "Manufacturers"
        list is ManufacturerRel.ManufacturerRecallReportPeriodSummary
    Organizations is a Panel
        title is "Organizations"
        list is OrganizationRel.RecallOrganizationReportPeriodSummary
    RecallTypes is a Panel
        title is "RecallTypes"
        list is RecallTypeRel.RecallTypeReportPeriodSummary
    RecallSource is a Panel
        title is "RecallSource"
        list is RecallReportPeriod set.RecallSourceReportPeriodSummary
    ReportPeriods is a Panel
        list is RecallReportPeriod set.primary
