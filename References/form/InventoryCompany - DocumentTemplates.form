DocumentTemplates is a Form
    Layout
        header2 of "Option"
        UseIDM
            label is "UseIDMTemplate"
        blank line
        header2 of "IDMTemplates"
        two column distributed
            column1
                visible when (not UseIDM)
                    paragraph
                        PickListPrintByShipmentDocumentLineTemplate
                            display only
                            select is WarehousePickListPrintByShipmentDocumentLineTemplate
                    paragraph
                        PickListPrintByShipmentBinLotSerialTemplate
                            display only
                            select is WarehousePickListPrintByShipmentBinLotSerial
                    paragraph
                        PickListPrintByShipmentBinLotSerialRQBasedTemplate
                            display only
                            select is WarehousePickListPrintByShipmentBinLotSerialRQ
                    paragraph
                        PickListPrintByBinItemUOMLotSerialTemplate
                            display only
                            select is WarehousePickListPrintByBinItemUOMLotSerial
                    visible when (KitchenOrderInUse)
                        paragraph
                            KitchenOrderTemplate
                                display only
                                select is KitchenOrderIDMTemplate
                    visible when (AdvancedWarehouseInUse)
                        paragraph
                            DeliveryChecklistTemplate
                                display only
                                select is DeliveryChecklistIDMTemplate
                visible when (UseIDM)
                    paragraph
                        PickListPrintByShipmentDocumentLineTemplate
                            select is WarehousePickListPrintByShipmentDocumentLineTemplate
                        button of "<PickListPrintByShipmentDocumentLineTemplate.Name>"
                            link is external "<PickListPrintByShipmentDocumentLineTemplate.RetrieveUrl>"
                                lazy url retrieval
                            display as link
                    paragraph
                        PickListPrintByShipmentBinLotSerialTemplate
                            select is WarehousePickListPrintByShipmentBinLotSerial
                        button of "<PickListPrintByShipmentBinLotSerialTemplate.Name>"
                            link is external "<PickListPrintByShipmentBinLotSerialTemplate.RetrieveUrl>"
                                lazy url retrieval
                            display as link
                    paragraph
                        PickListPrintByShipmentBinLotSerialRQBasedTemplate
                            select is WarehousePickListPrintByShipmentBinLotSerialRQ
                        button of "<PickListPrintByShipmentBinLotSerialRQBasedTemplate.Name>"
                            link is external "<PickListPrintByShipmentBinLotSerialRQBasedTemplate.RetrieveUrl>"
                                lazy url retrieval
                            display as link
                    paragraph
                        PickListPrintByBinItemUOMLotSerialTemplate
                            select is WarehousePickListPrintByBinItemUOMLotSerial
                        button of "<PickListPrintByBinItemUOMLotSerialTemplate.Name>"
                            link is external "<PickListPrintByBinItemUOMLotSerialTemplate.RetrieveUrl>"
                                lazy url retrieval
                            display as link
                    visible when (KitchenOrderInUse)
                        paragraph
                            KitchenOrderTemplate
                                select is KitchenOrderIDMTemplate
                            button of "<KitchenOrderTemplate.Name>"
                                link is external "<KitchenOrderTemplate.RetrieveUrl>"
                                display as link
                    visible when (AdvancedWarehouseInUse)
                        paragraph
                            DeliveryChecklistTemplate
                                select is DeliveryChecklistIDMTemplate
                            button of "<DeliveryChecklistTemplate.Name>"
                                link is external "<DeliveryChecklistTemplate.RetrieveUrl>"
                                    lazy url retrieval
                                display as link
            column2
                visible when (not UseIDM)
                    paragraph
                        BillOfLadingTemplate
                            display only
                            select is WarehouseBillOfLadingTemplate
                    paragraph
                        WarehouseProofOfDeliveryTemplate
                            display only
                            select is WarehouseProofOfDeliveryIDMTemplate
                    paragraph
                        WarehousePackingListTemplate
                            display only
                            select is WarehousePackingListIDMTemplate
                    paragraph
                        RequisitionTemplate
                            display only
                            select is RequisitionIDMTemplate
                    visible when (AdvancedWarehouseInUse)
                        paragraph
                            ShipmentLoadConsolidationTemplate
                                display only
                                select is ShipmentLoadConsolidationIDMTemplate
                visible when (UseIDM)
                    paragraph
                        BillOfLadingTemplate
                            select is WarehouseBillOfLadingTemplate
                        button of "<BillOfLadingTemplate.Name>"
                            link is external "<BillOfLadingTemplate.RetrieveUrl>"
                                lazy url retrieval
                            display as link
                    paragraph
                        WarehouseProofOfDeliveryTemplate
                            select is WarehouseProofOfDeliveryIDMTemplate
                        button of "<WarehouseProofOfDeliveryTemplate.Name>"
                            link is external "<WarehouseProofOfDeliveryTemplate.RetrieveUrl>"
                                lazy url retrieval
                            display as link
                    paragraph
                        WarehousePackingListTemplate
                            select is WarehousePackingListIDMTemplate
                        button of "<WarehousePackingListTemplate.Name>"
                            link is external "<WarehousePackingListTemplate.RetrieveUrl>"
                                lazy url retrieval
                            display as link
                    paragraph
                        RequisitionTemplate
                            select is RequisitionIDMTemplate
                        button of "<RequisitionTemplate.Name>"
                            link is external "<RequisitionTemplate.RetrieveUrl>"
                                lazy url retrieval
                            display as link
                    visible when (AdvancedWarehouseInUse)
                        paragraph
                            ShipmentLoadConsolidationTemplate
                                select is ShipmentLoadConsolidationIDMTemplate
                            button of "<ShipmentLoadConsolidationTemplate.Name>"
                                link is external "<ShipmentLoadConsolidationTemplate.RetrieveUrl>"
                                    lazy url retrieval
                                display as link
