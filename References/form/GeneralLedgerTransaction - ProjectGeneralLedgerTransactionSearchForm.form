ProjectGeneralLedgerTransactionSearchForm is a SearchForm
    title is "SearchTransactions"
    Layout
        four column
            FinanceCodeBlock.Project
                label is "<FinanceEnterpriseGroup.ProjectLabel>"
            FinanceCodeBlock.GeneralLedgerChartAccount
                label is "<FinanceEnterpriseGroup.AccountLabel>"
            System
            GeneralLedgerEvent
                label is "Event"
        four column distributed
            SearchDateRange.Begin
                label is "FromTransactionDate"
            SearchDateRange.End
                label is "ToTransactionDate"
            PostingDateRange.BeginDate
                label is "FromPostingDate"
            PostingDateRange.EndDate
                label is "ToPostingDate"
        four column distributed
            AccountingEntity
                label is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
            FinanceCodeBlock.AccountingUnit
                label is "<FinanceEnterpriseGroup.AccountingUnitLabel>"
            Status
        four column distributed
            Description
            ReportCurrencyAmount.ProjectAmount.EnteredCurrencyAmount
                label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Amount"
            FinanceCodeBlock.Project.Currency
                label is "Currency"
        four column distributed
            TransactionAmount
                label is "Amount"
            CurrencyCode
                label is "Currency"
            UnitsAmount
                label is "Units"
