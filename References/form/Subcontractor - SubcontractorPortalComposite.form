SubcontractorPortalComposite is a CompositeForm
    title is "Subcontractor<Subcontractor>_<SubcontractorName>"
    context form is SubcontractorPortalContext
        background is theme color
    Actions
        action is save
            hide when invalid
        action is save and new
            hide when invalid
        action is save and close
            hide when invalid
    CompanyInformation is a Panel
        form is SubcontractorPortalForm
    Diversity is a Panel
        list is SubcontractorDiversityRel.SubcontractorDiversityPortalList
    Contacts is a Panel
        list is SubcontractorContact set.SubcontractorContactPortalList
    Comments is a Panel
        list is SubcontractorComment set.SubcontractorCommentPortalList
    Certifications is a Panel
        list is SubcontractorCertification set.SubcontractorCertificationPortalList
    CommodityCodes is a Panel
        list is SubcontractorCommodityCode set.SubcontractorCommodityCodePortalList
            helper list is CommodityCodesRel.CommCodesSubcontractor
                action is AttachToSubcontractor
