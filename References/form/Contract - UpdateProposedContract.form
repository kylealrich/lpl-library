UpdateProposedContract is an ActionForm
    action is Draft.SupplierUpdateProposedContract
    Layout
        single column
            ParmName
                label is "ContractTitle"
            ParmDescription
                label is "Description"
            visible when (not UseIDMPerClassificationAndSubClass)
                ParmDocument.File
                    label is "ContractDocument"
            visible when (UseIDMPerClassificationAndSubClass)
                ParmIDMDocument.File
                    label is "ContractDocument"
                button of "ViewContractDocument"
                    link is external "<LatestRevisionRel.DerivedIDMDocumentURL>"
                    display as link
            ParmAttachment.File
                label is "AdditionalAttachment"
            ParmAttachment2.File
                label is "AdditionalAttachment"
            ParmEffectiveDate
                label is "EffectiveDate"
            ParmExpirationDate
                label is "ExpirationDate"
            ParmSubmit
                label is "SubmitProposedContract"
