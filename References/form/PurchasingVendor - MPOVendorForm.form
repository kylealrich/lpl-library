MPOVendorForm is a CompositeForm
    title is "Vendor<Vendor>PurchasingInformation"
    context form is POVendorFormContext
    restrict action Create
    restrict action Delete
    Actions
        action is refresh
            label is "_"
    Main is a Panel
        form is Main
    Options is a Panel
        form is MobileOptions
    ProcedureOutput is a Panel
        form is ProcedureInfoOutput
    PurchaseFromLocations is a Panel
        list is PurchaseFromLocationsRel.MPOPurchaseFromLocations
    CrossReferenceVendor is a Panel
        list is CrossReferenceVendorRel.MCrossReferenceVendorDetailList
    Attachments is a Panel
        title is "Attachments<DerivedPurchasingVendorAttachmentCount>"
        list is PurchasingVendorAttachment set.MAttachments
