InterfaceSelectedMatchInvoiceRunGroups is a Form
    is primary
    Layout
        two column
            SelectRunGroupsLike
            text of "EnterTextAndWildcards"
        single column
            ProcessingOption
        header2 of "Select"
        two column distributed
            PrmCompany
                label is "Company"
        two column distributed
            PrmVendorGroup
                label is "VendorGroup"
            PrmVendor
                label is "Vendor"
        visible when (ProcessingOption.DatabaseFile)
            two column distributed
                PrmProcessLevel
                    label is "ProcessLevel"
                PrmAuthorityCode
                    label is "AuthorityCode"
        header2 of "Options"
        two column distributed
            InvoiceProcess
                label is "InvoiceProcess"
            MatchPoint
                label is "MatchPoint"
        visible when (InvoiceProcess.AddAndMatch)
            text of "Note:AddAndMatchShouldNotBeUsedIfThePotentialExistsForInvoicesForTheSamePurchaseOrderToExistInDifferentRunGroups"
                bold
                color of red
        single column
            AutoCreateDetails
                label is "AutoCreateDetails"
                    left of field
        two column distributed
            DefaultProcessLevel
            DefaultAuthorityCode
        single column
            AllowMatchItemOrVendorItem
                label is "AllowMatchOnEitherItemOrVendorItem"
                    left of field
        two column distributed
            MoveErrorsToNewRunGroup
                label is "MoveErrorsToNewRunGroup"
                    left of field
        two column distributed
            visible when (ProcessingOption.DatabaseFile)
                IncludeInvoicesInError
                    label is "IncludeInvoicesInError"
                        left of field
            visible when (MoveErrorsToNewRunGroup)
                PrmErrorRunGroupPrefix
                    label is "ErrorRunGroupPrefix"
        visible when (ProcessingOption.CSVFile)
            two column distributed
                EnableGLNProcessing
                    label is "EnableGLNProcessing"
                        left of field
                EnableGTINProcessing
                    label is "EnableGTINProcessing"
                        left of field
            single column
                visible when (ProcessingOption.CSVFile)
                    RemoveExistingMatchInterfaceInvoices
                        label is "RemoveExistingDuplicateMatchInterfaceInvoices"
                            left of field
