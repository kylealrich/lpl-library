ReceiptAdjustmentEntry is a CompositeForm
    use for action Unreleased.CreateAdjustment
    context form is ReceiptAdjustmentEntryContext
        background is theme color
    Actions
        action is Unreleased.ReleaseAdjustment
            label is "Release"
            hide when invalid
        navigation is UnreleasedBuyerMessages
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is Unreleased.Delete
            label is "_"
        action is search
            action icon is filter
            label is "_"
        action is refresh
            label is "_"
    Lines1 is a Panel
        title is "Lines"
        visible when (not HasCatchWeightLines)
        template is POReceiptAdjLineRels
            business class is POReceiptAdjustmentAndInspectionLine
                list is ReceiptAdjustmentLines1
    Lines2 is a Panel
        title is "Lines"
        visible when (HasCatchWeightLines)
        template is POReceiptAdjLineRels
            business class is POReceiptAdjustmentAndInspectionLine
                list is ReceiptAdjustmentLines2
    Receiver is a Panel
        form is PO33Receiver
    Comments is a Panel
        title is "Comments<CommentCount>"
        list is POReceiptAdjustmentAndInspectionCommentRel.ReceiptAdjustmentCommentList
