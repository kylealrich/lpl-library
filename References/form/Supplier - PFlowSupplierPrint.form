PFlowSupplierPrint is a SummaryForm
    title is "SupplierInformation"
    Layout
        paragraph
            Supplier
            SupplierName
                no label
                display as text
        paragraph
            ParentSupplier
            ParentSupplierRel.SupplierName
                no label
                display as text
        two column
            DoingBusinessAs
            DandBNbr
                label is "Dun&Bradstreet"
        two column
            BusinessType
            Vendor
                label is "VendorID"
        two column
            paragraph
                TaxIdGroup.TaxIdType
                visible when (TaxIDTypeEntered)
                    text of "TaxIDTypeEntered"
                        italics
                visible when (not TaxIDTypeEntered)
                    text of "TaxIDTypeNotEntered"
                        italics
            paragraph
                visible when (TaxIDEntered)
                    text of "TaxIDEntered"
                        italics
                visible when (not TaxIDEntered)
                    text of "TaxIDNotEntered"
                        italics
        two column
            VATRegistrationCountry
            VATRegistrationNumber
        two column
            GlobalLocationNumber
            WebAddress
                label is "Website"
        blank line
        header3 of "MailingAddress"
        single column
            visible when (SupplierGroup.DisplayTypeForRegistration)
                MailingAddress
                    form is PostalAddressMinimumWithType
            visible when (not SupplierGroup.DisplayTypeForRegistration)
                MailingAddress
                    form is PostalAddressMinimum
        blank line
        header3 of "RemitToNameAndAddress"
        single column
            SameAsAddressAbove
        paragraph
            BillingFirstName
                label is "Name"
            BillingLastName
                no label
        single column
            visible when (SupplierGroup.DisplayTypeForRegistration)
                BillingAddress
                    form is PostalAddressMinimumWithType
            visible when (not SupplierGroup.DisplayTypeForRegistration)
                BillingAddress
                    form is PostalAddressMinimum
        header3 of "BankInformation"
        form is BankInformationFormNoMask
        visible when (DiversityCodesExist)
            list is SupplierDiversityResponse set.PFDiversityResponses
        visible when (CommentsExist)
            list is SupplierComment set.PFSupplierComment
        visible when (CertificationsExist)
            list is AllSupplierCertificationsRel.PFlowSupplierCertifications
        visible when (HasQuestions)
            list is SupplierQuestionResponseRels.SupplierResponses
        form is PflowSupplierContacts
