MobileCurrencyAmountsForm is a Form
    title is "CurrencyAmounts"
    Layout
        visible when (false)
            CompanyBaseData.BasePaymentAmount.AssignToCurrency
        blank line
        two column distributed
            CompanyBaseData.BasePaymentAmount.FunctionalAmount.EnteredCurrencyAmount
                label is "Functional"
                display as text
            CompanyBaseData.OverrideCompany.AccountingEntity.FunctionalCurrency
                no label
                display as text
        single column
            CompanyBaseData.BasePaymentAmount.FunctionalAmount.EnteredCurrencyRate
                no label
                display as text
        visible when (CompanyBaseData.OverrideCompany.AccountingEntity.AlternateCurrencyExists)
            two column distributed
                CompanyBaseData.BasePaymentAmount.AlternateAmount.EnteredCurrencyAmount
                    label is "Alternate"
                    display as text
                CompanyBaseData.OverrideCompany.AccountingEntity.AlternateCurrency
