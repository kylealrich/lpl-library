ElectronicPaymentFileCreationPrimary is a Form
    is primary
    Layout
        header2 of "Main"
        two column independent table
            column1
                paragraph
                    VendorGroup
                    VendorGroup.Description
                        no label
                        display as text
                paragraph
                    PayGroup
                        select is PayGroups
                    PayGroup.Name
                        no label
                        display as text
                paragraph
                    PaymentCode
                        select is CashPayments
                    PaymentCode.Description
                        no label
                        display as text
                paragraph
                    ProcGrp
                        label is "ProcessGroup"
                        select is ProcessGroups
                    ProcGrp.Description
                        no label
                        display as text
                paragraph
                    CashCode
                    CashCode.Description
                        no label
                        display as text
                paragraph
                    CombineOutputByAccount
                FileName
            column2
                blank line
                blank line
                blank line
                blank line
                blank line
                paragraph
                    CashCodeGrp
                        label is "OrCashCodeGroup"
                        select is CashCodeGroup
                    CashCodeGrp.Description
                        no label
                        display as text
                paragraph
                    SuppressNoPaymentError
        header2 of "SelectOptions"
        two column independent table
            column1
                paragraph
                    CheckDate
                        label is "PaymentDate"
                paragraph
                    StartTransNbr
                        label is "StartingNumber"
                paragraph
                    InclComments
                        label is "IncludeComments"
                paragraph
                    InclFaxEmail
                        label is "IncludeFax_andEmail"
            column2
                paragraph
                    EffectiveDate
                paragraph
                    DocCurrency
                        label is "DocumentCurrency"
                paragraph
                    UseAp150SortOpt
                        label is "UseCashRequirementsSortOptions"
        header2 of "RerunOptions"
        two column independent table
            column1
                paragraph
                    RecreateOption
                        label is "RecreateFile"
            column2
                paragraph
                    SerialNum
                        label is "SerialNumber"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                ElectronicPaymentCreationReport_RDG
                ElectronicPaymentEtebacRemit_RDG
                ElectronicPaymentErrorReport_RDG
            column2 of 25%
                ElectronicPaymentCreationReport_XPT
                ElectronicPaymentEtebacRemit_XPT
                ElectronicPaymentErrorReport_XPT
