EventResponse is a WizardForm
    title is "Event#<SourcingEvent.EventVersionString>"
    summary form is Submit
    show steps
    EventResponse is a Panel
        mouse over text is "<SourcingEvent.Company.HeaderResponseText>"
        panel header is "<SourcingEvent.Company.HeaderResponseText>"
        form is Response
        save on next
    LineResponses is a Panel
        valid when (DisplayBidAmountNoDateInWizard)
        mouse over text is "<SourcingEvent.Company.LineResponseText>"
        panel header is "<SourcingEvent.Company.LineResponseText>"
        template is SourcingEvent.SourcingEventLine set
            business class is SourcingEventLineResponse
                list is SupplierLineResponses
    EventLineResponses is a Panel
        valid when (DisplayBidAmountWithDateInWizard)
        mouse over text is "<SourcingEvent.Company.LineResponseText>"
        panel header is "<SourcingEvent.Company.LineResponseText>"
        template is SourcingEvent.SourcingEventLine set
            business class is SourcingEventLineResponse
                list is SupplierLineResponsesWithDate
    BestAndFinalLineResponses is a Panel
        valid when (CanEnterBestAndFinalAmounts)
        mouse over text is "<SourcingEvent.Company.LineResponseText>"
        panel header is "<SourcingEvent.Company.LineResponseText>"
        template is SourcingEvent.SourcingEventLine set
            business class is SourcingEventLineResponse
                list is SupplierLineResponses
    BidBondInformation is a Panel
        valid when (SourcingEvent.RequireBidBond)
        mouse over text is "<SourcingEvent.Company.BidBondText>"
        panel header is "<SourcingEvent.Company.BidBondText>"
        form is EnterBidBondNumber
    HeaderAOC is a Panel
        valid when (HeaderAOCsAndLineResponsesExist)
        panel header is "EnterAddOnChargePercentOrAmount"
        list is HeaderAddOnChargeResponseRel.SourcingEventResponseAOCsSupplier
    QuestionResponses is a Panel
        valid when (SEQuestionsOldExist)
        mouse over text is "<SourcingEvent.Company.QuestionResponseText>"
        panel header is "<SourcingEvent.Company.QuestionResponseText>"
        template is SourcingEventSupplierQuestionRel
            business class is SourcingEventQuestionResponse
                form is SupplierQuestion
    QuestionResponses2SP is a Panel
        valid when (SEQuestionsNewExist)
        mouse over text is "<SourcingEvent.Company.QuestionResponseText>"
        panel header is "<SourcingEvent.Company.QuestionResponseText>"
        template is SourcingEventSupplierQuestionPortalRel
            business class is SESupplierQuestionResponse
                form is SupplierQuestion
    ResponseAttachments is a Panel
        valid when (DisplayBid)
        mouse over text is "<SourcingEvent.Company.AttachmentResponseText>"
        panel header is "<SourcingEvent.Company.AttachmentResponseText>"
        list is SourcingEventResponseAttachment set.Attachments
