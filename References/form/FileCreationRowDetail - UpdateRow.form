UpdateRow is a CompositeForm
    title is "Row:<FileCreationSetup.Description>-<Description>"
    Actions
        action is save and close
            label is "Save"
    Detail is a Panel
        form is RowDetail
    Conditions is a Panel
        visible when (DisplayConditionsPanel)
        form is Conditions
    OneToMany is a Panel
        visible when (DisplayOneToManyConditionsPanel)
        form is OneToManyOptions
    RowsLinked is a Panel
        visible when (RowIsLinked)
        list is RowIsLinkedRel.RowsLinkedList
