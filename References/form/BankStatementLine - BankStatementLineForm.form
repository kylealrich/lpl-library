BankStatementLineForm is a CompositeForm
    is primary
    title is "BankStatementLine"
    context form is BankStatementLineContextForm
        background is theme color
    Detail is a Panel
        form is BankStatementLineDetailForm
    Distributions is a Panel
        visible when (CanViewDistributions)
        list is BankStatementLineDistribution set.StatementLineDistributionList
            helper list is CashAllocationCodeRel.CashAllocationCodesHelperList
                action is ApplyAllocationCode
                    invoked.PrmCashManagementAccount = CashManagementAccount
                    invoked.PrmBankStatement = BankStatement
                    invoked.PrmBankStatementLine = BankStatementLine
    CashLedgerTransaction is a Panel
        visible when (HasCashLedgerTransactionCreatedFromLine)
        list is CashLedgerTransaction set.StatementLineLevelCreatedFromBankStatementLine
    ReceivablePaymentBatch is a Panel
        visible when (HasReceivablePaymentHeaderCreatedFromLine)
        list is ReceivablePaymentHeader set.ReceivablePaymentHeaderList
    RelatedMatchedTransactions is a Panel
        visible when (StatementLinesLoadedIntoReconciliation)
        list is RelatedMatchedTransactionsRel.StatementLineRelatedTransactionsList
            helper list is OpenTransactionsAvailableToMatchRel.ReconciliationTransactionHelperList
                action is MatchAdditionalTransactions
    LineDetails is a Panel
        visible when (ShowLineDetails)
        list is BankStatementLineDetailRel.BankStatementLineDetails
    BudgetErrors is a Panel
        title is "BudgetErrors<DerivedHeaderBudgetErrorCount>"
        visible when (HeaderBudgetEditErrorExists)
        list is HeaderBudgetEditErrorRel.primary
    WarningBudgetErrors is a Panel
        title is "WarningBudgetErrors<DerivedWarningBudgetErrorCount>"
        visible when (DistributionBudgetEditErrorExists)
        list is DistributionBudgetEditErrorRel.primary
    ParsedData is a Panel
        visible when (ParsedDataUsed)
        form is ParsedData
