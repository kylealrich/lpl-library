CurrentTransactionDetailsContextForm is a Form
    title is "CurrentTransactionDetails"
    Layout
        paragraph
            StudentActivitySchool
                label is "School"
            StudentActivitySchool.SchoolName
                no label
            button of "PrintReport"
                link is PrintableCurrentTransactionDetails
        paragraph
            SchoolActivityAccount
                label is "Activity"
                compact format
            Description
                no label
                display only
        paragraph
            LatestClosePeriodEndingBalance
                label is "LastPeriodEndingBalance"
        paragraph
            CurrentReceipts
                display only
            CurrentDisbursements
                display only
            CurrentTransfers
                display only
        paragraph
            CurrentBalance
                label is "CurrentBalance"
