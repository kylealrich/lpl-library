ContractSubcontractorDiversityForm is a Form
    is primary
    title is "SubcontractorDiversity"
    Layout
        single column
            display as text
                ContractGroup
                Contract
                ContractSubcontractor
                PayablesDiversityCode
            DiversityPercent
        blank line
        display as text
            paragraph
                Contract.DerivedDiversityAmtFromPct
                    label is "ContractTotalDiversity:_"
                ContractSubcontractor.DerivedIsText
                    no label
                Contract.DiversityPctOfProposedTotal
                    no label
                ContractSubcontractor.DerivedOfText
                    no label
                Contract.ProposedTotalContractAmount
                    no label
            visible when (ContractSubcontractor.SubcontractorHasCommittedAmount)
                paragraph
                    ContractSubcontractor.DisplayCommittedAmount
                        label is "Subcontractor'sPortion:_"
                    ContractSubcontractor.DerivedIsText
                        no label
                    ContractSubcontractor.DerivedDiversityPctOfContract
                        no label
                    ContractSubcontractor.DerivedOfText
                        no label
                    Contract.DerivedDiversityAmtFromPct
                        no label
                paragraph
                    DisplayCommittedAmount
                        label is "DiversityPortion:_"
                    ContractSubcontractor.DerivedIsText
                        no label
                    DerivedDiversityPctOfContract
                        no label
                    ContractSubcontractor.DerivedOfText
                        no label
                    Contract.DerivedDiversityAmtFromPct
                        no label
