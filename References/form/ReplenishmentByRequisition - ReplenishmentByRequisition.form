ReplenishmentByRequisition is an ActionForm
    action is ReplenishmentByRequisition
    Layout
        header2 of "Main"
        paragraph
            Company
            Company.Name
                no label
                display as text
        blank line
        paragraph
            Location
            Location.Name
                no label
                display as text
        single column
            text of "\\or"
                bold
        two column
            ReportGrp
                label is "ReportGroup"
            ReportGrp.Name
                no label
                display as text
        header2 of "InventoryClass"
        two column
            paragraph
                InvenMajcl
                    label is "MajorInventoryClass"
                    show up to 10 characters
                InvenMajcl.Description
                    no label
                    display as text
            paragraph
                InvenMincl
                    label is "MinorInventoryClass"
                    show up to 10 characters
                InvenMincl.Description
                    no label
                    display as text
        blank line
        header2 of "_"
        text of "_"
        single column
            CreateUnrelDoc
                label is "UpdateOption"
            IncludeInactive
                label is "IncludeInactiveItems"
            PrintExceptionRpt
                label is "PrintExceptionReport"
            SortOption
            OverrideFromLoc
                label is "UseRequestingLocationAsFromLocation"
        header2 of "Filters"
        single column
            ReorderSunFl
                label is "Sunday"
            ReorderMonFl
                label is "Monday"
            ReorderTueFl
                label is "Tuesday"
            ReorderWedFl
                label is "Wednesday"
            ReorderThuFl
                label is "Thursday"
            ReorderFriFl
                label is "Friday"
            ReorderSatFl
                label is "Saturday"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                ReplenishmentByRequisitionReport_RDG
            column2 of 25%
                ReplenishmentByRequisitionReport_XPT
