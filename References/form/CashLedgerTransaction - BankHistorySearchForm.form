BankHistorySearchForm is a SearchForm
    title is "BankHistorySearch"
    Layout
        four column distributed
            CashCode
                label is "CashCode"
            TempCashCodeGroup
                label is "CashCodeGroup"
            PrmStatusAsOfDate
                label is "StatusAsOfDate"
            PrmTransStatus
                label is "TransactionStatus"
        four column distributed
            Company
            SourceCode
                label is "SystemCode"
            BankTransactionCode
                label is "TransactionCode"
            PrmDateOption
                label is "DateOption"
        four column distributed
            IssueDateRange.Begin
                label is "FromIssueDate"
            IssueDateRange.End
                label is "ToIssueDate"
            ReconcileDateRange.Begin
                label is "FromReconcileOrVoidDate"
            ReconcileDateRange.End
                label is "ToReconcileOrVoidDate"
        four column distributed
            IssuedBankAmountRange.FromAmount
                label is "FromIssuedBankAmount"
            IssuedBankAmountRange.ToAmount
                label is "ToIssuedBankAmount"
            IssuedBaseAmountRange.FromAmount
                label is "FromFunctionalAmount"
            IssuedBaseAmountRange.ToAmount
                label is "ToFunctionalAmount"
