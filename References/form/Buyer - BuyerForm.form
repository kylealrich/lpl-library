BuyerForm is a CompositeForm
    is primary
    title is "<DerivedFormTitle>"
    context form is BuyerFormContext
        background is theme color
    Main is a Panel
        form is Main
    Contact is a Panel
        form is Contact
    PurchasingClasses is a Panel
        list is BuyerPurchasingClass set.BuyerPurchasingClassses
    CompaniesAndLocations is a Panel
        title is "Companies_andLocations"
        list is BuyerLocation set.BuyerCompanyLocations
    VendorsAndLocations is a Panel
        title is "Vendors_andLocations"
        list is BuyerVendor set.BuyerVendors
