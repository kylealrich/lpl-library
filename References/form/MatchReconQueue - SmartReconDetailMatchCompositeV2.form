SmartReconDetailMatchCompositeV2 is a CompositeForm
    title is "MatchInvoice<Invoice>"
    context form is SmartReconDetailMatchContextOnLeftV2
        show context form on left
    Actions
        action is previous record
            label is "_"
            mouse over text is "PreviousRecord"
        action is next record
            label is "_"
            mouse over text is "NextRecord"
        action is refresh
            label is "_"
            mouse over text is "Refresh"
    InvoiceListWithDetails is a Panel
        title is "Exceptions_(<NumberInvoiceDetailsNotSelected>)"
        visible when (UnselectedLinesExist)
        panel text is "First,SelectThe_Receipt_LinesForEach_Invoice_Line._Next,SelectThe_Invoice_LineToAddTo_Working_Set."
        list is MatchWorkInvoiceDetailNotSelectedRel.UnselectedInvoiceLinesWithDetailSectionsV2
    UnselectedInvoices is a MultiListPanel
        title is "InvoicesAndReceipts"
        visible when (View.InvoiceMatch)
        list is NotSelectedInvoicesRel.SmartReconInvoiceNotSelectedListV2
        list is NotSelectedReceiptsRel.SmartReconReceiptNotSelectedListV2
    WorkingSet is a MultiListPanel
        visible when (AllowCreateSet)
        list is PendingSetInvoiceDetailRel.SmartReconInvoiceDetailSelectedListV2
        list is PendingSetReceiptLineRel.SmartReconReceiptLineSelectedListV2
    WorkingSet2 is a MultiListPanel
        title is "WorkingSet"
        visible when (AllowCreateInvoiceSet)
        list is PendingInvoicesRel.SmartReconInvoiceSelectedList
            visible when (ShowInvoiceMatch)
        list is PendingReceiptsRel.SmartReconReceiptSelectedList
            visible when (ShowInvoiceMatch)
    Sets is a Panel
        title is "SavedSets_(<NumberOfSets>)"
        valid when (HasSets)
        visible when (ShowDetailMatch)
        list is MatchReconQueueSetRel.MatchReconQueueSetsDetailMatchV2
    InvoiceSets is a Panel
        title is "SavedSets_(<NumberOfSets>)"
        valid when (HasSets)
        visible when (ShowInvoiceMatch)
        list is MatchReconQueueSetRel.MatchReconQueueSetsInvoiceMatch
    Messages is a Panel
        visible when (ShowMessagesPanel)
        quad width
        list is PayablesInvoice.OpenMatchInvoiceMessagesRel.MatchReconInvoiceMessageReview
    Chargebacks is a Panel
        visible when (ShowChargebackPanel)
        quad width
        list is PayablesInvoice.PayablesInvoiceChargebackRel.ChargebackMatchAnalysisList
