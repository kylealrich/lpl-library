BankInformation is a Form
    Layout
        two column
            column1
                header3 of "Bank"
                single column
                    BankEntity
                paragraph
                    CrossBorderIdentifier
                    SwiftID
                single column
                    BankName
                paragraph
                    BankAccountType
                        label is "AccountType"
                    GlobalBankAccountType
                header3 of "BankPostalAddress"
                BankPostalAddress
                    form is PostalAddressMinimumWithType
            column2
                header3 of "IntermediaryBank"
                single column
                    IntermediaryBankEntity
                paragraph
                    IntermediaryCrossBorderIdentifier
                    IntermediarySwiftID
                single column
                    IntermediaryBankName
                blank line
                header3 of "IntermediaryBankPostalAddress"
                IntermediaryBankPostalAddress
                    form is PostalAddressMinimumWithType
