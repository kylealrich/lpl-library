ICBSettlementGLTransactionDetailForm is a Form
    is primary
    title is "IntercompanyBillingSettlementDistribution"
    Layout
        display as text
            three column independent
                column1
                    IntercompanyBillingSettlementHeader.SettlementID
                        label is "SettlementID"
                    GLTransactionDetail.GLTransactionDetail.DocumentNumber
                    GLTransactionDetail.GLTransactionDetail.AccountingEntity
                        label is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
                column2
                    GLTransactionDetail.GLTransactionDetail.PostingDate
                    GLTransactionDetail.GLTransactionDetail.TransactionDate
                column3
                    GLTransactionDetail.GLTransactionDetail.FinanceCodeBlock
                        label is "FinanceStructure"
                        compact format
                    GLTransactionDetail.GLTransactionDetail.Description
            blank line
            header2 of "CurrencyAmounts"
            blank line
            two column
                column1
                    text of "Amount"
                    GLTransactionDetail.GLTransactionDetail.TransactionAmount
                        label is "Transaction"
                        display only
                column2
                    text of "Currency"
                    GLTransactionDetail.GLTransactionDetail.CurrencyCode
                        no label
                        display only
            GLTransactionDetail.GLTransactionDetail.ReportCurrencyAmount
