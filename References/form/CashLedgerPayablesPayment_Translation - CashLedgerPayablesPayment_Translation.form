CashLedgerPayablesPayment_Translation is a Form
    is primary
    Layout
        paragraph
            Base_BusinessClass.CashLedgerPayablesPayment
                display as text
            Base_BusinessClass.CashCodeDisplay
                no label
                display as text
            Base_BusinessClass.TransactionCodeDisplay
                no label
                display as text
            Base_BusinessClass.TransactionNumberDisplay
                no label
                display as text
        single column
            IsoLocale
                compact format
                raise yellow alert when (VerifyTranslations)
                    mouse over text is untranslatable:"The base data has been changed but this translation has not been verified"
        single column
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.InvoiceVendorName
                label is untranslatable:"Invoice Vendor Name"
            InvoiceVendorName
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.PaidName
                label is untranslatable:"Paid Name"
            PaidName
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.MailingName
                label is untranslatable:"Mailing Name"
            MailingName
                label is untranslatable:"Translated"
