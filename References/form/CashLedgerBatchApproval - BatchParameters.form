BatchParameters is a Form
    Layout
        paragraph
            visible when (BatchApprovalType.CashLedgerPayment)
                CashLedgerPaymentGroup
            visible when (BatchApprovalType.CashLedgerReceipt)
                CashLedgerReceiptGroup
            visible when (BatchApprovalType.ElectronicFundsTransaction)
                CashLedgerEFTTransactionGroup
        visible when (not BatchApprovalType.CashLedgerReceipt)
            paragraph
                CashManagementAccount
                CashManagementAccount.Title
                    no label
                    display as text
        paragraph
            CashCode
            CashCode.Description
                no label
                display as text
        paragraph
            BankTransactionCode
            BankTransactionCode.Description
                no label
                display as text
        paragraph
            GeneralLedgerSourceCode
            GeneralLedgerSourceCode.Description
                no label
                display as text
        blank line
        paragraph
            ApprovalCostCenter
            ApprovalCategory
        blank line
        paragraph
            DateRange.Begin
                label is "PostDate"
            text of "_-"
            DateRange.End
                no label
        paragraph
            SearchAmountRange.Begin
                label is "Amount"
            text of "_-"
            SearchAmountRange.End
                no label
        blank line
        single column
            CommentText
