WireTransferOutputForm is a CompositeForm
    is primary
    title is "WireTransferOutput"
    context form is WireTransferOutputContextForm
        background is theme color
    Detail is a Panel
        list is WireTransferOutputDetail set.WireTransferOutputList
    KyribaConnection is a Panel
        visible when (IsKyribaIntegration)
        form is KyribaConnectionResponseForm
    GTreasuryConnection is a Panel
        visible when (IsGTreasuryIntegration)
        form is GTreasuryConnectionResponseForm
