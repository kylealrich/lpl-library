        single column
            FinanceEnterpriseGroup
        two column
            column1
                DspName
                    label is "SelectionName"
                    select is MassDisposalSelections
                Release
                    label is "Update"
                paragraph
                    DspProperty
                        label is "Property"
                        select is DisposalProperty
                    DspProperty.Description
                        no label
                        display as text
            column2
                DisposalDate
                    label is "Date"
                AsdDspMethod
                    label is "Type"
                Voluntary
        blank line
        two column
            column1
                text of "Currency"
                    bold
                CurrencyCode
                    label is "Proceeds"
            column2
                text of "Amount"
                    bold
                Proceeds
                    no label
        two column
            column1
                CurrencyCode1
                    label is "Expense"
            column2
                CostSales
                    no label
        two column
        single column
            AllocateOption
                label is "AllocationOption"
        paragraph
            Posting
                label is "PostingEntries"
            PostingDate
        header2 of "OutputOptions"
        header3 of "PrintTotals"
        header4 of "Sequence"
        paragraph
            LocOption
                label is "Location"
            DivOption
                label is "Division"
        paragraph
            TypeOption
                label is "Type\\,Sub"
            AssetOption
                label is "Asset"
            blank line
        header4 of "Miscellaneous"
        single column
            PropOption
                label is "Property"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                AssetMassDisposalsReport_RDG
            column2 of 25%
                AssetMassDisposalsReport_XPT
