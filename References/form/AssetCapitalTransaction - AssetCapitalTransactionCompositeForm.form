AssetCapitalTransactionCompositeForm is a CompositeForm
    is primary
    propagate as a drill
    title is "Transaction_<AccountingEntity>_<JournalizeGroup>"
    context form is AssetCapitalTransactionForm
    MainPanel is a Panel
        title is "Main"
        form is AssetCapitalTransactionMainForm
    AmountsPanel is a Panel
        title is "CurrencyAmounts"
        form is AssetCapitalTransactionAmountsForm
    ProjectPanel is a Panel
        title is "ProjectRelated"
        form is AssetCapitalTransactionProjectForm
    UserFieldsPanel is a Panel
        title is "UserFields"
        form is UserFieldsForm
