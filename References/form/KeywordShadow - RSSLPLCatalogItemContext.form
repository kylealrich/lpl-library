RSSLPLCatalogItemContext is a Form
    title is "CatalogItem-<DerivedItem>Details"
    Layout
        display as text
            header2 of "Item"
            three column distributed
                column1
                    paragraph
                        ItemGroup
                        ItemGroup.Description
                            no label
                            display as text
                    paragraph
                        visible when (RSSLPLDisplayFromCompany)
                            DefaultFromCompany
                                label is "DeliverFromCompany"
                            FromCompanyRel.Name
                                no label
                    paragraph
                        ItemDescription
                            label is "Description"
                column2
                    paragraph
                        DerivedItem
                            label is "Item"
                    paragraph
                        visible when (RSSLPLDisplayFromLocation)
                            DefaultFromLocation
                                label is "DeliverFromLocation"
                            FromCompanyRel.Name
                                no label
                    paragraph
                        Item.Description2
                            label is "_"
                column3
                    paragraph
                        DefaultItemType
                            label is "ItemType"
                    paragraph
                        BlankField
                    paragraph
                        Item.Description3
                            label is "_"
