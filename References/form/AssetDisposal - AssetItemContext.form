AssetItemContext is a Form
    Layout
        two column
            column1
                paragraph
                    Asset
                    AssetRel.Description
                        no label
                        display as text
            column2
                DisposalStatus
                    label is "Status"
                    display as text
        two column
            column1
                paragraph
                    AssetRel.TagNumber
                        label is "Tag"
                        display as text
                paragraph
                    AssetGroup
                        label is "AssetGroup"
                        display as text
                    AssetGroup.Description
                        no label
                        display as text
            visible when (AssetRel.LeasedAsset)
            paragraph
                AssetLease.LeaseCompany
                    label is "LeaseCompany"
                    display as text
                AssetLease.LeaseCompany.Name
                    no label
                    display as text
            paragraph
                AssetLease.Lease
                    display as text
                AssetLease.Lease.Description
                    no label
                    display as text
        two column
            column1
                TotalItemQuantity
            column2
                DisposalQuantity
