CascadeDeleteByRangeForSpecifiedActor is an ActionForm
    action is CascadeDeleteByRangeForSpecifiedActor
    title is "CascadeDeleteByRangeForSpecifiedUser"
    Layout
        header2 of "DeleteJobExecutionRecordsForUser'sJobsFinishedWithinSpecifiedDateRange_(FinishedStatusOnly)"
        single column
            blank line
            Actor
                label is "User"
            blank line
            text of "Job_\\Execution_\\Completion_(\\Finished)_\\Date_\\Range:"
                bold
                align as label
            blank line
        three column grid
            column1
                DateRangeWithOffset.BeginDate
                    label is "BeginDate"
                blank line
                blank line
            column2
                text of "Through:"
                    right align
                text of "Or"
                    right align
                text of "Or"
                    right align
            column3
                DateRangeWithOffset.DateRangeEndDate.EndDate
                    label is "EndDate"
                DateRangeWithOffset.DateRangeEndDate.OffsetDays
                    label is "DaysPriorToCurrentDate"
                DateRangeWithOffset.DateRangeEndDate.CurrentDate
                    label is "CurrentDate"
                        left of field
        blank line
        single column
            text of untranslatable:"<CascadeDeleteConfirmationsMessage>"
                bold
                align as label
