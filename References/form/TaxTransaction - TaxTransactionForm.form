TaxTransactionForm is a CompositeForm
    is primary
    title is "TaxTransaction"
    context form is TaxTransactionFormContext
        background is theme color
    InvoiceInformation is a Panel
        form is InvoiceInformation
    SystemInformation is a Panel
        form is SystemInformation
    PostingInformation is a Panel
        form is PostingInformation
    OneSourceMessages is a Panel
        visible when (OneSourceInError)
        form is ShowSabrixMessagesForm
    WebserviceResults is a Panel
        visible when (ShowWebserviceResults)
        form is WebserviceResultsForm
    UserAttributes is a Panel
        visible when (OutputUserAttributesExist)
        list is OutputUserAttributesRel.primary
    BaseAmounts is a Panel
        visible when (TaxLogForFixBaseAmountsExist)
        form is BaseAmountsConversionForm
    TaxLogForFixBaseAmounts is a Panel
        visible when (TaxLogForFixBaseAmountsExist)
        list is TaxLogForFixBaseAmountsRel.TaxTransactionBaseAmountsReportList
