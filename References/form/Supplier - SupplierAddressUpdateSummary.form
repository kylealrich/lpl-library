SupplierAddressUpdateSummary is a Form
    Layout
        header2 of "AddressInformation"
        single column
            display as text
                visible when (not SameAsAddressAbove)
                    text of "MailingAddress"
                        bold
                visible when (SupplierGroup.DisplayTypeForRegistration)
                    MailingAddress
                        form is PostalAddressMinimumWithType
                visible when (not SupplierGroup.DisplayTypeForRegistration)
                    MailingAddress
                        form is PostalAddressMinimum
                visible when (not SameAsAddressAbove)
                    blank line
                    text of "RemitToAddress"
                        bold
                    visible when (SupplierGroup.DisplayTypeForRegistration)
                        BillingAddress
                            form is PostalAddressMinimumWithType
                    visible when (not SupplierGroup.DisplayTypeForRegistration)
                        BillingAddress
                            form is PostalAddressMinimum
