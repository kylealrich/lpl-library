WireTransferBatchContext is a Form
    title is "WireTransfer"
    Layout
        visible when (IsNotNew)
            two column distributed
                display as text
                    column1
                        paragraph
                            FinancialInstitution.FinancialInstitutionAndDescription
                                label is "FinancialInstitution"
                        paragraph
                            FinancialInstitutionBranch.FinancialInstitutionBranchAndDescription
                                label is "FinancialInstitutionBranch"
                        single column
                            TransferType
                            Description
                    column2
                        paragraph
                            TransactionDate
                            visible when (WireTransferBatch entered)
                                DerivedStatus
                        PostingDate
                        paragraph
                            FileName
                            visible when (WireTransferBatch entered)
                                FileNameDisplay
                                    no label
                        visible when (FinancialInstitutionBranch entered)
                            OutputFileType
                        visible when (CashManagementGroup.WireTransferApprovalRequired)
                            ApprovalCode
        visible when (IsNew)
            two column distributed
                column1
                    paragraph
                        FinancialInstitution
                        FinancialInstitution.Description
                            no label
                            display as text
                    paragraph
                        FinancialInstitutionBranch
                        FinancialInstitutionBranch.Description
                            no label
                            display as text
                    single column
                        TransferType
                        Description
                    blank line
                    paragraph
                        visible when (BudgetEditInProgress)
                            text of "BudgetEditsInProgress"
                                bold
                                color of red
                        visible when (BudgetEditFailure)
                            text of "BudgetsExceeded;CouldNotReleaseBatch;SeeBudgetErrorsTab"
                                bold
                                color of red
                column2
                    paragraph
                        TransactionDate
                        visible when (WireTransferBatch entered)
                            DerivedStatus
                                display as text
                    PostingDate
                    paragraph
                        FileName
                        visible when (WireTransferBatch entered)
                            FileNameDisplay
                                no label
                                display as text
                    visible when (FinancialInstitutionBranch entered)
                        OutputFileType
                    visible when (CashManagementGroup.WireTransferApprovalRequired)
                        ApprovalCode
