ReceivingAndReturns is a Form
    Layout
        header2 of "Receiving"
        two column distributed
            column1
                AllowReceiveAll
                AllowMultipleUnreleasedReceipts
            column2
                DisplayRemainingQuantity
                PrintQuantity
        two column distributed
            column1
                LastPurchaseOrderReceipt
                OvershipmentBudgetEdit
                DisableOvershipmentMessages
                UndershipmentTolerancePercentage
                    label is "UndershipmentCancelPercentage"
                PackageTrackingLevel
                visible when (PackageTrackingLevel.EnableMscmRadTracking)
                    PrintPutawayAndDeliveryTicket
            column2
                LastReceiptAdjustmentAndInspection
                CustomerOrderOvershipmentMessages
                    label is "MessageForExcessOnCustomerOrderPurchases"
                SubstituteMessages
                    label is "MessageForSubstitutes"
                OvershipmentTolerancePercentage
                    label is "OvershipmentPercentage"
                DefaultNumberOfLabelsTo1
                    label is "NumberOfLabelsDefault=1"
        blank line
        header2 of "VendorReturn"
        two column distributed
            column1
                RequireReasonCodeOnVendorReturn
                LastVendorReturn
            column2
                ReturnStockToInventoryOnVendorReturnCancel
                    label is "ReturnStockToInventoryOnCancel"
