EditorForm is a CompositeForm
    use for action EditReport
    title is "ConfiguredReport:<Name>"
    context form is ConfiguredContextForm
    restrict action View
    Filter is a Panel
        visible when (not IsPage)
        form is ReportInstanceSelectionForm
    Display is a Panel
        form is DisplayOptionsForm
    DataSource is a Panel
        form is AdvancedOptionsForm
    EditLPL is a Panel
        form is ConfiguredEditorForm
    ApplicationReport is a Panel
        form is ApplicationReportForm
    Description is a Panel
        valid when (HasUserListRecord)
        visible when (IsList)
        form is UpdateDescriptionForm
    PageDescription is a Panel
        title is "Description"
        valid when (HasUserPageRecord)
        visible when (IsPage)
        form is UpdatePageDescriptionForm
    LPLHistory is a Panel
        valid when (HasConfigEntityRecord)
        list is ConfigEntityRel.ConfigEntityHistory set.ConsoleLPLChangesList
    StatusHistory is a Panel
        valid when (HasConfigEntityRecord)
        list is ConfigEntityRel.ConfigEntityHistory set.ConsoleStatusChangesList
