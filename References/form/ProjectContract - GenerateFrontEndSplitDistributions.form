GenerateFrontEndSplitDistributions is an ActionForm
    action is GenerateFrontEndSplitDistributions
    Layout
        single column
            header2 of "SelectionCriteria"
            paragraph
                PrmAccountingEntity
                    label is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
                PrmAccountingEntityGroup
                    label is "Or<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>Group"
            paragraph
                PostingDateRange.Begin
                    label is "PostingDateSelectionRange"
                PostingDateRange.End
                    label is "Through"
            TransactionThroughDate
            header2 of "DistributionCriteria"
            PostingDate
            visible when (false)
                Journalize
                    label is "JournalizeImmediately"
            visible when (FinanceEnterpriseGroup.FundAccounting)
                PrmRunCashOffset
                PrmCashOffsetAccount
