WorkOrderScheduleDetail is a Form
    is primary
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            InventoryLocation
                label is "Location"
            InventoryLocation.Name
                no label
                display as text
        paragraph
            Item
                label is "FinishedGood"
            Item.Description
                no label
                display as text
        paragraph
            Item.StockUOM
                label is "UOM"
                display as text
        paragraph
            WorkOrderDataScheduleHeader.HeaderObjectID
                display as text
            WorkOrderScheduleDetail
                label is "WorkOrderScheduleDetail"
        paragraph
            StartDate
        paragraph
            EndDate
        paragraph
            Quantity
        paragraph
            WorkOrderNumber
