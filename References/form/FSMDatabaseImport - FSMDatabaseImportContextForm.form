FSMDatabaseImportContextForm is a Form
    title is "DatabaseImport"
    Layout
        paragraph
            FSMDatabaseImport
                display as text
            LastRunDate
                display as text
        visible when (FSMDatabaseImportControlExists)
            paragraph
                FSMDatabaseImportControlRel.Status
                    display as text
                FSMDatabaseImportControlRel.NumberOfProcesses
                    display as text
