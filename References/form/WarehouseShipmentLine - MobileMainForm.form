MobileMainForm is a Form
    title is "Main"
    Layout
        header3 of "_"
        two column distributed
            WarehouseShipmentLine
                label is "Line"
                display as text
            Item
                label is "Item"
                display as text
            visible when (IsHSNSACCodeEnabled)
                HSNSACCode
            WarehouseShipment.WarehouseBatch
                label is "BatchNumber"
                display as text
            Item.Description
                label is "Description"
                display as text
            visible when (IsHSNSACCodeEnabled)
                HSNSACCode.Description
                    no label
                    display as text
        single column
            DerivedPrintedQuantityInTransUOM
                display as text
            DerivedShippedQuantityInTransUOM
                display as text
            OpenQuantity
                label is "OpenQuantity"
                display as text
        two column distributed
            DerivedInProcessQuantityInTransUOM
                label is "InProcessQuantity"
                display as text
            TransactionUOM
                label is "UOM"
                display as text
        single column
            visible when (IsKitItem)
                TransientPreAssembledQuantity
                    label is "PreassembledKits"
            visible when (HasBackorder)
                TotalBackorderQuantity
                    label is "BackorderQuantity"
            visible when (HasVoid)
                TotalVoidedQuantity
                    label is "VoidedQuantity"
        header3 of "Options"
        single column
            MSDSRequired
                label is "MSDSRequired"
            MSDSDate
            MSDSDocument
            MSDSVersion
        header3 of "Intrastat"
        single column
            SupplementaryUOM
                label is "SupplementalU\\O\\M"
                display as text
            SupplementaryQuantity
                label is "SupplementalQuantity"
                display as text
            GlobalLineType
                label is "LineType"
