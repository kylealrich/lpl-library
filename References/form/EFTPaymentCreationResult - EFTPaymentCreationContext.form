EFTPaymentCreationContext is a Form
    Layout
        three column distributed
            column1
                paragraph
                    FinanceEnterpriseGroup.RepresentativeText
                        display as text
                paragraph
                    EftCompany.RepresentativeText
                        label is "ProcessingCompany"
                        display as text
                    EftCompany.Currency
                        label is "Currency"
                        display as text
                blank line
                RunTime
                    display as text
                create stamp.actor
                    label is "User"
                    display as text
                Status
                    display as tag
                        color of green when (Status.Complete)
                        color of gray when (Status.InProcess)
            column2
                text of "Parameters:"
                    bold
                    align as label
                single column
                    PostingDate
                        label is "PostDate"
                        display as text
                    DepositDate
                        display as text
                paragraph
                    ProcessLevel.RepresentativeText
                        label is "PaymentProcessLevel"
                        display as text
                paragraph
                    DiscReasCd.ReasonRepresentativeText
                        label is "DiscountReasonCode"
                        display as text
                button of "ShowMore..."
                    sub form is MorePaymentCreationParameters
                    display as link
            column3
                paragraph
                    TransientIncludeSummarizeDetailPanel
        visible when (not IsEFTPaymentOutputCreated)
            text of "NoRecordsToProcess"
                bold
            blank line
