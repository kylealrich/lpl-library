CashManagementAccountImportParameters is a Form
    title is "Import"
    Layout
        single column
            ImportAccountNumber
            NoBankStatementsExpected
            AllowLineCreationOnImportedStatements
        two column
            CreateReceivablePaymentsFromCAMT053
            visible when (CreateReceivablePaymentsFromCAMT053)
                ReceivableProcessLevel
        two column
            CreateReceivablePaymentThirdPartyPaymentProcessing
            visible when (CreateReceivablePaymentThirdPartyPaymentProcessing)
                ThirdPartyPaymentProcessingReceivableProcessLevel
        single column
            DefaultTransactionDate
        paragraph
            ImportNotification.FinanceResource
                label is "ImportNotification:_Resource"
            text of "Or"
            ImportNotification.FinanceTeam
                label is "Team"
            visible when (ImportNotification.FinanceResource entered)
                ImportNotification.FinanceResource.PreferredFirstAndLastName
                    no label
                    display as text
            visible when (ImportNotification.FinanceTeam entered)
                ImportNotification.FinanceTeam.Description
                    no label
                    display as text
