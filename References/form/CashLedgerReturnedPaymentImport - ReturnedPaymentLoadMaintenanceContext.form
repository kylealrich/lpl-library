ReturnedPaymentLoadMaintenanceContext is a Form
    title is "ReturnedPaymentInterfaceMaintenance"
    Layout
        visible when (RecordExists)
            three column distributed
                column1
                    display as text
                        CashManagementGroup
                        CashLedgerReturnedPaymentImport.BankTransactionCode.BankTransactionCodeName
                            label is "TransactionCode"
                column2
                    display as text
                        CashLedgerReturnedPaymentImport.RunGroup
                            label is "RunGroup"
                        paragraph
                            CashLedgerReturnedPaymentImport.TransactionNumber
                column3
                    display as text
                        paragraph
                            CashLedgerReturnedPaymentImport.CashCode.CashCodeAndDescription
                                label is "CashCode"
                            CashLedgerReturnedPaymentImport.CashCode.Currency
                        CashLedgerReturnedPaymentImport.SequenceNumber
            single column
                display as text
                    ErrorMessage
            button of "InterfacedReturnedPaymentRecord"
                visible when (IsPartiallyImported)
                link is CashLedgerReturnedPaymentRecordNavigation
                display as link
