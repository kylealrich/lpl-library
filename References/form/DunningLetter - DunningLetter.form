DunningLetter is a Form
    is primary
    title is "AdvancedDunningLetter_forDunningProcessCode<DunningLetter.DunningProcessCode>"
    Layout
        two column distributed
            column1
                paragraph
                    DunningLetter.DunningProcessCode
                        label is "AdvancedDunningProcessCode"
                        select is DunningProcessingCodes
                    DunningLetter.DunningProcessCode.Description
                        no label
                        display as text
                paragraph
                    DunningLetter.DunningLevel
                paragraph
                    DaysBetweenDunningLevels
                paragraph
                    IsoLocale
                        label is "Locale"
                        compact format
                    IsoLocale.LocaleName
                        no label
                        display as text
                paragraph
                    DunningTextCode
                        label is "AdvancedDunningTextCode"
                        select is AdvancedDunningTextCodeSelect
            column2
                paragraph
                    DunningFeeAmount
                paragraph
                    DunningExtractIndicator
                paragraph
                    SendToCreditAgency
                paragraph
                    SendTransactionsToCreditAgency
                paragraph
                    DunningFeePrintLocation
