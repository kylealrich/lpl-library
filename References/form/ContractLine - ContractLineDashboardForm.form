ContractLineDashboardForm is a CompositeForm
    title is "Contract<Contract.ContractNbr>Line<ContractLine>Details"
    context form is ContractLineContext
        background is theme color
    restrict action Active.CreateEventWorksheet
    restrict action Active.CreateEventWorksheetByContract
    restrict action Active.UpdateInvoiceDetailsFromContractLine
    restrict action CompareForecasts
    restrict action NegotiateContracts
    restrict action SelectSpecialItemForItemCreation
    restrict action SelectSpecialItemsOnContract
    restrict action CalculateNumberOfTimesPurchased
    restrict action CreateFromWizard
    restrict action IncludeInProcurementTemplateLine
    restrict action KeepEnteredItem
    restrict action Draft.SelectDraftItemForRebate
    restrict action Active.SelectActiveItemForRebate
    restrict action Amendment.SelectAmendmentItemForRebate
    restrict action BuildContractComplianceInvoiceLines
    restrict action SelectAllItemsForRebateQualification
    restrict action CountContractLine
    restrict action CreateFromContractLine
    restrict action Create
    restrict action CreateWithTierCosts
    restrict action CheckForContractLineErrors
    restrict action CloseLine
    restrict action HoldContractLine
    restrict action Delete
    restrict action BackorderSpecialItem
    Details is a MultiListPanel
        list is ContractLineOtherErrorRel.ContractLineErrorList
            visible when (OtherErrorsExist)
        list is PoLineRel.POLinesFromAContract
            visible when (HasPoLine)
        list is MatchedOrUnreleasedPayablesInvoiceDetailRel.InvoiceDetailsForAContract
            visible when (InvoicesForContractLine)
        list is PayablesInvoiceDistributionRel.DistributionsForAContract
            visible when (CommittedInvoicesForContractLine)
        list is RequisitionLinesByContractRel.RequisitionLinesFromAContract
            visible when (RequisitionLinesByContract)
        list is DistributorContractLineQuantityOrderedRel.LineQuantityOrdered
            visible when (DistributorContractLineQuantityOrderedRelExists)
        list is ContractPolineDistributionsByContractRel.DistributionsForAContract
            visible when (POCommitmentsExist)
        list is RequisitionLineDistributionsByContractRel.DistributionsForAContract
            visible when (RQCommitmentsExist)
        list is ContractPreferredManufacturerRel.ContractPreferredManufacturerForItem
            visible when (HasPrefMan)
        list is OtherPurchaseOrderLineForItemRel.OffContractPurchaseOrderLines
            visible when (OffContractItemPurchases)
    Amounts is a Panel
        visible when (HasAmounts)
        form is LineAmounts
