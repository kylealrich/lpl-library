    Layout
        header2 of "Bin_Reorder_Policy"
        display as text
            single column
                ReorderQuantityCode
                ReorderPoint
                ReorderQuantity
        header2 of "Warehouse_Storage_Information"
        display as text
            two column distributed
                column1
                    text of "From"
                        header4
                        bold
                    ReplenishFromBin.WarehouseStorageLocation.WarehouseStorageLocationIDName
                        label is "Storage_Location"
                    paragraph
                        ReplenishFromBin.WarehouseStorageLocation.DerivedWarehouseZoneAndDescription
                        ReplenishFromBin.WarehouseStorageLocation.DerivedZoneComponentAisle
                        ReplenishFromBin.WarehouseStorageLocation.DerivedWarehouseZoneComponentAndDescription
                        ReplenishFromBin.WarehouseStorageLocation.DerivedWarehouseZoneComponentTypeAndDescription
                        ReplenishFromBin.WarehouseStorageLocation.Row
                column2
                    text of "To"
                        header4
                        bold
                    Bin.WarehouseStorageLocation.WarehouseStorageLocationIDName
                        label is "Storage_Location"
                    paragraph
                        Bin.WarehouseStorageLocation.DerivedWarehouseZoneAndDescription
                        Bin.WarehouseStorageLocation.DerivedZoneComponentAisle
                        Bin.WarehouseStorageLocation.DerivedWarehouseZoneComponentAndDescription
                        Bin.WarehouseStorageLocation.DerivedWarehouseZoneComponentTypeAndDescription
                        Bin.WarehouseStorageLocation.Row
