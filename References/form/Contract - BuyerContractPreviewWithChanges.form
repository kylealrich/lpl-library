BuyerContractPreviewWithChanges is a CompositeForm
    title is "TermsAndConditionsNegotiation"
    context form is ContractContextShortNegotiation
        background is theme color
    restrict action SelectContractForRebate
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    ArticlesAndTerms is a Panel
        list is NonDeletedContractArticlesByDisplayRel.ContractArticleNegotiation
    ActiveNegotiationVersions is a Panel
        visible when (HasActiveContractTermVersion)
        list is ActiveContractTermVersionRel.ContractTermVersionsWithUpdates
    ActivatedVersions is a Panel
        visible when (HasActivatedContractTermVersions)
        list is ActivatedContractTermVersionRel.ActivatedContractTermVersions
