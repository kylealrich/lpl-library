BankStatementInterfaceHeaderComposite is a CompositeForm
    is primary
    title is "BankStatementInterface"
    context form is BankStatementInterfaceHeaderForm
        background is theme color
    Actions
        action is NotImported.Import
            label is "Import"
            mouse over text is "Import"
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    BAI2Data is a Panel
        visible when (BAIFormat)
        list is BankStatementInterface set.BAI2List
    MT940Data is a Panel
        visible when (FileFormat.MT940)
        list is BankStatementInterface set.MT940List
    MT942Data is a Panel
        visible when (FileFormat.MT942)
        list is BankStatementInterface set.MT940List
    CSVData is a Panel
        visible when (CSVFormat)
        list is BankStatementInterface set.CSVList
    EDI822Data is a Panel
        visible when (EDI822Format)
        list is BankStatementInterface set.EDI822List
    CAMT05XData is a Panel
        title is "<FileFormat>Data"
        visible when (CAMT05XFormat)
        list is BankStatementInterface set.CAMT05XList
    Attachment is a Panel
        form is ImportData
    ValidationErrors is a Panel
        title is "Errors"
        visible when (HasValidationErrors)
        list is BankStatementInterfaceHeaderErrorRel.BankStatementInterfaceHeaderErrors
