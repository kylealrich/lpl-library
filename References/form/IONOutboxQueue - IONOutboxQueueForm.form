IONOutboxQueueForm is a Form
    is primary
    title is "IONQueue"
    Layout
        single column
            IONConnection
                select is IONOutboxConnectionsRel.IONConnections
        two column
            visible when (IONConnection.HasActions)
                IONConnectionAction.Name
                    label is "Action"
                IONConnectionAction.Version
        single column
            FromLogicalId
            ToLogicalId
            BodType
            MessageId
            button of "OpenMessage"
                view BodXML
                    using mime type MessageMimeType
            visible when (IsIMS)
                IONMessageType
            visible when (IsReplication)
                ReplicationSet
                RepSetBC
            ExtendedStatus
            ExecuteTime
            visible when (HasFailures)
                FailureCount
            visible when (HasErrorMessage)
                ErrorMessage
                button of "ViewErrorSummary"
                    link is FailureExceptionSummaryFormNavigation
            visible when (IsIOBox)
                OutboxId
            IONAdditionalProperties
