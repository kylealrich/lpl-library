CashLedgerFundTransferDetailForm is a CompositeForm
    is primary
    title is "Detail<CashLedgerFundTransferHeader.DerivedTransferTypeLabel>"
    context form is UpdateTransferDetailContext
        background is theme color
    UpdateTransferDetail is a Panel
        title is "Main"
        form is UpdateTransferDetail
    BankCharge is a Panel
        valid when (CashLedgerFundTransferHeader.TransferType.MultipleWithdrawals)
        form is TransferBankCharges
    DetailCurrencyAmounts is a Panel
        form is DetailCurrencyAmountsForm
    HeaderCurrencyAmounts is a Panel
        form is HeaderCurrencyAmountsForm
