ProjectContractRevenueReversalNav is a Form
    title is "<FinanceEnterpriseGroup.ProjectLabel>ContractRevenueReversal"
    Layout
        two column distributed
            ProjectContract.DisplayContract
                label is "<FinanceEnterpriseGroup.ProjectLabel>Contract"
                display as text
            ProjectContract.Project.Description
                label is "Description"
                display as text
        two column
            column1 of 40%
                RunGroup
                    label is "RunGroup"
                    display as text
            column2 of 60%
                FinanceDimension2.DisplayDimension
                    label is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
                    display as text
        two column distributed
            paragraph
                FinanceDimension2.CompanyCustomerGroup.ReceivableCompany
                    label is "Company"
                    display as text
                PhoneDerivedCust
                    label is "Customer"
                    display as text
        two column distributed
            paragraph
                InvoiceDate
                    label is "RevenueDate"
                    display as text
                InvoiceTotalAllowance
                    label is "TotalAllowance"
                    display as text
        two column distributed
            paragraph
                InvoiceTotalAmount
                    label is "TotalAmount"
                    display as text
                InvoiceTotalFees
                    label is "Fees"
                    display as text
