MobileManageAssetDetailsContextForm is a Form
    title is "ReceiptLineDetails"
    Layout
        display as text
            single column
                Company.DerivedCompanyIDName
                    no label
            two column distributed
                PurchaseOrder
                    label is "PurchaseOrder"
                paragraph
                    PurchaseOrderReceipt
                        label is "Receipt"
                    PurchaseOrderLine.LineNumber
                        label is "Line"
            single column
                PurchaseOrderLineAssetRel.ReceivedQuantity
                    label is "TotalReceivedQuantity"
            two column distributed
                ShipToLocation.RepresentativeText
                    label is "ReceivingLocation"
                PurchaseOrderReceipt.Status
                    label is "ReceiptStatus"
                CreatedBy
                PurchaseOrderReceipt.ReceivedDate
                    label is "DateAndTimeReceived"
                Status
                    label is "LineStatus"
