ItemInformationWorksheetForm is a Form
    title is "ItemInformationWorksheet"
    Layout
        single column
            RunGroup
            visible when (ManualItemNumberingOrInterfacedItemEntered)
                InterfacedItem
        two column
            column1
                paragraph
                    visible when (FinalItemNumber entered)
                        FinalItemNumber
                    visible when (ManyItemsWithManufacturerInformation)
                        visible when (FinalItemNumber not entered)
                            FinalItemNumber
                                select is ItemsForAManufacturer
                    Description
            column2
                Description2
        single column
            Description3
                label is "LongItemDescription"
        two column
            Reference1
            Reference2
        two column
            StockUOM
            UOMConversion
        single column
            ImageURL
        two column
            Manufacturer
                label is "ManufacturerCode"
                compact format
            ManufacturerNumber
                label is "ManufacturerNumber"
        single column
            Active
        header3 of "Number_ofDecimals"
        paragraph
            NumberOfDecimalsCost
                label is "ForCost"
            NumberOfDecimalsQuantity
                label is "ForQuantity"
            NumberOfDecimalsPrice
                label is "ForPrice"
        header3 of "ItemCodes"
        single column
            GenericName
        two column
            MajorPurchasingClass
                show up to 10 characters
            MinorPurchasingClass
                show up to 10 characters
        two column
            MajorInventoryClass
                show up to 10 characters
            MinorInventoryClass
                show up to 10 characters
        two column
            MajorSalesClass
                show up to 10 characters
            MinorSalesClass
                show up to 10 characters
        two column
            visible when (not ContractGroupRel.AutoAddUNSPSCCode)
                UNSPSCCode
                    compact format
            visible when (ContractGroupRel.AutoAddUNSPSCCode)
                UNSPSCFull
            paragraph
                CommodityCode
                    compact format
                CommodityCode.Description
                    no label
                    display as text
        header3 of "SpecialHandling"
        two column distributed
            column1
                paragraph
                    SpecialHandlingCode
                        select is ItemGroup.SpecialHandlingCode set.ActiveSpecialHandlingCodes
                    SpecialHandlingCode.Description
                        no label
                        display as text
                visible when (SpecialHandlingCode.SpecialHandlingCodeExists)
                    SpecialHandlingInstructions
                        label is "SpecialHandlingInstructions"
                        use text area of 5 lines
        visible when (SpecialHandlingCode.SpecialHandlingCodeExists)
            single column
                SpecialHandlingCode.TimeSensitive
                    label is "TimeSensitive"
            visible when (SpecialHandlingCode.IsTimeSensitive)
                HandlingTimeInMinutes
                    label is "HandlingTimeInMinutes"
        header3 of "UserFields"
        paragraph
            UserField1
                label is "Alpha1"
            UserField2
                label is "Alpha2"
            UserField3
                label is "Alpha3"
        paragraph
            UserField4
                label is "Alpha4"
            UserField5
                label is "Alpha5"
        paragraph
            UserFieldN1
                label is "Numeric1"
            UserFieldN2
                label is "Numeric2"
            UserFieldN3
                label is "Numeric3"
        two column
            paragraph
                FreightClass
                    show up to 10 characters
                FreightClass.Description
                    no label
                    display as text
            paragraph
                ICNCode
                    show up to 10 characters
                ICNCode.Description
                    no label
                    display as text
        paragraph
            HazardCode
                show up to 15 characters
            HazardCode.Description
                no label
                display as text
        header3 of "TaxInformation"
        two column
            PurchaseTaxable
            PurchaseTaxCode
        single column
            ProductTaxCategory
        header3 of "Miscellaneous"
        two column
            ReplacementItem
            AddedDate
        two column
            EffectiveDate
            ExpirationDate
        two column
            SerialTracked
            LotTracked
        two column
            MSDSRequired
            CertificationRequired
        two column
            Consignment
        two column
            StockWeight
                label is "StockWeight"
            StockVolume
                label is "StockCubic"
        two column
            IssueAccount.GeneralLedgerChartAccount
                label is "IssueAccount"
            AllowCustomerOrderReturns
        single column
            BrokenCaseCharge
            visible when (IsHSNSACCodeEnabled)
                HSNSACCode
        three column
            HasExpirationDate
            CriticalItem
            Latex
        three column
            PersonalProtectiveEquipment
            MajorPPEClass
                show up to 10 characters
            MinorPPEClass
                show up to 10 characters
        three column distributed
            Sterile
            Reprocessed
            Pharmaceutical
        three column distributed
            SingleUse
            Dressing
            Reusable
        single column
            Analytics
        visible when (not ItemGroup.HideHealthcareInformation)
            header3 of "Healthcare"
            three column distributed
                Implantable
                Drugs
                Instrument
            three column distributed
                Narcotic
                ContainsHumanTissue
            paragraph
                ClinicalSystemUse
                PatientChargeable
                ChargeNumber
            paragraph
                HCPCSCode
                    label is "HCPCSAndSequence"
                    compact format
                HCPCSCode.HCPCS
                    display as text
                visible when (HCPCSCodeSequenceEnteredExists)
                    HCPCSCode.AlphaHCPCSSequence
                        label is "Sequence"
                        display as text
                visible when (HCPCSCodeSequenceEnteredNotExists)
                    ItemAlphaHCPCSSequence
                        label is "Sequence"
                        display as text
                HCPCSCode.ShortDescription
                    no label
                    display as text
            paragraph
                GMDNPTCode
                    select is ActiveGMDNPTCodes
                GMDNPTCode.Name
                    no label
                    display as text
                UDITracked
                    label is "UDI/DSCSATracked"
        header3 of "CatchWeightInformation"
        paragraph
            CatchWeightCode
                label is "Code"
            SecondaryUOM
            CatchWeightVariancePercent
                label is "VariancePercentage"
