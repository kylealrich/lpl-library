FlowDefinitionCompositeForm is a CompositeForm
    title is "ProcessDefinition"
    context form is FlowDefinitionForm
    ProcessVersions is a Panel
        list is PfiFlowVersion set.FlowVersionList
    VariableNames is a Panel
        form is VariableNamesPanel
    PruneDefinition is a Panel
        title is "WorkUnitDataCleanup"
        form is PruneDefinitionPanel
    ActivityLimiter is a Panel
        title is "ActivityLimiter"
        form is ActivityLimiterPanel
    EmailNotificationInformation is a Panel
        visible when (not NotifyByEmail.None)
        form is EmailNotificationInformationForm
    AutoRestartPanel is a Panel
        title is "AutoRestart"
        visible when (AutoRestartEnabled)
        form is AutoRestartPanel
