PatientChargeItem is a Form
    is primary
    Layout
        paragraph
            Company
            display as text
                Company.Name
                    no label
                Company.GeneralLedgerCompany.Currency
                    label is "Currency"
        single column
            PatientChargeItem.RevenueCenter
                label is "RevenueCenter"
            PatientChargeItem.ChargeNumber
                label is "ChargeNumber"
            paragraph
                PatientChargeItem.Item
                    label is "Item"
                PatientChargeItem.Item.Description
                    no label
                    display as text
            PriceClass
                select is PriceClasss
            Price
                label is "Price"
