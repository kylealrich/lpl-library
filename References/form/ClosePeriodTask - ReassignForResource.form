ReassignForResource is an ActionForm
    action is ReassignForResource
    title is "Reassign"
    Layout
        header3 of "Reassign<TaskName>"
        two column
            PrmFinanceTeam
                label is "Team"
            PrmFinanceTeam.Description
                no label
                display as text
        two column
            PrmTaskOwner
                label is "TaskOwner"
                compact format
                select is FinanceTeamMemberActiveSelect
            PrmTaskOwner.FinanceTeamMember.TeamMember.PreferredSimplePresentationName
                no label
                display as text
