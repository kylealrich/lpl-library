AccountAnalysisMain is a Form
    Layout
        three column distributed
            AccountAnalysisSettings
            Description
        three column distributed
            paragraph
                ReportingBasis
                    select is ReportingBasisList
                    raise red alert when (ReportingBasis.BasisIsIncomplete)
                        mouse over text is "ReportingBasisDefinitionIsIncomplete"
                visible when (not AccountAnalysisSettingsExists)
                    ReportingBasisRepresentativeText
                        no label
                        display as text
                visible when (AccountAnalysisSettingsExists)
                    visible when (IsOldBasis)
                        ReportingBasisRepresentativeText
                            no label
                            display as text
                    visible when (not IsOldBasis)
                        button of "<ReportingBasisRepresentativeText>"
                            link is ShadowBasisNav
                            display as link
            paragraph
                Scenario
                Scenario.Description
                    no label
                    display as text
            Scenario.CommitmentType
                display as text
        three column distributed
            paragraph
                GeneralLedgerChartAccount
                    label is "<actor.context.FinanceEnterpriseGroup.AccountLabel>"
                    select is ReportingBasis.ReportingChart.ChartAccountNode.ChartAccountChildrenRel.ChartAccountList
                GeneralLedgerChartAccount.AccountDescription
                    no label
                    right align
                    display as text
            paragraph
                System
                    left align
                System.Description
                    no label
                    display as text
            UseLTDBeginningBalance
        three column distributed
            Year
            ReportingPeriod
                label is "Period"
                left align
                select is SummaryCalendarList
            ThruReportingPeriod
                label is "ThruPeriod"
                left align
                select is SummaryThruCalendarList
        three column distributed
            paragraph
                AccountingEntity
                    label is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
                    select is ReportingBasisAccountingEntityList
                AccountingEntity.Name
                    no label
                    display as text
            visible when (FinanceEnterpriseGroup.AccountingUnitLabelExists)
                paragraph
                    AccountingUnit
                        label is "<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>"
                        select is AccountingEntity.ContextStructureAccountingUnitEntityNodeRel.AccountingUnitChildrenRel.AccountingUnitTreeSelect
                    AccountingUnit.Description
                        no label
                        display as text
            visible when (FinanceEnterpriseGroup.Dimension1LabelExists)
                paragraph
                    FinanceDimension1
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension1Label>"
                        select is FinanceDimension1Structure.DimensionNode.DimensionChildrenRel.FinanceDimension1TreeSelect
                    FinanceDimension1.Description
                        no label
                        display as text
            visible when (FinanceEnterpriseGroup.Dimension2LabelExists)
                paragraph
                    FinanceDimension2
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>"
                        select is FinanceDimension2Structure.DimensionNode.DimensionChildrenRel.FinanceDimension2SubSystemTreeSelect
                    FinanceDimension2.Description
                        no label
                        display as text
            visible when (FinanceEnterpriseGroup.Dimension3LabelExists)
                paragraph
                    FinanceDimension3
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension3Label>"
                        select is FinanceDimension3Structure.DimensionNode.DimensionChildrenRel.FinanceDimension3TreeSelect
                    FinanceDimension3.Description
                        no label
                        display as text
            visible when (FinanceEnterpriseGroup.Dimension4LabelExists)
                paragraph
                    FinanceDimension4
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension4Label>"
                        select is FinanceDimension4Structure.DimensionNode.DimensionChildrenRel.FinanceDimension4TreeSelect
                    FinanceDimension4.Description
                        no label
                        display as text
            visible when (FinanceEnterpriseGroup.Dimension5LabelExists)
                paragraph
                    FinanceDimension5
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension5Label>"
                        select is FinanceDimension5Structure.DimensionNode.DimensionChildrenRel.FinanceDimension5TreeSelect
                    FinanceDimension5.Description
                        no label
                        display as text
            visible when (FinanceEnterpriseGroup.Dimension6LabelExists)
                paragraph
                    FinanceDimension6
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension6Label>"
                        select is FinanceDimension6Structure.DimensionNode.DimensionChildrenRel.FinanceDimension6TreeSelect
                    FinanceDimension6.Description
                        no label
                        display as text
            visible when (FinanceEnterpriseGroup.Dimension7LabelExists)
                paragraph
                    FinanceDimension7
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension7Label>"
                        select is FinanceDimension7Structure.DimensionNode.DimensionChildrenRel.FinanceDimension7TreeSelect
                    FinanceDimension7.Description
                        no label
                        display as text
            visible when (FinanceEnterpriseGroup.Dimension8LabelExists)
                paragraph
                    FinanceDimension8
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension8Label>"
                        select is FinanceDimension8Structure.DimensionNode.DimensionChildrenRel.FinanceDimension8TreeSelect
                    FinanceDimension8.Description
                        no label
                        display as text
            visible when (FinanceEnterpriseGroup.Dimension9LabelExists)
                paragraph
                    FinanceDimension9
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension9Label>"
                        select is FinanceDimension9Structure.DimensionNode.DimensionChildrenRel.FinanceDimension9TreeSelect
                    FinanceDimension9.Description
                        no label
                        display as text
            visible when (FinanceEnterpriseGroup.Dimension10LabelExists)
                paragraph
                    FinanceDimension10
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension10Label>"
                        select is FinanceDimension10Structure.DimensionNode.DimensionChildrenRel.FinanceDimension10TreeSelect
                    FinanceDimension10.Description
                        no label
                        display as text
            visible when (FinanceEnterpriseGroup.ProjectLabelExists)
                paragraph
                    Project
                        label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
                        select is ProjectStructure.ProjectNode.ProjectChildrenRel.ProjectTreeSubSystemSelect
                    Project.Description
                        no label
                        display as text
        paragraph
            ReportingCurrency
                raise yellow alert when (PossibleMixedCurrencies)
                    mouse over text is "MakeSureAllEntitiesIn_Reporting_BasisUseTheSameCurrency"
            SuppressZeroRows
        two column
            visible when (AccountAnalysisSettingsExists)
                button of "AnalyzeAccounts"
                    link is ChartAnalysisNav
            visible when (BudgetAccountAnalysisSettingsExists)
                button of "AnalyzeBudget"
                    link is ChartAnalysisBudgetNav
