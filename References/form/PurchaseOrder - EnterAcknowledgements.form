EnterAcknowledgements is a CompositeForm
    title is "PurchaseOrderLinesToAcknowledge"
    called out actions only
    context form is POAcknowledgementsContext
        background is theme color
    Actions
        action is FinishManualProcess
            label is "FinishAcknowledgements"
            hide when invalid
        action is save
            label is "_"
        action is refresh
            label is "_"
    POLinesToAcknowledge is a Panel
        visible when (not AcknowledgedViaSupplierPortal)
        list is PurchaseOrderEDIAcknowledgementRel.AcknowledgedPurchaseOrderLinesList
    IssuesAndAcknowledgements is a Panel
        visible when (SupplierShowRevisionsManuallyEntered)
        list is PurchaseOrderRevisionsRel.SupplierRevisionsforaPurchaseOrder
