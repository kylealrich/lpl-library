CorporateItemGroup is a CompositeForm
    is primary
    title is "CorporateItemGroup"
    context form is CorporateItemGroupContext
        background is theme color
    Main is a Panel
        form is ItemGroupMain
    UnitsOfMeasure is a Panel
        list is UnitOfMeasure set.UnitOfMeasureList
    RecipeUnitsOfMeasure is a Panel
        visible when (RecipeManagementInUse)
        list is RecipeUnitOfMeasure set.RecipeUnitsOfMeasureForItemGroup
    ItemCommentTypes is a Panel
        form is ItemCommentTypes
    UserFields is a Panel
        form is UserFields
    MajorClasses is a MultiListPanel
        list is MajorClass set.MajorSalesClasses
        list is MajorClass set.MajorInventoryClasses
        list is MajorClass set.MajorPurchasingClasses
    FreightClasses is a Panel
        list is MajorClass set.FreightClasses
    Manufacturers is a Panel
        list is Manufacturer set.ManufacturerCodeLists
    GenericNames is a Panel
        list is GenericName set.GenericNamesLists
    Codes is a MultiListPanel
        list is HazardCode set.HazardCodes
        list is SpecialHandlingCode set.SpecialHandlingCodesList
        list is UNSPSCCode set.UNSPSCCodes
    GlobalLocationNumbers is a Panel
        list is GlobalLocationNumber set.GlobalLocationNumberList
    DocumentTemplates is a Panel
        form is DocumentTemplatesForm
