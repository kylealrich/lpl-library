MTaxInquiry is a Form
    title is "TaxInquiry"
    Layout
        header3 of "InvoiceTotalsInBillingCurrency"
        two column distributed
            display as text
                Currency
                    label is "Currency"
                TotalGoods
                    label is "GoodsAmount"
                FreightCharge.TransactionAmount
                    label is "Freight"
                TotalAddOnAmount
                    label is "Add-OnCharges"
                GrossAmount
                    label is "InvoiceGross"
                OrderDiscount.TransactionAmount
                    label is "Discount"
                EnteredDiscount.TransactionAmount
                    label is "AdditionalDiscount"
                InvoiceNetAmount
                    label is "InvoiceNet"
                TermsDiscount.TransactionAmount
                    label is "TermsDiscount"
                TaxableTotal
                    label is "TaxableAmount"
                TaxTotal.TransactionAmount
                    label is "SalesTax"
                InvoiceTotal
                    label is "InvoiceTotal"
                CashInAdvance.TransactionAmount
                    label is "DownPayment"
                CheckNumber
                    label is "Check"
                ProformaPrepayAmount
                visible when (ProformaPrepayAmount not entered)
                    InvoiceNetDue
                        label is "NetDue"
                visible when (ProformaPrepayAmount entered)
                    InvoiceNetPrepayDue
                        label is "NetDue"
        header3 of "InvoiceTotalsInBaseCurrency"
        two column distributed
            display as text
                Company.Currency
                    label is "Currency"
                TotalGoodsBaseAmount
                    label is "GoodsAmount"
                FreightCharge.BaseAmount.EnteredCurrencyAmount
                    label is "Freight"
                TotalAddOnBaseAmount
                    label is "Add-OnCharges"
                GrossBaseAmount
                    label is "InvoiceGross"
                OrderDiscount.BaseAmount.EnteredCurrencyAmount
                    label is "Discount"
                EnteredDiscount.BaseAmount.EnteredCurrencyAmount
                    label is "AdditionalDiscount"
                InvoiceNetBaseAmount
                    label is "InvoiceNet"
                TermsDiscount.BaseAmount.EnteredCurrencyAmount
                    label is "TermsDiscount"
                DerivedTaxableTotalBaseAmount
                    label is "TaxableAmount"
                TaxTotal.BaseAmount.EnteredCurrencyAmount
                    label is "SalesTax"
                InvoiceTotalBaseAmount
                    label is "InvoiceTotal"
                CashInAdvance.BaseAmount.EnteredCurrencyAmount
                    label is "DownPayment"
                CheckNumber
                    label is "Check"
                ProformaPrepayAmount
                visible when (ProformaPrepayAmount not entered)
                    InvoiceNetDueBase
                        label is "NetDue"
                visible when (ProformaPrepayAmount entered)
                    InvoiceNetPrepayBaseDue
                        label is "NetDue"
                TermsDescription1
                    label is "Terms"
                CurrencyRate
                    label is "ConversionRate"
                Revalue
                    label is "RevalueFlag"
