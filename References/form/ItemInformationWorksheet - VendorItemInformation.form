VendorItemInformation is a Form
    Layout
        header3 of "VendorItem1"
        visible when (VendorItemExistsForEnteredVendorItem1)
            single column
                text of "VendorItemExistsForItem<first VendorItem1ExistsRel.Item>"
                    italics
        two column
            VendorItemNew.VendorItemOccurs.VendorItemDetails[1].VendorItem
            VendorItemNew.VendorItemOccurs.VendorItemDetails[1].VendorItemDescription
        single column
            VendorItemNew.VendorItemOccurs.VendorItemDetails[1].Vendor
        two column
            VendorItemNew.VendorItemOccurs.VendorItemDetails[1].VendorBuyUOM.UnitOfMeasure
                label is "BuyU\\O\\M"
                select is ValidBuyUnitsOfMeasure
            VendorItemNew.VendorItemOccurs.VendorItemDetails[1].VendorPriceUOM.UnitOfMeasure
                label is "PriceU\\O\\M"
                select is ValidBuyUnitsOfMeasure
        two column
            VendorItemNew.VendorItemOccurs.VendorItemDetails[1].Manufacturer
                compact format
            VendorItemNew.VendorItemOccurs.VendorItemDetails[1].ManufacturerNumber
        two column
            VendorItemNew.VendorItemOccurs.VendorItemDetails[1].LabelerIdentificationCode
            VendorItemNew.VendorItemOccurs.VendorItemDetails[1].LastLeadtime
        two column
            VendorItemNew.VendorItemOccurs.VendorItemDetails[1].MinimumOrderQuantity
                label is "MinimumQuantityPerP\\OLine"
            VendorItemNew.VendorItemOccurs.VendorItemDetails[1].WarrantyPeriod
                label is "WarrantyDays"
        single column
            VendorItemNew.VendorItemOccurs.VendorItemDetails[1].Active
