InventoryStockStatusReport is an ActionForm
    action is InventoryStockStatusReport
    Layout
        three column
            column1
                paragraph
                    CompanyGroup
                        show up to 10 characters
                    CompanyGroup.Description
                        no label
                        display as text
                paragraph
                    Location
                        show up to 10 characters
                    Location.Name
                        no label
                        display as text
            column2
                text of "\\or"
                    bold
                text of "\\or"
                    bold
            column3
                paragraph
                    Company
                    Company.Name
                        no label
                        display as text
                paragraph
                    ReportGrp
                        label is "ReportGroup"
                    ReportGrp.Name
                        no label
                        display as text
        three column
            column1
                paragraph
                    InvenMajcl
                        label is "MajorInventoryClass"
                        show up to 10 characters
                    InvenMajcl.Description
                        no label
                        display as text
                paragraph
                    BuyerCode
                        label is "Buyer"
                        show up to 10 characters
                    BuyerCode.Name
                        no label
                        display as text
                IncludeInactive
                    label is "IncludeInactiveItems"
            column2
                text of "_"
            column3
                paragraph
                    InvenMincl
                        label is "MinorInventoryClass"
                        show up to 10 characters
                    InvenMincl.Description
                        no label
                        display as text
                ActivityFlag
                    label is "SelectItems_withNetChange"
                    show up to 15 characters
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                InventoryStockStatusReportReport_RDG
            column2 of 25%
                InventoryStockStatusReportReport_XPT
