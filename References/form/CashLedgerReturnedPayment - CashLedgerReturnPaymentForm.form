CashLedgerReturnPaymentForm is a CompositeForm
    is primary
    title is "CashLedgerReturnedPayment<TransactionNumber>"
    context form is ReturnedPaymentEntry
        background is theme color
    Actions
        action is Release
            label is "Release"
            hide when invalid
        action is SubmitForApproval
            label is "SubmitForApproval"
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is save
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    Main is a Panel
        form is MainForm
    Deposit is a Panel
        title is "DepositInformation"
        form is DepositInformation
    Bank is a Panel
        title is "BankCharges"
        form is BankCharges
    TransactionData is a Panel
        form is TransactionData
    DistributionList is a Panel
        title is "Distributions"
        list is CashLedgerGLDistributionsRel.ReturnedPaymentDistributionList
    CurrencyAmounts is a Panel
        title is "CurrencyAmounts"
        form is CurrencyAmountsForm
    BudgetErrors is a Panel
        title is "BudgetErrors<DerivedHeaderBudgetErrorCount>"
        visible when (HeaderBudgetEditErrorExists)
        list is HeaderBudgetEditErrorRel.primary
    WarningBudgetErrors is a Panel
        title is "WarningBudgetErrors<DerivedWarningBudgetErrorCount>"
        visible when (DistributionBudgetEditErrorExists)
        list is DistributionBudgetEditErrorRel.primary
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
    ApprovalComments is a Panel
        visible when (HasApprovalComments)
        list is CashLedgerRetPaymentReasonCodeUsage set.CashLedgerRetPaymentReasonCodeUsageList
    Errors is a Panel
        title is "Errors<DerivedReturnedPaymentErrorCount>"
        visible when (ReturnedPaymentErrorsExist)
        list is CashLedgerTransactionError set.CashLedgerReturnedPaymentErrorList
