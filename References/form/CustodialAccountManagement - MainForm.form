MainForm is a Form
    Layout
        single column
            paragraph
                AccountingEntity
                AccountingEntity.Name
                    no label
                    display as text
            paragraph
                CustodialAccountType
                visible when (CustodialAccountType.Other)
                    Title
                visible when (CustodialAccountType.Individual)
                    paragraph
                        FirstName
                        MiddleName
                        LastName
            visible when (FinanceEnterpriseGroup.FundDimension.AccountingUnit)
                paragraph
                    paragraph
                        AccUnitSummaryFund
                            label is "Summary_<FinanceEnterpriseGroup.AccountingUnitLabel>"
                            select is SummaryAccountingUnits
                            when value changed
                                AccUnitPostingFund =  blank
                        AccUnitSummaryFund.Description
                            no label
                            display as text
                    paragraph
                        AccUnitPostingFund
                            label is "Posting_<FinanceEnterpriseGroup.AccountingUnitLabel>"
                            select is AccUnitSummaryFund.DescendantsRel.PostingAccountingUnits
                        AccUnitPostingFund.Description
                            no label
                            display as text
            visible when (FinanceEnterpriseGroup.FundDimension.Dimension1)
                paragraph
                    paragraph
                        DimOneSummaryFund
                            label is "Summary_<FinanceEnterpriseGroup.FinanceDimension1Label>"
                            select is SummaryFinanceDimension1Select
                            when value changed
                                DimOnePostingFund =  blank
                        DimOneSummaryFund.Description
                            no label
                            display as text
                    paragraph
                        DimOnePostingFund
                            label is "Posting_<FinanceEnterpriseGroup.FinanceDimension1Label>"
                            select is DimOneSummaryFund.DescendantsRel.FinanceDimension1Select
                        DimOnePostingFund.Description
                            no label
                            display as text
            paragraph
                CamCategory
                    label is "Category"
                CamCategory.Description
                    no label
                    display as text
            visible when (CamCategory.SSNRequired.Yes)
                paragraph
                    CamCategory.TaxIDType
                    CamCategory.TaxIDType.Description
                    TaxID
                    visible when (HasTaxID)
                        text of "TaxIDEntered"
                            italics
                    visible when (not HasTaxID)
                        text of "TaxIDNotEntered"
                            italics
            paragraph
                CamCustodian
                    label is "Custodian"
                CamCustodian.Description
                    no label
                    display as text
            paragraph
                VendorGroup
                VendorGroup.Description
                    no label
                    display as text
                Vendor
                Vendor.VendorName
                    no label
                    display as text
            paragraph
                BankTransactionCode
                    label is "TransferCode"
                BankTransactionCode.Description
                    no label
                    display as text
