RequesterForm is a CompositeForm
    is primary
    title is "<DerivedFormTitle>"
    context form is RequesterFormContext
        background is theme color
    restrict action DeleteRequisition
    Main is a Panel
        form is Main
    Contact is a Panel
        form is RequesterContactInformation
    RequestingLocations is a Panel
        list is RequesterRequestingLocationsRel.RequestingLocationsForRequester
    PurchasingClasses is a Panel
        list is RequesterPurchasingClassesRel.RequesterPurchasingClasses
    RequesterFinanceStructure is a Panel
        title is "AccountingRestrictions"
        list is RequesterFinanceStructureRel.RequesterFinanceStructures
    RequisitionSelfService is a Panel
        title is "RSS"
        form is RequisitionSelfService
    RSSLPLRequisitionSelfService is a Panel
        title is "RSS_LPL"
        visible when (RequisitionSelfServiceLPLAllowed)
        form is RSSLPLRequisitionSelfService
    PunchoutVendors is a Panel
        list is RequesterPunchoutVendorsRel.PunchoutVendors
