ExpenseManagementTravelPlanCompositeForm is a CompositeForm
    is primary
    title is "ExpenseManagementTravelPlanHeader"
    context form is ExpenseManagementTravelPlanContextForm
    Main is a Panel
        form is ExpenseManagementTravelPlanMainForm
    DetailLines is a Panel
        list is ExpenseManagementTravelPlanDetail set.ExpenseManagementTravelPlanDetailList
    BudgetErrors is a Panel
        visible when (HeaderBudgetEditErrorExists)
        list is HeaderBudgetEditErrorRel.primary
    WarningBudgetErrors is a Panel
        visible when (DistributionBudgetEditErrorExists)
        list is DistributionBudgetEditErrorRel.primary
