PhysicalInventoryFreezeForm is a Form
    is primary
    title is "EnterCounts"
    Layout
        display as text
            paragraph
                PhysicalInventorySelect.Company.DerivedCompanyDisplay
                    label is "Company"
            two column
                paragraph
                    PhysicalInventorySelect.DisplaySelectID
                        label is "SelectID"
                PhysicalInventorySelect.Status
            blank line
            two column
                PhysicalInventoryFreeze.PhysicalInventoryFreezePage
                    label is "Page"
                PhysicalInventoryFreeze.PhysicalInventoryFreezeLine
                    label is "Line"
        paragraph
            visible when (RecordExists)
                InventoryLocation.DerivedInventoryLocationIDName
                    label is "InventoryLocation"
                    display as text
            visible when (RecordNotExists)
                InventoryLocation
                InventoryLocation.Name
                    no label
                    display as text
        paragraph
            visible when (RecordExists)
                Item
                    display as text
            visible when (RecordNotExists)
                Item
            display as text
                Item.Description
                    no label
                Item.Description2
                    no label
        visible when (RecordExists)
            visible when (ItemLocation.BinTracked)
                paragraph
                    Bin
                        display as text
            visible when (ItemLocation.LotTracked.LotRequiredAtReceipt)
                paragraph
                    display as text
                        Lot
                        Sublot
            visible when (ItemLocation.SerialTracked.SerialRequiredAtReceipt)
                paragraph
                    Serial
                        display as text
        visible when (RecordNotExists)
            visible when (ItemLocation.BinTracked)
                paragraph
                    Bin
            visible when (ItemLocation.LotTracked.LotRequiredAtReceipt)
                paragraph
                    Lot
                    Sublot
            visible when (ItemLocation.SerialTracked.SerialRequiredAtReceipt)
                paragraph
                    Serial
        paragraph
            visible when (not PhysicalInventorySelect.Status.SelectIDUpdated)
                EnteredCount
            visible when (PhysicalInventorySelect.Status.SelectIDUpdated)
                EnteredCount
                    display as text
            visible when (not CountInTransactionUOM)
                EnteredUOM
                    select is ValidTransactionUnitsOfMeasure
            visible when (CountInTransactionUOM)
                EnteredUOM
                    display as text
            visible when (not NotEnteredInStockUOM)
                visible when (CountEntered)
                    DerivedVariance
                        display as text
            visible when (BeyondCatchWeightVariance)
                single column
                    DerivedCatchWeightVariance
                        no label
                        display as text
        visible when (NotEnteredInStockUOM)
            paragraph
                display as text
                    text of "StockUnitOfMeasure:"
                    CountQuantity
                    UnitOfMeasure
                    visible when (CountEntered)
                        DerivedVariance
        paragraph
            visible when (RecordExistsForCatchWeightItem)
                visible when (not PhysicalInventorySelect.Status.SelectIDUpdated)
                    SecondaryCountQuantity
                    SecondaryUOM
                visible when (PhysicalInventorySelect.Status.SelectIDUpdated)
                    SecondaryCountQuantity
                        display as text
                    SecondaryUOM
                        display as text
                visible when (CountEntered)
                    SecondaryVarianceQuantity
                        display as text
            visible when (false)
                TransientDisplayUOM
                    display as text
                TransientDisplayCount
                    display as text
        paragraph
            visible when (DisplayUnitCost)
                TransientUnitCost
                    label is "UnitCostInEnteredUnitOfMeasure"
                visible when (UnitCostEntered)
                    visible when (NotEnteredInStockUOM)
                        UnitCost
                            label is "UnitCostInStockUnitOfMeasure"
                            display as text
        paragraph
            InventoryReasonCode
                label is "ReasonCode"
                select is ActiveReasonCodes
            InventoryReasonCode.Description
                no label
                display as text
