SupplierEvaluationSummaryFormForSupplier is a SummaryForm
    title is "EvaluationFormFor<Supplier.SupplierName>"
    Layout
        header2 of "OverallPerformanceEvaluation"
        blank line
        text of "Description:<Description>"
            bold
        blank line
        text of "RatingScale:"
            bold
        text of "<RatingScale>"
        blank line
        paragraph
            text of "OverallScore:<TotalPerformanceScore>"
                bold
            TotalPerformanceScore
                no label
                display as rating
        blank line
        list is SupplierPerformanceCriteriaGroupRel.CategoryScoresBarChart
        list is SupplierPerformanceCriteriaGroupRel.PortalCategoryScores
        header2 of "CategoryDetails"
        form is SupplierPerformanceCriteriaGroupRel.CategorySummarySupplier
