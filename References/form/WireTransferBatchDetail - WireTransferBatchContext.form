WireTransferBatchContext is a Form
    title is "WireTransfer"
    Layout
        two column distributed
            display as text
                column1
                    paragraph
                        WireTransferBatch.FinancialInstitution
                        WireTransferBatch.FinancialInstitution.Description
                            no label
                    paragraph
                        WireTransferBatch.FinancialInstitutionBranch
                        WireTransferBatch.FinancialInstitutionBranch.Description
                            no label
                    BlankField
                        no label
                    BlankField
                        no label
                column2
                    TransactionDate
                    paragraph
                        visible when (DisplayDistributionTotals)
                            RequiredDistributionAmount
                            visible when (ExternalTransfer)
                                TotalDistributions
                            visible when (FeeAmount entered)
                                TotalFeeDistributions
                            DistributionDifference
                    visible when (HasCashLedgerTransactionIdentifier)
                        first CashLedgerTransactionRel.Status
                            label is "CashLedgerTransactionStatus"
