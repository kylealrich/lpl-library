ContractPercentCompleteForm is a Form
    title is "<FinanceEnterpriseGroup.ProjectLabel>ContractPercentComplete"
    Layout
        three column distributed
            column1
                Project.DisplayProject
                    label is "<FinanceEnterpriseGroup.ProjectLabel>Contract"
                    display as text
            column2
                Project.Description
                    no label
                    display as text
        three column distributed
            column1
                ProjectContractPercentComplete
                    label is "PercentCompleteDate"
                    display as text
            column2
                Description
        visible when (HasProjectReportingSettings)
            three column distributed
                column1
                    ProjectReportingSettings
                        display as text
                column2
                    ProjectReportingSettings.Description
                        no label
                        display as text
            three column distributed
                column1
                    ProjectReportingSettings.Scenario
                        label is "Scenario"
                        display as text
                column2
                    ProjectReportingSettings.Scenario.Description
                        no label
                        display as text
            three column distributed
                column1
                    ProjectReportingSettings.ReportingAccount
                        label is "SummaryAccount"
                        display as text
                column2
                    ProjectReportingSettings.ReportingAccount.AccountDescription
                        no label
                        display as text
            three column distributed
                column1
                    ProjectReportingSettings.ReportingPeriod
                        label is "ReportingPeriod"
                        display as text
                column2
                    ProjectReportingSettings.ReportingDate.DisplayPeriod
                        no label
                        display as text
        visible when (HasProjectReportingSettingsAll)
            three column distributed
                column1
                    ProjectReportingSettingsAll
                        display as text
                column2
                    ProjectReportingSettingsAll.Description
                        no label
                        display as text
            three column distributed
                column1
                    ProjectReportingSettingsAll.Scenario
                        label is "Scenario"
                        display as text
                column2
                    ProjectReportingSettingsAll.Scenario.Description
                        no label
                        display as text
            three column distributed
                column1
                    ProjectReportingSettingsAll.ReportingAccount
                        label is "SummaryAccount"
                        display as text
                column2
                    ProjectReportingSettingsAll.ReportingAccount.AccountDescription
                        no label
                        display as text
            three column distributed
                column1
                    ProjectReportingSettingsAll.ReportingPeriod
                        label is "ReportingPeriod"
                        display as text
                column2
                    ProjectReportingSettingsAll.ReportingDate.DisplayPeriod
                        no label
                        display as text
        three column distributed
            column1
                BudgetAmount
                    display as text
            column2
                ActualAmount
                    display as text
            column3
                paragraph
                    CostsToComplete
                        display as text
                    OverrideCostsToComplete
        three column distributed
            column1
                ContractAmount
                    display as text
            column2
                PercentComplete
                    display as text
            column3
                OverridePercentComplete
        three column distributed
            column1
                EarnedRevenue
                    display as text
            column2
                RevenueToBeRecognized
                    display as text
            column3
                paragraph
                    GrossMarginPercent
                        display as text
                    OverrideGrossMarginPercent
