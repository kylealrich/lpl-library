MobileDetailForm is a Form
    title is "DetailForm"
    Actions
        action is save
            action icon is save
            label is "_"
    Layout
        single column
            PurchaseOrderReceipt
                label is "Receiver"
                display as text
            PurchaseOrderTransactionDetail.LineNumber
            Lot
                select is MobileAvailableLotNumbersListSelect
            Sublot
            Serial
                select is MobileSerialNumbersSelect
            Bin
                select is MobileBinSelect
            LotExpirationDate
            LotOnHold
            Quantity
            SecondaryQuantity
                label is "StockQuantity"
