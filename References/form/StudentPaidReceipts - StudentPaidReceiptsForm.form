StudentPaidReceiptsForm is a CompositeForm
    is primary
    title is "<mode>Receipt"
    context form is StudentPaidReceiptsContextForm
    ReceiptDetailLines is a Panel
        list is StudentActivityReceiptDetailRel.OriginatingReceiptDetailList
            helper list is StudentActivityReceipt.SchoolActivityAccountRel.SchoolActivityAccountSelectList
                action is AssignActivityToReceipt
                    invoked.ReceiptNumber = StudentActivityReceipt
