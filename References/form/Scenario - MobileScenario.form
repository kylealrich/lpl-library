MobileScenario is a CompositeForm
    title is "Scenario<Scenario>_<Description>"
    Main is a Panel
        form is MobileMain
    EditTemplates is a Panel
        visible when (CommitmentType.Budget)
        list is BudgetTemplate set.MobileScenarioTemplates
    Data is a Panel
        visible when (CommitmentType.Budget)
        list is AllTotalsRel.MobileBudgetScenarioList
    ChangeRequests is a Panel
        visible when (CanDisplayChangeOrders)
        list is BudgetChangeOrdersRel.BudgetChangeOrdersForScenario
    Changes is a Panel
        visible when (DisplayChanges)
        list is ReleasedLinesRel.ReleasedLinesForScenario
    OldChanges is a Panel
        visible when (DisplayOldChanges)
        list is BudgetChangesRel.BudgetChangeForScenarioList
    IncrementalLoads is a Panel
        visible when (HasIncrementalLoads)
        list is IncrementalLoadsRel.LoadList
    Imports is a Panel
        visible when (CommitmentType.Budget)
        list is BudgetImport set.ScenarioList
    ImportResults is a Panel
        visible when (CommitmentType.Budget)
        list is BudgetImportResult set.BudgetImportResults
