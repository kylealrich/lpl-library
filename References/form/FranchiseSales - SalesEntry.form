SalesEntry is a CompositeForm
    is primary
    title is "SalesEntry"
    context form is SalesEntryContext
        background is theme color
    Sales is a Panel
        form is Sales
    Detail is a Panel
        visible when (not IsSalesOnly)
        list is FranchiseCycleChargesRel.CycleCharges
    User is a Panel
        form is User
