ProjectTeam is a CompositeForm
    is primary
    title is "<FinanceEnterpriseGroup.ProjectLabel>Team_<ProjectTeam>_<Description>"
    context form is ProjectTeamContext
        background is theme color
    TeamMembers is a Panel
        list is ProjectTeamMember set.ProjectTeamMemberList
            helper list is ActiveEmployeeRel.ActiveEmployeesForProjects
                action is AddToProjectTeam
