FinanceGroupForm is a CompositeForm
    is primary
    title is "FinanceGroup"
    context form is FinanceGroupContextForm
    FinanceGroupDetails is a Panel
        title is "Main"
        visible when (FinanceGroupExists)
        form is FinanceGroupDetailsForm
    CloseManagement is a Panel
        title is "CloseAndReconciliation"
        visible when (FinanceGroupExists)
        form is CloseMgmtForm
    CashManagement is a Panel
        visible when (FinanceGroupExists)
        form is CashManagementForm
    Companies is a Panel
        visible when (FinanceGroupExists)
        list is FinanceCompany set.FinanceCompanyList
