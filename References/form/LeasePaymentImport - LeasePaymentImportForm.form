LeasePaymentImportForm is a Form
    title is "LeasePaymentImport"
    Layout
        two column
            column1
                LeasePaymentImport.RunGroup
                    label is "RunGroup"
                paragraph
                    Company
                    Company.Name
                        no label
                        display as text
                paragraph
                    Lease
                        select is AllLeases
                    Lease.Description
                        no label
                        display as text
                    Lease.Currency
                        display as text
                Description
                VendorAndRemitTo.Vendor
                VendorInvoiceNumber
                Period
                AccountGroup
                HistoryFl
                HistoricalVendorAndRemitTo.Vendor
            column2
                LeasePaymentImport.SequenceNumber
                    label is "SequenceNumber"
                text of "_"
                Suffix
                ProcessLevel
                VendorAndRemitTo.RemitToCode
                Year
                GeneralLedgerCompanyGroup
                GeneralLedgerBudgetNumber
                    label is "BudgetNumber"
                text of "_"
                HistoricalVendorAndRemitTo.RemitToCode
        header2 of "Payment"
        two column
            column1
                PaymentNumber
                PaymentAmount
                StraightLineFlag
                    label is "StraightLine"
                        left of field
                MaximumPayment
            column2
                DueDate
                InterestAmount
                StraightLineAmount
        header2 of "Tax"
        paragraph
            TaxableFlag
                label is "Taxable"
        two column
            column1
                TaxCode
                TransactionTaxableAmount
                    label is "TaxableAmount"
            column2
                TaxUsageCode
                PercentTaxable
                    label is "Or_Percent"
        header2 of "Codes"
        two column
            column1
                AccrueFlag
                ExecutoryCostCode
                    label is "NonLeaseComponent"
            column2
                AccrualCode
                AuthorityCode
