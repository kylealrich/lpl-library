MassUpdateActionForm is an ActionForm
    action is MassUpdate
    title is "TransactionMassUpdate"
    Layout
        single column
            PrmCashManagementGroup
                label is "CashManagementGroup"
        single column
            PrmCashCode
                label is "CashCode"
        single column
            PrmCashCodeGroup
                label is "CashCodeGroup"
        single column
            PrmBankTransactionCode
                label is "BankTransactionCode"
        paragraph
            TransNbrRangeUsingLike.FromTransNbr
                label is "TransactionNumber"
            text of "_-"
            TransNbrRangeUsingLike.ToTransNbr
                no label
            text of "TransactionNumberRangeInAlphaSortOrder"
                color of blue
        single column
            PrmAction
                label is "Action"
        single column
            PrmActionDate
                label is "ActionDate"
