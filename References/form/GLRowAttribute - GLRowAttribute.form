GLRowAttribute is a Form
    is primary
    title is "RowDimensionAttribute"
    Layout
        GLRowDimension.Dimension
            label is "Dimension"
            show up to 100 characters
        paragraph
            BirstModelBCField
                label is "AttributeID"
                select is BirstModelBCFieldRel.BirstModelBCFields
                when value changed
                    ColumnHeader =  BirstModelBCField.BirstFieldName
            visible when (RecordExists)
                BirstModelBCField.BirstFieldName
                    label is "AttributeName"
        ColumnHeader
            label is "Header"
        ColumnSize
            label is "Size"
