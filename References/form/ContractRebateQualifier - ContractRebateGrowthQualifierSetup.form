ContractRebateGrowthQualifierSetup is a Form
    title is "<mode>ContractRebateQualifier"
    Layout
        paragraph
            ContractRebate.ContractRebateType
                label is "RebateType"
                display as text
        paragraph
            PurchaseType
        paragraph
            TargetedPurchasesFromDate
                label is "TargetedPurchasesFrom"
            TargetedPurchasesToDate
                label is "Through"
        paragraph
            SpendType
                label is "Type"
        paragraph
            AllInvoicesForSupplier
        header2 of "EstimatedRebateAmounts"
        paragraph
            PriorYearPurchaseAmount
            GrowthRebatePercent
                label is "ProjectedRebatePercent"
            ProjectedRebateAmount
                label is "Projected_Rebate"
