WarehousePickListPrintPivotAction is an ActionForm
    action is WarehousePickListPrintPivotAction
    title is "WarehousePickListPrint"
    Layout
        paragraph
            PrmCompany
                label is "Company"
                when value changed
                    refresh PrmLocation
            PrmCompany.Name
                no label
                display as text
        paragraph
            PrmLocation
                label is "Location"
                show up to 10 characters
                when value changed
                    refresh PrmWarehouseWave
                    refresh PrmUseZonePick
            PrmLocation.Name
                no label
                display as text
        header2 of "Main"
        two column distributed
            column1
                PrmExpectedShipDate
                    label is "ExpectedShipDate"
                PrmSelectOption
                    label is "PrintOption"
                    show up to 20 characters
                visible when (PrmLocation.UseAsWarehouseLocation)
                    PrmUseZonePick
                        label is "UseZonePicking"
                PrmAllocateDetail
                    label is "AllocateDetails"
                PrmIncludeCost
                    label is "IncludeCost"
                    show up to 20 characters
                PrmPrintDeliveryData
                    label is "PrintDeliveryData"
                visible when (not Company.UseIDM)
                    PrmAlignForm
                        label is "AlignmentForms"
                PrmReprintBatchNumber
                    label is "ReprintBatch"
                PrmCreateFlatFile
                    label is "CreateFlatFile"
            column2
                PrmDocumentSource
                    label is "DocumentSource"
                PrmBinAllocationPurpose
                    label is "BinAllocationPurpose"
                PrmPreferOption
                    label is "ForcePreferredBin"
                PrmIncludeBackorders
                    label is "IncludeBackorders"
                PrmIncludeKillDetails
                    label is "IncludeKillDetails"
                PrmPrintRequester
                    label is "PrintRequester"
                PrmReprintShipmentNumber
                    label is "ReprintShipment"
                    select is ShipmentsForLocation
                PrmFlatFileName
                    label is "FileName"
        visible when (PrmCompany.UseIDM)
            header2 of "IDMOptions"
            paragraph
                PrmIDMPrinter
                PrmIDMPrinter.Description
                    no label
                    display as text
            paragraph
                PrmConsolidateFile
        header2 of "Filter"
        paragraph
            PrmDocumentNumber.DocNbr[1]
                label is "SourceDocument"
            PrmDocumentNumber.DocNbr[2]
                no label
            PrmDocumentNumber.DocNbr[3]
                no label
            PrmDocumentNumber.DocNbr[4]
                no label
        paragraph
            PrmDocumentNumber.DocNbr[5]
                no label
            PrmDocumentNumber.DocNbr[6]
                no label
            PrmDocumentNumber.DocNbr[7]
                no label
            PrmDocumentNumber.DocNbr[8]
                no label
        paragraph
            PrmDocumentNumber.DocNbr[9]
                no label
            PrmDocumentNumber.DocNbr[10]
                no label
            PrmDocumentNumber.DocNbr[11]
                no label
            PrmDocumentNumber.DocNbr[12]
                no label
        blank line
        paragraph
            PrmRequestingLocation
                label is "RequestingLocation"
                show up to 20 characters
            PrmRequestingLocation.Name
                no label
                display as text
            text of "\\or"
            PrmReqLocationGroup
                label is "RequestingLocationGroup"
            PrmReqLocationGroup.Name
                no label
                display as text
        paragraph
            PrmCustomer
                label is "Customer"
                show up to 20 characters
            PrmCustomer.Name
                no label
                display as text
            visible when (PrmLocation.UseAsWarehouseLocation)
                PrmWarehouseWave
                    label is "Wave"
        paragraph
            PrmItem
                label is "Item"
                show up to 20 characters
            PrmItem.Description
                no label
                display as text
        paragraph
            PrmProcessType
                label is "ProcessType"
                show up to 20 characters
            PrmProcessType.Description
                no label
                display as text
        paragraph
            PrmFromAllocPriority
                label is "FromPriority"
            PrmToAllocPriority
                label is "ToPriority"
        blank line
        paragraph
            PrmShipTo
                label is "ShipTo"
                show up to 20 characters
            PrmShipTo.Name
                no label
                display as text
        paragraph
            PrmCarrier
                label is "Carrier"
                show up to 20 characters
            PrmCarrier.VendorName
                no label
                display as text
        paragraph
            PrmRoute
                label is "Route"
                compact format
            PrmAllRoute
                label is "_"
        paragraph
            PrmSkipInsufBinDetails
                label is "SkipItemsWithInsufficientBinDetails"
