ApprovalHistory2 is a Form
    print form is ApprovalHistorySummaryForm2
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        single column
            visible when (first PfiWorkunitRel2.HasProcessIndicator)
                header2 of "Progress"
                first PfiWorkunitRel2.PfiProcessIndicatorRel.Model
                    no label
                    display as text
            UserActionRoutingAsHtml2
                no label
                display as text
            first PfiWorkunitRel2.ElapsedTimeAsString
                label is "ApprovalDuration"
