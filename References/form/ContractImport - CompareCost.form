CompareCost is an ActionForm
    action is CompareContractInterfaceCost
    title is "CompareInterfaceContractCostToContract"
    Layout
        paragraph
        single column
            ContractGroup
                display as text
            ContractImport
                display as text
            CompareContract
        paragraph
            CompareFromDate
                label is "DateRangeForQuantityOrderedCalculation"
            text of "-"
            CompareToDate
                no label
        single column
            ShowZeroDifference
                label is "CreateRecordForLinesWithNoCostDifference"
                    left of field
            ShowZeroOrdered
                label is "CreateRecordForLinesWithNoOrderedQuantity"
                    left of field
            visible when (TierCostsExist)
                CompareToTier
