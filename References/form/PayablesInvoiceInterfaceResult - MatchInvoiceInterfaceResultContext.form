MatchInvoiceInterfaceResultContext is a Form
    title is "MatchInvoiceInterfaceResult"
    Layout
        display as text
            visible when (InvoicesStillProcessing)
                paragraph
                    text of "<StillProcessingMsg>"
                        bold
                blank line
            three column distributed
                PayablesInvoiceInterfaceResult
                    label is "ID"
                DerivedStatus
                    label is "Status"
                paragraph
                    RunGroup
                    RunTime
                        no label
            paragraph
                visible when (IncompleteHasPendingRecords)
                    button of "Resubmit"
                        action is ResubmitMatchInvoiceInterface
                visible when (MatchOrReleaseError)
                    text of "InterfaceSuccessfulButSomeInvoicesDidNotMatch"
                        bold
                        italics
                        color of red
                visible when (ShowAutoMatch)
                    text of "AutoMatchSubmittedForInterfacedInvoices"
                        bold
                        italics
                        color of green
                visible when (NoRecordsToProcess)
                    text of "<NoRecordsToProcessMsg>"
                        bold
