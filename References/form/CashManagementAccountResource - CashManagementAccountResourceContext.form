CashManagementAccountResourceContext is a Form
    title is "Resource"
    Layout
        three column distributed
            column1
                CashManagementAccount
                paragraph
                    FinanceResource
                        label is "Resource"
                    FinanceResource.PreferredSimplePresentationName
                        no label
                        display as text
                FinanceResource.Title
                    display as text
            column2
                CashManagementAccount.Title
                    label is "AccountTitle"
                    display as text
                button of "<FinanceResource.EmailAddress>"
                    link is "mailto:<FinanceResource.EmailAddress>"
                    display as link
                FinanceResource.USResident
                    display as text
            column3
                Active
                FinanceResource.FullWorkPhone
                    label is "PhoneNumber"
                    display as text
