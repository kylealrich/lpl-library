AcknowledgementMessageMainDetail is a Form
    Layout
        header2 of "GeneralInformation"
        three column distributed
            column1
                PurchaseOrderLineRel.Item
                    display as text
                text of "EDI_Values"
                    header4
                EDIAmount
                    label is "UnitCost"
                    display as text
                EDIAlpha
                    label is "UnitOfMeasure"
                    display as text
                EDINumeric
                    label is "Quantity"
                    display as text
                visible when (PurchaseOrderLineSourceExists)
                    first PurchaseOrderLineSourceRel.RequestingLocation.RepresentativeText
                    display as text
            column2
                PurchaseOrderLineRel.Quantity
                    display as text
                text of "PO_Values"
                    header4
                PurchaseOrderLineRel.VendorBuyUnitCost
                    label is "UnitCost"
                    display as text
                PurchaseOrderLineRel.EnteredBuyUOM
                    label is "UnitOfMeasure"
                    display as text
                PurchaseOrderLineRel.Quantity
                    label is "Quantity"
                    display as text
            column3
                PurchaseOrderLineRel.EnteredBuyUOM
                    display as text
                visible when (POLineHasContract)
                    button of "<PurchaseOrderLineRel.ContractLine.BaseCost>"
                        link is ContractLineLink
                        label is "ContractUnitCost"
                        display as link
                    ContractRel.ContractType
                        label is "ContractType"
                        display as text
                    visible when (ContractDiffFromWorkingContract)
                        ContractRel.WorkingContractID
                            label is "WorkingContract"
        single column
            MessageDisplay
                label is "Message"
                display as text
