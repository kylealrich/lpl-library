PayablesBillOfExchangeDistribution is a Form
    is primary
    Layout
        header2 of "BillOfExchange"
        three column distributed
            column1
                paragraph
                    BankTransactionCode
                        label is "PaymentCode"
                    BankTransactionCode.Description
                        no label
                        display as text
                paragraph
                    PayablesBillOfExchange.DraftNumber
                        label is "BillOfExchangeNumber"
                    PayablesBillOfExchange.Suffix
                PayablesBillOfExchange.DraftDate
                    label is "BillOfExchangeDate"
                    display as text
                PayablesBillOfExchange.CashCode
            column2
                paragraph
                    Vendor
                        label is "PaymentVendor"
                        select is VendorName
                    Vendor.VendorName
                        no label
                        display as text
                PayablesBillOfExchange.Status
                    display as text
                PayablesBillOfExchange.DraftAmount
                    label is "BillOfExchangeAmount"
                    display as text
                PayablesBillOfExchange.CashCode.Description
                    display as text
            column3
                paragraph
                    PayablesBillOfExchange.RemitToCode
                        label is "RemitToLocation"
                        select is RemitToLocations
                    PayablesBillOfExchange.RemitToCode.VendorName
                        no label
                        display as text
                PayablesBillOfExchange.MaturityDate
                    label is "DueDate"
                    display as text
                PayablesBillOfExchange.Currency
                    label is "BOECurrency"
                    display as text
                PayablesBillOfExchange.CashCode.Currency
                    label is "CashCodeCurrency"
        header2 of "Distribution"
        three column distributed
            column1
                paragraph
                    Company
                    Company.Name
                        no label
                        display as text
                paragraph
                    Invoice
                    InvSuffix
                        label is "Suffix"
                paragraph
                    TransactionNumber
                    Suffix
                PostDate
            column2
                paragraph
                    PayGroup
                        select is PayGroups
                    PayGroup.Name
                        no label
                        display as text
                InvCancelSeq
                    label is "CancelSequence"
                Currency
                AccrualCode
            column3
                paragraph
                    InvVendor
                        label is "InvoiceVendor"
                    InvVendor.VendorName
                        no label
                        display as text
                InvVoidSeq
                    label is "VoidSequence"
                DistributionType
                BillOfExchangeAccrualCode
            column4
        header2 of "Ledger"
        single column
            GLTransactionAmount
                label is "DistributionAmount"
            GLFinanceCodeBlock
                label is "FinanceStructure"
                compact format
        blank line
        header2 of "CurrencyAmounts"
        paragraph
            visible when (false)
                GLBaseAmount.AssignToCurrency
            GLBaseAmount.FunctionalAmount.EnteredCurrencyAmount
                label is "Functional"
                display only
            GLBaseAmount.AccountingEntity.FunctionalCurrency
                no label
                display only
            GLBaseAmount.FunctionalAmount.EnteredCurrencyRate
                no label
                display only
        visible when (GLBaseAmount.AccountingEntity.AlternateCurrencyExists)
            paragraph
                GLBaseAmount.AlternateAmount.EnteredCurrencyAmount
                    label is "Alternate"
                    display only
                GLBaseAmount.AccountingEntity.AlternateCurrency
                    no label
                    display only
                GLBaseAmount.AlternateAmount.EnteredCurrencyRate
                    no label
                    display only
        visible when (GLBaseAmount.AccountingEntity.AlternateCurrency2Exists)
            paragraph
                GLBaseAmount.AlternateAmount2.EnteredCurrencyAmount
                    label is "Alternate2"
                    display only
                GLBaseAmount.AccountingEntity.AlternateCurrency2
                    no label
                    display only
                GLBaseAmount.AlternateAmount2.EnteredCurrencyRate
                    no label
                    display only
        visible when (GLBaseAmount.AccountingEntity.AlternateCurrency3Exists)
