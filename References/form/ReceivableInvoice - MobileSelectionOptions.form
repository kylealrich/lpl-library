MobileSelectionOptions is a CompositeForm
    title is "Transaction"
    context form is MobileReceivableInvoiceContext
    show panel control on left
    restrict action Create
        when (IsInvoiceBatchReleased)
    MainUnreleased is a Panel
        title is "Main"
        visible when (IsUnreleased)
        form is MobileMainUnreleased
    Main is a Panel
        visible when (IsReleased)
        form is MobileMain
    Expanded is a Panel
        form is MobileExpanded
    InvoiceDetail is a Panel
        title is "InvoiceDetails"
        visible when (ShowInvoiceDetail)
        list is ReceivableInvoiceDetailRel.MobileListReceivableInvoiceDetail
    LineItems is a Panel
        visible when (ShowLineItems)
        list is ReceivableInvoiceLineItemRel.MobileListReceivableInvoiceLineItem
    Distributions is a Panel
        list is ReceivableGLDistributionDetailTypeRel.MobileReceivableInvoiceDistributionList
    PendingObligations is a Panel
        visible when (IsReceivableInvoiceObligation)
        mouse over text is "MaintainPaymentObligations"
        list is ReceivableInvoiceObligationRel.ReceivableInvoiceObligationList
    Disputes is a Panel
        visible when (IsOpenDisputes)
        mouse over text is "MaintainOpenDisputes"
        list is OpenReceivableInvoiceDisputeRel.Disputes
    Comments is a Panel
        list is CompanyCustomerCommentRel.MobileInvoiceTransactionCommentsList
    Attachments is a Panel
        list is ReceivableInvoiceAttachment set.MobileAttachments
    OriginatingDocuments is a MultiListPanel
        visible when (ShowOriginatingDocuments)
        list is IntercompanyBillingDocumentRel.OriginatingJournalControlRel.GeneralLedgerJournalList
            visible when (OriginatingICBIsFromGL)
        list is IntercompanyBillingDocumentRel.OriginatingInvoiceRel.PayablesInvoiceList
            visible when (OriginatingICBIsFromInvoice)
        list is IntercompanyBillingDocumentRel.ReleasedDocumentList
