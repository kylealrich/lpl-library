TransactionDetailsForm is a Form
    title is "TransactionDetail"
    Layout
        display as text
            blank line
            single column
                Reference
                Description
                Operator
                    label is "CreatedBy"
        blank line
        header2 of "CurrencyAmounts"
        paragraph
            visible when (false)
                TransactionAmount.AssignToCurrency
            TransactionAmount.FunctionalAmount.EnteredCurrencyAmount
                label is "Functional"
                display only
            DerivedFunctionalCurrency
                no label
                display only
            TransactionAmount.FunctionalAmount.EnteredCurrencyRate
                no label
                display only
        visible when (TransactionAmount.AccountingEntity.AlternateCurrencyExists)
            paragraph
                TransactionAmount.AlternateAmount.EnteredCurrencyAmount
                    label is "Alternate"
                    display only
                TransactionAmount.AccountingEntity.AlternateCurrency
                    no label
                    display only
                TransactionAmount.AlternateAmount.EnteredCurrencyRate
                    no label
                    display only
        visible when (TransactionAmount.AccountingEntity.AlternateCurrency2Exists)
