RequisitionLineDistributionForm is a Form
    Layout
        single column
            Company
                display as text
            DocumentNumberNumeric
                label is "RequisitionNumber"
                display as text
            LineNumber
                display as text
        blank line
        header2 of "AccountDistribution"
        paragraph
            DistributionAccount
                label is "Account"
                compact format
            DistributionPercent
                label is "Percent"
            DistributionAmount
                label is "Amount"
            DistributionQuantity
                label is "Quantity"
        blank line
        header2 of "DistributionDetails"
        paragraph
            AssetTemplate
                select is AssetTemplates
        paragraph
            Asset
                select is AssetCodes
