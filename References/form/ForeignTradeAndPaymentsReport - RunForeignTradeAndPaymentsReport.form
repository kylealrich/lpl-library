RunForeignTradeAndPaymentsReport is an ActionForm
    action is RunForeignTradeAndPaymentsReport
    title is "ForeignTradeAndPaymentsReport"
    Layout
        paragraph
            PrmCompany
                label is "Company"
            PrmCompany.Name
                no label
                display as text
        two column
            PaymentDateRange.Begin
                label is "FromDate"
            PaymentDateRange.End
                label is "ToDate"
        single column
            PrmDescription
                label is "Description"
