TabletConfigurationCompositeForm is a CompositeForm
    title is "Configuration"
    context form is ConfigurationForm
    SystemProperty is a Panel
        title is "<PropertyCount>"
        list is PfiConfigurationProperty set.ConfigurationPropertyList
    Connections is a MultiListPanel
        title is "Connections"
        list is PfiClassicConnection set.ClassicConnectionList
            show 3 lines
    Channels is a MultiListPanel
        title is "Channels"
        list is PfiFTPConnection set.FTPConnectionList
            show 2 lines
        list is PfiFileActivityConnection set.FileActivityConnectionList
            show 2 lines
        list is PfiIONConnection set.IONConnectionList
            show 2 lines
    Cloverleaf is a Panel
        title is "<CloverleafCount>"
        list is PfiCloverleafConnection set.CloverleafConnectionList
    JDBC is a Panel
        title is "<JDBCCount>"
        list is PfiJDBCConnection set.JDBCConnectionList
    JMS is a Panel
        title is "<JMSCount>"
        list is PfiJMSConnection set.JMSConnectionList
    LdapConnection is a Panel
        title is "<LdapConnectionCount>"
        list is PfiLdapConnection set.LdapConnectionList
    MQ is a Panel
        title is "<MQCount>"
        list is PfiMQConnection set.MQConnectionList
    SystemCommand is a Panel
        title is "<SysCmdCount>"
        list is PfiSystemCommandConnection set.SystemCommandConnectionList
    Web is a Panel
        title is "<WebCount>"
        list is PfiWebRunConnection set.WebRunConnectionList
    WebService is a Panel
        title is "<WebServiceCount>"
        list is PfiWebServiceConnection set.WebServiceConnectionList
    Tx is a Panel
        title is "<TxCount>"
        list is PfiTXConnection set.TXConnectionList
