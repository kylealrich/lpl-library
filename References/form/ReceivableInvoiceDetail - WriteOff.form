WriteOff is an ActionForm
    action is WriteOff
    Layout
        display as text
            three column table
                column1
                    Company.RepresentativeText
                        label is "Company"
                    Customer.RepresentativeText
                        label is "Customer"
                column2
                    Company.Currency
                        label is "Currency"
                    DerivedSignedTransactionAmountSummarized
                column3
                    paragraph
                        ReceivableInvoiceDetail.ReceivableInvoiceDetailType
                            label is "Type"
                        ReceivableInvoiceDetail.Invoice
                            label is "Invoice"
                        ReceivableInvoiceDetail.PaymentSeq
                            label is "Sequence"
                    DerivedSignedOpenAmountSummarized
                        label is "OpenAmount"
        paragraph
            PrmGeneralLedgerDate
                label is "GlobalLedgerDate"
            visible when (Company.CashOperatorRequired)
                PrmOperator
                    label is "Operator"
