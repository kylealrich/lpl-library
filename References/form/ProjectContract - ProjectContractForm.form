ProjectContractForm is a CompositeForm
    is primary
    title is "<FinanceEnterpriseGroup.ProjectLabel>_Contract_<DisplayContract>"
    context form is ProjectContractContextForm
        background is theme color
    ContractDetails is a Panel
        title is "Main"
        form is ProjectContractDetailsForm
    Totals is a Panel
        title is "ContractTotals"
        form is ProjectContractTotalsForm
    ContractFundingSources is a Panel
        title is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
        list is ProjectFundingSource set.ContractFundingSources
    ProjectFundingSourceChangeRequest is a Panel
        title is "<FinanceEnterpriseGroup.FinanceDimension2Label>ChangeRequests"
        visible when (HasProjectFundingSourceChangeRequest)
        list is ProjectFundingSourceChangeRequest set.ProjectFundingSourceChangeRequestList
    ContractModifications is a Panel
        title is "Modifications"
        visible when (not RequiresApproval)
        list is ProjectContractModification set.ProjectContractModificationList
    PostingProjects is a Panel
        title is "Posting<FinanceEnterpriseGroup.ProjectLabel>"
        list is AllPostingProjectsRel.ContractPostingProjectList
    ProjectContractChangeRequest is a Panel
        title is "ContractChangeRequests"
        visible when (HasProjectContractChangeRequest)
        list is ProjectContractChangeRequest set.ProjectContractChangeRequestList
    ContractFees is a Panel
        title is "Fees"
        list is ProjectContractFee set.ProjectContractFees
    PercentComplete is a Panel
        title is "PercentComplete"
        visible when (HasPercentComplete)
        list is ProjectContractPercentCompleteAllRel.ContractPercentCompleteList
    ProjectVendorContract is a Panel
        title is "Vendors"
        list is ProjectVendorContractsRel.DisplayVendorContract
    InvoiceDetail is a Panel
        title is "Invoices"
        visible when (InvoicesExist)
        list is ProjectContractInvoiceRel.ProjectContractInvoices
    RevenueRecognition is a Panel
        title is "Revenue"
        visible when (RevenueExists)
        list is ProjectContractRevenueRel.ProjectContractRevenueList
    ReceivableInvoices is a Panel
        title is "ReceivableInvoices"
        visible when (HasReceivableInvoices)
        list is ReceivableInvoiceRel.ProjectContractReceivableInvoice
    ReceivablePrepayments is a Panel
        title is "Prepayments"
        visible when (HasReceivablePrepayments)
        list is ReceivablePrepaymentRel.ProjectContractPrepayments
    FrontEndSplitDistributions is a Panel
        visible when (FrontEndSplitsExist)
        list is ProjectContractFESDistribution set.ProjectContractFESDistributionList
    FrontEndSplitResults is a Panel
        visible when (FrontEndSplitResultsExist)
        list is ProjectContractFESResult set.ProjectContractFESResultList
    SelectedTransactionsForReallocation is a Panel
        title is "SelectedTransactions"
        visible when (ProjectContractReallocationSelectionsExist)
        list is ProjectContractReallocationSelection set.ProjectContractReallocationSelectionList
    ReallocationResults is a Panel
        visible when (ReallocationResultsExist)
        list is ProjectContractReallocationResult set.ProjectContractReallocationResultList
    PartiallyBilledTransactions is a Panel
        title is "PartiallyBilled"
        visible when (PartialBilledTransactionsExist)
        list is PartialBilledTransactionRel.ProjectContractPartialBilledTransactions
    PartiallyRecognizedTransactions is a Panel
        title is "PartiallyRecognized"
        visible when (PartialRecognizedTransactionsExist)
        list is PartialRecognizedTransactionRel.ProjectContractPartialRecognizedTransactions
    Errors is a Panel
        visible when (ErrorDetailExists)
        list is ProjectContractInvoiceErrorRel.ProjectContractInvoiceErrors
    Balances is a Panel
        title is "LTDBalances"
        visible when (ProjectContractInvoiceAllBalancesRelExists)
        list is ProjectContractInvoiceAllBalancesRel.ProjectContractInvoiceBalances
    Adjustments is a Panel
        title is "BalanceAdjustments"
        list is ProjectContractBalanceAdjustmentRel.ProjectContractBalanceAdjustments
    GlobalLedgerTotals is a Panel
        title is "Totals"
        list is ProjectContractGeneralLedgerTotalsRel.GeneralLedgerProjectContractTotalSearchList
    PerformanceObligations is a Panel
        title is "PerformanceObligations"
        list is ProjectPerformanceObligations set.ProjectContractPerformanceObligationsList
    UserFieldsPanel is a Panel
        title is "UserFields"
        form is UserFieldsForm
    Comments is a Panel
        list is ProjectContractComment set.ProjectCommentForAContractProjectList
    InvoiceExport is a Panel
        title is "InvoiceExport"
        visible when (ExternalInvoicesExist)
        list is ProjectContractInvoiceExternalRel.ProjectContractInvoicesExport
    InvoiceReversalExport is a Panel
        title is "ReversalExport"
        visible when (ExternalReversalInvoicesExist)
        list is ProjectContractInvoiceExternalReversalRel.ProjectContractReversalInvoicesExport
