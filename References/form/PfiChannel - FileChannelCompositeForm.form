FileChannelCompositeForm is a CompositeForm
    use for action FileChannel.Create
    use for action FileChannel.Update
    use for action Delete
    use for desktop
    use for browser
    use for tablet
    title is "FileChannel"
    restrict action TestConnectionWithTraceLog
        when (not FileChannel.FileChannelType.Remote)
    Overview is a Panel
        form is FileChannelContextForm
    FileChannelReceivers is a Panel
        list is PfiReceiver set.FileReceiverList
    OnErrorEmailNotificationInformation is a Panel
        visible when (NotifyConnectLoss.OnError)
        list is PfiChannelField set.EmailConfigurationList
