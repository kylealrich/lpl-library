EFTNotificationPrint is an ActionForm
    action is EFTNotificationPrint
    Layout
        paragraph
            FinanceEnterpriseGroup
            FinanceEnterpriseGroup.EnterpriseGroup.Description
                no label
                display as text
        paragraph
            CustGroup
                label is "CustomerGroup"
            CustGroup.Name
                no label
                display as text
        single column
            text of "Or"
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            BankTransactionCode
                label is "PaymentCode"
                select is CashPaymentAndOriginReceivablesList
            BankTransactionCode.Description
                no label
                display as text
        blank line
        single column
            DataFileName
        visible when (Company.UseIDM)
            header2 of "PrinterOption"
            paragraph
                IDMPrinter
                IDMPrinter.Description
                    no label
                    display as text
        visible when (CustGroup.UseIDMTemplate)
            header2 of "PrinterOption"
            paragraph
                IDMPrinter
                IDMPrinter.Description
                    no label
                    display as text
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                EFTNotificationPrintReport_RDG
            column2 of 25%
                EFTNotificationPrintReport_XPT
