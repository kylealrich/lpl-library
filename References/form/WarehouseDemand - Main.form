Main is a Form
    Layout
        two column
            DocumentNumberNumeric
                display as text
            RequireFreight
                display as text
            ShippingMethod
                display as text
            TransactionId
                display as text
            Currency
                display as text
            WarehouseProcessType
                display as text
            FreightCode
                display as text
        three column
            RequireCompleteShipment
                display as text
            NotReadyForShipmentCompletion
                display as text
            OnHold
                display as text
            RequirePickingFeedback
                display as text
            RequirePackingFeedback
                display as text
            CashOnDelivery
                display as text
        two column
            DestinationAddress
                label is "DestinationAddress"
                display as text
            BillingAddress
                label is "BillingAddress"
                display as text
