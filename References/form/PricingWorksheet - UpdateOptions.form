UpdateOptions is a Form
    Layout
        header2 of "Source"
        two column distributed
            column1
                paragraph
                    SourceCurrency
                        label is "Currency"
                    SourceCurrency.Description
                        no label
                        display as text
                paragraph
                    SourceLocation
                        label is "Location"
                    SourceLocation.Name
                        no label
                        display as text
                IssueCostPriority
                    label is "LastIssueCost"
            column2
                paragraph
                    SourceBaseName
                        label is "BaseName"
                    SourceBaseName.Description
                        no label
                        display as text
                paragraph
                    text of "Or"
                        bold
                    Location
                        label is "InventoryLocation"
                    Location.Name
                        no label
                        display as text
                LastReceivedCostPriority
                    label is "LastReceivedCost"
        header2 of "Update"
        paragraph
            UpdateCurrency
                label is "Currency"
            UpdateCurrency.Description
                no label
                display as text
        paragraph
            UpdateBaseName
                label is "BaseName"
            UpdateBaseName.Description
                no label
                display as text
        paragraph
            UpdateLocation
                label is "Location"
            UpdateLocation.Name
                no label
                display as text
        paragraph
            PlannedEffectiveDate
                label is "EffectiveDate"
