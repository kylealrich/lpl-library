ResolutionsComposite is a CompositeForm
    title is "ResolutionsFor<Owner.FirstLastName>"
    context form is ResolutionsContext
        background is theme color
    Actions
        navigation is RecallDocumentReport
            action icon is print
        navigation is RecallTaskReport
            action icon is print
        navigation is ResponderTransactionDetails
            label is "ViewDetails"
        action is refresh
            label is "_"
    Products is a Panel
        list is RecallProductTaskItemRel.ItemsWithActionsList
