RSSLPLRequisitionLineContext is a Form
    Layout
        display as text
            five column distributed
                column1
                    visible when (RSSLPLDisplayCompany)
                        Company.RepresentativeText
                            label is "Company"
                                header4
                            header3
                    visible when (RSSLPLDisplayRequestingLocationAndCompanyHidden)
                        Requisition.RequestingLocation.RequestingLocationNameID
                            label is "RequestingLocation"
                                header4
                            header3
                    visible when (RSSLPLHideCompanyAndRequestingLocation)
                        Requisition.RSSLPLDerivedTotalCurrencyField
                            label is "Total"
                                header4
                            header3
                    paragraph
                        visible when (HasContract)
                            button of "<OnContractLabel>" with icon.new-solid
                                link is ContractLineNav
                                display as link
                        visible when (OriginalItem entered)
                            SubstituteItemLabel
                                no label
                                display as tag
                                    color of yellow
                        visible when (LatexItem)
                            LatexLabel
                                no label
                                display as tag
                                    color of orange
                        visible when (MissingDistribution)
                            MissingDistribMessage
                                no label
                                display as tag
                                    color of yellow
                        visible when (ItemIsBackordered)
                            VendorBackorderMessage
                                no label
                                display as tag
                                    color of yellow
                        visible when (LineErrorsExist)
                            ErrorsLabel
                                no label
                                display as tag
                                    color of red
                        visible when (Item.Recipe)
                            RecipeItemLabel
                                no label
                                display as tag
                                    color of blue
                column2
                    visible when (RSSLPLDisplayCompanyAndRequestingLocation)
                        Requisition.RequestingLocation.RequestingLocationNameID
                            label is "RequestingLocation"
                                header4
                            header3
                    visible when (RSSLPLDisplayRequestingLocationAndCompanyHidden)
                        Requisition.RSSLPLDerivedTotalCurrencyField
                            label is "Total"
                                header4
                            header3
                    visible when (RSSLPLDisplayCompanyAndRequestingLocationHidden)
                        Requisition.RSSLPLDerivedTotalCurrencyField
                            label is "Total"
                                header4
                            header3
                    visible when (RSSLPLHideCompanyAndRequestingLocation)
                        Requisition.DerivedStatusMessage
                            label is "RequisitionStatus"
                                header4
                            display as tag
                                color of blue when (Requisition.IsUnreleased)
                                color of green when (Requisition.Status.Processed)
                                color of red when (Requisition.IsRejectedStatus)
                                color of yellow when (Requisition.Status.InProcess)
                                color of yellow when (Requisition.IsReleaseInProgress)
                                color of yellow when (Requisition.IsCreateInProgress)
                                color of yellow when (Requisition.NeedsApproval)
                column3
                    visible when (RSSLPLDisplayCompanyAndRequestingLocation)
                        Requisition.RSSLPLDerivedTotalCurrencyField
                            label is "Total"
                                header4
                            header3
                    visible when (RSSLPLDisplayRequestingLocationAndCompanyHidden)
