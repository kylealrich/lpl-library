                            display as tag
                                color of blue when (Requisition.IsUnreleased)
                                color of green when (Requisition.Status.Processed)
                                color of red when (Requisition.IsRejectedStatus)
                                color of yellow when (Requisition.Status.InProcess)
                                color of yellow when (Requisition.IsReleaseInProgress)
                                color of yellow when (Requisition.IsCreateInProgress)
                                color of yellow when (Requisition.NeedsApproval)
                    visible when (RSSLPLDisplayCompanyAndRequestingLocationHidden)
                        Requisition.DerivedStatusMessage
                            label is "RequisitionStatus"
                                header4
                            display as tag
                                color of blue when (Requisition.IsUnreleased)
                                color of green when (Requisition.Status.Processed)
                                color of red when (Requisition.IsRejectedStatus)
                                color of yellow when (Requisition.Status.InProcess)
                                color of yellow when (Requisition.IsReleaseInProgress)
                                color of yellow when (Requisition.IsCreateInProgress)
                                color of yellow when (Requisition.NeedsApproval)
                    visible when (RSSLPLHideCompanyAndRequestingLocation)
                        DerivedStatusMessage
