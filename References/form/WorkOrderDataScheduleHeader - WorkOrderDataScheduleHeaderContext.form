WorkOrderDataScheduleHeaderContext is a Form
    Layout
        visible when (RecordExists)
            display as text
                two column distributed
                    column1
                        Company.RepresentativeText
                            label is "Company"
                    column2
                        InventoryLocation.RepresentativeText
                            label is "Location"
                paragraph
                    Item.RepresentativeText
                        label is "FinishedGood"
                    Item.StockUOM
                        label is "UOM"
                        display as text
