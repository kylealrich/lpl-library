ActionRequestForm is a Form
    title is "ActionRequest"
    Layout
        single column
            ActionRequest
                display only
            Title
                display only
            BusinessClass
                display only
            BusinessAction
                display only
            BusinessObjectReference.BusinessObjectKey
                label is "Key"
                display only
            Status
                display only
        single column
            ProcessName
                display only
        two column
            WorkUnit
                display only
            StatusPending
                display only
        single column
            create stamp.actor
                label is "Actor"
                display only
            visible when (not IsOpen)
                FinalizedStamp
                    label is "<Status>Time"
                    display only
            ParameterView
                display only
