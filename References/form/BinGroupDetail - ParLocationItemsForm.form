ParLocationItemsForm is a Form
    use for action ParLocationUpdate
    title is "ParLocationCounts"
    Layout
        paragraph
            Company
                display as text
            Company.Name
                no label
                display as text
        paragraph
            InventoryLocation
                label is "ParLocation"
                display as text
            InventoryLocation.Name
                no label
                display as text
        blank line
        single column
            Requisition.Requester
                display as text
            Requisition
                display as text
            Requisition.FillOrKill
                display as text
        blank line
        single column
            TransientBinSequence
                display as text
            TransientBin
                label is "ShelfLocation"
                display as text
        blank line
        paragraph
            TransientItem
                display as text
            DerivedItemDescription
                no label
                display as text
        paragraph
            TransientUOM.UnitOfMeasure
                label is "UnitOfMeasure"
                display as text
        paragraph
            TransientParLevel
                label is "ParLevel"
                display as text
        paragraph
            TransientParLocationCount
                label is "Count"
