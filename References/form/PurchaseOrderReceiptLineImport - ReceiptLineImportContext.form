ReceiptLineImportContext is a Form
    title is "InterfaceReceiptLine"
    Layout
        four column distributed
            column1
                paragraph
                    Company.PurchasingCompany.RepresentativeText
                        label is "Company"
                        display as text
                paragraph
                    PurchaseOrderReceiptLineImport.LineNumber
                        label is "LineNumber"
                        raise red alert when (HasLineAddOnCostsWithInvalidRunGroup)
                            mouse over text is "<HasLineAddOnCostsWithInvalidRunGroupMsg>"
                paragraph
                    Item
                        show up to 10 characters
                visible when (IsHSNSACCodeEnabled)
                    paragraph
                        HSNSACCode
                            show up to 10 characters
                        HSNSACCode.Description
                            no label
                            display as text
            column2
                paragraph
                    PurchaseOrderReceiptImport
                        label is "ReferenceNumber"
                        display as text
                paragraph
                    PurchaseOrderReceiptLineImport.SeqNbr
                        label is "Sequence"
                        display as text
                paragraph
                    EnteredReceivedQuantity
            column3
                paragraph
                    PurchaseOrderReceiptImport.Vendor.RepresentativeText
                        label is "Vendor"
                        display as text
                text of "_"
                paragraph
                    ReceivedUOM
                    visible when (IsCatchWeightItem)
                        CatchWeightQuantity
            column4
                text of "_"
                text of "_"
                paragraph
                    ItemLocationRel.PreferredBin
                        label is "PreferredBin"
                        display as text
