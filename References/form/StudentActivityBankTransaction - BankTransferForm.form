BankTransferForm is a CompositeForm
    use for action Entered.CreateTransfer
    title is "BankTransfer"
    context form is CreateTransferContextForm
    show panel control on left
    Actions
        action is save
        action is save and new
        action is Entered.Release
            action icon is release
            hide when invalid
        action is Entered.Delete
            action icon is delete
            hide when invalid
        action is next record
        action is previous record
        action is refresh
    TransferToBankAccount is a Panel
        visible when (TransferHeader)
        list is TransferToBankTransactionRel.StudentActivityTransferToBankList
    TransferFromBankAccount is a Panel
        visible when (TransferDetail)
        list is TransferFromBankTransactionRel.StudentActivityTransferToBankList
