ContractLineItemInformationForm is a Form
    Layout
        header2 of "ItemCodes"
        display as text
            single column
                ContractLine.UNSPSCCode
                    label is "UNSPSC"
                    compact format
            paragraph
                ContractLine.MajorPurchasingClass
                ContractLine.MinorPurchasingClass
            paragraph
                ContractLine.MajorInventoryClass
                ContractLine.MinorInventoryClass
            single column
                ContractLine.GenericName
            single column
                Upc
                    label is "UPC"
                Sku
                    label is "SKU"
                NationalDrugCode
                HibccUpn
                    label is "HibccUPN"
                UccEanUpn
                    label is "UccEanUPN"
                ContractLine.LabelerIdentificationCode
            header2 of "UserFields"
            single column
                ContractLine.UserField1
                    label is "ItemAlpha1"
                ContractLine.UserField2
                    label is "ItemAlpha2"
                ContractLine.UserField3
                    label is "ItemAlpha3"
                ContractLine.UserField4
                    label is "ItemAlpha4"
                ContractLine.UserField5
                    label is "ItemAlpha5"
                ContractLine.UserFieldN1
                    label is "ItemNumeric1"
                ContractLine.UserFieldN2
                    label is "ItemNumeric2"
                ContractLine.UserFieldN3
                    label is "ItemNumeric3"
