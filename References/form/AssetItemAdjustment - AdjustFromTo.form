AdjustFromTo is a Form
    title is "TransferFrom&ToDetails"
    Layout
        single column
            ItemNumber
                display as text
            ToItemNumber
                display as text
            visible when (IsDiscriptionAdjusted)
                Description
                    display as text
            visible when (IsDiscriptionAdjusted)
                ToDescription
                    display as text
            visible when (IsPurchaseDateAdjusted)
                PurchaseDate
                    display as text
            visible when (IsPurchaseDateAdjusted)
                ToPurchaseDate
                    display as text
            visible when (IsQuantityAdjusted)
                ItemQuantity
                    display as text
            visible when (IsQuantityAdjusted)
                ToItemQuantity
                    display as text
            visible when (IsCostAdjusted)
                TransactionItemCost
                    label is "Cost"
                    display as text
            visible when (IsCostAdjusted)
                ToTransactionItemCost
                    label is "ToCost"
                    display as text
            visible when (IsTaxAdjusted)
                TransactionItemTax
                    label is "Tax"
                    display as text
            visible when (IsTaxAdjusted)
                ToTransactionItemTax
                    label is "ToTax"
                    display as text
            paragraph
                visible when (IsPOAdjusted)
                    PONumber
                        label is "PurchaseOrder"
                        display as text
                    PORelease
                        no label
                        display as text
                    POCode
                        no label
                        display as text
            paragraph
                visible when (IsPOAdjusted)
                    ToPONumber
                        label is "ToPurchaseOrder"
                        display as text
                    ToPORelease
                        no label
                        display as text
                    ToPOCode
                        no label
                        display as text
            visible when (IsVendorAdjusted)
                Vendor
                    display as text
            visible when (IsVendorAdjusted)
                ToVendor
                    display as text
            visible when (IsVendoeNameAdjusted)
                VendorName
                    display as text
            visible when (IsVendoeNameAdjusted)
                ToVendorName
                    display as text
            visible when (IsInvoiceAdjusted)
                PayablesInvoice
                    display as text
            visible when (IsInvoiceAdjusted)
                ToPayablesInvoice
                    display as text
            visible when (IsModelAdjusted)
                ModelNumber
                    display as text
            visible when (IsModelAdjusted)
                ToModelNumber
                    display as text
            visible when (IsSerialAdjusted)
                SerialNumber
                    display as text
            visible when (IsSerialAdjusted)
                ToSerialNumber
                    display as text
            visible when (IsLocAdjusted)
                LocationDetail
                    display as text
            visible when (IsLocAdjusted)
                ToLocationDetail
                    display as text
            visible when (IsBarcodeAdjusted)
                BarCode
                    display as text
            visible when (IsBarcodeAdjusted)
                ToBarCode
                    display as text
            paragraph
                visible when (IsInvDateAdjusted)
                    InventoryDate
                        label is "InvetoryDateTime"
                        display as text
            paragraph
                visible when (IsInvDateAdjusted)
                    ToInventoryDate
                        label is "ToInvetoryDateTime"
                        display as text
            visible when (IsItemConditionAdjusted)
                ItemCondition
                    display as text
            visible when (IsItemConditionAdjusted)
                ToItemCondition
                    display as text
