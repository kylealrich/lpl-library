GLDesignerSummary is a Form
    Layout
        display as text
            three column distributed
                column1
                    GLReport
                        label is "ReportID"
                column2
                    ReportTitle
                        label is "Title"
                column3
                    visible when (CompositeReport)
                        CompositeReportTag
                            no label
                            display as tag
                                color of green
            single column
                Description
            three column distributed
                column1
                    visible when (not CompositeReport)
                        RowView
                            label is "Row"
                column2
                    visible when (not CompositeReport)
                        ColumnView
                            label is "Column"
            three column distributed
                visible when (not DerivedReplicationSetInProcess)
                    DerivedReplicationRefreshDate
                        label is "ReplicationRefreshDate"
                        display as text
                visible when (DerivedReplicationSetInProcess)
                    DerviedReplicationStatusLabel
                        label is "ReplicationRefreshDate"
                        color of orange
                        display as text
                paragraph
                    create stamp.actor
                        label is "CreatedBy"
                    update stamp.actor
                        label is "UpdatedBy"
            four column
                visible when (actor.context.FinanceEnterpriseGroup.ReportingPowerUserExists)
                    button of "GenerateReport" with icon.process
                        valid when (GenerateReportIsValid)
                        action is GenerateReport
                        color of red
                        raise red alert when (ReportHasInvalidSubreportorColumn)
                            mouse over text is "<DerivedAlertMessageforOverview>"
                        is primary
                button of "RunReport"
                    valid when (RunReportIsValid)
                    action is RunReport
                    is primary
                button of "AddToFavorites"
                    visible when (not IsActorFavoriteReport)
                    action is AddToFavorite
                    is primary
                button of "RemoveFromFavorites"
                    visible when (IsActorFavoriteReport)
                    action is RemoveFromFavorite
                    is primary
            visible when (not GenerateReportInProcess)
                single column
                    visible when (IntegrationStatus.OutOfSync)
                        paragraph
                            ReportOutOfSyncLabel
                                label is "Status"
                                color of orange
                                raise yellow alert when (GenerateReportHasFailedAsyncActionTriggers)
                                    mouse over text is "FailedTriggersExist"
                            DerivedGeneratedDate
                                label is "_"
                                display as text
                    visible when (IntegrationStatus.Generated)
                        paragraph
                            ReportGeneratedLabel
                                label is "Status"
                                color of green
                                raise yellow alert when (GenerateReportHasFailedAsyncActionTriggers)
                                    mouse over text is "FailedTriggersExist"
                            DerivedGeneratedDate
                                label is "_"
                                display as text
                    visible when (IntegrationStatus.NotGenerated)
                        paragraph
                            ReportNotGeneratedLabel
                                label is "Status"
                                color of red
                                raise yellow alert when (GenerateReportHasFailedAsyncActionTriggers)
                                    mouse over text is "FailedTriggersExist"
                            DerivedGeneratedDate
                                label is "_"
                                display as text
            single column
                visible when (GenerateReportInProcess)
                    ReportIsGeneratingTag
                        label is "Status"
                        display as tag
                            color of yellow
                visible when (HasMultiSelectReportFilter)
                    DrillbackNotPossibleTag
                        no label
                        display as tag
                            color of yellow
                visible when (ShowMultiSelectColumnFilterAlert)
                    ColumnDrillbackNotPossibleTag
                        no label
                        display as tag
                            color of yellow
                visible when (DerivedHasPeriodRange)
                    DrillbackNotPossibleWhenPeriodRangeTag
                        no label
                        display as tag
                            color of yellow
                visible when (DerivedColumnStructureDifferAccrossSubReports)
                    ColumnStructureDifferAccrossSubReportsTag
                        no label
                        display as tag
                            color of yellow
                visible when (DerivedHasDimStructureMismatch)
                    DimensionStructureMismatchTag
                        no label
                        display as tag
                            color of red
