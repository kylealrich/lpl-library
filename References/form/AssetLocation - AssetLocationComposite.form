AssetLocationComposite is a CompositeForm
    title is "AssetLocation<AssetLocation>_-_<Description>"
    context form is AssetLocationContextForm
        background is theme color
    Address is a Panel
        form is AssetLocationAddress
    LocationDetails is a Panel
        list is AssetLocationDetail set.AssetLocationDetailList
    Assets is a Panel
        list is AssetsRel.AllAssetslist
