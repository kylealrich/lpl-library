FreezeSelectedItems is an ActionForm
    action is FreezeSelectedItems
    Layout
        paragraph
            PrmCompany
                label is "Company"
            PrmCompany.Name
                no label
                display as text
        paragraph
            PrmSelectId
                label is "SelectID"
                select is SelectIDsToFreeze
            PrmSelectId.Description
                no label
                display as text
        paragraph
            PrmActorGroup
                label is "ReportNotificationGroup"
