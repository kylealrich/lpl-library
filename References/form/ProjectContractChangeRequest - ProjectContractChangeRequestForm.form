ProjectContractChangeRequestForm is a Form
    is primary
    Layout
        two column distributed
            column1
                paragraph
                    ProjectContract.DisplayContract
                        label is "<FinanceEnterpriseGroup.ProjectLabel>Contract"
                        display as text
                    ProjectContract.Project.Description
                        label is "Description"
                        display as text
        two column distributed
            column1
                paragraph
                    ChangeRequestDate
                        label is "ChangeRequest"
                    Description
        two column distributed
            column1
                paragraph
                    ProjectContract.ContractAmount
                        display as text
                    NewContractAmount
            column2
                paragraph
                    ProjectContract.MaximumFees
                        display as text
                    NewMaximumFees
        two column distributed
            column1
                paragraph
                    Scenario
                        label is "BudgetScenario"
                        select is ProjectBudgetsSelect
                        show up to 5 characters
                    Scenario.Description
                        display as text
                        show up to 30 characters
            column2
                paragraph
                    BudgetChangeOrder
                        label is "BudgetChangeRequest"
                        select is BudgetChangeOrdersProjectSelect
                        show up to 5 characters
                    BudgetChangeOrder.Description
                        display as text
                        show up to 30 characters
        two column distributed
            column1
                paragraph
                    Status
                        display as text
                    RequestedBy
                        display as text
                    ApprovedBy
                        display as text
        two column distributed
            column1
                Comment
            column2
                DocumentURL
        single column
            paragraph
                Attachment
        single column
            paragraph
                ErrorMessage
                    display as text
