TaxCodeCompositeForm is a CompositeForm
    is primary
    title is "TaxCode"
    context form is TaxCodeMaster
        background is theme color
    Rates is a Panel
        visible when (TaxCodeExistAndNotATable)
        list is TaxRate set.primary
    TaxTable is a Panel
        visible when (TaxCodeExistAndIsATable)
        list is TaxTable(TaxCode) set.primary
    ListOfTaxTables is a Panel
        title is "TaxTablesFoundWithThisTaxCode"
        visible when (TaxLogHeaderRelExist)
        form is TaxTableReportForm
