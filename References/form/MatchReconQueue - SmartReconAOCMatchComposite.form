SmartReconAOCMatchComposite is a CompositeForm
    title is "<SmartReconCompositeAOCTitle>"
    context form is SmartReconAOCMatchContext
        background is theme color
    implements DashBoardView
        show context form on top
        show 4 columns
    Actions
        action is SmartReconAOCMatch
            label is "Match"
            hide when invalid
        action is SelectAllInvoiceAOCs
            label is "SelectAllInvoiceAO\\Cs"
            mouse over text is "SelectAllInvoiceAddOnCharges"
            hide when invalid
        action is UnselectAllInvoiceAOCs
            label is "UnselectAllInvoiceAO\\Cs"
            mouse over text is "UnselectAllInvoiceAddOnCharges"
            hide when invalid
        action is SelectAllReceiptAOCs
            label is "SelectAllReceiptAO\\Cs"
            mouse over text is "SelectAllReceiptAddOnCharges"
            hide when invalid
        action is UnselectAllReceiptAOCs
            label is "UnselectAllReceiptAO\\Cs"
            mouse over text is "UnselectAllReceiptAddOnCharges"
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "PreviousRecord"
        action is next record
            label is "_"
            mouse over text is "NextRecord"
        action is refresh
            label is "_"
            mouse over text is "Refresh"
    SelectedInvoiceAOCs is a Panel
        title is "SelectedInvoiceCosts"
        visible when (ShowAOCPanels)
        double width
        list is PendingInvoiceAOCsRel.SmartReconInvoiceAddOnChargeSelectedList
    SelectedReceiptAOCs is a Panel
        title is "SelectedReceiptCosts"
        visible when (ShowAOCPanels)
        double width
        list is PendingReceiptAOCsRel.SmartReconReceiptLineAOCSelectedList
    UnselectedInvoiceAOCs is a Panel
        title is "UnselectedInvoiceCosts"
        visible when (ShowAOCPanels)
        double width
        list is NotSelectedInvoiceAOCsRel.SmartReconInvoiceAddOnChargeNotSelectedList
    UnselectedReceiptAOCs is a Panel
        title is "UnselectedReceiptCosts"
        visible when (ShowAOCPanels)
        double width
        list is NotSelectedReceiptAOCsRel.SmartReconReceiptLineAOCNotSelectedList
