ContractComposite is a CompositeForm
    is primary
    title is "ContractInterface"
    context form is ContractContext
        background is theme color
    implements IndependentFormPanels
    Actions
        action is CreateContractFromInterfacedRecords
            hide when invalid
            valid when (CreateNewContract)
        action is LinkToContract
            label is "LinkToContract"
            hide when invalid
        action is ProcessContractLineUpdates
            hide when invalid
            valid when (NonBlankContractLinesExist)
        action is save
            label is "_"
        action is refresh
            label is "_"
    Contract is a Panel
        title is "InterfaceContract"
        form is ContractImport
    PurchaseTypeInformation is a Panel
        visible when (HasAPurchaseType)
        form is PurchaseTypeHeader
    PurchaseTypeDefault is a Panel
        visible when (HasAPurchaseType)
        form is PurchaseTypeDefault
    Tiers is a Panel
        visible when (IsManufacturerContract)
        list is ContractImportTier set.ContractImportTiers
    NonGPOMembers is a Panel
        title is "Members"
        visible when (IsManufacturerContractNotForGPO)
        list is ContractImportTierMember set.ContractImportTierMemberNonGPOList
    Members is a Panel
        visible when (IsManufacturerContractForGPO)
        list is ContractImportTierMember set.ContractImportTierMemberList
    Participants is a Panel
        visible when (ServicePurchaseTypeForGPO)
        list is ContractImportTierMember set.ServiceContractParticipants
    DistributorMembers is a Panel
        visible when (IsDistributorContractForGPO)
        list is ContractImportTierMember set.ContractImportDistributorTierMemberList
    Distributors is a Panel
        visible when (IsManufacturerContractForGPONotMultiTierDistributor)
        list is ContractImportDistributor set.ContractImportDistributorList
    PricingIdentifiers is a Panel
        visible when (IsDistributorContractForGPO)
        list is ContractImportDistributor set.DistributorContractList
    Lines is a Panel
        visible when (not ServicePurchaseType)
        list is ContractLineImport set.ContractLineImports
    ServiceImportLines is a Panel
        title is "Lines"
        visible when (ServicePurchaseType)
        list is ContractLineImport set.ServiceContractLineImports
    Retainage is a Panel
        visible when (UseRetainageOnContract)
        form is Retainage
    Commitments is a Panel
        visible when (IsNotDistributorAndNotManufacturerAndPurchaseType)
        form is PurchaseCommitments
    HealthTrustInformation is a Panel
        visible when (GPOAndIsHealthTrust)
        form is HealthTrustForm
    HealthTrustInformation2021 is a Panel
        title is "HealthTrustInformation"
        visible when (GPOAndIsHealthTrust2021)
        form is HealthTrust2021Form
    GHXInformation is a Panel
        visible when (GPOAndIsGHX)
        form is GHXForm
    Terms is a Panel
        visible when (GPOAndIsHealthTrust)
        form is ContractGPOTerms
    Categories is a Panel
        visible when (GPOAndIsHealthTrust)
        list is ContractGPOCategoryRel.CategoriesForAContract
    Categories2021 is a Panel
        title is "Categories"
        visible when (GPOAndIsHealthTrust2021)
        list is ContractGPOCategory2021Rel.CategoriesForAContract2021
    S2Contracts is a Panel
        visible when (HasS2Contracts)
        list is S2ContractRel.S2Contracts
    CostComparisons is a Panel
        visible when (ToCostComparisonExists)
        independent form is CostComparisons
