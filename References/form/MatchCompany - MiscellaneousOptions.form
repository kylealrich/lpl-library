MiscellaneousOptions is a Form
    Layout
        header2 of "Miscellaneous"
        paragraph
            DiscountRecognition
                label is "DiscountRecognition"
        paragraph
            DiscountAccrualAccount
                label is "DiscountAccrualAccount"
                compact format
        two column distributed
            column1
                PostingAccountNoReceiptLine
                    label is "InvoiceDetailNoReceipt"
                CurrencyExchangeSetPoint
                    label is "CurrencyRateSetPoint"
                HoldUntilInspected
                    label is "HoldReceiverUntilInspected"
                AllowZeroCostInvoice
                    label is "AllowZeroCostInvoice"
                        left of field
                EnableCreditForRebill
                    label is "EnableCreditForRebill"
                        left of field
                MatchMemoDelayHardEdits
                    label is "MatchMemoDelayHardEdits"
                        left of field
                BypassBudgetEditIfNoDifferences
                    label is "BypassBudgetEditIfThereAreNoDifferencesBetweenThePurchaseOrderAndInvoice"
                        left of field
            column2
                PostingAccountQuantityExceedsReceipt
                    label is "InvoiceDetailExceedsReceipt"
                paragraph
                    AutomaticChargebackHoldCode
                    CBHoldCodeRel.Description
                        no label
                        display as text
                SummarizeDistribution
                SummarizeAllDistributions
                visible when (SummarizeAllDistributions)
                    text of "Run_Summarize_Distributions_For_Match_And_Service_InvoicesActionToSummarizeExistingInvoices"
                        italics
                PurchaseOrderInvoiceCancel
                PerformBudgetEditsForApproval
                    label is "PerformBudgetEditsForApproval"
                        left of field
                CreateReconMessageForLines
                    label is "DisplayLineLevelChargebackOptionDuringReconciliation"
        header2 of "EDIAndERSOptions"
        three column distributed
            EDIAutoRelease
                label is "EDIInvoiceAutomaticRelease"
            ERSLocationControl
                label is "ERSLocationControl"
            UpdateERSCost
                label is "ERSCostUpdate"
        blank line
        header2 of "VendorClaims"
        two column distributed
            column1
                ShipOrHold
                    label is "ShipOrHoldReturnedGoods"
                VendorClaimType
                    label is "VendorClaimType"
                ReplaceGoods
                    label is "ReplaceReturnedGoods"
                        left of field
            column2
                WriteOffAmount
                    label is "VendorClaimWriteOffAmount"
            paragraph
                VendorClaimHoldCode
                    label is "VendorClaimsChargebackHoldCode"
                VendorClaimHoldCodeRel.Description
                    no label
                    display as text
        visible when (OneSourceGeneric)
            header2 of "OneSource"
            paragraph
                text of "TaxCall:"
                    bold
                visible when (MatchOneCallPerInvoice)
                    text of "OneCallPerInvoice(UpTo200Lines)"
                    button of "ChangeToLineLevel"
                        action is TurnOffOneCall
                visible when (not MatchOneCallPerInvoice)
                    text of "OneCallPerInvoiceLine"
                    button of "ChangeToInvoiceLevel"
                        action is TurnOnOneCall
