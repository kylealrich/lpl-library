InvoiceLinesForUNSPSCItem is a CompositeForm
    title is "SpendAnalysisInvoiceLines"
    context form is ItemConsolidationContext
    NoContractInvoices is a Panel
        visible when (NoContract)
        list is NoContractInvoiceLineFactsForUNSPSCItemRel.UNSPSCInvoiceLineFacts
    OffContractInvoices is a Panel
        visible when (OffContract)
        list is OffContractInvoiceLineFactsForUNSPSCItemRel.OffContractUNSPSCInvoiceLineFacts
    OnContractInvoices is a Panel
        visible when (OnContract)
        list is OnContractInvoiceLineFactsForUNSPSCItemRel.UNSPSCInvoiceLineFacts
