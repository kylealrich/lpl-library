AssetBookImport is a Form
    is primary
    title is "AssetBookImport"
    Layout
        two column
            column1 of 30%
                RunGroup
            column2 of 60%
                paragraph
                    RecordInError
                        display as text
                    ErrorMessage
                        display as text
        two column
            column1 of 30%
                FinanceEnterpriseGroup
            column2 of 60%
                paragraph
                    AssetImport
                        label is "AssetImportGroup"
                    AssetImport.Description
                        no label
                        display as text
        paragraph
            Book
            Book.Description
                no label
                display as text
            Book.Currency
                no label
                display as text
        paragraph
            Active
        visible when (AllowBookUpdateWithRegulation)
            paragraph
                BookRegulation
                BookRegulation.Description
                    no label
                    display as text
        visible when (DisplayBookRegulation)
            paragraph
                BookRegulation
                    display as text
                BookRegulation.Description
                    no label
                    display as text
        three column
            column1 of 20%
                Method
                InServiceDate
                    label is "InServiceDate"
                YearPlacedInService
                    label is "MethodTableYear"
                FirstYearExpenseAmount
                    label is "FirstYearExpense"
            column2 of 20%
                Life
                LifeRemaining
                Basis
                BusinessPercent
            column3 of 60%
                Convention
                SpreadOverRemainingLife
                LifeInYears
                    label is "LifeYears"
                PersonalUseCode
                    label is "VehicleLimitCode"
                SalvageValue
        blank line
        header3 of "Depreciation"
        three column
            column1 of 20%
                YearToDateDepreciation
                    label is "Year_toDate"
                YearToDateDepreciationAllowed
                    label is "Y\\T\\DAllowed"
            column2 of 20%
                LifeToDateDepreciation
                    label is "Life_toDate"
                LifeToDateDepreciationAllowed
                    label is "L\\T\\DAllowed"
            column3 of 60%
                YearToDateInactiveMonths
                    label is "InactiveMonthsY\\T\\D"
                LifeToDateInactiveMonths
                    label is "InactiveMonthsL\\T\\D"
        blank line
        three column
            column1
                header4 of "TaxCredit"
                AssetTaxCreditTable
                    label is "Table"
                    select is TaxCreditTables
                TaxCreditPropertyAmount
                    label is "Property"
                TaxCredit
                    label is "Amount"
            column2
                header4 of "Insurance"
                InsuranceCalculationMethod
                    label is "Method"
                ActualInsuranceValue
                    label is "ActualValue"
                CalculatedInsuranceValue
                    label is "ComputedValue"
            column3
                header4 of "Replacement"
                ReplacementCalculationMethod
                    label is "Method"
                CalculatedReplacementValue
                    label is "ComputedValue"
        blank line
        header3 of "TaxReportingFields"
        three column
            visible when (BookRegulationsExist)
                column1
                    BookRegulation
                        display as text
            column2
                ClassificationOfProperty
            column3
                DepreciableIntangible
        three column
            column1
                QualifiedImprovement
                PropertyType
                CostRecoverySystem
                BonusAmount
                Section179Expense
            column2
                Listed
                IndianReservation
                Used
                MineralReserves
                Section179
            column3
                ForeignProperty
                Farming
                LowIncomeHousing
                TransitionalRuleFlag
                NonConforming
