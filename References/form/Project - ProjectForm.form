ProjectForm is a CompositeForm
    is primary
    title is "<FinanceEnterpriseGroup.ProjectLabel>_<DisplayProject>"
    context form is ProjectContextForm
        background is theme color
    restrict action Create
    Main is a Panel
        title is "Main"
        form is MainProjectForm
    BillingOverrides is a Panel
        title is "BillingOverrides"
        visible when (BillingOverridesValid)
        form is BillingOverridesPanel
    LaborDistribution is a Panel
        title is "LaborDistribution"
        visible when (ProjectType.Posting)
        form is LaborDistributionPanel
    Assignments is a Panel
        title is "Assignments"
        visible when (PostingInEnterpriseStructure)
        list is ProjectAssignment(Project) set.ProjectAssignments
            helper list is ActiveEmployeeRel.ActiveEmployeesForProjects
                action is AddToProject
    Assignments2 is a Panel
        title is "Assignments"
        visible when (PostingNotInEnterpriseStructure)
        list is ProjectAssignment(Project) set.ProjectAssignmentsNoUpdate
    ProjectPersonnel is a Panel
        title is "Personnel"
        visible when (ProjectType.Posting)
        list is ProjectPersonnelRel.ProjectPersonnelList
    PeriodOfPerformance is a Panel
        title is "PeriodsOfPerformance"
        visible when (ProjectType.Posting)
        list is ProjectPeriodOfPerformance set.ProjectPeriodOfPerformanceList
    IndirectBurdens is a Panel
        title is "IndirectBurdens"
        visible when (ProjectType.Posting)
        list is ProjectIndirectBurden set.ProjectIndirectBurdens
    IndirectBurdenTransactions is a Panel
        title is "BurdenTransactions"
        visible when (ProjectType.Posting)
        list is ProjectIndirectBurdenTransaction set.ProjectIndirectBurdenTransactionList
    AccountingEntity is a Panel
        title is "<FinanceEnterpriseGroup.AccountingEntityLabel>List"
        visible when (HasAccountingEntityValidPairs)
        list is CodeBlockRelationDetailAccountingEntityRel.ProjectCodeBlockRelationDetailAccountingEntityList
    AccountingUnit is a Panel
        title is "<FinanceEnterpriseGroup.AccountingUnitLabel>List"
        visible when (HasAccountingUnitValidPairs)
        list is CodeBlockRelationDetailAccountingUnitRel.ProjectCodeBlockRelationDetailAccountingUnitList
    Accounts is a Panel
        title is "<FinanceEnterpriseGroup.AccountLabel>List"
        visible when (HasAccountVaidPairs)
        list is CodeBlockRelationDetailRel.ProjectCodeBlockRelationDetailList
    FinanceDimension1 is a Panel
        title is "<FinanceEnterpriseGroup.FinanceDimension1Label>List"
        visible when (HasDimension1ValidPairs)
        list is CodeBlockRelationDetailDimension1Rel.ProjectCodeBlockRelationDetailFD1List
    FinanceDimension3 is a Panel
        title is "<FinanceEnterpriseGroup.FinanceDimension3Label>List"
        visible when (HasDimension3ValidPairs)
        list is CodeBlockRelationDetailDimension3Rel.ProjectCodeBlockRelationDetailFD3List
    FinanceDimension4 is a Panel
        title is "<FinanceEnterpriseGroup.FinanceDimension4Label>List"
        visible when (HasDimension4ValidPairs)
        list is CodeBlockRelationDetailDimension4Rel.ProjectCodeBlockRelationDetailFD4List
    ProjectAssetInterfaceControl is a Panel
        title is "ProjectAssetInterfaceControl"
        visible when (HasAssetInterfaceOptions)
        list is AssetInterfaceOptionsRel.ProjectAssetInterfaceOptionsList
    Comments is a Panel
        list is ProjectComment set.ProjectCommentForAProjectList
    UserFields is a Panel
        form is UserFieldsForm
