LineResponsesSelect is a CompositeForm
    title is "LineResponsesToSelectForEvent#<SourcingEvent.EventVersionString>"
    context form is LineResponsesContext
        background is theme color
    Actions
        action is Submitted.AwardAllNewContract
            action icon is award
            label is "AwardToNewContract"
            hide when invalid
            valid when (DisplayAwardToActionsForContracts)
        action is Submitted.AwardAllAppendToContract
            action icon is award
            label is "AwardToExistingContract"
            hide when invalid
            valid when (DisplayAwardToActionsForContracts)
        action is Submitted.AwardAllNewPO
            action icon is award
            label is "AwardToNewPurchaseOrder"
            hide when invalid
            valid when (DisplayAwardToActionsForPurchaseOrders)
        action is Submitted.AwardAllAppendToPO
            action icon is award
            label is "AwardToExistingPurchaseOrder"
            hide when invalid
            valid when (DisplayAwardToActionsForPurchaseOrders)
        action is Submitted.FinalAward
            action icon is award
            hide when invalid
            valid when (FinalAwardAvailableForSelected)
        action is Submitted.CreateOutput
            action icon is export
            hide when invalid
        navigation is OutputCreatedForResponse
            action icon is award
            hide when invalid
    LineResponsesSelect is a MultiListPanel
        visible when (SourcingEvent.HasContractOutput)
        list is SourcingEventLineResponseRel.LineResponsesSelected
        list is SourcingEventLineResponseRel.LineResponsesToSelect
    LineResponsesSelectForPO is a MultiListPanel
        visible when (SourcingEvent.HasPurchaseOrderOutput)
        list is SourcingEventLineResponseRel.LineResponsesSelectedForPO
        list is SourcingEventLineResponseRel.LineResponsesToSelect
