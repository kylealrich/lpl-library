DirectForecast is a CompositeForm
    is primary
    title is "DirectForecast"
    context form is DirectForecastContextForm
        background is theme color
    DirectForecastPeriods is a Panel
        list is DirectForecastPeriod set.DirectForecastPeriodList
    CashInflowByCompanyAccount is a Panel
        title is "CashInflowDetails"
        visible when (InflowCompanyAccountEntered)
        list is ReceiptDetailRel.ForecastedAmountsList
    CashInflowByCompanyAccountGroup is a Panel
        title is "CashInflowDetails"
        visible when (InflowCompanyAccountGroupEntered)
        list is ReceiptAccountGroupDetailRel.ForecastedAmountsList
    CashInflowByCompany is a Panel
        title is "CashInflowDetails"
        visible when (InflowCompanyOnlyEntered)
        list is CompanyReceiptDetailRel.ForecastedAmountsList
    CashInflowByAccount is a Panel
        title is "CashInflowDetails"
        visible when (InflowAccountOnlyEntered)
        list is CashManagementAccountReceiptDetailRel.ForecastedAmountsList
    CashInflowByAccountGroup is a Panel
        title is "CashInflowDetails"
        visible when (InflowAccountGroupOnlyEntered)
        list is CashAccountGroupReceiptDetailRel.ForecastedAmountsList
    CashOutflowByCompanyAccount is a Panel
        title is "CashOutflowDetails"
        visible when (CashOutflowCompanyAccountForDueDate)
        list is CompanyAccountCashOutFlowsRel.ForecastedAmountsList
    CashOutflowByCompanyAccountByDiscountDate is a Panel
        title is "CashOutflowDetails"
        visible when (CashOutflowCompanyAccountForDiscountDate)
        list is CompanyAccountCashOutFlowsDiscountDateRel.ForecastedAmountsList
    CashOutflowByCompanyAccountGroup is a Panel
        title is "CashOutflowDetails"
        visible when (CashOutflowCompanyAccountGroupForDueDate)
        list is CompanyAccountGroupCashOutFlowsRel.ForecastedAmountsList
    CashOutflowByCompanyAccountGroupByDiscountDate is a Panel
        title is "CashOutflowDetails"
        visible when (CashOutflowCompanyAccountGroupForDiscountDate)
        list is CompanyAccountGroupCashOutFlowsDiscountDateRel.ForecastedAmountsList
    CashOutflowByCompany is a Panel
        title is "CashOutflowDetails"
        visible when (CashOutflowCompanyOnlyForDueDate)
        list is CompanyCashOutFlowsRel.ForecastedAmountsList
    CashOutflowByCompanyByDiscountDate is a Panel
        title is "CashOutflowDetails"
        visible when (CashOutflowCompanyOnlyForDiscountDate)
        list is CompanyCashOutFlowsDiscountDateRel.ForecastedAmountsList
    CashOutflowByAccount is a Panel
        title is "CashOutflowDetails"
        visible when (CashOutflowAccountOnlyEntered)
        list is CashManagementAccountCashOutFlowsRel.ForecastedAmountsList
    CashOutflowByAccountGroup is a Panel
        title is "CashOutflowDetails"
        visible when (CashOutflowAccountGroupOnlyEntered)
        list is CashAccountGroupCashOutFlowsRel.ForecastedAmountsList
