FranchiseContractMain is a Form
    Layout
        three column distributed
            column1
                paragraph
                    ContractDate
                    Active
                TerminateDate
                    label is "TerminationDate"
                paragraph
                    TaxableFlag
                        label is "Taxable"
                    visible when (TaxableFlag.Taxable)
                        CalculateTaxOnNetSales
                            label is "CalculateTaxOnNetSales"
                TaxCode
                FranchiseBudget
                    label is "Budget"
            column2
                FranchiseCalendar
                    label is "Calendar"
                FranchiseGroupCode
                    label is "GroupCode"
                EstimatesFlag
                    label is "Estimates"
                SalesFlag
                    label is "RequireAggregateSales"
                SalesByDate
                    label is "AggregateSalesByDate"
            column3
                BillingInvoiceType
                    label is "InvoiceType"
                BillingCreditMemoInvoiceType
                    label is "CreditMemoType"
                VarianceLimit
                CalculateInvoiceAmountOnGrossSales
                    label is "CalculateInvoiceAmountOnGrossSales"
                        left of field
                AdvanceCycleDateForNoActivity
