                    ProcessLevel
                    CashCode
                    Currency
                    ExchangeRate
                        label is "Rate"
                column3
                    CashLedgerUserField1
                    CashLedgerUserField2
                    CashLedgerUserField3
                    CashLedgerUserField4
                    BankCharges
                    BankChargesAccount
                        compact format
