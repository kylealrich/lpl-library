SupplierContactComposite is a WizardForm
    use for action NewContact
    title is "NewContact"
    summary form is ContactSummary
    show steps
    ContactInformation is a Panel
        mouse over text is "<SupplierGroup.AdditionalContactText>"
        panel header is "<SupplierGroup.AdditionalContactText>"
        form is SupplierSourceId
    BankInformation is a Panel
        valid when (CanDisplayBankInformation)
        mouse over text is "<SupplierGroup.BankInformationText>"
        panel header is "<SupplierGroup.BankInformationText>"
        form is BankInformationRegistrationForm
    CommodityCodes is a MultiListPanel
        valid when (SupplierGroup.CommoditySelectSupplierContactWithTree)
        mouse over text is "<SupplierGroup.CommodityCodesText>"
        list is SupplierCommodityCode set.ContactSelectedCommodityCodes
        list is CommodityCodes.AvailableCommodityCodesForASupplierContact
    CommodityCodesNoTree is a MultiListPanel
        title is "CommodityCodes"
        valid when (SupplierGroup.CommoditySelectSupplierContactNoTree)
        mouse over text is "<SupplierGroup.CommodityCodesText>"
        list is SupplierCommodityCode set.ContactSelectedCommodityCodes
        list is CommodityCodes.AvailableCommodityCodesForASupplierContactNoTree
    Certifications is a Panel
        valid when (SupplierGroup.AllowCertificationSelect)
        mouse over text is "<SupplierGroup.CertificationText>"
        panel header is "<SupplierGroup.CertificationText>"
        list is SupplierCertificationRel.SupplierSideContactCertifications
    ProxyNotifications is a Panel
        valid when (SupplierGroup.AllowProxySelect)
        mouse over text is "<SupplierGroup.ProxyNotificationsText>"
        panel header is "<SupplierGroup.ProxyNotificationsText>"
        list is SupplierContactProxy set.SupplierContactProxyList
