RegistrationSummaryForCompositeExternalEntity is a Form
    title is "Status"
    Layout
        visible when (not SupplierExists)
            single column
                text of "AfterEnteringRequiredInformationClickSaveInUpperRight"
                    bold
                    color of green
                text of "ThenViewYourRegistrationStatusHere"
                    bold
                    color of green
        visible when (SupplierExists)
            paragraph
                text of "<PrimaryContact.SummaryText>"
                    bold
            visible when (NeedCommoditiesOrQuestionsAnswered)
                paragraph
                    visible when (PrimaryContact.IncompleteCommodities)
                        text of "<PrimaryContact.NotCompleteCommodities>"
                            color of red
                    visible when (SomeRequiredQuestionsOrAttachmentsNotAnswered)
                        text of "<PrimaryContact.NotCompleteQMessage>"
                            color of red
                        button of "Respond_toQuestions"
                            link is SupplierAllQuestions
            visible when (not SomeRequiredQuestionsOrAttachmentsNotAnswered)
                visible when (HasQuestions)
                    paragraph
                        text of "QuestionsExist;UseThisButtonToAnswerOrChange"
                            bold
                        button of "Respond_toQuestions"
                            link is SupplierAllQuestions
