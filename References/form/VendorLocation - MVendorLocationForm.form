MVendorLocationForm is a CompositeForm
    title is "<DerivedFormTitle>"
    context form is MVendorLocationContext
    restrict action Create
    restrict action CopyVendorLocation
    restrict action Delete
    Main is a Panel
        form is MMainForm
    VendorBank is a Panel
        form is VendorBank
    Options is a Panel
        form is Options
    Balances is a Panel
        list is VendorLocationBalanceRel.MVendorLocationBalanceList
    Contact is a MultiListPanel
        list is VendorContactRel.MVendorContactsList
        list is CurrentAddressRel.MVendorAddressesList
    Certifications is a MultiListPanel
        list is VendorCertificationRel.MobileCertificationCodeAssignmentsVendorPanel
        list is VendorDiversityRel.MobileDiversityCodeCertificationsVendorPanel
    CompanyDefaults is a Panel
        list is VendorCompanyDefaultRel.MLocationCompanyCodes
    Comments is a Panel
        list is VendorLocationCommentsRel.MobileVendorComments
    Attachments is a Panel
        list is VendorLocationAttachmentsRel.MobileAttachments
    MultipleTaxAreas is a Panel
        visible when (VertexResultsExist)
        list is VertexTaxAreaIdResultsRel.TaxAreaResultList
    VertexError is a Panel
        visible when (VertexErrorExist)
        list is VertexWebserviceErrorRel.MobileVertexTaxAreaIdErrorList
