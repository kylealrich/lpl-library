ChangeSupplierAndVendor is an ActionForm
    action is ChangeSupplierAndVendor
    title is "ChangeSupplierAndVendorOnInterfaceAndContract"
    Layout
        display as text
            two column
                ParmNewSupplier
                    label is "NewSupplier"
                ParmNewSupplier.SupplierName
                    no label
            two column
                ParmNewSupplier.Vendor
                    label is "NewVendor"
                ParmNewSupplier.Vendor.VendorName
                    no label
            two column
                Supplier
                    label is "InterfaceSupplier"
                Supplier.SupplierName
                    no label
            two column
                Vendor
                    label is "InterfaceVendor"
                Vendor.VendorName
                    no label
            visible when (NewGPOContractExists)
                two column
                    ManufacturerContractRel.Supplier
                        label is "ContractSupplier"
                    ManufacturerContractRel.Supplier.SupplierName
                        no label
                two column
                    ManufacturerContractRel.Vendor
                        label is "ContractVendor"
                    ManufacturerContractRel.Vendor.VendorName
                        no label
        two column
            ParmNewSupplierContact
            ParmNewSupplierContact.FullName
                no label
                display as text
        two column
            ParmNewPurchaseFrom
            ParmNewPurchaseFrom.VendorLocationRel.VendorName
                no label
                display as text
        single column
            InactivateOldVendorItems
            visible when (Supplier.Active)
                InactivateOldSupplier
        visible when (NewGPOContractExists)
            visible when (OpenReqLinesExist)
                single column
                    UpdateVendorOnRequisitionLines
        single column
            ChangeReplenishmentVendor
