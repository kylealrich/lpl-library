MobilePaymentDistributionContextForm is a Form
    title is "PaymentDistributionContextForm"
    Layout
        two column distributed
            CashCode
                display only
            CashCode.Description
                no label
                display as text
        single column
            CashCode.Currency
                label is "Currency"
                display as text
            paragraph
                CashLedgerGLDistribution.BankTransactionCode
                    label is "TransactionCode"
                    display only
            paragraph
                CashLedgerPaymentRel.Reference
                    label is "TransactionNumber"
                    display only
        two column distributed
            CashLedgerPaymentRel.CompanyBaseData.OverrideCompany
                label is "Company"
                display only
            CashLedgerPaymentRel.CompanyBaseData.OverrideCompany.Name
                no label
                display as text
        single column
            CashLedgerPaymentRel.BankCheckAmount
                label is "Amount"
                display only
        two column distributed
            CashLedgerPaymentRel.DistributionTotal
                label is "AppliedAmount"
                display only
            CashLedgerPaymentRel.BalanceAmount
                label is "Balance"
                display only
