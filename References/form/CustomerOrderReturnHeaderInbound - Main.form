Main is a Form
    Layout
        two column distributed
            visible when (not RecordExists)
                paragraph
                    Company
                    Company.Name
                        no label
                        display as text
                CustomerOrderReturnHeaderInbound
                    label is "AuthorizationNumber"
        two column distributed
            paragraph
                InventoryLocation
                    label is "Location"
                InventoryLocation.Name
                    no label
                    display as text
            Status
        two column distributed
            RetRefNbr
                label is "ReferenceNumber"
            RtnDate
                label is "ReturnDate"
        two column distributed
            paragraph
                Customer
                    select is CustomerContextCompanySelectList
                Customer.Name
                    no label
                    display as text
            paragraph
                CustomerShipTo
                    label is "ShipTo"
                CustomerShipTo.Name
                    no label
                    display as text
        two column distributed
            OrderNbr
                label is "OrderNumber"
            RtnDate
                label is "UpdateDateAndTime"
