CombinedReportableIncomeResultParmForm is a Form
    title is "ReportableIncomeResults"
    Layout
        four column distributed
            column1
                paragraph
                    VendorGroup
                        display as required
                    VendorGroup.Description
                        no label
                        display as text
            column2
                paragraph
                    InvoiceCurrency
                        display as required
                    InvoiceCurrency.Description
                        no label
                        display as text
            column3
                paragraph
                    Vendor
                    Vendor.VendorName
                        no label
                        display as text
            column4
                PaymentNumber
        header2 of "Parameters"
            collapsible
                initially expanded
                use show more or less affordance
            four column distributed
                column1
                    paragraph
                        Company
                        Company.Name
                            no label
                            display as text
                    paragraph
                        ProcessLevel
                        ProcessLevel.Name
                            no label
                            display as text
                    paragraph
                        ReportableAmountRange.FromAmount
                            label is "FromReportableAmount"
                        ReportableAmountRange.ToAmount
                            label is "ToReportableAmount"
                column2
                    paragraph
                        IncomeCode
                        IncomeCode.Description
                            no label
                            display as text
                    paragraph
                        ReportableIncomeGroup
                            label is "IncomeGroup"
                        ReportableIncomeGroup.Name
                            no label
                            display as text
                    paragraph
                        PaymentDateRange.Begin
                            label is "FromPaymentDate"
                        PaymentDateRange.End
                            label is "ToPaymentDate"
                column3
                    Invoice
                    TaxID
                    FormType
                        show up to 16 characters
                column4
                    PaymentAmount
                    WithholdingAmount
                    IncludeAllInvoices
                        display as switch
                        initial value is false
