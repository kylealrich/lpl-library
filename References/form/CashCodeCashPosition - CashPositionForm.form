CashPositionForm is a Form
    Layout
        display as text
            four column distributed
                column1
                    paragraph
                        UpdateTimeStamp
                    paragraph
                        DebitPeriodAmount.TransactionAmount
                            label is "DebitPeriodAmount"
                        LastPeriodBalance.TransactionAmount
                            label is "LastPeriodBalance"
                    ClosedPeriodBalance
                column2
                    Company
                    paragraph
                        OpenBalance.TransactionAmount
                            label is "OpenBalance"
                        NetPeriodAmount.TransactionAmount
                            label is "CurrentReconciledAmount"
                        OpenRecordCount
                column3
                    paragraph
                        CashCode.CashCodeAndDescription
                            label is "CashCode"
                        BankAccountCurrency
                            no label
                    paragraph
                        PeriodPostedBalance.TransactionAmount
                            label is "PeriodPostedBalance"
                        EndingBalance.TransactionAmount
                            label is "EndingBalance"
                column4
                    paragraph
                        CashManagementAccount
                        CashCodeCashPosition
                            label is "CashPositionDate"
                    CreditPeriodAmount.TransactionAmount
                        label is "CreditPeriodAmount"
