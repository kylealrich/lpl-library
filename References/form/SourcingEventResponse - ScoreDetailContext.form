ScoreDetailContext is a Form
    title is "EventScoreDetails"
    Layout
        two column
            SourcingEvent.EventVersionString
                label is "Event#"
                display as text
            SourcingEvent.Name
                label is "EventName"
                display as text
        two column
            NotifiedSupplier.Supplier
                label is "Supplier"
                display as text
            NotifiedSupplier.Supplier.SupplierName
                display as text
        two column
            NotifiedSupplier.SupplierSourceId
                label is "SupplierContact"
                display as text
            NotifiedSupplier.SupplierSourceId.MainContact.FirstAndLastName
                label is "SupplierContactName"
                display as text
