UserMenuChangesForm is a Form
    disable field options
    Layout
        visible when (TransactionRecordExists)
            paragraph
                UserMenuRel.UserMenuName
                    header2
                    display as text
                visible when (UserMenuRel.Global)
                    text of "(Configuration)"
                        header3
                visible when (UserMenuRel.HasFolder)
                    text of "(Folder)"
                        header3
                visible when (UserMenuRel.IsActor)
                    text of "(Personalization)"
                        header3
                button of "<BusinessView>Details"
                    link is UserMenuNav
                    display as link
        visible when (not UserMenuExists)
            paragraph
                text of "<BusinessView>RecordDoesNotExist"
