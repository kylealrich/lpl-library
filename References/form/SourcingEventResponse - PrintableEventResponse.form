PrintableEventResponse is a SummaryForm
    title is "Event#<SourcingEvent.EventVersionString>"
    First Page Header
        Layout
            visible when (SourcingEvent.LogoEntered)
                visible when (SourcingEvent.UseCompanyLogo)
                    single column
                        Company.ReportLogo.File
                            no label
                            display as image
                visible when (SourcingEvent.UseGroupLogo)
                    single column
                        Company.SourcingGroup.ReportLogo.File
                            no label
                            display as image
            header2 of "_"
    Page Header
        Layout
            header2 of "_"
                underline color of black
            single column
                text of "Event#<SourcingEvent.EventVersionString>:_<SourcingEvent.Name>"
                    header2
                    left align
            header2 of "_"
            blank line
    Page Footer
        Layout
            two column
                text of "<current timestamp>"
                    left align
                text of "Page<page number>"
                    right align
    Layout
        form is ResponsePrint
        blank line
        visible when (SEQuestionResponseExists)
            header2 of "QuestionResponses"
                show border color of black
                shade
                center
            header2 of "_"
            list is SourcingEventQuestionResponse set.QuestionsResponsePrint
            blank line
        visible when (AttachmentsExist)
            header2 of "ResponseAttachments"
                show border color of black
                shade
                center
            header2 of "_"
            list is SourcingEventResponseAttachment set.AttachmentsPrint
            blank line
        visible when (HeaderResponseAOCsExist)
            header2 of "_"
            list is HeaderAddOnChargeResponseRel.SourcingEventResponseAOCPrint
            blank line
        header2 of "LineResponses"
            show border color of black
            shade
            center
        header2 of "_"
        form is SERLines
