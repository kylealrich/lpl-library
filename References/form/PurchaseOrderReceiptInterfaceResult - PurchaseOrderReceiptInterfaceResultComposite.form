PurchaseOrderReceiptInterfaceResultComposite is a CompositeForm
    is primary
    title is "PurchaseOrderReceiptInterfaceResult"
    context form is PurchaseOrderReceiptInterfaceResultContext
        background is theme color
    Actions
        action is ResubmitInterface
            hide when invalid
        action is Delete
            label is "_"
        action is refresh
            label is "_"
    ProcessingCounts is a Panel
        form is ProcessingCountsForm
    InterfaceTotals is a Panel
        visible when (ReceiptsCreated)
        list is PurchaseOrderReceiptRel.InterfaceTotals
    InterfacedReceipts is a Panel
        visible when (ReceiptsCreated)
        list is PurchaseOrderReceiptRel.InterfaceListing
    PendingReceipts is a Panel
        visible when (HasPendingReceipts)
        list is PurchaseOrderReceiptImportRel.PurchaseOrderReceiptImportList
    InterfacedAdjustments is a Panel
        visible when (AdjustmentsCreated)
        list is POReceiptAdjustmentAndInspectionRel.PurchaseOrderReceiptAdjustmentList
    InterfacedAdditions is a Panel
        visible when (AdditionsCreated)
        list is PurchaseOrderReceiptLineRel.InterfaceListing
    PendingAdjustments is a Panel
        visible when (HasPendingAdjustments)
        list is POReceiptAdjustmentAndInspectionImportRel.POReceiptAdjustmentAndInspectionImportList
    Parameters is a Panel
        form is ParameterForm
