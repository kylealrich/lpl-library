TaxConfigVersion is a Form
    title is "TaxVersion"
    Layout
        paragraph
            TaxConfiguration
            TaxConfigVersion.ThirdParty
            TaxConfigVersion.Version
            TaxConfigVersion.Release
        paragraph
            UserName
            visible when (OneSourceUserNameNotEntered)
                text of "One\\Source_\\User\\NameRequired"
                    bold
                    color of red
        paragraph
            TransientPassword
                label is "Password"
            visible when (OneSourcePassword entered)
                text of "*"
                    bold
                    color of red
            DerivedEncryptedPasswordEntered
                label is "EncryptedPasswordEntered"
                display only
        paragraph
        single column
            visible when (OneSourcePasswordNotEntered)
                text of "Warning...One\\Source_\\PasswordRequired"
                    bold
                    color of red
            NameSpace
            ReleaseNotes
            ReleaseComments
            visible when (TaxConfiguration.ThirdParty.Sabrix)
