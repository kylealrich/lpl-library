ProcessPurchaseRequest is an ActionForm
    action is ProcessPurchaseRequest
    Layout
        single column
            header2 of "Main"
            paragraph
                PrmPurchasingCompany
                PrmPurchasingCompany.Name
                    no label
                    display as text
            paragraph
                PrmPurchasingCompany.ProcurementGroup
                    label is "ProcurementGroup"
                    display as text
                PrmProcessByProcurementGroup
            paragraph
                PrmDefaultBuyer
                PrmDefaultBuyer.Employee.PresentationNameSnapshot
                    no label
                    display as text
            PrmDefaultDeliveryDays
            PrmIncludeAllCostOptions
            PrmMaximumPOLines
            PrmCombineOnPOLine
            PrmOneShipToPerPO
            PrmReleasePurchaseOrder
        blank line
        header2 of "Filters"
        single column
            PrmSystemCode
            PrmVendorOption
            paragraph
                PrmVendorAndPurchaseFrom.VendorAndPurchaseFrom[1].Vendor
                    label is "Vendor\\,PurchaseFrom"
                PrmVendorAndPurchaseFrom.VendorAndPurchaseFrom[1].PurchaseFromLocation
                    no label
            paragraph
                PrmVendorAndPurchaseFrom.VendorAndPurchaseFrom[2].Vendor
                    no label
                PrmVendorAndPurchaseFrom.VendorAndPurchaseFrom[2].PurchaseFromLocation
                    no label
            paragraph
                PrmVendorAndPurchaseFrom.VendorAndPurchaseFrom[3].Vendor
                    no label
                PrmVendorAndPurchaseFrom.VendorAndPurchaseFrom[3].PurchaseFromLocation
                    no label
            paragraph
                PrmVendorAndPurchaseFrom.VendorAndPurchaseFrom[4].Vendor
                    no label
                PrmVendorAndPurchaseFrom.VendorAndPurchaseFrom[4].PurchaseFromLocation
                    no label
            blank line
            paragraph
                PrmFilterBuyer
                PrmFilterBuyer.Employee.PresentationNameSnapshot
                    no label
                    display as text
            paragraph
                PrmBuyerOnPurchasingClasses
                PrmBuyerOnVendor
                PrmBuyerOnLocation
                PrmBuyerOnRequestingLocation
            paragraph
                PrmFilterLocation
                PrmFilterLocation.NameForPurchaseOrder
                    no label
                    display as text
                text of "Or"
                PrmFilterReportGroup
                PrmFilterReportGroup.Name
                    no label
                    display as text
            paragraph
                PrmFilterRequestingLocation
                PrmFilterRequestingLocation.Name
                    no label
                    display as text
            PrmPOCodeOption
            paragraph
                PrmFilterPOCode.POCode[1]
                    label is "P\\OCode"
                PrmFilterPOCode.POCode[2]
                    no label
                PrmFilterPOCode.POCode[3]
                    no label
                PrmFilterPOCode.POCode[4]
                    no label
            PrmBillCodeOption
            paragraph
                PrmFilterBillCode.BillCode[1]
                    label is "BillCode"
                PrmFilterBillCode.BillCode[2]
                    no label
            paragraph
                PrmFilterBillCode.BillCode[3]
                    no label
                PrmFilterBillCode.BillCode[4]
                    no label
            PrmFilterDeliveryDays
            paragraph
                PrmFilterFromPriority
                PrmFilterToPriority
            paragraph
                PrmFilterMajorPurchasingClass
                    show up to 10 characters
                PrmFilterMinorPurchasingClass
                    show up to 10 characters
            PrmFilterItem
            PrmFilterItemType
            PrmFilterManufacturer
                compact format
            PrmFilterCostCode
            paragraph
                PrmFilterRequestingCompany
                PrmFilterRequisition
                text of "Or"
                PrmFilterProcurementTemplate
            blank line
        header2 of "AccountFilters"
        paragraph
            PrmFilterAccountingUnit
            PrmFilterAccountingUnit.Description
                no label
                display as text
            PrmFilterActivity
            PrmFilterActivity.Description
                no label
                display as text
