PublishedSavedSearchesActionsForm is a Form
    disable field options
    Layout
        two column grid
            visible when (not Status.Active)
                button of "Activate"
                    action is Activate
                    display as link
                text of "TurnsOnAPreviouslyDeactivatedSavedSearchLibraryItem."
            visible when (not Status.Inactive)
                button of "Deactivate"
                    action is Deactivate
                    display as link
                text of "InactiveLibraryItemsWillNotAppearInThe_Library,ButWillWorkNormallyForUsersThatAreAlreadySubscribed."
            visible when (CanModify)
                button of "Rename"
                    action is Rename
                    display as link
                text of "RenameThisSavedSearchLibraryItemAndUpdateTheDescription."
            visible when (CanModify)
                button of "Delete"
                    action is Delete
                    display as link
                text of "DeletesThisLibraryItemAndAllSubscriptionsToIt.CannotBeRetrieved."
            visible when (not HasSubscription)
                button of "Subscribe"
                    action is SubscribeToMe
                    display as link
                text of "SubscribeToThisLibraryItem"
            visible when (HasSubscription)
                button of "Unsubscribe"
                    action is UnsubscribeFromLibraryItem
                    display as link
                text of "UnsubscribeFromThisLibraryItem."
