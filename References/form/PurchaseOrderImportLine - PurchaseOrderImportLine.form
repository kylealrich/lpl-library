PurchaseOrderImportLine is a CompositeForm
    is primary
    title is "ImportLineDetail"
    context form is PurchaseOrderImportLineContext
        background is theme color
    LineDetail is a Panel
        visible when (AddOrUpdateLine)
        form is LineDetail
    Detail is a Panel
        visible when (not AddOrUpdateLine)
        form is TextLineDetail
    Retainage is a Panel
        visible when (AddOrUpdateLine)
        form is LineRetainage
    Distributions is a Panel
        visible when (not CancelLine)
        list is DistributionsRel.primary
    AddOnCharges is a Panel
        visible when (not CancelLine)
        list is AddOnChargesRel.ItemAOCs
    Comments is a Panel
        visible when (not CancelLine)
        list is CommentsRel.LineComments
