POStatusReport is an ActionForm
    action is POStatusReport
    Layout
        header2 of "Main"
        paragraph
            CompanyGroup
                show up to 10 characters
            CompanyGroup.Description
                no label
                display as text
        single column
            text of "\\or"
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            Location
                show up to 10 characters
            Location.Name
                no label
                display as text
        single column
            text of "\\or"
        paragraph
            ReqLoc
                label is "RequestingLocation"
                show up to 10 characters
            ReqLoc.Name
                no label
                display as text
        header2 of "_"
        text of "_"
        paragraph
            BuyerGrp
                label is "BuyerGroup"
            BuyerGrp.Description
                no label
                display as text
        single column
            text of "\\or"
        paragraph
            BuyerCode
                label is "Buyer"
                show up to 10 characters
            BuyerCode.Employee.PresentationNameSnapshot
                no label
                display as text
        paragraph
            VendorGroup
        header2 of "Options"
        paragraph
            PoNumber.PO220PoNumberGroup[1].PoNumberGrp
                label is "P\\ONumber"
            PoNumber.PO220PoNumberGroup[2].PoNumberGrp
                no label
        paragraph
            PoNumber.PO220PoNumberGroup[3].PoNumberGrp
                no label
            PoNumber.PO220PoNumberGroup[4].PoNumberGrp
                no label
        paragraph
            PoNumber.PO220PoNumberGroup[5].PoNumberGrp
                no label
            PoNumber.PO220PoNumberGroup[6].PoNumberGrp
                no label
        paragraph
            PurchFrLoc.VendorName
                no label
                display as text
        paragraph
            FrEarlyDlDate
                label is "FromEarlyExpectedDeliveryDate"
            ToEarlyDlDate
                label is "ToEarlyExpectedDeliveryDate"
        paragraph
            SelStatus
                label is "Status"
        paragraph
            Vendor
                show up to 15 characters
            Vendor.VendorName
                no label
                display as text
        paragraph
            PurchFrLoc
                label is "PurchaseFrom"
        paragraph
            ReportSeq
                label is "SequenceOption"
            PrintItemDtl
                label is "PrintItemDetail"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                POStatusReportReport_RDG
            column2 of 25%
                POStatusReportReport_XPT
