MobileCashApplicationSearchFormNew is a SearchForm
    title is "CashApplicationSearchFormNew"
    Layout
        two column table
            column1
                CompanyRange.FromCompany
                    label is "Company"
                ReceivableProcessLevelRange.FromReceivableProcessLevel
                    label is "ProcessLevel"
                CurrencyRange.FromCurrency
                    label is "Currency"
                ProjectRange.FromProject
                    label is "Project"
                CustomerRange.FromCustomer
                    label is "Customer"
                    select is CustomerSelect
                TransactionDateRange.Begin
                    label is "TransactionDate"
                CreditMemoRange.FromInvoice
                    label is "Invoice"
            column2
                CompanyRange.ToCompany
                    no label
                ReceivableProcessLevelRange.ToReceivableProcessLevel
                    no label
                CurrencyRange.ToCurrency
                    no label
                ProjectRange.ToProject
                    no label
                CustomerRange.ToCustomer
                    no label
                    select is CustomerSelect
                TransactionDateRange.End
                    no label
                CreditMemoRange.ToInvoice
                    no label
