CreateUsingCopyFromRequisitionRSS is an ActionForm
    action is Unreleased.CreateUsingCopyFromRequisition
        on completion navigate to RequisitionView
    Layout
        paragraph
            PrmCompany
                label is "Company"
            PrmCompany.Name
                no label
                display as text
        single column
            PrmCopyFromRequisition
                label is "CopyFromRequisition"
                select is RequisitionsForACompany
        blank line
        paragraph
            PrmRequester
                label is "Requester"
            PrmRequester.Employee.PresentationNameSnapshot
                no label
                display as text
        paragraph
            PrmRequestingLocation
                label is "RequestingLocation"
                select is ActiveRequestingLocations
            PrmRequestingLocation.Name
                no label
                display as text
        paragraph
            PrmFromCompanyLocation.FromCompany
                label is "FromCompany"
            PrmFromCompanyLocation.FromCompany.Name
                no label
                display as text
        paragraph
            PrmFromCompanyLocation.FromLocation
                label is "FromLocation"
            PrmFromCompanyLocation.FromLocation.Name
                no label
                display as text
        paragraph
            PrmRequestedDeliveryDate
                label is "RequestedDeliveryDate"
        paragraph
            PrmVendor
                label is "DefaultVendor"
            PrmVendor.VendorName
                no label
                display as text
        paragraph
            PrmPurchaseFromLocation
                label is "DefaultPurchaseFromLocation"
        paragraph
            PrmRequisitionSource
                label is "RequisitionSource"
        paragraph
            PrmCopyNonRSS
                label is "CopyFromRSS"
        paragraph
            PrmDeliverTo
                label is "DeliverTo"
        paragraph
            PrmPriority
                label is "Priority"
        paragraph
            PrmBuyer
                label is "Buyer"
        paragraph
            PrmRequisitionDescription
                label is "RequisitionDescription"
        paragraph
            PrmCommodityCode
                label is "CommodityCode"
        paragraph
            PrmTaxCode
                label is "TaxCode"
            PrmPurchaseTaxable
                label is "PurchaseTaxable"
        paragraph
            PrmPoCode
                label is "PoCode"
