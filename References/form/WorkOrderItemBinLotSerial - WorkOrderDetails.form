WorkOrderDetails is a CompositeForm
    is primary
    title is "WorkOrderDetails"
    context form is WorkOrderDetailsForm
        background is theme color
    ComponentSerialCrossReference is a Panel
        visible when (IsValidForSerialCrossReference)
        list is CrossReferencedComponentItemsRel.CrossReferenceSerial
            helper list is ComponentItemsHelperListRel.CrossReferenceSerialHelperList
                action is AddCrossReferenceSerial
                    invoked.PrmItem = WorkOrderItem.Item
                    invoked.PrmKitSerial = ContextSerial
