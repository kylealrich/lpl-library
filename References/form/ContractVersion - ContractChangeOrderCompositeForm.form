ContractChangeOrderCompositeForm is a CompositeForm
    title is "ChangeOrder"
    context form is ChangeOrderContext
        background is theme color
    Actions
        action is SubmitChangeOrder
            hide when invalid
        action is CompleteChangeOrder
            hide when invalid
        action is RejectChangeOrder
            hide when invalid
        action is UnsubmitChangeOrder
            hide when invalid
        action is save
            label is "_"
        action is save and close
        action is refresh
            label is "_"
        navigation is ChangeOrderDocuments
            action icon is print
            label is "ChangeOrderPrintOptions"
        navigation is ContractLink
            label is "GoToContract"
    ChangeOrder is a Panel
        form is ChangeOrderMain
    LinesForChangeOrder is a Panel
        visible when (CanDisplayLinesForChangeOrderForService)
        list is ContractLineRel.ChangeOrderContractLinesExisting
    LinesForChangeOrderForNonService is a Panel
        title is "LinesForChangeOrder"
        visible when (CanDisplayLinesForChangeOrderForNonService)
        list is ContractLineRel.ChangeOrderContractLinesExistingForNonService
    ChangeOrderDetail is a Panel
        visible when (ContractLinesExistForService)
        list is ContractChangeOrder set.ChangeOrderList
    ChangeOrderDetailForNonService is a Panel
        title is "ChangeOrderDetail"
        visible when (ContractLinesExistForNonService)
        list is ContractChangeOrder set.ChangeOrderListForNonService
    AddContacts is a Panel
        list is ContractAttachedContactRel.ChangeOrderContractAttachedContact
    AddComments is a Panel
        list is ContractCommentRel.ChangeOrderContractComments
    AddAttachments is a Panel
        list is ContractAttachmentRel.ChangeOrderContractAttachmentList
    BudgetErrorMessages is a Panel
        visible when (CanDisplayBudgetWarningMessages)
        list is WarningBudgetEditErrorRel.primary
