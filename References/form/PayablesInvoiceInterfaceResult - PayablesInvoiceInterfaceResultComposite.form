PayablesInvoiceInterfaceResultComposite is a CompositeForm
    is primary
    title is "PayablesInvoiceInterfaceResult"
    context form is PayablesInvoiceInterfaceResultContext
        background is theme color
    Parameters is a Panel
        form is Parameters
    InterfaceTotals is a Panel
        visible when (HasInvoicesCreated)
        list is PayablesInvoicesRel.InterfaceTotals
    UnreleasedInvoicesCreated is a Panel
        visible when (HasUnreleasedInvoicesCreated)
        list is PayablesInvoicesUnreleasedRel.InterfaceListingWithErrorMsg
    ReleasedInvoicesCreated is a Panel
        visible when (HasReleasedInvoicesCreated)
        list is PayablesInvoicesNotUnreleasedRel.InterfaceListing
    MultiThreadedInvoices is a Panel
        visible when (HasMultiThreadSubGroupInvoices)
        list is PayablesInvoicesFromMTSubGroupRel.MultiThreadInterfaceListing
    PendingInvoices is a Panel
        visible when (IncompleteHasPendingRecords)
        list is PayablesInvoiceImportRel.InterfaceInvoices
    ErroredInvoices is a Panel
        visible when (IncompleteHasNoCompanyRecords)
        list is PayablesInvoiceImportNoCompanyRel.InterfaceInvoices
    Metrics is a Panel
        visible when (ShowMetrics)
        form is Metrics
