POReceivingLineDetailList is a CompositeForm
    is primary
    title is "PurchaseOrderReceiptLineDetails"
    context form is POReceivingLineDetailListContext
        background is theme color
    restrict action PopulateMissingUOM
    restrict action MobileUpdate
    restrict action MobileUpdateReceiptLine
    Actions
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is refresh
            label is "_"
    Detail is a Panel
        form is ReceiverLineSummary
    CostDetail is a Panel
        form is CostSummary
    QuantityDetail is a Panel
        form is QuantitySummary
    VendorItem is a Panel
        form is VendorDetail
    Source is a Panel
        visible when (IsFromRequisition)
        list is PurchaseOrderLineSourcesRel.PurchaseOrderLineSources
    CatchWeight is a Panel
        visible when (IsCatchWeightItemAndHasItemMaster)
        form is PO304CatchWeight
    InvoiceCost is a Panel
        visible when (PurchaseOrderReceipt.DisplayInvoiceAOCHdr)
        form is PO304InvoiceCost
    InvoiceAddOnCost is a Panel
        visible when (PurchaseOrderReceipt.DisplayInvoiceAOCHdr)
        list is ReceiptLineAOCsRel.PurchaseOrderReceiptLineAOCList
    Track is a Panel
        visible when (POTrackNotExistsAtLineLevel)
        form is POTrackForm
    TrackDetails is a Panel
        title is "Track"
        visible when (POTrackExistsAtLineLevel)
        list is PurchaseOrderReceiptTrackingRel.AdditionalTrackingNumbers
    TrackingNumber is a Panel
        title is "TrackingNumbers"
        visible when (IsTrackingPanelEnabled)
        list is MobileSupplyChainPurchaseOrderReceiptTrackingRel.PurchaseOrderReceiptTrackingList
    ReceivingDetails is a Panel
        title is "<ReceivingDetailLabel>"
        visible when (IsDetailTracked)
        list is PurchaseOrderTransactionDetailsRel.AdditionalReceivingEntrys
    UDIDetails is a Panel
        title is "UDI/DSCSADetail"
        visible when (IsUDIRequiredAtReceipt)
        list is PurchaseOrderUDIDetailRel.UDIEntrys
    Comments is a Panel
        title is "Comments<CommentCount>"
        list is PurchaseOrderReceiptLineCommentRel.PurchaseOrderReceiptLineCommentList
    AssetDetail is a Panel
        title is "AssetDetails<DerivedAssetDetailIndicator>"
        visible when (AssetDetailExist)
        list is PurchaseOrderLineAssetRel.PurchaseOrderLineAssetDetailsRels.PurchaseOrderLineAssetDetailList
