SummaryCompositeDashboard is a CompositeForm
    title is "AutoMatchSummaryDashboard"
    context form is SummaryContext
        background is theme color
    implements DashBoardView
        show context form on top
        show 4 columns
    VendorSummaryTotal is a Panel
        double width
        list is VendorSummaryTotalRel.VendorSummaryTotal
    VendorSummaryDetail is a Panel
        double width
        list is VendorSummaryRel.VendorSummary
    BuyerSummaryTotal is a Panel
        double width
        list is BuyerSummaryTotalRel.BuyerSummaryTotal
    BuyerSummaryDetail is a Panel
        double width
        list is BuyerSummaryRel.BuyerSummary
    ErrorNumberSummaryTotal is a Panel
        double width
        list is ErrorNumberSummaryTotalRel.ErrorNumberSummaryTotal
    ErrorNumberSummaryDetail is a Panel
        double width
        list is ErrorNumberSummaryRel.ErrorNumberSummary
