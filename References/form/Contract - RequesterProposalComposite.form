RequesterProposalComposite is a CompositeForm
    use for action Draft.RequesterCreateProposedContract
    title is "ContractProposal"
    context form is RequesterProposalContext
    restrict action Draft.Create
    restrict action Draft.CreateContractTemplate
    restrict action Draft.CreateContractFromTemplate
    restrict action Delete
    restrict action Draft.CreateContractFromContract
    restrict action Draft.CreateContractProposalTemplate
    restrict action Draft.CreateFromWizard
    restrict action Draft.CreateFromInterview
    restrict action SelectContractForRebate
    restrict action CreateContractDocument
    restrict action CreateDocumentWithFieldsOnly
    restrict action SetCanUseForCostDefaultingToYes
    restrict action HoldContract
    restrict action CloseContract
    restrict action CalculateQualifierUsagePercentagesForContractRange
    restrict action CalculateQualifierUsagePercentagesForOneContract
    restrict action Active.CreateEventWorksheetByContract
    restrict action Active.CreateNewCopy
    restrict action Active.RemoveFromExpireListAction
    restrict action SendEmailForExpiringContracts
    restrict action CompareContractCost
    restrict action Draft.RequesterCreateProposedContract
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    restrict action CheckForContractLineErrors
    restrict action CompareContractCost
    restrict action CreateContractBidWorksheet
    restrict action CreateContractEvaluation
    restrict action EmailContract
    restrict action EmailContractToSupplier
    restrict action EmployeeAddAdditionalSecurityAccessContact
    restrict action EnableForClinicalUse
    restrict action ImportParticipantsFromGroup
    restrict action CreateMessageAndDialog
    Actions
        action is Draft.SupplierSubmitProposedContract
            label is "Submit"
            hide when invalid
        action is save
            label is "_"
        action is refresh
            label is "_"
        action is EmailFromRequester
            label is "EmailPrimaryContact"
            hide when invalid
        navigation is ViewContract
            label is "ViewWholeContract"
    Contract is a Panel
        form is RequesterProposalWizardMain
    Lines is a Panel
        visible when (CanUpdateProposedContract)
        mouse over text is "EnterOrAttachADocumentWithLineDetails"
        panel header is "EnterOrAttachADocumentWithLineDetails"
        list is ContractLine set.RequesterWizardLines
    SubmittedLines is a Panel
        title is "Lines"
        visible when (not CanUpdateProposedContract)
        list is ContractLine set.RequesterWizardLinesSubmitted
    Attachments is a Panel
        list is ContractAttachmentRel.RequesterProposalAttachmentList
    Articles is a Panel
        visible when (NonDeletedContractArticlesExist)
        list is ContractArticlesByDisplayRel.RequesterContractArticles
    Email is a Panel
        title is "EmailNotifications<EmailIndicator>"
        visible when (EmailExists)
        list is ContractNotificationEmailRel.ContractNotificationEmails
