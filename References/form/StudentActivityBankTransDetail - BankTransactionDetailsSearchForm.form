BankTransactionDetailsSearchForm is a SearchForm
    Layout
        three column table
            column1
                paragraph
                    DateRangeFilter
                paragraph
                    StudentActivitySchool
                        label is "School"
                paragraph
                    BankTransactionType
                        label is "Type"
            column2
                paragraph
                    ClosedPeriod
                paragraph
                    StudentActivityBankAccount
                        label is "BankAccount"
                paragraph
                    StudentActivityBankTransaction
                        label is "TransactionNumber"
                        select is BankTransactions
            column3
                paragraph
                    Status
                paragraph
                    SchoolActivityAccount
                        label is "Activity"
                        compact format
                    BankOffsetDetailFilter
                paragraph
                    GLAccount
                        label is "Account"
                        compact format
