LineResponseCompareForm is a Form
    Layout
        four column
            column1
                NotifiedSupplier.Supplier.SupplierName
                text of "Supplier\\/\\Contact\\:_<SourcingEventResponse.NotifiedSupplier.Supplier>\\/<SourcingEventResponse.NotifiedSupplier.SupplierSourceId>"
                SourcingEventResponse.TotalBidAmount
                    label is "TotalBid"
            column2
                SourcingEventResponse.TermsAcceptedBySupplier
                    label is "AcceptTerms"
                NotifiedSupplier.Supplier.Active
                    label is "SupplierStatus"
                NotifiedSupplier.Supplier.PreQualified
                    label is "Pre\\-Qualified"
                NotifiedSupplier.Supplier.DiversityExist
                    label is "DiversityExists"
                blank line
                text of "VendorItem"
                    bold
                VendorItem
                    no label
                VendorItem.VendorItemDescription
                    no label
            column3
                ResponseAmount.UnitPrice
                    label is "UnitPrice"
                        bold
                    display when blank
                Quantity
                    display when blank
                ExtendedPrice
                    display when blank
                SourcingEventLine.UOM.UnitOfMeasure
                    display when blank
                DeliveryDate
                    display when blank
            column4
                text of "Contact"
                    bold
                NotifiedSupplier.SupplierSourceId.MainContact.FirstAndLastName
                    no label
                NotifiedSupplier.SupplierSourceId.TelephoneNumber
                    no label
                NotifiedSupplier.SupplierSourceId.EmailAddress
                    no label
                blank line
                text of "ResponseComments"
                    bold
                ResponseComments
                    no label
