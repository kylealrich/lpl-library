PerformCashRequirements is an ActionForm
    action is PerformCashRequirements
    Layout
        three column independent
            column1
                paragraph
                    VendorGroup
                        when value changed
                            CashRequirementsTemplate =  first CashRequirementsTemplateRel.CashRequirementsTemplate
                    VendorGroup.Description
                        no label
                        display as text
                visible when (RecordExists)
                    paragraph
                        ProcessGroup
                            display only
                        ProcessGroup.Description
                            no label
                            display as text
                visible when (not RecordExists)
                    paragraph
                        ProcessGroup
                            select is ProcessGroups
                            when value changed
                                CashRequirementsTemplate =  first CashRequirementsTemplateRel.CashRequirementsTemplate
                        ProcessGroup.Description
                            no label
                            display as text
            column2
                paragraph
                    PayGroup
                        select is PayGroups
                        when value changed
                            CashRequirementsTemplate =  first CashRequirementsTemplateRel.CashRequirementsTemplate
                    PayGroup.Name
                        no label
                        display as text
            column3
                CashRequirementsTemplate
        header2 of "Main"
        two column independent
            column1
                paragraph
                    AddendaOption
                        label is "EnforceAddendaRecordLimits"
                paragraph
                    DiscountOption
                        label is "TakeAllDiscounts"
                paragraph
                    ProcessFlag
                        label is "ProcessPayments"
                        when value changed
                            refresh CBPaymentsInvolved
                paragraph
                    Comment
                        label is "Comments"
            column2
                paragraph
                    PayThroughDate
                        label is "PayThroughDate"
                paragraph
                    PaymentDate
                paragraph
                    EditPaymentCode
                        select is CashPayments
                    EditPaymentCode.Description
                        no label
                        display as text
                visible when (CBPaymentsInvolved)
                    IncludeCommentOnCashLedgerPayments
        header2 of "CashCodeOptions"
        two column independent
            column1
                paragraph
                    CashCode
                        label is "CashCode"
                    CashCode.Description
                        no label
                        display as text
                paragraph
                    CashCodeGroup
                        label is "CashCodeGroup"
                        select is CashCodeGroup
                    CashCodeGroup.Description
                        no label
                        display as text
            column2
                visible when (not actor.context.FinanceEnterpriseGroup.FundAccounting)
                    paragraph
                        OverrideCashCode
                            label is "OverrideCashCode"
                        OverrideCashCode.Description
                            no label
                            display as text
                OverridePaymentRate
        header2 of "SelectOptions"
        two column distributed
            column1
                paragraph
                    Company
                        label is "InvoiceCompany"
                        select is Companies
                    Company.Name
                        no label
                        display as text
                paragraph
                    PaymentCode
                        select is CashPayments
                    PaymentCode.Description
                        no label
                        display as text
                InvoiceGroup
                InvoiceCurrency
                    label is "InvoiceCurrency"
                SpecialCreditHandling
            column2
                paragraph
                    ProcessLevel
                        label is "ProcessLevel"
                    ProcessLevel.Name
                        no label
                        display as text
                MaxCashOutlay
                    label is "MaximumCashOutlay"
                PayImmediatelyOnly
                    label is "PayImmediatelyOnly"
                CurrencyOption
                VendorSelectGroup
        three column
            VendorClasses.VenClass[1]
                label is "VendorClasses"
            VendorClasses.VenClass[2]
                no label
            VendorClasses.VenClass[3]
                no label
        three column
            VendorClasses.VenClass[4]
                no label
            VendorClasses.VenClass[5]
                no label
            VendorClasses.VenClass[6]
                no label
        three column
            VendorClasses.VenClass[7]
                no label
            VendorClasses.VenClass[8]
                no label
            VendorClasses.VenClass[9]
                no label
        three column
            Vendors.Vendor[1]
                label is "Vendors"
                show up to 5 characters
            Vendors.Vendor[2]
                no label
            Vendors.Vendor[3]
                no label
        three column
            Vendors.Vendor[4]
                no label
            Vendors.Vendor[5]
                no label
            Vendors.Vendor[6]
                no label
        three column
            Vendors.Vendor[7]
                no label
            Vendors.Vendor[8]
                no label
            Vendors.Vendor[9]
                no label
        header2 of "SortOptions"
        two column independent
            column1
                SortOption1
                    label is "SortOption1"
                SortOption3
                    label is "SortOption3"
                SignOver
                    label is "AmountLimit"
            column2
                SortOption2
                    label is "SortOption2"
        visible when (CBPaymentsInvolved)
            header2 of "CashPaymentSortOptions"
            two column independent
                column1
                    CashPaymentSortOption1
                        label is "CashPaymentSortOption1"
                    CashPaymentSortOption3
                        label is "CashPaymentSortOption3"
                column2
                    CashPaymentSortOption2
                        label is "CashPaymentSortOption2"
        header2 of "ReportDistribution"
        two column
            ReportDistributionGroup
