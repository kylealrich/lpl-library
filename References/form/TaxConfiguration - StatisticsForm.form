StatisticsForm is a Form
    Layout
        visible when (ThirdParty.VertexOSeries)
            header1 of "Vertex"
            single column
                VertexTaxLogMessagesCount
                    label is "#OfTaxLogRecords"
                    display as text
                VertexLookupResultsCount
                    label is "#OfLookupResultsRecords"
                    display as text
                VertexTaxAreaIdResultsCount
                    label is "#OfTaxAreaIdResultsRecords"
                    display as text
                JurisdictionResultsCount
                    label is "#OfJurisdictionResultsRecords"
                    display as text
        visible when (not ThirdParty.VertexOSeries)
            header1 of "TaxTransactions"
            single column
                TaxTransactionsCount
                    label is "TaxTransactions"
                    display as text
                OnesourceTaxLogMessagesCount
                    label is "#OfTaxLogRecords"
                    display as text
                VatPurchaseSaleTransactionCount
                    label is "VatPurchaseSaleTransactionRecords"
                    display as text
        visible when (WebserviceErrorsExist)
            WebserviceErrorCount
                label is "#OfWebserviceErrors"
                display as text
