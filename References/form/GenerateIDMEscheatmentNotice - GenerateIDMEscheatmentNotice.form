GenerateIDMEscheatmentNotice is an ActionForm
    action is GenerateIDMEscheatmentNotice
    title is "GenerateIDMEscheatmentNotice"
    Layout
        single column
            PrmCashManagementGroup
        two column
            PrmCashCode
                select is CashCode
            PrmCashCode.Description
                no label
                display as text
        single column
            text of "Or"
        two column
            PrmCashCodeGroup
                select is CashCodeGroup
            PrmCashCodeGroup.Description
                no label
                display as text
        two column
            PrmBankTransactionCode
                select is BankTransactionCodeIDMList
            PrmBankTransactionCode.Description
                no label
                display as text
        single column
            PrmProcessLevel
        single column
            PrmTransactionStatus
        two column
            PrmDateRange.Begin
                label is "FromIssueDate"
            PrmDateRange.End
                label is "ToIssueDate"
        two column
            PrmIDMPrinter
                select is ActiveIDMPrinter
            PrmIDMPrinter.Description
                no label
                display as text
