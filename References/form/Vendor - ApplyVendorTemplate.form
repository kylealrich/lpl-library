ApplyVendorTemplate is an ActionForm
    action is ApplyVendorTemplate
    Layout
        single column
            paragraph
                visible when (VendorTemplate entered)
                    VendorTemplate
                        label is "CurrentVendorTemplate"
                        display as text
                    VendorTemplate.Description
                        no label
                        display as text
            paragraph
                PrmTemplate
                    label is "VendorTemplate"
                PrmTemplate.Description
                    no label
                    display as text
