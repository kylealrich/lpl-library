PutawayDetailSumForm is a SummaryForm
    title is "-"
    Layout
        five column distributed
            column1
                DerivedLineNumber
                    no label
                visible when (PurchaseOrderReceiptLineRel.IsCatchWeightItem)
                    blank line
                PurchaseOrderReceipt
                    label is "Receipt"
                PurchaseOrderReceiptRel.ReceivedDate
                    label is "Date"
            column2
                DerivedItemAndDescription
                    no label
            column3
                PurchaseOrderReceiptLineRel.EnteredReceivedQuantity
                    no label
                visible when (PurchaseOrderReceiptLineRel.IsCatchWeightItem)
                    PurchaseOrderReceiptLineRel.CatchWeightQuantity
                        no label
                visible when (not HasPOCode)
                    PurchaseOrderReceiptRel.PurchaseOrder
                        label is "PO"
                visible when (HasPOCode)
                    DisplayPurchaseOrderAndPOCode
                        label is "PO"
                PurchaseOrderReceiptRel.PurchaseOrder.PurchaseOrderDate
                    label is "Date"
            column4
                PurchaseOrderReceiptLineRel.ReceivedUOM
                    no label
                visible when (PurchaseOrderReceiptLineRel.IsCatchWeightItem)
                    PurchaseOrderReceiptLineRel.CatchWeightUOM
                        no label
            column5
                PurchaseOrderReceiptLineRel.PurchaseOrderLine.DerivedHasSpecialHandling
                    no label
        blank line
        visible when (DetailExists)
            list is PurchaseOrderTransactionDetailsRel.DetailsForPutawayList
