EffectiveDatedEntryForm is a Form
    is primary
    Layout
        single column
            create stamp
                label is "Created"
                display only
            create stamp.actor
                label is "Actor"
                display only
        paragraph
            RecordID
                label is "BusinessObject"
                display only
            visible when (ObjectMissing)
                text of "BusinessObjectMissing" with icon.redalert
                    bold
                    italics
                    color of red
        paragraph
            EntryID
                label is "AuditEntry"
                display only
        paragraph
            visible when (TargetMissing)
                text of "<RedMessage>"
                    bold
                    italics
                    color of red
            visible when (PreviousPending)
                text of "<PreviousPendingMessage>"
                    bold
                    italics
                    color of red
                    link is PreviousPendingRequest
            visible when (YellowAlert)
                text of "<YellowMessage>" with icon.yellowalert
                    bold
                    italics
