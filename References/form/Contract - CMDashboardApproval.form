CMDashboardApproval is a Form
    print form is CMDashboardApprovalSummaryForm
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        create date
            label is "Created"
            display as text
        six column
            column1
                text of "Action"
                    header4
                visible when (SubmittedTimestampEntered)
                    text of "Submitted"
                        bold
                visible when (AcceptedOrRejectedTimestampEntered)
                    text of "Decision"
                        bold
                visible when (ReleasedTimestampEntered)
                    text of "Released"
                        bold
                visible when (InternalApprovalTimestampEntered)
                    text of "Internal_Approval"
                        bold
            column2
                text of "Date"
                    header4
                visible when (SubmittedTimestampEntered)
                    text of "<SubmittedTimestamp>"
                        bold
                visible when (AcceptedOrRejectedTimestampEntered)
                    text of "<AcceptedOrRejectedTimestamp>"
                        bold
                visible when (ReleasedTimestampEntered)
                    text of "<ReleasedTimestamp>"
                        bold
                visible when (InternalApprovalTimestampEntered)
                    text of "<InternalApprovalTimestamp>"
                        bold
            column3
                text of "Days"
                    header4
                visible when (SubmittedTimestampEntered)
                    text of "<DaysToSubmit>"
                        bold
                visible when (AcceptedOrRejectedTimestampEntered)
                    text of "<DaysToAccept>"
                        bold
                visible when (ReleasedTimestampEntered)
                    text of "<DaysToRelease>"
                        bold
                visible when (InternalApprovalTimestampEntered)
                    text of "<DaysToInternalApprove>"
                        bold
            column4
                text of "Action"
                    header4
                visible when (SupplierApprovalTimestampEntered)
                    text of "Supplier_Approval"
                        bold
                visible when (NegotiationApprovalTimestampEntered)
                    text of "Negotiation_Approval"
                        bold
                visible when (FinalApprovalTimestampEntered)
                    text of "Final_Approval"
                        bold
                visible when (ActivatedTimestampEntered)
                    text of "Contract_Activation"
                        bold
            column5
                text of "Date"
                    header4
                visible when (SupplierApprovalTimestampEntered)
                    text of "<SupplierApprovalTimestamp>"
                        bold
                visible when (NegotiationApprovalTimestampEntered)
                    text of "<NegotiationApprovalTimestamp>"
                        bold
                visible when (FinalApprovalTimestampEntered)
                    text of "<FinalApprovalTimestamp>"
                        bold
                visible when (ActivatedTimestampEntered)
                    text of "<ActivatedTimestamp>"
                        bold
            column6
                text of "Days"
                    header4
                visible when (SupplierApprovalTimestampEntered)
                    text of "<DaysToSupplierApprove>"
                        bold
                visible when (NegotiationApprovalTimestampEntered)
                    text of "<DaysToApproveNegotiation>"
                        bold
                visible when (FinalApprovalTimestampEntered)
                    text of "<DaysToFinalApprove>"
                        bold
                visible when (ActivatedTimestampEntered)
                    text of "<DaysToActivate>"
                        bold
        visible when (HasWorkunit2)
            header2 of "FinalApprovalProgress"
            visible when (first PfiWorkunitRel2.HasProcessIndicator)
                first PfiWorkunitRel2.PfiProcessIndicatorRel.Model
                    display as text
                header2 of "History"
            UserActionRoutingAsHtml2
                display as text
            first PfiWorkunitRel2.ElapsedTimeAsString
                label is "ApprovalDuration"
                display as text
        visible when (HasWorkunit3)
            header2 of "NegotiationsApprovalProgress"
            visible when (first PfiWorkunitRel3.HasProcessIndicator)
                first PfiWorkunitRel3.PfiProcessIndicatorRel.Model
                    display as text
                header2 of "History"
            UserActionRoutingAsHtml3
                display as text
            first PfiWorkunitRel3.ElapsedTimeAsString
                label is "ApprovalDuration"
                display as text
        visible when (HasWorkunit4)
            header2 of "NegotiationsApprovalProgress"
            visible when (first PfiWorkunitRel4.HasProcessIndicator)
                first PfiWorkunitRel4.PfiProcessIndicatorRel.Model
                    display as text
                header2 of "History"
            UserActionRoutingAsHtml4
                display as text
            first PfiWorkunitRel4.ElapsedTimeAsString
                label is "ApprovalDuration"
                display as text
        visible when (HasWorkunit5)
            header2 of "RedliningApprovalProgress"
            UserActionRoutingAsHtml5
                display as text
            first PfiWorkunitRel5.ElapsedTimeAsString
                label is "ApprovalDuration"
                display as text
        visible when (HasWorkunit)
            header2 of "ApprovalProgress"
            visible when (first PfiWorkunitRel.HasProcessIndicator)
                first PfiWorkunitRel.PfiProcessIndicatorRel.Model
                    display as text
                header2 of "History"
            UserActionRoutingAsHtml
                display as text
            first PfiWorkunitRel.ElapsedTimeAsString
                label is "ApprovalDuration"
                display as text
