CashApplicationSelectNewOld is a SearchForm
    Layout
        two column distributed
            column1
                paragraph
                    ApplicationCompanyRange.FromCompany
                        label is "FromCompany"
                    ApplicationCompanyRange.ToCompany
                        label is "ToCompany"
            column2
                paragraph
                    CustomerRange.FromCustomer
                        select is CustomerSelect
                    CustomerRange.ToCustomer
                        select is CustomerSelect
        three column distributed
            column1
                paragraph
                    BatchRange.BegBatch
                        label is "FromBatch"
                        show up to 4 characters
                    BatchRange.EndBatch
                        label is "ToBatch"
                        show up to 4 characters
            column2
                paragraph
                    TransactionAmountRange.FromAmount
                        label is "FromTransactionAmount"
                        show up to 4 characters
                    TransactionAmountRange.ToAmount
                        label is "ToTransactionAmount"
                        show up to 4 characters
            column3
                paragraph
                    PaymentNumberRange.FromTransNbr
                        label is "FromPayment"
                        show up to 4 characters
                    PaymentNumberRange.ToTransNbr
                        label is "ToPayment"
                        show up to 4 characters
        two column distributed
            column1
                paragraph
                    DepositDateRange.Begin
                        label is "FromDepositDate"
                    DepositDateRange.End
                        label is "ToDepositDate"
            column2
                paragraph
                    PostingDateRange.Begin
                        label is "FromGLDate"
                    PostingDateRange.End
                        label is "ToGLDate"
        three column
            column1
                paragraph
                    CurrencyRange.FromCurrency
                    CurrencyRange.ToCurrency
            column2
                HasSelectApplications
                    label is "PendingApplications"
            column3
                paragraph
                    DerivedStatus
                        label is "Status"
                        show up to 8 characters
