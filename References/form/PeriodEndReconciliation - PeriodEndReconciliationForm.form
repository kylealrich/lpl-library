PeriodEndReconciliationForm is a CompositeForm
    is primary
    propagate as a drill
    title is "PeriodReconciliationForPeriod<PeriodEndReconciliation.GeneralLedgerClosePeriod>:_<PeriodEndReconciliation.GeneralLedgerClosePeriod.PeriodName>"
    context form is PeriodEndReconciliationContextForm
        background is theme color
    Actions
        action is Open.StartReconciliation
            hide when invalid
        action is Open.StartReconciliationLate
            hide when invalid
        action is InProcess.Complete
            hide when invalid
        action is InProcess.CompleteReconciliationLate
            hide when invalid
        action is CreateAmortizationDistributions
            hide when invalid
        action is InProcess.ReleaseDistributionsOtherRequiredFields
            label is "ReleaseDistributions"
            hide when invalid
        action is InProcess.ReleaseDistributions
            hide when invalid
        action is InProcess.JournalizeDistributions
            hide when invalid
        action is Closed.Reopen
            hide when invalid
        navigation is DetailTransactionReconciliation
            hide when invalid
        navigation is SavedDetailTransactionReconciliation
            hide when invalid
        navigation is OtherReconciliations
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is open
            label is "_"
        action is save
            label is "_"
        action is Open.Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
            hide when invalid
        action is InProcess.Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
            hide when invalid
        action is search
            label is "_"
        action is refresh
            label is "_"
    Main is a Panel
        form is PeriodEndReconciliationMain
    Accounts is a Panel
        visible when (LevelType.Group)
        list is PeriodEndReconciliationChildrenRel.ChildrenPeriodEndReconciliationList
    ReconciliationDetails is a Panel
        visible when (ShowReconciliationDetailsTab)
        list is PeriodDetailsRel.PeriodListForDetails
            helper list is ReconciliationDetailRel.ReconciliationDetailHelperList
                action is CreatePeriodDetail
    ReconcilingItems is a Panel
        visible when (ShowReconcilingItemsTab)
        list is PeriodReconcilingDetailRel.PeriodListForReconcilingItems
            helper list is ReconcilingItemRel.ReconciliationDetailHelperList
                action is CreatePeriodDetail
    AmortizationDetails is a Panel
        visible when (HasAmortizationDetails)
        list is PeriodAmortizedDetailRel.PeriodEndAmortizationDetailList
    AmortizationDistributions is a Panel
        visible when (HasAmortizationDetails)
        list is PeriodEndDistribution set.AmortizationDistributionList
    AssetAmounts is a Panel
        visible when (ShowAssetAmountsTab)
        form is PeriodEndAssetAmountsForm
    AssetGroupAmounts is a Panel
        title is "AssetAmounts"
        visible when (ShowAssetGroupAmountsTab)
        form is PeriodEndAssetAmountsGroupForm
    AccrualCodes is a Panel
        visible when (ShowAccrualCodesTab)
        list is PayablesAccrualCodeDetailRel.ReconciliationAccrualCodeDetailList
    InvoicesPostedToAccrualAccount is a Panel
        visible when (ShowInvoicePostedToAccrualAccountTab)
        list is PayablesInvoiceDistributionToAccrualRel.ReconciliationPayablesInvoiceDistributionsList
    NonAPJournalTransactions is a Panel
        visible when (ShowNonAPJournalTransactionsTab)
        list is NonAPGeneralLedgerTransactionRel.ReconNonAPGeneralLedgerTransactionList
    NonARJournalTransactions is a Panel
        visible when (ShowNonARJournalTransactionsTab)
        list is NonARGeneralLedgerTransactionRel.ReconNonARGeneralLedgerTransactionList
    NonAssetJournalTransactions is a Panel
        visible when (ShowNonAssetJournalTransactionsTab)
        list is NonAssetGeneralLedgerTransactionRel.ReconNonAssetGeneralLedgerTransactionList
    NonCashAccountJournalTransactions is a Panel
        visible when (ShowNonCashAccount)
        list is NonCashAccountGeneralLedgerTransactionRel.ReconNonCashGeneralLedgerTransactionList
    NonRNIJournalTransactions is a Panel
        visible when (ShowNonRNIJournalTransactionsTab)
        list is NonRNIMNRGeneralLedgerTransactionRel.ReconNonRNIGeneralLedgerTransactionList
    NonMNRJournalTransactions is a Panel
        visible when (ShowNonMNRJournalTransactionsTab)
        list is NonRNIMNRGeneralLedgerTransactionRel.ReconNonMNRGeneralLedgerTransactionList
    NonInventoryJournalTransactions is a Panel
        visible when (ShowNonInventoryJournalTransactionsTab)
        list is NonInventoryGeneralLedgerTransactionRel.ReconNonInventoryGeneralLedgerTransactionList
    LedgerTransactions is a Panel
        visible when (ShowLedgerTransactionsTab)
        list is GeneralLedgerTransactionRel.ReconciliationGeneralLedgerTransactionList
    SavedDetailTransactionReconciliation is a Panel
        visible when (HasPeriodEndTransReconDetails)
        list is PeriodEndTransReconDetailRel.ReconciliationDetailsWithCurrentList
    Distributions is a Panel
        visible when (ShowDistributionsTab)
        list is PeriodEndDistribution set.EnteredPeriodEndDistributionList
    CommentsAndAttachments is a Panel
        visible when (ShowCommentsTab)
        list is PeriodEndAccountCommentRel.PeriodEndCommentList
    ApprovalInformation is a DashBoardPanel
        visible when (DisplayApprovalInformationPanel)
        show 4 columns
        ApprovalProgress is a Panel
            visible when (DisplayReconciliationApprovalProgressPanel)
            triple width
            form is ReconciliationProcessIndicatorForm
        RejectHistory is a Panel
            visible when (ShowApprovalCommentsTab)
            triple width
            list is RejectedReconciliationReasonCodeUsageRel.ReconciliationReasonCodeUsageList
    ReasonCodeHistory is a MultiListPanel
        visible when (ShowReasonCodeHistoryTab)
        list is ReconciliationReasonCodeUsageReopenRel.ReopenReasonCodes
            visible when (HasReopenReasonCodeHistory)
        list is ReconciliationReasonCodeUsageCloseManagementRel.CloseManagementReasonCodes
            visible when (HasCloseManagementReasonCodeHistory)
