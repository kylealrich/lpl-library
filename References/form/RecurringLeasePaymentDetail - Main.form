Main is a Form
    Layout
        two column
            column1
                header2 of "Compute"
                paragraph
                    ComputeGroup.Company
                    ComputeGroup.Company.Name
                        no label
                        display as text
                    text of "Or"
                    ComputeGroup.GeneralLedgerCompanyGroup
                        label is "CompanyGroup"
                    ComputeGroup.GeneralLedgerCompanyGroup.Description
                        no label
                        display as text
                paragraph
                    ComputeGroup.ComputeFromAccount
                        compact format
                paragraph
                    ComputeGroup.ComputeThroughAccount
                        compact format
                blank line
                paragraph
                    GeneralLedgerBudgetNumber
                        label is "BudgetNumber"
                header2 of "AdvancedComputeSettings"
                paragraph
                    AccrueFlag
                        label is "AccruePayment"
                    AccrueReverseFlag
                        label is "Reverse"
                blank line
                paragraph
                    ComputeBaseAmount
                    MaximumPayment
                        label is "MaximumComputeAmount"
                    AdvancedComputeOption
            column2
                header2 of "Tax"
                paragraph
                    Taxable
                paragraph
                    TaxCode
                    TaxCode.Description
                        no label
                        display as text
                paragraph
                    TransactionTaxableAmount
                        label is "TaxableAmount"
                    PercentTaxable
                        label is "_OrPercent"
                paragraph
                    TaxUsageCode
                    TaxUsageCode.Description
                        no label
                        display as text
                blank line
                visible when (AllowLandedTax)
                    paragraph
                        TransactionLandTax
                            label is "LandedTax"
                            display as text
                paragraph
                    TotalTaxAmount
                        display as text
                paragraph
                    TotalPaymentAmount
                        display as text
