WireTransferBatchForm is a CompositeForm
    is primary
    title is "WireTransfer<DerivedWireTransferBatch>"
    context form is WireTransferBatchContext
        background is theme color
    Actions
        action is New.Release
            label is "Release"
            hide when invalid
        action is New.SubmitForApproval
            label is "SubmitForApproval"
            hide when invalid
        action is Released.Close
            label is "Close"
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is New.Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is save
            label is "_"
        action is New.Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    InternalDetails is a Panel
        title is "Details"
        visible when (TransferType.Internal)
        list is WireTransferBatchDetail set.WireTransferBatchDetailInternalList
    ExternalDetails is a Panel
        title is "Details"
        visible when (TransferType.External)
        list is WireTransferBatchDetail set.WireTransferBatchDetailExternalList
