CreateProjectEmployee is an ActionForm
    action is CreateProjectEmployee
    Layout
        two column
            column1
                Name.PreferredFirstAndLastName
                    label is "Name"
                    display as text
            column2
                Employee
                    label is "ID"
                    display as text
        header4 of "LaborDistribution"
        single column
            PrmExcludeFromLabor
                label is "ExcludeFromLaborDistribution"
        visible when (not PrmExcludeFromLabor)
            single column
                PrmExcludeFromCertification
                    label is "ExcludeFromEffortCertification"
                PrmLaborFTE
                    label is "F\\T\\E"
            two column
                column1
                    PrmProjectLaborSchedule
                        label is "LaborSchedule"
                    visible when (not PrmExcludeFromCertification)
                        PrmCertifier
                            label is "Certifier"
                    PrmApprover
                        label is "LaborDistributionApprover"
                    PrmRedistributionApprover
                        label is "RedistributionApprover"
                column2
                    PrmProjectLaborSchedule.Description
                        no label
                        display as text
                    visible when (not PrmExcludeFromCertification)
                        PrmCertifier.PresentationNameSnapshot
                            no label
                            display as text
                    PrmApprover.PresentationNameSnapshot
                        no label
                        display as text
                    PrmRedistributionApprover.PresentationNameSnapshot
                        no label
                        display as text
            single column
                PrmFinanceCodeBlock
                    label is "LaborDistributionFinanceStructure"
                    compact format
                text of "DimensionsSetToBlank:"
                    align as label
                PrmFinanceCodeBlockSetBlanks
                    no label
            two column
                column1
                    PrmBurdenCode
                        label is "BurdenCode"
                column2
                    paragraph
                        PrmBurdenCode.Description
                            no label
                            display as text
                        PrmBurdenCode.Rate
                            label is "Rate"
                            display as text
            single column
                PrmBurdenCodeBlock
                    label is "FringeBurdenFinanceStructure"
                    compact format
                text of "DimensionsSetToBlank:"
                    align as label
                PrmBurdenCodeBlockSetBlanks
                    no label
            header4 of "SalaryEncumbrance"
            two column
                PrmSalaryEncumbrance
                    label is "SalaryEncumbrance"
                PrmFringeEncumbrance
                    label is "FringeEncumbrance"
            visible when (PrmSalaryEncumbrance)
                three column
                    column1
                        PrmEncumbranceSalaryAmount
                            label is "SalaryEncumbranceAmount"
                    column2
                        PrmCurrency
                            label is "SalaryEncumbranceCurrency"
                    column3
                        PrmEncumbranceBasis
                            label is "EncumbranceBasis"
                single column
                    PrmSalaryEncumbranceCodeBlock
                        label is "SalaryEncumbranceFinanceStructure"
                        compact format
            visible when (PrmFringeEncumbrance)
                single column
                    PrmFringeEncumbranceCodeBlock
                        label is "FringeEncumbranceFinanceStructure"
                        compact format
