RegulatoryForeignInvoices is an ActionForm
    action is RegulatoryForeignInvoices
    Layout
        header2 of "Main"
        paragraph
            Company
            Company.Name
                no label
                display as text
        single column
            text of "Or"
                bold
        paragraph
            CompanyGroup
                label is "CompanyGroup"
            CompanyGroup.Description
                no label
                display as text
        paragraph
            FrGlDate
                label is "FromPostDate"
            ToGlDate
                label is "ToPostDate"
        paragraph
            ProcessLevel
            ProcessLevel.Name
                no label
                display as text
        paragraph
            Customer
            Customer.Name
                no label
                display as text
        two column
            column1
                paragraph
                    CountryCode
                    CountryCode.Name
                        no label
                        display as text
            column2
                paragraph
                    ExclCountryCode
                        label is "ExcludeCountryCode"
                    ExclCountryCode.Name
                        no label
                        display as text
        single column
            PrintDetail
        paragraph
            ExclVatRegCtry
                label is "ExcludeVATCountryCode"
            ExclVatRegCtry.Name
                no label
                display as text
        single column
            VatReg
                label is "VATRegistrationNumber"
        header2 of "Selection"
        single column
            Status
        two column distributed
            column1
                NonArPay
                    label is "NonARPayment"
                Credits
                    label is "CreditMemos"
                Invoice
                    label is "Invoices,BOE"
                DebitMemo
            column2
                RetPay
                    label is "RTMDebitMemos"
                FinChg
                    label is "FinanceChargeDebitMemos"
                ChgBack
                    label is "ChargebackDebitMemos"
                AllTrans
                    label is "AllTransactions"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                RegulatoryForeignInvoicesReport_RDG
                RegulatoryForeignInvoicesErrorReport_RDG
            column2 of 25%
                RegulatoryForeignInvoicesReport_XPT
                RegulatoryForeignInvoicesErrorReport_XPT
