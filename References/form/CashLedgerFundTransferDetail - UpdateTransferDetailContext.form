UpdateTransferDetailContext is a Form
    Layout
        two column distributed
            column1
                display as text
                    paragraph
                        CashLedgerFundTransferHeader.CashCode
                            label is "<DerivedHeaderTransactionTypeLabel>CashCode"
                        CashLedgerFundTransferHeader.CashCode.Currency
                            no label
                    CashLedgerFundTransferHeader.Status
                    paragraph
                        CashLedgerFundTransferHeader.TransferBankAmount
                            label is "<DerivedHeaderTransactionTypeLabel>Amount"
                        visible when (CashLedgerFundTransferHeader.IsUnreleased)
                            CashLedgerFundTransferHeader.AppliedBankAmount
                                label is "<DerivedHeaderTransactionTypeLabel>AppliedBankAmount"
                        visible when (CashLedgerFundTransferHeader.IsUnreleased)
                            CashLedgerFundTransferHeader.BalanceAmount
                                label is "<DerivedHeaderTransactionTypeLabel>Balance"
                        visible when (CashLedgerFundTransferHeader.IsUnreleased)
                            CashLedgerFundTransferHeader.AppliedBaseAmount
                                label is "<DerivedHeaderTransactionTypeLabel>AppliedBaseAmount"
            column2
                display as text
                    paragraph
                        CashLedgerFundTransferHeader
                            label is "<DerivedHeaderTransactionTypeLabel>TransferNumber"
                        visible when (CashLedgerTransactionIdentifierExists)
                            DerivedTransactionIdentifier
                                label is "TransactionIdentifier"
                    paragraph
                        CashLedgerFundTransferHeader.Company.OrigCompany.RepresentativeText
                            label is "<DerivedHeaderTransactionTypeLabel>Company"
                        CashLedgerFundTransferHeader.Company.OrigCompany.Currency
                            no label
                        CashLedgerFundTransferHeader.CurrencyTable
                            label is "<DerivedHeaderTransactionTypeLabel>CurrencyTable"
                    visible when (ShowDetailAccountingUnit)
                        paragraph
                            CashLedgerFundTransferHeader.HeaderAccountingUnit
                    visible when (ShowDetailFund)
                        paragraph
                            CashLedgerFundTransferHeader.HeaderFund
                            CashLedgerFundTransferHeader.HeaderFund.Description
                                no label
