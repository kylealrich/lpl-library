InterfaceSelectedCashLedgerRunGroups is a Form
    is primary
    Layout
        single column
            CashManagementGroup
                display as text
        blank line
        header2 of "SelectRecordsToInterface"
        three column table
            column1
                InterfaceTransactions
                    label is "Transactions"
                InterfacePayments
                    label is "Payments"
                InterfaceReceipts
                    label is "Receipts"
                InterfaceReturnedPayments
                    label is "ReturnedPayments"
            column2
                visible when (CashManagementGroup.AutoTranNumberForTransactionForm)
                    AutoNumberingActionForTransactions
                        label is "AutoNumberingActionForTransactions"
                visible when (CashManagementGroup.AutoReferenceNumberForPaymentForm)
                    AutoNumberingActionForPayments
                        label is "AutoNumberingActionForPayments"
                visible when (CashManagementGroup.AutoTranNumberForReceiptForm)
                    AutoNumberingActionForReceipts
                        label is "AutoNumberingActionForReceipts"
                visible when (CashManagementGroup.AutoTranNumberForReturnedPaymentForm)
                    AutoNumberingActionForReturnedPayments
                        label is "AutoNumberingActionForReturnedPayments"
            column3
                text of "_"
                text of "_"
                visible when (CashManagementGroup.AutoReferenceNumberForEFTForm)
                    AutoNumberingActionForEFT
                        label is "AutoNumberingActionForEFT"
        blank line
        header2 of "Parameters"
        two column
            SelectRunGroupsLike
            text of "EnterTextAndWildcards"
        single column
            MoveErrorsToNewRunGroup
            visible when (MoveErrorsToNewRunGroup)
                ErrorRunGroupPrefix
            CurrencyTable
            JournalizeReleasedDistributions
            KeepEnteredSignForBankCredits
