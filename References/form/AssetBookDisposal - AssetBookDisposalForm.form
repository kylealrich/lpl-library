AssetBookDisposalForm is a Form
    is primary
    title is "AssetBookDisposal"
    restrict action Create
    restrict action Update
    restrict action UpdateBook
    restrict action Delete
    Layout
        paragraph
            Asset
                display as text
            Asset.Description
                no label
                display as text
            Book
        single column
            blank line
            AssetBookRel.PostingCurrency
                label is "Currency"
            Proceeds
            CostOfSale
            DepreciationExpense
            GainOrLoss
            DeferredGainOrLoss
