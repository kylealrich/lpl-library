ReceivableInvoiceDetailForm is a Form
    Layout
        display as text
            four column distributed
                column1
                    paragraph
                        visible when (IsManualEntry)
                            button of "<BatchNumber>"
                                link is ReceivableInvoiceBatchNav
                                label is "BatchNumber"
                                display as link
                        visible when (not IsManualEntry)
                            BatchNumber
                                label is "BatchNumber"
                                display as text
                        Origin
                column2
                    paragraph
                        ReceivableProcessLevel.RepresentativeText
                            display as text
                column3
                    paragraph
                        DueDate
                column4
                    paragraph
                        TermsCode.DerivedTermsCodeTitle
            four column distributed
                column1
                    paragraph
                        BankTransactionCode.BankTransactionCodeName
                            label is "PaymentCode"
                column2
                    paragraph
                        BankTransactionCode.TransactionCategoryAndOrigin
                            label is "PaymentCodeTransactionCategoryAndOrigin"
                column3
                    paragraph
                        EFTTransaction
                column4
                    paragraph
                        SalesRepresentative.DerivedSalesRepresentativeText
            four column distributed
                column1
                    paragraph
                        AlternateReceivableInvoiceType
                            label is "AlternateInvoiceType"
                        visible when (CreditDebitMemoReasonEntered)
                            CreditDebitMemoReason
                column2
                    paragraph
                        HistoryPeriods
                column3
                    paragraph
                        ItemReference
                column4
                    paragraph
                        CustomerPurchaseOrder
                            label is "PurchaseOrder"
                            display as text
            visible when (ShowDetailedFinanceStructure)
                header3 of "_"
                text of "Invoice_Accrual_Account:"
                    header4
                    bold
                paragraph
                    InvoiceFinanceCodeBlock
                        label is "InvoiceAccrualAccount"
            visible when (not ShowDetailedFinanceStructure)
                header3 of "_"
                two column
                    column1 of 25%
                        paragraph
                            ReceivableGeneralLedgerCode.DerivedReceivableGlobalLedgerCodeTitle
                    column2 of 75%
                        paragraph
                            ReceivableGeneralLedgerCode.DistributionAccount
            visible when (IsDiscount)
                header3 of "_"
            four column distributed
                column1
                    visible when (IsDiscount)
                        paragraph
                            DiscountDate.DiscDate[1]
                                label is "DiscountDate"
                            DiscountPercent.DiscPct[1]
                                label is "Percent"
                            DiscountAmount.OrigDiscAmt[1]
                                label is "Amount"
                column2
                    visible when (IsMultipleDiscount)
                        paragraph
                            DiscountDate.DiscDate[2]
                                label is "Discount2Date"
                            DiscountPercent.DiscPct[2]
                                label is "Discount2Percent"
                            DiscountAmount.OrigDiscAmt[2]
                                label is "Discount2Amount"
                column3
                    visible when (IsMultipleDiscount)
                        paragraph
                            DiscountDate.DiscDate[3]
                                label is "Discount3Date"
                            DiscountPercent.DiscPct[3]
                                label is "Discount3Percent"
                            DiscountAmount.OrigDiscAmt[3]
                                label is "Discount3Amount"
            visible when (FinanceDimension2OrProjectContractOrProjectEntered)
                header3 of "_"
                text of "Project_Information:"
                    header4
                    bold
                four column distributed
                    column1
                        paragraph
                            FinanceDimension2
                                label is "<GeneralLedgerCompanyRel.FinanceEnterpriseGroup.FinanceDimension2Label>"
                    column2
                        paragraph
                            ProjectContract.DisplayContract
                                label is "<GeneralLedgerCompanyRel.FinanceEnterpriseGroup.ProjectLabel>Contract"
                    column3
                        paragraph
                            Project
                                label is "<GeneralLedgerCompanyRel.FinanceEnterpriseGroup.ProjectLabel>"
                    column4
                        paragraph
            visible when (EFTTransaction)
                header3 of "_"
                text of "Electronic_Funds_Transfer_Information:"
                    header4
                    bold
                four column distributed
                    column1
                        paragraph
                            EftAction
                                label is "Action"
                    column2
                        paragraph
                            EFTNotification
                                label is "Notification"
                        paragraph
                            visible when (EFTNotificationExtractResultEntered)
                                EFTNotificationExtractResult
                                    label is "NotificationResultID"
                    column3
                        paragraph
                            EftExtract
                                label is "Extract"
                        visible when (EFTNotificationExtractResultEntered)
                            EFTExtractResult
                                label is "ExtractResultID"
                    column4
                        paragraph
                            visible when (first PaymentCreatedElectronicFundsTransferRelExists)
                                first PaymentCreatedElectronicFundsTransferRel.update stamp
                                    label is "PaymentCreateDate"
            header3 of "_"
            text of "Application_Information:"
                header4
                bold
            paragraph
                ApplicationAmount.CurrencyAmount
                    label is "AppliedAmount"
                AdjustmentAmount.CurrencyAmount
                    label is "AdjustmentAmount"
                LastApplicationSequence
                button of "ApplyCredit"
                    visible when (DisplayOnOpenCreditMemoList)
                    link is CreditApplicationFormOnTopNav
            button of "ReversePostedApplications"
                visible when (AvailableForReversal)
                link is TransactionReversalCompositeNav
                display as link
            visible when (LastNumbersEntered)
                header3 of "_"
                text of "Last:"
                    header4
                    bold
            visible when (LastNumbersEntered)
                four column distributed
                    column1
                        paragraph
                            LastFinanceChargeDate
                                label is "FinanceChargeDate"
                    column2
                        paragraph
                            LastStatementDate
                                label is "StatementDate"
                    column3
                        paragraph
                            LastDunningLetterDate
                                label is "DunningLetterDate"
                    column4
                        paragraph
                            LastDunningLetter
                                label is "DunningLetter"
            visible when (OriginIntrastatCountryOrForeignEconomicRegulatoryCodeEnteredOrIsHSNSACCode)
                header3 of "_"
            visible when (OriginIntrastatCountryEntered)
                text of "Intrastat:"
                    header4
                    bold
            four column distributed
                column1
                    visible when (OriginIntrastatCountryEntered)
                        paragraph
                            OriginIntrastatCountry
                                label is "ShipFromCountry/\\Jurisdiction"
                            OriginIntrastatCountry.Name
                                no label
                                display as text
                column2
                    visible when (DestinationIntrastatCountryEntered)
                        paragraph
                            DestinationIntrastatCountry
                                label is "ShipToCountry/\\Jurisdiction"
                            DestinationIntrastatCountry.Name
                                no label
                                display as text
                column3
                    paragraph
                        visible when (ForeignEconomicRegulatoryCodeEntered)
                            ForeignEconomicRegulatoryCode.DerivedForeignEconomicRegulatoryCodeTitle
                column4
                    paragraph
                        visible when (IsHSNSACCodeEnabled)
                            HSNSACCode
            visible when (ReceivableCrossReferenceEntered)
                header3 of "_"
                paragraph
                    ReceivableCrossReference
