ItemCostVarianceSumForm is a SummaryForm
    title is "ItemCostVariance"
    First Page Header
        Layout
            PrintDate
                right align
            header3 of "ItemCostVariance"
                center
    Layout
        visible when (PrintParameters)
            two column
                column1
                    SupplyManagementReportVendorGroupField.Vendor
                    paragraph
                        FromDate
                            label is "FromP\\O_\\Date"
                            display when blank
                        ThruDate
                            label is "ThroughP\\O_\\Date"
                            display when blank
                    AbovePercentageVariance
                        label is "AbovePercentVariance"
                        display when blank
                    AboveAmountVariance
                        label is "AboveAmountVariance"
                        display when blank
                    IncludeOnlyPurchasesWithVendorAgreement
                        label is "IncludeOnlyPurchasesWithVendorAgreement"
                        display when blank
                column1
                    Item
                        label is "Item"
                        display when blank
                    paragraph
                        ManufacturerCode
                            label is "ManufacturerCode"
                            display when blank
                        ManufacturerDivision
                            label is "ManufacturerDivision"
                            display when blank
                    CommodityCode
                        label is "CommodityCode"
                        display when blank
                    paragraph
                        PurchasingMajorClass
                            label is "MajorPurchasingClass"
                            show up to 10 characters
                            display when blank
                        PurchasingMinorClass
                            label is "MinorPurchasingClass"
                            show up to 10 characters
                            display when blank
                    UNSPSCCode
                        label is "U\\N\\S\\P\\S\\C_\\Segment"
                        compact format
                        display when blank
                    IncludeDetails
                        label is "IncludeDetail"
                        display when blank
                    PrintParameters
                        label is "PrintParameters"
                        display when blank
        form is VendorsForItemCostVarianceRel.ItemCostVarianceSumForm
