OneSourceGenericSetupForm is a Form
    title is "OneSourceSetup"
    Layout
        two column table
            column1
                single column
                    header2 of "IndataParameters"
                    HostSystem
                    CallingSystem
                    ExternalCompanyId
                    ExternalCompanyIdName
                    UseAsLegalEntity
                        label is "UseAs_\\External_\\Company_\\Id"
                    CompanyRole
                    header2 of "TaxAttribute"
                    TaxAttributeCode
                    header2 of "AdditionalOptions"
                    TaxAccountForSalesInvoices
            column2
                single column
                    header2 of "OneSourceGroupParameters"
                    FilterGroupOwner
                    FilterGroupName
                    ProductMappingGroupOwner
                    ProductMappingGroupName
