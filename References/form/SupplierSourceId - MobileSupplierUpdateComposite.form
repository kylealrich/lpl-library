MobileSupplierUpdateComposite is a CompositeForm
    title is "Supplier_-<Supplier.SupplierName>"
    context form is MobileSupplierContext
    restrict action Delete
    restrict action CreateVendorLocation
    restrict action Create
    restrict action NewContact
    restrict action BuyerCreateContacts
    restrict action SupplierUpdate
    restrict action Delete
    restrict action AttachToEvent
    restrict action ActivateSupplierContact
    restrict action InactivateSupplierContact
    restrict action InactivateContact
    restrict action AttachToEventAsPlanHolder
    restrict action UpdateSupplierPortalInformation
    restrict action UpdateContactBankInformation
    restrict action UpdateSupplierBankInformation
    Actions
        action is save
            label is "_"
            hide when invalid
        action is Delete
            hide when invalid
    Profile is a Panel
        form is MobileProfileMainForm
    Supplier is a Panel
        form is MobileSupplierDetails
    Banking is a DashBoardPanel
        SupplierBank is a Panel
            visible when (SupplierGroup.AllowBankInformation)
            overview navigation is SupplierBankInformationNav
            form is MobileSupplierBankForm
        MyBank is a Panel
            visible when (CanDisplayBankInformationPortalContact)
            overview navigation is BankInformationNav
            form is MobileBankInformationForm
        ContactBankInformation is a Panel
            visible when (CanDisplayBankInformationIsPrimaryPortalContact)
            list is Supplier.SupplierSourceId set.MobileSupplierContactBankCardList
    Contacts is a Panel
        list is Supplier.SupplierSourceId set.MobileSupplierContactCardList
    CommodityCodes is a Panel
        list is SupplierCommodityCode set.MobileSupplierCommodityCodeUpdateCardList
            helper list is CommodityCodes.MobileCommodityCodeHelper
                action is AttachToSupplierContact
    Certifications is a Panel
        visible when (CanDisplayAllCertificationsPortalContact)
        list is Supplier.AllSupplierCertificationsRel.MobileSupplierSideCertifications
    Questions is a DashBoardPanel
        Questions is a Panel
            overview navigation is SupplierAllQuestions
            list is Supplier.SupplierQuestionResponseRels.MobileSupplierQuestionsCardList
    DiversityCodes is a Panel
        visible when (PrimaryDiversityResponsePortalContact)
        list is Supplier.SupplierDiversityResponse set.MobileDiversityResponseUpdateCardList
    ProxyNotifications is a Panel
        visible when (PortalContact)
        list is SupplierContactProxy set.MobileSupplierContactProxyList
