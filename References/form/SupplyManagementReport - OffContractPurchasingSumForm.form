OffContractPurchasingSumForm is a SummaryForm
    title is "OffContractPurchasing"
    First Page Header
        Layout
            PrintDate
                right align
            header3 of "OffContractPurchasing"
                center
    Layout
        visible when (PrintParameters)
            header3 of "OffContractPurchasing"
                center
            single column
                ItemGroup
                Item
                    display when blank
                TransientManufacturer
                    label is "Manufacturer"
                    compact format
                    display when blank
                CommodityCode
                    label is "CommodityCode"
                    compact format
                    display when blank
            two column
                column1
                    TransientMajorPurchasingClass
                        label is "MajorPurchasingClass"
                        show up to 10 characters
                        display when blank
                column2
                    TransientMinorPurchasingClass
                        label is "MinorPurchasingClass"
                        show up to 10 characters
                        display when blank
            single column
                UNSPSCCode
                    label is "UNSPSCCode"
                    compact format
                    display when blank
            two column
                column1
                    FromDate
                        label is "PurchaseOrderDateRange"
                        display when blank
                column2
                    ThruDate
                        label is "\\-"
                        display when blank
            single column
                PrintParameters
        form is OffContractPurchasingItemRel.OffContractPurchasingItemSumForm
