SmartReconSettings is a Form
    Layout
        header2 of "Preferences"
        three column distributed
            MatchPreference
            InvoicePreference
            DefaultInvoiceTemplate
        three column distributed
            DisplayOnly
            RestrictChargebacks
            RestrictMatchedNotReceived
        header2 of "Assignments"
        single column
            button of "ViewAllAssignments"
                link is SmartReconAssignmentsNav
                display as link
        two column distributed
            column1
                visible when (SmartReconciliationCompanySetupMissing)
                    text of "Company_or_\\Company_\\Group_\\Required"
                        header4
                        color of red
                paragraph
                    button of "<DerivedSmartReconciliationCompanyGroups>"
                        link is SmartReconCompanyGroupsNav
                        label is "CompanyGroups"
                        display as link
                    button of "<DerivedSmartReconciliationCompanies>"
                        link is SmartReconCompaniesNav
                        label is "Companies"
                        display as link
            column2
                visible when (SmartReconciliationVendorSetupMissing)
                    text of "Vendor,_\\Vendor_\\Class,_or_\\Vendor_\\Range_\\Required"
                        header4
                        color of red
                paragraph
                    button of "<DerivedSmartReconciliationVendors>"
                        link is SmartReconVendorsNav
                        label is "Vendors"
                        display as link
                    button of "<DerivedSmartReconciliationVendorClasses>"
                        link is SmartReconVendorClassesNav
                        label is "VendorClasses"
                        display as link
                    button of "<DerivedSmartReconciliationVendorRanges>"
                        link is SmartReconVendorRangesNav
                        label is "VendorRanges"
                        display as link
