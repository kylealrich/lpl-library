LineResponseSearch is a SearchForm
    title is "SearchEventLineResponses"
    Layout
        four column distributed
            NotifiedSupplier.Supplier
            NotifiedSupplier.Supplier.SupplierName
            NotifiedSupplier.SupplierSourceId
                label is "Contact"
            SourcingEvent.Buyer
        four column distributed
            Company
            SourcingEvent
                label is "Event"
                select is EventSearchList
            SourcingEventLine
            SourcingEvent.SoleSourceEvent
        four column distributed
            ItemNumber
            VendorItem
            GlobalTradeItemNumber
            ExtendedPrice
        four column distributed
            AwardReason
                label is "Reason"
            NonAwardReasonEntered
                label is "NonAwardReason"
            NoBidReason
                select is NoBidReasonSelect
            SourcingEventLine.CommodityCode
                label is "CommodityCode"
                compact format
        four column distributed
            IsAwarded
                label is "Awarded"
            DiversityAward
                label is "AwardedToDiverseSupplier"
            EventFinalized
            AlternatesExist
