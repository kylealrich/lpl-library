RecognizeRevenue is an ActionForm
    action is RecognizeRevenue
    Layout
        single column
            paragraph
                DateRange.Begin
                    label is "PostingDateSelectionRange"
                DateRange.End
                    label is "Through"
            ThroughDate
                label is "TransactionThroughDate"
            SelectionGroup
            FundingSourceDate
            RevenueDate
            visible when (RevenueApprovalNotRequired)
                Journalize
                    label is "JournalizeImmediately"
            visible when (Journalize)
                PostingDate
            SummaryProject
                label is "<FinanceEnterpriseGroup.ProjectLabel>"
                select is ContractProjectsRel.ContractAllProjectSelect
            FundingSource
                select is ProjectContractFinanceDimension2Rel.FinanceDimension2FundingSourceSelect
            paragraph
                BillingGroup
                BillingGroup.Description
                    no label
                    display as text
