ItemInformationWorksheetResultForm is a CompositeForm
    is primary
    title is "ItemInformationWorksheetResult"
    context form is ItemInformationWorksheetResultContext
        background is theme color
    Items is a Panel
        visible when (ItemsAdded)
        list is ItemsAddedRel.AddedFromWorksheet
    VendorItems is a Panel
        visible when (VendorItemsAdded)
        list is VendorItemsAddedRel.AddedFromWorksheet
    ItemLocations is a Panel
        visible when (ItemLocationsAdded)
        list is ItemLocationsAddedRel.AddedFromWorksheet
    GTINNumbers is a Panel
        visible when (ItemGTINsAdded)
        list is ItemGTINsAddedRel.AddedFromWorksheet
    UPCCodes is a Panel
        visible when (ItemUPCsAdded)
        list is ItemUPCsAddedRel.AddedFromWorksheet
    SKUCodes is a Panel
        visible when (ItemSKUsAdded)
        list is ItemSKUsAddedRel.AddedFromWorksheet
    NationalDrugCodes is a Panel
        visible when (NationalDrugCodesAdded)
        list is NationalDrugCodesAddedRel.AddedFromWorksheet
    UCCEANItemUPNCodes is a Panel
        visible when (UCCEANItemUPNCodesAdded)
        list is UCCEANItemUPNCodesAddedRel.AddedFromWorksheet
    HIBCCItemUPNCodes is a Panel
        visible when (HIBCCItemUPNCodesAdded)
        list is HIBCCItemUPNCodesAddedRel.AddedFromWorksheet
    ContractLinesUpdated is a Panel
        visible when (ContractLinesFound)
        list is ContractLineRel.AddedFromWorksheet
