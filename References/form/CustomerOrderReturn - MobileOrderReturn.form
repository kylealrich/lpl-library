MobileOrderReturn is a CompositeForm
    title is "CustomerOrderReturn"
    context form is MobileReturnContext
    show panel control on left
    restrict action Entered.CopyReturn
    restrict action Entered.Create
    restrict action Entered.Delete
    Actions
        action is save
            label is "_"
        action is Entered.Delete
            hide when invalid
    Main is a Panel
        form is Main
    Lines is a Panel
        list is CustomerOrderReturnLinesRel.MobileReturnLines
    AddOnCharges is a Panel
        list is CustomerOrderReturnLineAddOnChargesRel.MobileReturnHeaderAddOnCharges
    Attachments is a Panel
        list is CustomerOrderReturnAttachment set.MobileCOReturnAttachmentList
