TaxLogSearch2 is a SearchForm
    title is "TaxLogSearchForFixedBaseAmounts"
    Layout
        four column distributed
            column1
                CreateDateRange.Begin
                    label is "FromCreateDate"
                LogTaxEntity
                    label is "TaxEntity"
            column2
                CreateDateRange.End
                    label is "ToCreateDate"
                SystemCode
            column3
                CreatedBy
                LogInvoice
                    label is "Invoice"
            column4
                TaxLogHeader
                LogCreatedTimestamp
                    label is "Timestamp"
