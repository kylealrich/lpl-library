AddProcedureToRequisition is an ActionForm
    action is AddProcedureToRequisition
    Layout
        display as text
            two column
                PrmCompany
                PrmRequisition
            single column
                Contract.RepresentativeText
                    label is "Contract"
            two column
                ItemDescription
                VendorItem
                UOM
        visible when (PrimaryOrRevisionProcedure)
            paragraph
                text of "ProcedureLevel:"
                PrmPrimaryProcedureLevel
                text of "or"
                PrmRevisionProcedureLevel
        visible when (PrimaryProcedure)
            single column
                text of "ProcedureIsPrimary"
                    bold
        visible when (RevisionProcedure)
            single column
                text of "ProcedureIsRevision"
                    bold
        paragraph
            PrmQuantity
            text of "QuantityWillDefaultTo1IfNotEntered"
        single column
            PrmNoChargeProcedure
