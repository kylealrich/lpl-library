PhoneGeneralLedgerJournalControl is a CompositeForm
    title is "JournalControl"
    context form is PhoneGeneralLedgerJournalControlForm
    restrict action Unreleased.Create
    TransactionPanel is a Panel
        title is "Transactions"
        list is GeneralLedgerTransaction set.PhoneGeneralLedgerJournalTransactionList
    CommentsPanel is a Panel
        title is "Comments"
        list is GeneralLedgerJournalComment set.Comment
    RelatedDocumentsPanel is a Panel
        title is "RelatedDocuments"
        list is GeneralLedgerJournalDocument set.GeneraLedgerJournalDocuments
