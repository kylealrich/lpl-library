BuyerSearchSuppliers is a SearchForm
    title is "SearchSuppliers"
    Layout
        four column distributed
            Supplier
            SupplierName
            SupplierStatus
                label is "Status"
            Vendor
        four column distributed
            ActiveFlagMismatch
            CreatedByBuyer
                label is "InternallyCreated"
            PayablesDiversityCode
                label is "DiversityCode"
            GlobalLocationNumber
        header3 of "ShowMore"
            collapsible
                initially collapsed
                use show more or less affordance
            four column distributed
                MailingAddress.Municipality
                    label is "City"
                MailingAddress.StateProvince
                    label is "State"
                MailingAddress.PostalCode
                first SupplierSourceIdRel.MainContact.FamilyName
                    label is "PrimaryContactLastName"
