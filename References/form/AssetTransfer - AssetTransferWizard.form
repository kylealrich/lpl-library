AssetTransferWizard is a WizardForm
    use for action Create
    title is "AssetTransfer"
    summary form is TransferWizardSummary
    show steps
    TransferDetail is a Panel
        mouse over text is "EnterTransferDetails"
        form is AssetTransferOptions
    Items is a Panel
        valid when (PartialTransfer)
        mouse over text is "EnterItemTransferDetails"
        list is AssetItemTransfer set.AssetItemTransferWizard
            helper list is AssetItemRel.AssetTransferItems
                action is TransferItem
    Funds is a Panel
        valid when (FundOverride)
        mouse over text is "UpdateFundDetails"
        list is AssetTransferFund set.AssetFundList
    ItemFunds is a Panel
        valid when (ItemFundAssignment)
        mouse over text is "UpdateFundDetail"
        list is AssetItemTransferFundsRel.AssetItemFunds
    Books is a Panel
        valid when (IsInterCompanyTransfer)
        mouse over text is "EnterBookTransferDetails"
        list is AssetDepreciationBookTransfersRel.AssetBookTransfersWizard
    Transactions is a Panel
        list is TransferToAssetTransactionsRel.TransferWizardAssetTransactionList
    BookTransactions is a Panel
        valid when (AssetBookTransferExists)
        list is AssetBookTransactionsRel.TransferWizardAssetBookTransactionList
