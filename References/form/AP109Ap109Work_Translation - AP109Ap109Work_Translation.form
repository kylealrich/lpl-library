AP109Ap109Work_Translation is a Form
    is primary
    Layout
        paragraph
            Base_BusinessClass.UniqueID
                display as text
        single column
            IsoLocale
                compact format
                raise yellow alert when (VerifyTranslations)
                    mouse over text is untranslatable:"The base data has been changed but this translation has not been verified"
        single column
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.VendorVname
                label is untranslatable:"Vendor Vname"
            VendorVname
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.VendorSname
                label is untranslatable:"Vendor Sname"
            VendorSname
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.VloVendorVname
                label is untranslatable:"Vlo Vendor Vname"
            VloVendorVname
                label is untranslatable:"Translated"
