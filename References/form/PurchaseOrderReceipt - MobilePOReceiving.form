MobilePOReceiving is a CompositeForm
    title is "ReceiptDetails"
    context form is MobilePOReceivingContext
        background is theme color
    restrict action Unreleased.ReleaseAndPrint
    restrict action Unreleased.ReleaseAndPrintNoBackorders
    restrict action Unreleased.MobileCreateReceiptWizard
    restrict action MobileReleaseFromWizard
    Actions
        action is save
            label is "_"
            hide when invalid
        navigation is UnreleasedBuyerMessages
            hide when invalid
    Main is a Panel
        form is MobileMainPOReceiving
    PurchaseOrderLines1 is a Panel
        title is "PurchaseOrderLiness"
        visible when (AllExtraColumns)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is MobilePurchaseOrderLines1
    PurchaseOrderLines2 is a Panel
        title is "PurchaseOrderLines"
        visible when (BinColumn)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is MobilePurchaseOrderLines2
    PurchaseOrderLines3 is a Panel
        title is "PurchaseOrderLines"
        visible when (RemainingColumn)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is MobilePurchaseOrderLines3
    PurchaseOrderLines4 is a Panel
        title is "PurchaseOrderLines"
        visible when (CatchWeightColumn)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is MobilePurchaseOrderLines4
    PurchaseOrderLines5 is a Panel
        title is "PurchaseOrderLines"
        visible when (CatchWeightAndBinTracked)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is MobilePurchaseOrderLines5
    PurchaseOrderLines6 is a Panel
        title is "PurchaseOrderLines"
        visible when (RemainingAndBinTracked)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is MobilePurchaseOrderLines6
    PurchaseOrderLines7 is a Panel
        title is "PurchaseOrderLines"
        visible when (RemainingAndCatchWeight)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is MobilePurchaseOrderLines7
    PurchaseOrderLines8 is a Panel
        title is "PurchaseOrderLines"
        visible when (NoExtraColumns)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is MobilePurchaseOrderLines8
    Lines is a Panel
        title is "ReceiptLines<ReceiptLinesIndicator>"
        list is PurchaseOrderReceiptLine set.MobilePOReceiptLineTemplateList
    Receiver is a Panel
        form is MobileReceiver
    Messages is a Panel
        valid when (HasPOReceiptMessage)
        form is MobileMessages
    Invoice is a Panel
        visible when (DisplayInvoiceAOCHdr)
        form is Invoice
    InvoiceAddOnCost is a Panel
        visible when (DisplayInvoiceAOCHdr)
        list is HeaderReceiptAddOnChargesRel.MobilePurchaseOrderReceiptLineAOCList
    Comments is a Panel
        title is "Comments<CommentCount>"
        list is PurchaseOrderReceiptComment set.MobilePurchaseOrderReceiptCommentList
