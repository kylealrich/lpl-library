UnansweredQuestions is a CompositeForm
    title is "UnansweredRequiredQuestions"
    Actions
        action is save and close
        action is refresh
            label is "_"
    UnansweredRequiredQuestions is a Panel
        visible when (not QuestionsExist)
        mouse over text is "<SupplierGroup.QuestionsText>"
        panel header is "<SupplierGroup.QuestionsText>"
        template is SupplierGroup.SupplierGroupRequiredQuestionsByDisplayOrder
            business class is SupplierQuestionResponse
                form is SupplierQResponseRequiredUnanswered
    UnansweredRequiredQuestionsConditional is a Panel
        visible when (QuestionsExist)
        mouse over text is "<SupplierGroup.QuestionsText>"
        panel header is "<SupplierGroup.QuestionsText>"
        template is RequiredQuestionsConditional
            business class is SupplierQuestionsSetResponse
                form is SupplierQResponseRequiredUnanswered
