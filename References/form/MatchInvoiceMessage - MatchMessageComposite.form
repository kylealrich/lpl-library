MatchMessageComposite is a CompositeForm
    title is "<DerivedFormTitle>"
    context form is MatchMessageContext
        background is theme color
    Actions
        action is ReleasePOCostMessage
            action icon is release
            label is "Release"
            hide when invalid
        action is ReleaseInvoiceMessage
            action icon is release
            label is "Release"
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
    Main is a Panel
        form is Main
    Comments is a Panel
        title is "Comments<DerivedCommentIndicator>"
        list is MatchInvoiceMessageComment set.MessageCommentsPanel
    ReceiptLines is a Panel
        title is "InvoicesAndReceipts<ReceiptIndicator>"
        visible when (ReconMatchHasPurchaseOrderLine)
        list is ReceiptLinkTransactionsRel.MatchInvoiceMessageList
    OtherMessages is a Panel
        title is "OtherMessages<OtherMessagesIndicator>"
        visible when (HasCostMessagesForOtherInvoices)
        list is CostMessagesForOtherInvoicesRel.OtherMessages
