CreateContract is an ActionForm
    action is CreateContractFromInterfacedRecords
    Layout
        display as text
            paragraph
                ContractGroup
                    select is ContractGroups
                ContractGroup.BusinessGroup.Description
                    no label
                    display as text
            paragraph
                ContractImport
                Description
        visible when (not CanDefaultAccountToDistributor)
            visible when (not NoDistributorLines)
                single column
                    text of "PressSubmitToCreateAContract"
        visible when (CanDefaultAccountToDistributor)
            single column
                ParmAccountToDistContract
                    label is "LoadDefaultAccountInformationToDistributorContract"
        visible when (ParmAccountToDistContract)
            single column
                ParmSpecialItemsAccountDefaultOnly
                    label is "DefaultAccountInformationForSpecialItemsOnly"
        visible when (NoDistributorLines)
            single column
                ParmDoNotCreateDistributorLines
                    label is "DistributorContractLinesWillNotAutomaticallyBeCreatedUnlessBoxIsUnchecked"
