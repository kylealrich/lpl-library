HealthcareInformation is a Form
    Layout
        visible when (ItemNumberExists)
            display as text
                three column distributed
                    PatientChargeable
                    ChargeNumber
                    paragraph
                        GMDNPTCode
                        GMDNPTCode.Name
                            no label
                            display as text
                        visible when (GMDNTermEntered)
                            button of "TermCategories"
                                link is GMDNCategoryNav
                                display as link
                three column distributed
                    NdcNbr
                        label is "NationalDrugCode"
                    HibccUpn
                    LowUOMCodeHibccUPN
                        label is "UnitOfUseHIBCC-DI"
                three column distributed
                    paragraph
                        HCPCSCode
                            label is "HCPCSAndSequence"
                            compact format
                        HCPCSCode.ShortDescription
                            no label
                            display as text
                    visible when (HCPCSCodeEntered)
                        paragraph
                            HCPCSCode.HCPCS
                                display as text
                            visible when (ItemNumber.HSPCSCodeSequenceEnteredExists)
                                HCPCSCode.AlphaHCPCSSequence
                                    label is "HCPCSSequence"
                                    display as text
                            visible when (ItemNumber.HSPCSCodeSequenceEnteredNotExists)
                                ItemNumber.ItemAlphaHCPCSSequence
                                    label is "HCPCSSequence"
                                    display as text
                    visible when (not HasVendorItem)
                        LabelerIdentificationCode
                    visible when (HasVendorItem)
                        VendorItem.LabelerIdentificationCode
                            display as text
                    ClinicalSystemUse
                three column distributed
                    Drugs
                    Implantable
                    Instrument
                three column distributed
                    Narcotic
                    ContainsHumanTissue
        visible when (not ItemNumberExists)
            three column distributed
                PatientChargeable
                ChargeNumber
                paragraph
                    GMDNPTCode
                        select is ActiveGMDNPTCodes
                    GMDNPTCode.Name
                        no label
                        display as text
                    visible when (GMDNTermEntered)
                        button of "TermCategories"
                            link is GMDNCategoryNav
                            display as link
            three column distributed
                NdcNbr
                    label is "NationalDrugCode"
                HibccUpn
                    select is HIBCCDIForAnItem
                visible when (SpecialItemLowUOMCodeEntered)
                    paragraph
                        LowUOMCodeHibccUPN
                            label is "UnitOfUseHIBCC-DI"
                        LowUOMCode.UnitOfMeasure
                            label is "UnitOfUseUOM"
            three column distributed
                paragraph
                    HCPCSCode
                        label is "HCPCSAndSequence"
                        compact format
                    HCPCSCode.ShortDescription
                        no label
                        display as text
                visible when (HCPCSCodeEntered)
                    paragraph
                        HCPCSCode.HCPCS
                            display as text
                        visible when (ItemNumber.HSPCSCodeSequenceEntered)
                            HCPCSCode.AlphaHCPCSSequence
                                label is "HCPCSSequence"
                                display as text
                visible when (not HasVendorItem)
                    LabelerIdentificationCode
                visible when (HasVendorItem)
                    VendorItem.LabelerIdentificationCode
                        display as text
                ClinicalSystemUse
            three column distributed
                Implantable
                Drugs
                Instrument
            three column distributed
                Narcotic
                ContainsHumanTissue
