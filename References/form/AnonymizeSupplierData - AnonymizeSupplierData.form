AnonymizeSupplierData is a Form
    is primary
    Layout
        header2 of "SelectRecordTypeToAnonymize(SelectOnlyOne)"
        single column
            AnonymizeSupplier
                label is "Supplier"
            AnonymizeSupplierContact
                label is "SupplierContact"
        blank line
        header2 of "Parameters"
        paragraph
            SupplierGroup
            SupplierGroup.Description
                no label
                display as text
        paragraph
            Supplier
                select is Suppliers
            Supplier.SupplierName
                no label
                display as text
        paragraph
            visible when (AnonymizeSupplierContact)
                SupplierSourceId
                    label is "SupplierContact"
                SupplierSourceId.MainContact.GivenName
                    no label
                    display as text
                SupplierSourceId.MainContact.FamilyName
                    no label
                    display as text
