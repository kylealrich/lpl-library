IntrastatReleased is a Form
    Layout
        display as text
            two column distributed
                column1
                    paragraph
                        OriginIntrastatCountry
                            label is "ShipFromCountry/\\Jurisdiction"
                        OriginIntrastatCountry.Name
                            no label
                    paragraph
                        DestinationIntrastatCountry
                            label is "ShipToCountry/\\Jurisdiction"
                        DestinationIntrastatCountry.Name
                            no label
                    paragraph
                        OriginIntrastatRegion
                            label is "ShipFromRegion"
                        OriginIntrastatRegion.Name
                            no label
                    paragraph
                        DestinationIntrastatRegion
                            label is "ShipToRegion"
                        DestinationIntrastatRegion.Name
                            no label
                    paragraph
                        ShipTerm
                            label is "ShipTerms"
                            select is ShipTerms
                        ShipTerm.Description
                            no label
                column2
                    paragraph
                        TransportMode
                            label is "ShipVia"
                    paragraph
                        IntrastatStatisticalProcedure
                            label is "StatisticalProcedure"
                        IntrastatStatisticalProcedure.Description
                            no label
                    paragraph
                        DropShipIndicator
                            label is "Dropship"
                    paragraph
                        IntrastatPortCode
                            label is "Port"
                        IntrastatPortCode.Description
                            no label
                    paragraph
                        IntrastatNatureOfTransactionCode
                            label is "NatureOfTransactionCode"
                        IntrastatNatureOfTransactionCode.Description
                            no label
