MobileCreate is an ActionForm
    action is Unreleased.MobileCreate
        on completion navigate to MobileRequesterShopping
    use for smartphone
    title is "CreateRequisition"
    Layout
        paragraph
            PrmCompany
                select is MobileInventoryCompanySelect
                initial value is actor.agent(Employee).Employee.Requester.Company
            PrmRequestingLocation
                label is "RequestingLocation"
                select is MobileActiveRequestingLocationsSelect
                initial value is actor.agent(Employee).Employee.Requester.RequestingLocation
            PrmFromCompanyLocation.FromCompany
                label is "FromCompany"
                select is MobileInventoryCompanySelect
                initial value is actor.agent(Employee).Employee.Requester.RequestingLocation.FromCompanyLocation.FromCompany
            PrmFromCompanyLocation.FromLocation
                label is "FromLocation"
                select is MobileInventoryLocations
                initial value is actor.agent(Employee).Employee.Requester.RequestingLocation.FromCompanyLocation.FromLocation
            PrmReferenceNumber
                label is "ReferenceNumber"
            PrmRequisitionDescription
                label is "Description"
