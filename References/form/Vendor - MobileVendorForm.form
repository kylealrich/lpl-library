MobileVendorForm is a CompositeForm
    title is "<DerivedFormTitle>"
    context form is MobileVendorContext
    restrict action ReleaseToARInterface
    restrict action LinkWithARCustomer
    restrict action Create
    restrict action CreateOneTimeVendor
    restrict action RequestNewVendor
    restrict action Delete
    Actions
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is refresh
            label is "_"
    Main is a Panel
        form is MobileMain
    Options is a Panel
        form is MobileOptions
    Balances is a Panel
        list is AllVendorBalanceMasterRel.MobileVendorLocationBalanceList
    Contacts is a Panel
        list is VendorContactRel.MobileVendorContactsList
    PayVendors is a Panel
        list is PayToVendor set.MobilePayToVendorCardList
    CompanyDefaults is a Panel
        list is VendorCompanyDefaultRel.MobileVendorCompanyDefaultCodes
    Locations is a Panel
        title is "Locations<DerivedLocationCount>"
        list is VendorLocation set.MobileVendorLocationsPanelList
    Global is a Panel
        form is Global
