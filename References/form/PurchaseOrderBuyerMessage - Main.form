Main is a Form
    Actions
        action is Unreleased.Release
            hide when invalid
        action is Unreleased.ReleaseAndCreateVendorReturn
            hide when invalid
        action is Unreleased.Update
    Layout
        single column
            paragraph
                visible when (Status.Unreleased)
                    TransientUnitCost
                visible when (Status.Released)
                    TransientUnitCost
                        display as text
                visible when (VendorUnitCostMessage)
                    NoVendorCost
                        no label
                        display as text
        header2 of "Receipt_/Adjustment_inProcess"
        display as text
            five column distributed
                paragraph
                    ReceiptQuantity
                        label is "ReceiptQuantity"
                    PurchaseOrderReceiptLine.ReceivedUOM
                        no label
                visible when (PurchaseOrderReceiptLine.IsCatchWeightItem)
                    paragraph
                        CatchWeightReceiptQuantity
                        PurchaseOrderReceiptLine.Item.StockUOM
                            no label
                paragraph
                    ExcessQuantityDisplay
                        label is "ExcessQuantity"
                    PurchaseOrderReceiptLine.ReceivedUOM
                        no label
                paragraph
                    PurchaseOrderLineRel.EnteredUnitCost
                        label is "UnitCost"
                    PurchaseOrderLineRel.VendorPriceUOM
                        no label
                PurchaseOrderReceiptLine.InspectionRequired
        header2 of "ProcessedQuantities"
        display as text
            five column distributed
                paragraph
                    PurchaseOrderLineRel.Quantity
                        label is "Ordered"
                    PurchaseOrderLineRel.EnteredBuyUOM
                        no label
                PurchaseOrderLineRel.CancelQuantity
                    label is "Canceled"
                PurchaseOrderLineRel.ReceivedQuantity
                    label is "Received"
                ReturnQuantity
                OpenQuantity
                    label is "Open"
        header2 of "ReturnToVendor"
        paragraph
            visible when (Status.Unreleased)
                ReturnQuantity
            visible when (Status.Released)
                ReturnQuantity
                    display as text
            PurchaseOrderReceiptLine.ReceivedUOM
                no label
                display as text
        visible when (Status.Released)
            display as text
                five column distributed
                    column1
                        PurchaseOrderReceiptLine.VendorReturn.VendorClaimType
                            label is "ClaimType"
                    column2
                        PurchaseOrderReceiptLine.VendorReturn.ShipOrHold
                            label is "ShipOrHold"
                    column3
                        PurchaseOrderReceiptLine.VendorReturn.ReferenceNumber
                            label is "ReferenceNumber"
        header2 of "InventoryQuantities"
        display as text
            five column distributed
                column1
                    paragraph
                        ItemLocationRel.DerivedStockOnHandQuantity
                            label is "CurrentStockOnHand"
                        PurchaseOrderLineRel.DerivedStockUOM
                            no label
                column2
                    DerivedReceivedStockQuantity
                        label is "ReceivedStock"
                column3
                    DerivedNewStockOnHandQuantity
                        label is "AfterStockOnHand"
