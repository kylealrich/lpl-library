MobileAssetTransferForm is a CompositeForm
    use for action MobileAssetTransfer
    title is "AssetTransfer"
    called out actions only
    show panel navigation
        last panel navigation is MAssetLink
            next button title is "ViewAsset"
    Actions
        action is save
            label is "_"
    Main is a Panel
        form is MobileAssetTransferOptions
    Items is a Panel
        valid when (PartialTransfer)
        mouse over text is "EnterItemTransferDetails"
        list is AssetItemTransfer set.MobileAssetItemTransfer
            helper list is AssetItemRel.MAssetTransferItems
                action is TransferItem
    Books is a Panel
        valid when (IsInterCompanyTransfer)
        mouse over text is "EnterBookTransferDetails"
        list is AssetDepreciationBookTransfersRel.MobileAssetBookTransfers
    Transactions is a Panel
        list is TransferToAssetTransactionsRel.MAssetTransactionList
    BookTransactions is a Panel
        list is AssetBookTransactionsRel.MobileAssetBookTransactions
    Summary is a Panel
        form is MobileTransferWizardSummary
