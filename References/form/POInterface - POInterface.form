POInterface is an ActionForm
    action is POInterface
    title is "ProcessPurchaseRequests"
    Layout
        header2 of "Main"
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            Company.ProcurementGroup
                label is "ProcurementGroup"
                display as text
            ProcessByProcurementGroup
                label is "ProcessAllCompaniesInProcurementGroup"
        paragraph
            DefBuyerCode
                label is "DefaultBuyer"
            DefBuyerCode.Employee.PresentationNameSnapshot
                no label
                display as text
        single column
            InclCstOpt23
                label is "IncludeAllCostOptions"
        two column distributed
            DefDlDays
                label is "DefaultDeliveryDays"
            ReleasePo
                label is "ReleasePurchaseOrders"
        two column distributed
            PrtPoCmts
                label is "PrintPOComments"
            CombineLine
                label is "Combine_onPOLine"
        single column
            ReportLevel
        two column distributed
            MaxLines
                label is "MaximumNumber_ofLines_onPO"
            OneShipToPerPO
                label is "OneShipTo_perPO"
        two column distributed
            PrtByStat
                label is "PrintByPOStatus"
            ExceptRpt
                label is "PrintExceptionReport"
        two column distributed
            SystemCode
        single column
            InclExclVendFl
                label is "VendorOption"
        two column table
            column headers
                "Vendor"
                "Purchase_From"
            column1
                Vendor.PO100VendorGroup[1].Vendor
                    no label
                Vendor.PO100VendorGroup[2].Vendor
                    no label
                Vendor.PO100VendorGroup[3].Vendor
                    no label
                Vendor.PO100VendorGroup[4].Vendor
                    no label
            column2
                Vendor.PO100VendorGroup[1].PurchFrLoc
                    no label
                Vendor.PO100VendorGroup[2].PurchFrLoc
                    no label
                Vendor.PO100VendorGroup[3].PurchFrLoc
                    no label
                Vendor.PO100VendorGroup[4].PurchFrLoc
                    no label
        header2 of "Filters"
        paragraph
            FilBuyerCode
                label is "Buyer"
            FilBuyerCode.Employee.PresentationNameSnapshot
                no label
                display as text
        four column distributed
            BuyPurchCl
                label is "BuyerPurchaseClasses"
            BuyVenFl
                label is "Vendors"
            BuyLocFl
                label is "Locations"
            BuyRqlFl
                label is "ReqLocations"
        four column distributed
            DelDays
                label is "DeliveryDays"
            FilFrmPri
                label is "PriorityRangeFrom"
            FilToPri
                label is "PriorityRangeTo"
        paragraph
            FilLocation
                label is "ShipToLocation"
            FilLocation.NameForPurchaseOrder
                no label
                display as text
            text of "\\or"
                bold
            FilReportGrp
                label is "ReportGroup"
            FilReportGrp.Name
                no label
                display as text
        single column
            InclExclDeliveryMethod
                label is "DeliveryMethodOption"
        paragraph
            FilDeliveryMethod.DeliveryMethod[1]
                label is "DeliveryMethod"
            FilDeliveryMethod.DeliveryMethod[2]
                no label
            FilDeliveryMethod.DeliveryMethod[3]
                no label
            FilDeliveryMethod.DeliveryMethod[4]
                no label
        single column
            blank line
        single column
            InclExclPocode
                label is "POCodeOption"
        paragraph
            FilPoCode1.POCode[1]
                label is "POCode"
            FilPoCode1.POCode[2]
                no label
            FilPoCode1.POCode[3]
                no label
            FilPoCode1.POCode[4]
                no label
        single column
            blank line
        paragraph
            FilPurchMajcl
                label is "MajorPurchasingClass"
                show up to 10 characters
            FilPurchMincl
                label is "MinorPurchasingClass"
                show up to 10 characters
        paragraph
            FilReqLocation
                label is "RequestingLocation"
            FilReqLocation.Name
                no label
                display as text
            text of "\\or"
            FilReqLocationGroup
                label is "RequestingLocationGroup"
            FilReqLocationGroup.Name
                no label
                display as text
        paragraph
            FilItem
                label is "Item"
            FilItemType
                label is "ItemType"
        paragraph
            FilManufCode
                label is "Manufacturer"
                compact format
            CostCode
                label is "UnitCostOrigin"
            blank line
        paragraph
            FilReqNumber
                label is "RequisitionNumber"
            text of "\\or"
                bold
            FilTempltDocNbr
                label is "ProcurementTemplateNumber"
        single column
            blank line
            InclExclCncode
                label is "BillCodeOption"
        paragraph
            FilCnCode.PO100FilCnCode[1]
                label is "BillCode"
            FilCnCode.PO100FilCnCode[2]
                no label
        paragraph
            FilCnCode.PO100FilCnCode[3]
                no label
            FilCnCode.PO100FilCnCode[4]
                no label
        header2 of "AccountFilters"
        paragraph
            FilAcctUnit
                label is "AccountingUnit"
            FilAcctUnit.Description
                no label
                display as text
            FilActivity
                label is "Project"
            FilActivity.Description
                no label
                display as text
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                POInterfaceReport_RDG
                PurchaseOrderExceptionReport_RDG
            column2 of 25%
                POInterfaceReport_XPT
                PurchaseOrderExceptionReport_XPT
