AlternateLocationImport is a CompositeForm
    use for action CreateVendorLocationImport
    context form is AlternateLocationImportContext
        background is theme color
    Main is a Panel
        form is LocationMain
    Contact is a Panel
        form is LocationContact
    Defaults is a Panel
        form is LocationDefaults
    VendorBank is a Panel
        form is VendorBank
    PurchaseFromLocation is a Panel
        form is POVendorMain
    PurchasingOptions is a Panel
        form is POVendorOptions
    Addresses is a Panel
        list is VendorAddressImportRel.VendorAddressImportList
    Contacts is a Panel
        list is VendorContactImportRel.VendorConversionContacts
    Balances is a Panel
        list is VendorBalanceImportRel.VendorBalanceImportList
    Certifications is a Panel
        list is VendorCertificationImportRel.VendorCertificationImportList
