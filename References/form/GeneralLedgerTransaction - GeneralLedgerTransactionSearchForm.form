GeneralLedgerTransactionSearchForm is a SearchForm
    title is "SearchFinanceTransactions"
    Layout
        paragraph distributed
            FinanceCodeBlock
                label is "FinanceStructure"
            FinanceCodeBlock.GeneralLedgerChartAccount.GeneralLedgerSubAccount
                label is "SubAccount"
        four column distributed
            AccountingEntity
                label is "From<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
            TransactionAmount
            CurrencyCode
            FinanceCodeBlock.Project.ProjectStatus
                label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Status"
        four column distributed
            SearchDateRange.Begin
                label is "FromTransactionDate"
            SearchDateRange.End
                label is "ToTransactionDate"
            PostingDateRange.BeginDate
                label is "FromPostingDate"
            PostingDateRange.EndDate
                label is "ToPostingDate"
        four column distributed
            System
            GeneralLedgerEvent
                label is "Event"
            GeneralLedgerJournalControl
                label is "Journal"
            SearchGroup
        four column distributed
            SearchPeriodRange.Begin
                label is "FromPeriod"
                select is AnyClosePeriods
            SearchPeriodRange.End
                label is "ToPeriod"
                select is AnyClosePeriods
            Status
        four column distributed
            Description
            ReportCurrencyAmount.ProjectAmount.EnteredCurrencyAmount
                label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Amount"
            FinanceCodeBlock.Project.Currency
                label is "Currency"
            UnitsAmount
                label is "Units"
