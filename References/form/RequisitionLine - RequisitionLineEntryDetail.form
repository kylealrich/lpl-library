RequisitionLineEntryDetail is a CompositeForm
    is primary
    title is "<DerivedFormTitle>"
    context form is RequisitionLineContext
        background is theme color
    restrict action Processed.UpdateLineReturn
    restrict action Create
        when (not Requisition.Status.Unreleased)
    restrict action MobileCopyRequisitionLine
    restrict action ModalCopyMultipleRequisitionLines
    restrict action NonModalCopyRequisitionLine
    restrict action Unreleased.RSSLPLUpdateRequisitionLine
    restrict action RSSLPLAddNewSpecialServiceToNewRequisition
    restrict action RSSLPLAddNewSpecialServiceToRequisition
    restrict action ModalCopyMultipleRequisitionLinesForSpecialAndService
    restrict action NonModalCopyRequisitionLineForSpecialAndService
    restrict action ModalCopyRequisitionLineForSpecialAndService
    restrict action MobileCopyRequisitionLineForSpecialAndService
    restrict action Unreleased.RSSLPLUpdateRequisitionLineQTY
    restrict action RSSLPLModalCopyLineOptionPromptForRecentlyRequestedItems
    restrict action RSSLPLModalCopyLineOptionPromptForSpecialAndService
    Actions
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is Create
            action icon is create
            label is "_"
            hide when invalid
        action is save
            label is "_"
        action is save and new
            label is "_"
        action is Unreleased.Delete
            action icon is delete
            label is "_"
            hide when invalid
        action is refresh
            label is "_"
    Main is a Panel
        form is RequisitionLineMain
    ProcessingDetail is a Panel
        visible when (IsProcessedStatus)
        form is ProcessingQuantities
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
    ProcedureInformation is a Panel
        visible when (DisplayOfProcedureInfo)
        form is ProcedureInformationForm
    Return is a Panel
        visible when (IsReturned)
        form is RequisitionReturnForm
    AddOnCharges is a Panel
        visible when (CreatePurchaseOrder)
        list is RequisitionLineAddOnCharge set.primary
    Comments is a Panel
        title is "Comments<CommentCount>"
        list is RequisitionLineCommentRel.RequisitionLineComments
    Accessories is a Panel
        visible when (IsKitItemFromLocation)
        list is KitItemFromLocationRel.KitComponentsRel.KitAccessories
    Substitutes is a Panel
        visible when (SubstituteItemsExists)
        list is ValidSubstituteItemsRel.ItemSubstitutes
    Distributions is a Panel
        title is "Distributions<DistributionCount>"
        visible when (IsDistributionAllowed)
        list is RequisitionLineDistributionsRel.primary
    Feedback is a Panel
        title is "ReturnDetails"
        visible when (EnableReturnDetails)
        list is RequisitionLineDetail set.FeedbackList
    Errors is a Panel
        visible when (LineErrorsExist)
        list is RequisitionLineError set.RequisitionLineErrorList
