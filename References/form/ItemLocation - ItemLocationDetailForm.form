ItemLocationDetailForm is a CompositeForm
    title is "CartParLocation"
    context form is ItemLocationDetailContext
        background is theme color
    Main is a Panel
        form is ParItemForm
    ReplenishFrom is a Panel
        visible when (not RecordExists)
        form is ReplenishFrom
    Replenish is a Panel
        title is "Replenishment"
        visible when (RecordExists)
        list is ItemReplenishmentSourcesRel.ReplenishmentPriority
