CashForecastingReport is an ActionForm
    action is CashForecastingReport
    Layout
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        single column
            text of "Or"
        paragraph
            CompanyGroup
                label is "CompanyGroup"
            CompanyGroup.Description
                no label
                display as text
        single column
            PaymentSchedule
        single column
            TotalingDays
        single column
            CutoffDays
                label is "FutureCutoffDays"
        four column
            AgePeriod1
                label is "AgingPeriods"
            AgePeriod2
                no label
            AgePeriod3
                no label
            AgePeriod4
                no label
        five column
            AgePct1
                label is "AgingPercents"
            AgePct2
                no label
            AgePct3
                no label
            AgePct4
                no label
            AgePct5
                no label
        paragraph
            ProcessLevel
                select is ProcessLevels
            ProcessLevel.Name
                no label
                display as text
        single column
            CurrencyType
                label is "Currency"
        paragraph
            OrigCurrency
                label is "CurrencySelect"
            OrigCurrency.Description
                no label
                display as text
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                CashForecastingReportReport_RDG
                CashForecastingErrorReport_RDG
            column2 of 25%
                CashForecastingReportReport_XPT
                CashForecastingErrorReport_XPT
