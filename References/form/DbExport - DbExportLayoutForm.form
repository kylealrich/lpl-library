DbExportLayoutForm is a Form
    is primary
    title is "Export_fromDatabase"
    Layout
        WorkDirectory
        header3 of "Select_aBusinessClass"
        single column
            BusinessClass
                select is BusinessViewDBList
        header3 of "OutputFile"
        single column
            FileFormat
                display as horizontal radio buttons
            FileName
        header3 of "ExportOptions"
        two column
            column1
                DoNotDumpLargeObjects
                IncludeUserFields
                IncludeTranslations
                CreateTemplate
            column2
                visible when (not CreateTemplate)
                    SetName
                        select is BusinessSetList
                FilterString
        DelimitersAndPatterns
        header3 of "Fields"
        single column
            FieldNameOccurs.FieldName[1]
                select is PersistentBusinessFieldList
            FieldNameOccurs.FieldName[2]
                select is PersistentBusinessFieldList
            FieldNameOccurs.FieldName[3]
                select is PersistentBusinessFieldList
            FieldNameOccurs.FieldName[4]
                select is PersistentBusinessFieldList
            FieldNameOccurs.FieldName[5]
                select is PersistentBusinessFieldList
            FieldNameOccurs.FieldName[6]
                select is PersistentBusinessFieldList
            FieldNameOccurs.FieldName[7]
                select is PersistentBusinessFieldList
            FieldNameOccurs.FieldName[8]
                select is PersistentBusinessFieldList
            FieldNameOccurs.FieldName[9]
                select is PersistentBusinessFieldList
            FieldNameOccurs.FieldName[10]
                select is PersistentBusinessFieldList
