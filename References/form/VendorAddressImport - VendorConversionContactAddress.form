VendorConversionContactAddress is a Form
    is primary
    title is "VendorAddressInterface"
    Layout
        three column distributed
            column1
                RunGroup
                VendorMappingTableInterfaceRel.Vendor
                    label is "VendorMappingVendor"
                    display as text
                VendorAddressImport.VendorLocation
                VendorAddressImport.VendorContact.VendorContactCode
                    label is "VendorContactCode"
                RecordInError
                    display as text
                visible when (RecordInError)
                    text of "*Error:<ErrorMessage>"
                        bold
            column2
                VendorGroup
                VendorAddressImport.Vendor
                VendorAddressImport.VendorContactImport.VendorContactCode
                    label is "ImportVendorContactCode"
                VendorAddressImport.VendorContact.ContactLevel
                    label is "VendorContactLevel"
            column3
                VendorGroup.Description
                    label is "Description"
                    display as text
                VendorAddressImport.OldVendor
                VendorAddressImport.VendorContactImport.ContactLevel
                    label is "ImportVendorContactLevel"
                EffectiveDate
        blank line
        PostalAddress
