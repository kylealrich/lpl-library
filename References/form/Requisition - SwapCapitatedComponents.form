SwapCapitatedComponents is a CompositeForm
    title is "SwapCapitatedComponentsForRequisition<Requisition>"
    called out actions only
    context form is RequisitionContextSwap
        background is theme color
    Actions
        action is SwapCapitatedCharges
        navigation is RequisitionView
            label is "ReturnToRequisition"
        action is refresh
            label is "_"
    Components is a MultiListPanel
        list is RequisitionLinesRel.ComponentsToSwapList
        list is RequisitionLinesRel.CapitatedComponentsNoChargeSwapList
        list is RequisitionLinesRel.CapitatedChargedComponentsSwapList
