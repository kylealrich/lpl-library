StageToActiveSpaces is a Form
    title is "StageToActiveSpaces"
    Layout
        single column
            text of "FSM_Analytics_Version"
                bold
        two column
            column1
                text of "Staging_Environment_Version_(STC)"
                text of "Active_Environment_Version_(ATC)"
            column2
                last ProductVersionSTCRel.Value
                    label is "_"
                last ProductVersionATCRel.Value
                    label is "_"
        single column
            text of "_"
            visible when (Status.InProgress)
                button of "RefreshProductVersion"
                    action is UpdateVersion
            blank line
        single column
            blank line
            header3 of "Upgrade_Active_Environment"
            blank line
            visible when (Status.InProgress)
                button of "Start_Active_Environment_Upgrade"
                    action is StartSTCAndATCSwap
            last LatestSTCAndATCSwapRel.Status
                label is "Status"
                raise red alert when (last LatestSTCAndATCSwapRel.IsFailed)
                raise yellow alert when (last LatestSTCAndATCSwapRel.IsCanceled)
                raise blue alert when (last LatestSTCAndATCSwapRel.IsActionPendingOrActionPendingInServer)
                    mouse over text is "ActionPending"
            button of "<STCAndATCSwapWorkUnit>"
                valid when (IsSTCAndATCSwapWorkunitExists)
                link is STCandATCSwapWorkUnitNav
                label is "WorkUnit"
                display as link
            blank line
            valid when (Status.InProgress)
                display as text
                StageToActiveIsCompleted
                    label is "Please_confirm_if_Status_of_workunit_is_completed_before_proceeding_with_the_next_steps."
