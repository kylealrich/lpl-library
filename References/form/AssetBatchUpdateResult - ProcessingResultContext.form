ProcessingResultContext is a Form
    Layout
        visible when (Source.AssetPeriodClose)
            three column distributed
                column1
                    paragraph
                        visible when (IsBookEntered)
                            Book
                                display as text
                            Book.Description
                                no label
                                display as text
                        visible when (IsBookGroupEntered)
                            BookGroup
                                display as text
                            BookGroup.Description
                                no label
                                display as text
                    paragraph
                        visible when (CompanyEntered)
                            Company
                                display as text
                            Company.GeneralLedgerCompany.Name
                                no label
                                display as text
                        visible when (CompanyGroupEntered)
                            CompanyGroup
                                display as text
                            CompanyGroup.Description
                                no label
                                display as text
                    paragraph
                        visible when (AssetTransactionsExists)
                            button of "<JournalizeGroup>"
                                link is ViewJournal
                                label is "JournalizeGroup"
                                display as link
                column2
                    CreateDate
                        label is "CreationDate"
                        display as text
                    CreatedBy
                        display as text
                column3
                    Release
                        label is "Update"
                        display as text
                    Status
                        display as tag
                            color of red when (Status.NoRecordsToProcess)
                            color of green when (Status.Completed)
                            color of yellow when (Status.InProcess)
                            color of red when (Status.CompletedWithErrors)
        visible when (Source.ReleaseAssets)
            three column distributed
                column1
                    paragraph
                        visible when (CompanyEntered)
                            Company
                                display as text
                            Company.GeneralLedgerCompany.Name
                                no label
                                display as text
                        visible when (CompanyGroupEntered)
                            CompanyGroup
                                display as text
                            CompanyGroup.Description
                                no label
                                display as text
                    visible when (ReleasedPostedTransactionsExists)
                        paragraph
                            button of "<JournalizeGroup>"
                                link is ViewJournal
                                label is "JournalizeGroup"
                                display as link
                column2
                    CreateDate
                        label is "CreationDate"
                        display as text
                    CreatedBy
                        display as text
                column3
                    DerivedRelease
                        label is "Update"
                        display as text
                    Status
                        display as tag
                            color of red when (Status.NoRecordsToProcess)
                            color of green when (Status.Completed)
                            color of yellow when (Status.InProcess)
                            color of red when (Status.CompletedWithErrors)
        visible when (Source.DepreciationCalculation)
            display as text
                three column distributed
                    column1
                        paragraph
                            visible when (IsBookEntered)
                                Book
                                Book.Description
                                    no label
                                Book.Currency
                                    no label
                            visible when (IsBookGroupEntered)
                                BookGroup
                                BookGroup.Description
                                    no label
                        paragraph
                            visible when (CompanyEntered)
                                Company
                                Company.GeneralLedgerCompany.Name
                                    no label
                            visible when (CompanyGroupEntered)
                                CompanyGroup
                                CompanyGroup.Description
                                    no label
                    column2
                        CreateDate
                            label is "CreationDate"
                        CreatedBy
                    column3
                        Status
                            display as tag
                                color of green when (Status.Completed)
                                color of gray when (Status.InProcess)
                                color of red when (Status.NoRecordsToProcess)
                                color of red when (Status.CompletedWithErrors)
                        visible when (IsBookAndCompanyEntered)
                            DepreciationCalculation
                                no label
