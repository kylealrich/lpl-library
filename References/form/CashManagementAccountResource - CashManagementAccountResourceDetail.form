CashManagementAccountResourceDetail is a Form
    title is "Resource"
    Layout
        two column
            Description
            MainContact
        blank line
        header2 of "SignerInformation"
        single column
        visible when (ActiveSigner)
            text of "IsASigner"
                bold
                italics
                color of green
        visible when (not ActiveSigner)
            text of "IsNotASigner"
                bold
                italics
                color of red
        three column
            valid when (not NoLimitSigning)
                SigningAmount
            NoLimitSigning
            SigningCurrency
                no label
        single column
            Status
            SigningComment
                use text area of 3 lines
        three column
            SignerBeginDate
            SignerInactivationDate
        blank line
        header2 of "OnlineBankingInformation"
        single column
        visible when (ActiveOnlineUser)
            text of "IsAnOnlineUser"
                bold
                italics
                color of green
        visible when (not ActiveOnlineUser)
            text of "IsNotAnOnlineUser"
                bold
                italics
                color of red
        two column
            OnlineSystemAdministrator
            OnlineSecurityToken
        single column
            ManuallyInitiateFundsTransfer
        header2 of "OnlineTransferInitiation"
        single column
        visible when (ActiveTransferInitiator)
            text of "CanInitiateOnlineTransfers"
                bold
                italics
                color of green
        visible when (not ActiveTransferInitiator)
            text of "CannotInitiateOnlineTransfers"
                bold
                italics
                color of red
        three column
            column1
                text of "InitiationLimit"
                valid when (not NoLimitACHTransfer)
                    TransferAmount
                        label is "ACH"
                valid when (not NoLimitWireTransfer)
                    WireTransferAmount
                        label is "Wire"
                valid when (not NoLimitInternalFundsTransfer)
                    InternalFundsTransferAmount
                        label is "InternalFunds"
                valid when (not NoLimitCheckOutsourcingTransfer)
                    CheckOutsourcingTransferAmount
                        label is "CheckOutsourcing"
            column2
                text of "NoLimit"
                NoLimitACHTransfer
                    no label
                NoLimitWireTransfer
                    no label
                NoLimitInternalFundsTransfer
                    no label
                NoLimitCheckOutsourcingTransfer
                    no label
            column3
                text of "Currency"
                TransferCurrency
                    no label
                WireTransferCurrency
                    no label
                InternalFundsTransferCurrency
                    no label
                CheckOutsourcingTransferCurrency
                    no label
        single column
            TransferComment
                use text area of 3 lines
        three column
            TransferInitiationBeginDate
            TransferInitiationInactivationDate
        header2 of "OnlineTransferApproval"
        single column
        visible when (ActiveTransferApprover)
            text of "CanApproveOnlineTransfers"
                bold
                italics
                color of green
        visible when (not ActiveTransferApprover)
            text of "CannotApproveOnlineTransfers"
                bold
                italics
                color of red
        three column
            column1
                text of "ApprovalLimit"
                valid when (not NoLimitACHApproval)
                    ApprovalAmount
                        label is "ACH"
                valid when (not NoLimitWireApproval)
                    WireApprovalAmount
                        label is "Wire"
                valid when (not NoLimitInternalFundsApproval)
                    InternalFundsApprovalAmount
                        label is "InternalFunds"
                valid when (not NoLimitCheckOutsourcingApproval)
                    CheckOutsourcingApprovalAmount
                        label is "CheckOutsourcing"
            column2
                text of "NoLimit"
                NoLimitACHApproval
                    no label
                NoLimitWireApproval
                    no label
                NoLimitInternalFundsApproval
                    no label
                NoLimitCheckOutsourcingApproval
                    no label
                blank line
            column3
                text of "Currency"
                ApprovalCurrency
                    no label
                WireApprovalCurrency
                    no label
                InternalFundsApprovalCurrency
                    no label
                CheckOutsourcingApprovalCurrency
                    no label
                blank line
        single column
            ApprovalComment
                use text area of 3 lines
        three column
            TransferApprovalBeginDate
            TransferApprovalInactivationDate
