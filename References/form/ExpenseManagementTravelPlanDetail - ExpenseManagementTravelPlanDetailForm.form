ExpenseManagementTravelPlanDetailForm is a Form
    is primary
    Layout
        paragraph
            ExpenseManagementTravelPlan
            ExpenseManagementTravelPlan.Description
            ExpenseManagementTravelPlanDetail
                label is "SequenceNumber"
        paragraph
            PayablesCompany
            PayablesCompany.Name
                display as text
        paragraph
            Vendor
                select is VendorNumber
            Vendor.VendorName
                no label
                display as text
        blank line
        paragraph
            ExpenseDate
        blank line
        paragraph
            AccountingEntity
            AccountingEntity.Name
                no label
                display as text
            ExpenseManagementInterfaceExpenseType
                label is "ExpenseType"
            ExpenseManagementInterfaceExpenseType.Description
                no label
                display as text
        blank line
        paragraph
            RequestAmount
            Currency
                no label
        paragraph
            CurrencyTable
        paragraph
            ExpensedAmount
        paragraph
            CommittedAmount
        blank line
        CommittedReportAmount
        blank line
        paragraph
            FinanceStructure
                compact format
        paragraph
            Status
