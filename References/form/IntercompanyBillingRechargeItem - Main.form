Main is a Form
    title is "IntercompanyBillingRechargeItem"
    Layout
        three column distributed
            column1
                Taxable
                    label is "Taxable"
                paragraph
                    TaxCategory
                    TaxCategory.Description
                        no label
                        display as text
            column2
                paragraph
                    DefaultTaxCode
                    DefaultTaxCode.Description
                        no label
                        display as text
                paragraph
                    TaxUsageCode
                        label is "Usage"
                    TaxUsageCode.Description
                        no label
                        display as text
            column3
                paragraph
                    AlternatePayablesTaxCode
                    AlternatePayablesTaxCode.Description
                        no label
                        display as text
                visible when (IsHSNSACCodeEnabled)
                    paragraph
                        HSNSACCode
                        HSNSACCode.Description
                            no label
                            display as text
        header2 of "Revenue_andExpenseAccounts"
        two column
            column1
                FromReceivablePLAccount
                    label is "ReceivablesRevenueAccount"
                    compact format
                ToPayablePLAccount
                    label is "PayablesExpenseAccount"
                    compact format
            column2
                paragraph
                    ReceivablesDistributionCode
                        select is IntercompanyBillingActiveReceivablesDistCodeSelectList
                    ReceivablesDistributionCode.Description
                        no label
                        display as text
                paragraph
                    PayablesDistributionCode
                        select is IntercompanyBillingActivePayablesDistCodeSelectList
                    PayablesDistributionCode.Description
                        no label
                        display as text
