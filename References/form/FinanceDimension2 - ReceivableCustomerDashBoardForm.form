ReceivableCustomerDashBoardForm is a Form
    title is "ReceivableCustomer"
    Layout
        visible when (InEnterpriseStructure)
            single column
                CompanyDescriptionMessage
                    label is "ReceivableCompany"
                    display as text
                CustomerDescriptionMessage
                    label is "ReceivableCustomer"
                    display as text
                BillToDescriptionMessage
                    label is "BillTo"
                    display as text
                ARInterfaceOption
                    display as text
                ReceivableProcessLevelDescriptionMessage
                    label is "ReceivableProcessLevel"
                    display as text
                visible when (not CompanyCustomerGroup.ReceivableCompany.DetailedInvoiceAccrualDistributions)
                    AccountsReceivableCodeDescriptionMessage
                        label is "AccountsReceivableCode"
                        display as text
                InvoiceTypeCodeDescriptionMessage
                    label is "InvoiceTypeCode"
                    display as text
            two column distributed
                column1
                    CustomerPoNumber
                        label is "CustomerP\\ONumber"
                        display as text
                column2
                    LetterOfCredit
                        display as text
            single column
                DocumentNumber
                    display as text
        visible when (not InEnterpriseStructure)
            single column
                CompanyDescriptionMessage
                    label is "ReceivableCompany"
                    display as text
                CustomerDescriptionMessage
                    label is "ReceivableCustomer"
                    display as text
                BillToDescriptionMessage
                    label is "BillTo"
                    display as text
                ARInterfaceOption
                    display as text
                ReceivableProcessLevelDescriptionMessage
                    label is "ReceivableProcessLevel"
                    display as text
                visible when (not CompanyCustomerGroup.ReceivableCompany.DetailedInvoiceAccrualDistributions)
                    AccountsReceivableCodeDescriptionMessage
                        label is "AccountsReceivableCode"
                        display as text
                InvoiceTypeCodeDescriptionMessage
                    label is "InvoiceTypeCode"
                    display as text
            two column distributed
                column1
                    CustomerPoNumber
                        label is "CustomerP\\ONumber"
                        display as text
                column2
                    LetterOfCredit
                        display as text
            single column
                DocumentNumber
                    display as text
