RSSCreateRequisition is an ActionForm
    action is CreateRequisition
        on completion navigate to RequesterRequisitionsView
    use for browser
    title is "CreateRequisition"
    Layout
        paragraph
            PrmCompany
                label is "Company"
            PrmRequestingLocation
                label is "RequestingLocation"
        paragraph
            PrmFromCompanyLocation.FromCompany
                label is "FromCompany"
            PrmFromCompanyLocation.FromCompany.Name
                no label
                display as text
            PrmFromCompanyLocation.FromLocation
                label is "FromLocation"
            PrmFromCompanyLocation.FromLocation.Name
                no label
                display as text
        paragraph
            PrmRequestedDeliveryDate
                label is "RequestedDeliveryDate"
            PrmDeliverTo
                label is "DeliverTo"
            PrmReqAllocationPriority
                label is "RequisitionPriority"
            PrmAllocationPriority
                label is "AllocationPriority"
        paragraph
            PrmVendor
                label is "Vendor"
            PrmPurchaseFromLocation
                label is "PurchaseFrom"
            PrmTransactionCurrencyCode
                label is "Currency"
        paragraph
            PrmBuyer
                label is "Buyer"
            PrmRequisitionDescription
                label is "RequisitionDescription"
            PrmCommodityCode
                label is "CommodityCode"
        paragraph
            PrmDefaultDistribution
                label is "Distribution"
                compact format
        paragraph
            PrmPurchaseTaxCode
                label is "TaxCode"
            PrmPOCode
                label is "POCode"
        paragraph
            PrmPurchaseTaxable
                label is "Taxable"
            PrmSourcingEventRequired
                label is "SourcingEventRequired"
        paragraph
            PrmRequisitionSource
                label is "RequisitionSource"
        blank line
        header2 of "ItemLineFields"
        paragraph
            PrmQuantity
                label is "Quantity"
            PrmUOM
                label is "UnitOfMeasure"
            PrmTransactionUnitCost
                label is "UnitCost"
            PrmCostOption
                label is "CostOption"
        paragraph
            PrmVendorAndPurchaseFrom.Vendor
                label is "ItemVendor"
            PrmVendorAndPurchaseFrom.PurchaseFromLocation
                label is "ItemPurchaseFrom"
        paragraph
            PrmDescription
                label is "Description"
        paragraph
            PrmMajorPurchasingClass
                label is "MajorPurchasingClass"
                show up to 10 characters
            PrmMinorPurchasingClass
                label is "MinorPurchasingClass"
                show up to 10 characters
        paragraph
            PrmCreatePurchaseOrder
                label is "CreatePurchaseOrder"
        paragraph
            PrmLineDistribution
                label is "Distribution"
                compact format
        paragraph
            PrmAsset
                label is "Asset"
            PrmAssetTemplate
                label is "AssetTemplate"
        paragraph
            PrmGLEncumbranceReference
                label is "G\\L_EncumbranceReference"
        paragraph
            PrmRSSLPLEligibleToAllocatePriorToSubstitution
                label is "RSSLPLEligibleToAllocatePriorToSubstitution"
                initial value is RSSLPLEligibleToAllocatePriorToSubstitution
        visible when (false)
            paragraph
                NewRequisition
            paragraph
                NewRequisitionLine
