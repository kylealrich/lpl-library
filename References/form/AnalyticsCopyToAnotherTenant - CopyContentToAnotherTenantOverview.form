CopyContentToAnotherTenantOverview is a Form
    title is "CopyContentToAnotherTenant"
    Actions
        action is refresh
    Layout
        single column
            blank line
            header2 of "Copy_Content_To_Another_Tenant"
            blank line
            text of "This_process_moves_your_current_tenant_Analytics_spaces_to_another_tenant._It_only_copies_metadata_and_will_require_you_to_process_the_data_in_your_desired_target_tenant."
                bold
            blank line
            visible when (AnalyticsCopyToAnotherTenantRel.IsDisableButtonText)
                text of "There_is_an_ongoing_upgrade_process._A_new_one_cannot_be_started_until_the_previous_one_is_completed_or_cancelled._Click_'Show_History'_to_see_more_details."
                    bold
                    color of red
            visible when (AnalyticsUpgradeActiveSpaceRel.IsStagingProcessRunning)
                text of "There_is_an_ongoing_upgrade_process_for_Analytics_Upgrade_Staging."
                    bold
                    color of red
            visible when (AnalyticsUpgradeStagingRel.IsActiveSpaceProcessRunning)
                text of "There_is_an_ongoing_upgrade_process_for_Analytics_Upgrade_Active_Space."
                    bold
                    color of red
            visible when (last PostPfiWorkunitsRel.Status.Processing)
                text of "There_is_an_ongoing_process_for_Update_ION_Schemas."
                    bold
                    color of red
            visible when (last DataFlowToAnalyticsFLRel.Status.Processing)
                text of "There_is_an_ongoing_process_for_DataFlow_To_Analytics_Full_Load."
                    bold
                    color of red
            visible when (last DataFlowToAnalyticsILRel.Status.Processing)
                text of "There_is_an_ongoing_process_for_Data_Flow_To_Analytics_Incremental_Load."
                    bold
                    color of red
            blank line
            visible when (AnalyticsCopyToAnotherTenantRel.IsValuesNotSetUp)
                text of "PleaseMakeSureThatBelowPropertiesAreEntered_:_TargetTenantID_,_TargetTenantBirstUserID_,_IONApiUrlTarget_,_SourceSpaceType_,_TargetSpaceType."
                    bold
                    color of red
            button of "Proceed_Copy_Content_to_Another_Tenant"
                valid when (AnalyticsCopyToAnotherTenantRel.IsEnableButton)
                action is Create
            blank line
            button of "ShowHistory"
                list is AnalyticsCopyToAnotherTenant.AnalyticsCopyToAnotherTenantList
        single column
            header2 of "CSF_Analytics_Version"
        two column
            column1
                text of "Latest_Metadata_Version_(SMD)"
                text of "Staging_Environment_Version_(STC)"
                text of "Active_Environment_Version_(ATC)"
            column2
                last ProductVersionSMDRel.Value
                    label is "<blank>"
                last ProductVersionSTCRel.Value
                    label is "<blank>"
                last ProductVersionATCRel.Value
                    label is "<blank>"
                    raise red alert when (last AnalyticsCopyToAnotherTenantRel.IsUpgradeCompleted)
                        mouse over text is "ActiveTenantContentVersion_isLower_than_theStandardMetaData"
        single column
            visible when (last AnalyticsCopyToAnotherTenantRel.UpdateNotRequired)
                text of "Your_Active_Tenant_Version_<last ProductVersionATCRel.Value>_is_not_supported_to_be_upgraded_using_the_Upgrade_automation_workflows._Kindly_request_for_reprovisioning_of_new_ATC_spaces_in_your_tenant_through_ServiceNow"
                    color of red
        single column
            text of "<blank>"
            button of "RefreshProductVersion"
                action is UpdateVersion
