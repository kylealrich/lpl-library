Date is a Form
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        paragraph
            create date
                display as text
            paragraph
                SubmittedTimestamp
                    label is "SubmittedDate"
                    display as text
                paragraph
                    AcceptedOrRejectedTimestamp
                        label is "DecisionDate"
                        display as text
                    paragraph
                        ReleasedTimestamp
                            label is "ReleaseDate"
                            display as text
                        paragraph
                            InternalApprovalTimestamp
                                label is "InternalApprovalDate"
                                display as text
                            paragraph
                                SupplierApprovalTimestamp
                                    label is "SupplierApprovalDate"
                                    display as text
                                paragraph
                                    NegotiationApprovalTimestamp
                                        label is "NegotiationApprovalDate"
                                        display as text
                                    paragraph
                                        FinalApprovalTimestamp
                                            label is "FinalApprovalDate"
                                            display as text
                                        paragraph
                                            ActivatedTimestamp
                                                label is "ActivationDate"
                                                display as text
