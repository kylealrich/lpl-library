PrintComposite is a CompositeForm
    title is "AddendumPrintingVersion<ContractVersion>"
    context form is PrintNavigation
        background is theme color
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Actions
        action is CreateAddendumContractDocument
            hide when invalid
        action is EmailAddendum
            action icon is export
            hide when invalid
        action is save
        action is refresh
    AddendumDocuments is a Panel
        visible when (not Contract.IsUsingIDMTemplate)
        list is ContractDocumentRel.PrintAddendumDocuments
    IDMAddendumDocuments is a Panel
        visible when (Contract.IsUsingIDMTemplate)
        list is ContractDocumentRel.PrintIDMAddendumDocuments
