RequesterProposalWizardFinish is a Form
    title is "ProposedContractSummary"
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        display as text
            single column
                ContractGroup
                Contract
                    label is "ProposedContract"
                Name
                first TemplateRelatedContractRel.RelatedContractID
                    label is "Template"
                blank line
        paragraph
            button of "ViewQuestionsWhichNeedAttention"
                visible when (QuestionsRequireAttention)
                link is QuestionsLink
        single column
            text of "SelectAnyMenuItemToExitProposedContract"
            blank line
            button of "SubmitProposedContract"
                visible when (CanUpdateProposedContract)
                action is Draft.SupplierSubmitProposedContract
            button of "ViewContract"
                link is RequesterProposalNav
                display as link
