ManageCopySelectedLinesForm is a CompositeForm
    title is "CreateContractOrLinesFromSelectedLines"
    context form is ContractContext
        background is theme color
    Actions
        action is CreateContractOrLinesFromSelectedLines
        navigation is SelectedLinesNav
            label is "ViewSelectedLines"
            hide when invalid
        navigation is ViewCopiedToContracts
            label is "ViewContractsCreatedOrUpdated"
            hide when invalid
        navigation is ViewContract
            label is "ViewThisContract"
        action is refresh
            label is "_"
    LinesToSelect is a Panel
        list is ContractLine set.LinesToSelect
