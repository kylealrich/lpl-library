PhoneActorForm is a CompositeForm
    title is "Actor:<Actor>"
    Detail is a Panel
        form is PhoneDetail
    Account is a Panel
        form is AccountInformation
    Contact is a Panel
        form is DefaultContactInfo
    Identities is a Panel
        list is IdentityActor set.SecondaryIdentityActorList
    Roles is a Panel
        list is ActorRole set.SecondaryActorRoleList
    CustomFields is a Panel
        form is CustomActorDetails
