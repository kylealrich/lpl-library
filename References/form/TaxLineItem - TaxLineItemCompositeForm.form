TaxLineItemCompositeForm is a CompositeForm
    is primary
    title is "TaxLineItem"
    context form is TaxLineItemForm
    show panel control on left
    Main is a Panel
        form is Main
    More is a Panel
        form is MoreForm
    ListUserFields is a Panel
        list is LineItemUserFieldsRel.TaxLineItemUserFieldsList
    VertexInputAttributes is a Panel
        title is "VertexVertexAttributes"
        list is VertexInputFlexibleFieldsRel.VertexFlexibleInputFieldsList
    VertexOutputAttribute is a Panel
        visible when (VertexFlexibleFieldsOutRelExist)
        list is VertexOutputFlexibleFieldsRel.VertexFlexibleOutputFieldsList
    InputUserAttributes is a Panel
        title is "OneSourceInputUserAttributes"
        list is InputUserAttributesRel.OneSourceUserAttributeLines
    OutputUserAttributes is a Panel
        title is "OutputUserAttributes"
        visible when (OneSourceUserAttributesExist)
        list is OneSourceUserAttributesRel.OneSourceUserAttributeLines2
    TaxDetailOut is a Panel
        title is "<TaxDetailOutCount>TaxTableRecords<TaxTableRecordsExist>"
        visible when (TaxDetailOutExists)
        list is TaxDetailOutRel.primary
    JurisdictionResults is a Panel
        visible when (JurisdictionResultsExist)
        list is JurisdictionResultsRel.JurisdictionResultsList
    MoreResults is a Panel
        form is MoreResultsForm
    CapturedXml is a Panel
        visible when (CapturedXml entered)
        form is HttpInfoForm
    MultipleTaxAreas is a Panel
        visible when (VertexResultsExist)
        list is VertexTaxAreaIdResultsRel.TaxAreaResultList
