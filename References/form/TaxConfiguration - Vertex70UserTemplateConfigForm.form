Vertex70UserTemplateConfigForm is a Form
    Layout
        header1 of "ConfigurationParametersForUserTemplate"
        visible when (VertexCalculateTax70ConfigExist)
            single column
                button of "UpdateVertexCalculateTax70ConfigurationParameter"
                    link is NavToVertexCalcTax70Parm
                    display as link
            two column
                column1
                    VertexCalculateTax70RelUTConfig.ConfigurationParameter.ConfigurationID
                    VertexCalculateTax70RelUTConfig.Type
                column2
                    VertexCalculateTax70RelUTConfig.ConfigurationParameter.Name
                    VertexCalculateTax70RelUTConfig.Length
            single column
                VertexCalculateTax70RelUTConfig.Value
                    show up to 120 characters
        visible when (not VertexCalculateTax70ConfigExist)
            single column
                text of "Warning...VertexCalculateTax70\\Configuration\\ParameterRecordDoesNotExist"
                    bold
                    color of red
                button of "CreateCalculateTax70ConfigurationParameter"
                    form invoke is Vertex70CalcTaxUserTemplateConfigParamRecord
                    display as link
        visible when (VertexLookupTaxAreas70ConfigExist)
            single column
                button of "UpdateVertexLookupTaxAreas70ConfigurationParameter"
                    link is NavToVertexLookupTaxArea70Parm
                    display as link
            two column
                column1
                    VertexLookupTaxAreas70RelUTConfig.ConfigurationParameter.ConfigurationID
                    VertexLookupTaxAreas70RelUTConfig.Type
                column2
                    VertexLookupTaxAreas70RelUTConfig.ConfigurationParameter.Name
                    VertexLookupTaxAreas70RelUTConfig.Length
            single column
                VertexLookupTaxAreas70RelUTConfig.Value
                    show up to 120 characters
        visible when (not VertexLookupTaxAreas70ConfigExist)
            single column
                text of "Warning...VertexLookupTaxAreas70\\Configuration\\ParameterRecordDoesNotExist"
                    bold
                    color of red
                button of "Create_\\Lookup_\\Tax_\\Areas70_\\ConfigurationParameter"
                    form invoke is Vertex70LookupTaxAreaUserTemplateConfigParamRecord
                    display as link
