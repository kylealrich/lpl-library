BillingReceivablesPortalPaymentSetup is a Form
    title is "PaymentSetup"
    Layout
        paragraph
            text of "In_order_to_allow_receivable_payments_via_portal,_certain_steps_need_to_take_place."
                bold
                color of blue
            text of "Please_refer_to_FSM_/_Paya_Integration_Guide_for_instructions."
                bold
                color of blue
            visible when (MissingThirdPartyPaymentInitialSetups)
                button of "LaunchInitialSetupsForThirdPartyPaymentProcessing"
                    action is ThirdPartyPaymentProcessingInitialSetups
                    is primary
        single column
            visible when (UseBillingReceivablesPortalAnyCustomerRepresentativeContactAttentionRelExists)
                paragraph
                    button of "SynchronizeCustomerContacts"
                        action is CreateActorContextForEachCustomerRepresentativeContact
                        is primary
        header1 of "ThirdPartyPaymentProcessingInformation"
        single column
        header3 of "ConfigurationParameters"
        visible when (ThirdPartyPaymentProcessingPaymentCurrencyConfigurationParameterRelExists)
            paragraph
                ThirdPartyPaymentProcessingPaymentCurrencyConfigurationParameterRel.ConfigurationParameter.ConfigurationID
                    show up to 3 characters
                ThirdPartyPaymentProcessingPaymentCurrencyConfigurationParameterRel.ConfigurationParameter.Name
                    show up to 20 characters
                button of "Edit"
                    link is ThirdPartyPaymentProcessingPaymentCurrencyConfigurationParameterNavigation
                    display as link
                ThirdPartyPaymentProcessingPaymentCurrencyConfigurationParameterRel.Value
                    show up to 5 characters
                text of "Important!__ThisValueMustMatchThe_CurrencyDefinitionFor_'U\\S_Dollar'_inThe_Currency_Business_Class."
                    bold
                    color of blue
        visible when (not ThirdPartyPaymentProcessingPaymentCurrencyConfigurationParameterRelExists)
            single column
                text of "Warning:__Third_Party_Payment_Processing_Payment_Currency_Configuration\\ParameterRecordDoesNotExist."
                    bold
                    color of red
        visible when (not ThirdPartyPaymentProcessingEndpointConfigurationParameterRelExists)
            single column
                text of "Warning:__Third_Party_Payment_Processing_Endpoint_Configuration\\ParameterRecordDoesNotExist."
                    bold
                    color of red
            paragraph
                button of "CreateEndpointConfigurationParameter"
                    form invoke is PayaConnectEndpointConfigurationParameterRecord
                    is primary
                text of "Note:__AfterClickingThisButton,Enter_Infor_IONAPI_Public_Facing_Proxy_Endpoint_for_Paya+'/v2'_(no_space)_EndpointInValueFieldOnNextScreen."
        visible when (ThirdPartyPaymentProcessingEndpointConfigurationParameterRelExists)
            paragraph
                ThirdPartyPaymentProcessingEndpointConfigurationParameterRel.ConfigurationParameter.ConfigurationID
                    show up to 3 characters
                ThirdPartyPaymentProcessingEndpointConfigurationParameterRel.ConfigurationParameter.Name
                    show up to 20 characters
                button of "Edit"
                    link is ThirdPartyPaymentProcessingEndpointConfigurationParameterNavigation
                    display as link
                ThirdPartyPaymentProcessingEndpointConfigurationParameterRel.Value
                    show up to 120 characters
        visible when (ThirdPartyPaymentProcessingPayformURIConfigurationParameterRelExists)
            paragraph
                ThirdPartyPaymentProcessingPayformURIConfigurationParameterRel.ConfigurationParameter.ConfigurationID
                    show up to 3 characters
                ThirdPartyPaymentProcessingPayformURIConfigurationParameterRel.ConfigurationParameter.Name
                    show up to 20 characters
                button of "Edit"
                    link is ThirdPartyPaymentProcessingPayformURIConfigurationParameterNavigation
                    display as link
                ThirdPartyPaymentProcessingPayformURIConfigurationParameterRel.Value
                    show up to 20 characters
        visible when (not ThirdPartyPaymentProcessingPayformURIConfigurationParameterRelExists)
            single column
                text of "Warning:__Third_Party_Payment_Processing_Payform_URI_Configuration\\ParameterRecordDoesNotExist."
                    bold
                    color of red
        header3 of "IntegrationFields"
        paragraph
            ThirdPartyPaymentProcessingContactId
            text of "[24_Characters_Required]"
                color of blue
        paragraph
            ThirdPartyPaymentProcessingLocationId
            text of "[24_Characters_Required]"
                color of blue
        paragraph
            ThirdPartyPaymentProcessingProductTransactionIdCC
            text of "[24_Characters_Required]"
                color of blue
        paragraph
            ThirdPartyPaymentProcessingProductTransactionIdACH
            text of "[24_Characters_Required]"
                color of blue
        paragraph
            ThirdPartyPaymentProcessingUserId
            text of "[24_Characters_Required]"
                color of blue
        paragraph
            ThirdPartyPaymentProcessingUserApiKey
            text of "[24_Characters_Required]"
                color of blue
        paragraph
            ThirdPartyPaymentProcessingUserHashKey
            text of "[24_Characters_Required]"
                color of blue
        header1 of "CashManagementAccount"
        paragraph
        visible when (not CashManagementAccountSetupWithThirdPartyPaymentProcessingReceivableProcessLevelRelLegalEntityMatchesAReceivableCompany)
            text of "ADDITIONAL_STEP_NEEDED-AtLeastOneCashManagementAccount(RelatedToAReceivableCompanyTiedToThisCustomerGroup)MustHaveAThirdPartyPaymentProcessingReceivableProcessLevel"
                bold
                italics
                color of red
        visible when (CashManagementAccountSetupWithThirdPartyPaymentProcessingReceivableProcessLevelRelLegalEntityMatchesAReceivableCompany)
            text of "AtLeastOneCashManagementAccount(RelatedToAReceivableCompanyTiedToThisCustomerGroup)HasAThirdPartyPaymentProcessingReceivableProcessLevel_No_Action_Needed."
                bold
                italics
                color of green
            two column
                column1
                    DerivedThirdPartyPaymentProcessingCashManagementAccountSetupWithThirdPartyPaymentProcessingReceivableProcessLevelRelCashManagementAccount
                        label is "ThirdPartyPaymentProcessingCashManagementAccount"
                column2
                    ReleasePortalPaymentProcessAutoCashApplication
            two column
                column1
                    DerivedThirdPartyPaymentProcessingCompany
                        label is "ThirdPartyPaymentProcessingCompany"
                column2
                    DerivedThirdPartyPaymentProcessingCashManagementAccountSetupWithThirdPartyPaymentProcessingReceivableProcessLevelRelThirdPartyPaymentProcessingReceivableProcessLevel
                        label is "ThirdPartyPaymentProcessingProcessLevel"
            blank line
        header1 of "RoleSecurityClass"
        paragraph
            CustomerPrimaryInvoicePaymentAccess
                label is "PrimaryContact"
            visible when (PrimaryFlaggedButPrimaryRoleSecurityClassNotExits)
                text of "Additional_Step_Needed:__The_CustomerRepresentativePrimaryContact_ST_does_not_exist.__To_add_the_role,_uncheck_this_box,_save,_check_this_box_and_save_again."
                    bold
                    color of red
        paragraph
            CustomerInvoicePaymentAccess
                label is "Other_Contacts"
            visible when (OtherContactsFlaggedButOtherContactsRoleSecurityClassNotExists)
                text of "Additional_Step_Needed:__The_CustomerRepresentativeContact_ST_does_not_exist.__To_add_the_role,_uncheck_this_box,_save,_check_this_box_and_save_again."
                    bold
                    color of red
        header1 of "TransactionApiId"
        ThirdPartyPaymentProcessingTransactionApiId
            label is "LastTransactionApiId"
            display as text
        header1 of "ContextProperties"
        paragraph
        visible when (not ContextPropertyRelExists)
            text of "ADDITIONAL_STEP_NEEDED-Customer_Group_Context_PropertyDoesNotExist._Run_ThirdPartyPaymentProcessingInitialSetups"
                bold
                italics
                color of red
        visible when (ContextPropertyRelExists)
            text of "Customer_Group_Context_PropertyExists-_No_Action_Needed."
                bold
                italics
                color of green
        blank line
        visible when (not CustomerRepresentativeContextPropertyRelExists)
            text of "ADDITIONAL_STEP_NEEDED-Customer_Representative_Context_PropertyDoesNotExist._Run_ThirdPartyPaymentProcessingInitialSetups"
                bold
                italics
                color of red
        visible when (CustomerRepresentativeContextPropertyRelExists)
            text of "Customer_Representative_Context_PropertyExists-_No_Action_Needed."
                bold
                italics
                color of green
        blank line
        visible when (not CustomerRepresentativeContactContextPropertyRelExists)
            text of "ADDITIONAL_STEP_NEEDED-Customer_Representative_Contact_Context_PropertyDoesNotExist._Run_ThirdPartyPaymentProcessingInitialSetups"
                bold
                italics
                color of red
        visible when (CustomerRepresentativeContactContextPropertyRelExists)
            text of "Customer_Representative_Context_Contact_PropertyExists-_No_Action_Needed."
                bold
                italics
                color of green
        blank line
