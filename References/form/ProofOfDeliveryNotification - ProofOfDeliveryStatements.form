ProofOfDeliveryStatements is an ActionForm
    action is ProofOfDeliveryNotification
    Layout
        two column distributed
            Company
                select is Companies
            VenClass
                label is "VendorClass"
                select is VendorClass
        two column distributed
            FrVendor
                label is "FromVendor"
                select is VendorName
            ToVendor
                label is "ToVendor"
                select is VendorName
        two column distributed
            CutoffDate
                label is "RunDate"
            AgingDays
        two column distributed
            PrevPrinted
                label is "PreviouslyRequested"
            VendorSequence
        two column distributed
            Alignment
                label is "AlignmentForms"
            PrtFormat
                label is "PrintFormat"
        single column
            Msg1
                label is "Message"
            Msg2
                no label
            Msg3
                no label
            Msg4
                no label
            Msg5
                no label
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                ProofOfDeliveryNotificationReport_RDG
                ProofOfDeliveryPOD_RDG
            column2 of 25%
                ProofOfDeliveryNotificationReport_XPT
                ProofOfDeliveryPOD_XPT
