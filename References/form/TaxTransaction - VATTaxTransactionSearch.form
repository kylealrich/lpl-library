VATTaxTransactionSearch is a SearchForm
    title is "VATTaxTransactionSearch"
    Layout
        five column distributed
            column1
                TransCalendarYear
                    label is "CalendarYear"
                TransCalendarPeriod
                    label is "Period"
                TaxPointDateRange.Begin
                    label is "FromTaxPointDate"
                TaxPointDateRange.End
                    label is "ToTaxPointDate"
            column2
                TaxCode
                TaxUsageCode
            column3
                TaxTransaction
                    select is TaxTransactionList
                TransactionSource
                InvoicedOrCreditMemo
            column4
                InvoiceCompany.OrigCompany
                SystemCode
                VendorCustomer
                Invoice
            column5
                VATRegistrationCountry
                TaxYear
                    label is "TaxYearReported"
                TaxPeriod
                    label is "TaxPeriodReported"
