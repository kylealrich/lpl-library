ContractComplianceReportSumForm is a SummaryForm
    title is "_"
    Layout
        visible when (HasContractComplianceInvoiceLinesOnContract)
            three column
                column1
                    header4 of "ItemsPurchasedOnContract"
                    blank line
                    blank line
                column2
                    header4 of "Contract:_<Contract>_-_<Name>"
                    header4 of "Supplier:_<Supplier>"
                    header4 of "Vendor:_<Vendor>_-_<Vendor.VendorName>"
                column3
                    blank line
                    blank line
                    blank line
            form is ContractLinesWithContractComplianceInvoiceLinesOnContractRel.ContractComplianceItemOnContractSumForm
            four column
                column1
                    blank line
                    blank line
                column2
                    blank line
                    blank line
                column3
                    OnContractComplianceReportQuantityTotal
                        label is "OnContractTotalQuantity"
                        display when blank
                    blank line
                column4
                    OnContractComplianceReportAmountTotal
                        label is "OnContractTotalCost"
                        display when blank
                    PercentOfOnContractReportAmountTotal
                        label is "PercentOfReportTotal"
                        display when blank
        visible when (HasContractComplianceInvoiceLinesOffContract)
            three column
                column1
                    header4 of "ItemsPurchasedOffContract"
                column2
                    header4 of "Contract:_<Contract>_-_<Name>"
                column3
                    blank line
            form is ContractLinesWithContractComplianceInvoiceLinesOffContractRel.ContractComplianceItemOffContractSumForm
            four column
                column1
                    blank line
                    blank line
                column2
                    blank line
                    blank line
                column3
                    OffContractComplianceReportQuantityTotal
                        label is "OffContractTotalQuantity"
                        display when blank
                    blank line
                column4
                    OffContractComplianceReportAmountTotal
                        label is "OffContractTotalCost"
                        display when blank
                    PercentOfOffContractReportAmountTotal
                        label is "PercentOfReportTotal"
                        display when blank
