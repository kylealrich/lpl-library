PerformAssetInterface is an ActionForm
    action is PerformAssetInterface
    Layout
        paragraph
            PrmRunGroup
                label is "RunGroup"
                display as text
        paragraph
            FinanceEnterpriseGroup
                display as text
            FinanceEnterpriseGroup.EnterpriseGroup.Description
                no label
                display as text
        blank line
        paragraph
            PrmCompany
                label is "Company"
                select is Companies
            text of "Or"
            PrmCompanyGroup
                label is "CompanyGroup"
        two column
            PrmLeaseCompany
                label is "LeaseCompany"
                select is Companies
            PrmLease
                label is "Lease"
        blank line
        single column
            PrmErrorLimit
                label is "ErrorLimit"
        blank line
        single column
            PrmComputeOption
                label is "ComputeOption"
                when value changed
                    refresh PrmComputeOption
            visible when (PrmComputeOption not entered)
                PrmAllowZeroLifeRemaining
                    label is "AllowZeroLifeRemaining"
            visible when (PrmComputeOption entered)
                visible when (PrmComputeOption.No)
                    PrmAllowZeroLifeRemaining
                        label is "AllowZeroLifeRemaining"
        single column
            PrmFilesToConvert
                label is "Files_toConvert"
        paragraph
            PrmFromRange
                label is "FromImportGroup"
            PrmToRange
                label is "ToImportGroup"
        blank line
        header2 of "ForTransactions/CommentsOnly"
        blank line
        single column
            PrmAttributeBy
                label is "Conversion"
