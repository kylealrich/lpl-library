AssetTemplateBookForm is a Form
    is primary
    title is "AssetTemplateBook"
    Layout
        paragraph
            AssetTemplate
            AssetTemplate.Description
                no label
                display as text
        paragraph
            Book
                select is ActiveDepreciationBooks
            Book.Description
                no label
                display as text
            Book.Currency
                no label
                display as text
        single column
            Active
        visible when (AllowBookUpdateWithRegulation)
            paragraph
                BookRegulation
                BookRegulation.Description
                    no label
                    display as text
        visible when (DisplayBookRegulation)
            paragraph
                BookRegulation
                BookRegulation.Description
                    no label
                    display as text
        two column distributed
            column1
                paragraph
                    Method
                    Method.Description
                        no label
                        display as text
                paragraph
                    Life
                    LifeInYears
                SalvagePercentage
                BusinessPercent
                FirstYearExpensePercent
            column2
                paragraph
                    Convention
                paragraph
                    InsuranceCalculationMethod
                    InsuranceCalculationMethod.Description
                        no label
                        display as text
                paragraph
                    ReplacementCalculationMethod
                    ReplacementCalculationMethod.Description
                        no label
                        display as text
                paragraph
                    PersonalUseCode
                        label is "VehicleLimitCode"
                    PersonalUseCode.Description
                        no label
                        display as text
                paragraph
                    AssetTaxCreditTable
                    AssetTaxCreditTable.Description
                        no label
                        display as text
        blank line
        header3 of "TaxReportingFields"
        three column
            visible when (BookRegulationsExist)
                column1
                    BookRegulation
                        display as text
            column2
                ClassificationOfProperty
            column3
                DepreciableIntangible
        three column
            column1
                QualifiedImprovement
                PropertyType
                CostRecoverySystem
                BonusAmount
                Section179Expense
            column2
                Listed
                IndianReservation
                Used
                MineralReserves
                Section179
            column3
                ForeignProperty
                Farming
                LowIncomeHousing
                TransitionalRuleFlag
                NonConforming
