MassVendorUpdate is an ActionForm
    action is MassVendorUpdate
    Layout
        paragraph
            VendorGroup
            VendorGroup.Description
                no label
                display as text
        single column
            SelectAllVend
                label is "AllVendors"
        paragraph
            VenClass
                label is "VendorClass"
            VenClass.Description
                no label
                display as text
        paragraph
            FrVendor
                label is "FromVendor"
            ToVendor
                label is "ToVendor"
        paragraph
            UpdateOption
                label is "Update"
            UpdateLocation
                label is "UpdateLocations"
        single column
            VenPriority
                label is "OverridePriority"
        header2 of "Invoice"
        two column table
            column headers
                "Before"
                "After"
            row headers
                "AuthorityCode"
                "AccrualCode"
                "IncomeCode"
                "InvoiceCurrency"
                "TaxCode"
                "HoldCode"
            column1
                OldAuthCode
                    no label
                OldAccrCode
                    no label
                OldIncomeCode
                    no label
                OldInvCurrency
                    no label
                OldTaxCode
                    no label
                OldHoldCode
                    no label
                    select is HoldOnVendorList
            column2
                NewAuthCode
                    no label
                NewAccrCode
                    no label
                NewIncomeCode
                    no label
                NewInvCurrency
                    no label
                NewTaxCode
                    no label
                NewHoldCode
                    no label
                    select is HoldOnVendorList
        header2 of "Payment"
        two column table
            column headers
                "Before"
                "After"
            row headers
                "PaymentCode"
                "CashCode"
                "PaymentTerms"
                "DiscountCode"
            column1
                OldBankInstCode
                    no label
                OldCashCode
                    no label
                OldTermCode
                    no label
                OldDiscountCode
                    no label
            column2
                NewBankInstCode
                    no label
                NewCashCode
                    no label
                NewTermCode
                    no label
                NewDiscountCode
                    no label
        header2 of "Distribution"
        two column table
            column headers
                "Before"
                "After"
            row headers
                "DistributionCode"
                "Project"
            column1
                OldDistCode
                    no label
                OldActivity
                    no label
            column2
                NewDistCode
                    no label
                NewActivity
                    no label
        header2 of "Match"
        two column table
            column headers
                "Before"
                "After"
            row headers
                "MatchTable"
                "RuleGroup"
            column1
                OldMatchTable
                    no label
                    select is MatchTablesForAVendor
                OldRuleGroup
                    no label
            column2
                NewMatchTable
                    no label
                    select is MatchTablesForAVendor
                NewRuleGroup
                    no label
        header2 of "VendorBank"
        single column
            UpdPrenot
                label is "UpdatePrenotificationStatus"
        header2 of "DiversityCode"
        two column table
            column headers
                "Before"
                "After"
            row headers
                "DiversityCode"
            column1
                OldDiverseCode
                    no label
            column2
                NewDiverseCode
                    no label
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                MassVendorUpdateReport_RDG
            column2 of 25%
                MassVendorUpdateReport_XPT
