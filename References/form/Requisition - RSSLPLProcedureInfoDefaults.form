RSSLPLProcedureInfoDefaults is a Form
    title is "PatientProcedureInformation_forRequisition<Requisition>"
    Actions
        action is save
    Layout
        two column distributed
            column1
                button of "DeletePatientProcedure"
                    visible when (IsDeletePatientProcAllowed)
                    action is DeletePatientProcedure
                    is primary
                header2 of "Visit"
                TransientVisitNumber
                    label is "VisitNumber"
                TransientPatientID
                    label is "MedicalRecordNumber"
                TransientCaseNumber
                    label is "CaseID"
                paragraph
                    DefaultProcedureInformation.DefaultProcedure
                        label is "Procedure"
                    DefaultProcedureInformation.DefaultProcedureModifier
                        label is "ProcedureModifier"
                DefaultProcedureInformation.DefaultProcedureDate
                    label is "ProcedureDate"
                DefaultProcedureInformation.DefaultPractitioner
                    label is "Practitioner"
            column2
                visible when (IsDeletePatientProcAllowed)
                    text of "_"
                header2 of "Patient"
                TransientPatientLastName
                    label is "LastName"
                TransientPatientFirstName
                    label is "FirstName"
                TransientPatientMiddleName
                    label is "MiddleInitial"
                paragraph
                    TransientBirthdate
                        label is "Birthdate"
                    TransientGender
                        label is "Gender"
                paragraph
                    visible when (TransientMaskSocialSecurityNumber)
                        TransientSocialSecurityNumber
                            label is "SSN"
                    visible when (not TransientMaskSocialSecurityNumber)
                        TransientSocialSecurityNumber
                            label is "SSN"
                            hidden entry
                    TransientMaskSocialSecurityNumber
                        label is "ShowSSN"
                DefaultProcedureInformation.DefaultNurseCoordinator
                    label is "CoordinatingNurse"
        header2 of "Order"
        two column distributed
            column1
                DefaultProcedureInformation.DefaultSalesRepresentative
                    label is "SalesRepresentative"
                DefaultProcedureInformation.DefaultRequester
                    label is "Requester"
            column2
                DefaultProcedureInformation.DefaultSupplierOrderNumber
                    label is "SupplierOrderNumber"
        header2 of "Item"
        two column distributed
            column1
                DefaultProcedureInformation.DefaultUserDefined1
                    label is "UserDefined1"
            column2
                DefaultProcedureInformation.DefaultUserDefined2
                    label is "UserDefined2"
