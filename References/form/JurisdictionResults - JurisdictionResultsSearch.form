JurisdictionResultsSearch is a SearchForm
    title is "JurisdictionResultsSearch"
    Layout
        three column distributed
            column1
                VertexConfig
                TaxDateRange.Begin
                    label is "FromTaxDate"
                TaxDateRange.End
                    label is "ToTaxDate"
                TaxDateYear
            column2
                JurisdictionResults.LookupKey
                JurisdictionLevel
                Jurisdiction
                Situs
            column3
                Imposition
                TaxType
                TaxRuleId
                    label is "TaxRuleID"
                TaxResult
