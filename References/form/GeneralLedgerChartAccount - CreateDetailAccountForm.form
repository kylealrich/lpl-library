CreateDetailAccountForm is an ActionForm
    action is AddPostingAccountToChart
    title is "Add<FinanceEnterpriseGroup.AccountLabel>ToChart"
    Layout
        single column
            PrmChart
                label is "Chart"
                select is AvailableCharts
            visible when (false)
                TransientContextAccount
                    label is "ContextAccount"
            PrmParentChartAccount
                label is "Parent<FinanceEnterpriseGroup.AccountLabel>"
                select is SummaryRecords
