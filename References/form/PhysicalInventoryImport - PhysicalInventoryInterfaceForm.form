PhysicalInventoryInterfaceForm is a Form
    is primary
    title is "PhysicalInventoryInterface"
    Layout
        single column
            RunGroup
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            PhysicalInventoryImport.SelectID
                label is "SelectID"
                select is SelectIDsForInterface
            PhysicalInventoryImport.SelectID.Description
                no label
                display as text
        paragraph
            PhysicalInventoryImport.InventoryLocation
                label is "Location"
            PhysicalInventoryImport.InventoryLocation.Name
                no label
                display as text
        paragraph
            PhysicalInventoryImport.Item
            PhysicalInventoryImport.Item.Description
                no label
                display as text
            PhysicalInventoryImport.Item.Description2
                no label
                display as text
        single column
            PhysicalInventoryImport.Bin
                label is "Bin"
        single column
            PhysicalInventoryImport.TransactionSequenceNumber
                label is "TransactionNumber"
        blank line
        paragraph
            display as text
                RecordInError
                ErrorMessage
        two column distributed
            column1
                CountQuantity
                CountUnitOfMeasure
                    label is "CountUOM"
                Lot
                    label is "LotNumber"
                InventoryReasonCode
                    label is "ReasonCode"
            column2
                SecondaryCountQuantity
                    label is "SecondaryCountQuantity"
                UnitCost
                Sublot
                    label is "SublotNumber"
                NewRecord
                    label is "NewRecord"
                    display as text
        single column
            Serial
