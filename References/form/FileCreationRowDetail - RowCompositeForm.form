RowCompositeForm is a CompositeForm
    title is "<FileCreationSetup.Description>-<FileCreationRowDetail.Description>"
    show panel control on left
    RowDetail is a Panel
        form is FileCreationRowForm
    OneToMany is a Panel
        visible when (FileCreationSetup.SupportsOneToMany)
        form is FileCreationOneToMany
    RowsLinked is a Panel
        visible when (RowIsLinked)
        list is RowIsLinkedRel.RowsLinkedList
