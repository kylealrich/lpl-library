CustodialAccountManagementCompositeForm is a CompositeForm
    is primary
    propagate as a drill
    title is "<FinanceEnterpriseGroup.FinanceDimension10Label>_Record"
    context form is CustodialAccountManagementContextForm
        background is theme color
    Main is a Panel
        form is MainForm
    Address is a Panel
        form is AddressForm
    References is a Panel
        form is AccountReferences
    DetailAmount is a Panel
        title is "<FinanceEnterpriseGroup.FinanceDimension10Label>_Detail"
        list is CustodialDetailAmount set.CustodialDetailAmountList
            helper list is CustodialSummary.DescendantsRel.CustodialDetailHelperList
                action is CreateCustodialDetail
                    invoked.CustodialAccountManagement = CustodialAccountManagement
    Activity is a Panel
        list is CustodialActivity set.primary
    Contacts is a Panel
        list is CustodialContact set.primary
    SourceOfFunds is a Panel
        list is CustodialSourceOfFunds set.primary
    FundingCharacteristics is a Panel
        list is CustodialFundChar set.primary
    RelatedDocuments is a Panel
        list is CustodialAttachment set.primary
    Comments is a Panel
        list is CustodialComment set.primary
    Deliverables is a Panel
        list is CustodialDeliverable set.primary
