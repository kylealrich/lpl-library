MobileCashManagementAccountContext is a Form
    title is "CashManagementAccountContext"
    Layout
        single column
            Image.File
                no label
                align as label
                display as full image up to 4 lines
                    missing image is missingpicture96px
        two column distributed
            column1
                CashManagementAccount
                FinancialInstitution
                FinancialInstitutionBranch.RoutingNumber
                    display as text
            column2
                paragraph
                    Title
                    visible when (IsInactiveWithActiveCashCode)
                        text of "Warning:AccountHasActiveCashCodes"
                            bold
                            color of red
                FinancialInstitutionBranch
                    select is FinancialInstitutionBranchSelect
                FinancialInstitutionBranch.BranchSwift
                    display as text
