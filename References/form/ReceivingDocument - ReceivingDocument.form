            PoNumber.PO130PoNumberGroup[4].PoNumberGrp
                no label
        paragraph
            PoNumber.PO130PoNumberGroup[5].PoNumberGrp
                no label
            PoNumber.PO130PoNumberGroup[6].PoNumberGrp
                no label
        blank line
        paragraph
            SelectedPoCode.SelectedPoCode[1]
                label is "P\\OCode"
            SelectedPoCode.SelectedPoCode[2]
                no label
            SelectedPoCode.SelectedPoCode[3]
                no label
        paragraph
            SelectedPoCode.SelectedPoCode[4]
                no label
            SelectedPoCode.SelectedPoCode[5]
                no label
            SelectedPoCode.SelectedPoCode[6]
                no label
        blank line
        two column
            FrEarlyDlDate
