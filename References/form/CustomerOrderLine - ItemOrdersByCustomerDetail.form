ItemOrdersByCustomerDetail is a Form
    restrict action CreateCustomerOrderReturnLine
    Layout
        paragraph
            OrderQuantity
                label is "Quantity"
        paragraph
            SellUOM
                no label
        paragraph
            EnteredPrice
                label is "Price"
        paragraph
            SellingPriceUOM
                no label
                select is ValidSellPriceUnitsOfMeasure
        paragraph
            CustomerOrder.OrderDiscountCode.Currency
                no label
