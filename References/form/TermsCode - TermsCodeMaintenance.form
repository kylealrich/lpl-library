TermsCodeMaintenance is a CompositeForm
    is primary
    title is "<DerivedFormTitle>"
    context form is TermsCodeMaintenanceContext
        background is theme color
    Net is a Panel
        visible when (ProcessType.Net)
        form is NetForm
    Prox is a Panel
        visible when (ProcessType.Prox)
        form is ProxForm
    Multiple is a Panel
        visible when (ProcessType.Multiple)
        form is MultipleForm
    FixedDays is a Panel
        visible when (ProcessType.FixedDays)
        form is FixedDaysForm
    Split is a Panel
        visible when (ProcessType.Split)
        form is SplitForm
    Prepayment is a Panel
        visible when (ProcessType.Prepayment)
        form is PrepayForm
    Preview is a Panel
        list is TermsCodePreviewRel.TermsCodePreviewList
