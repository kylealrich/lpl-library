MGeneralLedgerJournalControl is a CompositeForm
    propagate as a drill
    title is "JournalControl_<AccountingEntity>_<Ledger>"
    context form is GeneralLedgerJournalControlForm
    AdditionalInfoPanel is a Panel
        title is "Main"
        form is GeneralLedgerJournalControlAdditionalInfoForm
    CurencyRatesPanel is a Panel
        title is "CurrencyRates"
        visible when (AllowCurrencyRates)
        form is GeneralLedgerJournalControlRatesForm
    CommentsPanel is a Panel
        title is "Comments"
        list is GeneralLedgerJournalComment set.MComment
    TransactionPanel is a Panel
        title is "Transactions"
        list is GeneralLedgerTransaction set.MGeneralLedgerJournalTransactionList
    TotalsPanel is a Panel
        title is "Totals"
        form is GeneralLedgerJournalControlTotalsForm
    CurencyTotalsPanel is a Panel
        title is "TotalsByTransactionCurrency"
        visible when (BalanceByTransactionCurrency)
        list is GLCurrencyJournalControl set.MGLCurrencyJournalControlList
    RelatedDocumentsPanel is a Panel
        title is "RelatedDocuments"
        list is GeneralLedgerJournalDocument set.MGeneraLedgerJournalDocuments
    BudgetErrors is a Panel
        title is "BudgetErrors"
        visible when (BudgetErrorsExist)
        list is BudgetEditErrorRel.MBudgetEditErrorList
