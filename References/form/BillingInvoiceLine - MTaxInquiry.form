MTaxInquiry is a Form
    title is "TaxInquiry"
    Layout
        display as text
            two column distributed
                TaxTotal
                    label is "TaxAmount"
                TotalDiscount
                    label is "Discount"
                LineNetAmount.TransactionAmount
                    label is "NetAmount"
                LineTotal
                TaxCode
                EnteredDiscount
                    label is "Add-OnDiscount"
                LineGrossAmount
                    label is "GrossAmount"
