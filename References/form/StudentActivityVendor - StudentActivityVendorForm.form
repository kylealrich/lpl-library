StudentActivityVendorForm is a CompositeForm
    is primary
    title is "<mode>Vendor"
    show panel control on left
    Actions
        action is Create
        action is save
        action is save and new
        action is Delete
        action is next record
        action is previous record
        action is refresh
    Main is a Panel
        form is StudentActivityVendorContextForm
    VendorPayments is a Panel
        list is StudentActivityBankTransaction set.DisbursementsForVendorList
