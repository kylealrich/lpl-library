CreateInvoiceFromOrder is an ActionForm
    action is Unreleased.CreateInvoiceFromOrder
    title is "CreateUsingCopyFromOrder"
    Layout
        paragraph
            BillingCompany
                label is "Company"
        visible when (BillingCompany.ManualInvoiceNumbering)
            paragraph
                PrmGlobalInvoiceNumber
        paragraph
            PrmCustomerOrder
                label is "FromOrder"
        paragraph
            PrmWarehouseShipment
                label is "FromShipment"
        visible when (PrmCustomerOrder entered)
        paragraph
            PrmOverride
                label is "Override"
        header2 of "OverrideValues"
        visible when (PrmOverride)
            paragraph
                PrmBatchNumber
                    label is "Batch"
            paragraph
                PrmInvoiceDate
                    label is "InvoiceDate"
            paragraph
                Customer
                    label is "Customer"
                    select is CustomerContextCompanySelectList
                Customer.Name
                    no label
                    display as text
            paragraph
                PrmCustomerShipTo
                    label is "ShipTo"
                PrmCustomerShipTo.Name
                    no label
                    display as text
            paragraph
                PrmBillingProcessLevel
                    label is "ProcessLevel"
                PrmBillingProcessLevel.Name
                    no label
                    display as text
            paragraph
                PrmBillingInvoiceType
                    label is "InvoiceType"
                PrmBillingInvoiceType.Description
                    no label
                    display as text
            paragraph
                PrmReasonCode
                    label is "ReasonCode"
                PrmReasonCode.Description
                    no label
                    display as text
        visible when (not PrmOverride)
            paragraph
                PrmBatchNumber
                    label is "Batch"
                    display as text
            paragraph
                PrmInvoiceDate
                    label is "InvoiceDate"
                    display as text
            paragraph
                Customer
                    label is "Customer"
                    display as text
                Customer.Name
                    no label
                    display as text
            paragraph
                PrmCustomerShipTo
                    label is "ShipTo"
                    display as text
                PrmCustomerShipTo.Name
                    no label
                    display as text
            paragraph
                PrmBillingProcessLevel
                    label is "ProcessLevel"
                    display as text
                PrmBillingProcessLevel.Name
                    no label
                    display as text
            paragraph
                PrmBillingInvoiceType
                    label is "InvoiceType"
                    display as text
                PrmBillingInvoiceType.Description
                    no label
                    display as text
            paragraph
                PrmReasonCode
                    label is "ReasonCode"
                    display as text
                PrmReasonCode.Description
                    no label
                    display as text
