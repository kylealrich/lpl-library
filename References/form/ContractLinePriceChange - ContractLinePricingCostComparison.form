ContractLinePricingCostComparison is a Form
    title is "ContractLineCostComparison"
    Layout
        display as text
            three column
                ContractLine.ItemNumber
                    label is "Item"
                ContractLine.VendorItem
            paragraph
                ContractLine.VendorItemDescription
            two column
                ContractLine.UOM.UnitOfMeasure
            two column
                BaseCost
            paragraph
                EffectiveDate
                    label is "FromAndToDateForQuantityCalculation"
                text of "-"
                ExpirationDate
                    no label
            two column
                FutureCost
                FutureCostDate
                    label is "FutureEffectiveDate"
            two column
                DifferenceAmount
                DerivedDifferencePercent
                    label is "DifferencePercent"
            two column
                OffContractQuantity
                OffContractPurchases
                    label is "OffContractPurchaseAmount"
