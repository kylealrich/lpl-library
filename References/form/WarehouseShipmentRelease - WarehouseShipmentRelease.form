WarehouseShipmentRelease is an ActionForm
    action is WarehouseShipmentRelease
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            Location
                show up to 10 characters
            Location.Name
                no label
                display as text
        single column
            blank line
            UpdateOption
                label is "Update"
        two column distributed
            column1
                FrBatchNbr
                    label is "BatchNumbersFrom"
                    show up to 15 characters
            column2
                ToBatchNbr
                    label is "BatchNumbersTo"
                    show up to 15 characters
        two column distributed
            column1
                FrShipNbr
                    label is "ShipmentNumbersFrom"
                    select is ShipmentsForLocation
                    show up to 15 characters
            column2
                ToShipNbr
                    label is "ShipmentNumbersTo"
                    select is ShipmentsForLocation
                    show up to 15 characters
        two column distributed
            column1
                FrShipDate
                    label is "ShipmentDatesFrom"
            column2
                ToShipDate
                    label is "ShipmentDatesTo"
        single column
            SystemCd
                label is "DocumentSource"
            ShiprelFileOption
                label is "CreateFlatFile"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                WarehouseShipmentReleaseReport_RDG
                WarehouseShipmentReleaseErrorReport_RDG
            column2 of 25%
                WarehouseShipmentReleaseReport_XPT
                WarehouseShipmentReleaseErrorReport_XPT
