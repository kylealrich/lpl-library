SupplierResponseHistoryReportForm is a SummaryForm
    title is "SupplierResponseHistory"
    First Page Header
        Layout
            paragraph
                header3 of "SupplierResponseHistory"
                    center
            PrintDate
                right align
    Layout
        visible when (PrintParameters)
            single column
                SupplyManagementReportSupplierGroupField.SupplierGroup
                visible when (SupplyManagementReportSupplierGroupField.Supplier entered)
                    SupplyManagementReportSupplierGroupField.Supplier
                visible when (SupplyManagementReportSupplierGroupField.Supplier not entered)
                    AllTextForParameters
                        label is "Supplier"
                visible when (FromDate entered)
                    paragraph
                        FromDate
                            label is "FinalizedDateRange"
                        text of "_to_"
                        ThruDate
                            no label
                visible when (FromDate not entered)
                    AllTextForParameters
                        label is "FinalizedDateRange"
                EditDiversityCertificationDates
                    label is "EditDiversityCertificationDates"
                    display when blank
                RequireCurrentDiversityCertification
                    label is "RequireCurrentDiversityCertification"
                    display when blank
                PrintParameters
                    label is "PrintParameters"
                    display when blank
        form is SuppliersForEventResponsesHistoryRel.SupplierResponsesHistorySumForm
