LeaseCompanyTransferSummaryForm is a Form
    title is "LeaseCompanyTransferSummary"
    Layout
        paragraph
            text of "TheDetailsOfTheTransferedLeaseAreAsFollows"
                bold
        paragraph
            TransferDate
                label is "TransferDate"
                display only
        two column
            header4 of "FromLeaseCompanyDetails"
                center
            column1
                Lease
                    label is "FromLease"
                    display only
                Lease.Description
                    display only
                Lease.Company
                    label is "FromCompany"
                    display only
                visible when (ShowDebtAccount)
                    paragraph
                        Lease.DebtAccount
                            label is "LeaseLiability"
                            display as text
                            compact format
                visible when (ShortTermLiabilityAccounting)
                    paragraph
                        Lease.ShortTermLiabilityAccount
                            label is "ShortTermLiabilityAccount"
                            display as text
                            compact format
                    paragraph
                        Lease.LongTermLiabilityAccount
                            label is "LongTermLiabilityAccount"
                            display as text
                            compact format
                visible when (IsFinanceLease)
                    paragraph
                        Lease.InterestAccount
                            label is "InterestAccount"
                            display as text
                            compact format
                visible when (ToDeferredRentAccount entered)
                    paragraph
                        Lease.DeferredRentAccount
                            label is "DeferredRentAccount"
                            display as text
                            compact format
                visible when (IsOperatingLease)
                    paragraph
                        Lease.LongTermOperatingLeaseExpense
                            label is "LongTermOperatingLease"
                            display as text
                            compact format
            header4 of "ToLeaseCompanyDetails"
                center
            column2
                Lease
                    label is "ToLease"
                    display only
                Lease.Description
                    display only
                ToCompany.Company
                    label is "ToCompany"
                    display only
                visible when (ShowDebtAccount)
                    paragraph
                        ToDebtAccount
                            label is "LeaseLiability"
                            display as text
                            compact format
                visible when (ShortTermLiabilityAccounting)
                    paragraph
                        ToShortTermLiabilityAccount
                            label is "ShortTermLiabilityAccount"
                            display as text
                            compact format
                    paragraph
                        ToLongTermLiabilityAccount
                            label is "LongTermLiabilityAccount"
                            display as text
                            compact format
                visible when (IsFinanceLease)
                    paragraph
                        ToInterestAccount
                            label is "InterestAccount"
                            display as text
                            compact format
                visible when (ToDeferredRentAccount entered)
                    paragraph
                        ToDeferredRentAccount
                            label is "DeferredRentAccount"
                            compact format
                        display as text
                visible when (IsOperatingLease)
                    paragraph
                        ToLongTermOperatingLeaseExpense
                            label is "LongTermOperatingLease"
                            display as text
                            compact format
        blank line
        paragraph
            button of "ReleaseTransfer"
                valid when (CanAllowReleaseTransfer)
                action is Unreleased.ReleaseTransfer
