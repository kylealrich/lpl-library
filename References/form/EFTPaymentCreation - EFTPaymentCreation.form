EFTPaymentCreation is an ActionForm
    action is EFTPaymentCreation
    Layout
        header2 of "Main"
        paragraph
            EftCompany
                label is "E\\F\\TProcessingCompany"
            EftCompany.Name
                no label
                display as text
        single column
            PostingDate
                label is "PostDate"
            DepositDate
        paragraph
            DebitCashCode
            DebitCashCode.Description
                no label
                display as text
        paragraph
            CreditCashCode
            CreditCashCode.Description
                no label
                display as text
        paragraph
            ProcessLevel
                label is "PaymentProcessLevel"
            ProcessLevel.Name
                no label
                display as text
            BankTransactionCode
                label is "SelectPaymentCode"
                select is CashPaymentAndOriginReceivablesList
            BankTransactionCode.Description
                no label
                display as text
        paragraph
            SelectProcessLevel
            SelectProcessLevel.Name
                no label
                display as text
            TermsCode
                label is "SelectPaymentTerms"
            TermsCode.Description.Desc
                no label
                display as text
        paragraph
            NotAppldCd
                label is "ReceivedNotAppliedCode"
            NotAppldCd.Description
                no label
                display as text
        paragraph
            DiscReasCd
                label is "DiscountReasonCode"
                select is ReceivableReasonCodeDiscountList
            DiscReasCd.Description
                no label
                display as text
        single column
            OprCode
                label is "Operator"
            Reference
            CompanyIdOption
                label is "CompanyI\\DOption"
        paragraph
            FillLastBlock
                label is "FillA\\C\\HLastPhysicalBlock"
            BalancedTape
                label is "BalancedA\\C\\HTape"
            IncludeAddenda
                label is "IncludeA\\C\\HAddenda"
        header2 of "OptionalBankCharges"
        single column
            BankCharges
        single column
            GLAccount
                label is "GlobalLedgerAccount"
                compact format
        header2 of "UserFields"
        two column distributed
            column1
                CbUserField1
                    label is "CashLedgerUserField1"
            column2
                CbUserField2
                    label is "CashLedgerUserField2"
        two column distributed
            column1
                CbUserField3
                    label is "CashLedgerUserField3"
            column2
                CbUserField4
                    label is "CashLedgerUserField4"
        header2 of "FileNames"
        two column distributed
            column1
                ACHFileName
                BACSFileName
            column2
                NonTapeFileName
                SEPAFileName
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                EFTPaymentCreationReport_RDG
            column2 of 25%
                EFTPaymentCreationReport_XPT
