VehicleTrip is a CompositeForm
    is primary
    title is "Trip<DerivedFormTitle>"
    context form is VehicleTripContextForm
        background is theme color
    Actions
        action is Created.StartTrip
        navigation is PrintDeliveryDocument
            action icon is print
            label is "_"
            mouse over text is "Print_Delivery_Checklist"
            hide when invalid
        action is PrintIDMDeliveryChecklist
            action icon is print
            hide when invalid
        action is save
            label is "_"
    VehicleShipmentManagement is a Panel
        title is "-"
        list is VehicleTripLineRel.VehicleShipment
            helper list is ShipmentLoadConsolidationRel.LoadConsolidation
                action is SelectLoadNumber
