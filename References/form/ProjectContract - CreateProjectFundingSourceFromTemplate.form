CreateProjectFundingSourceFromTemplate is an ActionForm
    action is CreateProjectFundingSourceFromTemplate
    title is "CreateProjectFundingSourceFromTemplate"
    Layout
        three column
            PrmProjectFundingSourceTemplate
                label is "ProjectFundingSourceTemplate"
                show up to 15 characters
            PrmProjectFundingSourceTemplate.Description
                no label
                display as text
            PrmProjectFundingSourceTemplate.Type
                no label
                display as text
        three column
            PrmFinanceDimension2
                label is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
                show up to 15 characters
            PrmFinanceDimension2.Description
                no label
                display as text
            PrmFinanceDimension2.FundingType
                no label
                display as text
        three column
            PrmPriority
                label is "Priority"
            PrmFundingGroup
                label is "FundingGroup"
            paragraph
                PrmBillingGroup
                    label is "BillingGroup"
                PrmBillingGroup.Description
                    no label
                    display as text
        two column
            PrmPercent
                label is "Percent"
            paragraph
                PrmAmount
                    label is "FundedAmount"
                PrmDefaultAmountFromProjectContract
                    label is "DefaultAmountFromProjectContract"
        visible when (PrmFinanceDimension2.IsExternalGrantOrCustomer)
            single column
                PrmCustomerType
                    label is "CustomerType"
                    show up to 15 characters
            visible when (PrmFinanceDimension2.IsInforTax)
                paragraph
                    PrmBuyerTaxCode
                        label is "BuyerTaxCode"
                        show up to 10 characters
                    PrmBuyerTaxCode.Description
                        no label
                        display as text
            visible when (PrmFinanceDimension2.IsVertex)
                paragraph
                    PrmBuyerTaxLocation
                        label is "BuyerTaxLocation"
                    PrmBuyerTaxLocation.Description
                        no label
                        display as text
                    PrmBuyerTaxLocation.TaxCode
                        label is "BuyerLocationTaxCode"
                        display as text
                    PrmBuyerTaxLocation.AddressCode
                        label is "BuyerLocationAddressCode"
                        display as text
            visible when (PrmFinanceDimension2.IsOneSource)
                paragraph
                    PrmBuyerTaxLocation
                        label is "BuyerTaxLocation"
                    PrmBuyerTaxLocation.Description
                        no label
                        display as text
                    PrmBuyerTaxLocation.TaxCode
                        label is "BuyerLocationTaxCode"
                        display as text
                    PrmBuyerTaxLocation.AddressCode
                        label is "BuyerLocationAddressCode"
                        display as text
            visible when (PrmFinanceDimension2.CompanyCustomerGroup.ReceivableCompany.GeneralLedgerCompany.RequireHSNSACCode)
                paragraph
                    PrmHSNSACCode
                        label is "HSNSACCode"
                    PrmHSNSACCode.Description
                        no label
                        display as text
            paragraph
                PrmTermsCode
                    label is "PaymentTerms"
                PrmTermsCode.Description.Desc[1]
                    no label
                    display as text
        two column
            PrmPostingProject
                label is "DefaultMilestone<FinanceEnterpriseGroup.ProjectLabel>"
                select is PostingProjectsRel.ContractPostingProjectSelect
                show up to 15 characters
            PrmPostingProject.Description
                no label
                display as text
        visible when (RevenueRecognition.Separate)
            two column
                PrmClearingProject
                    label is "Clearing<FinanceEnterpriseGroup.ProjectLabel>"
                    select is PostingProjectsRel.ContractPostingProjectSelect
                    show up to 15 characters
                PrmClearingProject.Description
                    no label
                    display as text
        visible when (PrmProjectFundingSourceTemplate.AllowanceProjectRequired)
            two column
                PrmAllowanceProject
                    label is "Allowance<FinanceEnterpriseGroup.ProjectLabel>"
                    select is PostingProjectsRel.ContractPostingProjectSelect
                    show up to 15 characters
                PrmAllowanceProject.Description
                    no label
                    display as text
        visible when (PrmProjectFundingSourceTemplate.RetainageProjectRequired)
            two column
                PrmRetainageProject
                    label is "Retainage<FinanceEnterpriseGroup.ProjectLabel>"
                    select is PostingProjectsRel.ContractPostingProjectSelect
                    show up to 15 characters
                PrmRetainageProject.Description
                    no label
                    display as text
