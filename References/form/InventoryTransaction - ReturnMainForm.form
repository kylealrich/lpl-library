ReturnMainForm is a Form
    Layout
        text of "Issue_Transaction"
            header2
        two column distributed
            column1
                visible when (RecordExists)
                    display as text
                        paragraph
                            Company
                            Company.Name
                                no label
                        paragraph
                            OriginatingIssueLocation
                            OriginatingIssueLocation.Name
                                no label
                        paragraph
                            OriginatingIssueDocument
                visible when (not RecordExists)
                    paragraph
                        Company
                        Company.Name
                            no label
                            display as text
                    paragraph
                        OriginatingIssueLocation
                        OriginatingIssueLocation.Name
                            no label
                            display as text
                    paragraph
                        OriginatingIssueDocument
                            select is IssueInventoryTransactions
            column2
                display as text
                    paragraph
                        OriginatingIssueDocument.FromToCompanyLocation.FromToCompany
                            label is "FromCompany"
                        OriginatingIssueDocument.FromToCompanyLocation.FromToCompany.Name
                            no label
                    paragraph
                        OriginatingIssueDocument.FromToCompanyLocation.RequestingLocation
                            label is "RequestingLocation"
                        OriginatingIssueDocument.FromToCompanyLocation.RequestingLocation.Name
                            no label
        header2 of "_"
        text of "<DerivedInventoryReturnsTitle>"
            header2
        visible when (RecordExists)
            display as text
                paragraph
                    TransientReturnAllLines
                paragraph
                    TransientOverrideLocation
        visible when (not RecordExists)
            paragraph
                TransientReturnAllLines
            paragraph
                TransientOverrideLocation
        paragraph
            visible when (not TransientOverrideLocation)
                OriginatingIssueLocation
                    label is "InventoryLocation"
                    display as text
            visible when (TransientOverrideLocation)
                InventoryLocation
                    label is "InventoryLocation"
        two column distributed
            column1
                InventoryReturnDocumentType
                    label is "DocumentType"
                    display as text
                GeneralLedgerDate
                    label is "PostDate"
                paragraph
                    DefaultDistributionAccount
                        label is "DefaultCreditAccount"
                        compact format
            column2
                Company.GeneralLedgerCompany.Currency
                    label is "Currency"
                    display as text
                TransactionDate
                    label is "ReturnDateAndTime"
        two column distributed
            column1
                paragraph
                    Reference1
            column2
                paragraph
                    Reference2
