PayablesInvoicePaymentsSearch is a SearchForm
    title is "PaymentSearch"
    Layout
        five column distributed
            Company
            Vendor
            TransactionNumber
                label is "PaymentNumber"
            PayablesInvoice.Invoice
            CheckDate
                label is "PaymentDate"
        five column distributed
            ProcessLevel
            PayGroup
            CashCode
            BankTransactionCode
                label is "PaymentCode"
            Status
        header2 of "Additional"
            collapsible
                initially collapsed
                use show more or less affordance
            five column distributed
                PaymentAmountRange.FromAmount
                    label is "PaymentAmount_-_Minimum"
                PaymentAmountRange.ToAmount
                    label is "PaymentAmount_-_Maximum"
                PayablesInvoice.ExternalPurchaseOrder
                    label is "PurchaseOrder"
                IncomeCode
                PayablesInvoice.InvoiceHoldCode
                    label is "HoldCode"
            five column distributed
                PayablesInvoice.InvoiceDate
                    label is "InvoiceDate"
                DueDate
                    label is "DueDate"
                PayImmediately
                CanAddToCashRequirementsResult
                    label is "ReadyForPayment"
                TransientPaidLate
