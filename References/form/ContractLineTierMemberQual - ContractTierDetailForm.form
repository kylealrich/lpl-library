ContractTierDetailForm is a Form
    title is "_"
    Layout
        four column
            column1
                ContractLineMember.Company
                    label is "Company"
                    display when blank
                ContractLineMember.Location
                    label is "Location"
                    display when blank
                ContractLineMember.RequestingLocation
                    label is "RequestingLocation"
                    display when blank
                ContractLineMember.ManufacturerContract
                    label is "ManufacturerContract"
                    display when blank
            column2
                paragraph
                    Tier
                        label is "Tier"
                        display when blank
                    Tier.TierTitle
                        no label
                        display when blank
                ContractTierQualifier
                    label is "TierQualifier"
                    display when blank
                PurchaseTypeLabel
                    no label
                    display when blank
                MarkUpDiscount
                    no label
                    display when blank
            column3
                blank line
                blank line
                blank line
                blank line
            column4
                HasNextTierProgress
                    no label
                    display when blank
                LineMembersAlertText
                    no label
                    display when blank
                blank line
                blank line
