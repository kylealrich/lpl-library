UpgradeOverview is a Form
    title is "UpgradeActiveSpacesUsingStaging"
    Actions
        action is refresh
    Layout
        single column
            header2 of "Upgrade_process_using_a_Staging_Environment"
            blank line
            text of "This_process_is_recommended"
                bold
            blank line
            text of "Overview"
                bold
            blank line
            paragraph
                text of "This_option_incurs_minimal_to_no_downtime_for_end_users._The_new_release_content_is_first_staged_in_the_STC_environment,_where_you_can_merge_your_customizations_from_the_previous_version._Data_is_populated_through_full_extraction_from_the_Data_Lake_and_previewed._Upon_successful_validation,_the_content_is_transitioned_to_the_Active_Environment_during_non_working_hours,_typically_within_an_hour._End_users_can_start_using_the_system_without_disruption."
            blank line
            visible when (AnalyticsUpgradeStagingRel.IsDisableButtonText)
                text of "There_is_an_ongoing_upgrade_process._A_new_one_cannot_be_started_until_the_previous_one_is_completed_or_cancelled._Click_'Show_History'_to_see_more_details."
                    bold
                    color of red
            visible when (AnalyticsUpgradeStagingRel.IsActiveSpaceProcessRunning)
                text of "There_is_an_ongoing_upgrade_process_for_Analytics_Upgrade_Active_Space."
                    bold
                    color of red
            visible when (AnalyticsUpgradeStagingRel.IsCopyToAnotherTenantRunning)
                text of "There_is_an_ongoing_upgrade_process_for_Analytics_Copy_To_Another_Tenant."
                    bold
                    color of red
            visible when (last PostPfiWorkunitsRel.Status.Processing)
                text of "There_is_an_ongoing_process_for_Update_ION_Schemas."
                    bold
                    color of red
            visible when (last DataFlowToAnalyticsFLRel.Status.Processing)
                text of "There_is_an_ongoing_process_for_DataFlow_To_Analytics_Full_Load."
                    bold
                    color of red
            visible when (last DataFlowToAnalyticsILRel.Status.Processing)
                text of "There_is_an_ongoing_process_for_Data_Flow_To_Analytics_Incremental_Load."
                    bold
                    color of red
            blank line
            single column
                button of "Proceed_with_this_upgrade_process"
                    valid when (AnalyticsUpgradeStagingRel.IsEnableButton)
                    action is Create
                blank line
                button of "ShowHistory"
                    list is AnalyticsUpgradeStaging.AnalyticsUpgradeStagingList
        single column
            header2 of "FSM_Analytics_Version"
        two column
            column1
                text of "Latest_Metadata_Version_(SMD)"
                text of "Staging_Environment_Version_(STC)"
                text of "Active_Environment_Version_(ATC)"
            column2
                last ProductVersionSMDRel.Value
                    label is "_"
                last ProductVersionSTCRel.Value
                    label is "_"
                last ProductVersionATCRel.Value
                    label is "_"
                    raise red alert when (last AnalyticsUpgradeStagingRel.IsUpgradeCompleted)
                        mouse over text is "ActiveTenantContentVersion_isLower_than_theStandardMetaData"
        single column
            visible when (last AnalyticsUpgradeStagingRel.UpdateNotRequired)
                text of "Your_Active_Tenant_Version_<last ProductVersionATCRel.Value>_is_not_supported_to_be_upgraded_using_the_Upgrade_automation_workflows._Kindly_request_for_reprovisioning_of_new_ATC_spaces_in_your_tenant_through_ServiceNow"
                    color of red
        single column
            text of "_"
            button of "RefreshProductVersion"
                action is UpdateVersion
