SupplierMain is a Form
    disable field options
    Layout
        display as text
            three column distributed
                ReturnMaterialAuthorizationNumber
                ReturnFromLocation.RepresentativeText
                UserId
                    label is "CreatedBy"
            three column distributed
                ReferenceNumber
                paragraph
                    ReasonCode
                    ReasonCode.Description
                        no label
                BuyerAndName
                    label is "Buyer"
            three column distributed
                ReplacementPurchaseOrder.PurchaseOrder
                    label is "ReplacementPurchaseOrder"
                paragraph
                    VendorReturnPOCode
                    VendorReturnPOCode.Description
                        no label
                CreditReceived
            three column distributed
                VendorClaimType
                ReplaceGoods
                visible when (ReplaceGoods)
                    VendorRequestedReplacementDate
                        label is "ReplacementDate"
            three column distributed
                ShipOrHold
                    label is "ShipOrHoldGoods"
                paragraph
                    PurchaseFromLocation
                        label is "VendorReturnTo"
                    PurchaseFromName
                        no label
                VendorReturnShippingMethod
                    label is "ReturnShipVia"
            three column distributed
                ReplacementPurchaseOrderShippingMethod
                    label is "ReplacementShipVia"
                Contact
                PhoneNumber
            three column distributed
                OriginalFreightCreditAmount
                ReturnToVendorFreightCreditAmount
                HandlingAmount
                    label is "HandlingCharge"
            three column distributed
                ReplacementFreightAmount
                VendorReturnShippingMethod
                    label is "ShipVia"
                LoadingPort
                    label is "Port"
            three column distributed
                Carrier
                    label is "CarrierNumber"
                ShipmentDate
                    label is "ShipDate"
                ShippedBy
            three column distributed
                ShipmentBillOfLading
                    label is "BillOfLading"
                NatureOfTransactionCode
                ShipTerm
                    label is "ShipTerms"
            three column distributed
                StatisticalProcedure
                GlobalDocumentType
            three column distributed
                Dropship
                visible when (Dropship)
                    DropshipName
                        label is "Name"
                    DropshipAddress
                        form is PostalAddressMinimum
