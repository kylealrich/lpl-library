SelectContractForTierReporting is an ActionForm
    action is Create
    Layout
        display as text
            single column
                Contract.RepresentativeText
                    label is "ReportingContract"
        paragraph
            SelectedContractNumber
                select is ContractsForTierReporting
            SelectedContractNumber.Name
                no label
                display as text
        single column
            visible when (Contract.HasPercentTierQualifier)
                QualifiedOrCompetitiveContract
            visible when (not Contract.HasPercentTierQualifier)
                text of "ContractWillBeCreatedAsQualified"
            TransientUseAllItems
                label is "UseAllItems"
