PhonePaymentDistributionForm is a Form
    title is "CashLedgerPaymentDistribution"
    Layout
        display as text
            single column
                header2 of "Ledger"
                paragraph
                    GLTransactionAmount
                        label is "Amount"
                paragraph
                    GLFinanceCodeBlock
                        label is "FinanceStructure"
                        compact format
                blank line
                paragraph
                    Reference
                paragraph
                    Description
                header2 of "Tax"
                paragraph
                    TaxCode
                    TaxAccountingEntity
                        label is "TaxEntity"
                        select is TaxEntitiesRel.TaxCompanies
                    TaxableBankAmount
                        label is "TaxableAmount"
