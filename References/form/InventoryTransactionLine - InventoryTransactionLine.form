InventoryTransactionLine is a Form
    Layout
        two column distributed
            column1
                paragraph
                    visible when (InventoryTransaction.IsCostAdjustment)
                        TransactionCounter
                            label is "TransactionCounter"
                            display as text
                    visible when (IsBackOrderExist)
                        BackOrderExistMessage
                            no label
                            display as text
                    visible when (IsStorageCodeMismatch)
                        StorageCodeMismatchMessage
                            no label
                            display as text
                    visible when (DerivedToleranceMessage entered)
                        DerivedToleranceMessage
                            no label
                            display as text
                visible when (not InventoryTransaction.IsInventoryIntransitReceiving)
                    visible when (not InventoryTransaction.IsInventoryBinTransfer)
                        paragraph
                            Item
                                select is ItemsAtCompanyLocationRel.Items
                                when value changed
                                    refresh IsAWarehouseLocationItem
                                    refresh DMFromToCompanyLocationBinHasItemStorageLocation
                            Item.Description
                                no label
                                display as text
                    visible when (InventoryTransaction.IsInventoryBinTransfer)
                        visible when (RecordExists)
                            paragraph
                                Item
                                    display as text
                                Item.Description
                                    no label
                                    display as text
                        visible when (not RecordExists)
                            paragraph
                                Item
                                    select is ItemsAtCompanyLocationRel.Items
                                Item.Description
                                    no label
                                    display as text
                visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                    display as text
                        paragraph
                            Item
                            Item.Description
                                no label
                paragraph
                    TransientQuantityValue
                        label is "Quantity"
                    visible when (not InventoryTransaction.IsInventoryIntransitReceiving)
                        visible when (not InventoryTransaction.IsInventoryBinTransfer)
                            TransactionUOM
                                label is "TransactionUOM"
                                select is ValidTransactionUnitsOfMeasure
                                raise blue alert when (LocationStockUOMRounded)
                                    mouse over text is "<RoundingMessage>"
                    visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                        TransactionUOM
                            label is "TransactionUOM"
                            display as text
                visible when (not InventoryTransaction.IsInventoryIntransitReceiving)
                    visible when (false)
                        Item.CatchWeightCode
                    visible when (not Item.CatchWeightCode.NotCatchWeight)
                        paragraph
                            TransientSecondayQuantityValue
                                label is "SecondaryQuantity"
                            SecondaryUOM
                                label is "SecondaryUOM"
                visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                    visible when (not Item.CatchWeightCode.NotCatchWeight)
                        paragraph
                            SecondaryQuantity
                                label is "SecondaryQuantity"
                            SecondaryUOM
                                label is "SecondaryUOM"
                                display as text
                visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                    visible when (HasSupplementalUOM)
                        paragraph
                            DerivedQuantityInSupplementalUOM
                            DerivedSupplementalUOM
                                display as text
                visible when (false)
                    ItemLocation.BinTracked
                visible when (not InventoryTransaction.IsBinTransferOrInventoryTransfer)
                    visible when (ItemLocation.BinTracked)
                        visible when (not IsAWarehouseLocationItem)
                            paragraph
                                Bin
                                    select is BinsForItemLocationRel.BinListWithQuantity
                                text of "\\or"
                                MultipleBins
                        visible when (IsAWarehouseLocationItem)
                            paragraph
                                Bin
                                    select is BinHasItemStorageLocationRel.DistributionManagementBinList
                                text of "\\or"
                                MultipleBins
                visible when (InventoryTransaction.IsInventoryTransfer)
                    visible when (ItemLocation.BinTracked)
                        visible when (not IsAWarehouseLocationItem)
                            paragraph
                                Bin
                                    select is BinWithStockOnHandRel.BinListWithQuantity
                                text of "\\or"
                                MultipleBins
                        visible when (IsAWarehouseLocationItem)
                            paragraph
                                Bin
                                    select is BinHasItemStorageLocationWithStockOnHandRel.DistributionManagementBinList
                                text of "\\or"
                                MultipleBins
                visible when (InventoryTransaction.IsInventoryBinTransfer)
                    paragraph
                        visible when (ItemLocation.BinTracked)
                            visible when (not IsAWarehouseLocationItem)
                                Bin
                                    label is "FromBin"
                                    select is BinWithStockOnHandRel.BinListWithQuantity
                            visible when (IsAWarehouseLocationItem)
                                Bin
                                    label is "FromBin"
                                    select is BinHasItemStorageLocationWithStockOnHandRel.DistributionManagementBinList
                        visible when (not ItemLocation.IsSerialTracked)
                            TransactionUOM
                                label is "FromUOM"
                                select is ValidTransactionUnitsOfMeasure
                    paragraph
                        visible when (ItemLocation.BinTracked)
                            visible when (not IsAWarehouseLocationItem)
                                FromToCompanyLocationBin.FromToBin
                                    label is "ToBin"
                            visible when (IsAWarehouseLocationItem)
                                FromToCompanyLocationBin.FromToBin
                                    label is "ToBin"
                                    select is BinHasItemStorageLocationRel.DistributionManagementBinList
                        visible when (not ItemLocation.IsSerialTracked)
                            visible when (ItemLocation.TrackedInAlternateUnitsOfMeasure)
                                ToUOM
                                    label is "ToUOM"
                                    select is ValidTransactionUnitsOfMeasure
                paragraph
                    InventoryReasonCode
                        label is "ReasonCode"
                    display as text
                        InventoryReasonCode.Description
                            no label
                visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                    paragraph distributed
                        OpenQuantity
                            display as text
                        OriginalQuantity
                            display as text
                paragraph
                    visible when (not InventoryTransaction.IsInventoryTransferBinTransferAndIsInventoryIntransitReceiving)
                        OffsetAccount
                            label is "<InventoryTransaction.OffsetAccountLabel>"
                            compact format
                    visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                        paragraph
                            AddOnChargeUnitCost
                            text of "Per"
                visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                    single column
                        AddOnCostAccount
                            label is "AddOnCostAccount"
                            compact format
            column2
                visible when (not InventoryTransaction.IsInventoryIntransitReceiving)
                    single column
                        paragraph
                            GlobalTradeItemNumber
                                label is "GTIN"
                            GlobalTradeItemNumber.AbbreviatedDescription
                                no label
                                display as text
                visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                    single column
                        paragraph
                            display as text
                                GlobalTradeItemNumber
                                    label is "GTIN"
                                GlobalTradeItemNumber.AbbreviatedDescription
                                    no label
                visible when (InventoryTransaction.IsInventoryTransfer)
                    paragraph
                        InventoryTransaction.FromToCompanyLocation.FromToCompany.RepresentativeText
                            label is "ToCompany"
                            display as text
                        InventoryTransaction.FromToCompanyLocation.FromToLocation.RepresentativeText
                            label is "ToLocation"
                            display as text
                        visible when (DMFromToCompanyLocationBinHasItemStorageLocation)
                            FromToCompanyLocationBin.FromToBin
                                label is "ToBin"
                                select is DMFromToCompanyLocationBinHasItemStorageLocationRel.DistributionManagementBinList
                        visible when (not DMFromToCompanyLocationBinHasItemStorageLocation)
                            FromToCompanyLocationBin.FromToBin
                                label is "ToBin"
                paragraph
                    visible when (not InventoryTransaction.IsInventoryTransferOrIntransitReceiving)
                        visible when (InventoryTransaction.Status.Unreleased)
                            TransientPreciseUnitCost
                        visible when (InventoryTransaction.Status.Released)
                            visible when (not Item.IsCatchWeightItem)
                                TransientUnitCostValue
                                    label is "UnitCost"
                            visible when (Item.IsCatchWeightItem)
                                TransientUnitCostValue
                                    label is "UnitCostInSecondaryUOM"
                    visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                        display as text
                            TransientPreciseUnitCost
                    visible when (InventoryTransaction.IsInventoryTransfer)
                        visible when (InventoryTransaction.Status.Released)
                            display as text
                                TransientPreciseUnitCost
                    visible when (InventoryTransaction.IsIssuesOrAdjustments)
                        text of "\\or"
                        UseLastIssueCost
                            label is "UseLastIssueCost"
                visible when (InventoryTransaction.IsCostAdjustment)
                    paragraph
                        DerivedBaseCost
                    visible when (not Status.Released)
                        CurrentCost
                visible when (InventoryTransaction.IsIssuesOrAdjustments)
                    single column
                        EAMWorkOrderNumber
                visible when (IsInventoryReturns)
                    single column
                        EAMWorkOrder
                        EAMWorkOrderActivity
        two column distributed
            column1
                paragraph
                    Reference
                        label is "Reference"
            column2
                paragraph
                    LineComment
                        label is "LineComments"
                    visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                        blank line
                    visible when (InventoryTransaction.IsCostAdjustment)
                        LastUpdateDate
                        LastUpdateBy
        visible when (LineExists)
            visible when (not InventoryTransaction.IsInventoryIntransitReceiving)
                visible when (not IsBinTransferOrCostAdjustmentOrReceiptArchive)
                    header2 of "Stock_onHandQuantity"
                    text of "_"
                    display as text
                        four column table
                            column1
                                text of "_"
                                visible when (DisplayInTransactionUOM)
                                    text of "TransactionUOM"
                                        header4
                                visible when (DisplayInTrackedUOM)
                                    text of "TrackedUOM"
                                        header4
                                visible when (DisplayInItemStockUOM)
                                    text of "StockUOM"
                                        header4
                                visible when (DisplayInLocationStockUOM)
                                    text of "StockUOM"
                                        header4
                                visible when (DisplayInSecondaryUOM)
                                    text of "SecondaryUOM"
                                        header4
                                visible when (DisplayInSupplementalUOM)
                                    text of "SupplementalUOM"
                                        header4
                            column2
                                paragraph
                                    text of "_"
                                    text of "Before"
                                        header4
                                visible when (DisplayInTransactionUOM)
                                    paragraph
                                        text of "_"
                                        DerivedBeforeQuantityInTransactionUOM
                                            no label
                                visible when (DisplayInTrackedUOM)
                                    paragraph
                                        text of "_"
                                        DerivedBeforeQuantity
                                            no label
                                visible when (DisplayInItemStockUOM)
                                    paragraph
                                        text of "_"
                                        DerivedBeforeQuantity
                                            no label
                                visible when (DisplayInLocationStockUOM)
                                    paragraph
                                        text of "_"
                                        DerivedBeforeQuantity
                                            no label
                                visible when (DisplayInSecondaryUOM)
                                    paragraph
                                        text of "_"
                                        DerivedBeforeSecondaryQuantity
                                            no label
                                visible when (DisplayInSupplementalUOM)
                                    paragraph
                                        text of "_"
                                        DerivedBeforeQuantityInSupplementalUOM
                                            no label
                            column3
                                paragraph
                                    text of "_"
                                    text of "After"
                                        header4
                                visible when (DisplayInTransactionUOM)
                                    paragraph
                                        text of "_"
                                        DerivedAfterQuantityInTransactionUOM
                                            no label
                                visible when (DisplayInTrackedUOM)
                                    paragraph
                                        text of "_"
                                        DerivedAfterQuantity
                                            no label
                                visible when (DisplayInItemStockUOM)
                                    paragraph
                                        text of "_"
                                        DerivedAfterQuantity
                                            no label
                                visible when (DisplayInLocationStockUOM)
                                    paragraph
                                        text of "_"
                                        DerivedAfterQuantity
                                            no label
                                visible when (DisplayInSecondaryUOM)
                                    paragraph
                                        text of "_"
                                        DerivedAfterSecondaryQuantity
                                            no label
                                visible when (DisplayInSupplementalUOM)
                                    paragraph
                                        text of "_"
                                        DerivedAfterQuantityInSupplementalUOM
                                            no label
                            column4
                                text of "_"
                                visible when (DisplayInTransactionUOM)
                                    paragraph
                                        text of "_"
                                        TransactionUOM
                                            no label
                                visible when (DisplayInTrackedUOM)
                                    paragraph
                                        text of "_"
                                        DerivedUOM
                                            no label
                                visible when (DisplayInItemStockUOM)
                                    paragraph
                                        text of "_"
                                        DerivedUOM
                                            no label
                                visible when (DisplayInLocationStockUOM)
                                    paragraph
                                        text of "_"
                                        DerivedLocationStockUOM
                                            no label
                                visible when (DisplayInSecondaryUOM)
                                    paragraph
                                        text of "_"
                                        SecondaryUOM
                                            no label
                                visible when (DisplayInSupplementalUOM)
                                    paragraph
                                        text of "_"
                                        DerivedSupplementalUOM
                                            no label
