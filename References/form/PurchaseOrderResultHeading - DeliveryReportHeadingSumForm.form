DeliveryReportHeadingSumForm is a SummaryForm
    title is "DeliveryTicketsReport"
    Layout
        header3 of "DeliveryTicket"
            center
        two column distributed
            column1
                PurchaseOrderReceipt
                    label is "Receipt"
                paragraph
                    visible when (not HasPOCode)
                        PurchaseOrder
                            label is "PurchaseOrder"
                    visible when (HasPOCode)
                        DisplayPurchaseOrderAndPOCode
                            label is "PurchaseOrder"
                PurchaseOrderReceiptRel.ReceivedDate
                    label is "DateAndTimeReceived"
            column2
                PurchaseOrderReceiptRel.Status
                    label is "Status"
                PurchaseOrderReceiptRel.Vendor
                    label is "Vendor"
                text of "ReceivedBy:"
        blank line
        two column distributed
            column1
                RequisitionRel.Requester
                RequisitionRel.FromCompanyLocation.FromCompany
                RequisitionRel.DeliverTo
            column2
                RequisitionRel.RequestedDeliveryDate
                RequisitionRel.FromCompanyLocation.FromLocation
        blank line
        two column distributed
            column1
                header4 of "RequestingLocation"
                RequisitionRel.RequestingLocation
                    no label
                RequisitionRel.RequestingLocation.PostalAddress.DeliveryAddress.AddressLine1
                    no label
                RequisitionRel.RequestingLocation.PostalAddress.DeliveryAddress.AddressLine2
                    no label
                RequisitionRel.RequestingLocation.PostalAddress.DeliveryAddress.AddressLine3
                    no label
                RequisitionRel.RequestingLocation.PostalAddress.DeliveryAddress.AddressLine4
                    no label
                RequisitionRel.RequestingLocation.PostalAddress.Municipality
                    no label
                RequisitionRel.RequestingLocation.PostalAddress.StateProvince
                    no label
                RequisitionRel.RequestingLocation.PostalAddress.Country
                    no label
            column2
                header4 of "PurchaseFromLocation"
                PurchaseOrderRel.PurchaseFromLocation
                    no label
                PurchaseOrderRel.PurchaseFromLocation.VendorAddressLine1
                    no label
                PurchaseOrderRel.PurchaseFromLocation.VendorAddressLine2
                    no label
                PurchaseOrderRel.PurchaseFromLocation.VendorAddressLine3
                    no label
                PurchaseOrderRel.PurchaseFromLocation.VendorAddressLine4
                    no label
                PurchaseOrderRel.PurchaseFromLocation.VendorCity
                    no label
                PurchaseOrderRel.PurchaseFromLocation.VendorStateProvince
                    no label
                PurchaseOrderRel.PurchaseFromLocation.VendorPostalCode
                    no label
        blank line
        visible when (DeliveryCommentsExist)
            header4 of "Comments"
            list is PurchaseOrderReceiptDeliveryCommentsRel.DeliveryCommentForReceivingDocument
        list is PurchaseOrderResultDetailsRel.ReceivingDeliveryTicketLineList
