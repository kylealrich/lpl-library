ContractLineImportContext is a Form
    title is "ContractLineImports"
    Layout
        display as text
            three column
                paragraph
                    ContractGroup
                    ContractGroup.ProcurementGroup.Description
                        no label
                paragraph
                    ContractImport
                        label is "ContractInterface"
                    ContractImport.Name
                        no label
                paragraph
                    ContractImport.Supplier
                        label is "Supplier"
                    ContractImport.Supplier.SupplierName
                        no label
        visible when (ContractEntered)
            paragraph
                button of "ViewContractLine"
                    link is ContractLineNav
                    display as link
                display as text
                    Contract
                    ContractLine
                    ContractLineCreateDate
                        label is "ContractLineCreatedOn"
        paragraph
            ContractLineImport
                label is "Line"
                display as text
            UpdateFunction
                label is "UpdateFunction"
            visible when (not ContractImport.NoPurchaseType)
                CanUseForCostDefaulting
