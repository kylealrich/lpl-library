InventoryValuation is an ActionForm
    action is InventoryValuation
    Layout
        header2 of "Main"
        paragraph
            Company
            Company.Name
                no label
                display as text
        blank line
        three column
            column1
                paragraph
                    Location
                        show up to 10 characters
                    Location.Name
                        no label
                        display as text
            column2
                text of "\\or"
                    bold
            column3
                paragraph
                    ReportGrp
                        label is "ReportGroup"
                    ReportGrp.Name
                        no label
                        display as text
        blank line
        single column
            CostingMethod
        two column distributed
            column1
                paragraph
                    GlCategory
                        label is "GlobalLedgerCategory"
                    GlCategory.Description
                        no label
                        display as text
                paragraph
                    CurrencyCode
                        label is "Currency"
                    CurrencyCode.Description
                        no label
                        display as text
                PrintZeroQtys
                    label is "IncludeItemsWithZeroStockOnHand"
                ConsignmentItems
