ProjectUploadMain is a Form
    title is "Main"
    Layout
        two column distributed
            column1
                paragraph
                    ProjectUpload.RunGroup
                    ProjectUpload.Sequence
                        display only
        two column distributed
            column1
                paragraph
                    RecordType
                        label is "<FinanceEnterpriseGroup.ProjectLabel>Type"
                        show up to 5 characters
                    Project
                        label is "<FinanceEnterpriseGroup.ProjectLabel>"
                        show up to 10 characters
            column2
                paragraph
                    Description
                    Active
        visible when (RecordType.Posting)
            two column distributed
                column1
                    paragraph
                        SystemProject
                            label is "System<FinanceEnterpriseGroup.ProjectLabel>"
                        IncludeInBudgetEdit
                            label is "IncludeInBudgetEdit"
                        Watch
                        WatchedBy
                        WatchedBy.Name.PreferredFirstAndLastName
                            no label
                            display as text
                        FeesExempt
                        visible when (FinanceEnterpriseGroup.FrontEndSplits)
                            ExcludeFromFES
                                label is "ExcludeFromFrontEndSplit"
            single column
                DefaultCodeBlock
                    compact format
        three column distributed
            column1
                ParentProject
                    label is "Parent<FinanceEnterpriseGroup.ProjectLabel>"
            visible when (InEnterpriseStructure)
                column2
                    paragraph
                        ProjectDateRange.BeginDate
                            label is "BeginDate"
                        ProjectDateRange.EndDate
                            label is "EndDate"
            column3
                paragraph
                    AddressCode
                    AddressCode.Description
                        no label
                        display as text
        visible when (InEnterpriseStructure)
            single column
                visible when (RecordType.Posting)
                    LikeProjectField
                        label is "AddToStructureWhereThis<FinanceEnterpriseGroup.ProjectLabel>Exists"
                        select is PostingProjectRel.LikeProjectSelectList
            two column distributed
                visible when (RecordType.Summary)
                    column1
                        paragraph
                            BudgetIdentifier
                            BudgetIdentifier.Description
                                no label
                                display as text
            visible when (RecordType.Summary)
                single column
                    Contract
                visible when (Contract)
                    three column distributed
                        column1
                            paragraph
                                ContractID
                                ContractType
                        column2
                            paragraph
                                ContractAmount
                                ContractCurrency
                                    label is "Currency"
                    two column distributed
                        column1
                            paragraph
                                ContractBillingMethod
                                    label is "BillingMethod"
                                    show up to 10 characters
                                visible when (ContractBillingMethod.CostPlus)
                                    ContractMarkupPercent
                                        label is "MarkupPercent"
                                visible when (ContractBillingMethod.TimeAndMaterials)
                                    ContractMarkupPercent
                                        label is "MarkupPercent"
                                visible when (ContractBillingMethod.RateTable)
                                    ContractBillingRateTable
                                        label is "BillingRateTable"
                                        show up to 10 characters
                                    ContractMarkupPercent
                                        label is "MarkupPercent"
                    two column distributed
                        column1
                            paragraph
                                ContractRevenueRecognition
                                    label is "RevenueRecognition"
                                visible when (ContractRevenueRecognition.Separate)
                                    ContractRevenueMethod
                                        label is "RevenueMethod"
                        column2
                            RemainderOption
                    two column distributed
                        column1
                            SingleProjectInvoice
                        column2
                            visible when (ContractRevenueRecognition.Separate)
                                SingleProjectRevenue
                    single column
                        ContractMaximumFees
                            label is "MaximumFees"
                    paragraph
                        ContractAllocateExpenditures
                            label is "AllocateExpenditures"
                        ContractExcludeAP
                            label is "ExcludeUnpaidAP"
                        visible when (FinanceEnterpriseGroup.FrontEndSplits)
                            FrontEndSplits
                    three column distributed
                        column1
                            ProgramIncome
                        column2
                            ProgramIncomeType
            visible when (RecordType.Posting)
                two column distributed
                    column1
                        paragraph
                            ProjectStatus
                                label is "<FinanceEnterpriseGroup.ProjectLabel>Status"
                            ProjectStatus.Description
                                no label
                                display as text
                    column2
                        paragraph
                            Currency
                            Currency.Description
                                no label
                                display as text
                two column distributed
                    column1
                        paragraph
                            Billable
                            BillingMethod
                                label is "OverrideBillingMethod"
                    column2
                        paragraph
                            visible when (BillingMethod.CostPlus)
                                MarkupPercent
                            visible when (BillingMethod.TimeAndMaterials)
                                MarkupPercent
                            visible when (BillingMethod.RateTable)
                                BillingRateTable
                                    show up to 10 characters
                                MarkupPercent
                two column distributed
                    column1
                        BillingMaximum
                single column
                    TaxExempt
                        label is "OverrideTaxExempt"
                two column distributed
                    column1
                        paragraph
                            BuyerTaxCode
                                label is "OverrideBuyerTaxCode"
                                show up to 10 characters
                            BuyerTaxCode.Description
                                no label
                                display as text
                two column distributed
                    column1
                        paragraph
                            BuyerTaxLocation
                                label is "OverrideBuyerTaxLocation"
                            BuyerTaxLocation.Description
                                no label
                                display as text
                            BuyerTaxLocation.AddressCode
                                display as text
                two column distributed
                    column1
                        ProgramIncome
                            label is "ProgramIncome"
                    column2
                        ProgramIncomeType
                            label is "ProgramIncomeType"
                paragraph
                    PreAwardID
                    WorkType
                    ServiceType
                    UsageType
                two column
                    column1
                        FinanceDimension2
                            label is "RelatedFundingSource"
                    column2
                        FinanceDimension2.Description
                            no label
                            display as text
                single column
                    RevenueCodeBlockOption
                two column distributed
                    column1
                        RevenuePartialAccount
                            compact format
                    column2
                        RevenueFullAccount
                            compact format
            visible when (RecordType.Posting)
                single column
                    Capital
                    visible when (FinanceEnterpriseGroup.ExpenseManagementIntegrated)
                        ExpenseManagement
                three column distributed
                    column1
                        SalaryCap
                    visible when (HasSalaryCap)
                        column2
                            paragraph
                                SalaryCapDate
                                SalaryCap.Description
                                    no label
                                    display as text
                        column3
                            SalaryCap.MaximumAmount
                                no label
                                display as text
                single column
                    OverageCodeBlock
                        compact format
                    text of "DimensionsSetToBlank:"
                        align as label
                    OverageCodeBlockSetBlanks
                        no label
                single column
                    FringeBurden
                two column distributed
                    column1
                        paragraph
                            BurdenCode
                            BurdenCode.Description
                                no label
                                display as text
                    column2
                        BurdenCode.Rate
                            display as text
                single column
                    BurdenCodeBlock
                        compact format
                    text of "DimensionsSetToBlank:"
                        align as label
                    BurdenCodeBlockSetBlanks
                        no label
                    CertificationRequired
            visible when (RecordType.Posting)
                single column
                    PreAwardAssignmentDays
                    FinanceCodeBlock
                        compact format
                    text of "DimensionsSetToBlank:"
                        align as label
                    FinanceCodeBlockSetBlanks
                        no label
            visible when (RecordType.Summary)
                two column distributed
                    column1
                        paragraph
                            PersonResponsible
                            PersonResponsible.Name.PreferredFirstAndLastName
                                no label
                                display as text
                    column2
                        paragraph
                            ProjectTeam
                            ProjectTeam.Description
                                no label
                                display as text
                three column
                    column1
                        Watch
                    column2
                        WatchedBy
                    column3
                        WatchedBy.Name.PreferredFirstAndLastName
                            no label
                            display as text
        single column
            ErrorMessage
                display only
