ManualEventResponse is a CompositeForm
    is primary
    use for action Draft.ManualResponse
    use for action Draft.ManualResponseUpdate
    title is "EventResponse<DerivedEventText>"
    context form is ManualResponseContext
        background is theme color
    Actions
        action is Draft.Submit
            action icon is submit
            hide when invalid
        action is Submitted.ModifyResponse
            action icon is edit
            hide when invalid
        action is Submitted.WithdrawResponse
            action icon is undo
            hide when invalid
        action is Withdrawn.ReinstateResponse
            hide when invalid
        navigation is PrintResponse
            action icon is print
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
            hide when invalid
        action is next record
            label is "_"
            mouse over text is "Next_Record"
            hide when invalid
        action is refresh
            label is "_"
        action is save
            label is "_"
    EventResponse is a Panel
        form is ManualResponse
    LineResponses is a Panel
        visible when (DisplayBidAmountManualNoDate)
        template is LineOutputType
            business class is SourcingEventLineResponse
                list is LineResponse
    EventLineResponses is a Panel
        visible when (DisplayBidAmountManualWithDate)
        template is LineOutputType
            business class is SourcingEventLineResponse
                list is LineResponseWithDate
    EventQuestions is a Panel
        visible when (SEQuestionsOldExist)
        template is SourcingEvent.SourcingEventQuestion set
            business class is SourcingEventQuestionResponse
                form is Question
    Attachments is a Panel
        list is SourcingEventResponseAttachment set.AttachmentCardList
    AddOnCharges is a Panel
        visible when (HeaderAOCsAndLineResponsesExist)
        list is HeaderAddOnChargeResponseRel.SourcingEventResponseAOCs
