ParLocationForm is a CompositeForm
    use for action CreateParLocation
    title is "<LocationType>"
    context form is InventoryLocationContextForParCartCreation
        background is theme color
    Address is a Panel
        form is AddressForParLocation
    AccountGroups is a Panel
        list is GeneralLedgerCategory set.GeneralLedgerCategoriesForALocation
    ParItems is a Panel
        visible when (ParOrExchangeCart)
        list is BinGroupDetail set.CartParLocations
