ParametersForm is a Form
    title is "Parameters"
    Layout
        display as text
            two column distributed
                column1
                    header2 of "Main"
                    paragraph
                        CashCode.CashCodeAndDescription
                            label is "CashCode"
                        CashCode.Currency
                    paragraph
                        CashCodeGroup.CashCodeGroupAndDescription
                            label is "CashCodeGroup"
                    paragraph
                        BankTransactionCode.BankTransactionCodeName
                            label is "TransactionCode"
                    ProcessLevelProcessing
                    ProcessLevel
                    IncludeAllTransactions
                    FileName
                    blank line
                column2
                    header2 of "StaleDatingOptions"
                    single column
                        StaleDate
                        Days
                        PostDate
                            label is "PostingDate"
