CashCodeFundContextForm is a Form
    title is "FundAmounts"
    Layout
        visible when (EditLevel.Group)
            three column distributed
            paragraph
                CashCode
                CashCode.Description
                    no label
                    display as text
                EditGroup
                    display only
                AvailableAmount
        visible when (EditLevel.Fund)
            three column distributed
                paragraph
                    CashCode
                    CashCode.Description
                        no label
                        display as text
                paragraph
                    AccountingEntity
                    AccountingEntity.Name
                        no label
                        display as text
                paragraph
                    visible when (CashManagementGroup.FinanceEnterpriseGroup.FundDimension.AccountingUnit)
                        AccountingUnit
                        AccountingUnit.Description
                            no label
                            display as text
                    visible when (CashManagementGroup.FinanceEnterpriseGroup.FundDimension.Dimension1)
                        FinanceDimension1
                        FinanceDimension1.Description
                            no label
                            display as text
            visible when (not IsSourcCashCodeFund)
                three column distributed
                    column1
                        visible when (ParentCashCodeFund not entered)
                            paragraph
                                SourceCashCodeFund.CashCode
                                    label is "SourceCashCode"
                                SourceCashCodeFund.CashCode.Description
                                    no label
                                    display as text
                    column2
                        visible when (ParentCashCodeFund not entered)
                            paragraph
                                SourceCashCodeFund.CashCodeFund
                                    label is "SourceFund"
                                SourceCashCodeFund.CashCodeFund.FundValue
                                    no label
                                    display as text
                    column3
                        visible when (ViewSourceAvailableAmount)
                            EditAvailableAmount
                                label is "<EditAvailableAmountLabel>"
            three column distributed
                column1
                    ClaimOnCash
                    MinimumFundAmount
                    ExcludeFromEdit
                    FundUsedForCustodialAccounting
                column2
                    CommittedAmount
                    LineOfCreditAmount
                column3
                    PostedAmount
                    AvailableAmount
