ExpenseManagementInterfaceHeaderMainForm is a Form
    Layout
        paragraph
            VendorGroup
            VendorGroup.Description
                no label
                display as text
        paragraph
            PayablesCompany
            PayablesCompany.Name
                no label
                display as text
        paragraph
            Vendor
                select is VendorNumber
            Vendor.VendorName
                no label
                display as text
            PayVendor
            PayVendor.VendorName
                no label
                display as text
        paragraph
            BeginDate
                label is "DateRange"
            EndDate
                no label
        paragraph
            ExpenseManagementTravelPlan
            LastExpenseForTravelPlan
        blank line
        paragraph
            visible when (ShowInterfaceResult)
                button of "InterfaceResult"
                    link is ExpenseManagementInterfaceResultNav
                    display as link
            visible when (ShowErrorMessage)
                ErrorMessage
                    display as text
