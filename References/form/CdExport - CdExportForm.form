CdExportForm is a Form
    title is "ConfigurationDataExport_toFTP"
    Layout
        single column
            WorkDirectory.SubDirectory
            FileFormat
            FileName
            ExportApplicationData
                when value changed
                    ExportSnapshot =  false
                        when (ExportApplicationData)
            valid when (ExportApplicationData)
                ConfigAppSubType
            ExportAuthorizationSecurityData
                when value changed
                    ExportSnapshot =  false
                        when (ExportAuthorizationSecurityData)
            ExportMimeTypeData
                when value changed
                    ExportSnapshot =  false
                        when (ExportMimeTypeData)
            ExportProcessFlowData
                when value changed
                    ExportSnapshot =  false
                        when (ExportProcessFlowData)
            ExportWebServicesData
                when value changed
                    ExportSnapshot =  false
                        when (ExportWebServicesData)
            ExportReplicationData
                when value changed
                    ExportSnapshot =  false
                        when (ExportReplicationData)
            valid when (ExportReplicationData)
                ReplicationSet
            ExportSnapshot
                when value changed
                    ExportApplicationData =  false
                        when (ExportSnapshot)
                    ExportAuthorizationSecurityData =  false
                        when (ExportSnapshot)
                    ExportMimeTypeData =  false
                        when (ExportSnapshot)
                    ExportProcessFlowData =  false
                        when (ExportSnapshot)
                    ExportWebServicesData =  false
                        when (ExportSnapshot)
                    ExportReplicationData =  false
                        when (ExportSnapshot)
            valid when (ExportSnapshot)
                ConfigBundleSnapshot
            SuppressAuditLog
            ScrubPII
            ExportDeleted
            ExportAllStatus
            valid when (ExportApplicationData)
                visible when (not ExportAllStatus)
                    ExportActive
                    ExportInactive
                    ExportInvalid
