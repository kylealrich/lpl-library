EventLineResponseInformation is a Form
    Layout
        display as text
            three column distributed
                SourcingEventResponse.TermsAcceptedBySupplier
                    label is "AcceptTerms"
                NotifiedSupplier.Supplier.PreQualified
                    label is "SupplierPre\\-Qualified"
                NotifiedSupplier.Supplier.DiversityExist
                    label is "SupplierDiversityExists"
            three column distributed
                SourcingEventResponse.TotalBidAmount
                    label is "TotalBidForEvent"
                Status
                    label is "ResponseStatus"
                SourcingEventLine.Status
                    label is "LineStatus"
            three column distributed
                SourcingEventLine.OutputType
                visible when (NoBid)
                    ResponseAmount.NoBid
                    NoBidReason
                        label is "NoBidReason"
            three column distributed
                SourcingEventLine.ItemNumber
                    label is "Item"
                paragraph
                    SourcingEventLine.ItemDescription
                        label is "ItemDescription"
                    SourcingEventLine.ItemDescription2
                        no label
            visible when (SourcingEventLine.HasThirdDescription)
                single column
                    SourcingEventLine.ItemDescription3
                        label is "LongItemDescription"
            three column distributed
                paragraph
                    SourcingEventLine.CommodityCode
                        compact format
                    SourcingEventLine.CommodityCode.Description
                SourcingEventLine.GlobalTradeItemNumber
                    label is "EventGTIN"
                SourcingEventLine.GlobalTradeItemNumber.FullDescription
                    label is "EventGTINDescription"
            visible when (ResponseExists)
                three column distributed
                    visible when (SourcingEventLine.DisplayVendorItem)
                        paragraph
                            VendorItem
                                label is "VendorItem"
                            VendorItemDescription
                                label is "VendorItemDescription"
                    visible when (not SourcingEventLine.DisplayVendorItem)
                        VendorItemDescription
                            label is "ServiceDescription"
                    GlobalTradeItemNumber
                        label is "SupplierGTIN"
                    GlobalTradeItemNumber.FullDescription
                        label is "SupplierGTINDescription"
            visible when (ResponseExists)
                three column distributed
                    Quantity
                        label is "ResponseQuantity"
                    ResponseAmount.UnitPrice
                    ExtendedPrice
                visible when (SourcingEventResponse.DifferentResponseCurrency)
                    text of "PriceInResponseCurrency"
                        italics
                    three column distributed
                        CurrencyResponseUnitPrice
                        CurrencyExtendedPrice
                        SourcingEventResponse.EnteredCurrencyCode
                            no label
                visible when (LineHasAddOnCharge)
                    two column
                        ResponseUnitAOC
                            label is "AOCUnitPrice"
                        ResponseTotalAOC
                            label is "AOC"
                    two column
                        NetUnitPrice
                        TotalResponse
                            label is "TotalBid"
                visible when (IsAwarded)
                    three column distributed
                        AwardQuantity
                        AwardReason
                        AwardDate
                three column distributed
                    visible when (not IsAwarded)
                        AwardReason
                            label is "NonAwardReason"
                    visible when (SourcingEventLine.OutputType.PO)
                        DeliveryDateOverride
                            label is "SupplierDeliveryDate"
                visible when (IsAwarded)
                    visible when (ExistingPo)
                        paragraph
                            PurchaseOrder
                                label is "PONumber"
                            button of "ViewPO"
                                link is ViewPo
                                display as link
                    visible when (ToCm)
                        three column distributed
                            EffectiveDate
                            ExpirationDate
                    visible when (ParticipantInUse)
                        four column distributed
                            ParticipantGroup
                            Participant.Company
                            Participant.Location
                            Participant.RequestingLocation
                    visible when (ToCm)
                        three column distributed
                            paragraph
                                Contract
                                button of "ViewContract"
                                    link is ViewContract
                                    display as link
                            Contract.Name
                                label is "ContractName"
                            visible when (Committed)
                                LifetimeMaximumAmount
                        four column distributed
                            ContractType
                            ContractSubtype
                            ContractClassification
                            ContractSubclassification
                visible when (CommentsExist)
                    single column
                        ResponseComments
