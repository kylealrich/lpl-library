GeneralLedgerProjectExpenituresSearchForm is a Form
    title is "SearchFinanceTotals"
    Layout
        paragraph
            GeneralLedgerTotal.Project.ParentDisplayContract
                label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Contract"
            GeneralLedgerTotal.Project
                label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
            GeneralLedgerTotal.GeneralLedgerChartAccount
                label is "<actor.context.FinanceEnterpriseGroup.AccountLabel>"
            SearchDateRange.Begin
                label is "FromDate"
            SearchDateRange.End
                label is "ToDate"
            AccountingEntity
                label is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
            GeneralLedgerTotal.AccountingUnit
                label is "<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>"
            GeneralLedgerSubAccount
                label is "SubAccount"
            GeneralLedgerTotal.GeneralLedgerChartAccount.ChartSection
                label is "ChartSection"
            Account.AccountType
                label is "AccountType"
            GeneralLedgerTotal.Ledger
                label is "Ledger"
                select is LedgersList
            GeneralLedgerTotal.System
                label is "System"
