ReceivablePaymentRemittanceFormasText is a Form
    title is "<DerivedReceivablePaymentRemittance>"
    disable all actions
    Layout
        display as text
            three column distributed
                column1
                    Company.RepresentativeText
                        display as text
                    ReceivablePayment
                        label is "PaymentSequence"
                        display as text
                    ReceivableInvoiceType
                        label is "InvoiceType"
                    CertificationNumber
                column2
                    ReceivablePaymentHeader
                        label is "Batch"
                        display as text
                    ReceivablePaymentRemittance
                        label is "Remittance"
                        display as text
                    ReceivableInvoice
                        label is "Invoice"
                    CertificationDate
                column3
                    Status
                        display as text
