PortalPayablesInvoiceAddOnCharge is a Form
    title is "AddOnCharge"
    Layout
        header2 of "InvoiceInformation"
        display as text
            two column distributed
                column1
                    PayablesInvoice.Company
                    PayablesInvoice.Vendor
                    PayablesInvoice.Invoice
                    PayablesInvoice.InvoiceAmount.CurrencyAmount
                column2
                    PayablesInvoice.Company.Name
                        label is "CompanyName"
                    PayablesInvoice.Vendor.VendorName
                        label is "VendorName"
                    PayablesInvoice.Status
                    button of "ViewImage"
                        visible when (PayablesInvoice.InvoiceImageEntered)
                        view PayablesInvoice.PayablesInvoiceDocumentInvoiceTypeRel.Attachment.File
        single column
            blank line
        header2 of "AddOnChargeInformation"
        paragraph
            PurchaseOrder
                display as text
            PurchaseOrderLine.LineNumber
                label is "POLineNumber"
                display as text
            first PayablesInvoiceDetailsRel.PayablesInvoiceDetail
                label is "InvoiceLineNumber"
                display as text
        paragraph
            AddOnCharge
                label is "Code"
                display as text
            AddOnCharge.Description
                no label
                display as text
            AddOnCharge.TrackType
                label is "Type"
                display as text
        paragraph
            EnteredQuantity
                label is "InvoiceQuantity"
                display as text
            EnteredUnitCost
                label is "InvoiceCost"
                display as text
            TotalDistributionAmount
                label is "Amount"
                display as text
        header2 of "TaxInformation"
        paragraph
            TaxCode
                display as text
            EntityTaxCodeRel.AccruedOrInvoiced
                label is "TaxType"
            TaxAmount
                display as text
