SourcingEventWizard is a WizardForm
    use for action Draft.CreateFromWizard
    title is "CreateEvent"
    summary form is WizardSummary
    show steps
    Event is a Panel
        form is EventWizardMain
    Lines is a Panel
        list is SourcingEventLine set.EventWizardLines
    Terms is a Panel
        valid when (UseWizardTerms)
        list is SourcingEventArticle set.WizardTerms
            helper list is ArticleRel.SourcingEventArticles
                action is AttachToEventWithTerms
                action is AttachToEventWithoutTerms
    Attachments is a Panel
        valid when (UseWizardAttachments)
        list is SourcingEventAttachment set.WizardAttachments
    Questions is a Panel
        valid when (UseWizardQuestions)
        list is SourcingEventQuestion set.WizardQuestions
            helper list is SourcingQuestionsRel.EventHelperQuestions
                action is AttachToEvent
    Meetings is a Panel
        valid when (UseWizardMeetings)
        list is SourcingEventMeeting set.WizardMeetings
    Contacts is a Panel
        valid when (UseWizardContacts)
        list is SourcingEventContact set.WizardContacts
    Weighting is a Panel
        valid when (UseWizardWeighting)
        list is SourcingEventBuyerWeighting set.WizardWeighting
