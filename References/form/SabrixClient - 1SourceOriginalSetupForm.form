1SourceOriginalSetupForm is a Form
    title is "1SourceConfig"
    Layout
        three column distributed
            column1
                header4 of "SabrixClientTestParameter"
                single column
                    CallingSystemNumber
                    HostSystem
                    CompanyRole
                    ExternalCompany
                        label is "ExternalCompanyId"
                    ExternalCompanyName
                    FilterGroupName
                    FilterGroupOwner
                    MappingGroupName
                    MappingGroupOwner
                    IsAudited
                        label is "IsAudited"
                            left of field
                    TransactionType
            column2
                header4 of "TaxConfigurationDefaultParameter"
                TaxConfigurationRel.CallingSystem
                TaxConfigurationRel.HostSystem
                TaxConfigurationRel.CompanyRole
                TaxConfigurationRel.ExternalCompanyId
                TaxConfigurationRel.ExternalCompanyIdName
                TaxConfigurationRel.FilterGroupName
                TaxConfigurationRel.FilterGroupOwner
                TaxConfigurationRel.ProductMappingGroupName
                TaxConfigurationRel.ProductMappingGroupOwner
                TaxConfigurationRel.IsAudited
                    label is "IsAudited"
                        left of field
                text of "TransactionTypeHardCodedAs=GS"
            column3
                header4 of "TaxConfigurationVersionSavedParameter"
                first TaxConfigurationVersionRel.CallingSystem
                first TaxConfigurationVersionRel.HostSystem
                first TaxConfigurationVersionRel.CompanyRole
                first TaxConfigurationVersionRel.ExternalCompanyId
                first TaxConfigurationVersionRel.ExternalCompanyIdName
                first TaxConfigurationVersionRel.FilterGroupName
                first TaxConfigurationVersionRel.FilterGroupOwner
                first TaxConfigurationVersionRel.ProductMappingGroupName
                first TaxConfigurationVersionRel.ProductMappingGroupOwner
                text of "IsAuditedNotSavedOnHere"
                    bold
                    color of blue
                text of "TransactionTypeHardCodedAs=GS"
