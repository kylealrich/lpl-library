AddInterfaceToPurchaseOrderForm is an ActionForm
    action is AddToPurchaseOrder
    title is "AppendToPurchaseOrder"
    Layout
        single column
            text of "SelectedPurchaseRequest:<SelectedInterfacesByActorCount>"
                header4
                bold
                italics
                color of blue
        PrmCompany
            label is "Company"
        PrmPurchaseOrder
            label is "PurchaseOrder"
            show up to 10 characters
        two column
            PrmReleasePurchaseOrder
                label is "ReleasePurchaseOrder"
            PrmPOCodeOverride
                label is "POCodeOverride"
