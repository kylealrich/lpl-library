WarehouseShipmentLineDetail is a Form
    is primary
    title is "WarehouseShipment_<WarehouseShipment>_Line_<WarehouseShipmentLine>_Detail_<WarehouseShipmentLineDetail>"
    Layout
        display as text
            four column distributed
                column1
                    WarehouseShipment.Company.RepresentativeText
                        label is "Company"
                    WarehouseShipment.InventoryLocation.RepresentativeText
                        label is "Location"
                column2
                    WarehouseShipment
                        label is "ShipmentNumber"
                    WarehouseShipment.Status
                column3
                    WarehouseShipment.WarehouseDemand.DemandSystemCode
                        label is "SystemCode"
                    WarehouseShipment.WarehouseDemand.DemandDocument
                        label is "Document"
                column4
                    visible when (IsWarehouseStorageLocation)
                        WarehouseShipment.WarehouseWave
                            label is "Wave"
        header2 of "_"
        two column distributed
            paragraph
                WarehouseShipmentLine
                    label is "Line"
                    display as text
                WarehouseShipment.WarehouseBatch
                    label is "BatchNumber"
                    display as text
            visible when (HasComponentSequence)
                WarehouseShipmentLineComponent
                    label is "ComponentSequence"
                    display as text
                WarehouseShipmentLineComponent.Item.RepresentativeText
                    display as text
                visible when (IsLineComponentBinTracked)
                    WarehouseShipmentLineDetail.Bin
                        label is "Bin"
                visible when (IsLineComponentLotTracked)
                    paragraph
                        WarehouseShipmentLineDetail.Lot
                            label is "Lot"
                            select is ItemLotsRel.ItemLotList
                        WarehouseShipmentLineDetail.Sublot
                            label is "Sublot"
                visible when (IsLineComponentSerialTracked)
                    WarehouseShipmentLineDetail.Serial
                        label is "Serial"
                        select is ItemSerialNumbersRel.SerialNumbersList
                visible when (not IsLineComponentSerialTracked)
                    paragraph
                        visible when (not HasItemLocationStockUOM)
                            Quantity
                        visible when (HasItemLocationStockUOM)
                            TransientQuantityInTransactionUOM
                        visible when (not WarehouseShipmentLine.IsMultipleUOMTracked)
                            DerivedUOMDisplay
                                label is "U\\O\\M"
                                display as text
                        visible when (WarehouseShipmentLine.IsMultipleUOMTracked)
                            WarehouseShipmentLineDetail.UnitOfMeasure
                visible when (WarehouseShipmentLineComponent.Item.IsCatchWeightItem)
                    CatchWeightQuantity
            visible when (not HasComponentSequence)
                paragraph
                    WarehouseShipmentLine.Item
                        label is "Item"
                        display as text
                    WarehouseShipmentLine.Item.Description
                        no label
                        display as text
                visible when (WarehouseShipmentLine.Item.IsCatchWeightItem)
                    visible when (WarehouseShipmentLine.ItemLocation.BinTracked)
                        paragraph
                            visible when (not IsWarehouseStorageLocation)
                                WarehouseShipmentLineDetail.Bin
                                    label is "Bin"
                            visible when (IsWarehouseStorageLocation)
                                WarehouseShipmentLineDetail.Bin
                                    label is "Bin"
                                    select is BinHasItemStorageLocationRel.DistributionManagementBinList
                                    when value changed
                                        refresh WarehouseStorageBinRel.WarehouseStorageLocation
                                WarehouseStorageBinRel.WarehouseStorageLocation
                    visible when (WarehouseShipmentLine.ItemLocation.IsLotTracked)
                        paragraph
                            WarehouseShipmentLineDetail.Lot
                                label is "Lot"
                            WarehouseShipmentLineDetail.Sublot
                                label is "Sublot"
                    paragraph
                        Quantity
                        WarehouseShipmentLine.TransactionUOM
                            no label
                            display as text
                    paragraph
                        CatchWeightQuantity
                        WarehouseShipmentLine.CatchWeightUOM
                            no label
                            display as text
                    paragraph
                        visible when (IsCatchWeightQuantityOutOfTolerance)
                            CatchWeightOutOfToleranceMessage
                                no label
                                display as tag
                                    color of yellow
                visible when (not WarehouseShipmentLine.Item.IsCatchWeightItem)
                    visible when (WarehouseShipmentLine.ItemLocation.BinTracked)
                        paragraph
                            visible when (not IsWarehouseStorageLocation)
                                WarehouseShipmentLineDetail.Bin
                                    label is "Bin"
                            visible when (IsWarehouseStorageLocation)
                                WarehouseShipmentLineDetail.Bin
                                    label is "Bin"
                                    select is BinHasItemStorageLocationRel.DistributionManagementBinList
                                    when value changed
                                        refresh WarehouseStorageBinRel.WarehouseStorageLocation
                                WarehouseStorageBinRel.WarehouseStorageLocation
                                    label is "StorageLocation"
                                    display as text
                    visible when (WarehouseShipmentLine.ItemLocation.IsLotTracked)
                        paragraph
                            WarehouseShipmentLineDetail.Lot
                                label is "Lot"
                            WarehouseShipmentLineDetail.Sublot
                                label is "Sublot"
                    visible when (WarehouseShipmentLine.ItemLocation.IsSerialTracked)
                        WarehouseShipmentLineDetail.Serial
                            label is "Serial"
                    visible when (not WarehouseShipmentLine.ItemLocation.IsSerialTracked)
                        paragraph
                            visible when (not HasItemLocationStockUOM)
                                Quantity
                            visible when (HasItemLocationStockUOM)
                                TransientQuantityInTransactionUOM
                            visible when (not WarehouseShipmentLine.IsMultipleUOMTracked)
                                DerivedUOMDisplay
                                    label is "U\\O\\M"
                                    display as text
                            visible when (WarehouseShipmentLine.IsMultipleUOMTracked)
                                WarehouseShipmentLineDetail.UnitOfMeasure
                    visible when (false)
                        TransientOnline
                    visible when (WarehouseShipmentLine.IsTrackedByContainer)
                        TransientTrackingNumber
                            label is "TrackingNumber"
            column2
                display as text
                    visible when (IsWarehouseStorageLocation)
                        paragraph
                            WarehouseStorageLocationRel.DerivedWarehouseZoneAndDescription
                        paragraph
                            WarehouseStorageLocationRel.DerivedWarehouseZoneComponentAndDescription
                        paragraph
                            WarehouseStorageLocationRel.DerivedWarehouseZoneComponentTypeAndDescription
                            WarehouseStorageLocationRel.DerivedZoneComponentAisle
                            WarehouseStorageLocationRel.Row
                        visible when (DisplayExpirationDetails)
                            paragraph
                                WarehouseShipmentLineDetail.Lot.ManufacturingDate
                                WarehouseShipmentLineDetail.Lot.ShelfLife
                                WarehouseShipmentLineDetail.Lot.LotExpirationDate
                        visible when (IsLineUDITracked)
                            paragraph
                                InventoryUDIDetailRel2.ManufacturingDate
                                InventoryUDIDetailRel2.ExpirationDate
