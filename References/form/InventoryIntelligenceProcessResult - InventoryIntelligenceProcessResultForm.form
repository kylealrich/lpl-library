InventoryIntelligenceProcessResultForm is a Form
    is primary
    propagate as a drill
    title is "ProcessResult"
    Layout
        two column distributed
            column1
                paragraph
                    FinanceEnterpriseGroup
                        display as text
                paragraph
                    InventoryIntelligenceProcessResult
                        label is "ProcessResult"
                        display as text
                paragraph
                    Process
                        display as text
                paragraph
                    Step
                        display as text
                paragraph
                    LocationTypes
                        display as text
                paragraph
                    UserId
                        label is "User_ID"
                        display as text
                paragraph
                    APIVersion
                        display as text
            column2
                paragraph
                    Status
                        display as tag
                            color of green when (Status.Completed)
                            color of red when (Status.Failed)
                            color of gray when (Status.Terminated)
                            color of yellow when (Status.Processing)
                paragraph
                    visible when (Status.Failed)
                        FailedStep
                            display as text
                        FailedTimeStamp
                            display as text
                paragraph
                    StartTimeStamp
                        display as text
                paragraph
                    FinishedTimeStamp
                        display as text
                paragraph
                    RunTimeStamp
                        display as text
                paragraph
                    visible when (Process.Recommendations)
                        AnalysisDate.Begin
                            label is "Analysis_Begin_Date"
                            display as text
                        AnalysisDate.End
                            label is "Analysis_End_Date"
                            display as text
        paragraph
            ErrorMessage
                display as text
        paragraph
            visible when (ErrorItemsPresent)
                button of "ErrorItems"
                    link is ErrorItemLocations
