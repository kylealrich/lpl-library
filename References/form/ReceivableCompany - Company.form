Company is a CompositeForm
    is primary
    title is "Company"
    context form is CompanyContext
        background is theme color
    Actions
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is Create
            label is "_"
        action is save
            label is "_"
        action is refresh
            label is "_"
    Main is a Panel
        form is Main
    Address is a Panel
        form is Address
    Entry is a Panel
        form is Entry
    Process is a Panel
        form is Process
    Apply is a Panel
        form is Apply
    GL is a Panel
        title is "Accounts"
        form is GL
    Finance is a Panel
        title is "FinanceCharge"
        form is Finance
    EFT is a Panel
        title is "ElectronicFundTransfer"
        form is EFT
    BOE is a Panel
        title is "BillOfExchange"
        form is BOE
    ProcessLevels is a Panel
        list is ReceivableProcessLevel set.ProcessLevels
    Project is a Panel
        visible when (IsProjectInvoicing)
        form is ProjectBillingForm
    IntercompanyBilling is a Panel
        form is IntercompanyBillingForm
    SubleaseBilling is a Panel
        form is SubleaseBillingForm
    DocumentTemplates is a Panel
        visible when (UseIDM)
        form is DocumentTemplatesForm
