InvoiceAddresses is a Form
    restrict action Unreleased.Create
    restrict action Unreleased.CreateInvoiceFromInvoice
    restrict action Unreleased.CreateInvoiceFromOrder
    restrict action Unreleased.Delete
    restrict action Unreleased.ReleaseBatch
    restrict action Unreleased.ReleaseBatchAndBypassPrint
    restrict action Unreleased.TriggerSyncInvoiceBOD
    restrict action Unreleased.Release
    restrict action Unreleased.GroupPricing
    restrict action Unreleased.RemoveHoldCode
    restrict action Released.Delete
    restrict action PrintInvoice
    restrict action GetStatusPrintedBillingInvoiceRegisterIDM
    restrict action GetStatusEditedBillingInvoiceRegisterIDM
    restrict action PutOnHold
    restrict action RemoveHoldCode
    restrict action UpdateParallelReference
    restrict action SendInvoiceToPrinter
    restrict action SendInvoiceToEmail
    Layout
        display as text
            two column distributed
                column1
                    paragraph
                        BillingInvoice.InvoicePrefix
                            label is "Invoice"
                        ConditionalInvoiceNumber
                            no label
                column2
                    Status
                        label is "Status"
            two column distributed
                column1
                    header2 of "BillTo"
                    visible when (BillTo entered)
                        paragraph
                            BillTo
                                no label
                            BillTo.Name
                                no label
                        BillTo.PostalAddress.DisplayAddressMultiline
                            no label
                        paragraph
                            visible when (BillTo.PostalAddress.County entered)
                                BillTo.PostalAddress.County
                                    no label
                        paragraph
                            visible when (BillTo.PostalAddress.Region entered)
                                BillTo.PostalAddress.Region
                                    no label
                        paragraph
                            visible when (BillTo.PostalAddress.Country entered)
                                BillTo.PostalAddress.Country
                                    no label
                        paragraph
                            visible when (BillTo.EmailAddress entered)
                                BillTo.EmailAddress
                                    no label
                        paragraph
                            visible when (BillTo.Contact entered)
                                BillTo.Contact
                                    no label
                column2
                    header2 of "ShipTo"
                    visible when (CustomerShipTo entered)
                        paragraph
                            CustomerShipTo
                                no label
                            CustomerShipTo.Name
                                no label
                        CustomerShipTo.ShipToAddress.DisplayAddressMultiline
                            no label
                        paragraph
                            visible when (CustomerShipTo.ShipToAddress.Country entered)
                                CustomerShipTo.ShipToAddress.Country
                                    no label
                        paragraph
                            visible when (CustomerShipTo.ShipToAddress.Region entered)
                                CustomerShipTo.ShipToAddress.Region
                                    no label
                        paragraph
                            visible when (CustomerShipTo.PhoneNumber.InternationalPrefix entered)
                                CustomerShipTo.PhoneNumber.InternationalPrefix
                                    no label
                                    display as text
                            visible when (CustomerShipTo.PhoneNumber.SubscriberNumber entered)
                                CustomerShipTo.PhoneNumber.SubscriberNumber
                                    no label
                                    display as text
                            visible when (CustomerShipTo.PhoneNumber.Extension entered)
                                CustomerShipTo.PhoneNumber.Extension
                                    no label
                                    display as text
                        paragraph
                            visible when (CustomerShipTo.EmailAddress entered)
                                CustomerShipTo.EmailAddress
                                    no label
                        paragraph
                            visible when (CustomerShipTo.FaxNumber.InternationalPrefix entered)
                                CustomerShipTo.FaxNumber.InternationalPrefix
                                    no label
                                    display as text
                            visible when (CustomerShipTo.FaxNumber.SubscriberNumber entered)
                                CustomerShipTo.FaxNumber.SubscriberNumber
                                    no label
                                    display as text
                            visible when (CustomerShipTo.FaxNumber.Extension entered)
                                CustomerShipTo.FaxNumber.Extension
                                    no label
                                    display as text
