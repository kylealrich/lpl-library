MobileCashCodeDetail is a Form
    title is "CashCodeDetail"
    Layout
        header2 of "BankAccount"
        single column
            BankAccountNumber
        two column distributed
            Currency
                display as text
            Currency.Description
                no label
                display as text
        display as text
            two column distributed
                FinancialInstitution
                FinancialInstitutionBranch
            two column distributed
                Sepa
                SwiftID
            single column
                BankAccountValidationType
                    label is "AccountValidationType"
            two column distributed
                SendingFinancialInstitution
                SendingFinancialInstitutionBranch
                    label is "SendingBranch"
        blank line
        two column distributed
            ReturnOnInvestmentRate
            Active
        two column distributed
            SummarizeLedgerBalances
            AutoTransactionNumbering
                label is "CashLedgerAutoTransactionNumbering"
        paragraph
            NewCashLedgerPaymentForVoid
        visible when (CashManagementGroup.CashLedgerPaymentApprovalRequired)
            paragraph
                CancelRejectedPayments
        paragraph
            NewEFTTransactionForVoid
        header2 of "BankFundTransfer"
        paragraph
            BankFundTransferRel.FromBankTransactionCode
            BankFundTransferRel.FromBankTransactionCode.Description
                no label
                display as text
            button of "BankFundTransfer"
                form invoke is BankFundTransferDetails
        paragraph
            BankFundTransferRel.ToBankTransactionCode
            BankFundTransferRel.ToBankTransactionCode.Description
                no label
                display as text
        paragraph
            BankFundTransferRel.ExpenseBankTransactionCode
            BankFundTransferRel.ExpenseBankTransactionCode.Description
                no label
                display as text
        header2 of "ReceiverBankDetails"
            single column
                ReceiverBank
                ReceiverEmailAddress
