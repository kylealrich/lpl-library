ActivityComposite is a CompositeForm
    title is "Activity"
    context form is ActivityForm
    Log is a Panel
        title is "Log"
        form is ActivityLog
    Variable is a Panel
        visible when (IsStartOrAssignNode)
        list is PfiActivityVariable set.ActivityVariableList
    UserActions is a Panel
        visible when (IsUserActionOrHRUserAction)
        list is PfiQueueRel.QueueList
