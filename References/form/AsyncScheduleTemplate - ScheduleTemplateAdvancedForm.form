ScheduleTemplateAdvancedForm is a Form
    is primary
    title is untranslatable:"<ScheduleType>"
    Layout
        paragraph
            AsyncScheduleTemplate
            Description
        paragraph
            ScheduleType
                display only
            visible when (ScheduleType.Exclusion)
                ExcludeWhen
        paragraph
            visible when (ScheduleType.Template)
                text of "ViewAllSelectionsAsASetOfFilters.OnlyDatesThatPassAllFiltersWillBeUsedToScheduleAnAction."
            visible when (ScheduleType.Exclusion)
                visible when (ExcludeWhen.AnyMatch)
                    text of "SettingAValueTo_'Every'_meansThatPortionWillNotBeUsedWhenApplyingTheExclusion."
                visible when (ExcludeWhen.AllMatch)
                    text of "ViewAllSelectionsAsASetOfFilters.OnlyDatesThatPassAllFiltersWillBeExcluded."
        visible when (ScheduleType.Exclusion)
            paragraph
                AsyncRollOption.RollType
                    when value changed
                        AsyncRollOption.RollInterval =  AsyncRollOption.DeriveRollInterval
                        AsyncRollOption.RollAttempts =  AsyncRollOption.DeriveRollAttempts
                visible when (not AsyncRollOption.RollType.Skip)
                    AsyncRollOption.RollInterval
                    AsyncRollOption.RollAttempts
                text of "WhenUsedInAnExclusionSet,TheExclusionSetRollOptionsOverrideTheOptionsSpecifiedHere."
        header2 of "Date"
        paragraph
            ScheduleYearType
                when value changed
                    refresh DisplayScheduleYear
                    refresh DisplayYearElement2
            visible when (ScheduleType.Exclusion)
                visible when (ExcludeWhen.AnyMatch)
                    visible when (ScheduleYearType.EveryYear)
                        text of "NotUsed"
            visible when (DisplayScheduleYear)
                visible when (not ScheduleYearType.Range)
                    AsyncScheduleYear.ScheduleYear[1]
                        no label
                visible when (ScheduleYearType.Range)
                    AsyncScheduleYear.ScheduleYear[1]
                        label is "Range"
                    text of "-"
                visible when (DisplayYearElement2)
                    AsyncScheduleYear.ScheduleYear[2]
                        no label
                visible when (ScheduleYearType.Multiple)
                    AsyncScheduleYear.ScheduleYear[3]
                        no label
                    AsyncScheduleYear.ScheduleYear[4]
                        no label
                    AsyncScheduleYear.ScheduleYear[5]
                        no label
        paragraph
            ScheduleMonth
                label is "Month"
                when value changed
                    refresh DisplayScheduleMonth
                    refresh DisplayMonthElement2
            visible when (ScheduleType.Exclusion)
                visible when (ExcludeWhen.AnyMatch)
                    visible when (ScheduleMonth.EveryMonth)
                        text of "NotUsed"
            visible when (DisplayScheduleMonth)
                visible when (not ScheduleMonth.Range)
                    ScheduleMonthsArray.AsyncScheduleMonthSelect[1]
                        no label
                visible when (ScheduleMonth.Range)
                    ScheduleMonthsArray.AsyncScheduleMonthSelect[1]
