ContractGrowthRebateDueComposite is a CompositeForm
    title is "<mode>ContractRebateDue"
    context form is ContractGrowthRebateDueContext
        background is theme color
    ManualRebatesReceived is a Panel
        list is ContractRebateReceivedRel.ContractRebatesReceived
    PayablesRebatesReceived is a Panel
        visible when (HasAPRebateReceipts)
        list is APRebateReceivedRel.ContractRebateReceivedList
    ReceivablesRebatesReceived is a Panel
        visible when (HasARRebateReceipts)
        list is ARRebateReceivedRel.RebateNonCashARReceivablePaymentsList
    CashLedgerRebatesReceived is a Panel
        visible when (HasCLRebateReceipts)
        list is CLRebateReceivedRel.ContractRebateReceivedList
    InvoiceLineDetail is a Panel
        visible when (ShowInvoiceLineDetails)
        list is ContractRebateDueInvoiceLinesRel.ContractRebateDueInvoiceLineList
    InvoiceDetail is a Panel
        visible when (ShowInvoiceDetails)
        list is ContractRebateDueInvoiceLinesRel.ContractRebateDueInvoiceList
    QualifierProgress is a Panel
        visible when (ShowQualifierProgress)
        list is ContractRebateDueQualifiersRel.ContractRebateDueQualifierList
