MassTransferSelectionForm is a Form
    Layout
        paragraph
            MassSelection
        paragraph
            Company
            ToCompany
        paragraph
            AssetLocation
            ToLocation
                no label
        paragraph
            AssetDivision
                select is Divisions
            ToDivision
                no label
                select is ToDivisions
        paragraph
            AssetType
                compact format
                select is AssetTypesWithinCompany
            ToAssetType
                no label
                compact format
                select is ToCompanyAssetTypesWithinCompany
        paragraph
            AssetGroup
                select is AssetGroups
            ToAssetGroup
                no label
                select is ToAssetGroups
        paragraph
            button of "Books"
                list is MassSelectionBookDetailsRel.IntercompanyTransferBookss
        paragraph
            SelectAll
        blank line
