MobileParLocationForms is a CompositeForm
    title is "<LocationType>"
    context form is MobileInventoryLocationContext
        background is theme color
    restrict action CreateInventoryLocation
    restrict action CreateByCopy
    restrict action UpdateTaxCodeUsingVertex
    restrict action DeleteVertexResultsFiles
    restrict action Delete
    Actions
        action is save
            label is "_"
            hide when invalid
    Main is a Panel
        form is MobileMain
    Address is a Panel
        form is Address
    Options is a Panel
        visible when (not ParOrExchangeCart)
        form is MobileOptions
    WorkOrder is a Panel
        visible when (WorkOrderVisible)
        form is WorkOrder
    SubstituteLocations is a Panel
        visible when (SubstituteLocationsVisible)
        list is SubstituteLocationsRel.MobileSubstituteLocationList
    AccountGroups is a Panel
        visible when (not ParOrExchangeCart)
        list is GeneralLedgerCategory set.MobileGeneralLedgerCategoriesForALocation
    StorageCodes is a Panel
        visible when (not ParOrExchangeCart)
        list is StorageCode set.MobileStorageCodeList
    Bins is a Panel
        visible when (not ParOrExchangeCart)
        list is Bin set.MobileBinList
    BinGroups is a Panel
        visible when (not ParOrExchangeCart)
        list is BinGroup set.MobileBinGroupList
    BinReorderPolicies is a Panel
        visible when (not ParOrExchangeCart)
        list is BinReorderPolicy set.MobileBinReorderPoliciesForALocation
    ParItems is a Panel
        visible when (ParOrExchangeCart)
        list is BinGroupDetail set.MobileCartParLocations
    KitItems is a Panel
        visible when (not ParOrExchangeCart)
        list is KitItemRel.MobileKitItemList
    BurdenCodes is a Panel
        visible when (not ParOrExchangeCart)
        list is BurdenCode set.MobileBurdens
    MultipleTaxAreas is a Panel
        visible when (MultipleTaxAreasVisible)
        list is VertexTaxAreaIdResultsRel.MobileTaxAreaResultList
    VertexError is a Panel
        visible when (VertexErrorVisible)
        list is VertexWebserviceErrorRel.MobileVertexTaxAreaIdErrorList
