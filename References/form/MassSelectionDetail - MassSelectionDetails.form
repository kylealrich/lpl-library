MassSelectionDetails is a Form
    is primary
    Layout
        MassSelection
            display as text
        MassSelectionType
            display as text
        visible when (MassSelection.IsAdjustmentTransferOrDisposal)
            paragraph
                MassSelection.AssetLease.LeaseCompany
                    label is "Company,Lease"
                    display as text
                MassSelection.AssetLease.Lease
                    no label
                    display as text
                MassSelection.AssetLease.Lease.Description
                    no label
                    display as text
        two column
            column1
                paragraph
                    MassSelection.Company
                        display as text
                    MassSelection.Company.GeneralLedgerCompany.Name
                        no label
                        display as text
                paragraph
                    MassSelection.AssetLocation
                        label is "Location"
                        display as text
                    MassSelection.AssetLocation.Description
                        no label
                        display as text
                paragraph
                    MassSelection.LocationDetail
                        display as text
                    MassSelection.LocationDetail.Description
                        no label
                        display as text
                paragraph
                    MassSelection.AssetDivision
                        label is "Division"
                        display as text
                    MassSelection.AssetDivision.Description
                        no label
                        display as text
                paragraph
                    MassSelection.AssetGroup
                        label is "AssetGroup"
                        display as text
                    MassSelection.AssetGroup.Description
                        no label
                        display as text
                paragraph
                    MassSelection.AssetType
                        label is "Type"
                        display as text
                        compact format
                    MassSelection.AssetType.Description
                        no label
                        display as text
                paragraph
                    MassSelection.AssetAccountGroup
                        display as text
                    MassSelection.AssetAccountGroup.Description
                        no label
                        display as text
                paragraph
                    MassSelection.DistributionAccount
                        label is "Account"
                        display as text
                        compact format
                paragraph
                    MassSelection.AssetAccountingUnitGroup
                        label is "AssetDimensionGroup"
                        display as text
                    MassSelection.AssetAccountingUnitGroup.Description
                        no label
                        display as text
            column2
                visible when (MassSelectionType.Transfer)
                    paragraph
                        MassSelection.ToCompany
                            label is "ToCompany"
                            display as text
                        MassSelection.ToCompany.GeneralLedgerCompany.Name
                            no label
                            display as text
                    paragraph
                        MassSelection.ToLocation
                            display as text
                        MassSelection.ToLocation.Description
                            no label
                            display as text
                    paragraph
                        MassSelection.ToLocationDetail
                            display as text
                        MassSelection.ToLocationDetail.Description
                            no label
                            display as text
                    paragraph
                        MassSelection.ToDivision
                            display as text
                        MassSelection.ToDivision.Description
                            no label
                            display as text
                    paragraph
                        MassSelection.ToAssetGroup
                            display as text
                        MassSelection.ToAssetGroup.Description
                            no label
                            display as text
                    paragraph
                        MassSelection.ToAssetType
                            label is "ToType"
                            display as text
                            compact format
                        MassSelection.ToAssetType.Description
                            no label
                            display as text
                    paragraph
                        MassSelection.ToAssetAccountGroup
                            display as text
                        MassSelection.ToAssetAccountGroup.Description
                            no label
                            display as text
                    paragraph
                        MassSelection.ToDistributionAccount
                            label is "ToAccount"
                            display as text
                            compact format
                    paragraph
                        MassSelection.ToAssetAccountingUnitGroup
                            label is "ToAssetDimensionGroup"
                            display as text
                        MassSelection.ToAssetAccountingUnitGroup.Description
                            no label
                            display as text
        single column
            MassSelection.AssetCategory
                label is "Category"
                display as text
            MassSelection.Simulated
                label is "Simulated"
            paragraph
                Asset
                    label is "Asset"
                Asset.Description
                    no label
                    display as text
            TagNumber
                label is "Tag"
                display as text
            visible when (MassSelectionType.Transfer)
                ToTagNumber
                    label is "ToTag"
