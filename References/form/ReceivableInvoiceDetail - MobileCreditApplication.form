MobileCreditApplication is a CompositeForm
    title is "CreditApplication"
    context form is MobileCreditApplicationContextForm
        background is theme color
    Actions
        action is UpdateApplicationGeneralLedgerDates
            action icon is edit
            label is "UpdateGlobalLedgerDates"
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
    ApplicationPreviewNA is a Panel
        visible when (not CompanyCustomerRel.NationalAccountIndicator.No)
        list is DebitTransactionsForNationalAccountRel.MobileCreditApplicationList
    ApplicationPreview is a Panel
        visible when (CompanyCustomerRel.NationalAccountIndicator.No)
        list is DebitTransactionsForCustomerRel.CreditApplicationList
