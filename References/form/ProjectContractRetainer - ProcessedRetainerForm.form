ProcessedRetainerForm is a Form
    title is "<FinanceEnterpriseGroup.FinanceDimension2Label>RetainerApplication"
    Layout
        three column distributed
            column1
                Description
                    no label
                    display as text
            column2
                RetainerDate
                    display as text
            column3
                paragraph
                    TermsCode
                        display as text
                    TermsCode.Description.Desc
                        no label
                        display as text
        three column distributed
            column1
                RetainerProject
                    display as text
            column2
                RetainerProject.Description
                    no label
                    display as text
            column3
        three column distributed
            column1
                RetainerRule
                    display as text
            column2
                RetainerFullAccount
                    display only
                    compact format
            column3
        visible when (not FinanceDimension2.FundingType.IntercompanyBilling)
            three column distributed
                column1
                    Taxable
                        display as text
                column2
                    paragraph
                        OriginalAmount
                            display as text
                        ProjectContract.Currency
                            display as text
                column3
                    AvailableAmount
                        display as text
        visible when (FinanceDimension2.FundingType.IntercompanyBilling)
            two column distributed
                column1
                    paragraph
                        OriginalAmount
                            display as text
                        ProjectContract.Currency
                            display as text
                column2
                    AvailableAmount
                        display as text
        three column distributed
            column1
                ApplicationAmount
            column2
                ApplicationPercent
            column3
                paragraph
                    ApplicationHold
                    AppliedAmount
                    display as text
