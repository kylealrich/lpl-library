ContractSubcontractorComposite is a CompositeForm
    is primary
    title is "ContractSubcontractor"
    context form is ContractSubcontractContext
        background is theme color
    SubcontractorPayments is a Panel
        list is ContractSubcontractorPaymentRels.ContractSubcontractorPaymentList
    Comments is a Panel
        list is ContractSubcontractorCommentRels.ContractSubcontractorCommentList
    Diversity is a Panel
        list is ContractSubcontractorDiversity set.primary
            helper list is SubcontractorDiversitiesRel.ActiveSubcontractorDiversities
                action is AttachDiversityToContract
