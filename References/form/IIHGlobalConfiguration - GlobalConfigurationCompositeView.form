GlobalConfigurationCompositeView is a CompositeForm
    is primary
    title is "GlobalConfiguration"
    context form is GlobalConfigurationContextForm
        background is theme color
    Actions
        action is DailyDataEngine
        action is RecommendationsEngine
        navigation is EngineProcessResults
        action is save
            label is "_"
            hide when invalid
        action is refresh
            label is "_"
    AccessRights is a Panel
        list is FinanceEnterpriseGroup.HROrganization.InventoryIntelligenceProcessor set.AccessRightsDashboard
    LocationGroups is a Panel
        list is IIHLocationGroupRel.IIHLocationGroupDashboard
    DataCleansing is a Panel
        form is DataCleansing
    PostProcessing is a Panel
        form is PostProcessing
    PerpetualGlobal is a Panel
        form is PerpetualGlobal
    ParGlobal is a Panel
        form is ParGlobal
    ServiceLevel is a Panel
        list is ServiceLevelRulesRel.ServiceLevelConfigurations
    LeadTime is a Panel
        list is LeadTimeRulesRel.LeadTimeConfigurations
    LeadTimeCutoff is a Panel
        list is IIHItemLocationRel.LeadTimeCutoffForAllItemsList
    OrderDays is a Panel
        list is OrderDaysRulesRel.OrderDaysConfigurations
    ReceivingDays is a Panel
        list is ReceivingDaysRulesRel.ReceivingDaysConfigurations
    LowerAndUpperBounds is a Panel
        list is CutoffRulesRel.CutoffConfigurations
    PerpetualOrderPolicy is a Panel
        list is OptimizationMethodRulesRel.OptimizationMethodConfigurations
    ParTargetDays is a Panel
        list is ParTargetDaysRulesRel.ParTargetDaysConfigurations
    Exclusions is a Panel
        title is "RecommendationExclusions"
        list is ExclusionRulesRel.ExclusionConfigurations
    RemovalExclusions is a Panel
        list is RemovalExclusionRulesRel.RemovalExclusionConfigurations
    Summary is a Panel
        form is AllGlobalFields
