BackorderInformation is a CompositeForm
    context form is BackordersContext
    show panel control on left
    restrict action CreateCustomerOrderReturnLine
    OtherLocations is a Panel
        list is OtherLocationsForItemRel.ItemDetails
    ProjectedStatus is a Panel
        list is WarehouseDemandLinesForItemLocationRel.ProjectedStockStatus
    SubstituteItems is a Panel
        visible when (HasSubstituteItem)
        list is ItemSubstitutesRel.SubstituteItems
