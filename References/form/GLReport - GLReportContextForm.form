GLReportContextForm is a Form
    title is "G\\LReport"
    Layout
        display as text
            three column distributed
                FinanceEnterpriseGroup
                GLReport
                    label is "ReportID"
                visible when (CompositeReport)
                    CompositeReportTag
                        no label
                        display as tag
                            color of green
            three column distributed
                ReportTitle
                    label is "Title"
                Description
                    show up to 10 characters
            visible when (not GenerateReportInProcess)
                single column
                    visible when (IntegrationStatus.OutOfSync)
                        ReportOutOfSyncLabel
                            label is "Status"
                            color of orange
                            raise red alert when (ReportHasInvalidSubReport)
                                mouse over text is "<DerivedSubReportHeaderAlertMessageoroverview>"
                            raise yellow alert when (GenerateReportHasFailedAsyncActionTriggers)
                                mouse over text is "FailedTriggersExist"
                    visible when (IntegrationStatus.Generated)
                        ReportGeneratedLabel
                            label is "Status"
                            color of green
                            raise red alert when (ReportHasInvalidSubReport)
                                mouse over text is "<DerivedSubReportHeaderAlertMessageoroverview>"
                            raise yellow alert when (GenerateReportHasFailedAsyncActionTriggers)
                                mouse over text is "FailedTriggersExist"
                    visible when (IntegrationStatus.NotGenerated)
                        ReportNotGeneratedLabel
                            label is "Status"
                            color of red
                            raise red alert when (ReportHasInvalidSubReport)
                                mouse over text is "<DerivedSubReportHeaderAlertMessageoroverview>"
                            raise yellow alert when (GenerateReportHasFailedAsyncActionTriggers)
                                mouse over text is "FailedTriggersExist"
            single column
                visible when (GenerateReportInProcess)
                    ReportIsGeneratingTag
                        no label
                        display as tag
                            color of yellow
                visible when (HasMultiSelectReportFilter)
                    DrillbackNotPossibleTag
                        no label
                        display as tag
                            color of yellow
                visible when (ShowMultiSelectColumnFilterAlert)
                    ColumnDrillbackNotPossibleTag
                        no label
                        display as tag
                            color of yellow
                visible when (DerivedHasPeriodRange)
                    DrillbackNotPossibleWhenPeriodRangeTag
                        no label
                        display as tag
                            color of yellow
                visible when (DerivedColumnStructureDifferAccrossSubReports)
                    ColumnStructureDifferAccrossSubReportsTag
                        no label
                        display as tag
                            color of yellow
                visible when (DerivedHasDimStructureMismatch)
                    DimensionStructureMismatchTag
                        no label
                        display as tag
                            color of red
