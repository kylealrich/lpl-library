OverviewReportForm is a Form
    is primary
    title is "CreateReport"
    Layout
        single column
            DataAreaParam
                label is "DataArea"
                display only
            For
                display as horizontal radio buttons
            visible when (For.Actor)
                ParamSpecifyActor
                    label is "RunForSingleActor"
                visible when (ParamSpecifyActor)
                    ParamActor
                        select is MingleActiveActorList
            visible when (For.ActorGroup)
                ParamActorGroup
            AddDefaultContexts
                when value changed
                    Field1 =  "HROrganization"
                        when (IsFsm)
                    Field2 =  "EnterpriseGroup"
                        when (IsFsm)
                    Field3 =  "FinanceEnterpriseGroup"
                        when (IsFsm)
                    Field1 =  "HROrganization"
                        when (IsHcm)
                    Field2 =  "JobBoard"
                        when (IsHcm)
                    Field3 =  "FinanceEnterpriseGroup"
                        when (IsHcm)
                    Field1 =  blank
                        when (not AddDefaultContexts)
                    Field2 =  blank
                        when (not AddDefaultContexts)
                    Field3 =  blank
                        when (not AddDefaultContexts)
            text of untranslatable:"IncludeContextProperties"
                align as label
        three column
            column1
                Field1
                    no label
                Field2
                    no label
                Field3
                    no label
                Field4
                    no label
                Field5
                    no label
            column2
                Field6
                    no label
                Field7
                    no label
                Field8
                    no label
                Field9
                    no label
                Field10
                    no label
            column3
                Field11
                    no label
                Field12
                    no label
                Field13
                    no label
                Field14
                    no label
                Field15
                    no label
