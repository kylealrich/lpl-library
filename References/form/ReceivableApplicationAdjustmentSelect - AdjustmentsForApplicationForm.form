AdjustmentsForApplicationForm is a Form
    Layout
        display as text
            header2 of "Document"
            paragraph
                Company
                ReceivableApplicationAdjustmentSelect.BatchNumber
                ReceivableApplicationAdjustmentSelect.TransType
                ReceivableApplicationAdjustmentSelect.Invoice
            paragraph
                ReceivableApplicationAdjustmentSelect.AdjustmentSequence
        paragraph
            AdjustmentReason
                select is ReceivableReasonCodeInvoiceSelectionList
            Description
        paragraph
            AdjustAmount
            BaseAdjustAmount
                display as text
            CreditAdjustAmount
                display as text
        paragraph
            DistributionAccount
        visible when (MemoChargebackAdjustment)
            blank line
            header2 of "MemoChargeback"
            paragraph
                MemoChargeback.Company
                MemoChargeback.ProcessLevel
                    label is "ProcessLevel"
            paragraph
                MemoChargeback.Customer
                    label is "Customer"
            paragraph
                MemoChargeback.MemoChargeback
                    label is "CrossReferenceNumber"
        paragraph
            GeneralLedgerDate
                label is "GlobalLedgerDate"
            ReceivableOperator
        visible when (IsTaxWithholding)
            paragraph
                ARIncomeWithholdingCode
                ARIncomeWithholdingDocumentNumber
