SalesRepresentativeForm is a Form
    is primary
    title is "SalesRepresentative"
    Layout
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        paragraph
            SalesRepresentative
                select is SalesReps
            Name
                no label
        paragraph
            Active
        blank line
        paragraph
            CommisionPercent
                label is "CommissionPercent"
        paragraph
            Territory
                select is Territory
            Territory.Description
                no label
                display as text
