MobileItemLocationDetailForParItem is a Form
    title is "ParLocationDetail"
    Layout
        header3 of "ParItem"
        single column
            BinSequence
                label is "BinSequence"
            TransientShelfLocation
                label is "ShelfLocation"
                select is MobileBinSelect
            DefaultTransactionUOM.UnitOfMeasure
                label is "DefaultTransactionUnitOfMeasure"
                select is MobileUnitOfMeasureList
        two column
            column1
                ReorderPoint
                    label is "ParLevel"
                LeadtimeDays
                    label is "LeadtimeDays"
            column2
                MinimumOrderQuantity
                    label is "MinimumOrderQuantity"
                SuggestedReorderPoint
                    label is "SuggestedParLevel"
        single column
            GeneralLedgerCategory
                label is "InventoryLocationAccountGroup"
                select is MobileAccountGroupsSelect
            StandardCost
                label is "StandardCost"
            Company.GeneralLedgerCompany.Currency
                label is "Currency"
                display as text
            PatientChargeable
                label is "PatientChargeable"
            NoCharge
                label is "NoCharge"
            InventoryLocation.OneSourceDocumentToOnePO
                label is "SingleDocumentToAPurchaseOrder"
                display as text
            Manufacturer
                label is "ManufacturerCodeAndDivision"
                compact format
                select is MobileManufacturerList
            visible when (Manufacturer entered)
                Manufacturer.Description
                    label is "ManufacturerDescription"
                    display as text
            ManufacturerNumber
                label is "ManufacturerNumber"
