PaymentSearch is a SearchForm
    Layout
        four column distributed
            CashCode
            CashLedgerPayment.BankTransactionCode
            PaymentStatus
            Status
        four column distributed
            CanAddToCashRequirementsResult
                label is "ReadyForPayment"
            PaymentProcessingHold
                label is "OnHold"
            BankCheckAmountRange.FromAmount
                label is "FromIssuedBankAmount"
            BankCheckAmountRange.ToAmount
                label is "ToIssuedBankAmount"
        four column distributed
            TransactionNumberRange.FromTransNbr
                label is "FromPaymentNumber"
            TransactionNumberRange.ToTransNbr
                label is "ToPaymentNumber"
            ReferenceNumberRange.FromReferenceNbr
                label is "FromReferenceNumber"
            ReferenceNumberRange.ToReferenceNbr
                label is "ToReferenceNumber"
        two column distributed
            DistribUserFieldsSearch
                label is "DistributionUserFieldSearch"
                use text area of 1 lines
            text of "PaymentNumberAndReferenceRangeInAlphaSortOrder"
                color of blue
