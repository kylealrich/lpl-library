PaymentClearingFileCreation is an ActionForm
    action is PaymentClearingFileCreation
    Layout
        header2 of "Main"
        two column distributed
            paragraph
                VendorGroup
                VendorGroup.Description
                    no label
                    display as text
            paragraph
                PayGroup
                    select is PayGroups
                PayGroup.Name
                    no label
                    display as text
        two column distributed
            paragraph
                PaymentCode
                    select is CashPayments
                PaymentCode.Description
                    no label
                    display as text
            paragraph
                ProcGrp
                    label is "ProcessGroup"
                    select is ProcessGroups
                ProcGrp.Description
                    no label
                    display as text
        paragraph
            CashCode
                show up to 12 characters
            CashCode.Description
                no label
                display as text
        paragraph
            text of "Or"
                bold
        paragraph
            CashCodeGrp
                label is "CashCodeGroup"
                select is CashCodeGroup
            CashCodeGrp.Description
                no label
                display as text
        single column
            SuppressNoPaymentError
        single column
            FileName
        header2 of "SelectOptions"
        two column distributed
            column1
                CheckDate
                    label is "PaymentDate"
                StartTransNbr
                    label is "StartingNumber"
                CreatePrenot
                    label is "CreatePrenotifications"
                DocCurrency
                    label is "DocumentCurrency"
                UseAp150SortOpt
                    label is "UseCashRequirementsSortOptions"
            column2
                EffectiveDate
                PrintRemit
                    label is "PrintRemittance"
                BacsSerialNbr
                    label is "B\\A\\C\\SSerialNumber"
                PrintComments
                SendBankType
                    label is "ElectronicTransferBankType"
        header2 of "RerunOptions"
        two column distributed
            column1
                RecreateOption
                    label is "RecreateFile"
            column2
                SerialNum
                    label is "SerialNumber"
        header2 of "OutputOptions"
        two column distributed
            column1
                CreateLabelsOpt
                    label is "CreateLabels"
                BalancedTape
                    label is "BalancedFile"
                ImmediateOriginName
            column2
                Format
                Healthcare
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                PaymentClearingFileCreationReport_RDG
                PaymentClearingFileCheck_RDG
                PaymentClearingFileIatError_RDG
                PaymentClearingFileRemit_RDG
                PaymentClearingErrorReport_RDG
            column2 of 25%
                PaymentClearingFileCreationReport_XPT
                PaymentClearingFileCheck_XPT
                PaymentClearingFileIatError_XPT
                PaymentClearingFileRemit_XPT
                PaymentClearingErrorReport_XPT
