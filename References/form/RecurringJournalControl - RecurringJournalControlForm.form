RecurringJournalControlForm is a CompositeForm
    is primary
    title is "RecurringJournalControl_<AccountingEntity>_<System>"
    context form is RecurringJournalControlContext
    MainPanel is a Panel
        title is "Main"
        form is RecurringJournalMainForm
    SelectedPeriodPanel is a Panel
        title is "Schedule"
        visible when (RecurringJournalControlExists)
        list is RecurringJournalSchedule set.RecurringJournalScheduleList
    TransactionPanel is a Panel
        title is "Transactions"
        visible when (not RecuringBridgeExists)
        list is RecurringJournalTransaction set.RecurringJournalTransactionList
    CommentsPanel is a Panel
        title is "Comments"
        list is RecurringJournalComment set.Comment
    RelatedDocumentsPanel is a Panel
        title is "RelatedDocuments"
        list is RecurringJournalDocument set.RecurringJournalDocuments
    InterfacePanel is a Panel
        title is "Interface"
        visible when (RecuringBridgeExists)
        list is RecurringJournalBridge set.RecurringJournalBridgeList
    Encumbrances is a Panel
        title is "Encumbrances"
        visible when (EncumbrancesExist)
        list is RecurringJournalEncumbranceRel.RecurringJournalEncumbranceList
    BudgetErrors is a Panel
        title is "BudgetErrors"
        visible when (BudgetErrorsExist)
        list is BudgetEditErrorRel.BudgetEditErrorList
