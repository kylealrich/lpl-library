MobileReceivableInvoiceDistributionContextForm is a Form
    title is "ReceivableInvoiceDistributionContextForm"
    restrict action JournalizeDistributions
    restrict action NewJournalizeDistributions
    Layout
        header2 of "Distribution"
        paragraph
            ReceivableCompanyDataGroup.TransactionCompany
                label is "TransactionCompany"
        single column
            ReceivableCompanyDataGroup.ReceivableDistributionCategory
                label is "DistributionCategory"
            ReceivableCompanyGroup.GlCompany
                label is "GlobalLedgerCompany"
            GeneralLedgerAccount
                label is "GlobalLedgerAccount"
                compact format
        paragraph
            DistributionAmount.CurrencyAmount
                label is "DistributionAmount"
        paragraph
            Description
