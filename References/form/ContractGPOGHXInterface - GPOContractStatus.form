GPOContractStatus is a CompositeForm
    context form is GPOContractContext
        background is theme color
    Actions
        navigation is ToContractImportNav
            label is "ViewInterfaceContract"
            hide when invalid
        action is CreateNewContractForThisGPOContract
            label is "CreateNewContract"
            hide when invalid
    Items is a Panel
        list is SingleItemRel.ContractGPOItemList
