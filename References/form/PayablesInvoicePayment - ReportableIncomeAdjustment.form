ReportableIncomeAdjustment is a Form
    Layout
        two column
            column1
                paragraph
                    Company
                        display as text
                    Company.Name
                        no label
                        display as text
                paragraph
                    Vendor
                        display as text
                    Vendor.VendorName
                        no label
                        display as text
                paragraph
                    PayablesInvoice
                        display as text
                paragraph
                    PayablesInvoicePayment.PaymentSequence
                        display as text
                paragraph
                    TransactionNetPayment
                        display as text
                paragraph
                    ReportableIncomeAmount
                    IncomeCode
                        select is IncomeCodes
                    IncomeCode.Description
                        no label
                        display as text
            column2
                paragraph
                    PayGroup
                        display as text
                paragraph
                    ProcessLevel
                        display as text
                paragraph
                    InvoiceCurrency
                        display as text
                paragraph
                    CheckDate
                        display as text
                paragraph
                    PaymentStatus
                        display as text
