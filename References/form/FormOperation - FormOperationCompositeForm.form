FormOperationCompositeForm is a CompositeForm
    title is "FormBasedOperation"
    context form is FormOperationHeaderForm
    FormInputFields is a Panel
        list is FormInputField set.InputFields
            helper list is Form.AvailableFormInputField set.AvailableFormInputFields
                action is AttachInputFieldsToFormOperation
    FormOutputFields is a Panel
        list is FormOutputField set.OutputFields
            helper list is Form.AvailableFormOutputField set.AvailableFormOutputFields
                action is AttachOutputFieldsToFormOperation
