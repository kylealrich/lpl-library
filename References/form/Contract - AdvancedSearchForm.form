AdvancedSearchForm is a Form
    title is "ContractSearch"
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        four column distributed
            ContractGroup
            ContractSearchDocument
                label is "Contract,Name,Supplier"
            ContractStatus
            OnHold
        four column distributed
            Description
                use text area of 1 lines
                show up to 15 characters
            MyContract
                label is "MyContracts"
            CanCreateChangeOrder
                label is "EligibleForChangeOrder"
            PastExpirationDate
                label is "Expired"
        four column distributed
            ContractType
                show up to 10 characters
            ContractClassification
                show up to 10 characters
            ManufacturerOrDistributor.ManufacturerContract
            ManufacturerOrDistributor.DistributorContract
        four column distributed
            Vendor
            AnyChangeOrderExists
                label is "ChangeOrderExists"
            UnsupportedIDMAttachmentsExist
            EnableClinicalSystemUseLocations
                label is "LocationsForClinicalUse"
            EffectiveContracts
                label is "EffectiveContracts"
