BankExpenseCurrencyAmountsForm is a Form
    title is "BankExpenseCurrencyAmounts"
    Layout
        visible when (false)
            BankExpenseData.AssignToCurrency
        blank line
        four column table
            column headers
                "_"
                "Amount"
                "Currency"
                "Rate"
            column1
                text of "Functional"
                visible when (Company.OrigCompany.AccountingEntity.AlternateCurrencyExists)
                    text of "Alternate"
                visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency2Exists)
                    text of "Alternate2"
                visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency3Exists)
                    text of "Alternate3"
                visible when (BankExpenseData.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                    text of "<BankExpenseData.FinanceEnterpriseGroup.ReportOne>"
