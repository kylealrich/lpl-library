MatchInvoiceReferenceComposite is a CompositeForm
    is primary
    title is "InvoiceLineDetailPurchaseOrder"
    context form is MatchInvoiceReferenceContext
    Actions
        action is open
            label is "_"
        action is search
            label is "_"
    MemoDetails is a Panel
        title is "<DerivedInvoiceDetailsPanelTitle>"
        visible when (not PayablesInvoice.InvoiceSource.CreditForRebill)
        list is PayablesInvoiceDetailsRel.MemoDetailList
            helper list is OriginalInvoiceDetailsRel.OriginalInvoiceHelperList
                action is CreateMemoCostAdjustment
                    invoked.MemoCompany = Company
                    invoked.MemoInvoice = PayablesInvoice
                action is CreateMemoQuantityAdjustment
                    invoked.MemoCompany = Company
                    invoked.MemoInvoice = PayablesInvoice
    CreditForRebillDetails is a Panel
        title is "CreditForRebillDetails"
        visible when (PayablesInvoice.InvoiceSource.CreditForRebill)
        list is PayablesInvoiceDetailsRel.MemoDetailList
            helper list is OriginalInvoiceDetailsRel.OriginalInvoiceHelperList
                action is CreditForRebill
                    invoked.MemoCompany = Company
                    invoked.MemoInvoice = PayablesInvoice
    MemoAddOnChargesAndAllowances is a Panel
        title is "<DerivedInvoiceAddOnChargesPanelTitle>"
        visible when (ShowAOCMemo)
        list is PayablesInvoiceLineAddOnChargeRel.MemoAddOnChargeList
            helper list is OriginalInvoiceAddOnChargesRel.OriginalInvoiceHelperList
                action is CreateMemoCostAdjustment
                    invoked.MemoCompany = Company
                    invoked.MemoInvoice = PayablesInvoice
                action is CreateMemoQuantityAdjustment
                    invoked.MemoCompany = Company
                    invoked.MemoInvoice = PayablesInvoice
    MemoAddOnChargesAndAllowancesCreditRebill is a Panel
        title is "<DerivedInvoiceAddOnChargesPanelTitle>"
        visible when (ShowAOCCreditRebill)
        list is PayablesInvoiceLineAddOnChargeRel.MemoAddOnChargeList
            helper list is OriginalInvoiceAddOnChargesRel.OriginalInvoiceHelperList
                action is CreditForRebillLineAddOnCost
                    invoked.MemoCompany = Company
                    invoked.MemoInvoice = PayablesInvoice
