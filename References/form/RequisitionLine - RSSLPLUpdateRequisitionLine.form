RSSLPLUpdateRequisitionLine is an ActionForm
    action is Unreleased.RSSLPLUpdateRequisitionLine
    submit label is "Ok"
    title is "QuickUpdateLineItem"
    Layout
        two column distributed
            PrmQuantity
                label is "Quantity"
                raise blue alert when (DerivedMaximumRequisitionQuantity entered)
                    mouse over text is "<DerivedMaximumRequisitionQuantity>"
                initial value is Quantity
            visible when (RequesterRel.SetToDisplayOnlyTransactionUOM)
                PrmUOM
                    label is "UOM"
                    display only
                    select is ValidRequisitionUnitsOfMeasure
                    initial value is EnteredUOM
            visible when (not RequesterRel.SetToDisplayOnlyTransactionUOM)
                PrmUOM
                    label is "UOM"
                    select is ValidRequisitionUnitsOfMeasure
                    initial value is EnteredUOM
            visible when (IsCostUpdateProhibited)
                PrmTransactionUnitCost
                    label is "UnitCost"
                    display as text
                    initial value is TransactionUnitCost
            visible when (not IsCostUpdateProhibited)
                PrmTransactionUnitCost
                    label is "UnitCost"
                    initial value is TransactionUnitCost
            visible when (RSSLPLValidateDeliverTo)
                PrmDeliverTo
                    label is "DeliverTo"
                    display as required when (RequesterRel.RequireDeliverTo)
            visible when (RSSLPLValidateBuyer)
                PrmBuyer
                    label is "Buyer"
                    display as required when (RequesterRel.RequireBuyer)
            visible when (RSSLPLValidateLineCommodityCode)
                PrmCommodityCode
                    label is "CommodityCode"
                    display as required when (RequesterRel.RequireCommodityCodeDisplay)
            visible when (RSSLPLValidatePOCode)
                PrmPOCode
                    label is "POCode"
                    display as required when (RequesterRel.RequirePOCode)
            visible when (RSSLPLValidateManufacturer)
                PrmManufacturer
                    label is "Manufacturer"
                    compact format
                    display as required when (RequesterRel.RequireManufacturer)
            visible when (RSSLPLValidateManufacturerNumber)
                PrmManufacturerNumber
                    label is "ManufacturerNumber"
                    display as required when (RequesterRel.RequireManufacturerNumber)
            visible when (RSSLPLValidateAsset)
                PrmAsset
                    label is "Asset"
                    display as required when (RequesterRel.RequireAsset)
            visible when (RSSLPLValidateAssetTemplate)
                PrmAssetTemplate
                    label is "AssetTemplate"
                    display as required when (RequesterRel.RequireAssetTemplate)
            visible when (RSSLPLValidateGLEncumbranceReference)
                PrmGLEncumbranceReference
                    label is "GLEncumbranceReference"
                    display as required when (RequesterRel.RequireGLEncumbranceReference)
            visible when (RequesterRel.RequireRequestedDeliveryDate)
                PrmRequestedDeliveryDate
                    label is "RequestedDeliveryDate"
                    display as required when (RequesterRel.RequireRequestedDeliveryDate)
            visible when (RSSLPLValidateVendor)
                PrmVendor
                    label is "Vendor"
                    display as required when (RSSLPLRequireVendor)
            visible when (RSSLPLValidateVendorItem)
                PrmVendorItem
                    label is "VendorItem"
                    display as required when (RequesterRel.RequireVendorItem)
            visible when (RSSLPLMajorPurchasingClassRequired)
                PrmMajorPurchasingClass
                    label is "MajorPurchasingClass"
                    display as required when (RSSLPLMajorPurchasingClassRequired)
            visible when (MinorPurchasingClassRequired)
                PrmMinorPurchasingClass
                    label is "MinorPurchasingClass"
                    display as required when (MinorPurchasingClassRequired)
