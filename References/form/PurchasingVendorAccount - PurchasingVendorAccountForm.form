    is primary
    title is "PurchasingVendorAccount"
    Layout
        paragraph
            VendorGroup
            VendorGroup.Description
                no label
                display as text
        paragraph
            Vendor
            Vendor.VendorName
                no label
                display as text
        paragraph
            VendorLocation
            VendorLocation.VendorName
                no label
                display as text
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            InventoryLocation
                select is AllActiveLocations
            InventoryLocation.Name
                no label
                display as text
        single column
            VendorAccount
