AnalyticCubeLogDetail is a Form
    title is "<BusinessClass>LogDetails"
    Layout
        header1 of "Results"
            show border color of black
        single column
            Mode
                label is "Mode"
                raise red alert when (IsAlertForModeRed)
                    mouse over text is "<LogType>WasUnsuccessful"
                raise green alert when (IsAlertForModeGreen)
                    mouse over text is "<LogType>WasSuccessful"
            Threads
            Duration
                label is "<LogType>DurationH\\M\\S"
        visible when (not LogType.ReloadFactRowSimulation)
            Checkpoint
                label is "Checkpoint"
            CheckpointDACName
                label is "CheckpointDACName"
        visible when (not LogType.ReloadFactRowSimulation)
            header1 of "Dates"
                show border color of black
            single column
                AsOfDate
                PreviousRunDate
                    label is "PreviousRunDate"
                RecoveryDate
                    label is "RecoveryDate"
        header1 of "Statistics"
            show border color of black
        single column
            visible when (not LogType.ReloadFactRowSimulation)
                DimensionTime.HHMMSS
                    label is "DimensionTimeH\\H\\M\\M\\S\\S"
                DimensionCount
                    label is "DimensionCount"
            FactRowTime.HHMMSS
                label is "FactRowTimeH\\M\\S"
            FactRowCount
                label is "FactRowCount"
        visible when (not LogType.Refresh)
            header2 of "ThreadStats"
            single column
                FactRowThreadsAveTime.HHMMSS
                    label is "AveThreadTimeH\\M\\S"
                FactRowThreadsMinTime.HHMMSS
                    label is "MinThreadTimeH\\M\\S"
                FactRowThreadsMaxTime.HHMMSS
                    label is "MaxThreadTimeH\\M\\S"
                FactRowThreadsAveCount
                    label is "AveFactRowCount"
                FactRowThreadsMinCount
                    label is "MinFactRowCount"
                FactRowThreadsMaxCount
                    label is "MaxFactRowCount"
