RecallGroupGeneral is a Form
    title is "ItemLifecycleManagementGroup"
    Layout
        three column distributed
            column1
                RequireTaskAcceptance
                AllowOrganizations
            column2
                QuantityRequired
                visible when (AllowOrganizations)
                    EnterQuantitiesByLocation
            column3
            LastNRACRetrievalDate
                display as text
        header2 of "ReportLogo"
        paragraph
            visible when (HasReportLogo)
                ReportLogo.Title
            ReportLogo.File
                label is "Attachment"
        header2 of "ApprovalInformation"
        single column
            RecallApprovalRequired
        header2 of "RestrictProcessingDefaults"
        single column
            text of "IfItemFoundOnAnActiveNotice,RestrictCreationOf:"
                italics
        two column
            column1 of 20%
                RestrictPurchaseOrderCreation
                    label is "PurchaseOrderLine"
                RestrictInventoryIssue
                    label is "InventoryIssueTransactionLine"
            column2 of 80%
                RestrictRequisitionCreation
                    label is "RequisitionLine"
                RestrictInventoryReceipt
                    label is "InventoryReceiptTransactionLine"
        header2 of "ResolutionsAllowed"
        two column table
            column headers
                "Coordinator"
                "Responder"
            row headers
                "Transfers"
                "Issues"
                "Returns"
                "Adjustments"
            column1
                AllowTransferByCoordinator
                    no label
                AllowIssueByCoordinator
                    no label
                AllowReturnByCoordinator
                    no label
                AllowAdjustmentByCoordinator
                    no label
            column2
                AllowTransferByResponder
                    no label
                AllowIssueByResponder
                    no label
                AllowReturnByResponder
                    no label
                AllowAdjustmentByResponder
                    no label
        header2 of "DefaultProductTransactionQueryParameters"
        text of "_"
        text of "DaysAndMonthsForTransactionDateAndDueDate"
            header3
        paragraph
            text of "NumberOfDaysFromCurrentDayTasksAreDue"
                bold
            DefaultTaskNumberOfDaysClass1
                label is "FDAClass1"
            DefaultTaskNumberOfDaysNonClass1
                label is "NonFDAClass1"
        single column
            DefaultNumberOfMonthsForTransactions
        text of "Transactions"
            header3
        two column
            column1 of 20%
                TransactionQueryParameters.PerformRequisitionQuery
                    label is "RequisitionLines"
                TransactionQueryParameters.PerformIssueQuery
                    label is "Issues"
            column2 of 80%
                TransactionQueryParameters.PerformPurchaseOrderQuery
                    label is "PurchaseOrderLines"
                TransactionQueryParameters.PerformIntransitTransferQuery
                    label is "IncompleteTransfers"
        text of "_"
        text of "Inventory"
            header3
        two column
            column1 of 20%
                TransactionQueryParameters.PerformInventoryQuery
                    label is "Inventory"
            column2 of 80%
                TransactionQueryParameters.PerformParLocationQuery
                    label is "ParLocations"
        text of "_"
        text of "DetailTransactions"
            header3
        two column
            column1 of 20%
                TransactionQueryParameters.PerformLotNumberQuery
                    label is "LotNumbers"
            column2 of 80%
                paragraph
                    TransactionQueryParameters.PerformSerialNumberQuery
                        label is "SerialNumbers"
                    text of "(includesIssues,Transfers,AndStockOnHand)"
                        italics
        text of "_"
        text of "Documents"
            header3
        two column
            column1 of 20%
                TransactionQueryParameters.PerformContractQuery
                    label is "ContractLines"
                TransactionQueryParameters.PerformProcurementTemplateQuery
                    label is "ProcurementTemplateLines"
            column2 of 80%
                TransactionQueryParameters.PerformSourcingEventQuery
                    label is "SourcingEventLines"
        text of "_"
        text of "PatientProcedures"
            header3
        paragraph
            TransactionQueryParameters.PerformPatientProcedureQuery
                label is "LotAndSerialNumbers"
            text of "(includesRequisitionAndPurchaseOrderLines)"
                italics
