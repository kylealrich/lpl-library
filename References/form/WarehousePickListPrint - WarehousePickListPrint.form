WarehousePickListPrint is an ActionForm
    action is WarehousePickListPrint
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            Location
                show up to 10 characters
            Location.Name
                no label
                display as text
        header2 of "Main"
        two column distributed
            column1
                EstTranDt
                    label is "ExpectedShipDate"
                SelectOption
                    label is "PrintOption"
                    show up to 20 characters
                AllocDetail
                    label is "AllocateDetails"
                IncludeCost
                    show up to 20 characters
                PrintDeliveryData
                visible when (not Company.UseIDM)
                    AlignForm
                        label is "AlignmentForms"
                ReprintBatchNbr
                    label is "ReprintBatch"
                CreateFlatFile
            column2
                SystemCd
                    label is "DocumentSource"
                IclAllocPurp
                    label is "BinAllocationPurpose"
                PreferOption
                    label is "ForcePreferredBin"
                IncludeBackorders
                IncludeKillDetails
                ReprintShipmentNbr
                    label is "ReprintShipment"
                    select is ShipmentsForLocation
                FlatFileName
                    label is "FileName"
                PrintRequester
        visible when (Company.UseIDM)
            header2 of "IDMOptions"
            paragraph
                IDMPrinter
                IDMPrinter.Description
                    no label
                    display as text
            paragraph
                ConsolidateFile
        header2 of "Filter"
        paragraph
            DocNbr.DocNbr[1]
                label is "SourceDocument"
            DocNbr.DocNbr[2]
                no label
            DocNbr.DocNbr[3]
                no label
            DocNbr.DocNbr[4]
                no label
        paragraph
            DocNbr.DocNbr[5]
                no label
            DocNbr.DocNbr[6]
                no label
            DocNbr.DocNbr[7]
                no label
            DocNbr.DocNbr[8]
                no label
        paragraph
            DocNbr.DocNbr[9]
                no label
            DocNbr.DocNbr[10]
                no label
            DocNbr.DocNbr[11]
                no label
            DocNbr.DocNbr[12]
                no label
        blank line
        paragraph
            ReqCompany
                label is "RequestingCompany"
            ReqLocation
                label is "RequestingLocation"
                show up to 20 characters
            ReqLocation.Name
                no label
                display as text
            text of "\\or"
            ReqLocationGroup
                label is "RequestingLocationGroup"
            ReqLocationGroup.Name
                no label
                display as text
        paragraph
            Customer
                show up to 20 characters
            Customer.Name
                no label
                display as text
        paragraph
            Item
                show up to 20 characters
            Item.Description
                no label
                display as text
        paragraph
            ProcessType
                show up to 20 characters
            ProcessType.Description
                no label
                display as text
        paragraph
            FromAllocPri
                label is "FromPriority"
            ToAllocPri
                label is "ToPriority"
        blank line
        paragraph
            ShipTo
                show up to 20 characters
            ShipTo.Name
                no label
                display as text
        paragraph
            Carrier
                show up to 20 characters
            Carrier.VendorName
                no label
                display as text
        paragraph
            RouteGrp
                label is "Route"
                compact format
            AllRoute
                label is "_"
        paragraph
            SkipInsufBinDtl
                label is "SkipItemsWithInsufficientBinDetails"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                WarehousePickListPrintReport_RDG
                ByShipmentDocumentLine_RDG
                ByShipmentBinLotSerial_RDG
                ByShipmentBinLotSerialRequisitionBased_RDG
                ByBinItemUOMLotSerial_RDG
                WarehousePickListPrintSummary_RDG
                WarehousePickListPrintErrorReport_RDG
            column2 of 25%
                WarehousePickListPrintReport_XPT
                ByShipmentDocumentLine_XPT
                ByShipmentBinLotSerial_XPT
                ByShipmentBinLotSerialRequisitionBased_XPT
                ByBinItemUOMLotSerial_XPT
                WarehousePickListPrintSummary_XPT
                WarehousePickListPrintErrorReport_XPT
