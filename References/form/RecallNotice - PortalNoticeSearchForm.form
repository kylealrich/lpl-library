PortalNoticeSearchForm is a SearchForm
    title is "NoticeSearch"
    Layout
        single column
            RecallSearchDocument
                use text area of 1 lines
        two column
            RecallNotice
                select is NonPendingRecallNotices
            WorkingId
        two column
            Name
            Description
        two column
            RecallType
                label is "Type"
            RecallCoordinator
                select is RecallCoordinatorList
        two column
            paragraph
                ManufacturerCodeDivision
                    label is "ManufacturerCode"
                    compact format
                ManufacturerName
                    no label
            ManufacturerGln
        two column
            paragraph
                RecallNoticeDateRange.Begin
                    label is "AnnouncementDatesFrom"
                RecallNoticeDateRange.End
                    label is "To"
            paragraph
                Equipment
