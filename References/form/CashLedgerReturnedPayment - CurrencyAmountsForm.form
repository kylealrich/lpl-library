CurrencyAmountsForm is a Form
    title is "CurrencyAmounts"
    Layout
        visible when (false)
            CompanyReturnAmount.AssignToCurrency
        blank line
        four column table
            column headers
                "_"
                "Amount"
                "Currency"
                "Rate"
            column1
                text of "Functional"
                visible when (Company.OrigCompany.AccountingEntity.AlternateCurrencyExists)
                    text of "Alternate"
                visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency2Exists)
                    text of "Alternate2"
                visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency3Exists)
                    text of "Alternate3"
                visible when (CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                    text of "<CompanyReturnAmount.FinanceEnterpriseGroup.ReportOne>"
                visible when (CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                    text of "<CompanyReturnAmount.FinanceEnterpriseGroup.ReportTwo>"
                visible when (CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                    text of "<CompanyReturnAmount.FinanceEnterpriseGroup.ReportThree>"
                visible when (CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                    text of "<CompanyReturnAmount.FinanceEnterpriseGroup.ReportFour>"
                visible when (CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                    text of "<CompanyReturnAmount.FinanceEnterpriseGroup.ReportFive>"
            column2
                display as text
                    CompanyReturnAmount.FunctionalAmount.EnteredCurrencyAmount
                        no label
                        display only
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrencyExists)
                        CompanyReturnAmount.AlternateAmount.EnteredCurrencyAmount
                            no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency2Exists)
                        CompanyReturnAmount.AlternateAmount2.EnteredCurrencyAmount
                            no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency3Exists)
                        CompanyReturnAmount.AlternateAmount3.EnteredCurrencyAmount
                            no label
                    visible when (CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        CompanyReturnAmount.ReportAmount1.EnteredCurrencyAmount
                            no label
                    visible when (CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        CompanyReturnAmount.ReportAmount2.EnteredCurrencyAmount
                            no label
                    visible when (CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        CompanyReturnAmount.ReportAmount3.EnteredCurrencyAmount
                            no label
                    visible when (CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        CompanyReturnAmount.ReportAmount4.EnteredCurrencyAmount
                            no label
                    visible when (CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                        CompanyReturnAmount.ReportAmount5.EnteredCurrencyAmount
                            no label
            column3
                display as text
                    Company.OrigCompany.AccountingEntity.FunctionalCurrency
                        no label
                        display only
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrencyExists)
                        Company.OrigCompany.AccountingEntity.AlternateCurrency
                            no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency2Exists)
                        Company.OrigCompany.AccountingEntity.AlternateCurrency2
                            no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency3Exists)
                        Company.OrigCompany.AccountingEntity.AlternateCurrency3
                            no label
                    visible when (CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyOne
                            no label
                    visible when (CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyTwo
                            no label
                    visible when (CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyThree
                            no label
                    visible when (CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyFour
                            no label
                    visible when (CompanyReturnAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
