FileCreationColumnDetailPrimaryForm is a Form
    is primary
    title is "ColumnDetail"
    Layout
        header3 of "Field_Information"
        single column
            FinanceEnterpriseGroup
            FileCreationSetup
            FileCreationRowDetail
            FileCreationColumnDetail
            FieldName
            Description
            ColumnOrder
            FieldDataType
        visible when (FileCreationSetup.FileType.FixedLength)
            paragraph
                FixedLengthFieldRange.BeginPosition
                FixedLengthFieldRange.EndPosition
        single column
            visible when (FileCreationSetup.FileType.FixedLength)
                Length
                    display as text
            visible when (not FileCreationSetup.FileType.FixedLength)
                Length
        single column
            InputOption
            Comments
        header3 of "Formatting"
        single column
            visible when (not FieldDataType.Boolean)
                JavaFormat
                    when value changed
                        refresh FormattedValueTest
        visible when (SupportsReplacement)
            paragraph
                ReplacementRegularExpression
                    when value changed
                        refresh FormattedValueTest
                ReplacementReplaceWith
                    label is "ReplaceWith"
                    when value changed
                        refresh FormattedValueTest
        visible when (DisplayFormatTest)
            paragraph
                visible when (SupportsReplacement)
                    TestValueAlpha
                        when value changed
                            refresh FormattedValueTest
                visible when (FieldDataType.Numeric)
                    TestValueNumeric
                        when value changed
                            refresh FormattedValueTest
                visible when (FieldDataType.Decimal)
                    TestValueDecimal
                        when value changed
                            refresh FormattedValueTest
                visible when (FieldDataType.Date)
                    TestValueDate
                        when value changed
                            refresh FormattedValueTest
                visible when (FieldDataType.Time)
                    TestValueTime
                        when value changed
                            refresh FormattedValueTest
                visible when (FieldDataType.TimeStamp)
                    TestValueTimeStamp
                        when value changed
                            refresh FormattedValueTest
                FormattedValueTest
        header3 of "SourceValue"
        single column
            SourceRelation
            visible when (FileCreationRowDetail.OneToMany)
                OneToMany
            SourceFields
            visible when (FileCreationSetup.FileType.Separated)
                TruncateToLength
            visible when (FieldDataType.Alpha)
                FileName
                ConvertToUpperCase
        visible when (FieldDataType.Alpha)
            paragraph
                SubstringStart
                    label is "SubstringStart"
                SubstringEnd
                    label is "End"
        single column
            visible when (FieldDataType.Alpha)
                DefaultAlpha
            visible when (FieldDataType.Date)
                UseCurrentSystemDate
                ShowAlphaDefaultField
                visible when (not ShowAlphaDefaultField)
                    DefaultDate
                visible when (ShowAlphaDefaultField)
                    DefaultAlpha
            visible when (FieldDataType.Decimal)
                ShowAlphaDefaultField
                visible when (not ShowAlphaDefaultField)
                    DefaultDecimal
                visible when (ShowAlphaDefaultField)
                    DefaultAlpha
            visible when (FieldDataType.Numeric)
                OneToManySequenceNumber
                RecordCount1
                RecordCount2
                RecordCount3
                RecordCount4
                RecordCount5
                RecordCount6
                ShowAlphaDefaultField
                visible when (not ShowAlphaDefaultField)
                    DefaultNumeric
                visible when (ShowAlphaDefaultField)
                    DefaultAlpha
            visible when (FieldDataType.Time)
                UseCurrentSystemTime
                ShowAlphaDefaultField
            visible when (not ShowAlphaDefaultField)
                DefaultTime
            visible when (ShowAlphaDefaultField)
                DefaultAlpha
            visible when (FieldDataType.TimeStamp)
                UseCurrentSystemTimeStamp
                ShowAlphaDefaultField
                visible when (not ShowAlphaDefaultField)
                    DefaultTimeStamp
                visible when (ShowAlphaDefaultField)
                    DefaultAlpha
            visible when (FieldDataType.Text)
                DefaultText
        visible when (ColumnUsesField)
            single column
                ExcludeFieldWhenDeterminingIfRowChanged
            header3 of "AlternateFieldsToIdentifyThatThisColumnChanged"
            three column
                AlternateChangedField1
                    no label
                    align as label
                AlternateChangedField2
                    no label
                AlternateChangedField3
                    no label
