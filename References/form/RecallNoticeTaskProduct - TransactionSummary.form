TransactionSummary is a SummaryForm
    Layout
        blank line
        header3 of "Product:<RecallProduct.ProductName>"
            show border color of black
            shade
        header3 of "_"
        blank line
        visible when (HasLocationActivity)
            single column
                RecallProduct.RecallTaskInstruction
                RecallProduct.TaskInstructionDescription
                    no label
        blank line
        visible when (ShowProductQuestions)
            blank line
            header2 of "Questions"
                show border color of black
                center
            header2 of "_"
            list is RecallProduct.RecallProductQuestion set.RecallProductQuestionPrint
        visible when (HasSerialNumbers)
            blank line
            header2 of "RecallNoticeSerialNumbers"
                show border color of black
                center
            header2 of "_"
            list is RecallProduct.RecallProductSerial set.TaskSerialNumbers
        visible when (HasLotNumbers)
            blank line
            header2 of "RecallNoticeLotNumbers"
                show border color of black
                center
            header2 of "_"
            list is RecallProduct.RecallProductLot set.TaskLotNumbers
        visible when (HasEquipmentSerialNumbers)
            blank line
            header2 of "RecallNoticeSerialNumbers"
                show border color of black
                center
            header2 of "_"
            list is RecallProduct.RecallEquipmentSerial set.TaskEquipmentSerialNumbers
        header2 of "Transactions"
            show border color of black
            center
        header2 of "_"
        visible when (HasTransactionRequisitionLines)
            blank line
            list is RequisitionLineTransactionActivityRel.RecallRequisitionLines
        visible when (HasTransactionPurchaseOrderLines)
            blank line
            list is PurchaseOrderLineTransactionActivityRel.RecallPurchaseOrderLines
        visible when (HasTransactionPolinesrc)
            blank line
            list is PolinesrcTransactionActivityRel.RecallPolinesrc
        visible when (HasTransactionInventory)
            blank line
            list is InventoryTransactionActivityRel.RecallItemLocations
        visible when (HasTransactionParLocations)
            blank line
            list is ParLocationTransactionActivityRel.RecallParLocations
        visible when (HasTransactionLotNumbers)
            blank line
            list is LotNumberTransactionActivityRel.RecallStockOnHandDetail
        visible when (HasTransactionSerialNumbers)
            blank line
            list is SerialNumberTransactionActivityRel.RecallItserial
        visible when (HasTransactionIssues)
            blank line
            list is RecentIssueTransactionActivityRel.RecallIssues
        visible when (HasTransactionIssueDetail)
            blank line
            list is IssueDetailTransactionActivityRel.RecallIssuesDetail
        visible when (HasTransactionIntransitTransfers)
            blank line
            list is IntransitTransferTransactionActivityRel.RecallIntransitTransfers
        visible when (HasTransactionIntransitTransfersDetail)
            blank line
            list is IntransitTransferDetailTransactionActivityRel.RecallIntransitTransfersDetail
        visible when (HasTransactionContractLines)
            blank line
            list is ContractLineTransactionActivityRel.RecallContractLines
        visible when (HasTransactionProcurementTemplateLines)
            blank line
            list is ProcurementTemplateLineTransactionActivityRel.RecallProcurementTemplateLines
        visible when (HasTransactionSourcingEventLines)
            blank line
            list is SourcingEventLineTransactionActivityRel.RecallSourcingEventLines
        visible when (HasTransactionEquipment)
            blank line
            list is EquipmentTransactionActivityRel.RecallAssetItems
