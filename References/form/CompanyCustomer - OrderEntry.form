OrderEntry is a Form
    title is "OrderEntryDetail"
    Layout
        three column distributed
            column1
                paragraph
                    TransientDefaultCustomerShipTo
                        label is "DefaultCustomerShipTo"
                    TransientDefaultCustomerShipTo.Name
                        no label
                        display as text
                paragraph
                    TransientDefaultShippingLocation
                        label is "DefaultShippingLocation"
                    TransientDefaultShippingLocation.Name
                        no label
                        display as text
                paragraph
                    TransientDefaultShippingMethod
                        label is "DefaultShippingMethod"
                    TransientDefaultShippingMethod.Description
                        no label
                        display as text
                TransientShipCompleteFlag
                    label is "ShipComplete"
                TransientPurchFrLoc
                    label is "PurchaseFrom"
                paragraph
                    TransientVendor
                        label is "Vendor"
                    TransientVendor.VendorName
                        no label
                        display as text
                paragraph
                    TransientShipTerm
                        label is "DeliveryTerms"
                    TransientShipTerm.Description
                        no label
                        display as text
                TransientProofOfDeliveryRequired
                    label is "ProofOfDeliveryRequired"
                visible when (TransientProofOfDeliveryRequired)
                    TransientProofOfDeliveryGracePeriod
                        label is "ProofOfDeliveryGracePeriod"
            column2
                paragraph
                    TransientOrderDiscountCode
                        label is "OrderDiscountCode"
                    TransientOrderDiscountCode.Description
                        no label
                        display as text
                paragraph
                    TransientCreditLimitHoldCode
                        label is "CreditLimitHoldCode"
                    TransientCreditLimitHoldCode.Description
                        no label
                        display as text
                paragraph
                    TransientOrderLimitHoldCode
                        label is "OrderLimitHoldCode"
                    TransientOrderLimitHoldCode.Description
                        no label
                        display as text
                OrderLimit
                TransientPickingPriority
                    label is "PickingPriority"
                paragraph
                    TransientLastReturnAuthorizationDate
                        label is "ReturnDate"
                    TransientAllowReturns
                        label is "AllowReturns"
                TransientBackorderAllowed
                    label is "AllowBackorders"
                TransientCashInAdvanceRequired
                    label is "CashInAdvanceRequired"
                visible when (TransientCashInAdvanceRequired)
                    TransientCashInAdvanceRequiredPercent
                        label is "CashInAdvanceRequiredPercent"
            column3
                paragraph
                    TransientSingleOrderHoldCode
                        label is "SingleOrderHoldCode"
                    TransientSingleOrderHoldCode.Description
                        no label
                        display as text
                TransientSingleOrderLimit
                    label is "SingleOrderLimit"
                TransientTotalCODOrderLimit
                    label is "TotalCashOnDeliveryOrderLimit"
                paragraph
                    TransientCashOnDeliveryOrderHoldCode
                        label is "CashOnDeliveryOrderHoldCode"
                    TransientCashOnDeliveryOrderHoldCode.Description
                        no label
                        display as text
                TransientCustomeOrderAcknowledgments
                    label is "AcknowledgmentRequired"
                TransientPORequiredForCustomerOrder
                    label is "P\\ORequired"
                TransientPortCode
                    label is "LoadingPort"
                TransientProformaCashInAdvanceRequired
                    label is "ProformaCashInAdvanceRequired"
                visible when (TransientProformaCashInAdvanceRequired)
                    TransientProformaCashInAdvanceRequiredPercent
                        label is "ProformaCashInAdvanceRequiredPercent"
        paragraph
            TransientCheckCreditOnOrderAdd
                label is "CreditCheckEntry"
            TransientCheckCreditBeforePickingList
                label is "CreditCheckPrint"
