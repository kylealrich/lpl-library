UpdateCompanyTemplate is an ActionForm
    action is UpdateAssetCompany
    title is "Transfer"
    Layout
        single column
            paragraph
                ToCompany
                    label is "To_Company"
                text of "<ToCompany.Name>"
            paragraph
                ToTemplate
                text of "<ToTemplate.Description>"
            paragraph
                ToAssetType
                    compact format
                text of "<ToAssetType.Description>"
            paragraph
                ToAssetDimensionGroup
                text of "<ToAssetDimensionGroup.Description>"
            paragraph
                ToAssetAccountGroup
                text of "<ToAssetAccountGroup.Description>"
            paragraph
                ToDefaultTransactionDimensions
                    compact format
            paragraph
                ToLocation
                text of "<ToLocation.Description>"
            paragraph
                ToDivision
                text of "<ToDivision.Description>"
