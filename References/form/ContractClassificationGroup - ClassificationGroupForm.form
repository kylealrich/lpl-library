ClassificationGroupForm is a CompositeForm
    is primary
    title is "ContractClassificationGroup"
    context form is ContractClassificationGroupContext
    show panel control on left
    ContractClassifications is a Panel
        list is ContractClassificationGroupMembersRel.ClassificationGroupMembers
    ActorsAssignedToThisGroup is a Panel
        list is ActorContextRel.primary
    ActorsWithClassificationContext is a Panel
        visible when (HasClassificationContext)
        list is ClassificationActorContextRel.primary
    ActorsWithSubclassificationContext is a Panel
        visible when (HasSubclassificationContext)
        list is SubclassificationActorContextRel.primary
