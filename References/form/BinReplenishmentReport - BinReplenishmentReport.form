BinReplenishmentReport is an ActionForm
    action is BinReplenishmentReport
    Layout
        two column
            Company
            Company.Name
                no label
                display as text
        two column
            Location
            Location.Name
                no label
                display as text
        single column
            text of "\\or"
                bold
        two column
            ReportGrp
                label is "ReportGroup"
            ReportGrp.Name
                no label
                display as text
        text of "_"
        header2 of "_"
        text of "_"
        two column
            BinGroup
            BinGroup.Description
                no label
                display as text
        two column
            InvenMajcl
                label is "MajorInventoryClass"
                show up to 10 characters
            InvenMajcl.Description
                no label
                display as text
        two column
            InvenMincl
                label is "MinorInventoryClass"
                show up to 10 characters
            InvenMincl.Description
                no label
                display as text
        blank line
        two column
            Uom
                label is "UnitOfMeasure"
            Uom.Description
                no label
                display as text
        two column
            SeqOpt
                label is "Sequence"
            AllocateDetails
                label is "CreateDetails"
        single column
            CreateDocOpt
                label is "CreateDocument"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                BinReplenishmentReportReport_RDG
            column2 of 25%
                BinReplenishmentReportReport_XPT
