AwardSummaryInformation is a Form
    title is "Event<SourcingEvent>,Line<SourcingEventLine>:_AwardSummaryInformation"
    disable field options
    called out actions only
    Actions
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is Draft.Create
            action icon is create
            label is "_"
            hide when invalid
        action is save
            label is "_"
        action is Draft.Delete
            label is "_"
        action is refresh
            label is "_"
    Layout
        display as text
            two column
                Company
                Company.Name
            two column
                paragraph
                    SourcingEvent
                        label is "Event#"
                    SourcingEvent.LastVersionNumber
                        no label
                SourcingEvent.Name
                    label is "EventName"
            two column
                SourcingEventLine
                    label is "Line#"
                SourcingEventLine.OutputType
            two column
                SourcingEventLine.ItemNumber
                    label is "Item"
                paragraph
                    SourcingEventLine.ItemDescription
                        label is "ItemDescription"
                    SourcingEventLine.ItemDescription2
                        no label
            visible when (SourcingEventLine.HasThirdDescription)
                single column
                    SourcingEventLine.ItemDescription3
                        label is "LongItemDescription"
            two column
                SourcingEventLine.GlobalTradeItemNumber
                    label is "GTIN"
                SourcingEventLine.GlobalTradeItemNumber.FullDescription
                    label is "GTINDescription"
            single column
                AwardReason
            two column
                Quantity
                    label is "ResponseQuantity"
                visible when (PoData)
                    AwardQuantity
                visible when (CmData)
                    AwardForContract
            single column
                visible when (PoData)
                    DeliveryDateOverride
            two column
