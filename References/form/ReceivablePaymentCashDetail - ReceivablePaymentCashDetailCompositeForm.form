ReceivablePaymentCashDetailCompositeForm is a CompositeForm
    is primary
    title is "Cash_Detail<ReceivablePaymentCashDetail>_for_Payment<ReceivablePayment.PaymentNumber>"
    context form is ReceivablePaymentCashDetailContextForm
        background is theme color
    FinanceStructure is a Panel
        form is FinanceStructure
    CurrencyAmounts is a Panel
        visible when (ReceivablePaymentCashDetail entered)
        form is CurrencyAmounts
    AppliedCashDetails is a Panel
        visible when (ShowAppliedCashDetails)
        list is CashDetailApplication set.primary
