PaymentOutputFileHeaderForm is a CompositeForm
    is primary
    title is "PaymentOutputFile"
    context form is PaymentOutputFileHeaderContext
        background is theme color
    Actions
        action is PrintCheck
            action icon is print
            mouse over text is "Print_\\Checks"
            hide when invalid
        action is MarkCheckAsPrinted
            action icon is finalize
            mouse over text is "Mark_\\ChecksAs_\\Printed"
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "PreviousRecord"
        action is next record
            label is "_"
            mouse over text is "NextRecord"
        action is refresh
            label is "_"
    OutputFileDetails is a Panel
        list is PaymentOutputFileDetail set.PaymentOutputFileDetailList
    FlatFileOutput is a Panel
        visible when (IsFlatFile)
        list is FlatFileOutput set.SortedFlatFileOutputList
    KyribaConnection is a Panel
        visible when (IsKyribaIntegration)
        form is KyribaConnectionResponseForm
    GTreasuryConnection is a Panel
        visible when (IsGTreasuryIntegration)
        form is GTreasuryConnectionResponseForm
