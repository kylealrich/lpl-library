ManualFormsReservationForm is an ActionForm
    action is ReserveManualChecks
    title is "ReserveManualCheckNumbers"
    Layout
        single column
            column1
                paragraph
                    PrmVendorGroup
                        label is "VendorGroup"
                    PrmVendorGroup.Description
                        no label
                        display as text
                paragraph
                    PrmPayGroup
                        label is "PayGroup"
                    PrmPayGroup.Name
                        no label
                        display as text
                paragraph
                    PrmBankTransactionCode
                        label is "BankTransactionCode"
                    PrmBankTransactionCode.Description
                        no label
                        display as text
                paragraph
                    PrmBeginPaymentRange
                        label is "ReservePaymentNumberRangeFrom"
                    PrmEndOfPaymentRange
                        label is "ReservePaymentNumberRangeTo"
