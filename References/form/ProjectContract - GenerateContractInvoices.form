GenerateContractInvoices is an ActionForm
    action is GenerateContractInvoices
    Layout
        single column
            ContractGroup
            paragraph
                DateRange.Begin
                    label is "PostingDateSelectionRange"
                DateRange.End
                    label is "Through"
            ThroughDate
                label is "TransactionThroughDate"
            SelectionGroup
            FundingSourceDate
            InvoiceDate
            ApplyRetainers
            InvoiceMaximum
            visible when (IsThirdPartyInvoicePrint)
                PrintStatus
            paragraph
                BillingGroup
                BillingGroup.Description
                    no label
                    display as text
