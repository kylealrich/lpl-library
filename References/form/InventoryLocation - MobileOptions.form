MobileOptions is a Form
    title is "Options"
    Layout
        header3 of "Accounts"
        single column
            VendorReturnSuspenseAccount
                label is "ReturnsSuspense"
                compact format
            InspectionHoldAccount
                label is "InspectionHold"
                compact format
            FreightAccount
                label is "Freight"
                compact format
            HandlingAccount
                label is "Handling"
                compact format
            ScrapAccount
                label is "Scrap"
                compact format
        header3 of "Defaults"
        single column
            DisableOvershipmentMessages
            ProcessLevel
                label is "POProcessLevel"
                select is MobilePayablesProcessLevelsSelect
            POCode
                select is MobilePOCodeSelect
            DefaultDropshipPurchaseOrder
                label is "DropshipPurchaseOrder"
            TaxCode
                label is "SellTaxCode"
                select is MobileTaxCodeSelect
            PurchaseOrderTaxCode
                label is "PurchasingTaxCode"
                select is MobileTaxCodeSelect
            ReceivingDays
        header3 of "Roles"
        single column
            Vendor
                select is MobileVendorSelectSearch
            PurchaseFromLocation
                label is "PurchaseFrom"
                select is MobilePurchaseFromLocationsSelect
            Customer
                select is MobileCustomerSelect
            CustomerShipTo
                label is "ShipTo"
                select is MobileCustomerShipToSelect
            ReplenishmentRequester
                label is "Requester"
                select is MobileRequesterSelect
        single column
            blank line
            PricingGroup
                select is MobileActivePricingGroupsSelect
            ERSCapable
            PurchaseOrderApprovalType
            blank line
        single column
            AllocationPurpose
            ReceivingAllocation
            PrintLabels
                label is "PrintChargeLabels"
        header3 of "RequisitionOptions"
        paragraph
            WarehouseProcessType
                label is "ProcessType"
                select is MobileWarehouseProcessTypesSelect
            WarehouseProcessType.Description
                no label
                display as text
            ProcessStatus
                label is "RunStatus"
                display as text
        single column
            UseGLCategoryIssueAccountingUnitAsDefault
                label is "UseAccountGroupIssueAccountingUnit_as_default"
            OneSourceDocumentToOnePO
                label is "SingleDocument_to_aPurchaseOrder"
            blank line
        two column
            EAMLocation
                label is "LocationType"
            RetailLocationType
        single column
            MSCMDelivery
            blank line
        two column
            PreferredBin
                label is "DefaultPreferredBin"
                select is MobileBinSelect
            PreferredHoldBin
                label is "DefaultPreferredHoldBin"
                select is MobileBinSelect
        single column
            DirectPayCertificate
        header3 of "ItemMasterSync"
        single column
            ItemSyncOption
