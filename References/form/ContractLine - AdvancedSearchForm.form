AdvancedSearchForm is a Form
    title is "ItemSearch"
    Layout
        three column distributed
            Contract.Supplier
                label is "Supplier"
            Contract.Vendor
            OnHold
        three column distributed
            ItemNumber
            VendorItem
                show up to 10 characters
            PersonalProtectiveEquipment
        three column distributed
            Manufacturer
                label is "ManufacturerCode"
                compact format
            ManufacturerNumber
            CriticalItem
        three column distributed
            QuantityOrdered
            Contract.ContractClassification
                label is "Classification"
                show up to 10 characters
            PreferredLine
        single column
            TextSearch
                label is "ItemKeyword"
                use text area of 2 lines
