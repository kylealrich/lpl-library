MobileRecallActivityContext is a Form
    title is "Activity"
    Layout
        paragraph
            RecallNotice
                display as text
            WorkingId
                display as text
            Status
                display as text
        single column
            Name
                display as text
        two column table
            column1
                RecallDate
                    label is "AnnouncementDate"
                    display as text
            column2
                RecallReceivedDate
                    display as text
        single column
            visible when (ClosedNotice)
                ClosedTimestamp
                    label is "CloseDate"
                    display as text
            button of "PrintActivity"
                link is AuditReport
