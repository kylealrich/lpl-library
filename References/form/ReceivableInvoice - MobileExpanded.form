MobileExpanded is a Form
    title is "Expanded"
    Layout
        two column
            paragraph
                visible when (IsReleased)
                    ReceivableGeneralLedgerCode
                        label is "AccountsReceivableCode"
                        display as text
                visible when (IsUnreleased)
                    ReceivableGeneralLedgerCode
                        label is "AccountsReceivableCode"
                ReceivableGeneralLedgerCode.Description
                    no label
                    display as text
            paragraph
                SalesRepresentative
                SalesRepresentative.Name
                    no label
                    display as text
        two column
            paragraph
                BankTransactionCode
