ManufacturerID is a CompositeForm
    is primary
    context form is ManufacturerIDContext
        background is theme color
    Details is a Panel
        visible when (not RecordExists)
        form is ManufacturerIDDetail
    Main is a Panel
        visible when (RecordExists)
        form is ManufacturerIDMain
    ManufacturerCode is a Panel
        visible when (RecordExists)
        list is ManufacturerIDManufacturer set.ManufacturerCodesList
    DistributionCenter is a Panel
        visible when (RecordExists)
        list is ManufacturerIDDistributionCenter set.DistributionCentersList
    ShipToFacility is a Panel
        visible when (RecordExists)
        list is ManufacturerIDShipToFacility set.ShipToFacilitysList
