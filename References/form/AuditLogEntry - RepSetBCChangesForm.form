RepSetBCChangesForm is a Form
    title is "ReplicationSetBusinessClassChangesForm"
    disable field options
    Layout
        visible when (TransactionRecordExists)
            paragraph
                RepSetBCRel.ReplicationSet
                    header2
                    display as text
                RepSetBCRel.BusinessClass
                    header2
                    display as text
                button of "<BusinessView>Details"
                    link is RepSetBCNav
                    display as link
        visible when (not TransactionRecordExists)
            paragraph
                text of "<BusinessView>RecordDoesNotExist"
