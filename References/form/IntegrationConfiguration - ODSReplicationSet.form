ODSReplicationSet is a Form
    title is "ReplicationSet"
    Layout
        single column
            paragraph
            paragraph
                text of "SchedulingAsyncErrorsExist:"
                button of "<ODSSchedulingAsyncErrorsExist>"
                    link is ODSSchedulingAsyncNavigation
                    display as link
                text of "ReplicationAsyncErrorsExist:"
                button of "<ODSAsyncErrorsExist>"
                    link is ODSRepSetAsyncNavigation
                    display as link
        visible when (NoODSReplicationScheduled)
            single column
                text of "No_Scheduled_Replication"
                button of "Schedule_Replication"
                    action is ReplicateODSData
                    label is "No_Scheduled_Replication"
        visible when (not NoODSReplicationScheduled)
            single column
                paragraph
                    button of "<ODSRepSetCount>"
                        link is ODSRepSetMainAsyncNavigation
                        label is "NumberOf_Replications_Scheduled"
                        display as link
                ODSRepSetSchedFrequencyAndType
                    label is "SweepFrequency"
                paragraph
                    button of "<ODSRepSetNextRun>"
                        link is ODSRepSetMainAsyncNavigation
                        label is "Next_Replication_Run:"
                        display as link
                paragraph
                    visible when (ODSRepSetLastRunEntered)
                        button of "<ODSRepSetLastRun>"
                            link is ODSRepSetMainAsyncNavigation
                            label is "LastTimeToRun:"
                            display as link
                button of "CancelODSScheduledJobs"
                    action is CancelODSJobs
