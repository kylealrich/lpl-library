AddToRequisition is an ActionForm
    action is AddToRequisition
        on completion navigate to RequisitionsView
    Layout
        paragraph
            PrmCompany
                label is "Company"
        paragraph
            PrmRequisition
                label is "Requisition"
        paragraph
            PrmQuantity
                label is "Quantity"
        paragraph
            PrmUOM
                label is "UnitOfMeasure"
        paragraph
            PrmCostOption
                label is "CostOption"
        visible when (false)
            paragraph
                NewRequisition
            paragraph
                NewRequisitionLine
        visible when (not ContractItem)
            paragraph
                PrmTransactionUnitCost
                    label is "UnitCost"
        visible when (ContractItem)
            paragraph
                DefaultUnitCost
                    label is "UnitCost"
                    display as text
        paragraph
            PrmProject
                label is "Project"
        paragraph
            PrmAsset
                label is "Asset"
        paragraph
            PrmAssetTemplate
                label is "AssetTemplate"
        paragraph
            PrmCostCode
                label is "CostCode"
        paragraph
            PrmSourcingEventRequired
                label is "SourcingEventRequired"
        visible when (not ContractItem)
            paragraph
                PrmVendor
                    label is "Vendor"
        visible when (ContractItem)
            paragraph
                DerivedVendor
                    label is "Vendor"
                    display as text
        paragraph
            PrmPurchaseFromLocation
                label is "PurchaseFrom"
        paragraph
            PrmDescription
                label is "Description"
        paragraph
            PrmManufacturer
                label is "Manufacturer"
            PrmManufacturerNumber
                label is "ManufacturerNumber"
        paragraph
            PrmByPassUnitCost
                label is "ByPassUnitCost"
        paragraph
            PrmFromMobile
