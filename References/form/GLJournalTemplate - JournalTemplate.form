JournalTemplate is a CompositeForm
    is primary
    title is "JournalTemplate_<GLJournalTemplate>_<Description>"
    context form is MainContext
    MainPanel is a Panel
        title is "Main"
        form is Main
    DetailsPanel is a Panel
        title is "TemplateLines"
        visible when (TemplateExists)
        list is GLJournalTemplateDetail set.GLJournalTemplateDetailList
    WorkLinesPanel is a Panel
        title is "WorkingLines"
        visible when (WorkExists)
        list is GLJournalTemplateWork set.GLJournalTemplateWorkList
    TotalsPanel is a Panel
        title is "Totals"
        visible when (TemplateExists)
        form is JournalTemplateTotalsForm
    SpeedEntryPanel is a Panel
        title is "SpeedTransactionEntry"
        visible when (TemplateExists)
        list is GLJournalTemplateDetail set.GLJournalTemplateSpeedEntryList
