WorkFileForm is a Form
    is primary
    title is "WorkFile"
    Layout
        two column
            WorkFileClass
                display only
            WorkFileName
                display only
        two column
            ScheduledForDeletion
                display only
            IsOpen
                display only
        single column
            ProgramName
                display only
            SortSetName
                display only
        blank line
        two column
            update stamp
                label is "LastUpdated"
                display only
            update stamp.actor
                label is "Actor"
                display only
        two column
            create stamp
                label is "Created"
                display only
            create stamp.actor
                label is "Actor"
                display only
