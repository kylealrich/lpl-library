DemandCancellation is an ActionForm
    action is CancelDemand
    Layout
        paragraph
            Company
                display as text
            Company.Name
                no label
                display as text
        paragraph
            InventoryLocation
                label is "Location"
                display as text
            InventoryLocation.Name
                no label
                display as text
        paragraph
            WarehouseDemand.DemandDocument
                label is "DemandDocument"
                display as text
        paragraph
            WarehouseDemand.DemandSystemCode
                display as text
            WarehouseDemandLine.DemandDocumentType
                display as text
        paragraph
            Item
                display as text
            visible when (IsCatchWeight)
                TransactionUOM
                    label is "UOM"
                    display as text
            visible when (not IsCatchWeight)
                DerivedUOM
                    label is "StockUOM"
        paragraph
            WarehouseDemandLine.LineNumber
                label is "LineNumber"
        paragraph
            DerivedQuantityDisplay
                label is "DemandQuantity"
                display as text
            DerivedCancelableQuantityDisplay
                label is "CancelableQuantity"
                display as text
            DerivedCancelledQuantityDisplay
                label is "CancelledQuantity"
                display as text
        paragraph
            PrmCancelQuantity
                label is "QuantityToCancel"
