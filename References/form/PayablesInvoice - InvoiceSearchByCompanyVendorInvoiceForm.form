InvoiceSearchByCompanyVendorInvoiceForm is a SearchForm
    title is "SearchInvoices"
    Layout
        five column distributed
            Company
                mouse over text is "UseThisListToQuicklyFind_InvoicesForASpecific_CompanyAnd_Vendor"
            Vendor
            InvoiceSearchDocument
                label is "InvoiceKeyword"
                mouse over text is "SearchOn_Invoice_Number,_System_Invoice_Number,_Voucher_Number,_Vendor_Name,_Pay_Vendor_Name,Or_Company_Name"
                use text area of 1 lines
            Invoice
            Status
        five column distributed
            InvoiceAmount.CurrencyAmount
                label is "InvoiceAmount"
            InvoiceDate
            Vendor.VendorSearchDocument
                label is "VendorKeyword"
                mouse over text is "SearchOn_Vendor_Number,_Name,_Search_Name,_Legal_Name,_Reference_Number,Or_Tax_ID"
                use text area of 1 lines
            ExternalPurchaseOrder
            APClerk
        header2 of "Additional"
            collapsible
                initially collapsed
                use show more or less affordance
            five column distributed
                ProcessLevel
                DueDate
                    label is "DueDate"
                Vendor.VendorClass
                PayablesInvoice
                    label is "SystemInvoiceNumber"
                ProcessingTeam
            five column distributed
                AuthorityCode
                MatchProcessType
                    label is "ProcessType"
                RemitToCode
                ServiceContract
                    select is ServiceContractsForInvoicing
                ResponsiblePerson
            five column distributed
                ErrorsExists
                    label is "Error"
                MatchStatus
                PayGroup
                VoucherNumber
                    compact format
                InvoiceCurrency
                    label is "Currency"
            five column distributed
                HasRetainage
                InvoiceTypeForSearch
                DiversityCode
