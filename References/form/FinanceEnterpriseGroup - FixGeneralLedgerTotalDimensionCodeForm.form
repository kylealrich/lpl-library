FixGeneralLedgerTotalDimensionCodeForm is an ActionForm
    action is FixGeneralLedgerTotalDimensionCode
    title is "FixGlobalLedgerTotalDimensionCode"
    Layout
        single column
            PrmStartingPeriod
                select is GeneralLedgerCalendarPeriodEndDateList
            PrmEndingPeriod
                select is GeneralLedgerCalendarPeriodEndDateList
            PrmChartSection
            PrmAccountingEntityOption
            visible when (PrmAccountingEntityOption.Single)
                PrmAccountingEntity
            visible when (PrmAccountingEntityOption.Group)
                PrmAccountingEntityGroup
