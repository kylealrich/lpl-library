LeaseTransferWizardMain is a Form
    title is "LeaseDetails"
    Layout
        paragraph
            Lease
                display only
            Lease.Description
                display only
            Lease.Company
                display only
        single column
            TransferDate
        blank line
        header3 of "ToCompanyDetails"
        ToCompany
            label is "Company"
        visible when (ShowDebtAccount)
            paragraph
                ToDebtAccount
                    label is "LeaseLiability"
                    compact format
                ToDebtAccount.GeneralLedgerChartAccount.AccountDescription
                    label is "_"
                    display as text
        visible when (ShortTermLiabilityAccounting)
            paragraph
                ToShortTermLiabilityAccount
                    label is "ShortTermLiabilityAccount"
                    compact format
                ToShortTermLiabilityAccount.GeneralLedgerChartAccount.AccountDescription
                    label is "_"
                    display as text
            paragraph
                ToLongTermLiabilityAccount
                    label is "LongTermLiabilityAccount"
                    compact format
                ToLongTermLiabilityAccount.GeneralLedgerChartAccount.AccountDescription
                    label is "_"
                    display as text
        paragraph
            ToInterestAccount
                label is "InterestAccount"
                compact format
                display as required when (IsFinanceLease)
            ToInterestAccount.GeneralLedgerChartAccount.AccountDescription
                label is "_"
                display as text
        paragraph
            ToDeferredRentAccount
                label is "DeferredRentAccount"
                compact format
            ToDeferredRentAccount.GeneralLedgerChartAccount.AccountDescription
                label is "_"
                display as text
        paragraph
            ToLongTermOperatingLeaseExpense
                label is "LongTermOperatingLease"
                compact format
                display as required when (IsOperatingLease)
            ToLongTermOperatingLeaseExpense.GeneralLedgerChartAccount.AccountDescription
                label is "_"
                display as text
