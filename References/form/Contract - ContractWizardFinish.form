ContractWizardFinish is a Form
    title is "FinishContract"
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        display as text
            single column
                ContractGroup
                Contract
                Name
                WorkingContractID
                blank line
        single column
            text of "ClickTheLinkBelowToViewTheContract."
            button of "Contract"
                link is ViewContract
                display as link
            blank line
            button of "ReleaseMembers"
                visible when (HasMembersToRelease)
                action is ReleaseMembers
            button of "ReleaseContract"
                visible when (ContractStatus.Draft)
                action is Draft.Release
            button of "ActivateContract"
                visible when (ContractStatus.ReadyToActivate)
                action is ReadyToActivate.Activate
