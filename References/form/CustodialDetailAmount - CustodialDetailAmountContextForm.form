CustodialDetailAmountContextForm is a Form
    title is "<FinanceEnterpriseGroup.FinanceDimension10Label>_DetailAmount"
    Layout
        four column distributed
            column1
                CustodialAccountManagement.CustodialSummary
                    display as text
                visible when (EditLevel.Individual)
                    CustodialPosting.Expendable
                        display as text
            column2
                visible when (EditLevel.Group)
                    EditGroupName
                        display as text
                visible when (EditLevel.Individual)
                    CustodialPosting.DisplayDimension
                        display as text
                    CustodialPosting.ReceiveInterest
                        display as text
            column3
                visible when (EditLevel.Individual)
                    visible when (not DoesExist)
                        CustodialPosting
                            label is "<FinanceEnterpriseGroup.FinanceDimension10Label>Detail"
                            select is CustodialAccountManagement.CustodialSummary.DescendantsRel.FinanceDimension10Select
                    visible when (DoesExist)
                        CustodialPosting
                            label is "<FinanceEnterpriseGroup.FinanceDimension10Label>Detail"
                            display as text
                    StatusMessage
                        label is "Status"
                        display as tag
                            color of yellow when (not CustodialPosting.Active)
                            color of green when (CustodialPosting.Active)
            column4
                visible when (EditLevel.Individual)
                    CustodialPosting.Description
                        display as text
                    button of "<TransferMessage>"
                        form invoke is CreateUnreleasedDetailTransfer
                    visible when (DoesUnreleasedTransferExists)
                        button of "ResetTransfer"
                            action is ResetDetailTransfer
                        button of "ReleaseTransfer"
                            action is ReleaseDetailTransfer
