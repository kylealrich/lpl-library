PurchaseOrderAuditInquiry is a Form
    Layout
        paragraph
            Company
                select is POCompanies
            Company.Name
                no label
                display as text
            PurchaseOrder
                label is "P\\ONumber"
        paragraph
            PurchaseOrder.PORelease
                label is "P\\ORelease"
        paragraph
            PurchaseOrder.POCode
                label is "P\\OCode"
        paragraph
            PurchaseOrderRevision
                label is "P\\ORevision"
                select is RevisionsforaPurchaseOrder
        blank line
        paragraph
            PurchaseOrderLine
                label is "P\\OLineNumber"
                select is LinesForAPOByItem
        paragraph
            AddOnCharge
                label is "A\\O\\CCode"
                select is ActiveAddOnCostCodes
            AddOnCharge.Description
                no label
                display as text
        paragraph
            PurchaseOrderAudit.LineSequence
                label is "SequenceNumber"
        paragraph
            PurchaseOrderAuditType
                label is "AuditType"
        blank line
        paragraph
            PurchaseOrderAudit.UpdateDate
                label is "UpdateDate"
        paragraph
            Description
                label is "FieldDescription"
        paragraph
            PrintRevisions
        paragraph
            UserId
                label is "UserI\\D"
        paragraph
            PurchaseOrderAuditAction
                label is "AuditAction"
        blank line
        paragraph
            BeforeImage
        paragraph
            AfterImage
