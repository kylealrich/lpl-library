OpenLaborForm is a CompositeForm
    title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Employee"
    context form is ProjectSchedulePeriodContext
        background is theme color
    Actions
        action is LaborDistributionEmail
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is save
            label is "_"
            mouse over text is "Save"
        action is refresh
            label is "_"
            mouse over text is "Refresh"
    OpenLaborDistribution is a Panel
        list is ProjectAssignmentLaborRel.ProjectLaborListTalentProfile3
