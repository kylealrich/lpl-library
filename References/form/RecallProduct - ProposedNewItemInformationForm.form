ProposedNewItemInformationForm is a Form
    title is "NewItemProposalInformation"
    Layout
        display as text
            single column
                ProductDescription
                    label is "ProposedNewItemDescription"
                    use text area of 5 lines
            single column
                ProposedValue
                    use text area of 2 lines
            five column distributed
                ProposedVendorItem
                    label is "VendorItem"
                visible when (not RecallNotice.ItemGroupRel.AutomaticItemNumbering)
                    ProposedManualItemNumber
                ProposedManufacturerNumber
                    label is "ManufacturerNumber"
                ProposedGlobalTradeItemNumber
                    label is "GTIN"
                LatexFree
            four column distributed
                ProductServiceLine
                UnitCost
                ExpectedMonthlyUsage
                visible when (CurrentContractEntered)
                    paragraph
                        text of "CurrentContract"
                        button of "<CurrentContract>"
                            link is ContractInformation
                            display as link
            visible when (ItemsToReplaceEntered)
                header3 of "ItemBeingReplaced"
                four column distributed
                    VendorItem
                    ManufacturerInformation.ManufacturerNumber
                    GlobalTradeItemNumber
                    visible when (ItemEntered)
                        ItemCreated
            header3 of "UnitsOfMeasure"
            four column table
                column headers
                    "UnitOfMeasure"
                    "UnitsPerEach"
                    "UseForBuying"
                    "UseForIssues"
                column1
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[1].UnitOfMeasure
                        no label
                    visible when (SecondUOMEntered)
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[2].UnitOfMeasure
                            no label
                    visible when (ThirdUOMEntered)
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[3].UnitOfMeasure
                            no label
                    visible when (FourthUOMEntered)
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[4].UnitOfMeasure
                            no label
                column2
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[1].UOMConversion
                        no label
                    visible when (SecondUOMEntered)
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[2].UOMConversion
                            no label
                    visible when (ThirdUOMEntered)
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[3].UOMConversion
                            no label
                    visible when (FourthUOMEntered)
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[4].UOMConversion
                            no label
                column3
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[1].UseForBuying
                        no label
                    visible when (SecondUOMEntered)
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[2].UseForBuying
                            no label
                    visible when (ThirdUOMEntered)
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[3].UseForBuying
                            no label
                    visible when (FourthUOMEntered)
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[4].UseForBuying
                            no label
                column4
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[1].UseForIssues
                        no label
                    visible when (SecondUOMEntered)
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[2].UseForIssues
                            no label
                    visible when (ThirdUOMEntered)
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[3].UseForIssues
                            no label
                    visible when (FourthUOMEntered)
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[4].UseForIssues
                            no label
