PurgeNotToBePostedTransactionDetail is a Form
    is primary
    Layout
        visible when (false)
            single column
                FinanceEnterpriseGroup
        single column
            EntityOption
                label is "<FinanceEnterpriseGroup.AccountingEntityLabel>Option"
            visible when (EntityOption.Single)
                AccountingEntity
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>"
            visible when (EntityOption.Group)
                AccountingEntityGroup
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>Group"
        blank line
        single column
            DateOption
                label is "DateOption"
            visible when (DateOption.Year)
                PostingDateYear
                    label is "PostingDateYear"
        visible when (DateOption.Range)
            two column
                PostingDateRange.Begin
                    label is "PostingDateRange"
                PostingDateRange.End
                    label is "Thru"
