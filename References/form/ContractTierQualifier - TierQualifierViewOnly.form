TierQualifierViewOnly is a Form
    Layout
        display as text
            single column
                ContractTier.TierTitle
                ContractTier.Description
            paragraph
                PurchaseType
                PurchaseBy
            single column
                PurchaseOperator
            paragraph
                PurchaseAmtPct.PurchasePercent
                    label is "PurchasePercent_ofAllPurchases"
                PurchaseAmtPct.PurchaseAmount
                    label is "PurchaseAmount"
            visible when (PurchaseType.TargetedPurchases)
                paragraph
                    TargetedPurchasesFromDate
                        label is "TargetedPurchasesFromDate"
                    TargetedPurchasesToDate
                        label is "TargetedPurchasesToDate"
            three column
                PurchaseQuantity
                    label is "Quantity"
                PurchaseBeginRange
                    label is "BeginDateForQuantityQualifier"
                PurchaseEndRange
                    label is "EndDate"
            single column
                OrGroup
                    label is "Group"
