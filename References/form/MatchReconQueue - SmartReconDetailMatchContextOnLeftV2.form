SmartReconDetailMatchContextOnLeftV2 is a Form
    Layout
        visible when (not FilesRebuilt)
            visible when (View.DetailMatch)
                paragraph centered
                    button of "ShowExceptions" with icon.refresh
                        action is ResetWorkFiles
                        raise red alert when (true)
                            mouse over text is "MustClickOnShowExceptionsToBegin"
            visible when (View.InvoiceMatch)
                paragraph centered
                    button of "Start" with icon.refresh
                        action is ResetWorkFiles
                        raise red alert when (true)
                            mouse over text is "ClickToBeginSelectingInvoicesAndReceipts"
            header4 of "_"
        visible when (AllowDetailMatch)
            paragraph centered
                text of "AllExceptionsSelected."
                    header3
                    bold
            paragraph centered
                text of "ReadyToMatch!"
                    header3
                    bold
            paragraph centered
                button of "Match" with icon.connect
                    action is SmartReconDetailMatch
                    is primary
            header2 of "_"
        visible when (AllowInvoiceMatch)
            paragraph centered
                text of "ReadyToMatch!"
                    header3
                    bold
            paragraph centered
                button of "Match" with icon.connect
                    action is SmartReconInvoiceMatch
                    is primary
            header2 of "_"
        display as text
            paragraph
                button of "Vendor<Vendor.RepresentativeText>"
                    link is VendorNav
                    display as link
        visible when (not PayablesInvoice.HasBeenMatched)
            display as text
                paragraph
                    button of "Invoice<Invoice>"
                        link is PayablesInvoiceLink
                        display as link
                    visible when (PrepayPostDateNeedsUpdating)
                        button of "UpdatePostDate"
                            action is UpdatePostDate
                            display as link
                            raise red alert when (PrepayPostDateNeedsUpdating)
                                mouse over text is "PostDateIsNotWithinValidEntryDates"
                paragraph
                    button of "PO<PurchaseOrder>"
                        link is PurchaseOrderLink
                        display as link
                visible when (View.DetailMatch)
                    two column distributed
                        PayablesInvoice.InvoiceAmount.CurrencyAmount
                            label is "InvoiceAmount"
                            header4
        visible when (PayablesInvoice.HasBeenMatched)
            display as text
                paragraph
                    button of "Invoice<Invoice>"
                        link is PayablesInvoiceLink
                        display as link
                paragraph
                    button of "PO<PurchaseOrder>"
                        link is PurchaseOrderLink
                        display as link
        visible when (FilesRebuilt)
            visible when (View.DetailMatch)
                visible when (ShowDetailWorkingTotals)
                    header4 of "WorkingSet"
                    two column
                        column1
                            text of "_"
                            text of "Invoice"
                            text of "Receipt"
                            text of "Difference"
                                display as tag
                                    color of orange
                        column2
                            text of "_____Quantity"
                                right align
                                bold
                            PendingInvoiceLineQuantity
                                no label
                                right align
                                display as text
                                display 1 decimals
                            PendingReceiptLineQuantity
                                no label
                                right align
                                display as text
                                display 1 decimals
                            PendingLineQuantityDifference
                                no label
                                right align
                                display as text
                                display 1 decimals
                    two column
                        column1
                            text of "_"
                            text of "Invoice"
                            text of "Receipt"
                            text of "Difference"
                                display as tag
                                    color of orange
                        column2
                            text of "_____Cost"
                                right align
                                bold
                            PendingInvoiceLineExtendedAmount
                                no label
                                right align
                                display as text
                                display 2 decimals
                            PendingReceiptLineExtendedAmount
                                no label
                                right align
                                display as text
                                display 2 decimals
                            PendingLineTotalDifference
                                no label
                                right align
                                display as text
                                display 2 decimals
                    visible when (AllowCreateSet)
                        paragraph centered
                            button of "SaveSet" with icon.save
                                action is CreateSet
                                is primary
            visible when (View.InvoiceMatch)
                visible when (AllowCreateInvoiceSet)
                    header4 of "WorkingSet"
                    two column
                        column1
                            text of "_"
                            text of "Invoice"
                            text of "Receipt"
                            text of "Difference"
                                display as tag
                                    color of orange
                        column2
                            text of "_____Quantity"
                                right align
                                bold
                            PendingInvoiceQuantity
                                no label
                                display as text
                                display 1 decimals
                            PendingReceiptQuantity
                                no label
                                display as text
                                display 1 decimals
                            PendingQuantityDifference
                                no label
                                display as text
                                display 1 decimals
                    two column
                        column1
                            text of "_"
                            text of "Invoice"
                            text of "Receipt"
                            text of "Difference"
                                display as tag
                                    color of orange
                        column2
                            text of "_____Cost"
                                right align
                                bold
                            PendingInvoiceExtendedAmount
                                no label
                                display as text
                                display 2 decimals
                            PendingReceiptExtendedAmount
                                no label
                                display as text
                                display 2 decimals
                            PendingTotalDifference
                                no label
                                display as text
                                display 2 decimals
                    paragraph centered
                        button of "SaveSet" with icon.save
                            action is CreateInvoiceSet
                            is primary
        visible when (FilesRebuilt)
            visible when (View.DetailMatch)
                display as text
                    visible when (HasAddOnCharges)
                        paragraph
                            button of "AOC_Difference<CostComponentDifference>"
                                link is CostComponentLink
                                display as link
                    header4 of "Chargeback"
                    single column
                        text of "_"
                    paragraph centered
                        visible when (not Chargeback)
                            button of "AddChargeback" with icon.add
                                action is SetChargeback
                    paragraph
                        visible when (Chargeback)
                            ChargebackReason
                                label is "Reason"
                            ChargebackReference
                                label is "Reference"
                            button of "Remove"
                                action is SetChargeback
                                display as link
                    paragraph
                        visible when (CreateMatchReconMessage)
                            text of "ChargebackReconOption:"
                                left align
                            button of "<ChargebackReconOption>"
                                action is SetChargebackReconOption
                                display as link
                    visible when (DisplayLineChargebackAmount)
                        paragraph
                            visible when (UseNet)
                                NetLineChargebackAmount
                                    label is "Net"
                            visible when (UseOverbill)
                                OverbillLineChargebackAmount
                                    label is "Overbill"
                    visible when (not DisplayLineChargebackAmount)
                        paragraph
                            visible when (UseNet)
                                NetSelected
                                    label is "Net"
                            visible when (UseOverbill)
                                OverbillSelected
                                    label is "Overbill"
                                    left align
                    header4 of "AdditionalActions"
                    single column
                        text of "_"
                    visible when (ShowDetailMatch)
                        paragraph centered
                            visible when (UnselectedLinesExist)
                                button of "SelectAllDetails" with icon.check
                                    action is SelectAllDetails
                    visible when (ShowInvoiceMatch)
                        paragraph centered
                            button of "SelectAll" with icon.launch
                                action is SelectAllInvoicesAndReceipts
            visible when (View.InvoiceMatch)
                visible when (not PayablesInvoice.InvoiceType.MatchPrepayment)
                    display as text
                        single column
                            text of "_"
                        paragraph centered
                            visible when (not Chargeback)
                                button of "AddChargeback" with icon.add
                                    action is SetChargeback
                        paragraph
                            visible when (Chargeback)
                                ChargebackReason
                                    label is "Reason"
                                ChargebackReference
                                    label is "Reference"
                                button of "Remove"
                                    action is SetChargeback
                                    display as link
                        visible when (UseNet)
                            paragraph
                                NetSelectedInvoiceMatch
                                    label is "Net"
                        visible when (UseOverbill)
                            paragraph
                                OverbillSelectedInvoiceMatch
                                    label is "Overbill"
        visible when (View.MatchComplete)
            display as text
                single column
                    DerivedMatchStatus
                        label is "MatchStatus"
                    PayablesInvoice.Status
                        label is "InvoiceStatus"
                    paragraph
                        PayablesInvoice.RuleGroup
                        PayablesInvoice.RuleGroup.Description
                            no label
                    PayablesInvoice.MatchProcessType
                    PayablesInvoice.MatchLevel
                    PayablesInvoice.MatchDate
        single column
            visible when (PayablesInvoice.DisplayCreateInProgress)
                text of "<PayablesInvoice.CreateDetailsProcessingInBackgroundMsg>"
                    bold
                    italics
                    color of red
            visible when (PayablesInvoice.DisplayMatchInProgress)
                text of "<MatchProcessingInBackgroundMsg>"
                    bold
                    italics
                    color of red
            visible when (PayablesInvoice.DisplayMatchInProgressWithAsyncFailure)
                text of "<MatchProcessingInBackgroundAsyncFailureMsg>"
                    bold
                    italics
                    color of red
            visible when (PayablesInvoice.BudgetEditInProgress)
                text of "BudgetEditsInProgress"
                    bold
                    color of red
