UpdateCashPaymentForm is a Form
    title is "CashPaymentTransactionCode"
    Layout
        single column
            TransactionOrigin
            PaymentOutputOption
            TransactionNumberValue
                label is "TransactionNumbering"
            ManualPaymentNumberReservation
                label is "ManualPaymentReservation"
        blank line
        header2 of "Reconciliation"
        paragraph
            AutomaticReconciliation
            PaymentSummarization
            visible when (not PaymentSummarization)
                AllowDetailPaymentReject
        blank line
        header2 of "ElectronicPayment"
        two column distributed
            column1
                ElectronicPrenotification
                    label is "Prenotification"
                ElectronicAlternateTransactionCode.BankTransactionCode
                    label is "AlternateTransactionCode"
                    select is CashPayments
                ElectronicPaymentFormat
                    label is "PaymentFormat"
                ElectronicPaymentOrganizationIdentifier
                    label is "OrganizationIdentifier"
            column2
                ElectronicPaymentInstruction1
                    label is "PaymentInstructions1"
                ElectronicPaymentInstruction2
                    label is "PaymentInstructions2"
                ElectronicPaymentUserField1
                    label is "UserField1"
                ElectronicPaymentUserField2
                    label is "UserField2"
        blank line
        header3 of "Nordic"
        three column distributed
            column1
                PaymentOption
            column2
                PlusGiroWithBankGiro
                    label is "PlusGiroPaymentsWithBankGiro"
            column3
                SortOption
                    label is "MergeOption"
        blank line
