ProcedureInformationForm is a Form
    title is "ProcedureInformation"
    Layout
        visible when (false)
            TransientUpdatePatientProcedure
        two column distributed
            column1
                header2 of "Visit"
                TransientVisitNumber
                    label is "VisitNumber"
                TransientPatientID
                    label is "MedicalRecordNumber"
                TransientCaseNumber
                    label is "CaseID"
                paragraph
                    TransientProcedure
                        label is "Procedure"
                    TransientProcedureModifier
                        label is "ProcedureModifier"
                TransientProcedureDate
                    label is "ProcedureDate"
                TransientPhysicianName
                    label is "Practitioner"
                TransientNurseCoordinator
                    label is "CoordinatingNurse"
            column2
                header2 of "Patient"
                TransientPatientLastName
                    label is "LastName"
                TransientPatientFirstName
                    label is "FirstName"
                TransientPatientMiddleName
                    label is "MiddleName"
                TransientBirthdate
                    label is "Birthdate"
                TransientGender
                    label is "Gender"
                TransientSocialSecurityNumber
                    label is "SSN"
        header2 of "Order"
        paragraph
            TransientSalesRepresentative
                label is "SalesRepresentative"
            TransientOrderNumber
                label is "SupplierOrderNumber"
            TransientRequestor
                label is "Requester"
        header2 of "Item"
        two column distributed
            TransientUniqueDeviceId
                label is "UDI"
                display as text
            TransientGTIN
                label is "GTIN"
        two column distributed
            TransientLot
                label is "Lot"
            TransientSerialNumber
                label is "Serial"
        two column distributed
            TransientManufacturer
                label is "ManufacturerCode\\/\\Division"
                compact format
            TransientManufacturerNumber
                label is "ManufacturerNumber"
        two column distributed
            TransientManufacturingDate
                label is "ManufacturingDate"
            TransientExpirationDate
                label is "ExpirationDate"
        two column distributed
            TransientUserDefinedField1
                label is "UserDefined1"
            TransientUserDefinedField2
                label is "UserDefined2"
        paragraph
            button of "Delete"
                visible when (IsProcInfoDeleteAllowed)
                action is DeletePatientProcedure
