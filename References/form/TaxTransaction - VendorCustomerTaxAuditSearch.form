VendorCustomerTaxAuditSearch is a SearchForm
    title is "VendorCustomerTaxAudit"
    Layout
        four column distributed
            column1
                TaxPointDateRange.Begin
                    label is "FromPaymentDate"
                TaxPointDateRange.End
                    label is "ToPaymentDate"
            column2
                TaxEntity
                SystemCode
            column3
                ProcessLevel
                paragraph
                    VendorCustomer
                    VendorCustomerName
                        no label
                        display as text
            column4
                AccruedOrInvoiced
                    label is "AccruedOrInvoiced"
                TaxPayableOrDueOption
