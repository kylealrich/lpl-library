GrantReportingSettingsForm is a Form
    is primary
    Layout
        three column distributed
            column1
                paragraph
                    SequenceNumber
                        label is "ReportSequence"
                        display as text
                    Description
                        label is "ReportDescription"
                        display as text
            column2
                paragraph
                    ReportType
                        display as text
                    Basis
                        display as text
            column3
                paragraph
                    ReportingThruDate
                        display as text
                    FinalReport
                        label is "ReportStatus"
                        display as text
        three column distributed
            column1
                paragraph
                    FinanceDimension2.DisplayDimension
                        label is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
                        display as text
                    FinanceDimension2.Description
                        display as text
            column2
                paragraph
                    FinanceDimension2.GrantID
                        label is "GrantI\\D"
                        display as text
                    FinanceDimension2.DateRange.Begin
                        label is "BeginDate"
                        display as text
                    FinanceDimension2.DateRange.End
                        label is "EndDate"
                        display as text
            column3
                paragraph
                    FinanceDimension2.Amount
                        label is "FundedAmount"
                        display as text
                    FinanceDimension2.Currency
                        label is "Currency"
                        display as text
        three column distributed
            column1
                paragraph
                    ProjectContract.DisplayContract
                        label is "<FinanceEnterpriseGroup.ProjectLabel>Contract"
                        display as text
                    ProjectContract.Project.Description
                        no label
                        display as text
            visible when (GeneratedReportExists)
                column2
                    GrantFederalReportFormat
                        label is "GrantFederalReportingTemplate"
                        display as text
                column3
                    button of "ViewReport"
                        view GeneratedReport.File
        visible when (GeneratedIDMReportExists)
            three column distributed
                column1
                    button of "<GrantFederalReportIDMTemplate.Name>"
                        link is external "<GrantFederalReportIDMTemplate.RetrieveUrl>"
                        label is "GrantFederalReportingTemplate"
                        display as link
                column2
                    button of "ViewReport"
                        link is external "<DerivedIDMReportLink>"
                        label is "Document"
                        display as link
