FinishedGoodsByproductForm is a Form
    title is "Byproducts"
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            InventoryLocation
            InventoryLocation.Name
                no label
                display as text
        paragraph
            FinishedGoodsByproduct.ByproductSequence
                label is "Sequence"
        paragraph
            Item
            Item.Description
                no label
                display as text
        paragraph
            Quantity
                label is "StandardQuantity"
