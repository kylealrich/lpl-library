ViewContractRedlineDocumentForm is a Form
    title is "ContractRedlineDocument"
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        three column distributed
            column1
                paragraph
                    LatestRevisionRel.Title
                        display as text
                paragraph
                    visible when (DisplayIDMRedlineDocPanel)
                        button of "OpenRedlineDocument"
                            link is external "<LatestRevisionRel.DerivedDrillBack>"
                                lazy url retrieval
                            label is "File"
                            display as link
                            raise green alert when (LatestRevisionRel.DisplayCheckAlert)
                                mouse over text is "SignedDocument"
                    visible when (DisplayIDMRedlineDelete)
                        button of icon.delete
                            action is DeleteRedlineDocument
                            label is "Delete"
                paragraph
                    LatestRevisionRel.DerivedVersionNumber
                        label is "AttachmentVersion"
                        display as text
                paragraph
                    LatestRevisionRel.CreatedBySupplier
                        display as text
            column2
                paragraph
                    LatestRevisionRel.LatestRevision
                        display as text
                paragraph
                    LatestRevisionRel.DerivedLastChangedByName
                        label is "LastModifiedBy"
                        display as text
                paragraph
                    LatestRevisionRel.DerivedLastChangeTimeStamp
                        label is "LastModifiedDate"
                        display as text
            column3
                paragraph
                    LatestRevisionRel.DerivedIsCheckedOut
                        label is "CheckedOut"
                        display as text
                paragraph
                    visible when (IDMItem.IDMItemDetails.IsCheckedOut)
                        LatestRevisionRel.DerivedCheckOutByName
                            label is "CheckedOutBy"
                            display as text
                paragraph
                    visible when (IDMItem.IDMItemDetails.IsCheckedOut)
                        LatestRevisionRel.DerivedCheckOutTimeStamp
                            label is "CheckedOutDate"
                            display as text
        single column
            LatestRevisionRel.Description
                display as text
