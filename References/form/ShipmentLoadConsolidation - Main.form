Main is a Form
    Layout
        visible when (not RecordExists)
            paragraph
                Company
                    label is "Company"
                Company.Name
                    no label
                    display as text
            paragraph
                InventoryLocation
                    label is "Location"
                InventoryLocation.Name
                    no label
                    display as text
            paragraph
                Container
                    when value changed
                        refresh DerivedNumberOfContainer
                Container.Description
                    no label
                    display as text
            paragraph
                Vehicle
                    when value changed
                        refresh HasVehicle
                        refresh DerivedNumberOfContainer
                        MaxContainer =  blank
                Vehicle.Description
                    no label
                    display as text
            paragraph
                visible when (not HasVehicle)
                    MaxContainer
                        display as required
                visible when (HasVehicle)
                    DerivedNumberOfContainer
                        label is "Load_Container_Limit"
                        display as text
            paragraph
                ShipTo
                ShipTo.Name
                    no label
                    display as text
            paragraph
                two column distributed
                    column1
                        FromShipDate
                    column2
                        ToShipDate
        visible when (RecordExists)
            two column distributed
                column1
                    paragraph
                        Container
                            when value changed
                                refresh DerivedNumberOfContainer
                        Container.Description
                            no label
                            display as text
                column2
                    paragraph
                        ShipTo
                        ShipTo.Name
                            no label
                            display as text
            two column distributed
                column1
                    paragraph
                        Vehicle
                            when value changed
                                refresh EditableMaxContainer
                                MaxContainer =  blank
                        Vehicle.Description
                            no label
                            display as text
                column2
                    paragraph
                        WarehouseStorageLocation
                            label is "StagingLocationAfterReleased"
                            select is StagingWarehouseStorageLocationRel.ActiveWarehouseStorageLocations
                        WarehouseStorageLocation.Name
                            no label
                            display as text
            two column distributed
                column1
                    paragraph
                        visible when (EditableMaxContainer)
                            MaxContainer
                            ContainerUnitsLabel
                                no label
                                display as text
                        visible when (DisplayMaxContainer)
                            MaxContainer
                                display as text
                            ContainerUnitsLabel
                                no label
                                display as text
                column2
                    paragraph
                        two column distributed
                            column1
                                FromShipDate
                            column2
                                ToShipDate
