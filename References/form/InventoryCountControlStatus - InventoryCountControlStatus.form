InventoryCountControlStatus is an ActionForm
    action is InventoryCountControlStatus
    Layout
        two column
            Company
            Company.Name
                no label
                display as text
        two column
            Location
            Location.Name
                no label
                display as text
        single column
            text of "Or"
        two column
            ReportGrp
                label is "ReportGroup"
            ReportGrp.Name
                no label
                display as text
        single column
            paragraph
                LastCountDt
                    label is "LastCountCutoffDate"
                text of "Or"
                LastCycleCountDt
                    label is "LastCycleCountCutoffDate"
            IncludeInactive
                label is "IncludeInactiveItems"
        two column
            InvenMajcl
                label is "InventoryClass\\:Major"
                show up to 10 characters
            InvenMajcl.Description
                no label
                display as text
        two column
            InvenMincl
                label is "InventoryClass\\:Minor"
                show up to 10 characters
            InvenMincl.Description
                no label
                display as text
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                InventoryCountControlStatusReport_RDG
            column2 of 25%
                InventoryCountControlStatusReport_XPT
