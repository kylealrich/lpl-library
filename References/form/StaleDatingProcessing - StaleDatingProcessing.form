StaleDatingProcessing is an ActionForm
    action is StaleDatingProcessing
    Layout
        header2 of "MAIN"
        single column
            CashManagementGroup
        two column
            CashCode
            CashCode.Currency
                no label
                display as text
        two column
            CashCodeGrp
                label is "CashCodeGroup"
                select is CashCodeGroup
            CashCodeGrp.Description
                no label
                display as text
        blank line
        two column
            BankInstCode
                label is "TransactionCode"
                select is CashPayments
            BankInstCode.Description
                no label
                display as text
        single column
            PlProcessing
                label is "ProcessLevelProcessing"
        single column
            RunOption
        single column
            PrintAddress
        single column
            IncludeAll
                label is "IncludeAllTransactions"
        single column
            UpdateOption
                label is "Update"
        header2 of "ProcessLevelOptions"
        paragraph
            PayablesCompany
            PayablesCompany.Name
                no label
                display as text
        paragraph
            ProcLevel
                label is "ProcessLevel"
            ProcLevel.Name
                no label
                display as text
        header2 of "StaleDatingOptions"
        single column
            StaleDate
        single column
            StaleAgeDays
                label is "Days"
        single column
            StalePostDate
                label is "PostingDate"
        header2 of "EscheatmentOptions"
        single column
            EscheatDate
                label is "EscheatmentDate"
        single column
            EscheatAgeDays
                label is "Days"
        single column
            EscheatPostDate
                label is "PostingDate"
        single column
            EscheatState
                label is "State"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                StaleDatingProcessingReport_RDG
                StaleDateProcessingErrorReport_RDG
            column2 of 25%
                StaleDatingProcessingReport_XPT
                StaleDateProcessingErrorReport_XPT
