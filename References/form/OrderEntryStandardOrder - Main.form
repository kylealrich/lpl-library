    Layout
        two column distributed
            visible when (not RecordExists)
                paragraph
                    Company
                    Company.BillingCompany.Name
                        no label
                        display as text
                paragraph
                    Customer
                        select is CustomerContextCompanySelectList
                    Customer.Name
                        no label
                        display as text
        two column distributed
            visible when (not RecordExists)
                OrderEntryStandardOrder
                    select is CustomersStandardOrders
