WireTransferOutputContextForm is a Form
    title is "WireTransferOutput"
    Layout
        display as text
            two column distributed
                column1
                    paragraph
                        FinancialInstitution.FinancialInstitutionAndDescription
                            label is "FinancialInstitution"
                    CreateDate
                column2
                    paragraph
                        FinancialInstitutionBranch.FinancialInstitutionBranchAndDescription
                            label is "Branch"
                    single column
                        DerivedStatus
            paragraph
                DataFile.Title
                    label is "DataFile"
                visible when (HasDataFileAttachment)
                    button of "View"
                        view DataFile.File
                        display as link
                DataFileName
