CompareResponsesForNewPO is a MatrixForm
    title is "CompareResponsesForNewPurchaseOrder"
    context form is CompareResponsesContextForm
    Column
        Display Fields
            NotifiedSupplier.Supplier.SupplierName
            NotifiedSupplier.SupplierSourceId.MainContact.FirstAndLastName
            NotifiedSupplier.Supplier.CurrentSupplierPerformanceScore
                label is "PerformanceEvaluationScore"
            check control
                check action is Submitted.AwardAllNewPO
                uncheck action is Submitted.DeleteAward
                label is "AwardAll"
            AwardAllOnMatrixMessage
            TotalBidAmount
            button of "TotalEventScore<EventScore>"
                visible when (SourcingEventResponse.EventWeightingDefined)
                link is EventScoreDetail
            button of "ViewQuestionResponses"
                visible when (SourcingEventResponse.SEQuestionsExist)
                link is QuestionResponses
            button of "ViewSupplierAttachments"
                visible when (SourcingEventResponse.AttachmentsExist)
                link is AttachmentResponses
    Cell
        Display Fields
            check control
                checked state is IsAwarded
                check action is Submitted.AwardLineNewPO
                uncheck action is Submitted.DeleteAward
                label is "Line<SourcingEventLine>:<SourcingEventLine.DerivedStatus>:<SourcingEventLine.ItemNumber>_Output:<SourcingEventLine.OutputType>"
                    header2
            LineAward
                label is "AwardQuantity"
            Quantity
                label is "LineQuantity<SourcingEventLine.LineInfo>"
            SourcingEventLineResponse.ResponseAmount.UnitPrice
                label is "<DerivedUnitPriceLabel>"
            ExtendedPrice
                label is "<DerivedExtendedPriceLabel>"
            ResponseTotalAOC
                label is "<DerivedAOCLabel>"
                visible when (SourcingEventLineResponse.LineAOCExists)
            TotalResponse
                label is "<DerivedTotalBidLabel>"
                visible when (SourcingEventLineResponse.LineAOCExists)
            NetUnitPrice
                label is "<DerivedNetUnitPriceLabel>"
                visible when (SourcingEventLineResponse.LineAOCExists)
            TotalLineScore
                link is LineScoreDetail
                visible when (SourcingEventLineResponse.LineWeightingDefined)
            DerivedAlternateResponse
        Detail Fields
            DeliveryDate
            PackagingString
                label is "UOMDetail"
            VendorItem
            VendorItemDescription
