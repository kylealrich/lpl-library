Detail is a Form
    Layout
        four column distributed
            column1
                display as text
                    QuantityToInspect
                        label is "QuantityToInspect"
                    DisplayQuantityOrdered
                        label is "QuantityOrdered"
                    OvershipReturnQuantity
                    DisplayOriginalQuantity
                        label is "OriginalQuantityReceived"
            column2
                display as text
                    AcceptedQuantity
                    DisplayUnitCostInReceivedUOM
                        label is "UnitCost"
                    DerivedDisplayBin
                    VendorReturn
                        label is "ReturnNumber"
            column3
                TransientRejectedQuantity
                    label is "RejectedQuantity"
                Bin
                    label is "RejectedFromBin"
                    select is BinsForALocation
                WriteOff
                    label is "Write\\-\\Off"
                GlobalLineType
                    label is "LineType"
                    select is LineTypeCodes
            column4
                TransientAcceptReject
        single column
            header2 of "_"
        single column
            text of "Package_Details"
                header2
        four column distributed
            column1
                PackageCondition
            column2
                TemperatureControlledStorageType
            column3
                paragraph
                    Temperature
                        when value changed
                            refresh TemperatureEntered
                    TemperatureScaleValue
                        no label
                        display as text
                visible when (TemperatureEntered)
                    TemperatureScale
                        display as radio buttons
                        when value changed
                            refresh TemperatureScaleValue
            column4
                TemperatureExpirationDate
