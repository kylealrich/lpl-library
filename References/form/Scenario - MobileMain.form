MobileMain is a Form
    title is "Main"
    Layout
        visible when (not NotBudget)
            single column
                Description
        visible when (NotBudget)
            single column
                Description
                    display as text
        paragraph
            CommitmentType
                display as text
            visible when (Exporting)
                text of "Exporting"
                    bold
            visible when (CommitmentType.Budget)
                Status
                    display as text
        visible when (IsInIncrementalLoad)
            paragraph
                IncrementalLoadStatus
                    label is "IncrementalLoad"
                    display as text
                IncrementalLoadStartPeriod
                    label is "From"
                    display as text
                IncrementalLoadEndPeriod
                    label is "To"
                    display as text
        visible when (CommitmentType.Budget)
            blank line
            single column
                Basis
        visible when (CommitmentType.Budget)
            blank line
            single column
                IsDEPMBudget
                    label is "EPMBudget"
                Cycle
                Version
                blank line
                header3 of "BudgetChangeRequestOptions"
                ApprovalCode
                BalancedChanges
        blank line
        visible when (CommitmentType.Budget)
            header3 of "Dimensions"
            two column table
