                            show up to 10 characters
                visible when (ShowGLAccountPicker)
                    visible when (ShowTopNodes)
                        GLChartAccountTopNode
                            select is TopNodes
                            show up to 10 characters
                            initial value is ReportingBasis.ChartAccountTopNode
