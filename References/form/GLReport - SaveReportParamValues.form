                visible when (ShowTopNodes)
                    GeneralLedgerCalendar
                        show up to 10 characters
                        initial value is ReportingBasis.CalendarTopNode.GeneralLedgerCalendar
                        when value changed
                            refresh FiscalMonth
                            refresh FiscalQuarter
                            GeneralLedgerCalendarTopNode =  GeneralLedgerCalendar.TopNode
            column2
                visible when (ShowTopNodes)
                    ReportingBasis.CalendarTopNode.GeneralLedgerCalendar.Description
                        no label
                        display as text
        single column
            visible when (false)
                GeneralLedgerCalendarTopNode
                    display as text
                    initial value is ReportingBasis.CalendarTopNode.GeneralLedgerCalendar.TopNode
            FiscalYear
                initial value is ReportingBasis.BasisYear
            visible when (not DerivedIsFiscalQuarter)
                FiscalMonth
                    display as required when (not DerivedIsFiscalQuarter)
                    initial value is DerivedCurrentMonth
            visible when (DerivedIsFiscalQuarter)
                FiscalQuarter
                    display as required when (DerivedIsFiscalQuarter)
