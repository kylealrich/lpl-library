                        select is ReportingBasis.AccountingEntityTopNode.ChildrenRel.AccountingEntityHierarchyTreeView
                        show up to 10 characters
                visible when (ShowAccountingUnitPicker)
                    visible when (ShowTopNodes)
                        AccountingUnitStructure
                            show up to 10 characters
                            initial value is ReportingBasis.AccountingUnitStructure
                    visible when (HasAccountingUnitParam)
                        AccountingUnit
