VertexLookupResultsForm is a Form
    is primary
    title is "LookupTaxAreaResult"
    Layout
        three column
            display as text
                column1
                    VertexConfig
                        label is "ProcessGroup"
                column2
                    RunDate
                column3
                    LogSearchName
        single column
            display as text
                visible when (VendorEntered)
                    LookupVendorGroup
                    LookupVendor
                    button of "Vendor:<LookupVendor.VendorName>"
                        link is NavToVendor
                        display as link
                visible when (VendorAndLocationEntered)
                    LookupVendorGroup
                    LookupVendor
                    LookupVendorLocation
                    button of "Vendor:<LookupVendor.VendorName>,Location:<LookupVendorLocation>"
                        link is NavToVendorLocation
                        display as link
                visible when (CustomerOrShipToEntered)
                    LookupCustomerGroup
                visible when (CustomerEntered)
                    LookupARCompany
                    LookupCustomer
                    button of "ReceivablesCompany:<LookupARCompany>,Customer:<LookupCustomer.Name>"
                        link is NavToCustomer
                        display as link
                visible when (CustomerAndShipToEntered)
                    LookupARCompany
                    LookupCustomer
                    LookupCustomerShipToLoc
                    button of "Company:<LookupARCompany>,Customer:<LookupCustomer.Name>,ShipTo:<LookupCustomerShipToLoc>"
                        link is NavToCustomerShipTo
                        display as link
                visible when (InventoryLocationEntered)
                    LookupInventoryCompany
                    LookupInventoryLocation
                    button of "InventoryCompany:<LookupInventoryCompany>,InventoryLocation:<LookupInventoryLocation>"
                        link is NavToInventoryLocation
                        display as link
                visible when (RequestingLocationEntered)
                    LookupInventoryCompany
                    LookupRequestingLocation
                    button of "InventoryCompany:<LookupInventoryCompany>,RequestingLocation:<LookupRequestingLocation>"
                        link is NavToRequestingLocation
                        display as link
                visible when (BillingProcessLevelEntered)
                    LookupBillingCompany
                    LookupBillingProcessLevel
                    button of "BillingCompany:<LookupBillingCompany>,BillingProcessLevel:<LookupBillingProcessLevel>"
                        link is NavToBillingProcessLevel
                        display as link
                visible when (PayablesProcessLevelEntered)
                    LookupPayablesCompany
                    LookupPayablesProcessLevel
                    button of "PayablesCompany:<LookupPayablesCompany>,PayablesProcessLevel:<LookupPayablesProcessLevel>"
                        link is NavToPayablesProcessLevel
                        display as link
        single column
            Status
            DerivedMessage
                label is "Message"
                display as text
        display as text
            single column
                LookupPostalAddress.DeliveryAddress.AddressLine1
                LookupPostalAddress.DeliveryAddress.AddressLine2
                    no label
                LookupPostalAddress.DeliveryAddress.AddressLine3
                    no label
                LookupPostalAddress.DeliveryAddress.AddressLine4
                    no label
            three column
                column1
                    LookupPostalAddress.Municipality
                column2
                    LookupPostalAddress.StateProvince
                column3
                    LookupPostalAddress.PostalCode
            single column
                LookupPostalAddress.Country
