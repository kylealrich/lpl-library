Options is a Form
    Layout
        header2 of "FundsTransfer"
        paragraph
            ElectronicFundsTransferProcess
                label is "ProcessingElectronicFundTransfers"
            EFTProcessingLevel
                no label
        paragraph
            visible when (EFTProcessingLevel.GroupLevelProcessing)
                UseIDMTemplate
            visible when (UseIDMTemplate)
                paragraph
                    EFTNotificationPrintIDMTemplate
                        select is EFTNotificationPrintIDMTemplate
                    button of "<EFTNotificationPrintIDMTemplate.Name>"
                        link is external "<EFTNotificationPrintIDMTemplate.RetrieveUrl>"
                        display as link
            DiscountsNotAllowed
        paragraph
            EFTCalendar
                label is "SystemCalendar"
            EFTCalendar.Description
                no label
                display as text
        paragraph
            EFTDueDays
                label is "DueDays"
            EFTDueDaysType
                label is "Type"
        paragraph
            EFTGLDays
                label is "GlobalLedgerDays"
            EFTGLDaysType
                label is "Type"
        paragraph
            EFTDepositDays
                label is "DepositDays"
            EFTDepositDaysType
                label is "Type"
        blank line
        header2 of "AdditionalApplicationSortFields"
        single column
            ApplicationSort1Label
                label is "Sort1Label"
            ApplicationSort2Label
                label is "Sort2Label"
            ApplicationSort3Label
                label is "Sort3Label"
            ApplicationSort4Label
                label is "Sort4Label"
