Main is a Form
    Layout
        header2 of "Processing"
        three column distributed
            column1
                paragraph
                    BillingProcessLevel
                        label is "ProcessLevel"
                        select is ProcessLevels
                    BillingProcessLevel.Name
                        no label
                        display as text
            column2
                paragraph
                    Location
                    Location.Name
                        no label
                        display as text
            column3
                paragraph
                    CustomerShipTo
                        label is "ShipTo"
                    CustomerShipTo.Name
                        no label
                        display as text
        header2 of "Dates"
        three column distributed
            column1
                StartDate
                StopDate
            column2
                paragraph
                    RecurringInvoiceParameter.CurrentBillDate
                        label is "CurrentBillDate"
                        display as text
            column3
                paragraph
                    InvoiceCount
                        label is "NumberOfInvoicesCreated"
                        display as text
        header2 of "Billing"
        three column distributed
            column1
                paragraph
                    BillTo
                        select is BillTo
                    BillTo.Name
                        no label
                        display as text
            column2
                paragraph
                    Currency
                    Currency.Description
                        no label
                        display as text
            column3
                paragraph
                    TermCode
                        label is "PaymentTerms"
                    TermCode.Description.Desc
                        no label
                        display as text
        paragraph
            HoldCode
            HoldCode.Description
                no label
                display as text
        header2 of "Payment"
        three column distributed
            column1
                TaxExemptCode
            column2
                paragraph
                    TaxCode
                    TaxCode.Description
                        no label
                        display as text
        header2 of "SalesData"
        three column distributed
            column1
                paragraph
                    Territory
                    Territory.Description
                        no label
                        display as text
                paragraph
                    CommissionSplitPercentage
                        label is "CommissionSplit"
            column2
                paragraph
                    SalesRepresentative
                        label is "SalesRepresentative1"
                    SalesRepresentative.Name
                        no label
                        display as text
                paragraph
                    CommissionRate1
                        label is "Commission"
            column3
                paragraph
                    SalesRepresentative2
                        label is "SalesRepresentative2"
                    SalesRepresentative2.Name
                        no label
                        display as text
                paragraph
                    CommissionRate2
                        label is "Commission"
