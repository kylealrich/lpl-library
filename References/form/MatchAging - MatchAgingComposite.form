MatchAgingComposite is a CompositeForm
    is primary
    title is "MatchAging"
    context form is MatchAgingContext
    Actions
        action is search
            label is "_"
        action is refresh
            label is "_"
    VendorAgingWithPercents is a Panel
        title is "ByVendor"
        visible when (Status.Complete)
        list is VendorSummaryRel.VendorAgingWithPercent
    YearMonthAgingWithPercents is a Panel
        title is "ByMonth"
        visible when (Status.Complete)
        list is YearMonthSummaryRel.YearMonthAgingWithPercent
    YearMonthAgingChart is a Panel
        title is "ByMonthBarChart"
        visible when (Status.Complete)
        list is YearMonthSummaryRel.YearMonthAgingChart
    NotMatched is a Panel
        title is "Unmatched"
        visible when (Status.Complete)
        list is VendorUnmatchedSummaryRel.VendorUnmatchedCardList
