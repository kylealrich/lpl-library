MobileGLTransactionDetailCompositeForm is a CompositeForm
    propagate as a drill
    title is "Transaction_<AccountingEntity>_<JournalizeGroup>"
    context form is GLTransactionDetailForm
    MainPanel is a Panel
        title is "Main"
        form is GLTransactionDetailMainForm
    AmountsPanel is a Panel
        title is "CurrencyAmounts"
        form is MobileGLTransactionDetailAmountsForm
    ProjectPanel is a Panel
        title is "ProjectRelated"
        form is GLTransactionDetailProjectForm
