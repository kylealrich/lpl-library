AdjustmentsForApplicationFormPanel is a Form
    Layout
        two column distributed
            column1
                ReceivableApplicationAdjustmentSelect.TransType
                    label is "TransactionType"
                    display as text
                ReceivableApplicationAdjustmentSelect.AdjustmentSequence
                    display as text
                BaseAdjustAmount
                    display as text
                DistributionAccount
                    compact format
            column2
                ReceivableApplicationAdjustmentSelect.Invoice
                    display as text
                AdjustAmount
                    label is "TransactionAdjustAmount"
                    display as text
                CreditAdjustAmount
                    label is "ApplicationAdjustAmount"
                ReceivableOperator
                visible when (IsTaxWithholding)
                    paragraph
                        ARIncomeWithholdingCode
                        ARIncomeWithholdingDocumentNumber
        two column distributed
            visible when (MemoChargebackAdjustment)
                blank line
                header2 of "MemoChargeback"
                paragraph
                    MemoChargeback.Company
                    MemoChargeback.ProcessLevel
                        label is "ProcessLevel"
                paragraph
                    MemoChargeback.Customer
                        label is "Customer"
                paragraph
                    MemoChargeback.MemoChargeback
                        label is "CrossReferenceNumber"
