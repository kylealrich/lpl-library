MigrationResultDetailForm is a Form
    is primary
    title is "MigrationResultDetail"
    Layout
        display as text
            paragraph
                FinanceEnterpriseGroup
                FinanceEnterpriseGroup.EnterpriseGroup.Description
                    no label
            paragraph
                MigrationResult
                MigrationResult.MigrationJobType
            single column
                MigrationResultDetail
                Status
                visible when (not IsCompleted)
                    ErrorMessage
        paragraph
            OriginatingTransaction.SymbolicKeyText
                label is "DocumentInfo"
                display as text
            button of "ViewSourceTransaction"
                link is SourceTransaction
                display as link
            visible when (IsCompleted)
                button of "ViewGlobalLedgerCommit"
                    link is GlobalLedgerCommit
                    display as link
