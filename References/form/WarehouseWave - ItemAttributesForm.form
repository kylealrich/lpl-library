ItemAttributesForm is a Form
    Layout
        two column distributed
            column1
                header2 of "Classes/Codes"
            column2
                header2 of "Classification"
        three column
            column1 of 50%
                paragraph
                    InventoryClass.MajorInventoryClass
                        label is "MajorInventoryClass"
                        select is MajorClassRel.MajorClassList
                    InventoryClass.MajorInventoryClass.Description
                        no label
                        display as text
                paragraph
                    InventoryClass.MinorInventoryClass
                        label is "MinorInventoryClass"
                        select is MinorClassRel.MinorClasses
                    InventoryClass.MinorInventoryClass.Description
                        no label
                        display as text
                paragraph
                    MajorPPEClass
                        label is "MajorPPEClass"
                        select is MajorPPEClassRel.MajorPPEClasses
                    MajorPPEClass.Description
                        no label
                        display as text
                paragraph
                    MinorPPEClass
                        label is "MinorPPEClass"
                        select is MinorPPEClassRel.MinorPPEClasses
                    MinorPPEClass.Description
                        no label
                        display as text
                paragraph
                    SpecialHandlingCode
                        select is SpecialHandlingCodeClassRel.ActiveSpecialHandlingCodes
                    SpecialHandlingCode.Description
                        no label
                        display as text
            column2 of 25%
                paragraph
                    Analytics
                paragraph
                    ContainsHumanTissue
                paragraph
                    CriticalItem
                paragraph
                    Dressing
                paragraph
                    Drugs
                paragraph
                    Implantable
                paragraph
                    Instrument
            column3 of 25%
                paragraph
                    Narcotic
                paragraph
                    PersonalProtectiveEquipment
                paragraph
                    Pharmaceutical
                paragraph
                    Reprocessed
                paragraph
                    Reusable
                paragraph
                    SingleUse
                paragraph
                    Sterile
