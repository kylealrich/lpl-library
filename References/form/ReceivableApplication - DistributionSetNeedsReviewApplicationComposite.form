DistributionSetNeedsReviewApplicationComposite is a CompositeForm
    title is "DistributionSetSelection"
    context form is DistributionSetNeedsReviewApplicationContextForm
        background is theme color
    restrict action AvailableForPosting.Post
    restrict action UnreleasedCashApplication.Remove
    restrict action RequiresAuditListing.Remove
    restrict action AvailableForPosting.Remove
    Actions
        action is ProcessDistributionSetSelections
            label is "ProcessSelections"
            hide when invalid
        action is MatchDistributionsForFundAccounting
            hide when invalid
        action is ProrateAndSelectAll
            action icon is submit
            hide when invalid
        action is ReopenDistributionSet
            label is "Reopen"
            hide when invalid
        action is save
            label is "_"
        action is refresh
            label is "_"
    DistributionSet is a MultiListPanel
        visible when (AllowSelectDistributionSet)
        list is ReceivableInvoiceDetailRel.ReceivableInvoiceRel.ProrateDistributionsRel.ReceivableDistributionSetSelectList2
            visible when (not IsDistributionSetTypeCreditMemoApply)
        list is ReceivableInvoiceDetailRel.ReceivableInvoiceRel.ProrateDistributionsRel.ReceivableDistributionSetSelectList3
            visible when (IsDistributionSetTypeCreditMemoApply)
        list is ReceivableCreditMemoDetailRel.ReceivableInvoiceRel.ProrateDistributionsRel.ReceivableDistributionSetSelectList4
            visible when (IsDistributionSetTypeCreditMemoApply)
    Adjustments is a Panel
        visible when (FundDistributionSetShowAdjustments)
        list is ReceivableApplicationAdjustment set.AdjustmentsForApplication
