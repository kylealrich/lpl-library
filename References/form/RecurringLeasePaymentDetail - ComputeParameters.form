ComputeParameters is a Form
    Layout
        paragraph
            ComputeGroup.Company
            ComputeGroup.Company.Name
                no label
                display as text
            text of "Or"
            ComputeGroup.GeneralLedgerCompanyGroup
                label is "CompanyGroup"
            ComputeGroup.GeneralLedgerCompanyGroup.Description
                no label
                display as text
        paragraph
            ComputeGroup.ComputeFromAccount
                compact format
        paragraph
            ComputeGroup.ComputeThroughAccount
                compact format
        blank line
        paragraph
            GeneralLedgerBudgetNumber
                label is "BudgetNumber"
        header2 of "AdvancedSettings"
        paragraph
            AccrueFlag
                label is "AccruePayment"
            AccrueReverseFlag
                label is "Reverse"
        blank line
        paragraph
            ComputeBaseAmount
        paragraph
            MaximumPayment
                label is "MaximumComputeAmount"
        paragraph
            AdvancedComputeOption
        header2 of "AnnualPaymentChange"
        paragraph
            AnnualChangeAmount
                label is "Amount"
            text of "Or"
            AnnualChangePercent
                label is "Percent"
