AssetDetails is a Form
    is primary
    Layout
        display as text
            paragraph
                Company.DerivedCompanyIDName
                    label is "Company"
            paragraph
                PurchaseOrder
            paragraph
                PurchaseOrderLineAsset
            paragraph
                PurchaseOrderLineAssetDetail
                    label is "ID"
        header3 of "Information"
        two column
            column1
                BarCode
                AssetTemplate
                AssetAccountingUnit
                    label is "DefaultTransactionDimensions"
                    compact format
                AssetAccountingUnitGroup
                Project
                TagNumber
                    label is "Tag"
            column2
                SerialNumber
                AssetDescription
                AssetGroup
                HoldAsset
                Combine
                blank line
        header3 of "Item"
        paragraph
            ItemNumber
                label is "Item"
            ItemDescription
                no label
        paragraph
            ModelNumber
        paragraph
            PurchaseDate
            InServiceDate
                label is "InServiceDate"
        paragraph
            Invoice
            Suffix
                no label
        header3 of "Location"
        paragraph
            AssetLocation
                label is "Location"
                display as text
        paragraph
            AssetDivision
                label is "Division"
                display as text
        paragraph
            LocationDetail.Level1
                label is "LocationDetail"
                display as text
            LocationDetail.Level2
                no label
                display as text
            LocationDetail.Level3
                no label
                display as text
