ReconciliationCompanyForm is a Form
    title is "FinanceCompany<Company.Name>"
    Layout
        single column
            paragraph
                FinanceGroup
                    display as text
                FinanceGroup.Description
                    no label
                    display as text
            paragraph
                Company
                Company.GeneralLedgerCompany.Name
                    no label
                    display as text
            blank line
            header3 of "ReconciliationDefaults"
            two column
                ChartSection
                ReconciliationLevel
            blank line
            Company.GeneralLedgerCompany.Currency
                label is "BaseCurrency"
                display as text
