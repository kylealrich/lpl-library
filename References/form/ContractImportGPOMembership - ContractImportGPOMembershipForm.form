ContractImportGPOMembershipForm is a Form
    is primary
    title is "GPOMembers"
    Layout
        display as text
            single column
                ContractImportGPOMembership
                    label is "MembershipID"
                Name
                visible when (not ContractGroup.GPOIsHealthTrust2021)
                    ClassOfTrade
                    Address1
                    Address2
                    Municipality
                    StateProvince
                    PostalCode
                blank line
        header3 of "InternalLocation"
        single column
            ContractGroup
                display as text
        two column
            ParticipantLocation.Company
            CompanyName
                no label
                display as text
        two column
            ParticipantLocation.Location
            LocationName
                no label
                display as text
        two column
            ParticipantLocation.RequestingLocation
            RequestingLocationName
                no label
                display as text
        single column
            ParticipantLocation.PricingGroup
        visible when (ContractGroup.GPOIsHealthTrust)
            header3 of "AdditionalHealthTrustInformation"
            single column
                first ContractGPOMembershipRel.COID
                first ContractGPOMembershipRel.FacilityActiveIndicator
                first ContractGPOMembershipRel.EligibleDate
                first ContractGPOMembershipRel.IneligibleDate
                first ContractGPOMembershipRel.CategoryID
                first ContractGPOMembershipRel.TypeID
                first ContractGPOMembershipRel.TypeDescription
                first ContractGPOMembershipRel.PhoneNumber
                first ContractGPOMembershipRel.FederalTaxID
                first ContractGPOMembershipRel.GLN
                first ContractGPOMembershipRel.CountryCode
                first ContractGPOMembershipRel.HIN
                first ContractGPOMembershipRel.DEA
                first ContractGPOMembershipRel.OrganizationName
                first ContractGPOMembershipRel.CompanyName
                first ContractGPOMembershipRel.GroupName
                first ContractGPOMembershipRel.DivisionName
                first ContractGPOMembershipRel.MarketName
