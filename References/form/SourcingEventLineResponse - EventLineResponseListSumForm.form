EventLineResponseListSumForm is a SummaryForm
    title is "_"
    Layout
        list is CurrentSourcingEventLineRel.EventLineResponseListDetail
        single column
            Contract
                label is "AgreementReference"
                display when blank
            ContractLine.PerOrderMaximumQuantity
                label is "MaximumQuantity"
                display when blank
            EffectiveDate
                label is "BidEffectiveDate"
                display when blank
            ExpirationDate
                label is "BidExpirationDate"
                display when blank
        blank line
        header2 of "AdvancedPricing"
        five column
            text of "BreakpointQuantity"
            text of "Percentage"
            text of "UnitPrice"
            blank line
            blank line
        five column
            AdvancedPricing.BreakpointOccurs.QuantityPrice[1].Quantity
                no label
                display when blank
            AdvancedPricing.BreakpointOccurs.QuantityPrice[1].UnitPercent
                no label
                display when blank
            AdvancedPricing.BreakpointOccurs.QuantityPrice[1].CurrencyUnitPrice
                no label
                display when blank
            blank line
            blank line
        five column
            AdvancedPricing.BreakpointOccurs.QuantityPrice[2].Quantity
                no label
                display when blank
            AdvancedPricing.BreakpointOccurs.QuantityPrice[2].UnitPercent
                no label
                display when blank
            AdvancedPricing.BreakpointOccurs.QuantityPrice[2].CurrencyUnitPrice
                no label
                display when blank
            blank line
            blank line
        five column
            AdvancedPricing.BreakpointOccurs.QuantityPrice[3].Quantity
                no label
                display when blank
            AdvancedPricing.BreakpointOccurs.QuantityPrice[3].UnitPercent
                no label
                display when blank
            AdvancedPricing.BreakpointOccurs.QuantityPrice[3].CurrencyUnitPrice
                no label
                display when blank
            blank line
            blank line
        five column
            AdvancedPricing.BreakpointOccurs.QuantityPrice[4].Quantity
                no label
                display when blank
            AdvancedPricing.BreakpointOccurs.QuantityPrice[4].UnitPercent
                no label
                display when blank
            AdvancedPricing.BreakpointOccurs.QuantityPrice[4].CurrencyUnitPrice
                no label
                display when blank
            blank line
            blank line
        five column
            AdvancedPricing.BreakpointOccurs.QuantityPrice[5].Quantity
                no label
                display when blank
            AdvancedPricing.BreakpointOccurs.QuantityPrice[5].UnitPercent
                no label
                display when blank
            AdvancedPricing.BreakpointOccurs.QuantityPrice[5].CurrencyUnitPrice
                no label
                display when blank
            blank line
            blank line
        blank line
        header2 of "LineScoreDetails"
        five column
            blank line
            text of "Price"
            text of "Delivery"
            text of "Diversity"
            text of "Question"
        five column
            text of "Scores"
            LinePriceScore
                no label
                display when blank
            LineDeliveryScore
                no label
                display when blank
            LineDiversityScore
                no label
                display when blank
            LineTotalQuestionScore
                no label
                display when blank
