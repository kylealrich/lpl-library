TaxCurrencyAmountsForm is a Form
    title is "TaxCurrencyAmounts"
    Layout
        visible when (false)
            TaxAmountData.AssignToCurrency
        blank line
        paragraph
            TaxAmountData.FunctionalAmount.EnteredCurrencyAmount
                label is "Functional"
                display only
            TaxAmountData.AccountingEntity.FunctionalCurrency
                no label
                display only
            TaxAmountData.FunctionalAmount.EnteredCurrencyRate
                no label
                display only
        visible when (TaxAmountData.AccountingEntity.AlternateCurrencyExists)
            paragraph
                TaxAmountData.AlternateAmount.EnteredCurrencyAmount
                    label is "Alternate"
                    display only
                TaxAmountData.AccountingEntity.AlternateCurrency
                    no label
                    display only
                TaxAmountData.AlternateAmount.EnteredCurrencyRate
                    no label
                    display only
        visible when (TaxAmountData.AccountingEntity.AlternateCurrency2Exists)
            paragraph
                TaxAmountData.AlternateAmount2.EnteredCurrencyAmount
                    label is "Alternate2"
                    display only
                TaxAmountData.AccountingEntity.AlternateCurrency2
                    no label
                    display only
                TaxAmountData.AlternateAmount2.EnteredCurrencyRate
                    no label
                    display only
        visible when (TaxAmountData.AccountingEntity.AlternateCurrency3Exists)
            paragraph
                TaxAmountData.AlternateAmount3.EnteredCurrencyAmount
                    label is "Alternate3"
                    display only
                TaxAmountData.AccountingEntity.AlternateCurrency3
                    no label
                    display only
                TaxAmountData.AlternateAmount3.EnteredCurrencyRate
                    no label
                    display only
        visible when (TaxAmountData.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
            paragraph
                TaxAmountData.ReportAmount1.EnteredCurrencyAmount
                    label is "<TaxAmountData.FinanceEnterpriseGroup.ReportOne>"
                    display only
                TaxAmountData.FinanceEnterpriseGroup.ReportCurrencyOne
                    no label
                    display only
                TaxAmountData.ReportAmount1.EnteredCurrencyRate
                    no label
                    display only
        visible when (TaxAmountData.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
            paragraph
                TaxAmountData.ReportAmount2.EnteredCurrencyAmount
                    label is "<TaxAmountData.FinanceEnterpriseGroup.ReportTwo>"
                    display only
                TaxAmountData.FinanceEnterpriseGroup.ReportCurrencyTwo
                    no label
                    display only
                TaxAmountData.ReportAmount2.EnteredCurrencyRate
                    no label
                    display only
        visible when (TaxAmountData.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
            paragraph
                TaxAmountData.ReportAmount3.EnteredCurrencyAmount
                    label is "<TaxAmountData.FinanceEnterpriseGroup.ReportThree>"
                    display only
                TaxAmountData.FinanceEnterpriseGroup.ReportCurrencyThree
                    no label
                    display only
                TaxAmountData.ReportAmount3.EnteredCurrencyRate
                    no label
                    display only
        visible when (TaxAmountData.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
            paragraph
                TaxAmountData.ReportAmount4.EnteredCurrencyAmount
                    label is "<TaxAmountData.FinanceEnterpriseGroup.ReportFour>"
                    display only
                TaxAmountData.FinanceEnterpriseGroup.ReportCurrencyFour
                    no label
                    display only
                TaxAmountData.ReportAmount4.EnteredCurrencyRate
                    no label
                    display only
        visible when (TaxAmountData.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
            paragraph
                TaxAmountData.ReportAmount5.EnteredCurrencyAmount
                    label is "<TaxAmountData.FinanceEnterpriseGroup.ReportFive>"
                    display only
                TaxAmountData.FinanceEnterpriseGroup.ReportCurrencyFive
                    no label
                    display only
                TaxAmountData.ReportAmount5.EnteredCurrencyRate
                    no label
                    display only
