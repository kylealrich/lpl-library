EventTypeForm is a Form
    is primary
    title is "EventType<SourcingEventType>"
    Layout
        single column
            SourcingGroup
            SourcingEventType
                label is "EventType"
            Description
            Active
            visible when (SourcingGroup.SupplierGroupRel.UseIDM)
                paragraph
                    BidTabulationIDMTemplate
                        select is BidTabulationIDMTemplate
                    button of "<BidTabulationIDMTemplate.Name>"
                        link is external "<BidTabulationIDMTemplate.RetrieveUrl>"
                            lazy url retrieval
                        display as link
                    visible when (MissingBidTabulationIDMTemplate)
                        text of "<MissingBidTabulationIDMTemplateMessage>"
                            italics
                paragraph
                    AwardDetailsIDMTemplate
                        select is AwardDetailsIDMTemplate
                    button of "<AwardDetailsIDMTemplate.Name>"
                        link is external "<AwardDetailsIDMTemplate.RetrieveUrl>"
                            lazy url retrieval
                        display as link
                    visible when (MissingAwardDetailsIDMTemplate)
                        text of "<MissingAwardDetailsIDMTemplateMessage>"
                            italics
