PayablesInvoiceWithholdingContextForm is a Form
    is primary
    title is "GlobalWithholding"
    Layout
        three column distributed
            column1
                paragraph
                    Vendor
                        display as text
                    Vendor.VendorName
                        no label
                        display as text
                InvoiceNumber
                    display as text
            column2
                EnrolledWithholding
                    display as text
                visible when (PayablesInvoicePayment entered)
                    PayablesInvoicePayment.PaymentSequence
            column3
                Type
                    display as text
                visible when (PayablesInvoicePayment entered)
                    PayablesInvoicePayment.VoidSequence
        three column distributed
            display as text
                visible when (PayablesInvoiceDistribution entered)
                    PayablesInvoiceDistribution
                visible when (InvoiceCancelDate entered)
                    InvoiceCancelDate
                visible when (PaymentVoidDate entered)
                    PaymentVoidDate
        three column distributed
            paragraph
                IncomeWithholdingCode
                IncomeWithholdingCode.Description
                    no label
                    display as text
            paragraph
                WithholdingCategory
                WithholdingCategory.Description
                    no label
                    display as text
            WithholdingDate
                display as text
        header2 of "WithholdingAmountForInvoiceCurrency<InvoiceCurrency>"
        three column distributed
            InvoiceReportableAmount
            paragraph
                InvoiceWithholdingAmount
                    display as text
                InvoiceCurrency
                    no label
                    display as text
            PaymentExchangeRate
                display as text
        header2 of "WithholdingAmountForPaymentCurrency<PaymentCurrency>"
        four column distributed
            display as text
                BasisForWithholding
                paragraph
                    WithholdingAmount
                    PaymentCurrency
                        no label
                visible when (not CalculatedUsingScale)
                    WithholdingRate
                visible when (CalculatedUsingScale)
                    WithholdingScale
            IncomeWithholdingCode.Accumulated
        single column
            WithholdingDistributionAccount
                display as text
                compact format
        header2 of "PostingAmounts"
        display as text
            paragraph
                visible when (false)
                    PaymentWithholdingAmount.AssignToCurrency
            four column table
                column headers
                    "_"
                    "Amount"
                    "Currency"
                    "WithholdingRate"
                column1
                    text of "Functional"
                    visible when (PaymentWithholdingAmount.AccountingEntity.AlternateCurrencyExists)
