ContractDashboardForm is a CompositeForm
    title is "Contract<RepresentativeText>"
    context form is ContractContextShort
        background is theme color
    restrict action Draft.SupplierCreateProposedContract
    restrict action Draft.SupplierUpdateProposedContract
    restrict action Draft.SupplierSubmitProposedContract
    restrict action Draft.RequesterCreateProposedContract
    restrict action Draft.RequesterUpdateProposedContract
    restrict action Draft.RSSRequesterUpdateProposedContract
    restrict action Draft.CreateContractTemplate
    restrict action Draft.CreateContractProposalTemplate
    restrict action Active.CreateEventWorksheetByContract
    restrict action Active.RemoveFromExpireListAction
    restrict action Active.CreateNewCopy
    restrict action Addendum.RemoveFromExpireListAction
    restrict action Amendment.RemoveFromExpireListAction
    restrict action SetPreferredLineToYes
    restrict action SetPreferredLineToNo
    restrict action CreateProcurementTemplateFromContract
    restrict action SelectContractForRebate
    restrict action CalculateQualifierUsagePercentagesForContractRange
    restrict action EmailFromRequester
    restrict action SendEmailForExpiringContracts
    restrict action CalculateExpectedRebateDueAmount
    restrict action AcceptAllCostToleranceRecords
    restrict action RemoveAllCostToleranceRecords
    restrict action RemoveAllCostComparisonRecords
    restrict action RemoveAllBidWorksheetRecords
    restrict action EditHeaderSpread
    restrict action CheckOut
    restrict action UndoCheckOut
    restrict action CheckIn
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Actions
        navigation is ViewContract
            label is "UpdateContract"
    ContractDocuments is a Panel
        valid when (ContractDocumentExists)
        list is ContractDocumentRel.ContractDocumentHistoryList
    BlanketPurchaseOrders is a Panel
        visible when (BlanketPurchaseOrdersExist)
        list is PurchaseOrderForBlanketStandingRel.BlanketStandingPurchaseOrderList
    StandingPurchaseOrders is a Panel
        visible when (StandingPurchaseOrdersExist)
        list is PurchaseOrderForBlanketStandingRel.BlanketStandingPurchaseOrderList
    PurchaseOrderLines is a Panel
        visible when (ContractPolinesByContract)
        list is ContractPolinesByContractRel.POLinesFromAContract
    RequisitionLines is a Panel
        visible when (RequisitionLinesByContract)
        list is RequisitionLinesByContractRel.RequisitionLinesFromAContract
    ServiceInvoices is a Panel
        visible when (ServiceInvoicesExist)
        list is ReleasedServiceInvoicesRel.ContractMatchedInvoices
    Commitments is a Panel
        visible when (DrillCommitmentsExist)
        list is GLCommitDrillRel.DrillGLCommitList
    SupplierCertifications is a Panel
        visible when (SupplierCertificationExist)
        list is ContractSupplierCertificationRel.ContractDashboardCertifications
    ProjectContracts is a Panel
        visible when (RelatedProjectContractsExist)
        list is ProjectContractsRel.ProjectContracts
    ProcurementTemplates is a Panel
        visible when (ProcurementTemplatesExist)
        list is ProcurementTemplateRel.TemplateForContractDashboard
    Emails is a Panel
        visible when (EmailExists)
        list is ContractNotificationEmailRel.ContractNotificationEmails
    AttributeUpdates is a Panel
        visible when (HasAttributeUpdates)
        list is AttributeUpdatesRel.UpdatesForAContract
    ParticipantGroups is a Panel
        visible when (ParticipantGroupsExist)
        list is ParticipantGroupContractRel.ContractParticipantGroupList
    ProcessingTimes is a Panel
        list is audit log records.ProcessingTimes
    ApprovalHistory is a Panel
        visible when (ApprovalsExist)
        form is CMDashboardApproval
