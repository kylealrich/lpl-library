BudgetTemplate is a CompositeForm
    is primary
    title is "<BudgetTemplateTitle>"
    context form is Main
        background is theme color
    implements IndependentFormPanels
    Actions
        action is Activate
            hide when invalid
        action is LazyActivate
            hide when invalid
        action is ProcessPending
            hide when invalid
        action is ProcessPendingRemove
            hide when invalid
        action is LazyProcessPending
            hide when invalid
        action is RecoverAfterProcessingError
            hide when invalid
        action is Deactivate
            hide when invalid
        action is DeleteTemplate
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is save
            action icon is save
            label is "_"
            hide when invalid
        action is save and new
            label is "_"
            hide when invalid
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
            hide when invalid
        action is refresh
            label is "_"
    Dimensions is a Panel
        title is "Main"
        form is Dimensions
    Structures is a Panel
        form is Structures
    ManualElements is a Panel
        visible when (HasManual)
        list is BudgetTemplateManual.BudgetTemplateManualForTemplate
    Groups is a Panel
        visible when (IsCreated)
        list is BudgetGroupsRel.BudgetGroupForHeaderList
    PendingMembers is a Panel
        visible when (HasNewNonManualembers)
        list is BudgetPendingsRel.BudgetTemplatePendingForTemplate
    PendingManual is a Panel
        visible when (HasPendingManual)
        list is BudgetManualPendingsRel.BudgetTemplateManualForTemplate
    PendingRecalculate is a Panel
        visible when (HasRecalculate)
        list is BudgetRecalculatesRel.BudgetTemplateRecalculateForTemplate
    PendingRemove is a Panel
        visible when (HasRemove)
        list is BudgetRemovesRel.BudgetTemplateRemoveForTemplate
    ObligationsLog is a Panel
        title is "ObligationLogs"
        visible when (HasLogs)
        list is LogsForTemplateRel.BudgetGroupTotalLogForTemplateList
    PendingUpdates is a Panel
        title is "UnreleasedObligations"
        visible when (Status.Ready)
        list is BudgetTemplateGroupTotalsRel.TemplateUpdates
    CheckedGroups is a Panel
        visible when (DisplayCheckedGroups)
        list is CheckedGroupsRel.CheckedGroups
    GroupData is a MultiListPanel
        visible when (IsCreated)
        list is BudgetGroupsRel.BudgetGroupObligations
            Special Actions
                navigation is CubeObligationsData
                    label is "CubeData"
    VarianceReport is a Panel
        visible when (HasVarianceReport)
        list is VarianceReportRel.BudgetGroupObligationVarianceRunPerTemplateList
    CubeUpdateDetails is a Panel
        form is UpdateDetails
