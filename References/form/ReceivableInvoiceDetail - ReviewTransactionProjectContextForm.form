ReviewTransactionProjectContextForm is a Form
    title is "ReviewProjectTransaction"
    Layout
        display as text
            paragraph
                ProjectContract.Project.DisplayProject
                    label is "<GeneralLedgerCompanyRel.FinanceEnterpriseGroup.ProjectLabel>Contract"
                ProjectContract.Project.Description
                    no label
            paragraph
                Project
                    label is "<GeneralLedgerCompanyRel.FinanceEnterpriseGroup.ProjectLabel>"
                Project.Description
                    no label
            paragraph
                FinanceDimension2
                    label is "<GeneralLedgerCompanyRel.FinanceEnterpriseGroup.FinanceDimension2Label>"
                FinanceDimension2.Description
                    no label
            paragraph
                Company
                Company.Name
                    no label
                Customer
                Customer.Name
                ReceivableProcessLevel
                    label is "ProcessLevel"
                TransientSignedTransactionAmount
            paragraph
                ReceivableInvoiceDetail.Invoice
                BatchNumber
                    label is "Batch"
                ReceivableInvoiceDetail.PaymentSeq
                    label is "Sequence"
                GeneralLedgerDate
                Status
                TransientSignedOpenAmount
