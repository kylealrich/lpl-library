CountryAddressRulesSummary is a Form
    title is "Country/\\JurisdictionAddressRulesSummary"
    Layout
        single column
            text of "Address_Format"
                align as label
            FormattedAddress1
                no label
                align as label
                display as text
            FormattedAddress2
                no label
                align as label
                display as text
        single column
            AddressRequiresStreetAddress
                label is "RequireStreetAddress"
                display as text
            AddressRequiresMunicipality
                label is "RequireCity"
                display as text
            AddressRequiresStateProvince
                label is "RequireState/Province"
                display as text
            AddressRequiresPostalCode
                label is "RequirePostCode"
                display as text
            AddressRequiresCounty
                label is "RequireCounty"
                display as text
            AddressRequiresRegion
                label is "RequireRegion"
                display as text
            AddressRequiresNationalIDNumber
                label is "RequireNationalIDNumber"
                display as text
            AddressRequiresStateProvinceIDNumber
                label is "RequireStateProvinceIDNumber"
                display as text
            AddressRequiresLocaleIDNumber
                label is "RequireLocaleIDNumber"
                display as text
            AddressRequiresIndividualIDNumber
                label is "RequireIndividualIDNumber"
                display as text
