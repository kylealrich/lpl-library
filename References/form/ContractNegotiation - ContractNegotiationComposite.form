ContractNegotiationComposite is a CompositeForm
    is primary
    title is "ContractNegotiationsForItem<Item>"
    context form is ContractNegotiationOneOnTopOfTheOther
    show panel control on left
    ContractAmounts is a Panel
        visible when (NegotiationDetailsExist)
        list is ContractNegotiationDetailsRel.ContractNegotiationDetails
    ContractSplits is a Panel
        visible when (NegotiationSplitsExist)
        list is ContractNegotiationSplitsRel.ContractNegotiationSplits
    QuantitiesNeededForTiers is a Panel
        visible when (NegotiationDetailsExist)
        form is TierQuantites
    ContractInformation is a Panel
        visible when (ForSpecificContracts)
        list is ContractsRel.Contracts
