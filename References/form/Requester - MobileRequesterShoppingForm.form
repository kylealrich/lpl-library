MobileRequesterShoppingForm is a CompositeForm
    title is "<MobileRequesterShoppingFormTitle>"
    called out actions only
    context form is MobileRequesterFormContext
    Actions
        action is DeleteRequisition
            action icon is delete
            label is "_"
            hide when invalid
    Cart is a Panel
        title is "Items_(<RequisitionRel.NumberOfLines>)"
        visible when (RequisitionRel.HasLines)
        list is RequisitionLineRel.MobileShoppingCart
    Catalog is a Panel
        list is KeywordShadowRel.MobileItemForRequisition
    ShoppingLists is a Panel
        list is ProcurementTemplateRel.MobileMyShoppingLists
    Recent is a Panel
        list is RequesterRequisitionLinesRel.MobileRecentlyRequestedItemsImageView
    RecentItems is a Panel
        visible when (false)
        list is RequesterRequisitionLinesRel.MobileRecentlyRequestedItems
