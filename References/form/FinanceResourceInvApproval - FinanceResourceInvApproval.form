FinanceResourceInvApproval is a Form
    is primary
    title is "InvoiceApprovalAssignment"
    Layout
        visible when (VendorGroupEntered)
            paragraph
                VendorGroup
                    display only
                VendorGroup.Description
                    no label
                    display as text
        visible when (not VendorGroupEntered)
            paragraph
                VendorGroup
                VendorGroup.Description
                    no label
                    display as text
        paragraph
            FinanceResource
            FinanceResource.Employee.Name.PreferredFirstAndLastName
                no label
                display as text
        paragraph
            InvoiceApprovalAuthCode
            InvoiceApprovalAuthCode.Description
                no label
                display as text
        paragraph
            InvoiceRoutingCategory
            InvoiceRoutingCategory.Description
                no label
                display as text
