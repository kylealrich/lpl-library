VendorLocationForm is a CompositeForm
    is primary
    title is "<DerivedFormTitle>"
    context form is VendorLocationContext
        background is theme color
    Main is a Panel
        form is Main
    VendorBank is a Panel
        title is "Banking"
        form is VendorBank
    Options is a Panel
        form is Options
    Balances is a Panel
        list is VendorLocationBalanceRel.VendorLocationBalanceList
    Contact is a MultiListPanel
        list is VendorContactRel.VendorContactsList
        list is CurrentAddressRel.VendorAddressesList
    Certifications is a MultiListPanel
        list is VendorCertificationRel.CertificationCodeAssignmentsVendorPanel
        list is VendorDiversityRel.DiversityCodeCertificationsVendorLocationPanel
    CompanyDefaults is a Panel
        list is VendorCompanyDefaultRel.VendorCompanyDefaultCodes
    Comments is a Panel
        list is VendorLocationCommentsRel.VendorComments
    Attachments is a Panel
        list is VendorLocationAttachmentsRel.Attachments
    MultipleTaxAreas is a Panel
        visible when (VertexResultsExist)
        list is VertexTaxAreaIdResultsRel.TaxAreaResultList
    VertexError is a Panel
        visible when (VertexErrorExist)
        list is VertexWebserviceErrorRel.VertexTaxAreaIdErrorList
    IncomeWithholdingDefault is a Panel
        visible when (VendorGroup.GlobalWithholding)
        list is VendorWithholdingDefault set.VendorWithholdingDefaultList
