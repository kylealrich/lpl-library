PayablesInvoiceForm is a CompositeForm
    is primary
    title is "<DerivedFormTitle>"
    context form is PayablesInvoiceContextForm
        background is theme color
    implements IndependentFormPanels
    restrict action Unreleased.RouteForApproval
    Actions
        navigation is ViewImage
            hide when invalid
        navigation is ChargesAndAllowances
            hide when invalid
        action is ProcessVertex
            hide when invalid
        action is ProcessOneSource
            hide when invalid
        action is OneSourceMatch
            hide when invalid
        action is VertexMatch
            hide when invalid
        action is ShortPayAndAccrueTax
            hide when invalid
        action is ProcessTaxOnHold
            hide when invalid
        action is ProcessTaxOnHoldForMatch
            hide when invalid
        action is CreateDetailFromContract
            hide when invalid
        action is CalculateRate
            hide when invalid
        action is Unreleased.Match
            hide when invalid
        action is Unreleased.Release
            hide when invalid
        action is Unreleased.SubmitForApproval
            hide when invalid
        action is Unreleased.ApproveInvoice
            hide when invalid
        action is PendingApproval.ReassignApprover
            hide when invalid
        navigation is RerouteInvoiceNav
            label is "RerouteInvoice"
            hide when invalid
            valid when (Status.PendingApproval)
        action is PendingApproval.ManualApprove
            hide when invalid
        action is PendingApproval.ManualApprovePlus
            hide when invalid
        action is PendingApproval.ManualReject
            hide when invalid
        action is Rejected.RestoreRejectedInvoice
            hide when invalid
        action is Approved.Match
            hide when invalid
        action is Approved.Release
            hide when invalid
        action is Amended.ReleaseAmendedInvoice
            hide when invalid
        action is Released.Match
            hide when invalid
        action is AssignAPClerk
            hide when invalid
        action is ReassignAPClerk
            hide when invalid
        action is EmailResponsiblePerson
            hide when invalid
        action is EmailInvoice
            hide when invalid
        action is EmailCreditMemoToVendor
            hide when invalid
        action is Unreleased.CreateInvoice
            label is "CreateInvoice"
            mouse over text is "Create"
            hide when invalid
        action is Unreleased.CreateAndRelease
            label is "Create_andRelease"
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
            hide when invalid
        action is next record
            label is "_"
            mouse over text is "Next_Record"
            hide when invalid
        action is save
            label is "_"
            mouse over text is "Save"
            hide when invalid
        action is save and new
            label is "_"
            mouse over text is "SaveAnd_New"
            hide when invalid
        action is refresh
            label is "_"
            mouse over text is "Refresh"
    Main is a Panel
        form is MainFormWithTemplate
    Errors is a MultiListPanel
        title is "Warnings_andErrors"
        visible when (ErrorsExists)
        list is PayablesInvoiceError set.PayablesInvoiceWarningAndErrorList
            visible when (InvoiceErrorsExists)
        list is HeaderBudgetEditErrorRel.primary
            visible when (HeaderBudgetEditErrorExists)
        list is DistributionBudgetEditErrorRel.primary
            visible when (DistributionBudgetEditErrorExists)
    Additional is a Panel
        form is PrimaryOptionsForm
    Distributions is a Panel
        title is "Distributions<DerivedDistributionIndicator>"
        visible when (ShowDistributionsPanel)
        list is PayablesInvoiceDistributionsDisplayOnInvoiceRel.PayablesInvoiceDistributionsList
    PurchaseOrders is a Panel
        title is "PurchaseOrders<DerivedPurchaseOrderIndicator>"
        visible when (ShowPurchaseOrderPanel)
        list is MatchPurchaseOrderInvoicesRel.MatchPurchaseOrderInvoiceDetail
            helper list is VendorPurchaseOrdersRel.MatchPurchaseOrderInvoiceHelperList
                action is CreateInvoicePurchaseOrder
                    invoked.Company = Company
                    invoked.PrmPayablesInvoice = PayablesInvoice
                valid when (AllowAdditionalPO)
    Messages is a Panel
        title is "Messages<DerivedMessageIndicator>"
        visible when (HasInvoiceMessages)
        list is MatchInvoiceMessageRel.InvoiceMessageReview
    OriginalInvoice is a Panel
        title is "MemoDetails"
        visible when (ShowDetailsOriginalInvoice)
        list is MatchInvoiceReferenceRel.MatchInvoiceReferenceWithDetails
            helper list is VendorNonPerfectMatchInvoicesRel.MatchedInvoices
                action is CreateMatchReferenceInvoice
                    invoked.Company = Company
                    invoked.MemoInvoice = PayablesInvoice
                valid when (AllowAddOfInvoiceReference)
    ChargebackDetails is a MultiListPanel
        title is "ChargebackDetails"
        visible when (ShowChargebackDetails)
        list is PayablesInvoiceDetailRel.MemoDetailList
    AddOnChargebackDetails is a MultiListPanel
        title is "AddOnChargeDetails"
        visible when (ShowAddOnChargebackDetails)
        list is PayablesInvoiceAddOnChargeRel.ChargebackAddOnChargesList
    ServiceContracts is a Panel
        title is "ServiceContracts<DerivedPurchaseOrderIndicator>"
        visible when (ShowServiceContractPanel)
        list is MatchServiceContractInvoicesRel.MatchServiceContracts
            helper list is VendorContractsRel.ServiceContractsForInvoicing
                action is CreateMatchServiceContractInvoice
                    invoked.PrmCompany = Company
                    invoked.PrmPayablesInvoice = PayablesInvoice
                valid when (IsUpdateable)
    InvoiceDetails is a MultiListPanel
        title is "ContractDetails<DerivedContractDetailsIndicator>"
        visible when (ShowContractDetailPanel)
        list is ContractLineRel.ContractLinesToInvoiceHelperList
    MemoContractDetails is a MultiListPanel
        title is "ContractDetails<DerivedContractDetailsIndicator>"
        visible when (ShowMemoContractDetailPanel)
        list is InvoiceDetailForContractRel.MemoDetailList
            helper list is OriginalInvoiceDetailsRel.OriginalInvoiceHelperList
                action is CreateMemoCostAdjustment
                    invoked.MemoCompany = Company
                    invoked.MemoInvoice = PayablesInvoice
                action is CreateMemoQuantityAdjustment
                    invoked.MemoCompany = Company
                    invoked.MemoInvoice = PayablesInvoice
    MiscellaneousDetails is a Panel
        visible when (CanUseMiscellaneousDetail)
        list is MiscellaneousInvoiceDetailsRel.MiscellaneousInvoiceDetails
    AddOnCharges is a MultiListPanel
        title is "AddOnCharges<DerivedAOCIndicator>"
        visible when (DisplayAOCPanel)
        list is MiscPayablesInvoiceAddOnChargeRel.MiscellaneousInvoiceAddOnCharges
            helper list is PurchaseOrderAddOnChargeRel.MiscellaneousAddOnChargesHelperList
                action is CreateInvoiceMiscellaneousAddOnCharges
                    invoked.PayablesCompany = Company
                    invoked.PayablesInvoice = PayablesInvoice
            visible when (ShowAOCsForInvoice)
        list is PayablesInvoiceSpreadAddOnChargeRel.PayablesInvoiceSpreadAddOnChargeInline
            helper list is PurchaseOrderSpreadAddOnChargeRel.PurchaseOrderSpreadAddOnChargeHelperList
                action is CreateInvoiceSpreadAddOnCharge
                    invoked.PayablesCompany = Company
                    invoked.PayablesInvoice = PayablesInvoice
            visible when (ShowSpreadForInvoice)
        list is PayablesInvoiceSupplierAddOnChargeRel.SupplierCreatedAddOnCharges
            visible when (HasSupplierAddOnCharges)
    AddOnChargesCreditRebill is a MultiListPanel
        title is "AddOnCharges<DerivedAOCIndicator>"
        visible when (ShowAOCsForCreditOrDebit)
        list is MiscPayablesInvoiceAddOnChargeRel.MemoMiscellaneousInvoiceAddOnCharges
            helper list is MatchInvoiceReferenceRel.OriginalInvoice.MiscPayablesInvoiceAddOnChargeRel.OriginalInvoiceMiscHelperList
                action is CreditForRebillMiscAOC
                    invoked.MemoCompany = Company
                    invoked.MemoInvoice = PayablesInvoice
        list is PayablesInvoiceSpreadAddOnChargeRel.MemoPayablesInvoiceSpreadAddOnCharges
            helper list is MatchInvoiceReferenceRel.OriginalInvoice.PayablesInvoiceSpreadAddOnChargeRel.PayablesInvoiceSpreadAddOnChargeHelperList
                action is CreditForRebill
                    invoked.MemoCompany = Company
                    invoked.MemoInvoice = PayablesInvoice
    Payment is a Panel
        form is PrimaryPaymentForm
    PaymentSchedule is a Panel
        title is "PaySchedule<DerivedPaymentIndicator>"
        list is PayablesInvoicePaymentRel.PaymentSchedules
    InvoiceWithholding is a Panel
        visible when (GlobalInvoiceWithholding)
        list is PayablesInvoiceWithholdingRel.InvoiceWithholdingList
    WithholdingDefaults is a Panel
        visible when (GlobalLineWithholding)
        list is PayablesInvoiceWithholdingRel.LineWithholdingDefaultsList
    AppliedCredits is a Panel
        title is "AppliedCredits<DerivedAppliedCreditIndicator>"
        visible when (HasCreditApplied)
        list is PayablesCreditMemoRel.CreditsAppliedList
    AppliedInvoices is a Panel
        title is "AppliedInvoices<DerivedAppliedInvoiceIndicator>"
        visible when (IsInvoiceApplied)
        list is PayablesCreditMemo2Rel.InvoicesAppliedList
    Documentation is a MultiListPanel
        title is "Documentation<DerivedDocumentationIndicator>"
        list is PayablesInvoiceDocument set.XiPayablesInvoiceDocuments
        list is PayablesInvoiceCommentRel.XiComments
    ContractAttachmentsAndComments is a MultiListPanel
        title is "ContractAttachments"
        visible when (ShowServiceContractAttachmentsPanel)
        list is ContractCommentRel.ContractComments
        list is ContractAttachmentRel.ContractAttachmentList
    Chargebacks is a Panel
        visible when (ShowChargebackPanel)
        list is PayablesInvoiceChargebackRel.ChargebackMatchAnalysisList
    OriginatingDocuments is a MultiListPanel
        visible when (ShowOriginatingDocuments)
        list is IntercompanyBillingDocumentRel.OriginatingJournalControlRel.GeneralLedgerJournalList
            visible when (OriginatingICBIsFromGL)
        list is IntercompanyBillingDocumentRel.OriginatingInvoiceRel.OriginatingPayablesInvoiceListForICB
            visible when (OriginatingICBIsFromInvoice)
        list is IntercompanyBillingDocumentRel.OriginatingReleasedDocumentListForICB
    OneSourceAttributes is a Panel
        visible when (ShowOneSourceAttributesPanel)
        list is AllOneSourceOutputUserAttributesRel.OneSourceAttributesForPayablesInvoice
    TaxLineItem is a Panel
        visible when (ShowOneSourceTaxLineItemPanel)
        list is TaxLineItemRel.TaxLineItemsForPayablesInvoice
    CurrencyAmounts is a Panel
        visible when (RecordExists)
        independent form is GLTransAmountsForm
    Global is a Panel
        independent form is PrimaryMiscForm
    ApprovalInformation is a DashBoardPanel
        visible when (DisplayApprovalInformationPanel)
        show 4 columns
        Progress is a Panel
            triple width
            form is ProcessIndicatorForm
        History is a Panel
            visible when (AuditExists)
            list is PayablesInvoiceAudit set.PayablesInvoiceHistoryCardList
    ApprovalTracking is a Panel
        title is "ApprovalInformation"
        visible when (DisplayApprovalTrackingPanel)
        list is PayablesInvoiceAudit set.PayablesInvoiceHistoryList
    Garnishments is a Panel
        visible when (IsGarnishmentsExists)
        form is GarnishmentsForm
    UserFields is a Panel
        independent form is UserFieldForm
    MatrixAggregation is a Panel
        visible when (AllowMatrixApprovalsList)
        list is APDistributionAggregationRel.AggregationSummary
