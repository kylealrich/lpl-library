UserListViewChangesForm is a Form
    disable field options
    Layout
        visible when (TransactionRecordExists)
            paragraph
                UserListViewRel.BusinessView
                    header2
                    display as text
                    mouse over text is "ConfiguredBusinessView"
                UserListViewRel.BusinessList
                    header2
                    display as text
                UserListViewRel.UserFolder
                    header2
                    display as text
                UserListViewRel.Name
                    header2
                    display as text
                button of "<BusinessView>Details"
                    link is UserListViewNav
                    display as link
        visible when (not TransactionRecordExists)
            paragraph
                text of "<BusinessView>RecordDoesNotExist"
