MassRetainagePaymentAdjust is an ActionForm
    action is MassRetainagePaymentAdjust
    Layout
        paragraph
            ContractGroup
                label is "ContractGroup"
            ContractGroup.BusinessGroup.Description
                no label
                display as text
        single column
            Contract
            ContractLine
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
            Company.GeneralLedgerCompany.Currency
                display as text
        three column
            PurchaseOrder
        single column
            PurchaseOrderLine
                label is "P\\OLine"
            ReportOption
                label is "ReportOptions"
        paragraph
            NewPercent
                label is "NewRetainagePercent"
            text of "%"
        single column
            PaymentDate
                label is "DueDate"
            PayInvImmed
                label is "PayImmediately"
            PrintPayDtl
                label is "PrintInvoiceDetail"
            Update
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                MassRetainagePaymentAdjustReport_RDG
            column2 of 25%
                MassRetainagePaymentAdjustReport_XPT
