Contact is a Form
    title is "SupplierContactInformation"
    Layout
        paragraph
            ContactTitle
                label is "Title"
            MainContact.GivenName
                label is "FirstName*"
            MainContact.FamilyName
                label is "LastName*"
        paragraph
            TelephoneNumber.InternationalPrefix
            TelephoneNumber.SubscriberNumber
                label is "PhoneNumber*"
            TelephoneNumber.Extension
        paragraph
            MobilePhone
        paragraph
            FaxNumber
        paragraph
            EmailAddress
                label is "EmailAddress*"
            ReceiveEmailNotification
        visible when (SupplierGroup.RequireEmailConfirmation)
            single column
                ConfirmEmailAddress
                    label is "ConfirmEmailAddress*"
        header2 of "MailingAddress"
        single column
            visible when (SupplierGroup.DisplayTypeForRegistration)
                LocationAddress
                    form is PostalAddressMinimumWithType
            visible when (not SupplierGroup.DisplayTypeForRegistration)
                LocationAddress
                    form is PostalAddressMinimum
        single column
            blank line
            blank line
            text of "*__=_Required"
