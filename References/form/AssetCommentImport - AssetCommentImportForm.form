AssetCommentImportForm is a Form
    title is "AssetCommentInterface"
    Layout
        two column
            column1
                RunGroup
                FinanceEnterpriseGroup
                paragraph
                    Company
                        select is Companies
                    Company.GeneralLedgerCompany.Name
                        no label
                        display as text
                ConversionNumber
                    label is "AssetImportGroup"
                Name
                    label is "Title"
                Type
            column2
                RecordInError
                    display as text
                ErrorMessage
                    display as text
                AssetRel.TagNumber
                    label is "TagNumber"
                    display as text
                AssetCommentImport
                    display as text
                AssetComment
                    display as text
        paragraph
            Comment
        paragraph
            Attachment
                compact format
        paragraph
            visible when (HasAttachment)
                button of "View"
                    view Attachment.File
                    display as link
