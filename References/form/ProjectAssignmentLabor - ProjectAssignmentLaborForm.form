ProjectAssignmentLaborForm is a Form
    is primary
    title is "<FinanceEnterpriseGroup.ProjectLabel>AssignmentLabor"
    Layout
        three column distributed
            column1
                ProjectLaborSchedule
                    display as text
            column2
                LaborPeriod.Period
                    display as text
            column3
                paragraph
                    Employee
                        show up to 10 characters
                    Employee.Employee.Name.PreferredFirstAndLastName
                        no label
                        display as text
        three column distributed
            column1
                paragraph
                    Project
                        label is "<FinanceEnterpriseGroup.ProjectLabel>"
                        show up to 15 characters
                    Project.Description
                        no label
                        display as text
            column2
                paragraph
                    ProjectAssignment.ProjectDateRange.BeginDate
                        label is "AssignmentBeginDate"
                        display as text
                    ProjectAssignment.ProjectDateRange.EndDate
                        label is "AssignmentEndDate"
                        display as text
        three column distributed
            column1
                paragraph
                    DateRange.Begin
                        label is "LaborPeriodBeginDate"
                        display as text
                    DateRange.End
                        label is "LaborPeriodEndDate"
                        display as text
            column2
                Status
                display as text
        three column distributed
            column1
                PlannedPercent
            column2
                visible when (Status.Entered)
                    ActualPercent
                visible when (Status.Approved)
                    ActualPercent
                visible when (Status.Processed)
                    ActualPercent
                        display as text
                visible when (Status.Certified)
                    ActualPercent
                        display as text
        single column
            FinanceCodeBlock
                compact format
            text of "DimensionsSetToBlank:"
                align as label
            FinanceCodeBlockSetBlanks
                no label
            visible when (ErrorMessage entered)
                ErrorMessage
                    display only
