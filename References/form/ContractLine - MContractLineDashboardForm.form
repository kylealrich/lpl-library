MContractLineDashboardForm is a CompositeForm
    title is "ContractLineDetails"
    context form is MContractLineDashboardContext
    implements DashBoardView
    restrict action Create
    restrict action CreateWithTierCosts
    restrict action CreateFromWizard
    restrict action Delete
    Actions
        action is save
            label is "_"
            hide when invalid
    RelatedPOLines is a Panel
        visible when (HasPoLine)
        list is PoLineRel.POLinesFromAContract
    RelatedInvoiceLines is a Panel
        visible when (InvoicesForContractLine)
        list is MatchedPayablesInvoiceDetailRel.MInvoiceDetailsForAContract
    CommittedInvoiceLineDistributions is a Panel
        visible when (CommittedInvoicesForContractLine)
        list is PayablesInvoiceDistributionRel.DistributionsForAContract
    RelatedRequisitionLines is a Panel
        visible when (RequisitionLinesByContract)
        list is RequisitionLinesByContractRel.RequisitionLinesFromAContract
    DistributorContractFromManufacturerContractPurchases is a Panel
        visible when (DistributorContractLineQuantityOrderedRelExists)
        list is DistributorContractLineQuantityOrderedRel.LineQuantityOrdered
    CommittedPOLineDistributions is a Panel
        visible when (POCommitmentsExist)
        list is ContractPolineDistributionsByContractRel.DistributionsForAContract
    CommittedRequisitionLineDistributions is a Panel
        visible when (RQCommitmentsExist)
        list is RequisitionLineDistributionsByContractRel.DistributionsForAContract
    OffContractPurchasesByItem is a Panel
        visible when (OffContractItemPurchases)
        list is OtherPurchaseOrderLineForItemRel.OffContractPurchaseOrderLines
    OffContractPurchasesByVendorItem is a Panel
        visible when (OffContractVendorItemPurchases)
        list is OtherPurchaseOrderLineForVendorItemRel.OffContractPurchaseOrderLines
    OffContractPurchasesByManufacturerInformation is a Panel
        visible when (OffContractManufacturerNumberPurchases)
        list is OtherPurchaseOrderLineForManufacturerInformationRel.OffContractPurchaseOrderLines
