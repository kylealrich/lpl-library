AssignmentLaborAuditLogForm is a Form
    title is "AssignmentLaborAuditDetail"
    Layout
        single column
            action
            applied stamp
            actor
        blank line
        blank line
        visible when (ActionCreate)
            single column
                text of "NewValues"
                    header2
                Status
                PlannedPercent
                ActualPercent
                Adjusted
                effective date
        visible when (ActionUpdate)
            two column
                column1
                    text of "NewValues"
                        header2
                    visible when (StatusUpdate)
                        Status
                    visible when (PlannedPercentUpdate)
                        PlannedPercent
                    visible when (ActualPercentUpdate)
                        ActualPercent
                    visible when (AdjustedUpdate)
                        Adjusted
                column2
                    text of "OldValues"
                        header2
                    visible when (StatusUpdate)
                        old Status
                    visible when (PlannedPercentUpdate)
                        old PlannedPercent
                    visible when (ActualPercentUpdate)
                        old ActualPercent
                    visible when (AdjustedUpdate)
                        old Adjusted
