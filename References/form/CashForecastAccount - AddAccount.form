AddAccount is an ActionForm
    action is AddAccount
    title is "CashForecastAccount"
    Layout
        paragraph
            CashForecastAccount.CashManagementAccount
            CashForecastAccount.CashManagementAccount.Title
                no label
                display as text
        blank line
        header3 of "ExchangeRateToCorporateBaseCurrency<CashManagementGroup.Currency>"
        paragraph
            CorporateBaseCurrencyRate
        header3 of "ExchangeRateToLocationCurrency<CashForecastAccount.CashManagementAccount.CashManagementLocation.Currency>"
        paragraph
            LocationCurrencyRate
