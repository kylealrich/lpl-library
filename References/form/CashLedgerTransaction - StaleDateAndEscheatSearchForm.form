StaleDateAndEscheatSearchForm is a SearchForm
    title is "StaleDatedAndEscheatedTransactionSearch"
    Layout
        four column distributed
            CashCode
                label is "CashCode"
            TempCashCodeGroup
                label is "CashCodeGroup"
            SearchByStaleDateEscheatStatus
                label is "Status"
            PayablesCompanyVendorProcLevel.PayablesProcessLevel
                label is "ProcessLevel"
                select is PayablesProcessLevels
        four column distributed
            SearchByReportDateRange.Begin
                label is "FromStale_/_EscheatDate"
            SearchByReportDateRange.End
                label is "ToStale_/_EscheatDate"
            BankTransactionCode
            SearchByState
                label is "State_/_Province"
