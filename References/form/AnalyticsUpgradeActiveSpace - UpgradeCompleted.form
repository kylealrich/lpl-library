UpgradeCompleted is a Form
    title is "UpgradeStatus"
    Layout
        single column
            text of "FSM_Analytics_Version"
                bold
        two column
            column1
                text of "Latest_Metadata_Version_(SMD)"
                text of "Active_Environment_Version_(ATC)"
            column2
                last ProductVersionSMDRel.Value
                    label is "_"
                last ProductVersionATCRel.Value
                    label is "_"
        single column
            text of "_"
            visible when (Status.InProgress)
                button of "RefreshProductVersion"
                    action is UpdateVersion
            blank line
            visible when (not IsUpgradeCompleted)
                header2 of "Your_Active_Environment_is_now_on_the_latest_Metadata_Version"
                button of "EndUpgrade"
                    list is AnalyticsUpgradeActiveSpace.AnalyticsUpgradeActiveSpaceList
