CompareSupplierAndVendorData is an ActionForm
    action is CompareSupplierAndVendor
    title is "CompareSupplier_andVendorData"
    Layout
        single column
            ParmSupplierGroup
                label is "SupplierGroup"
        paragraph
            ParmSupplier
                label is "Supplier"
            ParmSupplier.SupplierName
                no label
                display as text
        header2 of "FieldsToCompare"
        text of "MustSelectAtLeastOneCompareFieldSet"
        single column
            CompareSupplier
                label is "Suppliers_andVendors"
            CompareContacts
                label is "Contacts_andVendorLocations"
            CompareDiversity
                label is "Supplier_andVendorDiversityCodes"
            CompareCertification
                label is "Supplier,Vendor,Contact_andVendorLocationCertificationCodes"
        header2 of "CustomSupplierSelection"
        single column
            SupplierList
                label is "CustomSupplierSelection"
