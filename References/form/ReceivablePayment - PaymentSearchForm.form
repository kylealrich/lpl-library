PaymentSearchForm is a SearchForm
    Layout
        four column distributed
            PaymentNumber
            Customer
            FilterGeneralLedgerCompanyGroup
                label is "CompanyGroup"
            ReferenceNumber
        four column distributed
            ApplicationCompanyRange.FromCompany
                label is "FromCompany"
            ApplicationCompanyRange.ToCompany
                label is "ToCompany"
            ReceivableOperator
            OriginalCurrency
                label is "Currency"
        four column distributed
            CashCode
            ReceivableProcessLevelRange.FromReceivableProcessLevel
                label is "FromProcessLevel"
            ReceivableProcessLevelRange.ToReceivableProcessLevel
                label is "ToProcessLevel"
            TypeOfPayment
        four column distributed
            PostingDateRange.Begin
                label is "FromPostingDate"
            PostingDateRange.End
                label is "ToPostingDate"
            ReceivablePaymentHeader.ReceivableLockBox
                label is "Lockbox"
            DerivedStatus
                label is "Status"
        four column distributed
            DepositDateRange.Begin
                label is "FromDepositDate"
            DepositDateRange.End
                label is "ToDepositDate"
            TransactionAmountRange.FromAmount
                label is "FromPaymentAmount"
            TransactionAmountRange.ToAmount
                label is "ToPaymentAmount"
        four column distributed
            ReceivablePaymentHeader
                label is "Batch"
            BaseAmountRange.FromAmount
                label is "FromBaseAmount"
            BaseAmountRange.ToAmount
                label is "ToBaseAmount"
            IsZeroPayment
                label is "IsZeroPayment"
