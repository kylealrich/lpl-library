VendorCompanyCode is a Form
    is primary
    title is "VendorCompanyDefault"
    Layout
        two column distributed
            column1
                paragraph
                    visible when (not RecordExists)
                        Company
                    visible when (RecordExists)
                        Company.RepresentativeText
                            display as text
                    Company.Name
                        no label
                        display as text
                paragraph
                    visible when (not RecordExists)
                        VendorLocation
                            label is "Location"
                    visible when (RecordExists)
                        VendorLocation.RepresentativeText
                            label is "Location"
                            display as text
                paragraph
                    visible when (not RecordExists)
                        Vendor
                    visible when (RecordExists)
                        Vendor.RepresentativeText
                            display as text
                    Vendor.VendorName
                        no label
                        display as text
            column2
                paragraph
                    visible when (not RecordExists)
                        VendorGroup
                    visible when (RecordExists)
                        VendorGroup.RepresentativeText
                            display as text
                    VendorGroup.Description
                        no label
                        display as text
                paragraph
                    Default
                paragraph
                    Affiliate
                    visible when (Affiliate)
                        visible when (Company.ClaimsAndLiabilitiesReporting)
                            AffiliateOwnership
        header2 of "Payment"
        two column distributed
            column1
                paragraph
                    TermsCode
                    TermsCode.Desc1
                        no label
                        display as text
            column2
                paragraph
                    CashCode
                    CashCode.Description
                        no label
                        display as text
                paragraph
                    BankTransactionCode
                    BankTransactionCode.Description
                        no label
                        display as text
        header2 of "Invoice"
        two column distributed
            column1
                paragraph
                    ProcessLevel
                    ProcessLevel.Name
                        no label
                        display as text
                paragraph
                    AccrualCode
                    AccrualCode.Description
                        no label
                        display as text
                paragraph
                    DiscountCode
                    DiscountCode.Description
                        no label
                        display as text
                paragraph
                    TaxUsageCode
                    TaxUsageCode.Description
                        no label
                        display as text
                paragraph
                    AuthorityCode
                    AuthorityCode.Description
                        no label
                        display as text
                paragraph
                    IncomeCode
                    IncomeCode.Description
                        no label
                        display as text
            column2
                paragraph
                    DiversityCode
                    DiversityCode.Description
                        no label
                        display as text
                paragraph
                    RetainageAccrualCode
                    RetainageAccrualCode.Description
                        no label
                        display as text
                paragraph
                    DistributionCode
                    DistributionCode.Description
                        no label
                        display as text
                paragraph
                    TaxCode
                        select is TaxCodeFilteredList
                    TaxCode.Description
                        no label
                        display as text
                paragraph
                    InvoiceRoutingCategory
                    InvoiceRoutingCategory.Description
                        no label
                        display as text
                paragraph
                    Vendor.ForeignEconomicRegulatoryCode
                        display as text
                    Vendor.ForeignEconomicRegulatoryCode.Description
                        no label
                        display as text
        two column distributed
            SecondaryWithholdingCode1
            SecondaryWithholdingCode2
            SecondaryWithholdingCode3
        header2 of "E\\D\\IOrderNumber"
        two column distributed
            PoEdiNumber
            RevisedEDINumber
