MobileSupplyChainDeliveryTicketContext is a Form
    Layout
        five column distributed
            column1
                DeliverToCompanyLocation.MobileSupplyChainCompany
                    label is "DeliverToCompany"
                    display as text
                DeliveryType
                    display as text
            column2
                DeliverToCompanyLocation.MobileSupplyChainLocation
                    label is "DeliverToLocation"
                    display as text
                visible when (DocumentNumber entered)
                    DocumentNumber
                        display as text
                visible when (DocumentNumber not entered)
                    DerivedBlankDisplay
                        label is "DocumentNumber"
                        display as text
            column3
                InforTrackingNumber
                    display as text
                visible when (PurchaseOrder entered)
                    PurchaseOrder
                        display as text
                visible when (PurchaseOrder not entered)
                    DerivedBlankDisplay
                        label is "PurchaseOrder"
                        display as text
            column4
                Status
                    display as text
                DerivedVendorName
                    display as text
            column5
                create stamp
                    label is "CreateDate"
                    display as text
