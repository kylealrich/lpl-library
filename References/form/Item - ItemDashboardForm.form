ItemDashboardForm is a CompositeForm
    title is "<DerivedFormTitle>"
    context form is ItemDashboardContext
        background is theme color
    implements DashBoardView
        show context form on top
        use 12 column grid
    PurchaseOrderLines is a Panel
        visible when (HasPurchaseOrderLines)
        span 12 columns
        overview navigation is PurchaseOrderLines
        list is InventoryOrNonStockPurchaseOrderLineRel.PurchaseOrderLinesForAnItem
    OverduePurchaseOrderLinesPastExpectedDeliveryDate is a Panel
        visible when (OverduePurchaseOrderLines)
        span 12 columns
        overview navigation is OverduePurchaseOrderLinesPastExpectedDeliveryDate
        list is InventoryOrNonStockOverduePurchaseOrderLineRel.PurchaseOrderLinesForAnItem
    UnitsOfMeasure is a Panel
        span 12 columns
        overview navigation is UnitsOfMeasure
        list is ItemUOM set.UOMsForItemDashboard
    VendorItems is a Panel
        visible when (HasVendorItems)
        span 12 columns
        overview navigation is AllVendorItemNav
        list is AllVendorItemRel.VendorItemsForItemDashboard
    ParLocations is a Panel
        visible when (HasParCartLocation)
        span 12 columns
        overview navigation is ParLocations
        list is AllItemLocationsRel.ItemsAtParLocation
    ContractLines is a Panel
        visible when (ContractsLink)
        span 12 columns
        overview navigation is ItemContractLines
        list is ItemContractLinesRel.ItemDashboardContractLines
    ContractInterfaceLines is a Panel
        visible when (HasContractInterfaceLines)
        span 12 columns
        overview navigation is ContractInterfaceLines
        list is ContractInterfaceItemRel.ContractInterfaceLinesForAnItem
    AssociatedItems is a Panel
        visible when (AssociatedItemsExist)
        span 12 columns
        overview navigation is AssociatedItems
        list is AssociatedItemsRel.AssociatedItemDashboard
    ConvertedContractSpecialItems is a Panel
        visible when (ContractSpecialItemExists)
        span 6 columns
        overview navigation is ContractSpecialItems
        list is ContractSpecialItemsRel.OldSpecialItemsforItems
    RecallNotices is a Panel
        visible when (RecallNoticesExist)
        span 6 columns
        overview navigation is RecallNotices
        list is RecallProductItemsRel.RecallsForAnItem
    LocationBalances is a Panel
        visible when (HasLocations)
        span 6 columns
        overview navigation is LocationBalances
        list is AllActiveInventoriedItemLocationsRel.ItemStockOnHandByLocation
    LocationBalancesWithCatchWeight is a Panel
        title is "LocationBalances"
        visible when (HasLocationsWithCatchWeight)
        span 6 columns
        overview navigation is LocationBalancesWithCatchWeight
        list is AllActiveInventoriedItemLocationsRel.ItemStockOnHandByLocationWithCatchWeight
    LocationBalancesWithSupplemental is a Panel
        title is "LocationBalances"
        visible when (HasLocationsWithSupplemental)
        span 6 columns
        overview navigation is LocationBalancesWithSupplemental
        list is AllActiveInventoriedItemLocationsRel.ItemStockOnHandByLocationWithSupplemental
    ProcurementTemplates is a Panel
        visible when (HasProcurementTemplateLines)
        span 6 columns
        overview navigation is ProcurementTemplates
        list is ItemProcurementTemplateLinesRel.LinesForItemDashboard
    Attachments is a Panel
        visible when (HasItemAttachment)
        span 6 columns
        overview navigation is Attachments
        list is ItemAttachment set.ItemAttachmentDashboardList
    GTINs is a Panel
        title is "GTI\\Ns"
        visible when (HasGTINs)
        span 6 columns
        overview navigation is GTINs
        list is ItemGTIN set.GTINsForItemDashboard
    NationalDrugCodes is a Panel
        visible when (PharmacyOrDrugAndShowHealthcare)
        span 6 columns
        overview navigation is NationalDrugCodes
        list is NationalDrugCode set.NationalDrugCodesItemDashboard
    HIBCCCodes is a Panel
        title is "HIBCC-D\\Is"
        visible when (NotPharmacyOrDrugAndShowHealthcare)
        span 6 columns
        overview navigation is HIBCCCodes
        list is HIBCCItemUPN set.ItemHIBCCsDashboardCardList
    UCCEANCodes is a Panel
        title is "UCC_/EAN_UP\\Ns"
        visible when (NotPharmacyOrDrugAndShowHealthcare)
        span 6 columns
        overview navigation is UCCEANCodes
        list is UCCEANItemUPN set.UCCEANItemDashboardCardList
    UPCCodes is a Panel
        title is "UP\\Cs"
        span 6 columns
        overview navigation is UPCCodes
        list is ItemUPC set.ItemUPCsDashboardCardList
    SKUCodes is a Panel
        visible when (not DrugPharmacyOrNarcoticUsage)
        span 6 columns
        overview navigation is SKUCodes
        list is ItemSKU set.ItemSKUsDashboardCardList
