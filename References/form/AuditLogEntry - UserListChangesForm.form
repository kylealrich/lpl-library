UserListChangesForm is a Form
    disable field options
    Layout
        visible when (TransactionRecordExists)
            paragraph
                UserListRel.BusinessView
                    header2
                    display as text
                    mouse over text is "ConfiguredBusinessView"
                UserListRel.BusinessList
                    header2
                    display as text
                visible when (UserListRel.GlobalList)
                    text of "(Configuration)"
                        header3
                visible when (UserListRel.IsFolder)
                    text of "(Folder)"
                        header3
                visible when (UserListRel.IsActor)
                    text of "(Personalization)"
                        header3
                button of "<BusinessView>Details"
                    link is UserListNav
                    display as link
        visible when (not TransactionRecordExists)
            paragraph
                text of "<BusinessView>RecordDoesNotExist"
