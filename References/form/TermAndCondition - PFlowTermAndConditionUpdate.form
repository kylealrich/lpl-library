PFlowTermAndConditionUpdate is a CompositeForm
    title is "RequestToUpdateTermAndCondition"
    context form is TermAndConditionUpdateDraftViewContext
    TermAndCondition is a Panel
        form is PFlowTermAndConditionUpdateMain
    AdditionalTermLevels is a Panel
        visible when (HasChildren)
        list is ChildTermAndConditionRel.ChildTermsAndConditionsUpdateApprove
    AttachmentsToBeAdded is a Panel
        visible when (AttachmentsToBeAddedExist)
        list is TermAndConditionAddAttachmentRel.PendingAddAttachmentUpdates
    AttachmentsToBeDeleted is a Panel
        visible when (AttachmentsToBeDeletedExist)
        list is TermAndConditionDeleteAttachmentRel.PendingDeleteAttachmentUpdates
    ExistingAttachments is a Panel
        visible when (TermAndConditionAttachmentExists)
        list is TermAndConditionAttachmentRel.PFTermAndConditionAttachments
