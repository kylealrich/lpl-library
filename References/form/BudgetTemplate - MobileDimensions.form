MobileDimensions is a Form
    title is "Dimensions"
    Layout
        two column distributed
            column1
                paragraph
                    Scenario.Basis.GeneralLedgerCalendar
                        label is "Calendar"
                paragraph
                    BudgetPeriodType
                        label is "BudgetEditPeriodType"
                paragraph
                    PeriodToDate
                        label is "BudgetEditDateRange"
                paragraph
                    Status
                        display as text
            column2
                visible when (PeriodToDate.Period)
                    BudgetYearRange.Start
                        label is "StartYear"
                        select is AllCalendarYearsRel.Years
                    BudgetYearRange.End
                        label is "EndYear"
                        select is AllCalendarYearsRel.Years
                visible when (PeriodToDate.YearToDate)
                    BudgetYearRange.Start
                        label is "StartYear"
                        select is AllCalendarYearsRel.Years
                    BudgetYearRange.End
                        label is "EndYear"
                        select is AllCalendarYearsRel.Years
                ToleranceOption
                visible when (ToleranceOption.Percentage)
                    Tolerance
                        display zero
                visible when (ToleranceOption.Amount)
                    ToleranceAmount
                        display zero
                CheckGroups
                visible when (CheckGroups)
                    CheckThreshold
                visible when (DisplayErrorCheckStatus)
                    visible when (DisplayErrorCheckTime)
                        ErrorCheckTime
                            label is "LastErrorCheck"
                            display as text
                    visible when (CheckingForErrors)
                        text of "CheckingForErrors"
                            header3
