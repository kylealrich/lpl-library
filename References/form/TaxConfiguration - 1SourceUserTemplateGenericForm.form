1SourceUserTemplateGenericForm is a Form
    Layout
        single column
            visible when (OneSourceUserNameOrPasswordMissing)
                text of "Warning...One\\Source_\\User\\NameDoesNotExist"
                    bold
                    color of red
            visible when (OneSourcePasswordNotEntered)
                text of "Warning...One\\Source_\\User\\PasswordDoesNotExist"
                    bold
                    color of red
        display as text
            two column independent
                column1
                    header2 of "IndataUserTemplate"
                    visible when (1SourceIndataUserTemplateExist)
                        button of "UpdateIndataUserTemplate"
                            link is NavTo1SourceUT
                            display as link
                        first OneSourceUserTemplatesIndataRel.UserTemplate
                        first OneSourceUserTemplatesIndataRel.BusinessClass
                        first OneSourceUserTemplatesIndataRel.Description
                        first OneSourceUserTemplatesIndataRel.MimeType
                    visible when (not 1SourceIndataUserTemplateExist)
                        text of "Warning...IndataUserTemplateRecordDoesNotExist"
                            bold
                            color of red
                        button of "CreateIndataUserTemplate"
                            form invoke is OneSourceGenericUserTemplateIndata
                            display as link
                column2
                    header2 of "UserTokenTemplate"
                    visible when (1SourceUserTokenUserTemplateExist)
                        button of "UpdateUserTokenTemplate"
                            link is NavTo1SourceUserTokenUT
                            display as link
                        first 1SourceUserTokenUserTemplateRel.UserTemplate
                        first 1SourceUserTokenUserTemplateRel.BusinessClass
                        first 1SourceUserTokenUserTemplateRel.Description
                        first 1SourceUserTokenUserTemplateRel.MimeType
                    visible when (not 1SourceUserTokenUserTemplateExist)
                        text of "Warning...UserTokenTemplateRecordDoesNotExist"
                            bold
                            color of red
                        button of "CreateUserTokenTemplate"
                            form invoke is OneSourceGenericUserTemplateUserToken
                            display as link
            two column independent
                column1
                    header2 of "TaxEstimateUserTemplate"
                    visible when (1SourceCalcUserTemplateExist)
                        button of "UpdateCalculateUserTemplate"
                            link is NavTo1SourceCalcUT
                            display as link
                        1SourceUserTokenUserTemplateCalcRel.UserTemplate
                        1SourceUserTokenUserTemplateCalcRel.BusinessClass
                        1SourceUserTokenUserTemplateCalcRel.Description
                        1SourceUserTokenUserTemplateCalcRel.MimeType
                    visible when (not 1SourceCalcUserTemplateExist)
                        text of "Warning...CalculateUserTemplateRecordDoesNotExist"
                            bold
                            color of red
                        button of "CreateCalculateUserTemplate"
                            form invoke is OneSourceGenericUserTemplateUserTokenCalc
                            display as link
                column2
                    header2 of "ProcessHistoryUserTokenTemplate"
                    visible when (1SourceHistUserTemplateExist)
                        button of "UpdateHistoryUserTokenTemplate"
                            link is NavTo1SourceHistUT
                            display as link
                        1SourceUserTokenUserTemplateHistRel.UserTemplate
                        1SourceUserTokenUserTemplateHistRel.BusinessClass
                        1SourceUserTokenUserTemplateHistRel.Description
                        1SourceUserTokenUserTemplateHistRel.MimeType
                    visible when (not 1SourceHistUserTemplateExist)
                        text of "Warning...HistoryUserTokenTemplateRecordDoesNotExist"
                            bold
                            color of red
                        button of "CreateHistoryUserTokenTemplate"
                            form invoke is OneSourceGenericUserTemplateUserTokenHist
                            display as link
        header2 of "GenericConfigurationParameter"
        display as text
            visible when (not 1SourceGenericConfigExist)
                single column
                    text of "Warning...ConfigurationParameterRecordDoesNotExist"
                        bold
                        color of red
                    button of "CreateConfigurationParameter"
                        form invoke is OneSourceGenericConfigParmRelRecord
                        display as link
            visible when (1SourceGenericConfigExist)
                single column
                    button of "UpdateConfigurationParameter"
                        link is NavTo1SourceGenericConfigParm
                        display as link
                two column independent
                    column1
                        OneSourceGenericConfigParmRel.ConfigurationParameter.ConfigurationID
                        OneSourceGenericConfigParmRel.Type
                    column2
                        OneSourceGenericConfigParmRel.ConfigurationParameter.Name
                        OneSourceGenericConfigParmRel.Length
                single column
                    OneSourceGenericConfigParmRel.Value
