MobileSupplyChainForm is a Form
    title is "MobileParCounting"
    Layout
        header3 of "LocationActivities"
        four column distributed
            column1
                visible when (DeliverToDisabled)
                    DeliveryTo
                        display only
                visible when (not DeliverToDisabled)
                    DeliveryTo
            column2
                visible when (IsRequestingLocation)
                    ReceivingAndDeliveryTransfer
                        display only
                visible when (not IsRequestingLocation)
                    ReceivingAndDeliveryTransfer
            column3
                visible when (IsRequestingLocation)
                    ReceivingShipTo
                        display only
                visible when (not IsRequestingLocation)
                    ReceivingShipTo
        visible when (IsParOrExchangeCartExist)
            header3 of "ParCount"
            four column distributed
                column1
                    MaximumAdditionalQuantityMultiplier
                        label is "MaximumAdditionalQuantity(MultiplyParLevelBy)"
                column2
                    PickForParEnabled
        header3 of "InventoryReplenishment"
        four column distributed
            column1
                DisplayInventoryReplenishmentLocationQOO
                    label is "DisplayQOO"
            column2
                DisplayInventoryReplenishmentLocationMinimumOrder
                    label is "DisplayMinimumOrder"
        header3 of "PurchasingOrderReceiving"
        four column distributed
            column1
                DisplayCarrierTrackingNumber
                DisplayTrackingNumber
                DisplayAddReceiptButton
            column2
                DisplayCarrier
                DisplayBillOfLading
            column3
                DisplayReferenceNumber
                CheckCatchWeightTolerance
            column4
                PrintPurchaseOrderDeliveryTicket
                    label is "PrintDeliveryTicket"
                PrintPurchaseOrderDeliveryLabel
                    label is "PrintDeliveryLabel"
        header3 of "Picking"
        four column distributed
            column1
                TrackDeliveryOption
            column2
                PrintPickingOptions
            column3
                DownloadedShipmentOrder
            column4
                ItemNumberValidation
        header3 of "InventoryIssuesAndReturns"
        single column
            PrintInventoryIssueTicket
                label is "PrintTicket"
        header3 of "InventoryAdjustments"
        single column
            PrintInventoryAdjustmentTicket
                label is "PrintTicket"
        header3 of "InventoryTransfers"
        single column
            PrintInventoryTransferTicket
                label is "PrintTicket"
        header3 of "Inventory_Receiving_Transfers"
        single column
            PrintInventoryReceivingTransferTicket
                label is "PrintTicket"
