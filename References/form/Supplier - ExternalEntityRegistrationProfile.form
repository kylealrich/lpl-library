ExternalEntityRegistrationProfile is a CompositeForm
    use for action SupplierCreateImport
    title is "<DerivedExternalEntityRegistrationTitle>"
    disable field options
    called out actions only
    Actions
        action is save
            hide when invalid
            valid when (not IsAnonymous)
        action is Delete
            hide when invalid
            valid when (not IsAnonymous)
        action is refresh
            hide when invalid
            valid when (not IsAnonymous)
    Credentialing is a Panel
        visible when (ShowCredentialingPanelAndIsAnonymous)
        mouse over text is "<SupplierGroup.SupplierGroupExtension.VendormateCredentialText>"
        panel header is "<SupplierGroup.SupplierGroupExtension.VendormateCredentialText>"
        form is Credentialing
    Registration is a Panel
        visible when (IsAnonymousAndFullRegistration)
        mouse over text is "<SupplierGroup.CompanyInformationText>"
        panel header is "<SupplierGroup.CompanyInformationText>"
        form is ExternalEntitySupplierPortalRegistration
    CompleteAccountRegistration is a Panel
        visible when (IsAnonymousAndCompleteAccountRegistration)
        form is ExternalEntityRegistrationForExistingContact
    BankInformation is a Panel
        visible when (ShowBankInformationPanel)
        mouse over text is "<SupplierGroup.BankInformationText>"
        panel header is "<SupplierGroup.BankInformationText>"
        form is BankInformationRegistrationForm
    DiversityCodes is a Panel
        visible when (ShowDiversityCodesPanel)
        mouse over text is "<SupplierGroup.CompanyDiversityText>"
        panel header is "<SupplierGroup.CompanyDiversityText>"
        list is SupplierDiversityResponse set.DiversityResponseList
    Questions is a Panel
        visible when (ShowQuestionsPanel)
        mouse over text is "<SupplierGroup.QuestionsText>"
        panel header is "<SupplierGroup.QuestionsText>"
        template is SupplierGroup.SupplierGroupQuestionsByDisplayOrder
            business class is SupplierQuestionResponse
                form is SupplierQResponse
    QuestionsConditional is a Panel
        visible when (ShowQuestionsPanel)
        mouse over text is "<SupplierGroup.QuestionsText>"
        panel header is "<SupplierGroup.QuestionsText>"
        template is SupplierQuestions set
            business class is SupplierQuestionsSetResponse
                form is SupplierQResponse
    CommodityCodes is a MultiListPanel
        visible when (ShowCommoditySelectWithTree)
        mouse over text is "<SupplierGroup.CommodityCodesText>"
        list is PrimaryContact.SupplierCommodityCode set.ContactSelectedCommodityCodes
        list is CommodityCodeRel.RegistrationCommodityCodesForASupplierContact
    CommodityCodesNoTree is a MultiListPanel
        title is "CommodityCodes"
        visible when (ShowCommoditySelectLevelOne)
        mouse over text is "<SupplierGroup.CommodityCodesText>"
        list is PrimaryContact.SupplierCommodityCode set.ContactSelectedCommodityCodes
        list is CommodityCodeRel.RegistrationCommodityCodesForASupplierContactNoTree
    Certifications is a Panel
        visible when (ShowCertificationsPanel)
        mouse over text is "<SupplierGroup.CertificationText>"
        panel header is "<SupplierGroup.CertificationText>"
        list is AllSupplierCertificationsRel.SupplierSideCertifications
    ProxyNotifications is a Panel
        visible when (ShowProxyNotificationsPanel)
        mouse over text is "<SupplierGroup.ProxyNotificationsText>"
        panel header is "<SupplierGroup.ProxyNotificationsText>"
        list is PrimaryContact.SupplierContactProxy set.SupplierContactProxyList
    Status is a Panel
        visible when (not IsAnonymous)
        form is RegistrationSummary
