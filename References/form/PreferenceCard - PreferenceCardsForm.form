PreferenceCardsForm is a CompositeForm
    is primary
    title is "PreferenceCards"
    context form is PreferenceCardContext
        background is theme color
    Main is a Panel
        form is Main
    ComponentItems is a Panel
        visible when (RecordExists)
        list is ComponentItem set.ComponentItems
    ClinicalProcedures is a Panel
        visible when (RecordExists)
        list is PreferenceCardProcedure set.PreferenceCardProcedureList
    Providers is a Panel
        visible when (RecordExists)
        list is PreferenceCardProvider set.PreferenceCardProviders
    Errors is a Panel
        visible when (ErrorExists)
        list is PreferenceCardError set.PreferenceCardErrorList
