MobileBillingInvoiceSearchForm is a SearchForm
    title is "BillingInvoiceSearchForm"
    Layout
        single column
            Company
            Status
        two column distributed
            InvoiceDateRange.Begin
                label is "FromInvoiceDate"
            InvoiceDateRange.End
                label is "ToInvoiceDate"
        two column distributed
            GeneralLedgerDateRange.Begin
                label is "FromGlobalLedgerDate"
            GeneralLedgerDateRange.End
                label is "ToGlobalLedgerDate"
