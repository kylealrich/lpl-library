RequisitionDashboardForm is a CompositeForm
    title is "Requisition<Requisition>Details"
    context form is RequisitionDashboardContext
        background is theme color
    restrict action Unreleased.UpdateRequisitionLineFillOrKill
    restrict action Unreleased.MobileCreate
    restrict action Unreleased.RSSLPLCreate
    restrict action Unreleased.RSSLPLDelete
    restrict action Unreleased.RSSLPLShop
    restrict action Unreleased.RSSLPLCreateUsingCopyFromRequisition
    restrict action Unreleased.RSSLPLCreateUsingCopyFromTemplate
    restrict action Unreleased.RSSLPLCopyAllLinesFromRequisition
    Actions
        action is Release
            action icon is release
        action is IssueDraft
            hide when invalid
        action is IssueFinal
            action icon is print
            hide when invalid
        action is IssueFinalIDM
            action icon is print
            hide when invalid
        action is Released.ManualApprove
            hide when invalid
        action is Released.ManualReject
            hide when invalid
        action is Released.ManualUnrelease
            hide when invalid
        action is Processed.ReleaseReturns
            hide when invalid
        navigation is RequisitionView
            action icon is open
            label is "_"
        action is refresh
            label is "_"
    Details is a MultiListPanel
        list is RequisitionLinesRel.DashboardRequisitionLines
            visible when (HasLines)
        list is PurchaseOrderLineSourcesRel.PurchaseOrderLineForSourceList
            visible when (HasPOLines)
        list is RequisitionCommentRel.DashboardRequisitionComments
            visible when (CommentsExist)
        list is GLCommitDrillRel.DrillGLCommitList
            visible when (CommitmentsExist)
        list is RequisitionAddOnChargesRel.DashboardMiscAddOnCosts
            visible when (AddOnChargesExist)
        list is RequisitionSpreadAddOnCharge set.DashboardSpreadAddOnCharges
            visible when (SpreadAddOnChargesExist)
    ApprovalInformation is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
    MatrixApprovalInformation is a Panel
        visible when (DisplayMatrixApprovalHistory)
        list is ResponsibilityMatrixApprovalProcessorRel.ApprovalProcessorAuditRel.ApprovalProcessorHistoryCardHistoryCardList
    MatrixProcessIndicator is a DashBoardPanel
        title is "MatrixApprovalInformation"
        visible when (ResponsibilityMatrixApprovalProcessorRel.DisplayApprovalInformationPanel)
        show 4 columns
        Progress is a Panel
            triple width
            form is MatrixProcessIndicatorForm
        History is a Panel
            list is ResponsibilityMatrixApprovalProcessorRel.ApprovalProcessorAuditRel.ApprovalProcessorHistoryCardHistoryCardList
