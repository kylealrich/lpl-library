TransferPricingSourceForm is a CompositeForm
    is primary
    propagate as a drill
    title is "<TransferPricingSourceTitle>"
    context form is TransferPricingSourceContextForm
    Actions
        action is GenerateWeights
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "Previous"
        action is next record
            label is "_"
            mouse over text is "Next"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is save
            action icon is save
            label is "_"
            mouse over text is "Save"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
            mouse over text is "Refresh"
    SourceAddress is a Panel
        form is TransferPricingSourceAddressForm
    Weights is a Panel
        visible when (RechargeCalculationOption.SplitUsingSourceWeights)
        list is TransferPricingEntityWeight set.primary
    Structures is a Panel
        form is Structures
    SourceOverrides is a Panel
        list is TransferPricingSourceOverride set.primary
