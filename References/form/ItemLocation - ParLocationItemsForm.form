ParLocationItemsForm is a Form
    use for action ParLocationUpdate
    title is "ParLocationCounts"
    Layout
        paragraph
            Company
                display as text
            Company.Name
                no label
                display as text
        paragraph
            InventoryLocation
                label is "ParLocation"
                display as text
            InventoryLocation.Name
                no label
                display as text
        blank line
        single column
            Requisition.Requester
                display as text
            Requisition
                display as text
            Requisition.FillOrKill
                display as text
        blank line
        single column
            BinSequence
                display as text
            PreferredBin
                label is "ShelfLocation"
                display as text
        blank line
        paragraph
            Item
                display as text
            Item.Description
                no label
                display as text
        paragraph
            DefaultTransactionUOM.UnitOfMeasure
                label is "UnitOfMeasure"
                display as text
        paragraph
            ReorderPoint
                label is "ParLevel"
                display as text
        paragraph
            ParLocationCount
                label is "Count"
