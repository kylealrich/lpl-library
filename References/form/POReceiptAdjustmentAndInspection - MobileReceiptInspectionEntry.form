MobileReceiptInspectionEntry is a CompositeForm
    title is "ReceiptInspections"
    context form is MobileReceivingInspectionContext
    Actions
        action is save
            label is "_"
            hide when invalid
    Main is a Panel
        form is MobileMainReceiptInspection
    Lines is a Panel
        visible when (Status.Unreleased)
        template is POReceiptLineRels
            business class is POReceiptAdjustmentAndInspectionLine
                list is MobileReceiptInspectionLines
    ReleasedLines is a Panel
        title is "Lines"
        visible when (Status.Released)
        list is POReceiptAdjustmentAndInspectionLine set.MobileReceiptInspectionLines
    RejectOptions is a Panel
        form is RejectOptions
    Receiver is a Panel
        form is MobileReceiver
    Comments is a Panel
        title is "Comments<CommentCount>"
        list is POReceiptAdjustmentAndInspectionCommentRel.MobileReceiptInspectionCommentList
