AmendedContractLineMembersSummary is a Form
    title is "ContractLineMembers"
    Layout
        three column
            PricingIdentifier
                label is "PricingIdentifier"
                display as text
            MfgContractTierInfo.Tier
                label is "Tier"
                display as text
            MfgContractTierInfo.MfgContract
                label is "ManufacturerContract"
                display as text
        single column
            Contract.Supplier.SupplierName
                label is "Supplier"
                display as text
        two column
            ItemNumber
                display as text
            ContractLine.ItemDescription
                label is "ItemDescription"
                display as text
        single column
            VendorItem
                label is "VendorItemNumber"
                display as text
        single column
            BaseCost
                label is "Manufacturer'sNetCost"
                display as text
            PriceFactor
                label is "DistributorAdjustment"
                display as text
            FreightFactor
                label is "FreightFactor"
                display as text
        two column
            CurrentNetCost
                label is "LandedCost"
                display as text
            NetCostOverride
                label is "LandedCostOverride"
                display as text
        visible when (LineDistributorPricingEntered)
            header2 of "DistributorPrice"
            three column
                LineDistributorPricing.PricingMarkupDiscount
                    label is "Type"
                    display as text
                LineDistributorPricing.LineDistributorPricingAmtPct.PricingCost
                    label is "Cost"
                    display as text
                LineDistributorPricing.LineDistributorPricingAmtPct.PricingPercent
                    label is "Percent"
                    display as text
        visible when (DistributorFreightPricingExists)
            header2 of "DistributorFreightFactor"
            three column
                LineDistributorFreightPricing.FreightPricingMarkupDiscount
                    label is "MarkupOrDiscount"
                    display as text
                LineDistributorFreightPricing.LineDistributorFreightPricingAmtPct.FreightCost
                    label is "Cost"
                    display as text
                LineDistributorFreightPricing.LineDistributorFreightPricingAmtPct.FreightPercent
                    label is "Percent"
                    display as text
        header2 of "TierPrice"
        three column
            TierMarkupDiscount
                label is "Type"
                display as text
            LineTierPricingAmtPct.TierCost
                label is "Cost"
                display as text
            LineTierPricingAmtPct.TierPercent
                label is "Percent"
                display as text
        visible when (FreightMarkupDiscountEntered)
            header2 of "TierFreightFactor"
            three column
                FreightMarkupDiscount
                    label is "Type"
                    display as text
                LineFreightPricingAmtPct.FreightCost
                    label is "Cost"
                    display as text
                LineFreightPricingAmtPct.FreightPercent
                    label is "Percent"
                    display as text
