Miscellaneous is a Form
    Layout
        header2 of "StandardInformation"
        three column distributed
            HasExpirationDate
            CriticalItem
            Latex
        three column distributed
            PersonalProtectiveEquipment
            paragraph
                MajorPPEClass
                    label is "MajorPPEClass"
                MajorPPEClass.Description
                    no label
                    display as text
            paragraph
                MinorPPEClass
                    label is "MinorPPEClass"
                MinorPPEClass.Description
                    no label
                    display as text
        three column distributed
            Sterile
            Reprocessed
            Pharmaceutical
        three column distributed
            SingleUse
            Dressing
            Reusable
        single column
            Analytics
        visible when (CanShowHealthcareInformation)
            header2 of "HealthcareInformation"
            three column distributed
                Drugs
                Implantable
                Instrument
            three column distributed
                Narcotic
                ContainsHumanTissue
            three column distributed
                PatientChargeable
                ChargeNumber
                paragraph
                    GMDNPTCode
                        select is ActiveGMDNPTCodes
                    GMDNPTCode.Name
                        no label
                        display as text
                    visible when (GMDNTermEntered)
                        button of "TermCategories"
                            link is GMDNCategoryNav
                            display as link
            three column distributed
                paragraph
                    HCPCSCode
                        label is "HCPCS_andSequence"
                        compact format
                    HCPCSCode.ShortDescription
                        no label
                        display as text
                visible when (HCPCSCodeEntered)
                    paragraph
                        HCPCSCode.HCPCS
                            display as text
                        visible when (HSPCSCodeSequenceEnteredExists)
                            HCPCSCode.AlphaHCPCSSequence
                                label is "HCPCSSequence"
                                display as text
                        visible when (HSPCSCodeSequenceEnteredNotExists)
                            ItemAlphaHCPCSSequence
                                label is "HCPCSSequence"
                                display as text
                ClinicalSystemUse
            three column distributed
                UDITracked
                    label is "UDI/\\DSCSATracked"
                ConsignCode
                    label is "BillCode"
        header2 of "AdditionalInformation"
        paragraph
            PropertyID
