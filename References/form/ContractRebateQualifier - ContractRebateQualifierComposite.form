ContractRebateQualifierComposite is a CompositeForm
    is primary
    title is "<mode>ContractRebateQualifier"
    context form is ContractRebateQualifierContext
        background is theme color
    Main is a Panel
        form is ContractRebateQualifierSetup
    ContractSelection is a Panel
        visible when (ShowContractSelection)
        list is RebateQualifierContract set.AttachedRebateQualifierContracts
            helper list is HierarchicalContractsRel.OpenHierarchicalContracts
                action is SelectContractForRebate
                    invoked.RebateContract = Contract
                    invoked.ContractRebateType = ContractRebateType
                    invoked.ContractRebateQualifier = ContractRebateQualifier
    GPOContractSelection is a Panel
        visible when (ContractSupplierIsGPO)
        list is RebateQualifierContract set.AttachedRebateQualifierContracts
            helper list is GPOContractsRel.GPONegotiatedContracts
                action is SelectContractForRebate
                    invoked.RebateContract = Contract
                    invoked.ContractRebateType = ContractRebateType
                    invoked.ContractRebateQualifier = ContractRebateQualifier
    UNSPSC is a Panel
        visible when (NotAllInvoicesForSupplier)
        list is ContractRebateQualifierCode set.UnspscQualifierCodes
            helper list is UNSPSCCodeRel.UNSPSCCodesForRebates
                action is AttachToRebateQualifier
    ManufacturerInformation is a Panel
        visible when (CanDisplay)
        list is ContractRebateQualifierCode set.ManufacturerQualifierCodes
            helper list is ManufacturersRel.ManufacturerCodesForRebates
                action is AttachToRebateQualifier
    CommodityCode is a Panel
        visible when (NotAllInvoicesForSupplier)
        list is ContractRebateQualifierCode set.CommodityQualifierCodes
            helper list is CommodityCodeRel.CmCommodityCodesForRebates
                action is AttachToRebateQualifier
    MajorPurchasingClass is a Panel
        visible when (CanDisplay)
        list is ContractRebateQualifierCode set.PurchasingClassQualifierCodes
            helper list is MajorClassRel.CMMajorPurchasingClassesForRebates
                action is AttachToRebateQualifier
    MinorPurchasingClass is a Panel
        visible when (CanDisplay)
        list is ContractRebateQualifierCode set.PurchasingClassQualifierCodes
            helper list is MinorClassRel.CMMinorPurchasingClassesForRebates
                action is AttachToRebateQualifier
