CashLedgerCashReceiptDetailForm is a Form
    title is "CashReceiptDetail"
    Layout
        visible when (IsRebatePaymentType)
            blank line
            header2 of "Rebate"
            single column
                ContractRebateGroup.ContractGroup
                ContractRebateGroup.Contract
                    label is "CMContract"
                ContractRebateGroup.ContractRebateType
                    label is "RebateType"
                ContractRebateGroup.ContractRebateDue
                    label is "DueDate"
        blank line
        two column
            column1
                header2 of "Ledger"
                paragraph
                    ReceiptBankAmount
                        label is "Amount"
                paragraph
                    CatTaxEntity
                        label is "EntityForCategoryOrTaxCode"
                paragraph
                    CashCategory
                        label is "Category"
                paragraph
                    CashAllocationCode
                        display as text
                paragraph
                    ReceiptAccount
                        label is "FinanceStructure"
                        compact format
                paragraph
                    Reference
                paragraph
                    Description
                paragraph
                    UnitsAmount
            column2
                header2 of "Tax"
                paragraph
                    TaxCode
                paragraph
                    TaxableData.CurrencyAmount
                        label is "TaxableAmount"
                paragraph
                    TaxAmountData.CurrencyAmount
                        label is "TaxAmount"
