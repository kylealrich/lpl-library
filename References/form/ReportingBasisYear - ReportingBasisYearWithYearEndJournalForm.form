ReportingBasisYearWithYearEndJournalForm is a CompositeForm
    title is "ReportingBasis_<ReportingBasis>_<ReportingBasisYear>_Year_End_Journals"
    BalanceSheetOpeningJournal is a Panel
        list is YearEndJournalTransactionPerReportingBasisYearRel.BalanceSheetOpeningYearEndReportList
    ProfitandLossClosingJournals is a Panel
        title is "IncomeStatementClosingJournal"
        visible when (not ReportingBasis.ProcessRetainedEarnings)
        list is YearEndJournalTransactionPerReportingBasisYearRel.ProfitAndLossYearEndReportList
    BalanceSheetClosingJournal is a Panel
        list is YearEndJournalTransactionPerReportingBasisYearRel.BalanceSheetClosingYearEndReportList
