    Layout
        single column
            NewCompany
                label is "ToCompany"
            NewTemplate
                label is "ToTemplate"
            paragraph
                NewProjectInterfaceControl
                    label is "ToProjectInterfaceControl"
                PostingDate
                    display as required when (HasWIPAndClearingTransactions)
            NewAssetType
                label is "ToAssetType"
                compact format
            NewAssetDimensionGroup
                label is "ToAssetDimensionGroup"
            NewAssetAccountGroup
                label is "ToAssetAccountGroup"
            NewDefaultTransactionDimensions
                label is "ToDefaultTransactionDimensions"
                compact format
            NewLocation
                label is "ToLocation"
            NewDivision
                label is "ToDivision"
