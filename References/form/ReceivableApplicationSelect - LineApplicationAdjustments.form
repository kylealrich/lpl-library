LineApplicationAdjustments is a CompositeForm
    title is "SelectedLineApplication"
    context form is ReceivableApplicationLineSelectForm
        background is theme color
    DistributionSet is a Panel
        visible when (AllowSelectDistributionSet)
        list is ReceivableInvoiceDetailRel.ReceivableInvoiceRel.ProrateDistributionsRel.ReceivableDistributionSetSelectList1
    Adjustments is a Panel
        visible when (ShowAdjustments)
        list is ReceivableApplicationAdjustmentSelectRel.AdjustmentsForApplicationLine
