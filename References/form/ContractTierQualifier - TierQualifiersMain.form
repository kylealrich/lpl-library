TierQualifiersMain is a Form
    title is "ContractTierQualifiers"
    Layout
        single column
            BaseTier
        visible when (FirstTierQualifier)
            visible when (not BaseTier)
                visible when (QualifierContractDoesNotExist)
                    paragraph
                        TransientCreateQualifierContract
                            label is "AutoCreateAQualifierContractForTierCalculations"
                            bold
                            color of green
                        visible when (TransientCreateQualifierContract)
                            TransientCreateForAllItems
                                label is "CreateQualifierItemsForAllItemsOnQualifierContract"
                                bold
                                color of green
        visible when (not BaseTier)
            paragraph
                PurchaseType
                PurchaseBy
            single column
                PurchaseOperator
            paragraph
                PurchaseAmtPct.PurchasePercent
                    label is "PurchasePercent_ofAllPurchases"
                PurchaseAmtPct.PurchaseAmount
                    label is "PurchaseAmount"
            visible when (PurchaseType.TargetedPurchases)
                paragraph
                    TargetedPurchasesFromDate
                        label is "TargetedPurchasesFromDate"
                    TargetedPurchasesToDate
                        label is "TargetedPurchasesToDate"
            three column
                PurchaseQuantity
                    label is "Quantity"
                PurchaseBeginRange
                    label is "BeginDateForQuantityQualifier"
                PurchaseEndRange
                    label is "EndDate"
            single column
                OrGroup
                    label is "Group"
