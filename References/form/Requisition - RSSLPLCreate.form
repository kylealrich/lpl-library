RSSLPLCreate is an ActionForm
    action is Unreleased.RSSLPLCreate
        on completion navigate to RequesterShoppingPageNav
    submit label is "Ok"
    use for browser
    title is "CreateRequisition"
    Layout
        section of Main
            three column independent
                column1
                    blank line
                column2
                    paragraph
                        visible when (RSSLPLDisplayCompany)
                            PrmCompany
                                display as required
                                initial value is actor.agent(Employee).Employee.Requester.Company
                                when value changed
                                    PrmFromCompanyLocation.FromCompany =  PrmRequestingLocation.FromCompanyLocation.FromCompany
                                    PrmFromCompanyLocation.FromLocation =  PrmRequestingLocation.FromCompanyLocation.FromLocation
                                    PrmDefaultDistributionAccount =  PrmRequestingLocation.IssueAccount
                                    PrmPOCode =  PrmRequestingLocation.POCode
                                    PrmBuyer =  PrmRequestingLocation.Buyer
                                    PrmPurchaseTaxable =  PrmRequestingLocation.PurchaseTaxable
                                    PrmPurchaseTaxCode =  PrmRequestingLocation.PurchaseTaxCode
                            PrmCompany.Name
                                no label
                                display as text
                    paragraph
                        visible when (RSSLPLDisplayRequestingLocation)
                            PrmRequestingLocation
                                label is "RequestingLocation"
                                select is MobileActiveRequestingLocationsSelect
                                display as required
                                initial value is actor.agent(Employee).Employee.Requester.RequestingLocation
                                when value changed
                                    PrmFromCompanyLocation.FromCompany =  PrmRequestingLocation.FromCompanyLocation.FromCompany
                                    PrmFromCompanyLocation.FromLocation =  PrmRequestingLocation.FromCompanyLocation.FromLocation
                                    PrmDefaultDistributionAccount =  PrmRequestingLocation.IssueAccount
                                    PrmPOCode =  PrmRequestingLocation.POCode
                                    PrmBuyer =  PrmRequestingLocation.Buyer
                                    PrmPurchaseTaxable =  PrmRequestingLocation.PurchaseTaxable
                                    PrmPurchaseTaxCode =  PrmRequestingLocation.PurchaseTaxCode
                            PrmRequestingLocation.Name
                                no label
                                display as text
                    visible when (DisplayRequestedDeliveryDate)
                        PrmRequestedDeliveryDate
                            label is "RequestedDeliveryDate"
                            display as required when (RequesterRel.RequireRequestedDeliveryDate)
                    visible when (DisplaySingleDocToPO)
                        PrmOneSourceDocumentToOnePO
                            label is "SingleDocument_to_aPurchaseOrder"
                    visible when (DisplayDeliverTo)
                        PrmDeliverTo
                            label is "DeliverTo"
                            display as required when (RequesterRel.RequireDeliverTo)
                column3
                    paragraph
                        visible when (RSSLPLDisplayFromCompany)
                            PrmFromCompanyLocation.FromCompany
                                label is "DeliverFromCompany"
                                display as required
                                initial value is actor.agent(Employee).Employee.Requester.RequestingLocation.FromCompanyLocation.FromCompany
                            PrmFromCompanyLocation.FromCompany.Name
                                no label
                                display as text
                    paragraph
                        visible when (RSSLPLDisplayFromLocation)
                            PrmFromCompanyLocation.FromLocation
                                label is "DeliverFromLocation"
                                select is RQInventoryLocations
                                display as required
                                initial value is actor.agent(Employee).Employee.Requester.RequestingLocation.FromCompanyLocation.FromLocation
                            PrmFromCompanyLocation.FromLocation.Name
                                no label
                                display as text
                    visible when (DisplayRequisitionDescription)
                        PrmRequisitionDescription
                            label is "RequisitionDescription"
                            display as required when (RequesterRel.RequireRequisitionDescription)
                        visible when (RSSLPLDisplayAllocationPriorityLevel)
                            paragraph
                                PrmAllocationPriorityLevel
                                    initial value is RSSLPLAllocationPriorityLevelRel.AllocationPriorityLevel
                                PrmAllocationPriorityLevel.Description
                                    no label
                                    display as text
                        visible when (RSSLPLDisplayPriority)
                            PrmAllocationPriority
                                label is "Priority"
                        visible when (DisplayShippingNotes)
                            PrmShippingNotes
                                label is "ShippingNotes"
                                display as required when (RequesterRel.RequireShippingNotes)
            header2 of "LineDefaults"
            four column independent
                column1
                    blank line
                column2
                    visible when (DisplayVendor)
                        PrmVendor
                            label is "Vendor"
                            select is ActiveVendorsByName
                            display as required when (RequesterRel.RequireVendor)
                    visible when (DisplayPurchaseFrom)
                        PrmPurchaseFromLocation
                            label is "PurchaseFrom"
                            select is NonRemitToActivePurchaseFromLocations
                    visible when (RSSLPLDisplayCurrencyCode)
                        PrmTransactionCurrencyCode
                            label is "CurrencyCode"
                    PrmDefaultCommentCode
                        label is "CommentCode"
                column3
                    visible when (DisplayBuyer)
                        PrmBuyer
                            label is "Buyer"
                            display as required when (RequesterRel.RequireBuyer)
                            initial value is PrmRequestingLocation.Buyer
                    visible when (DisplayOnlyBuyer)
                        PrmBuyer
                            label is "Buyer"
                            display only
                            initial value is PrmRequestingLocation.Buyer
                    visible when (DisplayPOCode)
                        PrmPOCode
                            label is "POCode"
                            display as required when (RequesterRel.RequirePOCode)
                            initial value is PrmRequestingLocation.POCode
                    visible when (DisplayCommodityCode)
                        PrmCommodityCode
                            label is "CommodityCode"
                            show up to 20 characters
                    visible when (DisplayDeliveryMethod)
                        PrmDeliveryMethod
                            label is "DeliveryMethod"
                column4
                    visible when (DisplayStrategicSourcingUsed)
                        PrmSourcingEventRequired
                            label is "SourcingEventRequired"
                    visible when (DisplayBillCode)
                        PrmDefaultBillCode
                            label is "BillCode"
                    paragraph
                        visible when (DisplayTaxInfo)
                            PrmPurchaseTaxable
                                label is "Taxable"
                                initial value is PrmRequestingLocation.PurchaseTaxable
                            visible when (PrmPurchaseTaxable)
                                PrmPurchaseTaxCode
                                    label is "TaxCode"
                                    select is ValidTaxCodesForEntity
                                    initial value is PrmRequestingLocation.PurchaseTaxCode
            three column independent
                column1
                    blank line
                column2
            visible when (DisplayAccounting)
                PrmDefaultDistributionAccount
                    label is "DistributionAccount"
                    compact format
                    initial value is PrmRequestingLocation.IssueAccount
            visible when (false)
                PrmCurrentDefaultDistributionAccount
                    initial value is PrmRequestingLocation.IssueAccount
                PrmDefaultDistributionAccount.ToAccountingEntity
                    when value changed
                        PrmCurrentDefaultDistributionAccount =  PrmDefaultDistributionAccount
                PrmDefaultDistributionAccount.GeneralLedgerChartAccount
                    when value changed
                        PrmCurrentDefaultDistributionAccount =  PrmDefaultDistributionAccount
                PrmDefaultDistributionAccount.AccountingUnit
                    when value changed
                        PrmCurrentDefaultDistributionAccount =  PrmDefaultDistributionAccount
                PrmDefaultDistributionAccount.Project
                    when value changed
                        PrmCurrentDefaultDistributionAccount =  PrmDefaultDistributionAccount
                PrmDefaultDistributionAccount.FinanceDimension1
                    when value changed
                        PrmCurrentDefaultDistributionAccount =  PrmDefaultDistributionAccount
                PrmDefaultDistributionAccount.FinanceDimension2
                    when value changed
                        PrmCurrentDefaultDistributionAccount =  PrmDefaultDistributionAccount
                PrmDefaultDistributionAccount.FinanceDimension3
                    when value changed
                        PrmCurrentDefaultDistributionAccount =  PrmDefaultDistributionAccount
                PrmDefaultDistributionAccount.FinanceDimension4
                    when value changed
                        PrmCurrentDefaultDistributionAccount =  PrmDefaultDistributionAccount
                PrmDefaultDistributionAccount.FinanceDimension5
                    when value changed
                        PrmCurrentDefaultDistributionAccount =  PrmDefaultDistributionAccount
                PrmDefaultDistributionAccount.FinanceDimension6
                    when value changed
                        PrmCurrentDefaultDistributionAccount =  PrmDefaultDistributionAccount
                PrmDefaultDistributionAccount.FinanceDimension7
                    when value changed
                        PrmCurrentDefaultDistributionAccount =  PrmDefaultDistributionAccount
                PrmDefaultDistributionAccount.FinanceDimension8
                    when value changed
                        PrmCurrentDefaultDistributionAccount =  PrmDefaultDistributionAccount
                PrmDefaultDistributionAccount.FinanceDimension9
                    when value changed
                        PrmCurrentDefaultDistributionAccount =  PrmDefaultDistributionAccount
                PrmDefaultDistributionAccount.FinanceDimension10
                    when value changed
                        PrmCurrentDefaultDistributionAccount =  PrmDefaultDistributionAccount
        section of ShippingInformation visible when (DisplayDropShip)
            header2 of "<actor.agent(Employee).Employee.Requester.AlternateAddressDropshipTitle>"
            visible when (false)
                actor.agent(Employee).Employee.Requester.AllowAlternateShipToAddress
            paragraph
                visible when (DisplayDropShip)
                    PrmDropShipDefaulting
                        display as horizontal radio buttons
                        when value changed
                            PrmDropshipName =  PrmRequestingLocation.Name
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipAddress.Country =  PrmRequestingLocation.PostalAddress.Country
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipAddress.DeliveryAddress.AddressLine1 =  PrmRequestingLocation.PostalAddress.DeliveryAddress.AddressLine1
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipAddress.DeliveryAddress.AddressLine2 =  PrmRequestingLocation.PostalAddress.DeliveryAddress.AddressLine2
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipAddress.DeliveryAddress.AddressLine3 =  PrmRequestingLocation.PostalAddress.DeliveryAddress.AddressLine3
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipAddress.DeliveryAddress.AddressLine4 =  PrmRequestingLocation.PostalAddress.DeliveryAddress.AddressLine4
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipAddress.Municipality =  PrmRequestingLocation.PostalAddress.Municipality
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipAddress.StateProvince =  PrmRequestingLocation.PostalAddress.StateProvince
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipAddress.PostalCode =  PrmRequestingLocation.PostalAddress.PostalCode
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipPhoneNumber.InternationalPrefix =  blank
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipPhoneNumber.SubscriberNumber =  blank
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipPhoneNumber.Extension =  blank
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipAddress.County =  PrmRequestingLocation.PostalAddress.County
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipAddress.Region =  PrmRequestingLocation.PostalAddress.Region
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipAddress.NationalIDNumber =  PrmRequestingLocation.PostalAddress.NationalIDNumber
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipAddress.StateProvinceIDNumber =  PrmRequestingLocation.PostalAddress.StateProvinceIDNumber
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipAddress.LocaleIDNumber =  PrmRequestingLocation.PostalAddress.LocaleIDNumber
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipAddress.IndividualIDNumber =  PrmRequestingLocation.PostalAddress.IndividualIDNumber
                                when (PrmDropShipDefaulting.RequestingLocationAddress)
                            PrmDropshipName =  actor.agent(Employee).Employee.Requester.Name
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipAddress.Country =  actor.agent(Employee).Employee.Requester.RequesterPostalAddress.Country
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipAddress.DeliveryAddress.AddressLine1 =  actor.agent(Employee).Employee.Requester.RequesterPostalAddress.DeliveryAddress.AddressLine1
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipAddress.DeliveryAddress.AddressLine2 =  actor.agent(Employee).Employee.Requester.RequesterPostalAddress.DeliveryAddress.AddressLine2
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipAddress.DeliveryAddress.AddressLine3 =  actor.agent(Employee).Employee.Requester.RequesterPostalAddress.DeliveryAddress.AddressLine3
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipAddress.DeliveryAddress.AddressLine4 =  actor.agent(Employee).Employee.Requester.RequesterPostalAddress.DeliveryAddress.AddressLine4
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipAddress.Municipality =  actor.agent(Employee).Employee.Requester.RequesterPostalAddress.Municipality
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipAddress.StateProvince =  actor.agent(Employee).Employee.Requester.RequesterPostalAddress.StateProvince
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipAddress.PostalCode =  actor.agent(Employee).Employee.Requester.RequesterPostalAddress.PostalCode
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipPhoneNumber.InternationalPrefix =  actor.agent(Employee).Employee.Requester.DerivedPInternationalPrefix
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipPhoneNumber.SubscriberNumber =  actor.agent(Employee).Employee.Requester.DerivedPSubscriberNumber
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipPhoneNumber.Extension =  actor.agent(Employee).Employee.Requester.DerivedPExtension
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipAddress.County =  actor.agent(Employee).Employee.Requester.RequesterPostalAddress.County
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipAddress.Region =  actor.agent(Employee).Employee.Requester.RequesterPostalAddress.Region
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipAddress.NationalIDNumber =  actor.agent(Employee).Employee.Requester.RequesterPostalAddress.NationalIDNumber
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipAddress.StateProvinceIDNumber =  actor.agent(Employee).Employee.Requester.RequesterPostalAddress.StateProvinceIDNumber
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipAddress.LocaleIDNumber =  actor.agent(Employee).Employee.Requester.RequesterPostalAddress.LocaleIDNumber
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipAddress.IndividualIDNumber =  actor.agent(Employee).Employee.Requester.RequesterPostalAddress.IndividualIDNumber
                                when (PrmDropShipDefaulting.RequesterAddress)
                            PrmDropshipName =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                            PrmDropshipAddress.Country =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                            PrmDropshipAddress.DeliveryAddress.AddressLine1 =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                            PrmDropshipAddress.DeliveryAddress.AddressLine2 =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                            PrmDropshipAddress.DeliveryAddress.AddressLine3 =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                            PrmDropshipAddress.DeliveryAddress.AddressLine4 =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                            PrmDropshipAddress.Municipality =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                            PrmDropshipAddress.StateProvince =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                            PrmDropshipAddress.PostalCode =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                            PrmDropshipPhoneNumber.InternationalPrefix =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                            PrmDropshipPhoneNumber.SubscriberNumber =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                            PrmDropshipPhoneNumber.Extension =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                            PrmDropshipAddress.County =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                            PrmDropshipAddress.Region =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                            PrmDropshipAddress.NationalIDNumber =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                            PrmDropshipAddress.StateProvinceIDNumber =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                            PrmDropshipAddress.LocaleIDNumber =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                            PrmDropshipAddress.IndividualIDNumber =  blank
                                when (PrmDropShipDefaulting.ClearDefaults)
                visible when (actor.agent(Employee).Employee.Requester.AllowDropship)
                    PrmReceivingOptions
                        label is "ReceivingOptions"
                        when value changed
                            PrmDropship =  false
                                when (PrmReceivingOptions.ReceivingRequired)
                            PrmDropship =  true
                                when (PrmReceivingOptions.ReceivingNotAllowed)
                visible when (false)
                    PrmDropship
            two column
                PrmDropshipName
                    label is "Name"
                    display as required when (PrmDropship)
                PrmDropshipContact
                    label is "Contact"
            single column
                PrmDropshipPhoneNumber
            visible when (not PrmDropship)
                PrmDropshipAddress
                    form is PostalAddressShortNotRequired
            visible when (PrmDropship)
                PrmDropshipAddress
                    form is PostalAddressShortRequired
