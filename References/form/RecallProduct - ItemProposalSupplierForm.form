ItemProposalSupplierForm is a Form
    Layout
        visible when (ProposedItemNotSubmitted)
            two column
                ProductName
                    label is "ItemName"
                ProductDescription
                    label is "ItemDescription"
                    use text area of 2 lines
            single column
                ProposedValue
                    label is "ProductJustification_orROI*"
                    use text area of 2 lines
            two column
                visible when (RecallNotice.ReportedBySupplierEntered)
                    ProposedVendorItem
                        label is "MyItemNumber*"
                visible when (RecallNotice.ReportedByRecallUserEntered)
                    ProposedVendorItem
                        label is "ProposedVendorItem*"
                    visible when (not RecallNotice.ItemGroupRel.AutomaticItemNumbering)
                        ProposedManualItemNumber
                            label is "ProposedItemNumber"
                UnitCost
                    label is "UnitCost_forLowUnit_ofMeasure*"
            header3 of "Units_ofMeasure"
            four column table
                column headers
                    "UnitOfMeasure"
                    "UnitsPerEach"
                    "UseForBuying"
                    "UseForIssues"
                column1
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[1].UnitOfMeasure
                        no label
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[2].UnitOfMeasure
                        no label
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[3].UnitOfMeasure
                        no label
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[4].UnitOfMeasure
                        no label
                column2
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[1].UOMConversion
                        no label
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[2].UOMConversion
                        no label
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[3].UOMConversion
                        no label
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[4].UOMConversion
                        no label
                column3
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[1].UseForBuying
                        no label
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[2].UseForBuying
                        no label
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[3].UseForBuying
                        no label
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[4].UseForBuying
                        no label
                column4
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[1].UseForIssues
                        no label
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[2].UseForIssues
                        no label
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[3].UseForIssues
                        no label
                    ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[4].UseForIssues
                        no label
            single column
                ProposedManufacturerNumber
                    label is "<DerivedManufacturerNumberLabel>"
                ProposedGlobalTradeItemNumber
                    label is "GTIN"
                LatexFree
                ProductServiceLine
                ExpectedMonthlyUsage
                    label is "ExpectedMonthlyUsageInLowUnitOfMeasure"
            paragraph
                CurrentContract
                    select is SupplierContractsRel.SupplierContractsForNewItemProposal
                CurrentContract.Name
                    display as text
            header3 of "ItemBeingReplaced"
            single column
                visible when (RecallNotice.ReportedBySupplierEntered)
                    VendorItem
                        label is "MyItemNumber"
                visible when (RecallNotice.ReportedByRecallUserEntered)
                    VendorItem
                ManufacturerInformation.ManufacturerNumber
                GlobalTradeItemNumber
        visible when (not ProposedItemNotSubmitted)
            display as text
                two column
                    ProductName
                        label is "ItemName"
                    ProductDescription
                        label is "ItemDescription"
                    ProposedValue
                    ProposedVendorItem
                        label is "ItemNumber"
                    visible when (not RecallNotice.ItemGroupRel.AutomaticItemNumbering)
                        ProposedManualItemNumber
                    ProposedManufacturerNumber
                        label is "ManufacturerNumber"
                    ProposedGlobalTradeItemNumber
                        label is "GTIN"
                    LatexFree
                    ProductServiceLine
                    UnitCost
                    ExpectedMonthlyUsage
                    CurrentContract
                header3 of "Units_ofMeasure"
                four column table
                    column headers
                        "UnitOfMeasure"
                        "UnitsPerEach"
                        "UseForBuying"
                        "UseForIssues"
                    column1
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[1].UnitOfMeasure
                            no label
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[2].UnitOfMeasure
                            no label
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[3].UnitOfMeasure
                            no label
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[4].UnitOfMeasure
                            no label
                    column2
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[1].UOMConversion
                            no label
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[2].UOMConversion
                            no label
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[3].UOMConversion
                            no label
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[4].UOMConversion
                            no label
                    column3
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[1].UseForBuying
                            no label
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[2].UseForBuying
                            no label
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[3].UseForBuying
                            no label
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[4].UseForBuying
                            no label
                    column4
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[1].UseForIssues
                            no label
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[2].UseForIssues
                            no label
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[3].UseForIssues
                            no label
                        ItemUOMContract.ItemUOMContractOccurs.ItemUOMContractDetails[4].UseForIssues
                            no label
                header3 of "ItemBeingReplaced"
                three column distributed
                    VendorItem
                        label is "MyItemNumber"
                    ManufacturerInformation.ManufacturerNumber
                    GlobalTradeItemNumber
