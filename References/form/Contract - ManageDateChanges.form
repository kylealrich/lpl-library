ManageDateChanges is a CompositeForm
    context form is ContractContextShort
        background is theme color
    Actions
        action is UpdateContractDate
        navigation is ViewContract
            label is "GoToContract"
        action is refresh
            label is "_"
    CurrentDates is a Panel
        form is ContractDates
    LinesWithDifferentDates is a Panel
        visible when (HasLinesWithMismatchedDates)
        list is LineDateMismatchRel.MismatchedDates
    DistributorContractDates is a Panel
        visible when (HasDistributorContract)
        list is AttachedManufacturerContractsRel.DistributorContractDates
    ManufacturerContractDates is a Panel
        visible when (HasManufacturerContract)
        list is ManufacturerContractForDistContractRel.ManufacturerContractDates
