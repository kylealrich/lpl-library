BuyerSpendAllInvoiceLinesForItem is a CompositeForm
    title is "SpendAnalysisInvoiceLines"
    context form is ItemConsolidationWithEventContext
    OnContractInvoices is a Panel
        visible when (HasOnContractInvoiceLinesForLocationItem)
        list is OnContractInvoiceLineFactsByBuyerLocItemRel.OnContractInvoiceLineFacts
    OffContractInvoices is a Panel
        visible when (HasOffContractInvoiceLinesForLocationItem)
        list is OffContractInvoiceLineFactsByBuyerLocItemRel.OffContractInvoiceLineFacts
    NoContractInvoices is a Panel
        visible when (HasNoContractInvoiceLinesForLocationItem)
        list is NoContractInvoiceLineFactsByBuyerLocItemRel.InvoiceLineFacts
