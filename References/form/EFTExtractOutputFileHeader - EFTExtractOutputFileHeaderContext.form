EFTExtractOutputFileHeaderContext is a Form
    title is "EFTExtractOutputFile"
    Layout
        display as text
            two column distributed
                column1
                    CashManagementAccount
                    CashCode
                    BankTransactionCode
                    CreateDate
                column2
                    TotalNumberOfRecords
                    TotalPaymentAmount
                    FileName
            paragraph
                HeaderFile.Title
                    label is "HeaderFile"
                visible when (HasHeaderAttachment)
                    button of "View"
                        view HeaderFile.File
                        display as link
                HeaderOutputFileName
            paragraph
                DetailFile.Title
                    label is "DetailFile"
                visible when (HasDetailAttachment)
                    button of "View"
                        view DetailFile.File
                        display as link
                DetailOutputFileName
            paragraph
                FlatFile.Title
                    label is "FlatFile"
                visible when (HasFlatFileAttachment)
                    button of "View"
                        view FlatFile.File
                        display as link
                FlatFileOutputFileName
