CreatePurchasingByDiversityCodeCertificationSummary is a Form
    title is "ViewCreatedReport"
    Layout
        single column
            paragraph
                text of "ClickLinkToViewReport:"
                    color of gray
                button of "ViewReport"
                    link is PurchasingByDiversityCodeCertification
                    display as link
            Description
                label is "ReportDescription"
                display as text
            SupplyManagementReportSourcingGroupField.SourcingGroup
                display as text
            PurchasingCompany
                label is "Company"
                display as text
            paragraph
                FromDate
                    display as text
                ThruDate
                    display as text
            IncludeDetails
                display as text
            multi-select field is ExcludePOCodeArray
                label is "ExcludePOCode(s)"
                select is POCodesRel.POCodes
                use 6 lines
            PrintParameters
                display as text
