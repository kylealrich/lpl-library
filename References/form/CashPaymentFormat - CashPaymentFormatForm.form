CashPaymentFormatForm is a Form
    title is "CashPaymentFormat"
    Layout
        visible when (RecordExists)
            display as text
                two column distributed
                    column1
                        CashManagementGroup
                        paragraph
                            CashCode.CashCodeAndDescription
                                label is "CashCode"
                    column2
                        paragraph
                            CashPaymentFormat.BankTransactionCode.BankTransactionCodeName
                                label is "BankTransactionCode"
                        paragraph
                            visible when (CashPaymentFormat.PayablesProcessGroup entered)
                                CashPaymentFormat.PayablesProcessGroup.PayablesProcessGroupName
                                    label is "PayablesProcessGroup"
                            visible when (CashPaymentFormat.PayablesProcessGroup not entered)
                                CashPaymentFormat.PayablesProcessGroup
                                    label is "PayablesProcessGroup"
