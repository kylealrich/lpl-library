ProjectContractMilestoneForm is a CompositeForm
    is primary
    title is "<FinanceEnterpriseGroup.FinanceDimension2Label>_<MilestoneLabel>_<Description>"
    context form is ProjectContractMilestoneContext
        background is theme color
    MainForm is a Panel
        title is "Main"
        form is MainForm
    BillingOverrides is a Panel
        title is "RevenueOverrides"
        visible when (BillingOverridesValid)
        list is ProjectContractMilestoneDistribution set.ProjectContractMilestoneDistributions
    RevenueOverrides is a Panel
        visible when (RevenueOverridesValid)
        list is ProjectContractMilestoneDistribution set.ProjectContractMilestoneDistributions
    RevenueFinanceStructures is a Panel
        visible when (RevenueAccountAllowed)
        list is ProjectContractMilestoneDistribution set.OverrideMilestoneDistributions
