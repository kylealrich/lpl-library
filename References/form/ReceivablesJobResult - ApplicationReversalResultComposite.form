ApplicationReversalResultComposite is a CompositeForm
    title is "<DerivedFormTitle>"
    context form is ApplicationReversalResultContext
        background is theme color
    Actions
        action is ApplicationReversalCreation
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is DeleteReversalResults
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
            hide when invalid
    ApplicationsPreview is a Panel
        title is "Applications"
        visible when (ReversedApplicationsExistForPreview)
        list is ReversedApplicationsRel.ReversedApplicationsPreview
    Applications is a Panel
        visible when (ReversedApplicationsExist)
        list is ReversedApplicationsRel.ReversedApplications
    ApplicationsWithErrors is a Panel
        title is "Errors"
        visible when (ApplicationsWithErrors)
        list is ReceivablesJobErrorResultRecordsRel.ReversalsWithErros
    NoApplicationsError is a Panel
        title is "Errors"
        visible when (NoApplicationsError)
        list is ReceivablesJobErrorResultWithNoApplicationRel.ReversalErrosWithNoApplication
