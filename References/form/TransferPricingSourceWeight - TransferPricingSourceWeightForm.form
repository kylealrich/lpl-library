TransferPricingSourceWeightForm is a Form
    is primary
    propagate as a drill
    title is "FinanceTotalAddress"
    Layout
        text of "SelectedAmountTypeIsConvertedToRechargeCurrency"
        single column
            TransferPricing
                label is "RechargeName"
            TransferPricingSourceWeight
                label is "TotalAddressName"
            paragraph
                SourceEntity
                    label is "BillToEntity"
                visible when (SourceEntity.SpecificEntity)
                    SourceAddress.AccountingEntity
            visible when (FinanceEnterpriseGroup.AccountingUnitSelected)
                paragraph
                    SourceAddress.AccountingUnit
                        label is FinanceEnterpriseGroup.AccountingUnitLabel
                        select is AccountingUnitTreeSelect
                    visible when (not FinanceEnterpriseGroup.AccountingUnitRequired)
                        TreatBlankDimension1AsEmpty
                            label is "TreatBlank<FinanceEnterpriseGroup.AccountingUnitLabel>AsEmpty"
            SourceAddress.GeneralLedgerChartAccount
                label is FinanceEnterpriseGroup.AccountLabel
                select is ChartAccountList
            visible when (FinanceEnterpriseGroup.ProjectEntry)
                paragraph
                    SourceAddress.Project
                        label is FinanceEnterpriseGroup.ProjectLabel
                        select is ProjectTreeSelect
                    visible when (not FinanceEnterpriseGroup.ProjectRequired)
                        TreatBlankProjectAsEmpty
                            label is "TreatBlank<FinanceEnterpriseGroup.ProjectLabel>AsEmpty"
            visible when (FinanceEnterpriseGroup.FinanceDimension1Entry)
