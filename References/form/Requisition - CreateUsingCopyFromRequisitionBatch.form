            PrmFromCompanyLocation.FromLocation
                label is "FromLocation"
            PrmFromCompanyLocation.FromLocation.Name
                no label
                display as text
        paragraph
            PrmRequestedDeliveryDate
                label is "RequestedDeliveryDate"
        paragraph
            PrmVendor
                label is "Vendor"
            PrmVendor.VendorName
                no label
                display as text
        paragraph
            PrmPurchaseFromLocation
                label is "PurchaseFromLocation"
        paragraph
            PrmRequisitionSource
                label is "RequisitionSource"
        paragraph
            PrmDeliverTo
                label is "DeliverTo"
        paragraph
            PrmPriority
                label is "Priority"
        paragraph
            PrmBuyer
                label is "Buyer"
        paragraph
            PrmRequisitionDescription
                label is "RequisitionDescription"
        paragraph
            PrmCommodityCode
                label is "CommodityCode"
        paragraph
            PrmTaxCode
                label is "TaxCode"
            PrmPurchaseTaxable
                label is "PurchaseTaxable"
        paragraph
            PrmPoCode
