ScheduledProcedureLineContext is a Form
    Layout
        four column distributed
            display as text
                column1
                    ScheduledProcedureLine
                    ComponentOpenQuantity
                        label is "Open"
                column2
                    Item.RepresentativeText
                        label is "Item"
                    ComponentHoldQuantity
                        label is "Hold"
                column3
                    ComponentUOM
                        label is "UOM"
                    DerivedTotalQuantity
                        label is "Total"
                column4
                    blank line
                    PickedQuantity
                        label is "TotalPickedQuantity"
