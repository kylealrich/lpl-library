CreateProjectContractChangeRequest is an ActionForm
    action is CreateProjectContractChangeRequest
    title is "Create<FinanceEnterpriseGroup.ProjectLabel>ContractChangeRequest"
    Layout
        two column
            DisplayContract
                label is "<FinanceEnterpriseGroup.ProjectLabel>Contract"
                display as text
            Description
                display as text
        two column
            PrmChangeRequestDate
                label is "ChangeRequestDate"
            PrmDescription
                label is "Description"
        two column
            ContractAmount
                display as text
            PrmNewContractAmount
                label is "NewContractAmount"
        two column
            MaximumFees
                display as text
            PrmNewMaximumFees
                label is "NewMaximumFees"
        two column
            PrmScenario
                label is "BudgetScenario"
                select is ProjectBudgetsSelect
            PrmScenario.Description
                display as text
        two column
            PrmBudgetChangeOrder
                label is "BudgetChangeRequest"
                select is BudgetChangeOrdersProjectSelect
            PrmBudgetChangeOrder.Description
                display as text
        single column
            PrmRequestedBy
                label is "RequestedBy"
                display as text
            PrmComment
                label is "Comment"
            PrmDocumentURL
                label is "DocumentURL"
            PrmAttachment
                label is "Attachment"
