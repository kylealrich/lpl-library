TransactionDetailsContextForm is a Form
    title is "TransactionDetails"
    Layout
        paragraph
            StudentActivityClosedPeriod
                label is "Period"
            StudentActivityAccount
                label is "Activity"
                select is StudentActivityAccountSelectList
            StudentActivityAccount.Description
                no label
                display only
            button of "PrintReport"
                link is PrintableTransactionDetails
        paragraph
            PeriodReceipts
                label is "Receipts"
                display only
            PeriodDisbursements
                label is "Disbursements"
                display only
            PeriodTransfers
                label is "Transfers"
                display only
