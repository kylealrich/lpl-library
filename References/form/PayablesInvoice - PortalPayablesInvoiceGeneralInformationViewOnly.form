PortalPayablesInvoiceGeneralInformationViewOnly is a Form
    title is "<DerivedFormTitle>"
    print form is PortalPayablesInvoiceGeneralInformationViewOnlyPrintForm
    Layout
        display as text
            three column distributed
                column1
                    DerivedCompanyNameID
                        label is "Company"
                    paragraph
                        APClerk.PreferredSimplePresentationName
                            label is "Processor"
                        button of "<APClerk.Employee.EmployeeWorkEmailAddress>"
                            visible when (APClerk.Employee.EmployeeWorkEmailAddressExists)
                            link is "mailto:<APClerk.Employee.EmployeeWorkEmailAddress>"
                            display as link
                        button of "<APClerk.Employee.EmployeeWorkTelephone>"
                            visible when (APClerk.Employee.EmployeeWorkPhoneExists)
                            link is "tel:<APClerk.Employee.EmployeeWorkTelephone>"
                            display as link
                    visible when (DiscountAmountEntered)
                        DiscountDate
                    visible when (TaxInformationEntered)
                        TaxableAmount
                            label is "EnteredTaxableAmount"
                        PayablesInvoiceTaxRel.TotalInvoicedTaxableAmount
                        PayablesInvoiceTaxRel.TotalAccruedTaxableAmount
                column2
                    VoucherNumberDisplay
                    DerivedRemitToNameID
                    visible when (DiscountAmountEntered)
                        DiscountAmount.CurrencyAmount
                            label is "DiscountAmount"
                    visible when (TaxInformationEntered)
                        TaxAmount
                            label is "EnteredTaxAmount"
                        PayablesInvoiceTaxRel.TotalInvoicedTaxAmount
                        PayablesInvoiceTaxRel.TotalAccruedTaxAmount
                column3
                    visible when (PurchaseOrderEntered)
                        paragraph
                            DisplayPurchaseOrder
                            button of "ViewPO"
                                visible when (DisplayPOLink)
                                link is FirstPOInformation
                                display as link
                    visible when (ServiceContractEntered)
                        ServiceContract
        visible when (AllowUpdateOnApprove)
            visible when (not UsesMatrixApproval)
                header2 of "ApproverInformation"
                display as text
                    visible when (first PfiWorkunitRel.HasProcessIndicator)
                        single column
                            first PfiWorkunitRel.PfiProcessIndicatorRel.Model
                                no label
                            paragraph
                                last PayablesInvoiceAudit set.Status
                                    display as tag
                                        color of blue when (AuditStatusTagBlue)
                                        color of red when (AuditStatusTagRed)
                                        color of green when (AuditStatusTagGreen)
                                visible when (last PayablesInvoiceAudit set.ActionReason entered)
                                    last PayablesInvoiceAudit set.ActionReason
                                        label is "Reason"
                                visible when (last PayablesInvoiceAudit set.Comment entered)
                                    last PayablesInvoiceAudit set.Comment
                                        show up to 30 characters
                    visible when (not first PfiWorkunitRel.HasProcessIndicator)
                        two column
                            DerivedPreviousRoutingApprover
                            paragraph
                                DerivedNextRoutingApprover
                                button of "ViewTeam"
                                    visible when (ApprovalTeamEntered)
                                    link is ApprovalTeam
                                    display as link
                                button of "ViewApprovers"
                                    visible when (DisplayResponsibleRole)
                                    link is NextApproversForRole
                                    display as link
                two column distributed
                    column1
                        header2 of "ReassignInformation"
                        paragraph
                            visible when (ReassignToPreviousApproverValid)
                                ReassignToPreviousApprover
                                DerivedPreviousRoutingApprover
                                    no label
                                    display as text
                        visible when (ReassignToPreviousApproverValid)
                            text of "-\\Or_-"
                        paragraph
                            ReassignToApprovalLevel
                                label is "ReassignTo"
                                select is InvoiceApproversPFlowSelect
                            ReassignToApprovalLevel.ApprovalLevel
                                display as text
                            ReassignToApprovalLevel.DerivedDescription
                                no label
                                display as text
                            text of "ApprovalLevelRequiredIfReassigningTheInvoiceToAnotherLevel"
                                bold
                                italics
                                color of red
                    column2
                        visible when (RoutingCode.UsesTeamMemberAssignment)
                            header2 of "RoutingInformation"
                            paragraph
                                RoutingFinanceTeam
                                RoutingFinanceTeamMember
                                    label is "RoutingTeamMember"
                                    compact format
                                RoutingFinanceTeamMember.TeamMember.PreferredSimplePresentationName
                                    no label
                                    display as text
                                text of "Routing_\\Team_\\MemberIndicatesAssignedApproverWhenThe_\\Finance_\\TeamMatchesApprovalLevelTeam"
                                    bold
                            paragraph
                                text of "ReassignToNewTeamByUpdatingRoutingTeam,MemberAndApprovalLevel"
                                    bold
                                    italics
                                    color of red
                            paragraph
                                text of "ReassignToNewTeamMemberByUpdatingRoutingTeamMember"
                                    bold
                                    italics
                                    color of red
