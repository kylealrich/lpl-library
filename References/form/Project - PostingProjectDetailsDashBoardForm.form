PostingProjectDetailsDashBoardForm is a Form
    title is "Details"
    Layout
        two column distributed
            column1
                ProjectStatus
                    label is "<FinanceEnterpriseGroup.ProjectLabel>Status"
                    show up to 10 characters
            column2
                ProjectStatus.Description
                    no label
                    display as text
        two column distributed
            column1
                Currency
                    show up to 3 characters
            column2
                Currency.Description
                    no label
                    display as text
        two column distributed
            column1
                Active
            column2
                Billable
        three column distributed
            column1
                Watch
            column2
                paragraph
                    WatchedBy
                    WatchedBy.Name.PreferredFirstAndLastName
                        no label
                        display as text
            column3
                IncludeInBudgetEdit
        three column distributed
            column1
                ExcludeFromFES
                    label is "ExcludeFromFrontEndSplit"
            column2
                Capital
            column3
                SystemProject
                    label is "System<FinanceEnterpriseGroup.ProjectLabel>"
        two column distributed
            column1
                visible when (FinanceEnterpriseGroup.ExpenseManagementIntegrated)
                    ExpenseManagement
        three column distributed
            column1
                WorkType
            column2
                ServiceType
            column3
                UsageType
