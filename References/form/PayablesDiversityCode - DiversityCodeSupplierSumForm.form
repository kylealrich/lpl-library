DiversityCodeSupplierSumForm is a SummaryForm
    title is "_"
    Layout
        visible when (SupplyManagementReport.SupplierDetail)
            header4 of "DiversityCode:\\<PayablesDiversityCode>"
            form is SupplierDiversityCodeRel.EventNotificationByDiversitySupplierSumForm
        header4 of "TotalsForDiversityCode\\:_<PayablesDiversityCode>"
        three column
            column1
                DerivedTotalEventsBidByDiversity
                    label is "TotalEventsBid"
                    display when blank
                DerivedTotalEventsAwardedByDiversity
                    label is "TotalEventsAwarded"
                    display when blank
                DerivedEventsBidPercentByDiversity
                    label is "EventBidSuccess"
                    display when blank
            column2
                DerivedTotalEventLinesBidByDiversity
                    label is "TotalLinesBid"
                    display when blank
                DerivedTotalEventLinesAwardedByDiversity
                    label is "TotalLinesAwarded"
                    display when blank
                DerivedEventLinesBidPercentBySupplier
                    label is "LineBidSuccess"
                    display when blank
            column3
                DerivedBidTotalByDiversity
                    label is "BidTotal"
                    display when blank
                DerivedAwardedTotalByDiversity
                    label is "TotalAwardedAmount"
                    display when blank
                DerivedTotalBidPercentByDiversity
                    label is "AmountBidSuccess"
                    display when blank
