UpdateCrossReferenceInvoice is an ActionForm
    action is Released.UpdateCrossReferenceInvoice
    Layout
        four column
            column1
                PrmCrossReferenceType
                    label is "Type"
            column2
                PrmCrossReferenceInvoice
                    label is "Invoice"
            column3
                text of "Or"
            column4
                PrmCrossReferenceICBDocument.ICBDocument
                    label is "DocumentNumber"
                    select is AvailableICBDocumentsForCrossReferenceRel.AvailableCrossReferenceICBDocumentList
        paragraph
            PrmCrossReferenceAmountToApply
                label is "AmountToApply"
            PrmCrossReferenceAutoApply
                label is "AutoApply"
