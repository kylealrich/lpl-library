VehicleGroupForm is a CompositeForm
    is primary
    title is "VehicleGroup"
    context form is VehicleGroupContextForm
        background is theme color
    VehicleGroupSchedule is a Panel
        form is VehicleGroupSchedule
    AssignedVehicles is a Panel
        list is AssignedVehicleRel.AssignedVehicleList
            helper list is UnassignedVehicleRel.VehicleHelperList
                action is AssignVehicle
