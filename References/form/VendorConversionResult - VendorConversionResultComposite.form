VendorConversionResultComposite is a CompositeForm
    is primary
    title is "VendorInterfaceResult"
    context form is VendorConversionResultContext
        background is theme color
    Vendors is a Panel
        visible when (VendorInterfaceType.Vendors)
        list is VendorRel.VendorImportList
    VendorLocations is a Panel
        visible when (VendorInterfaceType.Vendors)
        list is VendorLocationRel.VendorLocationImportList
    VendorBalances is a Panel
        visible when (VendorBalanceRelExists)
        list is VendorBalanceRel.VendorBalanceResultList
    VendorAddresses is a Panel
        visible when (VendorAddressRelExists)
        list is VendorAddressRel.VendorAddressResultList
    PendingVendors is a Panel
        visible when (VendorImportRelExists)
        list is VendorImportRel.VendorImportList
    PendingBalances is a Panel
        visible when (VendorBalanceImportRelExists)
        list is VendorBalanceImportRel.VendorBalanceImportOnlyList
    PendingAddresses is a Panel
        visible when (VendorAddressImportRelExists)
        list is VendorAddressImportRel.VendorAddressImportOnlyList
