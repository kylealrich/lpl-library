CashForecastCurrencyForm is a Form
    use for action UpdateCurrencyExchangeRates
    title is "CashForecastAccountCurrencies"
    Layout
        paragraph
            CashForecastAccount.CashManagementAccount
                label is "Account"
                display as text
            CashForecastAccount.CashManagementAccount.Title
                no label
                display as text
            CashForecastAccount.CashManagementAccount.Currency
                label is "Currency"
                display as text
        blank line
        visible when (AccountCurrencyNotCorporateBaseCurrency)
            header3 of "ExchangeRateToCorporateBaseCurrency<CashManagementGroup.Currency>"
            paragraph
                CorporateBaseCurrencyRate
        blank line
        visible when (AccountCurrencyNotLocationCurrency)
            header3 of "ExchangeRateToLocation:<CashForecastAccount.CashManagementAccount.CashManagementLocation>Currency<CashForecastAccount.CashManagementAccount.CashManagementLocation.Currency>"
            paragraph
                LocationCurrencyRate
