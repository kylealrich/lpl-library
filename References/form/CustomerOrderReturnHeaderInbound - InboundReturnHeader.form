InboundReturnHeader is a Form
    Layout
        two column distributed
            UserId
                label is "UserI\\D"
            PostingDate
        two column distributed
            paragraph
                CustomerBillTo
                    label is "BillTo"
                CustomerBillTo.Name
                    no label
                    display as text
            ReasonCode
        two column distributed
            Dropship
                label is "DropshipFlag"
            paragraph
                OrigInvcPre
                    label is "OriginalInvoicePrefix"
                OrigInvcNbr
                    label is "Number"
        two column distributed
            CustReqAct
                label is "CustomerRequiredAccount"
            paragraph
                BillingInvoiceType
                    label is "InvoiceType"
                BillingInvoiceType.Description
                    no label
                    display as text
        two column distributed
            WarehouseShipment
                label is "ShipmentNumber"
            Contact
        two column distributed
            PhoneNumber
            RestkChgCode
                label is "RestockChargeCode"
        two column distributed
            paragraph
                BillingProcessLevel
                    label is "ProcessLevel"
                BillingProcessLevel.Name
                    no label
                    display as text
            paragraph
                Currency
                    label is "CurrencyCode"
                Currency.Description
                    no label
                    display as text
        two column distributed
            LastLineNbr
                label is "LastLineNumber"
            LastSeq
                label is "LastSequence"
        two column distributed
            LastCommSeq
                label is "LastCommentSequence"
            UndItemCnt
                label is "UndecidedItemCount"
        two column distributed
            UndCmCnt
                label is "UndecidedCommentCount"
            CmCnt
                label is "CommentCount"
        two column distributed
            EpType
                label is "ElectronicPaymentType"
            EpExpDate
                label is "ElectronicPaymentExpirationDate"
        two column distributed
            EpAccount
                label is "ElectronicPaymentAccount"
            RecordError
