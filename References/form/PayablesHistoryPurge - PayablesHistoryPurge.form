PayablesHistoryPurge is an ActionForm
    action is PayablesHistoryPurge
    Layout
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        blank line
        header2 of "InvoicePurge"
        two column
            PurgeInvoices
            ArchiveInvoices
                label is "ArchiveIfPurging"
        paragraph
            FrVendorInv
                label is "FromVendor"
                select is AllVendorName
            ToVendorInv
                label is "ToVendor"
                select is AllVendorName
        single column
            InvCutoffDate
                label is "CutoffDate"
        header2 of "VendorPurge"
        two column
            PurgeOnetimeVens
                label is "PurgeOne\\-\\TimeVendors"
            ArchiveOnetimeVens
                label is "ArchiveIfPurging"
        single column
            VenCutoffDate
                label is "CutoffDate"
        blank line
        two column
            PurgeNoActVens
                label is "PurgeNoActivityVendors"
            ArchiveNoActVens
                label is "ArchiveIfPurging"
        paragraph
            NoActBegCreateDte
                label is "BeginCreateDate"
            NoActEndCreateDte
                label is "EndCreateDate"
        paragraph
            FrVendor
                label is "FromVendor"
                select is AllVendorName
            ToVendor
                label is "ToVendor"
                select is AllVendorName
        blank line
        two column
            PurgeVenComments
                label is "PurgeVendorComments"
            ArchiveVenComments
                label is "ArchiveIfPurging"
        single column
            ApcCutoffDate
                label is "CutoffDate"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                PayablesHistoryPurgeReport_RDG
            column2 of 25%
                PayablesHistoryPurgeReport_XPT
