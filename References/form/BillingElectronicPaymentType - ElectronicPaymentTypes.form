ElectronicPaymentTypes is a Form
    is primary
    Layout
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        two column
            BillingElectronicPaymentType
                label is "ElectronicPaymentType"
                select is CompanyElectronicPaymentTypes
                show up to 15 characters
            ElectronicPaymentService
                label is "ElectronicPaymentService"
            MinimumMargin
                label is "ElectonicPaymentMinimumMargin"
            Description
                label is "Description"
            ElectronicPaymentServiceDescription
                label is "Description"
            MinimumMarginPercent
                label is "MarginPercent"
        paragraph
            Active
        paragraph
            AuthorizationDuration
                label is "AuthorizationDuration"
        paragraph
            RequiresAuthorization
        paragraph
            PendingAuthorizationHoldCode
            PendingAuthorizationHoldCode.Description
                no label
                display as text
        paragraph
            FailedAuthorizationHoldCode
            FailedAuthorizationHoldCode.Description
                no label
                display as text
        paragraph
            RequiresSettlement
        paragraph
            PendingSettlementHoldCode
            PendingSettlementHoldCode.Description
                no label
                display as text
        paragraph
            FailedSettlementHoldCode
            FailedSettlementHoldCode.Description
                no label
                display as text
        paragraph
            FailedSettlementAuthorizedHoldCode
                label is "FailedSettle\\,\\AuthorizedHoldCode"
            FailedSettlementAuthorizedHoldCode.Description
                no label
                display as text
        blank line
        paragraph
            MerchantNumber
                label is "ServiceAssignedMerchantNumber"
            MerchantAccount
        paragraph
            MerchantName
                label is "ServiceAssignedMerchantName"
        paragraph
            MerchantConfigurationFile
        blank line
        paragraph
            ReceivableGeneralLedgerCode
                label is "AccountsReceivableCode"
                select is ARCodes
            ReceivableGeneralLedgerCode.Description
                no label
                display as text
        paragraph
            ReceivableGeneralLedgerCode.DistributionAccount
                display only
                compact format
