InterfaceTransaction is a CompositeForm
    use for action CreateInterfaceInvoice
    title is "InterfaceInvoices"
    context form is InterfaceTransactionContext
        background is theme color
    Main is a Panel
        form is Main
    CurrencyRates is a Panel
        form is CurrencyRates
    UserFields is a Panel
        form is ARUserFields
    Global is a Panel
        form is Global
    LineItems is a Panel
        list is AlternateReceivableInvoiceDetailImportRel.LineItems
    Distributions is a Panel
        list is ReceivableInvoiceDistributionImportRel.Distributions
    Comments is a Panel
        list is ReceivableInvoiceCommentImportRel.Comments
