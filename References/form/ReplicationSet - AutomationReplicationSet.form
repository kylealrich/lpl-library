AutomationReplicationSet is a CompositeForm
    context form is AutomationReplicationSetContext
    BusinessClasses is a Panel
        list is RepSetBC set.AutomationReplicationClasses
    History is a Panel
        list is ReplicationSetHistory set.ReplicationHistoryList
    DataLakeQueue is a Panel
        visible when (ShowAutomationDataLakeQueue)
        list is DataLakeQueueRel.ReplicationSetList
