CashLedgerTransactionSearch is a SearchForm
    Layout
        five column distributed
            Company.OrigCompany
                select is Company
            CashCode
            BankTransactionCode
            Status
            CashLedgerTransactionIdentifier
                label is "TransactionIdentifier"
        five column distributed
            SourceCode
                label is "SystemCode"
            GeneralLedgerPostDate
            TransactionNumberRange.FromTransNbr
                label is "FromTransactionNumber"
                mouse over text is "Transaction_NumberRangeInAlphabeticSortOrder"
            TransactionNumberRange.ToTransNbr
                label is "ToTransactionNumber"
            DistribUserFieldsSearch
                label is "DistributionUserFieldSearch"
        header2 of "Additional"
            collapsible
                initially collapsed
                use show more or less affordance
            five column distributed
                IssueDateRange.Begin
                    label is "FromIssueDate"
                IssueDateRange.End
                    label is "ToIssueDate"
                IssuedBankAmountRange.FromAmount
                    label is "FromIssuedBankAmount"
                IssuedBankAmountRange.ToAmount
                    label is "ToIssuedBankAmount"
