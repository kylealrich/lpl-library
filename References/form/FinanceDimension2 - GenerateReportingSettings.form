GenerateReportingSettings is an ActionForm
    action is GenerateReportingSettings
    title is "GenerateFederalFinancialReport"
    Layout
        single column
            PrmDescription
                label is "ReportDescription"
            PrmReportType
                label is "ReportType"
            PrmBasis
                label is "Basis"
            PrmReportingThruDate
                label is "ReportingThruDate"
            PrmUseIDM
            visible when (PrmUseIDM)
                PrmIDMTemplate
                    select is GrantFederalReportingIDMTemplate
            visible when (not PrmUseIDM)
                PrmGrantFederalReportFormat
