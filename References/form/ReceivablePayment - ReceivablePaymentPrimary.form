ReceivablePaymentPrimary is a Form
    title is "TransferredReceivablePayment<PaymentNumber>"
    Layout
        display as text
            two column distributed
                column1
                    Company
                    button of "<ReceivablePaymentHeader>"
                        link is BatchNav
                        label is "Batch"
                        display as link
                    TransactionAmount
                    BankEntity
                    DerivedStatus
                        label is "Status"
                        display only
                column2
                    Customer
                        select is CustomerContextCompanyCustomerSelectList
                    PaymentNumber
                    ReferenceNumber
                    BankAccountNumber
