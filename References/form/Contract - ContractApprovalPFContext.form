ContractApprovalPFContext is a Form
    title is "Contract<ContractNbr>Review"
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        display as text
            paragraph
                ContractGroup
                Contract
                Name
                    no label
                Status
                WorkingContractID
            paragraph
                Supplier
                Supplier.SupplierName
                    no label
                    display as text
                SupplierSourceId
                    label is "SupplierContact"
                SupplierSourceId.MainContact.FirstAndLastName
                    no label
        single column
            button of "ViewEntireContract"
                link is ContractPrint
