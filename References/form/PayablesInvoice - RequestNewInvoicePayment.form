RequestNewInvoicePayment is an ActionForm
    action is RequestNewInvoicePayment
    title is "RequestInvoicePayment"
    Layout
        visible when (not actor.initiator)
            display as text
                three column distributed
                    column1
                        RequestedBy
                        NeededBy
                    column2
                        button of "<RequesterEmail>"
                            link is "mailto:<RequesterEmail>"
                            label is "EmailAddress"
                            display as link
                        RequestPriority
                            label is "Priority"
                            display as tag
                                color of red when (RequestPriority.High)
                                color of yellow when (RequestPriority.Medium)
                                color of green when (RequestPriority.Low)
                    column3
                        button of "<RequesterPhone.SubscriberNumber>"
                            link is "tel:<RequesterPhone.SubscriberNumber>"
                            label is "PhoneNumber"
                            display as link
            two column distributed
                ApproverNotes
                    label is "MessageToRequester"
                Notes
                    label is "MessageFromRequester"
                    display as text
        visible when (actor.initiator)
            text of "Request_Information"
                header3
            text of "This_request_will_be_routed_for_approval;_after_it_is_approved_an_invoice_will_be_created."
                header4
            three column distributed
                column1
                    RequestPriority
                        label is "Priority"
                    NeededBy
                column2
                    Notes
                        label is "MessageToApprover"
                column3
                    visible when (ApproverNotes entered)
                        ApproverNotes
                            label is "MessageFromApprover"
                            display as text
        header3 of "InvoiceInformation"
        two column distributed
            paragraph
                Company
                    show up to 10 characters
                Company.Name
                    no label
                    display as text
            paragraph
                ProcessLevel
                    show up to 10 characters
                ProcessLevel.Name
                    no label
                    display as text
        two column distributed
            paragraph
                Vendor
                    select is InvoiceVendorSearch
                    show up to 10 characters
                Vendor.VendorName
                    no label
                    display as text
            visible when (Vendor.CurrentAddressExists)
                paragraph
                    Vendor.VendorAddressLine1
                        label is "VendorAddress"
                        display as text
                    Vendor.VendorAddressLine2
                        no label
                        display as text
                    Vendor.CityStateZIPDisplay
                        no label
                        display as text
        two column distributed
            RemitToCode
                select is APVendorRemitLocationsSelect
                show up to 8 characters
            visible when (RemitToCode entered)
                paragraph
                    RemitToCode.VendorAddressLine1
                        label is "RemitAddress"
                        display as text
                    RemitToCode.VendorAddressLine2
                        no label
                        display as text
                    RemitToCode.CityStateZIPDisplay
                        no label
                        display as text
        two column distributed
            Invoice
                show up to 8 characters
            InvoiceTypeForExpense
                label is "InvoiceType"
        two column distributed
            PrmInvoiceAmount
            InvoiceCurrency
                label is "Currency"
        two column distributed
            InvoiceDate
            ReceiptOfInvoiceDate
        two column distributed
            DueDate
            PrmDistributionDate
        two column distributed
            paragraph
                TermsCode
                TermsCode.Desc1
                    no label
                    display as text
        two column distributed
            paragraph
                TaxCode
                    show up to 10 characters
                TaxCode.Description
                    no label
                    display as text
            TaxableAmount
        two column distributed
            TaxAmount
            visible when (not InvoiceTypeForExpense.Prepayment)
                LinkTaxOnDistributions
        two column distributed
            Description
            Attachment.File
                label is "Attachment"
        header3 of "Payment"
        two column distributed
            column1
                paragraph
                    BankTransactionCode
                    BankTransactionCode.Description
                        no label
                        display as text
                OverrideSeparatePayment
                    label is "SeparatePayment"
            column2
                paragraph
                    CashCode
                        show up to 10 characters
                    CashCode.Description
                        no label
                        display as text
                OverridePayImmediately
                    label is "PayImmediately"
        visible when (not InvoiceTypeForExpense.Prepayment)
            header3 of "Distributions"
            paragraph
                DistributionCode
                DistributionCode.Description
                    no label
                    display as text
            paragraph
                DistributionCodeAccount
                    compact format
            text of "Distribution_Line1"
                header3
            paragraph
                DistributionArray.SpeedDistribution[1].InternationalAmount
                    label is "Amount"
                DistributionArray.SpeedDistribution[1].TaxIndicator
                    label is "TaxIndicator"
                DistributionArray.SpeedDistribution[1].UnitAmount
                    label is "UnitAmount"
            single column
                DistributionArray.SpeedDistribution[1].FinanceCodeBlock
                    label is "FinanceStructure"
                    compact format
                PrmShowLine2
                    label is "AddLine2"
            visible when (PrmShowLine2)
