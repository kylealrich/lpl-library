AllocationControl is a CompositeForm
    is primary
    title is "True\\CostAllocationControl<TrueCostAllocationControl>"
    context form is Header
        background is theme color
    Main is a Panel
        form is Main
    Allocations is a Panel
        list is TrueCostAllocation set.primary
    AllocationLines is a Panel
        list is TrueCostAllocationLine set.AllocationList
    JournalizeLog is a Panel
        title is "JournalizeLogs"
        visible when (JournalizedRecordsExist)
        list is AllocationJournalizeLogRel.TrueCostAllocationJournalizeLog
    Runs is a Panel
        list is RunsRel.TrueCostAllocationControlList
    SourceValidationReport is a Panel
        visible when (SourceValidationRunDate entered)
        list is SourceValidationReportRel.SourceTotalPercentageList
    RunParameter is a Panel
        title is "RunParameters"
        list is TrueCostRunParameter set.TrueCostRunParameterList
    PeriodCostRuns is a Panel
        list is PeriodCostRunsRel.PeriodCostRunsDashBoard
