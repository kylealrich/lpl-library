PerformEFTExtractReportOnly is an ActionForm
    action is EFTExtractReportOnly
    title is "EFTExtractReportOnly"
    Layout
        paragraph
            FinanceEnterpriseGroup
            FinanceEnterpriseGroup.EnterpriseGroup.Description
                no label
                display as text
        paragraph
            CustomerGroup
                select is CustomerGroups
            CustomerGroup.Name
                no label
                display as text
        single column
            text of "Or"
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        single column
            ProcessDate
                label is "ProcessingDate"
        paragraph
            FromOverrideDate
            ToOverrideDate
        paragraph
            BankTransactionCode
                label is "PaymentCode"
                select is CashPaymentAndOriginReceivablesList
            BankTransactionCode.Description
                no label
                display as text
        paragraph
            TermsCode
                label is "PaymentTerms"
            TermsCode.Description.Desc[1]
                no label
                display as text
        single column
            UpdateRecords
                label is "Update"
