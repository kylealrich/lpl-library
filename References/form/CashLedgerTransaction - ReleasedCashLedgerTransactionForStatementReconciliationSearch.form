ReleasedCashLedgerTransactionForStatementReconciliationSearch is a SearchForm
    Layout
        three column
            Company.OrigCompany
                select is Company
            SourceCode
                label is "SystemCode"
            Status
            IssueDateRange.Begin
                label is "IssueDateRangeFrom"
            IssueDateRange.End
                label is "IssueDateRangeTo"
        paragraph
            BankTransactionCode
                label is "TransactionCode"
            AppliedTransactionsFlag
                label is "AppliedTransactions"
            IssuedBankAmountRange.FromAmount
                label is "IssuedBankAmountRangeFrom"
            IssuedBankAmountRange.ToAmount
                label is "IssuedBankAmountRangeTo"
