QualifierContractAndItemForm is a CompositeForm
    title is "QualifierInformation"
    context form is ContractContext
        background is theme color
    restrict action Draft.SupplierCreateProposedContract
    restrict action Draft.RequesterCreateProposedContract
    restrict action Draft.CreateContractProposalTemplate
    restrict action Draft.CreateContractFromTemplate
    Actions
        action is PerformTierCalculations
            label is "CalculateForContract"
        navigation is ViewContract
            label is "ViewContract"
    QualifierContracts is a Panel
        list is TierQualifierContractsRel.AttachedTierQualifierContracts
    AllQualifierItems is a Panel
        visible when (HasPercentTierQualifier)
        list is TierQualifierItemsRel.AllTierQualifierItems
    QualifiedItems is a Panel
        list is TierQualifierItemsRel.QualifiedTierQualifierItems
    CompetitiveItems is a Panel
        visible when (HasPercentTierQualifier)
        list is TierQualifierItemsRel.CompetitiveTierQualifierItems
    Qualifiers is a Panel
        list is TierQualifierRel.ContractTierQualifiersForQualifierInformation
    QualifierCodes is a Panel
        visible when (HasTierQualifierCodes)
        list is TierQualifierCodesRel.ContractTierQualifierCodes
    CalculatedAmounts is a Panel
        visible when (HasCalculations)
        list is TierQualifierCalculationsRel.TierQualifierCalculationsForAContract
