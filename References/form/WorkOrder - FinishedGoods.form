FinishedGoods is a Form
    Layout
        visible when (not RecordExists)
            single column
                paragraph
                    Company
                        select is CompaniesWorkOrderInUse
                    Company.Name
                        no label
                        display as text
                paragraph
                    InventoryLocation
                        label is "Location"
                        select is ProductionLocationForWO
                    InventoryLocation.Name
                        no label
                        display as text
                paragraph
                    visible when (not IsWorkOrderExists)
                        Item
                            select is FinishedGoodsForWO
                        Item.Description
                            no label
                            display as text
        single column
            ReversedByWorkOrder
        three column
            column1
                visible when (not Status.InProduction)
                    paragraph
                        StartDate
                        EndDate
                        DateNeeded
                visible when (Status.InProduction)
                    display as text
                        paragraph
                            StartDate
                            EndDate
                paragraph
                    PlanQuantity
                visible when (not Status.InProduction)
                    HoldCode
                        display as text
                visible when (Status.InProduction)
                    ActualQuantity
                    ShrinkQuantity
                    ConfirmationMessage
                        no label
                        display as text
            column2
                visible when (IsOffsiteOrReturned)
                    header3 of "OffSiteDetails"
                    display as text
                        HoldCode.RepresentativeText
                            label is "HoldCode"
                        Vendor.RepresentativeText
                        OffsiteDate
                            label is "DateSent"
                        ReturnedDate
            column3
                visible when (Status.InProduction)
                    paragraph
                        ActualQuantity
                            label is "QuantityNeeded"
                            display as text
                        Item.StockUOM
                            no label
                            display as text
                    QuantityAssigned
                        label is "QuantityAssigned"
                        display as text
        visible when (not Status.InProduction)
            paragraph
                ProductionCreditAccount
                    compact format
                BurdenAbsorptionAccount
                    compact format
        header2 of "Costs"
        display as text
            two column table
                column headers
                    "Estimated"
                    "Actual"
                row headers
                    "Component"
                    "Packaging"
                    "Burden"
                    "TotalUnitCost"
                column1
                    EstimatedComponentCost
                        no label
                    EstimatedPackagingCost
                        no label
                    EstimatedBurdenCost
                        no label
                    TotalPlanCost
                        no label
                column2
                    ActualComponentCost
                        no label
                    ActualPackagingCost
                        no label
                    ActualBurdenCost
                        no label
                    TotalActualCost
                        no label
