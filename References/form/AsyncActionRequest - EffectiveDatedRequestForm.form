EffectiveDatedRequestForm is a Form
    title is "EffectiveDatedRequest"
    Layout
        two column
            Name
            SystemRequest
        visible when (InvokedFromExists)
            two column
                button of "InvokedFromRequest<InvokedFrom.Name>"
                    link is ViewInvokedFrom
                    display as link
        visible when (WaitForExists)
            two column
                StatusMessage
                    label is "Status"
                visible when (WaitForSingletonActionGroupExists)
                    button of "DependentOnActionGroup<WaitForName>"
                        link is ViewDependsOnActionGroup
                        display as link
                visible when (WaitForSingletonRequestExists)
                    button of "DependentOnRequest<WaitForName>"
                        link is ViewDependsOnRequest
                        display as link
                visible when (WaitForSingletonBackgroundGroupExists)
                    button of "DependentOnBackgroundGroup<WaitForName>"
                        link is ViewDependsOnBackgroundGroup
                        display as link
                visible when (WaitForMultipleActionGroupsExists)
                    button of "DependentOnActionGroup<WaitForName>"
                        link is ViewDependsOnActionGroupList
                        display as link
                visible when (WaitForMultipleBackgroundGroupsExists)
                    button of "DependentOnBackgroundGroup<WaitForName>"
                        link is ViewDependsOnBackgroundGroupList
                        display as link
        header2 of "Action"
        three column
            DataArea
                display only
            IsAsyncActive
                label is "Active"
                raise red alert when (not IsAsyncActive)
                    mouse over text is untranslatable:"<SuspendedMessage>"
        two column
            Actor
                label is "Actor"
                display only
            IsProxy
                display only
        two column
            AuthenticatedActor
                display only
            DBSessionDebugIsEnabled
                label is "LogDBSessionDebug"
                display only
        two column
            column1
                visible when (EmailAddress entered)
                    EmailAddress
                        raise yellow alert when (true)
                            mouse over text is "ThisFieldHasBeenDeprecated.PleaseUseTheOtherEmailAddressField"
                EmailAddressMultiple
                    use text area of 1 lines
            column2
                NotificationType
        header2 of "Scheduling"
        paragraph
            CreatedStamp
                label is "Created"
                display only
            PendingScheduling
            IsScheduled
            valid when (not DisableConcurrency)
                ScheduleConcurrency
                    label is "Concurrency"
        paragraph
            TimeToExec
                label is "TimeToExecute"
            LastTimeToExec
                label is "LatestTimeToExecute"
            visible when (IsScheduled)
                NextScheduledExecution1
                    no label
                NextScheduledExecution2
                    no label
                NextScheduledExecution3
                    no label
            text of untranslatable:"<ContextTimeZoneDisplayName>"
        visible when (DisplayExclusionsUpdate)
            paragraph
                ScheduleExclusion.ScheduleExclusion
                    select is ScheduleExclusionsList
                ScheduleExclusion.ScheduleExclusion.Description
                    no label
                text of "or"
                ScheduleExclusion.ScheduleExclusionSet
                ScheduleExclusion.ScheduleExclusionSet.Description
                    no label
