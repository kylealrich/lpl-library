GLReportElement is a Form
    is primary
    title is "SubReport"
    Layout
        single column
            GLReport
                label is "CompositeReport"
            SubReport
                select is GLCompositeReportList
            visible when (not GLReport.SuppressRowHeader)
                SuppressRowHeader
            visible when (GLReport.SuppressRowHeader)
                SuppressRowHeader
                    display only
            visible when (not GLReport.SuppressColumnHeader)
                SuppressColumnHeaders
                    label is "SuppressColumnHeader"
            visible when (GLReport.SuppressColumnHeader)
                SuppressColumnHeaders
                    label is "SuppressColumnHeader"
                    display only
            ExtendedHeader
                label is "SubReportHeader"
            AddBlankRow
                label is "InsertBlankRowAfterSubReport"
