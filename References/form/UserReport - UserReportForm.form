UserReportForm is a Form
    is primary
    title is "<ReportDocument.FormTitle>"
    restrict action PopulateExpirationDates
        when (not AnyBlankExpiration)
    Layout
        single column
            Name
                display only
            create stamp
                label is "Created"
                display only
            ExpirationDate
            MimeType
                display only
            ReportDocument.SourceType
                display only
            visible when (ReportDocument.UsesSourceKey1)
                SourceKey1
                    label is ReportDocument.SourceKey1Label
                    display only
            visible when (ReportDocument.UsesSourceKey2)
                SourceKey2
                    label is ReportDocument.SourceKey2Label
                    display only
            visible when (ReportDocument.UsesSourceKey3)
                SourceKey3
                    label is "KeyHash"
                    display only
            Public
            button of "View"
                visible when (ReportDocument.HasDocument)
                view ReportDocument.Document
