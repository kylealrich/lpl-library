WarehouseRouteCreation is an ActionForm
    action is WarehouseRouteCreation
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        blank line
        paragraph
            InvLoc
                label is "Location"
                show up to 10 characters
            InvLoc.Name
                no label
                display as text
        text of "_"
        header2 of "DeliveryDates"
        paragraph
            FrExpDelivDt
                label is "From"
            ToExpDelivDt
                label is "To"
        blank line
        paragraph
            RouteGrp
                label is "Route"
                compact format
            AllSpec
                label is "All_\\or_Specific"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                WarehouseRouteCreationReport_RDG
            column2 of 25%
                WarehouseRouteCreationReport_XPT
