DebtInstrument is a CompositeForm
    is primary
    title is "DebtInstrument"
    context form is DebtInstrumentContext
    Detail is a Panel
        form is DebtInstrumentDetail
    Identifiers is a Panel
        list is DebtInstrumentIdentifier set.DebtInstrumentIdentifierList
    Derivatives is a Panel
        visible when (DebtInstrumentType.DebtInstrument)
        list is DebtInstrumentDerivative set.DebtInstrumentDerivativeList
            helper list is DerivativeInstrumentRel.DerivativeHelperList
                action is New.AddDerivative
                    invoked.PrmCashManagementGroup = CashManagementGroup
                    invoked.PrmDebtInstrument = DebtInstrument
    CreditSupport is a Panel
        visible when (not DebtInstrumentType.CreditSupport)
        list is DebtInstrumentCreditSupport set.DebtInstrumentCreditSupportList
            helper list is CreditSupportInstrumentRel.CreditSupportHelperList
                action is New.AddCreditSupport
                    invoked.PrmCashManagementGroup = CashManagementGroup
                    invoked.PrmDebtInstrument = DebtInstrument
    Fees is a Panel
        list is DebtInstrumentFee set.DebtInstrumentFeeList
            helper list is DebtFeeCodeRel.DebtFeeCodeHelperList
                action is CreateFee
                    invoked.PrmCashManagementGroup = CashManagementGroup
                    invoked.PrmDebtInstrument = DebtInstrument
    Counterparties is a Panel
        list is DebtInstrumentCounterparty set.DebtInstrumentCounterpartyList
            helper list is FinancialInstitutionBranchRel.FinancialInstitutionBranchHelperList
                action is CreateCounterparty
                    invoked.CashManagementGroup = CashManagementGroup
                    invoked.DebtInstrument = DebtInstrument
    Ratings is a Panel
        list is DebtInstrumentRating set.DebtInstrumentRatingList
    Documents is a Panel
        list is DebtInstrumentDocument set.DebtInstrumentDocumentList
    Comments is a Panel
        list is DebtInstrumentComment set.DebtInstrumentCommentList
