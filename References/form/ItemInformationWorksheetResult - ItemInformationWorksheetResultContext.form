ItemInformationWorksheetResultContext is a Form
    title is "ItemInformationWorksheetResult"
    Layout
        display as text
            single column
                ItemInformationWorksheetResult
                    label is "ResultID"
            paragraph
                RunGroup
                RunTime
            visible when (NoRecordsAdded)
                text of "NoRecordsWereAdded"
                    italics
            paragraph
                ItemCount
                    label is "ItemsAdded"
                VendorItemCount
                    label is "VendorItemsAdded"
                ItemLocationCount
                    label is "ItemLocationsAdded"
            paragraph
                visible when (ItemGTINsCounted)
                    ItemGTINCount
                        label is "GTINNumbersAdded"
                visible when (ItemSKUsCounted)
                    ItemSKUCount
                        label is "SKUCodesAdded"
                visible when (ItemUPCsCounted)
                    ItemUPCCount
                        label is "UPCCodesAdded"
            paragraph
                visible when (NationalDrugCodesAdded)
                    NationalDrugCodeCount
                        label is "NationalDrugCodesAdded"
                visible when (UCCEANItemUPNCodesAdded)
                    UCCEANItemUPNCount
                        label is "UCCEANItemUPNCodesAdded"
                visible when (HIBCCItemUPNCodesAdded)
                    HIBCCItemUPNCount
                        label is "HIBCCItemUPNCodesAdded"
