CashManagementAccountForRecon is a CompositeForm
    title is "CashManagementAccount"
    context form is CashManagementAccountContext
        background is theme color
    Detail is a Panel
        form is CashManagementAccountDetail
    CashCodes is a Panel
        visible when (HasCashCodes)
        list is CashCodeRel.CashCodeSelectList
    Reconciliation is a Panel
        form is AccountReconciliationInformation
    Import is a Panel
        form is CashManagementAccountImportParameters
    Documents is a Panel
        list is CashManagementAccountDocument set.CashManagementAccountDocumentList
    Comments is a Panel
        list is CashAccountComment set.CashAccountCommentList
