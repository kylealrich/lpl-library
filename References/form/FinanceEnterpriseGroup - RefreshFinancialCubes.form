RefreshFinancialCubes is an ActionForm
    action is RefreshFinancialCubes
    title is "RefreshFinancialCubes"
    Layout
        single column
            text of "SelectTheAnalyticCubesYouWouldLikeToRefresh:"
                header2
                bold
        blank line
        two column
            column1
                UpdateFinanceEnterpriseGroup
                    label is "FinanceEnterpriseGroup"
                UpdateGeneralLedgerTotal
                    label is "GlobalLedgerTotal"
                UpdateDEPMDimensionMap
                    label is "DEPMDimensionMap"
                visible when (actor.context.FinanceEnterpriseGroup.UseAverageDailyBalance)
                    UpdateDailyPeriodTotal
            column2
                paragraph
                    FinanceEnterpriseGroupAnalyticCubeRel.LastRefreshDate
                        display as text
                paragraph
                    GeneralLedgerTotalAnalyticCubeRel.LastRefreshDate
                        display as text
                paragraph
                    DEPMDimensionMapAnalyticCubeRel.LastRefreshDate
                        display as text
                visible when (actor.context.FinanceEnterpriseGroup.UseAverageDailyBalance)
                    paragraph
                        DailyPeriodTotalAnalyticCubeRel.LastRefreshDate
                            display as text
