MySavedSearchesForm is a Form
    disable field options
    Layout
        two column distributed
            column1
                TName
                    no label
                    header1
                    bold
                    display as text
                    raise red alert when (Status.Invalid)
                        mouse over text is "Invalid"
                    raise yellow alert when (Status.Inactive)
                        mouse over text is "Inactive"
                    raise green alert when (Status.Active)
                        mouse over text is "Active"
                paragraph
                    BusinessView
                        label is "BusinessClass"
                        header3
                        display as text
                    visible when (not IsFolder)
                        BusinessList
                            label is "List"
                            header3
                            display as text
                    visible when (IsFolder)
                        UserFolder.Name
                            label is "Report"
                            header3
                            display as text
                    visible when (LibraryItem)
                        Subscribers
                            display as tag
                                color of orange
                paragraph
                    visible when (IsDefault)
                        DefaultLabel
                            no label
                            display as tag
                                color of blue
            column2
                paragraph
                    visible when (ShowSubscriptionTag)
                        SubscribedLabel
                            no label
                            right align
                            display as tag
                                color of green
                    DerivedLastChange
                        no label
                        right align
                        display as tag
                            color of blue
        visible when (UserFolder entered)
            header2 of "ReportDetails"
            three column
                UserFolder.Sharing
                    display as text
                visible when (UserFolder.Sharing.Group)
                    UserFolder.ActorGroup
                        display as text
                visible when (UserFolder.Sharing.Role)
                    UserFolder.Role
                        display as text
                visible when (UserFolder.Sharing.RoleGroup)
                    UserFolder.RoleGroup
                        display as text
                UserFolder.DataSource
                    display as text
        header2 of "Description"
        visible when (TDescription entered)
            TDescription
                display as text
        visible when (TDescription not entered)
            text of "Not_Entered"
                italics
        visible when (CanModify)
            button of "UpdateDescription"
                action is UpdateDescription
                display as link
        visible when (Subscription)
            header2 of "SubscriptionDetails"
            text of "OwnedBy<SubscribedTo.Owner>,Last_ChangedAt<update stamp>"
        header2 of "Actions"
        visible when (not LibraryItem)
            form is PrivateSavedSearchesActionsForm
        visible when (LibraryItem)
            form is PublishedSavedSearchesActionsForm
