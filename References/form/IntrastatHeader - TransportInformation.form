TransportInformation is a Form
    Layout
        two column table
            column1
                paragraph
                    TransportMode
                        label is "Mode_ofTransport"
                paragraph
                    DeliveryTerms
                paragraph
                    StatisticalProcedure
                        label is "StatisticalProcedureCode"
                    StatisticalProcedure.Description
                        no label
                        display as text
            column2
                paragraph
                    DropShipIndicator
                paragraph
                    DeliveryDate
        two column table
            column1
                header2 of "Origination"
                paragraph
                    OriginCountry
                        label is "Country/\\Jurisdiction"
                paragraph
                    OriginRegion
                        label is "Region"
                paragraph
                    LoadingPort
                        label is "Loading"
            column2
                header2 of "Destination"
                paragraph
                    DestinationCountry
                        label is "Country/\\Jurisdiction"
                paragraph
                    DestinationRegion
                        label is "Region"
                paragraph
                    UnloadingPort
                        label is "Unloading"
