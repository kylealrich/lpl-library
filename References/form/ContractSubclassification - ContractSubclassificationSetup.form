ContractSubclassificationSetup is a CompositeForm
    is primary
    title is "ContractSubclassification"
    context form is ContractSubclassificationContext
    Approvals is a Panel
        visible when (BasicApprovals)
        form is ApprovalsWithNoRedlining
    ContractRedlineApprovals is a Panel
        visible when (ContractRedlining)
        form is ApprovalsWithContractRedlining
    DocumentRedlineApprovals is a Panel
        visible when (DocumentRedlining)
        form is ApprovalsWithDocumentRedlining
    SubclassificationDetail is a Panel
        form is ContractSubclassification
