MMatchMessageComposite is a CompositeForm
    title is "MatchMessage"
    context form is MMatchMessageContext
    Actions
        action is ReleasePOCostMessage
            action icon is release
            label is "Release"
            hide when invalid
        action is ReleaseInvoiceMessage
            action icon is release
            label is "Release"
            hide when invalid
        action is previous record
            label is "_"
            hide when invalid
        action is next record
            label is "_"
            hide when invalid
        action is save
            label is "_"
            hide when invalid
    Main is a Panel
        form is MMain
    Comments is a Panel
        list is MatchInvoiceMessageComment set.MMatchInvoiceMessageCommentList
    ReceiptLines is a Panel
        visible when (MatchMessageOrigin.ReconMatch)
        list is MatchIDReceiptLinesRel.MatchInvoiceMessageList
    OtherMessages is a Panel
        visible when (HasCostMessagesForOtherInvoices)
        list is CostMessagesForOtherInvoicesRel.OtherMessages
