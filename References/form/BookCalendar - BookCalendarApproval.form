BookCalendarApproval is a Form
    title is "BookCalendarFor<Book>"
    restrict action SelectBookCalendars
    restrict action ApplyMidQuarterConvention
    restrict action MQProjection
    restrict action ResetAssetBooks
    restrict action ReleaseAssetBooks
    Actions
        navigation is MidQuarterTest
            valid when (IsTaxBookCal)
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is Delete
            label is "_"
        action is refresh
            label is "_"
    Layout
        three column distributed
            column1
                paragraph
                    Book
                    Book.Description
                        no label
                        display as text
                paragraph
                    Company
                    Company.GeneralLedgerCompany.Name
                        no label
                        display as text
                paragraph
                    CalendarStatus
                        label is "Status"
                paragraph
                    DepreciationCalculationStatus
                        label is "ApprovalStatus"
                        display as text
                paragraph
                    YearBeginDate
                        label is "BeginDate"
                visible when (not PriorYearCalendarIsShortYear)
                    paragraph
                        YearEndDate
                            label is "EndDate"
                visible when (PriorYearCalendarIsShortYear)
                    paragraph
                        TransientYearEndDate
                            label is "EndDate"
                    paragraph
                        visible when (CalendarStatus.Current)
                            button of "RebuildCurrentYearEndDateAndBalances"
                                action is RebuildCurrentYearEndDateAndBalances
                            text of "IfYearEndDateChanged;RunRebuildCurrentYearEndDateAndBalances"
                visible when (IsHistory)
                    paragraph
                        ShortYear
                        OriginalYearEndDate
            column2
                paragraph
                    Book.PostCurrency
                        display as text
                paragraph
                    Book.Currency
                        display as text
                paragraph
                    Book.Ledger
                        display as text
                paragraph
                    DepreciationPeriodsPerYear
                        label is "TotalPeriods"
                visible when (not IsShortYearAndFuture)
                    paragraph
                        NumberOfDepreciationPeriodsClosed
                            label is "PeriodsClosed"
            column3
                RequiredBook
                    label is "Required"
                        left of field
                PostingBook
                    label is "PostingBook"
                        left of field
                visible when (not IsAssignedImpairmentNonPostingBook)
                    CloseBook
                visible when (IsCurrent)
                    text of "Current_Period_End_<CurrentPeriodDate>"
                        color of red
                visible when (IsTaxBookCal)
                    button of "MidQuarterAmounts"
                        sub form is MidQuarterAmountsForm
                button of "TaxDetail"
                    link is TaxBookLink
