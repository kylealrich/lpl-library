TransactionsResultsForm is a CompositeForm
    title is "TransactionMassReleaseResult<CashLedgerMassReleaseResult>"
    context form is TransactionsResultsContext
        background is theme color
    Actions
        action is RerunMassReleaseForTransactionErrors
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    Parameters is a Panel
        form is ParametersForm
    TransactionsReleased is a Panel
        title is "Released_(<ReleaseCount>)"
        list is TransactionsReleasedRel.ReleasedCashLedgerTransactionsList
    TransactionsInError is a Panel
        title is "Errors_(<ErrorCount>)"
        visible when (TransactionErrorsExist)
        list is TransactionsWithErrorsRel.MassReleaseTransactionErrorsList
