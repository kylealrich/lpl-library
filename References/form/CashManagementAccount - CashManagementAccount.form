CashManagementAccount is a CompositeForm
    is primary
    title is "CashManagementAccount"
    context form is CashManagementAccountContext
        background is theme color
    Detail is a Panel
        form is CashManagementAccountDetail
    CashCodes is a Panel
        visible when (HasCashCodes)
        list is CashCodeRel.CashCodeSelectList
    Reconciliation is a Panel
        form is AccountReconciliationInformation
    Import is a Panel
        form is CashManagementAccountImportParameters
    Resources is a Panel
        list is CashManagementAccountResource set.CashManagementAccountResourceList
            helper list is FinanceResourceRel.FinanceResourceHelperList
                action is CreateAccountResource
                    invoked.CashManagementGroup = CashManagementGroup
                    invoked.CashManagementAccount = CashManagementAccount
    Lockboxes is a Panel
        list is CashAccountLockbox set.CashAccountLockboxList
    Correspondence is a Panel
        list is CashAccountCorrespondence set.CashAccountCorrespondenceList
    Documents is a Panel
        list is CashManagementAccountDocument set.CashManagementAccountDocumentList
    Comments is a Panel
        list is CashAccountComment set.CashAccountCommentList
    StatementParsing is a Panel
        form is StatementParsing
