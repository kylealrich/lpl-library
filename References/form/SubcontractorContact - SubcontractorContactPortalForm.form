SubcontractorContactPortalForm is a Form
    title is "SubcontractorContact"
    Actions
        action is save
    Layout
        paragraph
            Subcontractor.SubcontractorName
                no label
                display as text
        paragraph
            MainContact.GivenName
                label is "FirstName"
            MainContact.FamilyName
                label is "LastName"
        paragraph
            ContactTitle
        paragraph
            TelephoneNumber
        paragraph
            FaxNumber
        paragraph
            EmailAddress
        header2 of "MailingAddress"
        single column
            LocationAddress
                form is PostalAddressMinimum
