ReceivableProjectTranPanel is a Form
    Layout
        four column distributed
            display as text
                column1
                    OriginalCurrency
                    DiscountDate.DiscDate
                        label is "DiscountDate"
                    CrossReferenceReceivableInvoiceType
                    GeneralLedgerDate
                        label is "GlobalLedgerDate"
                    TransactionUserField1
                    GainLossAmount
                    CurrencyRateToCustomerGroupCurrency
                    AlternateReceivableInvoiceType
                column2
                    CurrencyDecimalsNumber
                    OrigDiscAmts.OrigDiscAmt
                        label is "OriginalDiscountAmount"
                    CrossReferenceReceivableInvoice
                    CancelDate
                    TransactionAmount
                    Currency
                    CustomerGroupCurrencyMultiplicationOrDivision
                    SummarizeDetail
                        label is "SummarizeDetailForDisplay"
                column3
                    CurrencyRate
                    CustomerPurchaseOrder
                    BankTransactionCode
                    DisputeSequence
                    BaseAmount
                    BaseNumberOfDecimals
                    DiscountPercent.DiscPct
                        label is "DiscountPercent"
                column4
                    CurrencyMultiplicationOrDivision
                    TransactionDate
                    BankTransactionType
                    RevalueCurrencyTransactions
                    DueDate
                    AdjustmentIndicator
                    ProjectContractCustomer
