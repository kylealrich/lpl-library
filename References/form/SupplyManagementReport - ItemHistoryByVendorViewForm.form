ItemHistoryByVendorViewForm is a Form
    title is "ItemHistoryByVendor"
    restrict action Delete
    Actions
        action is refresh
            label is "_"
    Layout
        display as text
            header1 of "SupplyManagementReport<SupplyManagementReport>"
            two column distributed
                visible when (HasItemHistoryByVendorReport)
                    paragraph
                        button of "ItemHistoryByVendorReport.pdf" with icon.pdf-file
                            view ListingReport
                            display as link
                visible when (Description entered)
                    Description
                        label is "ReportDescription"
            header1 of "ReportParameters"
            three column distributed
                column1
                    SupplyManagementReportVendorGroupField.VendorGroup
                    visible when (SupplyManagementReportVendorGroupField.Vendor entered)
                        SupplyManagementReportVendorGroupField.Vendor.DerivedVendorNameID
                    visible when (SupplyManagementReportVendorGroupField.Vendor not entered)
                        SupplyManagementReportVendorGroupField.Vendor
                    PayablesCompany.DerivedPayablesCompanyTitle
                        label is "Company"
                    paragraph
                        FromDate
                            label is "FromInvoiceDate"
                        ThruDate
                            label is "ToInvoiceDate"
                column2
                    ItemType
                    Item
                    Item.Description
                    blank line
                column3
                    PrintParameters
                    PrintGraph
                    IncludeInvoiceDetail
                    blank line
