MobileSupplyChainDeliveryTicketForm is a CompositeForm
    is primary
    title is "DeliveryTicket"
    called out actions only
    context form is MobileSupplyChainDeliveryTicketContext
        background is theme color
    Actions
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is refresh
            label is "_"
        action is Print
            action icon is print
            label is "_"
            mouse over text is "PrintForDelivery"
    Lines is a Panel
        list is MobileSupplyChainDeliveryLine set.MobileSupplyChainDeliveryLine
    Events is a Panel
        list is MobileSupplyChainDeliveryEvent set.MobileSupplyChainDeliveryEvent
    Notes is a Panel
        list is MobileSupplyChainDeliveryNoteRel.MobileSupplyChainDeliveryNote
