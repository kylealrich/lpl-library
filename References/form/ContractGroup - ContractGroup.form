ContractGroup is a Form
    title is "ContractGroupSetup"
    Actions
        action is save
            label is "_"
        action is RecalculateTotalLineMaximumAmountToIncludeClosedLines
            hide when invalid
    Layout
        three column
            LastContract
                label is "LastContractNumber"
            DefaultCurrency
            paragraph
            visible when (FinanceEnterpriseGroupAllowsMatrixApproval)
                paragraph
                    AllowResponsibilityMatrixApprovals
                    text of "ForServiceAndStandingContractsOnly"
                        italics
        header2 of "SpecialItemHandling"
        paragraph
            UseManufacturerAndVendorItem
            text of "AConflictExistsIfVendorItemIsAlsoAValidItemMasterItem"
                italics
        paragraph
            UpdateItemFromChangedVendorItem
            UpdateVendorItemFromChangedManufacturerNumber
        header3 of "OtherItemAndAttributeHandling"
        paragraph
            ChangeItemIfDuplicate
        paragraph
            AutoAddUNSPSCCode
            text of "UNSPSCCodesWillBeAddedFromItemWorksheet,AttributeUpdate,AndContractInterface"
                italics
        header2 of "ActiveContractProcessing"
        paragraph
            LeaveContractActive
                label is "WhenANewLineIsAddedToAnActiveContractLeaveContractActive"
            text of "Note:ContractsWithLineCommitmentsWillAlwaysBeSetToAmendmentWhenANewLineIsAdded"
                italics
        two column distributed
            AllowActiveUpdate
            text of "MustAlsoAttachCM_Update_Active_ContractSecurityClassToRoles"
                italics
        visible when (AllowActiveUpdate)
            single column
                text of "DisallowUpdatesToTheFollowingWhenContractIsNotDraft,Amendment,OrAddendum"
            five column distributed
                DisallowDateUpdate
                DisallowMaxAmountUpdate
                DisallowRetainageUpdate
                DisallowSupplierUpdate
                DisallowCommitmentUpdate
        header2 of "Commitments"
        single column
            AllowCommitmentsForActiveContracts
        three column
            IncludeInYearEndCommitmentRollover
            visible when (IncludeInYearEndCommitmentRollover)
                visible when (HasSpreadCommitments)
                    YearEndSpreadCommitmentProcessing
        header2 of "AttributeUpdate"
        two column distributed
            AutoUpdateAttributesFrom
            visible when (StoreConsolidatedItemAndLocation)
                visible when (not AutoUpdateAttributesFrom.NotUsed)
                    UseConsolidatedItemMasterForAttributeUpdate
        header2 of "ContractLocationsAndConsolidatedItemLevelMasterData"
        three column distributed
            EnableClinicalSystemUseLocations
            StoreConsolidatedItemAndLocation
            EnableTaxOverrideLocations
        visible when (EnableClinicalSystemUseLocations)
            paragraph
                DefaultParticipantGroupForContractLocations
                    select is ParticipantSelectForLocations
                LineLevelForContractLocation
            two column distributed
                AutoAddFromParticipantGroup
                AutoAddFromNewLocations
            three column distributed
                DoNotAutoSetToClinical
                DoNotUseRequestingLocations
                DoNotUseParLocations
        header2 of "ContractAlerts"
        two column distributed
            paragraph
                YellowAlert
                    label is "ExpirationYellowAlert"
                text of "DaysBeforeContractExpirationDate"
            paragraph
                RedAlert
                    label is "ExpirationRedAlert"
                text of "DaysBeforeContractExpirationDate"
        header2 of "_"
        two column distributed
            paragraph
                DeliverableYellowAlert
                text of "DaysBeforeContractDeliverableDueDate"
            paragraph
                DeliverableRedAlert
                text of "DaysBeforeContractDeliverableDueDate"
        header2 of "_"
        two column distributed
            paragraph
                MilestoneYellowAlert
                text of "DaysBeforeContractMilestoneDate"
            paragraph
                MilestoneRedAlert
                text of "DaysBeforeContractMilestoneDate"
        paragraph
            DefaultLifetimeEdit
            text of "IfUsingLifetimeMaximumEdits;DefineDefaultEditLevel"
                italics
        header2 of "ContractProposalProcessing"
        three column table
            column headers
                "ContractType"
                "ContractClassification"
                "PrimaryContact"
            row headers
                "Requesters"
                "Suppliers"
                "itemProposals"
            column1
                ProposalFromRequesterDefaultType
                    no label
                ProposalDefaultContractType
                    no label
                ProposalFromItemProposalDefaultType
                    no label
                    select is NonServicePurchaseTypeSelect
            column2
                ProposalFromRequesterDefaultClass
                    no label
                ProposalDefaultContractClassification
                    no label
                ProposalFromItemProposalDefaultClass
                    no label
            column3
                ProposalFromRequesterDefaultContact
                    no label
                ProposalDefaultPrimaryContact
                    no label
                ProposalFromItemProposalDefaultContact
                    no label
        single column
            RequireProposalTemplate
        header2 of "ContractEvaluations"
        single column
            AppendDueDateToContractEvaluation
        header2 of "CatalogInterfaceDefaults"
        two column distributed
            EdiDefaultContractType
                label is "DefaultContractType"
            EdiDefaultContractClassification
                label is "DefaultContractClassification"
        header2 of "GPOProcessing"
        single column
            paragraph
                GPOUsed
                visible when (GPOUsed.HealthTrust2021)
                    UseHealthtrustDeltaFiles
            header3 of "GPODefaults"
            three column distributed
                paragraph
                    DefaultGPOSupplier
                        label is "GPOSupplier"
                        select is GPOSuppliersSelect
                    DefaultGPOSupplier.SupplierName
                        no label
                        display as text
                paragraph
                    DefaultGPOContractType
                        label is "ContractType"
                    DefaultGPOContractType.Description
                        no label
                        display as text
                paragraph
                    DefaultGPOContractClassification
                        label is "ContractClassification"
                    DefaultGPOContractClassification
                        no label
                        display as text
            three column distributed
                paragraph
                    DefaultGPOContact
                        label is "PrimaryContact"
                    DefaultGPOContact.FirstLastName
                        no label
                        display as text
                paragraph
                    DefaultGPOPricing
                        label is "PricingCodeAndTitleForDistributorContracts"
                    DefaultGPOPricingTitle
                        no label
                DefaultMemberProcessing
                    label is "MemberProcessing"
            two column
                DefaultGPOMultipleUOMHandling
                    label is "MultipleUOMHandling"
                DefaultGPOClosedLineHandling
                    label is "ClosedLineHandling"
        visible when (GPOIsUsed)
            three column distributed
                DoNotAllowGPOUpdates
                    label is "DoNotAllowManualUpdatesToGPORelatedFields"
                DoNotAutoCreateDistributorLines
                visible when (GPOUsed.HealthTrust2021)
                    UseUNSPSCCode
        header2 of "_"
        paragraph
            IncludeClosedLinesInTotalLineMax
                label is "IncludeClosedContractLinesInContractTotalLineMaximumAmount"
            visible when (IncludeClosedLinesInTotalLineMax)
                visible when (not LineMaxForClosedLinesCalculatedForExistingContracts)
                    text of "Run_Recalculate_Total_Line_Maximum_AmountToIncludeClosedLinesForExistingContracts"
                        italics
                        color of green
        paragraph
            TopContractAmountThreshold
            text of "ContractsWithAnAmountOrderedGreaterThanThisAmountWillDisplayOnContractManagerTopContractsWidget"
        single column
            DocMgmtSystemURL
                label is "ContractURLForDocumentManagement"
