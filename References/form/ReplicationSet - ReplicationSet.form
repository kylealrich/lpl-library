ReplicationSet is a CompositeForm
    is primary
    title is "ReplicationSet<ReplicationSet>"
    context form is ReplicationSetContext
    Actions
        action is save
        action is Create
        action is Replicate
            hide when invalid
        action is AddBusinessClasses
            hide when invalid
        action is ValidateSchemas
            hide when invalid
        action is refresh
        action is previous record
        action is next record
    BusinessClasses is a Panel
        list is RepSetBC set.ReplicationClassDetails
    Details is a Panel
        form is ReplicationDetailsForm
    ProcessingNow is a Panel
        visible when (InProcess)
        list is RootAsyncActionRequestProcessingRel.AsyncActionRequestInProcessReplications
    Upcoming is a Panel
        visible when (ActionGroupPending)
        list is RootAsyncActionRequestPendingRel.AsyncActionRequestPendingReplications
    History is a Panel
        list is ReplicationSetHistory set.ReplicationHistoryList
    IMSQueue is a Panel
        valid when (IMSQueueNewOrFailed)
        visible when (ShowImsQueue)
        list is IONOutboxQueueNewOrFailedRel.IMSReplicationSetList
    DataLakeQueue is a Panel
        valid when (DataLakeQueueRel.IsNewOrFailedDataLakeQueue)
        visible when (ShowDataLakeQueue)
        list is DataLakeQueueRel.DataLakeBatchQueueRSList
    StreamingQueue is a Panel
        valid when (DataLakeQueueRel.IsNewOrFailedStreamingQueue)
        visible when (ShowStreamingQueue)
        list is DataLakeQueueRel.DataLakeStreamingQueueRSList
