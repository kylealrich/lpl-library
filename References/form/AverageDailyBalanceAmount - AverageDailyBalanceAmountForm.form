AverageDailyBalanceAmountForm is a Form
    is primary
    title is "DailyBalance"
    Layout
        five column grid
            column1
                FinanceEnterpriseGroup
                    display as text
                AccountingEntity
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>"
                    display as text
            column2
                visible when (FinanceEnterpriseGroup.IncludeLedgerInAverageDailyBalance)
                    TransactionLedger
                        display as text
                AverageDailyBalanceAmount.GeneralLedgerChartAccount
                    label is "<FinanceEnterpriseGroup.AccountLabel>"
                    display as text
            column3
                AverageDailyBalanceAmount.RunDate
                    label is "Date"
                    display as text
                visible when (FinanceEnterpriseGroup.ADBAccountWithDimension)
                    ADBDimension1
                        label is "<FinanceEnterpriseGroup.ADBDimensions.Dimension1.Label>"
                        display as text
            column4
                blank line
                visible when (FinanceEnterpriseGroup.ADBAccountWithTwoDimensions)
                    ADBDimension2
                        label is "<FinanceEnterpriseGroup.ADBDimensions.Dimension2.Label>"
                        display as text
            column5
                blank line
                visible when (FinanceEnterpriseGroup.ADBSelection.AccountAndThreeDimensions)
                    ADBDimension3
                        label is "<FinanceEnterpriseGroup.ADBDimensions.Dimension3.Label>"
                        display as text
        header2 of "_"
        four column grid
            column1
                visible when (FinanceEnterpriseGroup.CalculateADBOption.AllCurrencyAmounts)
                    TransactionCurrency
                        display as text
                AccountingEntity.FunctionalCurrency
                    display as text
                visible when (FinanceEnterpriseGroup.CalculateADBOption.AllCurrencyAmounts)
                    visible when (AccountingEntity.AlternateCurrency entered)
                        AccountingEntity.AlternateCurrency
                            display as text
                    visible when (AccountingEntity.AlternateCurrency2 entered)
                        AccountingEntity.AlternateCurrency2
                            display as text
                    visible when (AccountingEntity.AlternateCurrency3 entered)
                        AccountingEntity.AlternateCurrency3
                            display as text
                    visible when (FinanceEnterpriseGroup.ReportCurrencyOne entered)
                        FinanceEnterpriseGroup.ReportCurrencyOne
                            display as text
                    visible when (FinanceEnterpriseGroup.ReportCurrencyTwo entered)
                        FinanceEnterpriseGroup.ReportCurrencyTwo
                            display as text
                    visible when (FinanceEnterpriseGroup.ReportCurrencyThree entered)
                        FinanceEnterpriseGroup.ReportCurrencyThree
                            display as text
                    visible when (FinanceEnterpriseGroup.ReportCurrencyFour entered)
                        FinanceEnterpriseGroup.ReportCurrencyFour
                            display as text
                    visible when (FinanceEnterpriseGroup.ReportCurrencyFive entered)
                        FinanceEnterpriseGroup.ReportCurrencyFive
                            display as text
            column2
                visible when (FinanceEnterpriseGroup.CalculateADBOption.AllCurrencyAmounts)
                    TransactionAmount
                        label is "<TransactionCurrency>Amount"
                Amount
                    label is "<AccountingEntity.FunctionalCurrency>Amount"
                visible when (FinanceEnterpriseGroup.CalculateADBOption.AllCurrencyAmounts)
                    visible when (AccountingEntity.AlternateCurrency entered)
                        OnPostDateAlternateAmount1
                            label is "<AccountingEntity.AlternateCurrency>EndingBalanceOnPostDate"
                    visible when (AccountingEntity.AlternateCurrency2 entered)
                        OnPostDateAlternateAmount2
                            label is "<AccountingEntity.AlternateCurrency2>EndingBalanceOnPostDate"
                    visible when (AccountingEntity.AlternateCurrency3 entered)
                        OnPostDateAlternateAmount3
                            label is "<AccountingEntity.AlternateCurrency3>EndingBalanceOnPostDate"
                    visible when (FinanceEnterpriseGroup.ReportCurrencyOne entered)
                        OnPostDateReportAmount1
                            label is "<FinanceEnterpriseGroup.ReportCurrencyOne>EndingBalanceOnPostDate"
                    visible when (FinanceEnterpriseGroup.ReportCurrencyTwo entered)
                        OnPostDateReportAmount2
                            label is "<FinanceEnterpriseGroup.ReportCurrencyTwo>EndingBalanceOnPostDate"
                    visible when (FinanceEnterpriseGroup.ReportCurrencyThree entered)
                        OnPostDateReportAmount3
                            label is "<FinanceEnterpriseGroup.ReportCurrencyThree>EndingBalanceOnPostDate"
                    visible when (FinanceEnterpriseGroup.ReportCurrencyFour entered)
                        OnPostDateReportAmount4
                            label is "<FinanceEnterpriseGroup.ReportCurrencyFour>EndingBalanceOnPostDate"
                    visible when (FinanceEnterpriseGroup.ReportCurrencyFive entered)
                        OnPostDateReportAmount5
                            label is "<FinanceEnterpriseGroup.ReportCurrencyFive>EndingBalanceOnPostDate"
            column3
                visible when (FinanceEnterpriseGroup.CalculateADBOption.AllCurrencyAmounts)
                    OnPostDateTransactionAmount
                        label is "<TransactionCurrency>EndingBalanceOnPostDate"
                BalanceOnPostDate
                    label is "<AccountingEntity.FunctionalCurrency>EndingBalanceOnPostDate"
            column4
                visible when (FinanceEnterpriseGroup.CalculateADBOption.AllCurrencyAmounts)
                    blank line
                BalanceOnTransactionDate
                    label is "<AccountingEntity.FunctionalCurrency>EndingBalanceOnTransactionDate"
