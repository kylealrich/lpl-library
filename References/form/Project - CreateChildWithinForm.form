CreateChildWithinForm is an ActionForm
    action is CreateChildWithin
    title is "CreateChild<actor.context.FinanceEnterpriseGroup.ProjectLabel>For<DisplayProject>"
    Layout
        single column
            visible when (InEnterpriseStructure)
                NewProjectType
                    label is "<FinanceEnterpriseGroup.ProjectLabel>Type"
            visible when (not InEnterpriseStructure)
                NewProjectType
                    label is "<FinanceEnterpriseGroup.ProjectLabel>Type"
                    display only
        three column distributed
            column1
                NewProject
                    label is "<FinanceEnterpriseGroup.ProjectLabel>"
            column2
                NewDescription
                    label is "Description"
            column3
                NewActive
                    label is "Active"
        visible when (InEnterpriseStructure)
            three column distributed
                column1
                    NewWatch
                        label is "Watch"
                column2
                    NewWatchedBy
                        label is "WatchedBy"
                column3
                    NewWatchedBy.Name.PreferredFirstAndLastName
                        no label
                        display as text
        visible when (InEnterpriseStructure)
            three column
                column1
                    NewProjectDateRange.BeginDate
                        label is "BeginDate"
                column2
                    NewProjectDateRange.EndDate
                        label is "EndDate"
                column3
                    paragraph
                        NewAddressCode
                            label is "AddressCode"
                        NewAddressCode.Description
