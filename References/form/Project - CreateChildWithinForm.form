                            display as text
            three column
                column1
                    visible when (NewProjectType.Summary)
                        paragraph
                            NewBudgetIdentifier
                                label is "BudgetIdentifier"
                            NewBudgetIdentifier.Description
                                no label
                                display as text
            four column
                column1
                    visible when (NewProjectType.Posting)
                        paragraph
