                                NewExcludeFromFES
                                    label is "ExcludeFromFrontEndSplit"
            visible when (NewProjectType.Posting)
                single column
                    NewDefaultCodeBlock
                        label is "DefaultFinanceStructure"
                        compact format
        visible when (not InEnterpriseStructure)
            single column
                paragraph
                    NewBudgetIdentifier
                    NewBudgetIdentifier.Description
                        no label
