                    column3
                        NewContractCurrency.Description
                            no label
                            display as text
            three column distributed
                visible when (NewContract)
                    column1
                        NewContractID
                            label is "ContractID"
                            show up to 25 characters
                    column2
                        NewContractType
                            label is "ContractType"
                            show up to 10 characters
                    column3
                        NewContractMaximumFees
                            label is "MaximumFees"
            three column distributed
                visible when (NewContract)
                    column1
                        NewContractBillingMethod
                            label is "BillingMethod"
                            show up to 10 characters
                    column2
                        visible when (NewContractBillingMethod.CostPlus)
                            NewContractMarkupPercent
                                label is "MarkupPercent"
                        visible when (NewContractBillingMethod.TimeAndMaterials)
                            NewContractMarkupPercent
                                label is "MarkupPercent"
                        visible when (NewBillingMethod.RateTable)
                            NewContractMarkupPercent
                                label is "MarkupPercent"
                    column3
