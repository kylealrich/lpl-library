Overrides is a Form
    Layout
        single column
            Fas13Flag
                label is "PerformClassificationTest"
            visible when (not IsMonthToMonthRenewal)
                LeaseClassification
                    label is "Classification"
            visible when (IsMonthToMonthRenewal)
                LeaseClassification
                    label is "Classification"
                    display as text
            visible when (IsOperating)
                visible when (not IsUnreleased)
                    DeferredRentBalance
                        label is "DeferredRentAmountBalance"
                        display as text
                visible when (IsUnreleased)
                    DeferredRentBalance
                        label is "DeferredRentAmountOverride"
            visible when (CanOverrideShortTermLease)
                OverrideShortTermLease
                    label is "ClassifyShortTermLeaseAsALongTermLease"
            visible when (CanOverrideLongTermLease)
                OverrideLongTermLease
                    label is "ClassifyLongTermLeaseAsAShortTermLease"
