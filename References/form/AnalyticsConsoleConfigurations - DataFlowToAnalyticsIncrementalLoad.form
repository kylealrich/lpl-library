DataFlowToAnalyticsIncrementalLoad is a Form
    title is "DataFlowToAnalyticsIncrementalLoad"
    disable all actions
    Layout
        button of "StartDataFlowIncrementalLoad"
            valid when (AnalyticsConsoleConfigurationsRel.IsWorkflowNotInProcess)
            action is StartDataFlowIncrementalLoad
        single column
            visible when (last PostPfiWorkunitsRel.Status.Processing)
                text of "There_is_an_ongoing_process_for_Update_ION_Schemas."
                    bold
                    color of red
            visible when (last DataFlowToAnalyticsFLRel.Status.Processing)
                text of "There_is_an_ongoing_process_for_Data_Flow_To_Analytics_Full_Load."
                    bold
                    color of red
            visible when (last DataFlowToAnalyticsILRel.Status.Processing)
                text of "There_is_an_ongoing_process_for_Data_Flow_To_Analytics_Incremental_Load."
                    bold
                    color of red
        button of "ScheduleDataFlowIncrementalLoad"
            action is ScheduleDataFlowIncrementalLoad
        single column
            last DataFlowToAnalyticsILRel.ExecutionStartDate
                label is "LastUpdate"
            last DataFlowToAnalyticsILRel.Status
                label is "Status"
                raise red alert when (last DataFlowToAnalyticsILRel.IsFailed)
                raise yellow alert when (last DataFlowToAnalyticsILRel.IsCanceled)
                raise blue alert when (last DataFlowToAnalyticsILRel.IsActionPendingOrActionPendingInServer)
                    mouse over text is "ActionPending"
            button of "<last DataFlowToAnalyticsILRel.PfiWorkunit>"
                valid when (AnalyticsConsoleConfigurationsRel.IsIncrementalWorkunitExists)
                link is DataAnalyticsILWorkUnitNav
                label is "WorkUnit"
                display as link
                raise blue alert when (AnalyticsConsoleConfigurationsRel.UserActionAvailableForIncrementalLoad)
                    mouse over text is "There_is_a_pending_user_action_before_the_process_will_proceed._Please_check_your_email_or_go_to_the_workunit_to_perform_the_user_action"
