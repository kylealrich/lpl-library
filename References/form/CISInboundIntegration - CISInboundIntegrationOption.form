CISInboundIntegrationOption is a Form
    title is untranslatable:"\\[\\inbound Integration\\] <IntegrationName>"
    Layout
        valid when (!YellowAlert)
            header4 of untranslatable:"Data Transformation"
            three column
                column1 of 25%
                    InputFormat

                column2 of 25%
                    visible when (InputFormat.NonInfor)
                        TransformationMethod

                column3 of 50%
                visible when (TransformationMethod.JavaScript)
                    FileNameNonInfor

            single column
                MappingTable.File
                    label is untranslatable:"Mapping Table"
                // visible when (TransformationMethod.JavaScript)
                    
                visible when (TransformationMethod.SeparateProcess)
                    paragraph
                        Process
                        text of untranslatable:"Used for complex mapping or using XREF tables"
                            italics
            header4 of untranslatable:"Archiving"
            paragraph
                Archive
                visible when (Archive)
                    ArchiveDirectory
                        label is untranslatable:"Archive File Location"
                    ArchiveFileRename
                        label is untranslatable:"Archive File Rename Method"
            header4 of untranslatable:"Decryption"
            paragraph
                Decrypt
                visible when (Decrypt)
                    DecryptionServiceName
            header4 of untranslatable:"_"
            single column
                ShowDerivedFields
                    display as switch
                visible when (ShowDerivedFields)
                    visible when (TransformationMethod.JavaScript)
                        FormattedFileDirectory
                            display only