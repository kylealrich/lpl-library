ItemInformation is a Form
    Layout
        header2 of "ManufacturerInformation"
        two column
            column1
                Item.Manufacturer
                    label is "CurrentManufacturer"
                    display only
                    compact format
                Item.ManufacturerNumber
                    label is "CurrentManufacturerNumber"
                    display only
            column2
                Manufacturer
                    label is "ReplacementManufacturer"
                    compact format
                ManufacturerNumber
                    label is "ReplacementManufacturerNumber"
        header2 of "Descriptions"
        two column
            column1
                Item.Description
                    label is "CurrentDescription1"
                    display only
                Item.Description2
                    label is "CurrentDescription2"
                    display only
                Item.Description3
                    label is "CurrentLongDescription"
                    display only
                    use text area of 3 lines
            column2
                ReplacedDescription1
                    label is "ReplacementDescription1"
                ReplacedDescription2
                    label is "ReplacementDescription2"
                ReplacedDescription3
                    label is "ReplacementLongDescription"
                    use text area of 3 lines
