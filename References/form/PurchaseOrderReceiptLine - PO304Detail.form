PO304Detail is a Form
    title is "Detail"
    Layout
        paragraph
            ShipToLocation
                label is "ReceivingLocation"
            ShipToLocation.Name
                no label
                display as text
        paragraph
            visible when (IsBinTracked)
                visible when (MultipleBins)
                    MultipleBins
                        display as text
                visible when (not MultipleBins)
                    Bin
                        label is "PreferredOrDefaultedBin"
                BinMismatchMessage
                    no label
                    display as text
            BackOrderExistMessage
                no label
                display as text
        two column
            text of "<SubstituteMessageDisplay>"
                bold
        paragraph
            Description
                display as text
        paragraph
            ItemType
                display as text
            CatchWeightMessage
                no label
                display as text
        paragraph
            PurchaseOrderLine.EnteredBuyUOM
                label is "PurchaseOrderU\\O\\M"
                display as text
        three column
            PurchaseOrderLine.EarlyDeliveryDate
                label is "Deliver"
                display as text
            PurchaseOrderLine.LateDeliveryDate
                no label
                display as text
            BuyerMessageDisplay
                no label
                display as text
        paragraph
            PurchaseOrderLine.ShipToLocation
                label is "Purchase_\\Order_\\Line_\\Location"
                display as text
            PurchaseOrderLine.ShipToLocation.Name
                no label
                display as text
        paragraph
            CancelBackorder
        paragraph
            InspectionRequired
                label is "Inspection"
        paragraph
            GlobalLineType
                label is "LineType"
                display as text
