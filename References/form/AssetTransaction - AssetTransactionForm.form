AssetTransactionForm is a Form
    is primary
    title is "AssetTransaction"
    Layout
        paragraph
            Company
                display as text
            Company.GeneralLedgerCompany.Name
                no label
                display as text
        paragraph
            Asset
                display as text
            Asset.Description
                no label
                display as text
        visible when (Asset.LeasedAsset)
            paragraph
                Asset.AssetLease.LeaseCompany
                    label is "LeaseCompany"
                    display as text
                Asset.AssetLease.LeaseCompany.Name
                    no label
                    display as text
            paragraph
                Asset.AssetLease.Lease
                    display as text
                Asset.AssetLease.Lease.Description
                    no label
                    display as text
        paragraph
            Asset.TagNumber
                display as text
        paragraph
            TransactionDate
                display as text
        paragraph
            PostingDate
                display as text
        paragraph
            Book
            Book.Description
                no label
                display as text
        blank line
        paragraph
            DerivedAssetAccountType
        paragraph
            Description
        paragraph
            Reference
        paragraph
            DistributionAccount
                label is "Account"
                compact format
        paragraph
            TransactionAmount
                label is "TransactionAmount"
            TransientBookCurrency
                label is "TransactionCurrency"
                display as text
