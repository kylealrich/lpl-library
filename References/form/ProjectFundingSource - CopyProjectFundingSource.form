CopyProjectFundingSource is an ActionForm
    action is CopyProjectFundingSource
    title is "Copy<FinanceEnterpriseGroup.ProjectLabel>FundingSource"
    Layout
        header2 of "CopyFrom"
        two column
            ProjectContract
                label is "<FinanceEnterpriseGroup.ProjectLabel>Contract"
                display as text
            ProjectContract.Description
                no label
                display as text
        two column
            ProjectContract.Project.ProjectDateRange.BeginDate
                label is "BeginDate"
                display as text
            ProjectContract.Project.ProjectDateRange.EndDate
                label is "EndDate"
                display as text
        three column
            ProjectContract.RevenueRecognition
                display as text
            ProjectContract.BillingMethod
                display as text
            visible when (ProjectContract.RevenueRecognition.Separate)
                ProjectContract.RevenueMethod
                    display as text
        three column
            FinanceDimension2
                label is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
                display as text
            FinanceDimension2.Description
                no label
                display as text
            FinanceDimension2.FundingType
                display as text
        two column
            FinanceDimension2.DateRange.Begin
                label is "BeginDate"
                display as text
            FinanceDimension2.DateRange.End
                label is "EndDate"
                display as text
        header2 of "CopyTo"
        two column
            NewProjectContract
                label is "<FinanceEnterpriseGroup.ProjectLabel>Contract"
                select is ProjectContractCopySelect
            NewProjectContract.Description
                no label
                display as text
        two column
            NewProjectContract.Project.ProjectDateRange.BeginDate
                label is "BeginDate"
                display as text
            NewProjectContract.Project.ProjectDateRange.EndDate
                label is "EndDate"
                display as text
        three column
            NewProjectContract.RevenueRecognition
                label is "RevenueRecognition"
                display as text
            NewProjectContract.BillingMethod
                label is "BillingMethod"
                display as text
            visible when (NewProjectContract.RevenueRecognition.Separate)
                NewProjectContract.RevenueMethod
                    label is "RevenueMethod"
                    display as text
        three column
            NewFinanceDimension2
                label is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
                select is FinanceDimension2FundingSourceSelect
            NewFinanceDimension2.Description
                no label
                display as text
            NewFinanceDimension2.FundingType
                label is "FundingType"
                display as text
        two column
            NewFinanceDimension2.DateRange.Begin
                label is "BeginDate"
                display as text
            NewFinanceDimension2.DateRange.End
                label is "EndDate"
                display as text
        single column
            NewPriority
                label is "Priority"
            NewPercent
                label is "Percent"
            NewFundedAmount
                label is "FundedAmount"
            NewFundingGroup
                label is "FundingGroup"
            paragraph
                NewBillingGroup
                    label is "BillingGroup"
                NewBillingGroup.Description
                    no label
                    display as text
