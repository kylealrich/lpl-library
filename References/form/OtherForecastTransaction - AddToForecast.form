AddToForecast is an ActionForm
    action is AddToForecast
    title is "AddToForecast"
    Layout
        header2 of "ForecastInformation"
        paragraph
            PrmCashForecast
                label is "CashForecast"
                select is UpdatableCashForecastList
            PrmCashForecast.Description
                no label
                display as text
        paragraph
            PrmCashForecastAccount.CashManagementAccount
                label is "Account"
                select is CashForecastAccountSelect
                display as required when (true)
            PrmCashForecastAccount.CashManagementAccount.Title
                no label
                display as text
        paragraph
            PrmAction
                display as horizontal radio buttons
        header2 of "TransactionOverride"
        paragraph
            PrmCashTransactionCategory
                label is "Category"
                mouse over text is "LeaveFieldBlankToUseThe_\\CategoryDefinedOnThe_\\Cash_\\Forecast_\\Interface"
                select is CashTransactionCategorySelect
            PrmCashTransactionCategory.Description
                no label
                display as text
