PCardMerchant is a CompositeForm
    is primary
    title is "Merchant<Merchant>_-_<Name>inProgram<ProcurementCardProgram>_-_<ProcurementCardProgram.Description>"
    context form is PCardMerchantContext
        background is theme color
    Main is a Panel
        form is Main
    MerchantGroups is a Panel
        list is MerchantGroupAssociationsRel.MerchantGroupsForMerchant
    VendorCrossReference is a Panel
        list is MerchantCrossReference set.MerchantVendorCrossReferences
