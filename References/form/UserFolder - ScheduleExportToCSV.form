ScheduleExportToCSV is an ActionForm
    action is ScheduleExportToCSV
    Layout
        single column
            Format
            Headers
            IncludeKeyContext
            ReportName
        paragraph
            valid when (not AllRecords)
                RecordLimit
            AllRecords
            visible when (Format.Excel)
                text of "MaximumOf<ExcelMaxRows>"
        single column
            RetentionDuration
                mouse over text is "DocumentRetentionDuration,InDays"
        visible when (false)
            BusinessView
            BusinessList
        visible when (ShowCSVExportLevels)
            two column
                column1
                    visible when (IsCSVListTreeOnly)
                        TreeOnlyOptions
                    visible when (IsCSVListTreeAndDetails)
                        TreeAndDetailsOptions
                column2
                    visible when (IsCSVListTreeOnly)
                        valid when (TreeOnlyOptions.SpecifiedMaximumLevelsToPrint)
                            MaxLevels
                    visible when (IsCSVListTreeAndDetails)
                        visible when (TreeAndDetailsOptions.SpecifiedMaximumLevelsToPrint)
                            valid when (TreeAndDetailsOptions.SpecifiedMaximumLevelsToPrint)
                                MaxLevels
                        visible when (TreeAndDetailsOptions.SpecifiedMaximumLevelsToPrintWithDetails)
                            valid when (TreeAndDetailsOptions.SpecifiedMaximumLevelsToPrintWithDetails)
                                MaxLevels
                        visible when (TreeAndDetailsOptions.PrintAllLevels)
                            valid when (false)
                                MaxLevels
                        visible when (TreeAndDetailsOptions.PrintAllLevelsWithDetails)
                            valid when (false)
                                MaxLevels
            visible when (TreeAndDetailsOptions.PrintAllLevelsWithDetails)
                three column
                    valid when (not AllListDetailSections)
                        UserBusinessListDetailSection
                            compact format
                            select is UserBusListDetailListSectionSeqRel.BySequenceFolderSelectList
                    valid when (DetailType.DetailSection)
                        AllListDetailSections
                    DetailPanelOptions
            visible when (TreeAndDetailsOptions.SpecifiedMaximumLevelsToPrintWithDetails)
                three column
                    valid when (not AllListDetailSections)
                        UserBusinessListDetailSection
                            compact format
                            select is UserBusListDetailListSectionSeqRel.BySequenceFolderSelectList
                    valid when (DetailType.DetailSection)
                        AllListDetailSections
                    DetailPanelOptions
        visible when (DisplayDetailsOnlyOptions)
            two column
                DetailsOnlyOptions
                    when value changed
                        DetailType =  blank
                visible when (IsCSVListDetailsAndAuditChanges)
                    visible when (not DetailsOnlyOptions.PrintCurrentListOnly)
                        DetailType
            visible when (DetailsOnlyOptions.PrintCurrentListWithDetails)
                visible when (not DetailType.AuditChanges)
                    visible when (IsCSVListDetailsOrBoth)
                        three column
                            valid when (not AllListDetailSections)
                                UserBusinessListDetailSection
                                    compact format
                                    select is UserBusListDetailListSectionSeqRel.BySequenceFolderSelectList
                            valid when (DetailType.DetailSection)
                                AllListDetailSections
                            DetailPanelOptions
        paragraph
            visible when (DisplayUserListViewSelect)
                ParamUserListView
                    select is UserListViewRel.UserListFolderActiveSelectList
                ParamUserListView.Name
        single column
            ReportDistributionGroup
            visible when (ReportDistributionGroup.HasDynamicActorReportDistribution)
                DynamicDistributionActors
            visible when (IsListReport)
                valid when (ReportDistributionGroup not entered)
                    EmptyReportOption
        visible when (Format.CSV)
            visible when (MultiThreadingIsValid)
                paragraph
                    MultiThreadField
                        select is MultiThreadFieldsListCfgUID
                    NumberOfThreads
        paragraph
            visible when (false)
                SessionKeys
