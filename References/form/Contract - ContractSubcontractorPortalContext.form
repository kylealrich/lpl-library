ContractSubcontractorPortalContext is a Form
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        paragraph
            Name
                label is "ContractName"
                display as text
        two column
            ProposedTotalContractAmount
                label is "ProposedTotalAmount"
                display as text
            visible when (SubcontractorContractAndDiversity)
                paragraph
                    DiversityPctOfProposedTotal
                        label is "SubcontractorDiversityPercentOfProposedTotal"
                        display as text
                    button of "Overview"
                        visible when (HasSubcontractors)
                        link is DiversityBySubcontractorsPortal
                        display as link
        single column
            visible when (false)
                Contract
