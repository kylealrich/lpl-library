ItemLocationDetailForParItem is a Form
    Layout
        header2 of "ParItem"
        two column distributed
            column1
                BinSequence
                TransientShelfLocation
                    label is "ShelfLocation"
                ReorderPoint
                    label is "ParLevel"
                DefaultTransactionUOM
                    raise yellow alert when (InvalidOnLocationItemUOM)
                        mouse over text is "<DerivedInvalidUOMText>"
                GeneralLedgerCategory
                    label is "InventoryLocationAccountGroup"
                paragraph
                    PatientChargeable
                    NoCharge
                    Active
                visible when (false)
                    TransientOnline
                    Vendor
                    PurchaseFromLocation
                    ReplenishFrom
            column2
                InventoryLocation.OneSourceDocumentToOnePO
                    label is "SingleDocumentToAPurchaseOrder"
                    display as text
                MinimumOrderQuantity
                OrderMultiple
                SuggestedReorderPoint
                    label is "SuggestedParLevel"
                LeadtimeDays
                paragraph
                    StandardCost
                    Company.GeneralLedgerCompany.Currency
                        no label
                        display as text
                paragraph
                    Manufacturer
                        compact format
                    Manufacturer.Description
                        no label
                        display as text
                ManufacturerNumber
