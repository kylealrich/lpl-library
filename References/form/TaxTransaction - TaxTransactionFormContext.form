TaxTransactionFormContext is a Form
    Layout
        four column
            paragraph
                TaxEntity
                TaxEntity.Name
                    no label
                    display as text
            paragraph
                TaxCode
                    select is TaxCodeFilteredList
                TaxCode.Description
                    no label
                    display as text
            TaxTransaction
                label is "TransactionNumber"
                select is TaxTransactions
            paragraph
                Status
                    display as text
                visible when (ManuallyEntered)
                    text of "Manually_Entered"
                visible when (not ManuallyEntered)
                    text of "System_Generated"
