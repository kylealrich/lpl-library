ManufacturerRelatedRecords is a CompositeForm
    title is "ManufacturerRelatedRecords"
    context form is ManufacturerRelatedRecordContext
        background is theme color
    restrict action Create
    restrict action Delete
    restrict action AttachToImportTierQualifier
    restrict action AttachToRebateQualifier
    restrict action AttachToTierQualifier
    restrict action ChangeThisManufacturer
    Actions
        action is refresh
            label is "_"
    ActiveItems is a Panel
        valid when (ItemsExist)
        list is ActiveItemsRel.ItemsForManufacturerCodeDivision
    ActiveVendorItems is a Panel
        valid when (VendorItemsExist)
        list is ActiveVendorItemsRel.VendorItemsForManufacturer
    OpenContractLines is a Panel
        valid when (ContractLinesExist)
        list is OpenContractLinesRel.ContractLinesForAManufacturer
