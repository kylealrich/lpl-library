RecallNoticeSetupReport is a SummaryForm
    title is "Notice#<RecallNotice>"
    First Page Header
        Layout
            visible when (RecallGroup.HasReportLogo)
                single column
                    RecallGroup.ReportLogo.File
                        no label
                        display as image
                header2 of "_"
    Page Header
        Layout
            header2 of "_"
                underline color of black
            single column
                text of "Notice#<RecallNotice>:_<Name>"
                    header2
                    left align
            header2 of "_"
            blank line
    Page Footer
        Layout
            two column
                text of "<current timestamp>"
                    left align
                text of "Page<page number>"
                    right align
    Layout
        form is RecallNoticeSummary
        blank line
        visible when (HasRecallNoticeDomains)
            header2 of "Domains"
                show border color of black
                shade
                center
            header2 of "_"
            list is RecallNoticeDomain set.RecallNoticeDomainPrint
        visible when (HasNonItemProposalTasks)
            header2 of "Tasks"
                show border color of black
                shade
                center
            header2 of "_"
            list is RecallNoticeTaskNotItemProposalRel.RecallNoticeTaskPrint
        visible when (HasRecallReferences)
            header2 of "References"
                show border color of black
                shade
                center
            header2 of "_"
            list is RecallNoticeReference set.RecallNoticeReferencePrint
        visible when (HasRecallComments)
            header2 of "Comments"
                show border color of black
                shade
                center
            header2 of "_"
            list is RecallComment set.RecallCommentPrint
        visible when (HasRecallNotifications)
            header2 of "Notifications"
                show border color of black
                shade
                center
            header2 of "_"
            list is RecallNotification set.RecallNotificationPrint
        visible when (HasAdhocNotifications)
            header2 of "AdhocNotifications"
                show border color of black
                shade
                center
            header2 of "_"
            list is RecallAdhocNotificationRel.AdhocWatcherPrint
        visible when (HasRecallProducts)
            header2 of "Products"
                show border color of black
                shade
                center
            header2 of "_"
            form is RecallProducts
