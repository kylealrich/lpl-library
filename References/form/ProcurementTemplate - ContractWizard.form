ContractWizard is a WizardForm
    use for action CreateFromContract
    title is "ProcurementTemplate"
    summary form is ProcurementTemplateSummary
    show steps
    ProcurementTemplate is a Panel
        form is ProcurementTemplateForContract
        save on next
    ProcurementTemplateLine is a Panel
        valid when (not TransientCopyAllLines)
        mouse over text is "SelectContractLinesForTemplate"
        list is ProcurementTemplateLinesRel.ProcurementTemplateLinesForContract
            helper list is ContractLinesForProcurementTemplateRel.ContractLinesForProcurementTemplateHelperList
                action is IncludeInProcurementTemplateLine
                    invoked.PrmProcurementGroup = ProcurementGroup
                    invoked.PrmProcurementTemplate = ProcurementTemplate
    ProcurementTemplateParticipants is a Panel
        mouse over text is "CreateAtLeastOneParticipantForTemplate"
        list is ProcurementTemplateParticipantsRel.ProcurementTemplateParticipantWizardList
