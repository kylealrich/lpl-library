UpdateStatementCurrencyRates is an ActionForm
    action is UpdateStatementCurrencyRates
    Layout
        paragraph
            PrmDateRange.Begin
                label is "StatementDateRange"
            text of "_-"
            PrmDateRange.End
                no label
        blank line
        header3 of "ExchangeRateForStatementDateWillBeUsedUnlessOverriden"
        single column
            CashManagementGroup.FinanceEnterpriseGroup.CurrencyTable
                label is "CorporateCurrencyTable"
                display as text
            visible when (AccountCurrencyDifferentFromBase)
                PrmOverrideBaseCurrencyRate
                    label is "OverrideBaseRateFrom<Currency>To<CashManagementGroup.Currency>"
            visible when (AccountCurrencyDifferentFromLocation)
                PrmOverrideLocationCurrencyRate
                    label is "OverrideBaseLocationRateFrom<Currency>To<CashManagementAccount.CashManagementLocation.Currency>"
            PrmRevalueUsingPeriodEndRate
                label is "RevalueUsingPeriodEndRate"
