OrderEntryInterfaceInput_Translation is a Form
    is primary
    Layout
        paragraph
            Base_BusinessClass.UniqueID
                display as text
        single column
            IsoLocale
                compact format
                raise yellow alert when (VerifyTranslations)
                    mouse over text is untranslatable:"The base data has been changed but this translation has not been verified"
        single column
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.OrderBillName
                label is untranslatable:"Order Bill Name"
            OrderBillName
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.ShipToName
                label is untranslatable:"Ship To Name"
            ShipToName
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.LineItemDescription
                label is untranslatable:"Line Item Description"
            LineItemDescription
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.AOCDescription
                label is untranslatable:"AOC Description"
            AOCDescription
                label is untranslatable:"Translated"
