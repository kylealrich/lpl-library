CalendarComposite is a CompositeForm
    is primary
    title is "GlobalLedgerCalendar_<GeneralLedgerCalendar>_<Description>"
    context form is GeneralLedgerCalendarContext
    Actions
        action is CreateCalendarYear
            action icon is create
            label is "CreateCalendarYear"
            mouse over text is "CreateANewCalendarYear"
        action is previous record
            label is "_"
            mouse over text is "Previous"
        action is next record
            label is "_"
            mouse over text is "Next"
        action is Create
            action icon is create
            label is "_"
        action is save
            action icon is save
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
        action is refresh
            label is "_"
    Main is a Panel
        form is GeneralLedgerCalendarForm
    Hierarchy is a Panel
        visible when (TopNodeEntered)
        list is TopNodeRel.ChildrenRel.CalendarTreeView
    YTDHierarchy is a Panel
        visible when (YTDTopNode entered)
        list is YTDTopNodeRel.ChildrenRel.CalendarTreeView
    LTDHierarchy is a Panel
        visible when (LTDTopNode entered)
        list is LTDTopNodeRel.ChildrenRel.CalendarTreeView
    UnusedPeriods is a Panel
        list is FinanceEnterpriseGroup.GeneralLedgerCalendarPeriod set.UnusedPeriodList
    CloseConfiguration is a Panel
        title is "CloseConfiguration"
        visible when (PeriodDataExists)
        list is GeneralLedgerCloseConfigurationRel.GeneralLedgerCloseConfigurationList
    RebuildInProcess is a Panel
        visible when (GenerationInProcess)
        form is GenerationInProcessForm
    PeriodCrossReference is a Panel
        list is FinanceEnterpriseGroup.GeneralLedgerCalendarPeriodXref set.primary
