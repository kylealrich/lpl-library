BankServiceStatementComposite is a CompositeForm
    is primary
    title is "FeeServiceStatement"
    context form is BankServiceStatementForm
        background is theme color
    FeeServiceDetails is a Panel
        list is BankServiceDetail set.BankServiceDetailList
    Exceptions is a Panel
        visible when (HasUnresolvedBankStatementDetailExceptions)
        list is BankServiceDetail set.BankServiceExceptionsList
