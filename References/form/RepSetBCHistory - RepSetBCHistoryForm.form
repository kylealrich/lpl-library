RepSetBCHistoryForm is a Form
    is primary
    title is "History"
    Layout
        single column
            ReplicationSet
        two column
            RepSetBC.BusinessClass
            RepSetBC.ReplicateClassName
        visible when (not RepSetBCHistory.Backfill.False)
            paragraph
                BackfillFrom
                visible when (RepSetBCHistory.Backfill.Backfill)
                    RepSetBCHistory.HistoryStamp
                        label is "BackfillThrough"
                visible when (RepSetBCHistory.Backfill.ArchiveBackfill)
                    RepSetBCHistory.HistoryStamp
                        label is "ArchiveBackfillThrough"
                RepSetBCHistory.Backfill
        visible when (RepSetBCHistory.Backfill.False)
            paragraph
                RepSetBCHistory.HistoryStamp
                RepSetBCHistory.Backfill
        single column
            DerivedVariationID
        paragraph
            RecordCount
            Segments
        paragraph
            NumberCreated
            NumberUpdated
            NumberDeleted
            visible when (RepSetBC.ExpectedNumberOfRecordsIsValid)
                ExpectedNumberOfRecords
                    mouse over text is "ExpectedNumberOfRecordsInTheTargetSystemAtTheTimeOfThisReplication"
        paragraph
            Started
            Finished
            ElapsedTimeComponentsText
        paragraph
            ReplicationWarnings
                use text area of 20 lines
