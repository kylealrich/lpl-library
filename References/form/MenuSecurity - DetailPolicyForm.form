DetailPolicyForm is a CompositeForm
    title is "NavigationPolicy"
    Policy is a Panel
        form is MenuSecurityConsoleDetailForm
    ReviewChanges is a Panel
        visible when (HasUnreviewedChanges)
        form is SuggestedDifferencePanelForm
    History is a Panel
        list is MenuSecurityHistory set.ConsoleConfigurationChangesList
    DeliveredHistory is a Panel
        visible when (BasedOnDelivered)
        list is MenuSecurityDeliveredHistory set.ConsoleConfigurationChangesList
