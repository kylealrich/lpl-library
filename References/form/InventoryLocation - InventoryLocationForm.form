InventoryLocationForm is a CompositeForm
    is primary
    use for action CreateInventoryLocation
    title is "<LocationType>"
    context form is InventoryLocationContext
        background is theme color
    Actions
        action is CreateInventoryLocation
            action icon is create
            hide when invalid
        action is CreateByCopy
            action icon is copy
            hide when invalid
        action is UpdateTaxCodeUsingVertex
            action icon is process
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is refresh
            label is "_"
    Address is a Panel
        form is Address
    Options is a Panel
        visible when (not ParOrExchangeCart)
        form is Options
    WorkOrder is a Panel
        visible when (WorkOrderVisible)
        form is WorkOrder
    KitchenOrder is a Panel
        visible when (KitchenOrderVisible)
        form is KitchenOrder
    SubstituteLocations is a Panel
        visible when (SubstituteLocationsVisible)
        list is SubstituteLocationsRel.SubstituteLocationList
    AccountGroups is a Panel
        visible when (not IsCartLocation)
        list is GeneralLedgerCategory set.GeneralLedgerCategoriesForALocation
    WarehouseStorageLocations is a Panel
        visible when (UseAsWarehouseLocation)
        list is WarehouseStorageLocation set.WarehouseStorageLocationList
    StorageCodes is a Panel
        visible when (not ParOrExchangeCart)
        list is StorageCode set.StorageCodeList
    Bins is a Panel
        visible when (not ParOrExchangeCart)
        list is Bin set.BinList
    BinGroups is a Panel
        visible when (not ParOrExchangeCart)
        list is BinGroup set.BinGroupList
    BinReorderPolicies is a Panel
        visible when (not ParOrExchangeCart)
        list is BinReorderPolicy set.BinReorderPoliciesForALocation
    ParItems is a Panel
        visible when (ParOrExchangeCart)
        list is BinGroupDetail set.CartParLocations
    KitItems is a Panel
        visible when (not ParOrExchangeCart)
        list is KitItemRel.KitItemList
    BurdenCodes is a Panel
        visible when (not ParOrExchangeCart)
        list is BurdenCode set.Burdens
    MultipleTaxAreas is a Panel
        visible when (MultipleTaxAreasVisible)
        list is VertexTaxAreaIdResultsRel.TaxAreaResultList
    VertexError is a Panel
        visible when (VertexErrorVisible)
        list is VertexWebserviceErrorRel.VertexTaxAreaIdErrorList
    ParItemsWithInvalidUOM is a Panel
        visible when (DisplayInvalidParUOM)
        list is ItemLocationInvalidUOMRel.ParItemInvalidUOMOnLocation
    Docks is a Panel
        visible when (UseAsWarehouseLocation)
        list is WarehouseDock set.WarehouseDockInventoryLocationList
