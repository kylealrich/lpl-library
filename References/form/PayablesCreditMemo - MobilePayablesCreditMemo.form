MobilePayablesCreditMemo is a Form
    title is "PayablesCreditMemo"
    Layout
        two column distributed
            Company
                display as text
            Company.Name
                label is "CompanyName"
                display as text
        two column distributed
            Vendor
                display as text
            Vendor.VendorName
                label is "VendorName"
                display as text
        paragraph
            PayablesCreditMemo.Invoice
                display as text
            PayablesCreditMemo.Suffix
                display as text
        paragraph
            PayablesCreditMemo.PayablesSequence
                label is "InvoicePaymentSequence"
        paragraph
            PayablesCreditMemo.CreditMemoNumber
        paragraph
            PayablesCreditMemo.CreditMemoSuffix
        paragraph
            PayablesCreditMemo.CreditMemoSequence
        paragraph
            AmountApplied
