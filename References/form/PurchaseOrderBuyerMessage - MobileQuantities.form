MobileQuantities is a Form
    title is "Quantities"
    Actions
        action is Unreleased.Release
            hide when invalid
        action is Unreleased.ReleaseAndCreateVendorReturn
            hide when invalid
        action is Unreleased.Update
    Layout
        header2 of "Receiver_/AdjustmentInProcess"
        two column
            display as text
                column1 of 50%
                    paragraph
                        ReceiptQuantityWithUOM
                            label is "ReceiptQuantity"
                column2 of 50%
                    paragraph
                        ExcessWithUOM
                            label is "ExcessQuantity"
        single column
            paragraph
                TransientUnitCost
                    label is "UnitCost"
                    show up to 10 characters
                PurchaseOrderLineRel.VendorPriceUOM
                    no label
                    display as text
            PurchaseOrderReceiptLine.InspectionRequired
                label is "InspectionRequired"
                display as text
