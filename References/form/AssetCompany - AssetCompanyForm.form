AssetCompanyForm is a CompositeForm
    is primary
    title is "AssetCompany<Company>_-_<Company.Name>"
    context form is AssetCompanyFormContext
        background is theme color
    Main is a Panel
        form is AssetCompanyMain
    Types is a Panel
        list is AssetTypesRel.AssetTypesAll
    Templates is a Panel
        list is AssetTemplatesRel.TemplatesAndInterfaceRecords
    AccountGroups is a Panel
        list is AssetAccountGroup set.AccountGroupsWithinCompany
    DimensionGroups is a Panel
        list is AssetAccountingUnitGroup set.AssetAccountUnitGroupsWithinACompany
    BookCalendars is a Panel
        list is BookCalendarForCompanyRel.AssetCompanyBookCalendarList
    Assets is a Panel
        list is AssetsByCompanyAssetRel.CompanyAssetList
    AssetReview is a Panel
        visible when (AssetReview)
        list is AssetReviewRel.AssetReviewList
