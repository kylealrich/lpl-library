RoleCompositeForm is a CompositeForm
    is primary
    title is "Role"
    context form is RoleForm
    restrict action UpdateConfigurableFeatures
    Detail is a Panel
        form is RoleDetail
    RoleAssignedToActors is a Panel
        list is ActorRole set.HelperActorRoleList
            helper list is ActorRel.ActiveActorList
                action is AssignRoleToActor
    SecurityClassesAssignedToRole is a Panel
        list is RoleSecurityClass set.HelperRoleSecurityClass
    ConfigurableFeatures is a Panel
        form is ConfigurableFeaturesPanel
    ReportCatalog is a Panel
        form is InaccessibleReports
