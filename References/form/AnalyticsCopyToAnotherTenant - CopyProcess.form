CopyProcess is a Form
    title is "CopyProcess"
    Layout
        single column
            blank line
            header2 of "Setup"
            blank line
            TargetTenantID
                display as text
            TargetEnvironment
                label is "TargetEnvironmentType"
                display as text
            blank line
            header3 of "CopyToAnotherTenant"
            blank line
            visible when (Status.InProgress)
                button of "StartCopyingToAnotherTenant"
                    action is StartUpgradeTenant
            last LatestUpgradeTenantToTenantRel.Status
                label is "Status"
                raise red alert when (last LatestUpgradeTenantToTenantRel.IsFailed)
                raise yellow alert when (last LatestUpgradeTenantToTenantRel.IsCanceled)
                raise blue alert when (last LatestUpgradeTenantToTenantRel.IsActionPendingOrActionPendingInServer)
                    mouse over text is "ActionPending"
            button of "<UpgradeTenantToTenantWorkunit>"
                valid when (IsCopyConsumerCustomizationWorkunitExists)
                link is TenantToTenantUpgradeWorkUnitNav
                label is "WorkUnit"
                display as link
            valid when (Status.InProgress)
                display as text
                CopyProcessCompleted
                    label is "Please_confirm_if_Status_of_workunit_is_completed_before_proceeding_the_next_steps."
