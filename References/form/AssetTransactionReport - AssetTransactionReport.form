AssetTransactionReport is an ActionForm
    action is AssetTransactionReport
    Layout
        header2 of "SelectionOptions"
        single column
            ReportOption
        paragraph
            FromDate
                label is "FromDate"
            ToDate
                label is "ToDate"
        paragraph
            Transaction
            Book
        paragraph
            Company
                label is "Company"
            text of "Or"
            CompanyGroup
                label is "CompanyGroup"
        single column
            Classification
        single column
            TransactionDimension
                label is "TransactionDimensions"
                compact format
        two column
            Asset
                select is AllAssets
            Asset.Description
                no label
                display as text
        paragraph
            AssetLease.LeaseCompany
                label is "_orLease"
            AssetLease.LeaseCompany.Name
                no label
                display as text
            AssetLease.Lease
                no label
        single column
            TagNumber
                label is "Tag"
        single column
            AssetGroup
                select is AssetGroups
        header2 of "AccountRanges"
        two column table
            column1
                text of "From"
                FrAccount.AM275FrAccountGroup[1].FrAccount
                    no label
                FrAccount.AM275FrAccountGroup[2].FrAccount
                    no label
                FrAccount.AM275FrAccountGroup[3].FrAccount
                    no label
                FrAccount.AM275FrAccountGroup[4].FrAccount
                    no label
                FrAccount.AM275FrAccountGroup[5].FrAccount
                    no label
                FrAccount.AM275FrAccountGroup[6].FrAccount
                    no label
                FrAccount.AM275FrAccountGroup[7].FrAccount
                    no label
                FrAccount.AM275FrAccountGroup[8].FrAccount
                    no label
                FrAccount.AM275FrAccountGroup[9].FrAccount
                    no label
                FrAccount.AM275FrAccountGroup[10].FrAccount
                    no label
            column2
                text of "To"
                FrAccount.AM275FrAccountGroup[1].ToAccount
                    no label
                FrAccount.AM275FrAccountGroup[2].ToAccount
                    no label
                FrAccount.AM275FrAccountGroup[3].ToAccount
                    no label
                FrAccount.AM275FrAccountGroup[4].ToAccount
                    no label
                FrAccount.AM275FrAccountGroup[5].ToAccount
                    no label
                FrAccount.AM275FrAccountGroup[6].ToAccount
                    no label
                FrAccount.AM275FrAccountGroup[7].ToAccount
                    no label
                FrAccount.AM275FrAccountGroup[8].ToAccount
                    no label
                FrAccount.AM275FrAccountGroup[9].ToAccount
                    no label
                FrAccount.AM275FrAccountGroup[10].ToAccount
                    no label
        header2 of "OutputOptions"
        single column
            PrintAssetTot
                label is "PrintTotals_byAsset"
        blank line
        blank line
        blank line
        blank line
        single column
            PageBreak
        blank line
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                AssetTransactionReportReport_RDG
            column2 of 25%
                AssetTransactionReportReport_XPT
