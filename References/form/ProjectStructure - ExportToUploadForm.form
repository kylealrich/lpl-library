ExportToUploadForm is an ActionForm
    action is ExportToUpload
    title is "ExportToUpload"
    Layout
        single column
            PrmRunGroup
                label is "RunGroup"
            text of "LeaveTheFollowingParametersBlankToExportTheEntireStructure"
            PrmProjectGroup
                label is "CustomGroup"
            text of "Or"
            PrmSummaryProject
                label is "Summary<FinanceEnterpriseGroup.ProjectLabel>"
                select is SummaryProjectRecords
            text of "Or"
            PrmPostingProject
                label is "Posting<FinanceEnterpriseGroup.ProjectLabel>"
                select is ProjectNode.DescendantsRel.ProjectSelect
            visible when (FinanceEnterpriseGroup.ProjectInvoicing)
                PrmBypassEdits
                    label is "BypassEdits"
