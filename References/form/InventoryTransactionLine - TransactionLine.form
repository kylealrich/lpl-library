TransactionLine is a CompositeForm
    is primary
    title is "<DerivedInventoryTransactionLineTitle>"
    context form is TransactionLineContext
        background is theme color
    Actions
        action is Unreleased.ReleaseInventoryTransaction
            action icon is release
            hide when invalid
        action is previous record
            label is "_"
            hide when invalid
        action is next record
            label is "_"
            hide when invalid
        action is Unreleased.Create
            label is "_"
        action is save
            label is "_"
        action is save and new
            label is "_"
        action is Unreleased.Delete
            label is "_"
        action is refresh
            label is "_"
    Main is a Panel
        form is InventoryTransactionLine
    Details is a Panel
        visible when (ShowDetailPanel)
        list is InventoryTransactionLineDetailsRel.Details
    UDIDetails is a Panel
        visible when (ShowUDIDetailPanel)
        list is InventoryUDIDetailRel.UDIEntrys
    UDIDetailsFromWarehouse is a Panel
        visible when (ShowUDIDetailFromWHPanel)
        list is InventoryUDIDetailForWHRel.UDIEntrys
    Distributions is a Panel
        visible when (HasDistributions)
        list is InventoryTransactionLineDistribution set.InventoryTransactionLineDistributions
