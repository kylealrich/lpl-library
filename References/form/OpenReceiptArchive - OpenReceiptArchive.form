OpenReceiptArchive is an ActionForm
    action is OpenReceiptArchive
    Layout
        single column
            paragraph
                CompanyGroup
                text of "Or"
                Company
            ProcessLevel
                select is ProcessLevels
        single column
            paragraph
                ReceiptDateFrom
                    label is "FromReceiptDate"
                ReceiptDateTo
                    label is "ToReceiptDate"
                text of "Or"
                CreateDateFrom
                    label is "FromP\\OCreateDate"
                CreateDateTo
                    label is "ToP\\OCreateDate"
        two column
            column1 of 50%
                PostingOption
            column2 of 50%
                paragraph
                    UpdateOption
                        label is "Update"
                    SequenceOption
                        label is "ProcessLevelSequence"
        header2 of "Selection"
        single column
            paragraph
                RecNumberFrom
                    label is "FromReceiver"
                    select is ReceiversByReferenceNumber
                RecNumberTo
                    label is "ToReceiver"
                    select is ReceiversByReferenceNumber
                text of "Or"
                RecNumber
                    label is "Receiver"
                    select is ReceiversByReferenceNumber
            paragraph
                Vendor
                    select is ActiveVendorsByName
                VendorGroup
        single column
            PoNumberSelect
                label is "P\\ONumberSelect"
        three column
            column1 of 33%
                PoNumber.MA180PoNumberGroup[1]
                    label is "P\\ONumbers"
                PoNumber.MA180PoNumberGroup[4]
                    no label
            column2 of 34%
                PoNumber.MA180PoNumberGroup[2]
                    no label
                PoNumber.MA180PoNumberGroup[5]
                    no label
            column3 of 33%
                PoNumber.MA180PoNumberGroup[3]
                    no label
                PoNumber.MA180PoNumberGroup[6]
                    no label
        two column
            column1 of 33%
                PoCodeSelect
                    label is "P\\OCodeSelect"
            column2 of 67%
                PoCodeGroup.ProcurementGroup
                    label is "ProcurementGroup"
        three column
            column1 of 33%
                PoCodeGroup.PoCodeSel.PoCodeSel[1]
                    label is "P\\OCodes"
                PoCodeGroup.PoCodeSel.PoCodeSel[4]
                    no label
                PoCodeGroup.PoCodeSel.PoCodeSel[7]
                    no label
            column2 of 34%
                PoCodeGroup.PoCodeSel.PoCodeSel[2]
                    no label
                PoCodeGroup.PoCodeSel.PoCodeSel[5]
                    no label
                PoCodeGroup.PoCodeSel.PoCodeSel[8]
                    no label
            column3 of 34%
                PoCodeGroup.PoCodeSel.PoCodeSel[3]
                    no label
                PoCodeGroup.PoCodeSel.PoCodeSel[6]
                    no label
                PoCodeGroup.PoCodeSel.PoCodeSel[9]
                    no label
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                OpenReceiptArchiveReport_RDG
                OpenReceiptArchiveErrorReport_RDG
            column2 of 25%
                OpenReceiptArchiveReport_XPT
                OpenReceiptArchiveErrorReport_XPT
