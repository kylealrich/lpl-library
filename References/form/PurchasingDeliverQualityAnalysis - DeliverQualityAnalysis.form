DeliverQualityAnalysis is an ActionForm
    action is PurchasingDeliverQualityAnalysis
    Layout
        paragraph
            CompanyGroup
                show up to 10 characters
            CompanyGroup.Description
                no label
                display as text
        single column
            text of "\\or"
        paragraph
            Company
            Company.Name
                no label
                display as text
        header2 of "_"
        text of "_"
        paragraph
            VendorGroup
            Vendor
            Vendor.VendorName
                no label
                display as text
        paragraph
            Location
            Location.Name
                no label
                display as text
        paragraph
            PurchMajcl
                label is "MajorPurchaseClass"
            PurchMincl
                label is "MinorPurchaseClass"
        header2 of "_"
        text of "_"
        paragraph
            BuyerGrp
                label is "BuyerGroup"
            BuyerGrp.Description
                no label
                display as text
        single column
            text of "\\or"
        paragraph
            BuyerCode
                label is "Buyer"
                show up to 10 characters
            BuyerCode.Employee.PresentationNameSnapshot
                no label
                display as text
        single column
            Item.Item[1]
                label is "Items"
                show up to 20 characters
            Item.Item[2]
                no label
                show up to 20 characters
            Item.Item[3]
                no label
                show up to 20 characters
        paragraph
            FrRecActDate
                label is "FromLastReceivedDate"
            ToRecActDate
                label is "ToLastReceivedDate"
        paragraph
            SeqOption
                label is "SequenceOption"
            SummaryOpt
                label is "SummaryOnly"
                show up to 10 characters
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                PurchasingDeliverQualityAnalysisReport_RDG
            column2 of 25%
                PurchasingDeliverQualityAnalysisReport_XPT
