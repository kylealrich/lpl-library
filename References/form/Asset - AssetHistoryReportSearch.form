AssetHistoryReportSearch is a SearchForm
    title is "SearchFields"
    Layout
        four column independent
            column1
                Company
                Asset
                TagNumber
                    label is "Tag"
            column2
                Status
                AssetType
                    label is "AssetType"
                AssetSummaryGroup
                    label is "SummaryGroup"
            column3
                AssetGroup
                AssetLocation
                    label is "Location"
                AssetDivision
                    label is "Division"
            column4
                text of "PurchaseDate"
                PurchaseDateRange.Begin
                    label is "From"
                PurchaseDateRange.End
                    label is "To"
