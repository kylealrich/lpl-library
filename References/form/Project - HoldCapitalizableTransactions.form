HoldCapitalizableTransactions is an ActionForm
    action is HoldCapitalizableTransactions
    title is "<HoldActionTitle>"
    Layout
        header2 of "<HoldActionHeader>"
        single column
            SummaryProject
                label is "Summary<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
                select is EnterpriseSummaryProjectSelect
            PostingProject
                label is "Posting<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
            visible when (PrmFinanceEnterpriseGroup.CapitalizationUpgradeProgess.NotEnabled)
                SelectionGroup
            visible when (PrmFinanceEnterpriseGroup.CapitalizationUpgradeProgess.Completed)
                AssetSelectionGroup
            DateSelection
        two column
            DateRange.Begin
                label is "DateRange"
            DateRange.End
                label is "Through"
