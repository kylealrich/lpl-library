PortalApprovalPayablesInvoiceForm is a CompositeForm
    title is "InvoiceApproval"
    context form is PortalPayablesInvoiceSingleRowContextForm
        background is theme color
    Invoice is a Panel
        form is PortalPayablesInvoiceGeneralInformationViewOnly
    Distributions is a Panel
        title is "Distributions"
        visible when (not Company.VendorGroup.DistributionEntryOnApproval)
        list is PayablesInvoiceDistributionsDisplayOnInvoiceRel.PortalViewOnlyPayablesInvoiceDistributions
    DistributionsEdit is a Panel
        title is "Distributions"
        visible when (Company.VendorGroup.DistributionEntryOnApproval)
        list is PayablesInvoiceDistributionsDisplayOnInvoiceRel.PortalEditablePayablesInvoiceDistributions
    PurchaseOrders is a Panel
        title is "PurchaseOrders"
        visible when (PurchaseOrderInvoice)
        list is MatchPurchaseOrderInvoicesRel.PortalMatchPurchaseOrderInvoiceDetail
    Messages is a Panel
        title is "Messages"
        visible when (HasInvoiceMessages)
        list is MatchInvoiceMessageRel.PayablesInvoiceCostMessageList
    RelatedDocuments is a Panel
        title is "RelatedDocuments"
        list is PayablesInvoiceDocument set.PortalPayablesInvoiceDocuments
    Comments is a Panel
        title is "Comments"
        list is PayablesInvoiceCommentRel.XiPayablesInvoiceCommentCardList
    Errors is a Panel
        title is "Errors"
        visible when (ErrorsExists)
        list is PayablesInvoiceError set.PayablesInvoiceErrorList
    ApprovalTracking is a Panel
        visible when (AuditExists)
        list is PayablesInvoiceAudit set.PayablesInvoiceHistoryList
