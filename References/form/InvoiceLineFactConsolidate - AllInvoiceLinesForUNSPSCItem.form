        visible when (HasOnContractInvoiceLinesForUNSPSCItem)
        list is OnContractInvoiceLineFactsForUNSPSCItemRel.OnContractUNSPSCInvoiceLineFacts
    OffContractInvoices is a Panel
        visible when (HasOffContractInvoiceLinesForUNSPSCItem)
