AppendReleasedAsset is an ActionForm
    action is AppendToReleasedAsset
    title is "AppendToAsset"
    Layout
        visible when (AppendsToReleasedOrAdjustedAsset)
            paragraph
                PrmAdjustmentDate
            paragraph
                PrmAppendToAsset
                    label is "Asset"
            paragraph
                PrmAssetItem
                    label is "AddToItem"
                    select is AssetItemsRel.AssetItems
            paragraph
                PrmItemQuantity
                    label is "ItemQuantity"
