BankExpenses is a Form
    is primary
    Layout
        paragraph
            ReceivableCompanyRel.Company
                select is ARCompanies
            ReceivableCompanyRel.Name
                no label
                display as text
        paragraph
            CashCode
                select is CashCode
            CashCode.Description
                no label
                display as text
            Currency
                display as text
            ExchangeRate
                display as text
        paragraph
            BankTransactionCode
                label is "PaymentCode"
        paragraph
            TransactionDate
                label is "PostDate"
        paragraph
            CustomerDraftBankCashingCharges
                display as text
        paragraph
            BankExpenseAmount
                label is "BankExpenseAmount"
            visible when (NonBaseCurrency)
                BaseBankExpenseAmount
                    no label
                    display as text
                ReceivableCompanyRel.Currency
                    no label
                    display as text
        paragraph
            BankExpenseAccount
                compact format
        paragraph
            AdministrationExpenseAmount
                label is "AdministrationExpenseAmount"
            visible when (NonBaseCurrency)
                BaseAdministrationExpenseAmount
                    no label
                    display as text
                ReceivableCompanyRel.Currency
                    no label
                    display as text
        paragraph
            AdministrationExpenseAccount
                compact format
