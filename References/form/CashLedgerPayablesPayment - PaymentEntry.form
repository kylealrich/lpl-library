PaymentEntry is a Form
    Layout
        four column distributed
            display as text
                column1
                    paragraph
                        CashCode.CashCodeAndDescription
                            label is "CashCode"
                    paragraph
                        CashLedgerPayablesPayment.BankTransactionCode.BankTransactionCodeName
                            label is "TransactionCode"
                        CashLedgerTransactionIdentifier
                column2
                    TransactionNumber
                    LastElectronicPaymentSerialNumber
                        label is "ElectronicPaymentSerialNumber"
                column3
                    PaymentDate
                    paragraph
                        PaymentAmount
                        CashCode.Currency
                            label is "Currency"
                column4
                    Status
                    paragraph
                        PaymentStatus
                        visible when (IsRejected)
                            CancelRequestType
                                label is "CancelType"
