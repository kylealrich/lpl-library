RecallGroup is a CompositeForm
    is primary
    title is "ItemLifecycleManagementGroup"
    context form is RecallGroupContext
        background is theme color
    General is a Panel
        form is RecallGroupGeneral
    EmailConfiguration is a Panel
        form is RecallGroupEmailConfiguration
    NewItemProposals is a Panel
        form is NewItemProposalForm
    Users is a Panel
        list is RecallUser set.AllUsersList
    Organizations is a Panel
        list is RecallOrganization set.RecallOrganizationsTree
    TaskInstructions is a Panel
        list is RecallTaskInstruction set.RecallTaskInstructions
    NoticeTypes is a Panel
        list is RecallType set.RecallTypes
    Domains is a Panel
        list is RecallDomain set.RecallDomains
