AssetBarCodeLabels is an ActionForm
    action is AssetBarCodeLabels
    Layout
        single column
            FinanceEnterpriseGroup
        single column
            NbrLabels
                label is "PrintNumber_ofLabels"
        single column
            NbrCopies
                label is "Number_ofDuplicates"
        blank line
        blank line
        single column
            text of "PrintAssetBarCodeLabel"
        two column
            Asset
                select is AllAssets
            Asset.Description
                no label
                display as text
        two column
            Item
                select is Items
            Item.Description
                no label
                display as text
        blank line
        blank line
        single column
            text of "PrintSpaceBarCodeLabel"
        two column
            LocationName
                label is "Location"
                select is Locations
            LocationName.Description
                no label
                display as text
        single column
            Level1
                label is "LocationDetailLevel1"
                select is LocationDetail
        single column
            Level2
                label is "LocationDetailLevel2"
                select is LocationDetail
        single column
            Level3
                label is "LocationDetailLevel3"
                select is LocationDetail
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                AssetBarCodeLabelsReport_RDG
            column2 of 25%
                AssetBarCodeLabelsReport_XPT
