MerchantGroupMerchantsForm is a Form
    is primary
    title is "<TitleLabel>inMerchantGroup<MerchantGroup>_-_<MerchantGroup.Description>"
    Layout
        paragraph
            MerchantGroup.ProcurementCardProgram.ProcurementGroup
                label is "ProcurementGroup"
                display as text
            MerchantGroup.ProcurementCardProgram.ProcurementGroup.Description
                no label
                display as text
        paragraph
            MerchantGroup.ProcurementCardProgram
                label is "ProcurementCardProgram"
                display as text
            MerchantGroup.ProcurementCardProgram.Description
                no label
                display as text
        paragraph
            MerchantGroup.MerchantGroup
                label is "MerchantGroup"
                display as text
            MerchantGroup.Description
                no label
                display as text
        paragraph
            MerchantGroup.MerchantGroupType
                label is "MerchantGroupType"
                display as text
        paragraph
            MerchantGroup.Active
                display as text
        blank line
        blank line
        visible when (MerchantGroup.MerchantGroupType.Name)
            paragraph
                MerchantGroupMerchants.Merchant
                    label is "Merchant"
                MerchantGroupMerchants.Merchant.Name
                    no label
                    display as text
        visible when (MerchantGroup.MerchantGroupType.Code)
            paragraph
                MerchantGroupMerchants.MerchantCategoryCode
                    label is "MerchantCategoryCode"
