PayablesAssetInterfaceForm is a Form
    use for action CreatePayablesInterface
    title is "PayablesAssetInterface"
    restrict action Create
    Layout
        two column
            paragraph
                Company
                Company.GeneralLedgerCompany.Name
                    no label
                    display as text
            InvoiceAndPurchaseOrderInformation.InvoiceCompany
                label is "InvoiceCompany"
                display as required
            paragraph
                AssetTemplate
                    select is ValidTemplatesRel.AssetTemplates
                AssetTemplate.Description
                    no label
                    display as text
            InvoiceAndPurchaseOrderInformation.Vendor
                display as required
        paragraph
            InvoiceAndPurchaseOrderInformation.PayablesInvoice
                display as required
        paragraph
            button of "<InvoiceAndPurchaseOrderInformation.PayablesInvoice.Invoice>"
                visible when (HasInvoice)
                link is PayablesInvoiceLink
        paragraph
            AssetProject
                compact format
        two column
            ItemQuantity
                label is "Quantity"
            TransactionItemCost
                label is "Cost"
        two column
            TransactionItemTax
                label is "Tax"
            PurchaseDate
        two column
            AssetDescription
            AssetGroup
        two column
            TagNumber
            Currency
        two column
            Combine
            HoldAsset
        header3 of "Item"
        two column
            column1
                ItemNumber
                    label is "Item"
                InvoiceAndPurchaseOrderInformation.PurchaseOrder
                InvoiceAndPurchaseOrderInformation.PurchaseOrderLine
                BarCode
                SerialNumber
            column2
                Description
                LocationDetail
                    select is ActiveAssetLocationDetails
                ModelNumber
                InServiceDate
        header3 of "TemplateOverrides"
        paragraph
            AssetAccountingUnit
                label is "DefaultTransactionDimensions"
                compact format
        paragraph
            DepreciationExpenseProject
                label is "DepreciationExpenseProject"
                compact format
        three column
            AssetLocation
                label is "Location"
            AssetDivision
                label is "Division"
            AssetType
                compact format
            AssetAccountingUnitGroup
                label is "AssetDimensionGroup"
            AssetAccountGroup
                label is "AccountGroup"
            AssetCategory
                label is "Category"
            visible when (AssetGuidelineClassesExists)
                AssetGuidelineClass
                    label is "GuidelineClass"
        paragraph
            WorkInProcess
            TaxExempt
            Simulated
            Used
