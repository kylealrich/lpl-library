MAssetAdjustmentMain is a Form
    title is "Detail"
    Layout
        single column
            header3 of "AssetDetails"
            visible when (Asset.LeasedAsset)
                paragraph
                    Asset.AssetLease.LeaseCompany
                        label is "LeaseCompany"
                        display as text
                    Asset.AssetLease.LeaseCompany.Name
                        no label
                        display as text
                paragraph
                    Asset.AssetLease.Lease
                        display as text
                    Asset.AssetLease.Lease.Description
                        no label
                        display as text
            paragraph
                Asset.TagNumber
                    display as text
            paragraph
                CreationDate
                    display as text
                CreatedBy
                    display as text
            header3 of "CurrentAssetDetail"
            paragraph
                Company
                    display as text
                Description
                    display as text
            paragraph
                visible when (IsTagAdjusted)
                    TagNumber
                        display as text
                visible when (IsCurrencyAdjusted)
                    Currency
                        display as text
            visible when (IsAssetCategoryAdjusted)
                AssetCategory
                    display as text
            visible when (IsPhysicalInventoryAdjusted)
                IncludeInPhysicalInventory
                    display as text
            paragraph
                visible when (IsSimulatedAdjusted)
                    Simulated
                        display as text
                visible when (IsWorkInProcessAdjusted)
                    WorkInProcess
                        display as text
            paragraph
                visible when (IsTaxExemptAdjusted)
                    TaxExempt
                        display as text
                visible when (IsUsedAdjusted)
                    Used
                        display as text
            visible when (IsAssetGroupAdjusted)
                AssetGroup
                    display as text
            paragraph
                visible when (IsAssetLocationAdjusted)
                    AssetLocation
                        label is "Location"
                        display as text
                visible when (IsAssetDivisionAdjusted)
                    AssetDivision
                        label is "Division"
                        display as text
            visible when (IsAssetTypeAdjusted)
                AssetType
                    display as text
                    compact format
            visible when (IsAssetGuidelineClassAdjusted)
                AssetGuidelineClass
                    display as text
            paragraph
                visible when (IsAssetAccountGroupAdjusted)
                    AssetAccountGroup
                        display as text
                visible when (IsAssetAccountingUnitGroupAdjusted)
                    AssetAccountingUnitGroup
                        display as text
            paragraph
                visible when (IsAssetAccountingUnitAdjusted)
                    AssetAccountingUnit
                        label is "DefaultTransactionDimensions"
                        display as text
                        compact format
            paragraph
                visible when (IsDepreciationExpenseProjectAdjusted)
                    text of "Depreciation_Expense"
                        color of gray
                        align as label
                    DepreciationExpenseProject
                        no label
                        display as text
            paragraph
                visible when (IsAssetOwnerAdjusted)
                    text of "AssetOwner"
                        color of gray
                        align as label
                    AssetOwner
                        no label
                        display as text
                visible when (IsFirstAndLastNameAdjusted)
                    AssetOwner.Name.FirstAndLastName
                        no label
            header3 of "NewAssetDetail"
            paragraph
                AssetAdjustmentTo.Company
                    display as text
                AssetAdjustmentTo.Description
                    display as text
            paragraph
                visible when (IsTagAdjusted)
                    AssetAdjustmentTo.TagNumber
                        display as text
                visible when (IsCurrencyAdjusted)
                    AssetAdjustmentTo.Currency
                        display as text
            visible when (IsAssetCategoryAdjusted)
                AssetAdjustmentTo.AssetCategory
                    display as text
            visible when (IsPhysicalInventoryAdjusted)
                ToIncludeInPhysicalInventory
                    label is "IncludeInPhysicalInventory"
                    display as text
            paragraph
                visible when (IsSimulatedAdjusted)
                    AssetAdjustmentTo.Simulated
                        display as text
                visible when (IsWorkInProcessAdjusted)
                    AssetAdjustmentTo.WorkInProcess
                        display as text
            paragraph
                visible when (IsTaxExemptAdjusted)
                    AssetAdjustmentTo.TaxExempt
                        display as text
                visible when (IsUsedAdjusted)
                    AssetAdjustmentTo.Used
                        display as text
            visible when (IsAssetGroupAdjusted)
                AssetAdjustmentTo.AssetGroup
                    display as text
            paragraph
                visible when (IsAssetLocationAdjusted)
                    AssetAdjustmentTo.Location
                        display as text
                visible when (IsAssetDivisionAdjusted)
                    AssetAdjustmentTo.Division
                        display as text
            visible when (IsAssetTypeAdjusted)
                AssetAdjustmentTo.AssetType
                    display as text
                    compact format
            visible when (IsAssetGuidelineClassAdjusted)
                ToAssetGuidelineClass
                    display as text
            paragraph
                visible when (IsAssetAccountGroupAdjusted)
                    AssetAdjustmentTo.AssetAccountGroup
                        display as text
                visible when (IsAssetAccountingUnitGroupAdjusted)
                    AssetAdjustmentTo.AssetAccountingUnitGroup
                        display as text
            paragraph
                visible when (IsAssetAccountingUnitAdjusted)
                    AssetAdjustmentTo.AssetAccountingUnit
                        display as text
                        compact format
            paragraph
                visible when (IsDepreciationExpenseProjectAdjusted)
                    text of "Depreciation_Expense"
                        color of gray
                        align as label
                    ToDepreciationExpenseProject.Project
                        no label
                        display as text
            paragraph
                visible when (IsAssetOwnerAdjusted)
                    text of "AssetOwner"
                        color of gray
                        align as label
                    ToAssetOwner
                        no label
                        display as text
                visible when (IsFirstAndLastNameAdjusted)
                    ToAssetOwner.Name.FirstAndLastName
                        no label
                        display as text
