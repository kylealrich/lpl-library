TestingParametersForm is a Form
    title is "TestingParameters"
    Layout
        single column
            single column
                DoNotIncludeHeader
                    display as switch
                DoNotIncludeTrailer
                    display as switch
                header4 of "FailureNotifications"
                TestingEmailFrom
            paragraph
                TestingEmailTo
                    display as text
                button of "AddUser"
                    action is AddEmailForTesting
                button of "Clear"
                    action is ClearEmail
