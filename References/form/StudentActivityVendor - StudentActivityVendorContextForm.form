StudentActivityVendorContextForm is a Form
    title is "<mode>Vendor"
    Layout
        four column table
            column1
                paragraph
                    StudentActivityDistrict
                        label is "SchoolDistrict"
                        display as text
                paragraph
                    StudentActivityVendor
                        label is "VendorNumber"
                        display as text
                paragraph
                    VendorGroup
            column2
                paragraph
                    StudentActivityDistrict.Name
                        no label
                        display as text
                paragraph
                    Name
                paragraph
                    IncomeCode
            column3
                paragraph
                    blank line
                paragraph
                    Active
                paragraph
                    TaxId.TaxNumber
            column4
                paragraph
                    blank line
                paragraph
                    blank line
                paragraph
                    TaxId.TaxNumberType
        header2 of "CurrentAddress"
        blank line
        Address
