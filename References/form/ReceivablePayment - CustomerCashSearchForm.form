CustomerCashSearchForm is a SearchForm
    Layout
        four column distributed
            Customer
                select is CustomerSelect
            Company
            ReceivableProcessLevel
                label is "ProcessLevel"
            ReceivablePaymentHeader.ReceivableLockBox
                label is "Lockbox"
        four column distributed
            Prepayment
            PaymentNumber
                show up to 16 characters
            ReferenceNumber
                label is "Reference"
                show up to 16 characters
            OriginalCurrency
        four column distributed
            DepositDateRange.Begin
                label is "FromDepositDate"
            DepositDateRange.End
                label is "ToDepositDate"
            TransactionAmountRange.FromAmount
                label is "FromTransactionAmount"
            TransactionAmountRange.ToAmount
                label is "ToTransactionAmount"
