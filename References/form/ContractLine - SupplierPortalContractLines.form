SupplierPortalContractLines is a CompositeForm
    title is "ContractLines"
    called out actions only
    context form is SupplierPortalContractLineContext
    show panel control on left
    Actions
        action is refresh
    Line is a Panel
        form is SupplierPortalContractLine
    Tiers is a Panel
        title is "Members*"
        visible when (IsManufacturerContractWithLineMembers)
        list is ContractLineMember set.ContractLineMembersPortal
    DistributorPricing is a Panel
        title is "Members*"
        visible when (IsDistributorContractWithLineMembers)
        list is ContractLineMember set.ContractLineDistPricingMembersPortal
    TierCosts is a Panel
        title is "TierCosts"
        visible when (ContractLineTierCostExists)
        list is ContractLineTierCostRel.ContractLineTierCostPortal
    Comments is a Panel
        title is "Comments"
        visible when (HasSupplierContractLineComments)
        list is SupplierContractLineCommentsRel.SupplierPortalContractLineCommentList
    Attachments is a Panel
        title is "Attachments"
        visible when (HasSupplierContractLineAttachments)
        list is SupplierContractLineAttachmentsRel.SupplierPortalContractLineAttachmentList
    Retainage is a Panel
        title is "Retainage"
        visible when (not Contract.RetainagePercentCompleteTracking.NoRetainage)
        form is Retainage
    AOC is a Panel
        title is "AOC"
        visible when (AOCs)
        list is ContractLineAOC set.SupplierPortalContractLineAOCs
    ChangeOrdersAndAllowances is a MultiListPanel
        visible when (Contract.AllowChangeOrder)
        list is ChangeOrderLineDetailRel.SupplierChangeOrderListForALine
            visible when (ChangeOrdersExist)
        list is ContractAllowance set.SupplierAllowancesForALine
    ComponentGroups is a Panel
        visible when (IsACapitatedProcedure)
        list is ContractCapitatedComponentGroup set.SupplierContractCapitatedComponentGroupsComp
    LineChangeOrderDetails is a Panel
        visible when (ChangeOrdersExistForCurrentVersionForService)
        list is ChangeOrderLineDetailRel.ChangeOrdersForALineCreate
    LineChangeOrderDetailsForNonService is a Panel
        title is "LineChangeOrderDetails"
        visible when (ChangeOrdersExistForCurrentVersionForNonService)
        list is ChangeOrderLineDetailRel.ChangeOrdersForALineCreateForNonService
