BirstModelQuerySourceField is a Form
    is primary
    Layout
        single column
            FieldType
            ReplicationSet
            RepSetBC
            BusinessClass
                label is "ReplicationSetBusinessClass"
                display only
            RelatedValue
                label is "ReplicationSetField"
            SQLName
            BirstFieldName
            IsAttribute
                label is "Attribute"
            IsMeasure
                label is "Measure"
            IncludeInBirstModel
                label is "IncludeInModel"
            IsDimensionKey
                label is "DimensionKey"
            visible when (TransientSourceType.Fact)
                IsForeignKey
                    label is "ForeignKey"
                valid when (IsForeignKey)
                    ForeignKeyBusClass
                        label is "RelatedBC"
            DataType
            valid when (DataTypeIsDate)
                AnalyzeByDate
            Width
            IsBooleanField
                label is "BooleanField"
            IsStateField
                label is "StateField"
            visible when (JoinType.None)
                FilterType
            visible when (FilterType.LiteralValue)
                FilterValue
            visible when (FilterType.None)
                JoinType
            visible when (not JoinType.None)
                JoinField
        two column
            column1
                visible when (not JoinType.None)
                    valid when (OverrideSQLName)
                        JoinFieldSQLName
            column2
                visible when (not JoinType.None)
                    OverrideSQLName
