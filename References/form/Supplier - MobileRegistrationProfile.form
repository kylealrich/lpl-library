MobileRegistrationProfile is a WizardForm
    use for action MobileSupplierCreate
    title is "SupplierRegistration"
    summary form is MobileRegistrationSummary
    show steps
    Credentialing is a Panel
        valid when (SupplierGroup.ShowCredentialingPanel)
        panel header is "<SupplierGroup.SupplierGroupExtension.VendormateCredentialText>"
        form is Credentialing
    Registration is a Panel
        panel header is "<SupplierGroup.CompanyInformationText>"
        form is MobileSupplierPortalRegistration
    BankInformation is a Panel
        valid when (SupplierGroup.ShowBankInformationPanel)
        panel header is "<SupplierGroup.BankInformationText>"
        form is BankInformationRegistrationForm
    DiversityCodes is a Panel
        valid when (SupplierGroup.ShowDiversityCodesPanel)
        panel header is "<SupplierGroup.CompanyDiversityText>"
        list is SupplierDiversityResponse set.MobileDiversityResponseList
    Questions is a Panel
        valid when (SupplierGroup.ShowQuestionsPanel)
        panel header is "<SupplierGroup.QuestionsText>"
        template is SupplierGroup.SupplierGroupQuestionsByDisplayOrder
            business class is SupplierQuestionResponse
                form is SupplierQResponse
    QuestionsConditional is a Panel
        valid when (ShowQuestionsPanel)
        panel header is "<SupplierGroup.QuestionsText>"
        template is SupplierQuestions set
            business class is SupplierQuestionsSetResponse
                form is SupplierQResponse
    CommodityCodes is a Panel
        valid when (SupplierGroup.CommoditySelectWithTree)
        mouse over text is "<SupplierGroup.CommodityCodesText>"
        panel header is "<SupplierGroup.CommodityCodesText>"
        list is PrimaryContact.SupplierCommodityCode set.MobileSupplierCommodityCode
            helper list is PrimaryContact.CommodityCodes.MobileCommodityCodeHelper
                action is AttachToContact
    CommodityCodesNoTree is a Panel
        title is "CommodityCodes"
        valid when (SupplierGroup.CommoditySelectLevelOne)
        mouse over text is "<SupplierGroup.CommodityCodesText>"
        panel header is "<SupplierGroup.CommodityCodesText>"
        list is PrimaryContact.SupplierCommodityCode set.MobileSupplierCommodityCode
            helper list is PrimaryContact.CommodityCodes.MobileCommodityCodeHelperNoTree
                action is AttachToContact
    Certifications is a Panel
        valid when (SupplierGroup.ShowCertificationsPanel)
        mouse over text is "<SupplierGroup.CertificationText>"
        panel header is "<SupplierGroup.CertificationText>"
        list is AllSupplierCertificationsRel.MobileSupplierSideCertifications
    ProxyNotifications is a Panel
        valid when (SupplierGroup.ShowProxyNotificationsPanel)
        mouse over text is "<SupplierGroup.ProxyNotificationsText>"
        panel header is "<SupplierGroup.ProxyNotificationsText>"
        list is PrimaryContact.SupplierContactProxy set.MobileSupplierContactProxyList
