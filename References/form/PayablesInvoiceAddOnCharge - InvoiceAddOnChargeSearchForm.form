InvoiceAddOnChargeSearchForm is a SearchForm
    title is "InvoiceAddOnCharges"
    Layout
        five column distributed
            Company
            GeneralLedgerCompanyGroup
                label is "CompanyGroup"
                show up to 10 characters
            Vendor
            PurchaseFromLocation
                show up to 10 characters
            AddOnCharge
        five column distributed
            InvoiceDateRange.Begin
                label is "FromInvoiceDate"
            InvoiceDateRange.End
                label is "ToInvoiceDate"
            PaymentDateRange.Begin
                label is "FromPaymentDate"
            PaymentDateRange.End
                label is "ToPaymentDate"
