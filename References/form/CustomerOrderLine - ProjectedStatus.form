ProjectedStatus is an ActionForm
    action is Unreleased.ProjectedStatus
    Layout
        paragraph
            PrmCompany
                label is "Company"
                display as text
            PrmCompany.Name
                no label
                display as text
        paragraph
            PrmInventoryLocation
                label is "InventoryLocation"
                display as text
            PrmInventoryLocation.Name
                no label
                display as text
        paragraph
            PrmItem
                label is "Item"
                display as text
            PrmItem.Description
                no label
                display as text
        paragraph
            PrmSellUOM
                label is "UOM"
                display as text
        paragraph
            PrmStockOnHandQuantity
                label is "StockOnHandQuantity"
                display as text
        paragraph
            PrmAvailableQuantity
                label is "AvailableQuantity"
                display as text
        paragraph
            PrmEstimatedTransactionDate
                label is "EstimatedTransactionDate"
                display as text
        paragraph
            PrmDemandSystemCode
                label is "SystemCode"
                display as text
