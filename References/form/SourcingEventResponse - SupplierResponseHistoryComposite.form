SupplierResponseHistoryComposite is a CompositeForm
    title is "ResponsesForSupplier\\:_<NotifiedSupplier.Supplier.SupplierName>_ForEventsFinalized_<SourcingEvent.FinalizedDate>"
    context form is SupplierResponseHistoryContext
        background is theme color
    restrict action Draft.Create
    restrict action Draft.Update
    restrict action Draft.Delete
    Actions
        action is refresh
    SupplierResponseHistoryPanel is a Panel
        title is "SourcingEventLineResponses"
        list is AllSourcingEventLineResponseRel.SupplierResponseList
