CompanyCustomerForm is a CompositeForm
    is primary
    title is "<CompanyCustomerPrimaryFormTitle>"
    context form is CustomerFormContext
        background is theme color
    Main is a Panel
        form is Main
    Address is a Panel
        form is Address
    Billing is a Panel
        form is Billing
    OrderEntry is a Panel
        form is OrderEntry
    ProcessingRules is a Panel
        form is ProcessingRules
    CashApplication is a Panel
        form is CashApplication
    Dunning is a Panel
        form is Dunning
    CustomerComments is a Panel
        list is ReceivableCommentsRel.CustomerCommentsList
    TransactionComments is a Panel
        list is ReceivableCommentsRel.TransactionCommentsList
    NationalAccountChildren is a Panel
        visible when (not NationalAccountIndicator.Child)
        list is NationalAccountCustomerRel.CompanyCustomerNationalAccountSpeedEntry
    ProcessLevelInclusion is a Panel
        list is CustomerProcessLevelsRel.CustomerProcessLevelInclusions
    Contacts is a Panel
        list is CompanyCustomerContactRel.CustomerContacts
    Banks is a Panel
        visible when (ElectronicFundsOrBOEProcessing)
        list is CustomerDraftBankRel.BanksList
    BankAccounts is a Panel
        list is CompanyCustomerBankAccountRel.CompanyCustomerBankAccountInlineList
    FundsTransfer is a Panel
        visible when (Company.CustomerGroupField.CustomerGroup.CompanyEFTProcessing)
        list is EFTNotificationRel.EFTNotificationList
    FundsTransferCustomers is a Panel
        visible when (Company.CustomerGroupField.CustomerGroup.CompanyEFTProcessing)
        list is ElectronicFundsTransferCustomerGroupRel.GroupEFTCustomerList
    Attachments is a Panel
        list is CompanyCustomerAttachment set.Attachments
    InvoiceTypes is a Panel
        list is CustomerInvoiceTypeCodesRel.InvoiceTypeCodeList
    AgePeriodBalance is a Panel
        visible when (ShowAgePeriodBalance)
        list is AgePeriodBalanceHeaderRel.primary
    Certifications is a Panel
        list is CustomerCertificationsRel.CustomerCertificationPanelList
    OrderEntryAttachments is a Panel
        visible when (OrderEntryCustomerExists)
        list is OrderEntryCustomerAttachmentRel.primary
