Main is a Form
    Layout
        two column
            column1
                paragraph
                    Provider
                        select is ActiveProvidersList
                display as text
                    paragraph
                        Provider.Name.FamilyName
                            label is "LastName"
                        Provider.Name.GivenName
                            label is "FirstName"
                        Provider.Name.MiddleName
                            label is "MiddleInitial"
            column2
                visible when (ValidProviderExists)
                    Provider.ActiveDisplay
                        no label
                        display as tag
                            color of red when (not Provider.Active)
                            color of green when (Provider.Active)
