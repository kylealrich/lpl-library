MobilePaymentEntryForm is a CompositeForm
    title is "<DerivedFormTitle>"
    context form is MobilePaymentEntry
    show panel control on left
    Main is a Panel
        form is Main
    Payee is a Panel
        title is "PayeeInformation"
        form is Payee
    Defaults is a Panel
        form is Defaults
    Bank is a Panel
        title is "BankInformation"
        form is Bank
    RemittanceComment is a Panel
        title is "Comment"
        form is RemittanceComment
    TransactionData is a Panel
        form is TransactionData
    CurrencyAmounts is a Panel
        title is "CurrencyAmounts"
        form is MobileCurrencyAmountsForm
    BankCurrencyAmounts is a Panel
        title is "BankCurrencyAmounts"
        visible when (BankCurrencyDiffersFromCurrency)
        form is BankCurrencyAmountsForm
    UserFieldsPanel is a Panel
        title is "UserFields"
        form is UserFieldForm
    DistributionList is a Panel
        title is "Distributions"
        list is CashLedgerGLDistributionsRel.MobilePaymentGLDistributionList
    DistributionsNotImported is a Panel
        visible when (ImportInProcess)
        list is CashLedgerPaymentImportInErrorRel.primary
    ApprovalComments is a Panel
        visible when (HasApprovalComments)
        list is CashLedgerPaymentReasonCodeUsage set.CashLedgerPaymentReasonCodeUsageList
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
    BudgetErrors is a Panel
        title is "BudgetErrors<DerivedHeaderBudgetErrorCount>"
        visible when (HeaderBudgetEditErrorExists)
        list is HeaderBudgetEditErrorRel.primary
    WarningBudgetErrors is a Panel
        title is "WarningBudgetErrors<DerivedWarningBudgetErrorCount>"
        visible when (DistributionBudgetEditErrorExists)
        list is DistributionBudgetEditErrorRel.primary
    Errors is a Panel
        title is "Errors<DerivedCashLedgerPaymentErrorCount>"
        visible when (CashLedgerPaymentErrorsExist)
        list is CashLedgerTransactionError set.CashLedgerPaymentErrorList
