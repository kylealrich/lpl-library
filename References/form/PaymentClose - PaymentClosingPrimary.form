PaymentClosingPrimary is a Form
    is primary
    title is "PaymentClose"
    Layout
        paragraph
            VendorGroup
            VendorGroup.Description
                no label
                display as text
        blank line
        paragraph
            PayGroup
                select is PayGroups
            PayGroup.Name
                no label
                display as text
        blank line
        paragraph
            ProcGrp
                label is "ProcessGroup"
                select is ProcessGroups
            ProcGrp.Description
                no label
                display as text
        blank line
        single column
            ProcLevelSeq
                label is "ProcessLevelSequence"
        blank line
        single column
            IncludeVoucherNumber
                label is "IncludeVoucherNumber"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                PaymentCloseReport_RDG
            column2 of 25%
                PaymentCloseReport_XPT
