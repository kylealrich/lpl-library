ReconciliationVarianceAdjustmentContextForm is a Form
    title is "ReconciliationVarianceAdjustmentDistribution"
    Layout
        display as text
            two column distributed
                column1
                    paragraph
                        CashCode.CashCodeAndDescription
                            label is "CashCode"
                        CashCode.Currency
                            no label
                    CashLedgerReconciliationAdjustmentRel.TransactionNumber
                        label is "TransactionNumber"
                    paragraph
                        CashLedgerReconciliationAdjustmentRel.ReconciliationData.CurrencyAmount
                            label is "ReconciledAmount"
                        CashLedgerReconciliationAdjustmentRel.IssuedBankAmount
                            label is "IssuedAmount"
                        visible when (CashLedgerReconciliationAdjustmentRel.IsUnreleased)
                            CashLedgerReconciliationAdjustmentRel.DerivedBankDifferenceAmount
                                label is "Difference"
                            CashLedgerReconciliationAdjustmentRel.BalanceAmount
                                label is "RemainingBalance"
                column2
                    CashLedgerGLDistribution.BankTransactionCode.BankTransactionCodeName
                        label is "TransactionCode"
                    paragraph
                        CashLedgerReconciliationAdjustmentRel.ReconciliationData.ExchangeDate
                            label is "ReconciledDate"
                        CashLedgerReconciliationAdjustmentRel.PostDate
