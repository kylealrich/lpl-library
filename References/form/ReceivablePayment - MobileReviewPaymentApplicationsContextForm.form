MobileReviewPaymentApplicationsContextForm is a Form
    title is "ReviewApplications"
    Layout
        display as text
            two column distributed
                Company
                Company.Name
                    label is "CompanyName"
            paragraph
                ReceivablePaymentHeader
                    label is "Batch"
                Company.Currency
                    label is "Currency"
            two column distributed
                ReceivablePaymentHeader.Status
                    label is "Status"
                PaymentOpen
            paragraph
                PaymentNumber
                TypeOfPayment
            paragraph
                OriginalCurrency
                DerivedMessage
                    no label
            two column
                Customer
                Customer.Name
                    label is "CustomerName"
            paragraph
                DepositDate
                GeneralLedgerDate
                    label is "GlobalLedgerDate"
            paragraph
                TransactionAmount
                ReceivableProcessLevel
                    label is "ProcessLevel"
