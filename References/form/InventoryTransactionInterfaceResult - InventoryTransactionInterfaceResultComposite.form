InventoryTransactionInterfaceResultComposite is a CompositeForm
    is primary
    title is "InventoryTransactionInterfaceResult"
    context form is InventoryTransactionInterfaceResultContext
    show panel control on left
    Actions
        action is RerunForErrors
            hide when invalid
        action is Delete
    InventoryTransactionsCreated is a Panel
        visible when (not NoRecordsToProcess)
        list is InventoryTransactionsRel.InterfaceListing
    PendingInventoryTransactionsForRunGroup is a Panel
        visible when (ShowRunGroupPanel)
        list is RunGroupProcessedInventoryTransactionImportRel.InventoryTransactionImportListForResults
    PendingInventoryTransactionsForErrorRunGroup is a Panel
        visible when (ShowErrorRunGroupPanel)
        list is ErrorRunGroupInventoryTransactionImportRel.InventoryTransactionImportListForResults
