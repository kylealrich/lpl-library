MobilePOReceivingInspectionLineDetailListContext is a Form
    title is "InspectionLineDetails"
    Layout
        visible when (LineExists)
            paragraph distributed
                PurchaseOrderReceipt
                    label is "Receiver"
                    display as text
                ReceiptLinesForTemplateRel.PurchaseOrderLine.LineNumber
                    label is "Line"
                    display as text
                PurchaseOrderReceiptLine.Item
                    label is "Item"
                    display as text
                PurchaseOrderReceiptLine.PurchaseOrderLine.VendorItem
                    display as text
        visible when (not LineExists)
            paragraph distributed
                ReceiptLinesForTemplateRel.PurchaseOrderReceipt
                    label is "Receiver"
                    display as text
                ReceiptLinesForTemplateRel.PurchaseOrderLine.LineNumber
                    label is "Line"
                    display as text
                ReceiptLinesForTemplateRel.Item
                    label is "Item"
                    display as text
                ReceiptLinesForTemplateRel.PurchaseOrderLine.VendorItem
                    display as text
        paragraph
            QuantityToInspect
                label is "QuantityToInspect"
                display as text
