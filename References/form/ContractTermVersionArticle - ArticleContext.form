ArticleContext is a Form
    title is "ContractArticle"
    Layout
        paragraph
            ContractGroup
            ContractGroup.ProcurementGroup.Description
                no label
                display as text
        paragraph
            Contract
                display only
            Contract.Name
                no label
                display only
        paragraph
            Contract.Supplier
                label is "Supplier"
                display only
            Contract.Supplier.SupplierName
                no label
                display as text
            Contract.ContractStatus
                display only
        paragraph
            Title
                label is "ArticleTitle"
