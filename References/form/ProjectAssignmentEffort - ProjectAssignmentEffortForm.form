ProjectAssignmentEffortForm is a Form
    is primary
    title is "<FinanceEnterpriseGroup.ProjectLabel>AssignmentEffort"
    Layout
        single column
            ProjectEffortSchedule
                display as text
            ProjectEffortPeriod
                display as text
            paragraph
                Employee
                    display as text
                Employee.Employee.Name.PreferredFirstAndLastName
                    no label
                    display as text
            paragraph
                Project
                    label is "<FinanceEnterpriseGroup.ProjectLabel>"
                    display as text
                Project.Description
                    no label
                    display as text
            paragraph
                ProjectAssignment.ProjectDateRange.BeginDate
                    label is "AssignmentDates"
                    display as text
                ProjectAssignment.ProjectDateRange.EndDate
                    label is "-"
                    display as text
            paragraph
                DateRange.Begin
                    label is "EffortPeriod"
                    display as text
                DateRange.End
                    label is "-"
                    display as text
            AveragePlannedPercent
                display as text
            AverageActualPercent
                display as text
            visible when (Certified)
                EffortPercent
                    display as text
            visible when (not Certified)
                EffortPercent
            Certified
                display as text
