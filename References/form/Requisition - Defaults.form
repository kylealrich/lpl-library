Defaults is a Form
    restrict action Unreleased.UpdateRequisitionLineFillOrKill
    Layout
        single column
            DefaultDistributionAccount
                label is "DistributionAccount"
                compact format
            visible when (ManualEncumbrancesMayExist)
                DefaultGLEncumbranceReference
                    label is "GLEncumbranceReference"
        two column distributed
            paragraph
                visible when (CreatedBySupplier)
                    Vendor
                        display as text
                visible when (not CreatedBySupplier)
                    Vendor
                        select is ActiveVendorsByName
                Vendor.VendorName
                    no label
                    display as text
            paragraph
                PurchaseFromLocation
                    label is "PurchaseFrom"
                    select is NonRemitToActivePurchaseFromLocations
                PurchaseFromLocation.VendorName
                    no label
                    display as text
        two column distributed
            TransactionCurrencyCode
                label is "CurrencyCode"
            paragraph
                Buyer
                Buyer.Employee.FirstLastName
                    no label
                    display as text
                visible when (HasBuyer)
                    button of icon.profile
                        link is MobileBuyerInformation
        two column distributed
            paragraph
                visible when (false)
                    TransientTaxEntity
                PurchaseTaxable
                    label is "Taxable"
                visible when (PurchaseTaxable)
                    PurchaseTaxCode
                        label is "TaxCode"
                        select is ValidTaxCodesForEntity
                    PurchaseTaxCode.Description
                        no label
                        display as text
            SourcingEventRequired
        two column distributed
            DefaultCommentCode
                label is "CommentCode"
            DefaultBillCode
                label is "BillCode"
        two column distributed
            POCode
            paragraph
                CommodityCode
                CommodityCode.CcDescription
                    no label
                    display as text
        two column distributed
            DeliveryMethod
