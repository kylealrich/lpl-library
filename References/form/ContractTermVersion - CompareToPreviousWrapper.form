CompareToPreviousWrapper is a SummaryForm
    title is "TermsAndConditionsChanges"
    Layout
        header2 of "ContractInformation"
        two column
            Contract
            Contract.Name
        two column
            Contract.WorkingContractID
            Contract.WorkingContractType
        two column
            SupplierAndName
                label is "Supplier"
            Contract.SupplierSourceId.MainContact.FirstAndLastName
                label is "SupplierContact"
        blank line
        header2 of "TermsAndConditions"
        visible when (not Contract.HasActivatedContractTermVersions)
            form is ContractArticlesByDisplayRel.CompareCurrentToOriginal
        visible when (Contract.HasActivatedContractTermVersions)
            form is ContractArticlesByDisplayRel.CompareToPreviousActivated
