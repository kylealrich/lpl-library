SupplierPortalMetrics is a Form
    title is "EventMetrics"
    Actions
        action is refresh
    Layout
        display as text
            header2 of "MyEventMetrics"
            four column table
                column1
                    NumberOfClosedEventInvites
                        label is "EventNotifications"
                    NumberOfEventsNoBid
                        label is "EventsNoBid"
                    TotalParticipation
                        label is "PercentEventsBid"
                column2
                    TotalSourceIdEventsBid
                        label is "EventsBid"
                    TotalSourceIdEventAwards
                        label is "EventsAwarded"
                    TotalSourceIdEventBidSuccess
                        label is "BidSuccess"
                column3
                    TotalSourceIdLinesBid
                        label is "LinesBid"
                    TotalSourceIdAwardedLines
                        label is "LinesAwarded"
                    TotalSourceIdLineBidSuccess
                        label is "LineBidSuccess"
                column4
                    TotalSourceIdBid
                        label is "AmountBid"
                    TotalSourceIdAwarded
                        label is "AmountAwarded"
                    TotalSourceIdAmountBidSuccess
                        label is "AmountSuccess"
            visible when (Supplier.MultipleContact)
                header2 of "SupplierEventMetrics"
                four column table
                    column1
                        Supplier.NumberOfClosedEventInvites
                            label is "EventNotifications"
                        Supplier.EventResponsesNoBid
                            label is "EventsNoBid"
                        Supplier.TotalParticipation
                            label is "PercentEventsBid"
                    column2
                        Supplier.TotalSourceIdEventsBid
                            label is "EventsBid"
                        Supplier.TotalSourceIdEventAwards
                            label is "EventsAwarded"
                        Supplier.TotalSourceIdEventBidSuccess
                            label is "BidSuccess"
                    column3
                        Supplier.TotalSourceIdLinesBid
                            label is "LinesBid"
                        Supplier.TotalSourceIdAwardedLines
                            label is "LinesAwarded"
                        Supplier.TotalSourceIdLineBidSuccess
                            label is "LineBidSuccess"
                    column4
                        Supplier.TotalSourceIdBid
                            label is "AmountBid"
                        Supplier.TotalSourceIdAwarded
                            label is "AmountAwarded"
                        Supplier.TotalSourceIdAmountBidSuccess
                            label is "AmountSuccess"
