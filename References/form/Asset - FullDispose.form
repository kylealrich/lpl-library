FullDispose is an ActionForm
    action is FullDispose
        on completion navigate to MAsset
    Layout
        visible when (LeasedAsset)
            paragraph
                AssetLease.LeaseCompany
                    label is "LeaseCompany"
                AssetLease.LeaseCompany.Name
                    no label
                    display as text
            paragraph
                AssetLease.Lease
                AssetLease.Lease.Description
                    no label
                    display as text
        single column
            PrmQuickDisposal
                label is "QuickDisposal"
                    left of field
            PrmDisposalDate
                label is "DisposalDate"
            PrmDisposalMethod
                label is "Method"
            PrmDisposalProperty
                label is "Property"
            PrmProceedsCurrency
                label is "ProceedsCurrency"
            PrmProceeds
                label is "ProceedsAmount"
            PrmCostOfSaleCurrency
                label is "ExpenseCurrency"
            PrmCostOfSale
                label is "ExpenseAmount"
        single column
            PrmRelease
                label is "Release"
