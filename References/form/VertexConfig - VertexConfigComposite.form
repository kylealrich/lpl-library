VertexConfigComposite is a CompositeForm
    is primary
    title is "VertexConfiguration"
    context form is VertexConfigContext
        background is theme color
    Versions is a Panel
        list is VertexVersionRel.VertexConfigVersionList
    TaxLog is a Panel
        visible when (LogFileRecordsExist)
        list is TaxLogHeaderRel.TaxLogHeaderList
    TaxConfigurations is a Panel
        list is TaxConfigurationRel.TaxConfigurationList2
    TaxConfigCommentText is a Panel
        visible when (TaxConfigurationRel.CommentText entered)
        form is CommentTextForm
