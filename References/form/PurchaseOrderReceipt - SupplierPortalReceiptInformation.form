SupplierPortalReceiptInformation is a CompositeForm
    title is "Receipt<PurchaseOrderReceipt>Details"
    called out actions only
    context form is SupplierPortalContext
        background is theme color
    Actions
        action is previous record
            hide when invalid
        action is next record
            hide when invalid
    ReceiptLines is a Panel
        list is PurchaseOrderReceiptLine set.SupplierPortalPurchaseOrderReceiptLines
    PurchaseOrder is a Panel
        visible when (SupplierShowPO)
        list is PurchaseOrderRel.SupplierPortalPurchaseOrders
    Invoices is a Panel
        visible when (SupplierShowInvoice)
        list is MatchPurchaseOrderInvoicesRel.SupplierPortalInvoices
