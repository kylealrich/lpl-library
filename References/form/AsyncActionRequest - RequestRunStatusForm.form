RequestRunStatusForm is a Form
    title is "RequestRunStatus"
    Layout
        display as text
            visible when (not PendingScheduling)
                header2 of "NotPendingScheduling"
                single column
                    visible when (IsScheduled)
                        visible when (SystemRequest.AutoDisable)
                            text of "RequestIsSetToAutoDisable.ItWillBeEnabledByTheSystemWhenAppropriate."
                        visible when (not SystemRequest.AutoDisable)
                            text of "RequestHasCompletedAllScheduledExecutionTimesOrIsDisabled."
                                bold
                                color of red
                    visible when (not IsScheduled)
                        text of "RequestIsARunOnceScheduleAndHasEitherCompletedOrHasBeenManuallyDisabled."
                            color of green
            visible when (PendingScheduling)
                header2 of "PendingScheduling"
                single column
                    text of "RequestHasPendingExecutions."
                        bold
                        color of green
                visible when (FutureExecution)
                    two column
                        TimeToExec
                            label is "NextScheduledRun"
                            header3
                            bold
                            color of green
                        CurrentTime
                            label is "CurrentTimeAtFormLoad"
                            header3
                            bold
                visible when (SynchronizedGroupSuspended)
                    text of untranslatable:"<SynchronizedGroupSuspendedMessage>."
                        bold
                        color of red
                visible when (WaitingOnSynchronizedMembers)
                    text of untranslatable:"<WaitingOnSynchronizedMembersMessage>."
                        bold
                        color of red
                visible when (WaitingOnRequest)
                    text of untranslatable:"<WaitingOnRequestMessage>."
                        bold
                        color of red
                visible when (not AnyTypeUpgradeRequest)
                    visible when (UnfinishedSystemUpgradeDataAreaRequestsRelExists)
                        text of "ActionCannotRunUntilSystemDataAreaUpgradeRequestsComplete."
                            bold
                            color of red
                    visible when (UnfinishedSystemUpgradeSystemRequestsRelExists)
                        text of "ActionCannotRunUntilSystemUpgradeRequestsComplete."
                            bold
                            color of red
                visible when (DerivedAsyncStatusMT)
                    visible when (not DerivedAsyncStatusMTRunning)
                        text of "Multi-tenantAsyncProcessorIsNotRunning"
                            bold
                            color of red
                visible when (DerivedAsyncStatusTenantStateNotRunning)
                    text of "TenantAsyncProcessorIsNotRunning"
                        bold
                        color of red
                visible when (DerivedAsyncStatusTenantStateRunning)
                    visible when (DerivedAsyncStatusTenantWarn)
                        text of "TenantAsyncProcessorIsRunning:<DerivedAsyncStatusTenantMessage>."
                            color of yellow
                visible when (DerivedAsyncStatusTenantStateSuspended)
                    text of "TenantAsyncProcessorIsSuspended"
                        bold
                        color of red
                visible when (DerivedAsyncStatusTenantStateError)
                    text of "TenantAsyncProcessorIsInError"
                        bold
                        color of red
                visible when (not DerivedAsyncStatusTenantStateNotRunning)
                    visible when (DerivedAsyncStatusDataAreaNotRunning)
                        text of "DataAreaIsNotRunning"
                            bold
                            color of red
                    visible when (DerivedAsyncStatusDataAreaSuspended)
                        text of "DataAreaProcessorIsSuspended"
                            bold
                            color of red
                    visible when (DerivedAsyncStatusDataAreaError)
                        text of "DataAreaIsInError"
                            bold
                            color of red
                    visible when (RequestThresholdExceeded)
                        text of "CurrentlyRunningActionCountMeetsOrExceedsMaximumAllowed"
                            bold
                            color of red
                    visible when (DerivedAsyncStatusProcReqsNotRunning)
                        text of "ProcessRequestsSweepIsNotRunning"
                            bold
                            color of red
                    visible when (DerivedAsyncStatusProcReqsSuspended)
                        text of "ProcessRequestsSweepIsSuspended"
                            bold
                            color of red
                    visible when (DerivedAsyncStatusProcReqsError)
                        text of "ProcessRequestsSweepIsInError"
                            bold
                            color of red
                    visible when (DerivedAsyncStatusProcQueuesNotRunning)
                        text of "ProcessQueuesSweepIsNotRunning"
                            bold
                            color of red
                    visible when (DerivedAsyncStatusProcQueuesSuspended)
                        text of "ProcessQueuesSweepIsSuspended"
                            bold
                            color of red
                    visible when (DerivedAsyncStatusProcQueuesError)
                        text of "ProcessQueuesSweepIsInError"
                            bold
                            color of red
                    visible when (DerivedAsyncStatusConsCheckNotRunning)
                        text of "ConsistencySweepIsNotRunning"
                            color of red
                    visible when (DerivedAsyncStatusConsCheckSuspended)
                        text of "ConsistencySweepIsSuspended"
                            bold
                            color of red
                    visible when (DerivedAsyncStatusConsCheckError)
                        text of "ConsistencySweepIsInError"
                            bold
                            color of red
                    visible when (DerivedAsyncStatusBatchNotRunning)
                        text of "BatchExecutorNodeIsNotRunning"
                            bold
                            color of red
                    visible when (DerivedAsyncStatusBatchError)
                        text of "BatchExecutorNodeIsInError"
                            bold
                            color of red
                    visible when (DerivedAsyncStatusDocumentNotRunning)
                        text of "DocumentExecutorNodeIsNotRunning"
                            bold
                            color of red
                    visible when (DerivedAsyncStatusDocumentError)
                        text of "DocumentExecutorNodeIsInError"
                            bold
                            color of red
            visible when (NonFinishedTriggersExist)
                text of "RequestHasUnfinishedTriggers."
                    link is ViewTriggers
