ProjectContractInvoicesNav is a Form
    title is "<FinanceEnterpriseGroup.ProjectLabel>ContractInvoices"
    Layout
        paragraph
            ProjectContract.DisplayContract
                label is "<FinanceEnterpriseGroup.ProjectLabel>Contract"
                display as text
            ProjectContract.Project.Description
                no label
                display as text
        paragraph
            RunGroup
                display as text
            FinanceDimension2.DisplayDimension
                label is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
                display as text
            FinanceDimension2.CompanyCustomerGroup.ReceivableCompany
                label is "Company"
                display as text
            FinanceDimension2.CompanyCustomerGroup.Customer
                label is "Customer"
                display as text
        paragraph
            ProjectContractInvoice
                label is "Invoice"
                display as text
            InvoiceDate
                display as text
            Status
                display as text
        paragraph
            InvoiceTotalAmount
                label is "TotalAmount"
                display as text
            InvoiceTotalTaxAmount
                label is "TaxAmount"
                display as text
            InvoiceTotalAmountaAndTaxAmount
                label is "TotalInvoicedAmount"
                display as text
        paragraph
            InvoiceBilledAmount
                label is "TotalBilledAmount"
                display as text
            InvoiceTotalFees
                display as text
            InvoiceTotalAllowance
                label is "TotalAllowance"
                display as text
        single column
            InvoiceTotalAdjustment
                label is "Adjustments"
                display as text
        paragraph
            button of "ViewInvoice"
                visible when (GeneratedInvoiceExists)
                view GeneratedInvoice.File
            button of "ViewInvoice"
                visible when (GeneratedIDMInvoiceExists)
                link is external "<DerivedIDMInvoiceLink>"
                display as link
        button of "ViewInvoiceURL"
            visible when (ThirdPartyInvoicePrinted)
            link is external "<VendorInvoiceLink>"
            display as link
