AssetFundForm is a Form
    is primary
    title is "AssetFund"
    Layout
        single column
            paragraph
                Asset
                Asset.Description
                    no label
            paragraph
                AccountingEntity
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>"
                AccountingEntity.Name
                    no label
            visible when (AccountingUnitSelected)
                paragraph
                    AccountingUnit
                        label is "<FinanceEnterpriseGroup.AccountingUnitLabel>"
                    AccountingUnit.Description
                        no label
            visible when (ProjectSelected)
                paragraph
                    Project
                        label is "<FinanceEnterpriseGroup.ProjectLabel>"
                    Project.Description
            visible when (FinanceDimension1Selected)
                paragraph
                    FinanceDimension1
                        label is "<FinanceEnterpriseGroup.FinanceDimension1Label>"
                    FinanceDimension1.Description
            visible when (FinanceDimension2Selected)
                paragraph
                    FinanceDimension2
                        label is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
                    FinanceDimension2.Description
            visible when (FinanceDimension3Selected)
                paragraph
                    FinanceDimension3
                        label is "<FinanceEnterpriseGroup.FinanceDimension3Label>"
                    FinanceDimension3.Description
            visible when (FinanceDimension4Selected)
                paragraph
                    FinanceDimension4
                        label is "<FinanceEnterpriseGroup.FinanceDimension4Label>"
                    FinanceDimension4.Description
            visible when (FinanceDimension5Selected)
                paragraph
                    FinanceDimension5
                        label is "<FinanceEnterpriseGroup.FinanceDimension5Label>"
                    FinanceDimension5.Description
            visible when (FinanceDimension6Selected)
                paragraph
                    FinanceDimension6
                        label is "<FinanceEnterpriseGroup.FinanceDimension6Label>"
                    FinanceDimension6.Description
            visible when (FinanceDimension7Selected)
                paragraph
                    FinanceDimension7
                        label is "<FinanceEnterpriseGroup.FinanceDimension7Label>"
                    FinanceDimension7.Description
            visible when (FinanceDimension8Selected)
                paragraph
                    FinanceDimension8
                        label is "<FinanceEnterpriseGroup.FinanceDimension8Label>"
                    FinanceDimension8.Description
            visible when (FinanceDimension9Selected)
                paragraph
                    FinanceDimension9
                        label is "<FinanceEnterpriseGroup.FinanceDimension9Label>"
                    FinanceDimension9.Description
            visible when (FinanceDimension10Selected)
                paragraph
                    FinanceDimension10
                        label is "<FinanceEnterpriseGroup.FinanceDimension10Label>"
                    FinanceDimension10.Description
            paragraph
                TransactionAmount
                    label is "Amount"
                    display only
                visible when (not FundOverride)
                    PercentContribution
                        display as text
                visible when (FundOverride)
                    PercentContribution
