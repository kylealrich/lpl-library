FileCreationSetupDetailPanelForm is a Form
    title is "_"
    Layout
        two column distributed
            column1
                ExportFile
            column2
                Active
        paragraph
            button of "<LastGeneration>"
                link is LastFileNav
                label is "Last_File:"
                display as link
        single column
            visible when (ApplicationBusinessClass)
                NextTimeToGeneratePreviewRecords
                    label is "NextGeneratePreviewRecords"
        visible when (DisplayGenerateFileThreaded)
            paragraph
                button of "GenerateFile"
                    action is GenerateFileThreaded
                    is primary
        visible when (GeneratePreviewRecordsValid)
            paragraph
                button of "GeneratePreviewRecords"
                    action is GeneratePreviewRecords
                    is primary
