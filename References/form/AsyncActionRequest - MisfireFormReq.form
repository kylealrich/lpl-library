MisfireFormReq is a Form
    title is "ActionFailedToRunOnTime"
    Layout
        header2 of "ActionFailedToRunOnTime"
        paragraph
            valid when (not DisableMisfire)
                MisfireStrategy
                    label is "Strategy"
            MisfireReportMode
                label is "ReportOptions"
            MisfireThreshold
                label is "ThresholdMinutes"
            ContinueOnMisfire
                label is "ContinueOnFailureToRun"
        header2 of "RetryWhenActionFails"
        paragraph
            NoOfRetries
                label is "NumberOfRetries"
