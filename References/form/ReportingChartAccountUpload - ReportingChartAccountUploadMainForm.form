ReportingChartAccountUploadMainForm is a Form
    title is "Main"
    Layout
        three column distributed
            column1
                ParentAccount
                    label is "Parent<FinanceEnterpriseGroup.AccountLabel>"
                ChartType
                paragraph
                    TranslationCode
                    Revalue
                    RequireProject
                        label is "Require_<FinanceEnterpriseGroup.ProjectLabel>"
            column2
                paragraph
                    AccountType
                    AccountSubType
                ChartSection
                XbrlTag
            column3
                visible when (not ChartType.Summary)
                    paragraph
                        AccountActive
                        ChartAccountActive
                DisplayOrder
                    label is "DisplayOrderWithinALevel"
                DisplayAccount
                    label is "Display<FinanceEnterpriseGroup.AccountLabel>"
        three column distributed
            visible when (ChartType.Summary)
                SummaryAccount
                    label is "Summary<FinanceEnterpriseGroup.AccountLabel>"
                AccountDescription
                    label is "Summary<FinanceEnterpriseGroup.AccountLabel>Description"
                BudgetIdentifier
        three column distributed
            visible when (not ChartType.Summary)
                column1
                    Account
                        label is "Posting<FinanceEnterpriseGroup.AccountLabel>"
                    SubAccount
                        label is "PostingSub<FinanceEnterpriseGroup.AccountLabel>"
            visible when (not ChartType.Summary)
                column2
                    AccountDescription
                        label is "<FinanceEnterpriseGroup.AccountLabel>Description"
                    SubAccountDescription
                        label is "Sub<FinanceEnterpriseGroup.AccountLabel>Description"
        three column distributed
            visible when (ChartType.Posting)
                IncludeInBudgetEdit
                AllowUnits
                NaturalBalance
                InterfaceInDetail
                IsReconcilable
        three column distributed
            UnrealizedGainAccount
                label is "UnrealizedGain<FinanceEnterpriseGroup.AccountLabel>"
            UnrealizedLossAccount
                label is "UnrealizedLoss<FinanceEnterpriseGroup.AccountLabel>"
        three column distributed
            TranslationGainAccount
                label is "TranslationGain<FinanceEnterpriseGroup.AccountLabel>"
            TranslationLossAccount
                label is "TranslationLoss<FinanceEnterpriseGroup.AccountLabel>"
            visible when (ChartSection.BalanceSheet)
                paragraph
                    CloseToAccount
