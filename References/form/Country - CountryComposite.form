CountryComposite is a CompositeForm
    is primary
    title is "Country/\\Jurisdiction<Country>"
    context form is Country
    AddressRules is a Panel
        form is AddressRules
    StateProvinces is a Panel
        title is "States/\\Provinces"
        visible when (CountryExists)
        list is StateProvince set.StateProvincesForCountry
    Regions is a Panel
        visible when (CountryExists)
        list is Region set.RegionsForCountry
    AddressEntryTest is a Panel
        form is AddressEntryTest
    ExpandedAddressEntryTest is a Panel
        form is V2AddressEntryTest
