FaxConfigurationForm is a Form
    is primary
    title is "FaxConfiguration"
    Layout
        paragraph
            FinanceEnterpriseGroup
            FinanceEnterpriseGroup.EnterpriseGroup.Description
                no label
                display as text
        header2 of "FaxConfigurationVendor"
        paragraph
            FaxConfiguration
            Description
            Default
            Configuration
            blank line
        visible when (Configuration.Efax)
            header2 of "AccountDetails"
            two column
                column1
                    AccountID
                    UserName
                    FaxPassword
                        hidden entry
                column2
                    blank line
                    blank line
                    ConfirmPassword
                        hidden entry
            header2 of "DispositionDefaults"
            paragraph
                Priority
                    label is "FaxPriority"
            paragraph
                Language
                    label is "Language"
            paragraph
                Level
                    label is "Level"
            paragraph
                Method
                    label is "Method"
        visible when (Configuration.Consensus)
            header2 of "AccountDetails"
            two column
                column1
                    AccountID
                        label is "App\\I\\D"
                    UserName
                        label is "User\\I\\D"
                    FaxPassword
                        label is "A\\P\\IKey"
                        hidden entry
                column2
                    blank line
                    blank line
                    ConfirmPassword
                        label is "ConfirmA\\P\\IKey"
                        hidden entry
        header2 of "NotificationSettings"
        paragraph
            NotificationType
            visible when (not NotificationType.None)
                NotificationLevel
            visible when (NotificationType.Email)
                FromNotificationEmail
                    label is "FromEmail"
                ToNotificationEmail
                    label is "ToEmail"
            visible when (NotificationType.Both)
                FromNotificationEmail
                    label is "FromEmail"
                ToNotificationEmail
                    label is "ToEmail"
