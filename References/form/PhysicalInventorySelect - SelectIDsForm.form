SelectIDsForm is a CompositeForm
    is primary
    title is "ManageSelectID"
    context form is SelectIDsFormContext
        background is theme color
    Initial is a Panel
        form is Initial
    InventoryClasses is a Panel
        form is InventoryClasses
    BinGroups is a Panel
        form is BinGroups
    Logic is a Panel
        form is Logic
    Items is a Panel
        form is Items
    CountsByPage is a Panel
        title is "EnterCountsByPage"
        visible when (IsSelectIdInUseOrCountInProgress)
        list is PhysicalInventoryFreezesRel.LinesByPage
    CountsByItem is a Panel
        title is "EnterCountsByItem"
        visible when (IsSelectIdInUseOrCountInProgress)
        list is PhysicalInventoryFreezesRel.LinesByItem
    CountsByPhysicalInventoryBin is a Panel
        title is "EnterCountsByPhysicalInventoryBin"
        visible when (PhysicalInventoryBinInUseOrCountInProgress)
        list is PhysicalInventoryFreezeBinRel.LinesByPhysicalInventoryBin
    VarianceReasons is a Panel
        title is "EnterVarianceReasons"
        visible when (UnexplainedVariancesOnly)
        list is PhysicalInventoryFreezesWithVarianceRel.EnterVarianceReasons
