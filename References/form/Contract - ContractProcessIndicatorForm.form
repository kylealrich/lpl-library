ContractProcessIndicatorForm is a Form
    print form is Contract
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        single column
            visible when (DisplayProcessIndicator)
                header2 of "Progress"
                first PfiWorkunitRel.PfiProcessIndicatorRel.Model
                    no label
                    display as text
            visible when (DisplayFinalProcessIndicator)
                header2 of "Progress"
                first PfiWorkunitRel2.PfiProcessIndicatorRel.Model
                    no label
                    display as text
            visible when (DisplayNegotiationsProcessIndicator)
                header2 of "Progress"
                first PfiWorkunitRel3.PfiProcessIndicatorRel.Model
                    no label
                    display as text
            visible when (DisplayNegotiationsNoUpdateProcessIndicator)
                header2 of "Progress"
                first PfiWorkunitRel4.PfiProcessIndicatorRel.Model
                    no label
                    display as text
        header2 of "ContractDetails"
        form is Contract
