BankAccountPaymentHistory is an ActionForm
    action is BankAccountPaymentHistory
    Layout
        single column
            VendorGroup
        two column
            PayGroup
                select is PayGroups
            PayGroup.Name
                no label
                display as text
        two column
            CashCode
            CashCode.Description
                no label
                display as text
        blank line
        paragraph
            FrCheckDate
                label is "FromPaymentDate"
            ToCheckDate
                label is "ToPaymentDate"
        paragraph
            FrCheckNum
                label is "FromPaymentNumber"
            ToCheckNum
                label is "ToPaymentNumber"
        single column
            PaymentCode
                select is CashPayments
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                BankAccountPaymentHistoryReport_RDG
            column2 of 25%
                BankAccountPaymentHistoryReport_XPT
