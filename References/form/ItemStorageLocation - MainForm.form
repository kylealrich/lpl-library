MainForm is a Form
    Layout
        visible when (not RecordExists)
            three column distributed
                column1
                    paragraph
                        Company
                        Company.Name
                            no label
                            display as text
                    paragraph
                        InventoryLocation
                            label is "Location"
                            select is InventoryLocationsRel.InventoryLocationList
                        InventoryLocation.Name
                            no label
                            display as text
                    paragraph
                        Item
                        Item.Description
                            no label
                            display as text
                column2
                    paragraph
                        WarehouseStorageLocation
                            label is "Storage_Location"
                            select is WarehouseIsNotAStagingLocationRel.WarehouseStorageLocationList
                        WarehouseStorageLocation.Name
                            no label
                            display as text
                    paragraph
                        WarehouseStorageLocation.StorageLocationType
                            display as text
                column3
                    paragraph
                        Active
        two column distributed
            display as text
                column1
                    header2 of "Attributes"
                    paragraph
                        DerivedWarehouseZoneRepresentativeText
                            label is "Warehouse_Zone"
                    paragraph
                        DerivedWarehouseZoneComponentRepresentativeText
                            label is "Storage_System_Unit"
                    paragraph
                        DerivedWarehouseComponentTypeAndDescription
                            label is "Storage_System"
                    paragraph
                        DerivedAisle
                            label is "Warehouse_Aisle"
                    paragraph
                        WarehouseStorageLocation.Row
                column2
                    visible when (HasBinStorageCode)
                        header2 of "Capacity"
                        paragraph
                            DerivedStorageCodeAndDescription
                                label is "Storage_Code"
                        paragraph
                            DerivedStorageCodeMaximumVolume
                        paragraph
                            DerivedStorageCodeMaximumWeight
                        paragraph
                            DerivedStorageCodeMaximumQuantity
                        paragraph
                            DerivedStorageCodeUOM
        single column
            visible when (IsLotTracked)
                header2 of "Expiration_Notification"
                paragraph
                    ExpirationWarning
                        label is "Warning"
                    ExpirationWarningLabel
                        no label
                        display as text
                paragraph
                    text of "ExpirationDateWarningsAreRecommendedFor_Lot_TrackedItems"
                        bold
                        italics
                        color of blue
