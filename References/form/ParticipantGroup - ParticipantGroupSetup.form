ParticipantGroupSetup is a CompositeForm
    is primary
    title is "ParticipantOrMemberGroup"
    context form is ParticipantContext
    show panel control on left
    Actions
        action is Create
        action is CreateNewMembersOrParticipantsForNewParticipantGroupLines
            hide when invalid
        action is Copy
        navigation is ParticipantGroupContracts
            hide when invalid
        action is save
    Participants is a Panel
        visible when (not MemberGroup)
        list is ParticipantGroupLine set.ParticipantGroupLines
    Members is a Panel
        visible when (MemberGroup)
        list is ParticipantGroupLine set.ParticipantGroupMemberLines
