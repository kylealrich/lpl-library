MatchInvoiceDashboardContext is a Form
    Layout
        five column distributed
            display as text
                DerivedCompanyNameID
                    label is "Company"
                paragraph
                    InvoiceAmount.CurrencyAmount
                        label is "InvoiceAmount"
                    InvoiceCurrency
                        no label
                DerivedVendorNameID
                button of "<Invoice>"
                    link is PayablesInvoicePrimary
                    label is "Invoice"
                    display as link
                DerivedProcessLevel
                visible when (HasSingleMatchPurchaseOrder)
                    FirstPurchaseOrder
                    FirstPurchaseOrder.PurchaseOrderDate
                    FirstPurchaseOrder.TotalOrderAmount
                    Buyer
                    FirstPurchaseOrder.DerivedPurchaseOrderStatus
                visible when (HasMultipleMatchPurchaseOrders)
                    MultiplePurchaseOrdersMsg
                        label is "PurchaseOrders"
                MatchStatus
                    label is "MatchStatus"
                    display as tag
                        color of green when (MatchStatus.Matched)
                        color of red when (MatchStatus.Unmatched)
                        color of red when (MatchStatus.POCostMessagesExist)
                        color of green when (MatchStatus.MatchedInTolerance)
                        color of green when (MatchStatus.MatchedWithAutoChargeback)
                        color of yellow when (MatchStatus.MatchedOutOfTolerance)
                MatchAmount
                RetailAmount
                ReceiptOfInvoiceDate
                    label is "InvoiceReceiptDate"
                MatchDate
                MatchTable
                MatchProcessType
