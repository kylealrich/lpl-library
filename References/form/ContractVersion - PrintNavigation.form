PrintNavigation is a Form
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        paragraph
            ContractGroup
            ContractGroup.ProcurementGroup.Description
                no label
        paragraph
            Contract
                display only
            Contract.Name
                no label
        visible when (Contract.IsUsingIDMTemplate)
            paragraph
                AddendumDocumentIDMTemplate
                    select is AddendumDocumentIDMTemplate
                    raise yellow alert when (IDMAndNoAddendumTemplate)
                        mouse over text is "<DerivedNoIDMTemplateWarningMessage>"
                button of "<AddendumDocumentIDMTemplate.Name>"
                    link is external "<AddendumDocumentIDMTemplate.RetrieveUrl>"
                        lazy url retrieval
                    display as link
        visible when (not Contract.IsUsingIDMTemplate)
            two column
                AddendumDocumentTemplate
                    label is "AddendumDocumentTemplate"
                AddendumDocumentTemplate.AddendumDocumentTemplateGroup.Title
                    no label
