ReleaseDistributionsOtherRequiredFields is an ActionForm
    action is InProcess.ReleaseDistributionsOtherRequiredFields
    title is "ReleaseDistributions"
    Layout
        visible when (false)
            paragraph
                AccountingEntity
        visible when (LevelType.Account)
            paragraph
                ReconciliationCodeBlock
                    display as text
                ReconciliationCodeBlock.GeneralLedgerChartAccount.AccountDescription
                    no label
                    display as text
            blank line
        header3 of "AdditionalRequiredDimensions"
        visible when (RequireAccountingUnit)
            paragraph
                AccountingUnit
                    label is "<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>"
                AccountingUnit.Description
                    no label
                    display as text
        visible when (RequireProject)
            paragraph
                Project
                    label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
                Project.Description
                    no label
                    display as text
        visible when (RequireFinanceDimension1)
            paragraph
                FinanceDimension1
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension1Label>"
                FinanceDimension1.Description
                    no label
                    display as text
        visible when (RequireFinanceDimension2)
            paragraph
                FinanceDimension2
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>"
                FinanceDimension2.Description
                    no label
                    display as text
        visible when (RequireFinanceDimension3)
            paragraph
                FinanceDimension3
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension3Label>"
                FinanceDimension3.Description
                    no label
                    display as text
        visible when (RequireFinanceDimension4)
            paragraph
                FinanceDimension4
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension4Label>"
                FinanceDimension4.Description
                    no label
                    display as text
        visible when (RequireFinanceDimension5)
            paragraph
                FinanceDimension5
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension5Label>"
                FinanceDimension5.Description
                    no label
                    display as text
        visible when (RequireFinanceDimension6)
            paragraph
                FinanceDimension6
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension6Label>"
                FinanceDimension6.Description
                    no label
                    display as text
        visible when (RequireFinanceDimension7)
            paragraph
                FinanceDimension7
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension7Label>"
                FinanceDimension7.Description
                    no label
                    display as text
        visible when (RequireFinanceDimension8)
            paragraph
                FinanceDimension8
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension8Label>"
                FinanceDimension8.Description
                    no label
                    display as text
        visible when (RequireFinanceDimension9)
            paragraph
                FinanceDimension9
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension9Label>"
                FinanceDimension9.Description
                    no label
                    display as text
        visible when (RequireFinanceDimension10)
            paragraph
                FinanceDimension10
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension10Label>"
                FinanceDimension10.Description
                    no label
                    display as text
        paragraph
            OffsetDescription
        visible when (HasDistributionApproval)
            header2 of "ApproveDistributions"
            paragraph
                PrmApprovalCode
                    select is ApprovalCodesWithLevels
                PrmApprovalCode.Description
                    no label
                    display as text
