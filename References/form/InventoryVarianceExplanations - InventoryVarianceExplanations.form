InventoryVarianceExplanations is an ActionForm
    action is InventoryVarianceExplanations
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            Location
            Location.Name
                no label
                display as text
        two column distributed
            paragraph
                FrPeriod
                    label is "FromPeriod"
                FrYear
                    label is "Year"
            paragraph
                ThruPeriod
                    label is "ThroughPeriod"
                ThruYear
                    label is "Year"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                InventoryVarianceExplanationsReport_RDG
            column2 of 25%
                InventoryVarianceExplanationsReport_XPT
