SetItemToNotAvailableForUse is an ActionForm
    action is SetItemToNotAvailableForUse
    Layout
        single column
            display as text
                ItemGroup
                paragraph
                    Item
                    Description
                        no label
            visible when (HasOpenContractLines)
                ResetContractLinesToSpecialUsing
            blank line
        two column
            button of "VendorItems"
                visible when (HasActiveVendorItemRel)
                link is VendorItemNav
                display as link
            button of "GTIN_s"
                visible when (HasActiveGTINs)
                link is GTINNav
                display as link
        two column
            button of "ItemLocations"
                visible when (HasLocations)
                link is ItemLocationNav
                display as link
            button of "ContractLines"
                visible when (HasOpenContractLines)
                link is ContractLineNav
                display as link
