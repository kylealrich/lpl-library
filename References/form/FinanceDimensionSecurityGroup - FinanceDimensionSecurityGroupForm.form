FinanceDimensionSecurityGroupForm is a Form
    title is "<DimensionGroupType>SecurityGroup"
    Layout
        two column
            FinanceEnterpriseGroup
            DimensionGroupType
                label is "SecurityGroupType"
        two column
            FinanceDimensionSecurityGroup
                label is "<DimensionGroupType>SecurityGroup"
            Description
        two column
            visible when (DimensionGroupType.AccountingEntity)
                AccountingEntityGroup
                AccountingEntityGroup.Description
                    no label
                    display as text
            visible when (DimensionGroupType.Company)
                GeneralLedgerCompanyGroup
                GeneralLedgerCompanyGroup.Description
                    no label
                    display as text
            visible when (DimensionGroupType.CashCode)
                CashCodeGroup
                CashCodeGroup.Description
                    no label
                    display as text
            visible when (DimensionGroupType.VendorClass)
                VendorClassGroup
                VendorClassGroup.Description
                    no label
                    display as text
            visible when (DimensionGroupType.Ledger)
                LedgerBasisGroup
                    label is "LedgerGroup"
                LedgerBasisGroup.Description
                    no label
                    display as text
        single column
            visible when (MemberDimension)
                visible when (GroupExists)
                    RequiresRebuild
