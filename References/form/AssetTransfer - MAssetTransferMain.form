MAssetTransferMain is a Form
    title is "AssetTransferMain"
    Layout
        single column
            header3 of "AssetTransferDetails"
            visible when (Asset.LeasedAsset)
                paragraph
                    Asset.AssetLease.LeaseCompany
                        label is "LeaseCompany"
                        display as text
                    Asset.AssetLease.LeaseCompany.Name
                        no label
                        display as text
                paragraph
                    Asset.AssetLease.Lease
                        display as text
                    Asset.AssetLease.Lease.Description
                        no label
                        display as text
            AssetRel.EnteredQuantity
                label is "AssetEnteredQuantity"
                display as text
            AssetRel.LastAssetItemPurchaseDate
                label is "LastItemPurchaseDate"
                display as text
            paragraph
                TransferQuantity
                TransferDate
            paragraph
                FullOrPartial
                    display as text
                Asset.DerivedStatus
                    label is "AssetStatus"
                    display as text
        header3 of "TransferFrom"
        paragraph
            Company
                display as text
            Company.GeneralLedgerCompany.Name
                no label
                display as text
                show up to 20 characters
        visible when (IsTagTransfered)
            paragraph
                TagNumber
                    display as text
        visible when (IsAssetGroupTransfered)
            paragraph
                AssetGroup
                    display as text
                AssetGroup.Description
                    no label
                    display as text
                    show up to 20 characters
        visible when (IsLocationTransfered)
            paragraph
                AssetLocation
                    label is "Location"
                    display as text
                AssetLocation.Description
                    no label
                    display as text
                    show up to 20 characters
        visible when (IsAssetDivTransfered)
            paragraph
                AssetDivision
                    label is "Division"
                    display as text
                AssetDivision.Description
                    no label
                    display as text
                    show up to 20 characters
        visible when (IsAssetTypeTransfered)
            paragraph
                AssetType
                    label is "Type"
                    display as text
                    compact format
                AssetType.Description
                    no label
                    display as text
                    show up to 20 characters
        visible when (IsAcctGroupTransfered)
            paragraph
                AssetAccountGroup
                    display as text
                AssetAccountGroup.Description
                    no label
                    display as text
                    show up to 20 characters
        visible when (IsAcctUnitGrpTransfered)
            paragraph
                AssetAccountingUnitGroup
                    display as text
                AssetAccountingUnitGroup.Description
                    no label
                    display as text
                    show up to 20 characters
        visible when (IsAcctUnitTransfered)
            paragraph
                Asset.AssetAccountingUnit
                    label is "DefaultTransactionDimensions"
                    display as text
                    compact format
        visible when (IsExpenseProTransfered)
            paragraph
                text of "Depreciation_Expense_Project"
                    color of gray
                    align as label
                text of "<Asset.DepreciationExpenseProject>"
        visible when (IsAstProTransfered)
            paragraph
                text of "AssetProject"
                    color of gray
                    align as label
                text of "<Asset.AssetProject>"
        header3 of "TransferTo"
        paragraph
            AssetTransferTo.ToAssetCompany
                label is "Company"
                display as text
            AssetTransferTo.ToAssetCompany.GeneralLedgerCompany.Name
                no label
                display as text
                show up to 20 characters
        visible when (IsTagTransfered)
            paragraph
                AssetTransferTo.ToTagNumber
                    label is "TagNumber"
        visible when (IsAssetGroupTransfered)
            paragraph
                AssetTransferTo.ToAssetGroup
                    label is "AssetGroup"
                    display as text
                AssetTransferTo.ToAssetGroup.Description
                    no label
                    display as text
                    show up to 20 characters
        visible when (IsLocationTransfered)
            paragraph
                AssetTransferTo.ToAssetLocation
                    label is "Location"
                    display as text
                AssetTransferTo.ToAssetLocation.Description
                    no label
                    display as text
                    show up to 20 characters
        visible when (IsAssetDivTransfered)
            paragraph
                AssetTransferTo.ToAssetDivision
                    label is "Division"
                AssetTransferTo.ToAssetDivision.Description
                    no label
                    display as text
                    show up to 20 characters
        visible when (IsAssetTypeTransfered)
            paragraph
                AssetTransferTo.ToAssetType
                    label is "Type"
                    compact format
                AssetTransferTo.ToAssetType.Description
                    no label
                    display as text
                    show up to 20 characters
        visible when (IsAcctGroupTransfered)
            paragraph
                AssetTransferTo.ToAssetAccountGroup
                AssetTransferTo.ToAssetAccountGroup.Description
                    no label
                    display as text
                    show up to 20 characters
        visible when (IsAcctUnitGrpTransfered)
            paragraph
                AssetTransferTo.ToAssetAccountingUnitGroup
                    label is "AssetDimensionGroup"
                AssetTransferTo.ToAssetAccountingUnitGroup.Description
                    no label
                    display as text
                    show up to 20 characters
        visible when (IsAcctUnitTransfered)
            paragraph
                AssetTransferTo.ToAssetAccountingUnit
                    label is "DefaultTransactionDimensions"
                    compact format
        visible when (IsExpenseProTransfered)
            paragraph
                text of "Depreciation_Expense"
                    color of gray
                    align as label
                AssetTransferTo.ToDepreciationExpenseProject
        visible when (IsAstProTransfered)
            paragraph
                text of "Asset"
                    color of gray
                    align as label
                AssetTransferTo.ToAssetProject
