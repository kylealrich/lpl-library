ReconciliationVarianceAdjustment is a CompositeForm
    is primary
    title is "ReconciliationVarianceAdjustment<TransactionNumber>"
    context form is CashLedgerReconciliationAdjustmentContext
        background is theme color
    Actions
        action is Release
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is save
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    Main is a Panel
        form is MainForm
    Comment is a Panel
        title is "Comments"
        form is Comment
    CashLedgerGLDistribution is a Panel
        title is "Distributions"
        list is CashLedgerGLDistributionsRel.ReconciliationVarianceAdjustmentDistributions
    BudgetErrors is a Panel
        title is "BudgetErrors<DerivedHeaderBudgetErrorCount>"
        visible when (HeaderBudgetEditErrorExists)
        list is HeaderBudgetEditErrorRel.primary
    WarningBudgetErrors is a Panel
        title is "WarningBudgetErrors<DerivedWarningBudgetErrorCount>"
        visible when (DistributionBudgetEditErrorExists)
        list is DistributionBudgetEditErrorRel.primary
