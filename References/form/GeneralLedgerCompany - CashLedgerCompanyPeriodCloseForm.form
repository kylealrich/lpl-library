CashLedgerCompanyPeriodCloseForm is a CompositeForm
    title is "CurrentCashLedgerPeriodCloseStatusForCompany<Company>"
    context form is CashLedgerCompanyPeriodCloseCompanyContext
    show panel control on left
    Parameters is a Panel
        visible when (CashLedgerJobResultDetailExistsForPeriod)
        form is ParametersForm
    Errors is a Panel
        visible when (CashLedgerJobResultErrorExistsForPeriod)
        list is CashLedgerJobResultErrorsRel.CashLedgerCompanyPeriodCloseErrorList
    UnreleasedTransactions is a Panel
        visible when (CashLedgerTransactionsUnreleased)
        list is UnreleasedCashLedgerTransactionsRel.OutstandingTransactionsForCompanyPeriodCloseList
    PendingApprovalTransactions is a Panel
        visible when (CashLedgerTransactionsPendingApproval)
        list is PendingApprovalCashLedgerTransactionsRel.OutstandingTransactionsForCompanyPeriodCloseList
    PendingStopPayConfirmationTransactions is a Panel
        visible when (CashLedgerTransactionsPendingStopPayConfirmation)
        list is PendingStopPayConfirmationCashLedgerTransactionsRel.OutstandingTransactionsForCompanyPeriodCloseList
    VoidOrStopPaymentTransactions is a Panel
        visible when (CashLedgerTransactionsVoidOrStopPayment)
        list is VoidOrStopPaymentCashLedgerTransactionsRel.OutstandingTransactionsForCompanyPeriodCloseList
    UnreleasedCashPayments is a Panel
        visible when (CashPaymentsUnreleased)
        list is UnreleasedCashLedgerPaymentsRel.OutstandingCashPaymentsForCompanyPeriodCloseList
    PendingApprovalCashPayments is a Panel
        visible when (CashPaymentsPendingApproval)
        list is PendingApprovalCashLedgerPaymentsRel.OutstandingCashPaymentsForCompanyPeriodCloseList
    PendingStopPaymentApprovalCashPayments is a Panel
        visible when (CashPaymentsPendingStopPaymentApproval)
        list is PendingStopPaymentApprovalCashLedgerPaymentsRel.OutstandingCashPaymentsForCompanyPeriodCloseList
    UnreleasedReturnedPayments is a Panel
        visible when (ReturnedPaymentsUnreleased)
        list is UnreleasedCashLedgerReturnedPaymentsRel.OutstandingReturnedPaymentsForCompanyPeriodCloseList
    UnreleasedReconciliationAdjustments is a Panel
        visible when (ReconciliationAdjustmentsUnreleased)
        list is UnreleasedCashLedgerReconciliationAdjustmentsRel.OutstandingReconciliationAdjustmentsForCompanyPeriodCloseList
    UnreleasedCashReceipts is a Panel
        visible when (CashReceiptsUnreleased)
        list is UnreleasedCashLedgerCashReceiptHeadersRel.OutstandingCashReceiptsForCompanyPeriodCloseList
    PendingApprovalCashReceipts is a Panel
        visible when (CashReceiptsPendingApproval)
        list is PendingApprovalCashLedgerCashReceiptHeadersRel.OutstandingCashReceiptsForCompanyPeriodCloseList
    UnreleasedBankFundTransfers is a Panel
        visible when (BankFundTransfersUnreleased)
        list is UnreleasedCashLedgerFundTransferHeadersRel.OutstandingBankFundTransfersForCompanyPeriodCloseList
    UnpostedCashLedgerGLDistributions is a Panel
        visible when (CashLedgerGLDistributionsUnposted)
        list is UnpostedCashLedgerGLDistributionsRel.OutstandingDistributionsForCompanyPeriodCloseList
    UnpostedReconciliationVarianceDistributions is a Panel
        visible when (NonCashLedgerDistributionsUnposted)
        list is UnpostedNonCashLedgerDistributionsRel.OutstandingNonCashLedgerDistributionsForCompanyPeriodCloseList
    PendingApprovalReconciliationVarianceDistributions is a Panel
        visible when (NonCashLedgerDistributionsPendingApproval)
        list is PendingApprovalNonCashLedgerDistributionsRel.OutstandingNonCashLedgerDistributionsForCompanyPeriodCloseList
    UnpostedBankStatementLineDistributions is a Panel
        visible when (BankStatementLineDistributionsUnposted)
        list is UnpostedBankStatementLineDistributionsRel.OutstandingBankStatementLineDistributionsForCompanyPeriodCloseList
    PendingApprovalBankStatementLineDistributions is a Panel
        visible when (BankStatementLineDistributionsPendingApproval)
        list is PendingApprovalBankStatementLineDistributionsRel.OutstandingBankStatementLineDistributionsForCompanyPeriodCloseList
    UnconfirmedWireTransferBatchDetails is a Panel
        visible when (WireTransferBatchDetailsUnConfirmed)
        list is UnConfirmedWireTransferBatchDetailsRel.OutstandingWireTransferBatchDetailsForCompanyPeriodCloseList
