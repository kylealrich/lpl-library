CreateAssetDefaults is an ActionForm
    action is CreateAssetDefaults
    Layout
        single column
            display as text
                PrmCompany
                    label is "Company"
                PurchaseOrderImport
                LineNumber
            blank line
            PrmRecordType
                label is "RecordType"
            AssetDescription
            TagNumber
                label is "Tag"
            AssetGroup
            AssetAccountingUnitGroup
            Combine
            HoldAsset
            blank line
            text of "Item"
                bold
            ItemNumber
            ItemDescription
            Project
            ModelNumber
            PurchaseDate
            InServiceDate
                label is "InServiceDate"
            blank line
            text of "Location"
                bold
            AssetLocation
                label is "Location"
            AssetDivision
                label is "Division"
            LocationDetail
                label is "LocationDetail"
                select is LocationDetail
