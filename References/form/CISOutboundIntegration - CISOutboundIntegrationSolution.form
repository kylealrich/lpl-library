CISOutboundIntegrationSolution is a Form
    Layout
        valid when (not YellowAlert)
            single column
                paragraph
                    Solution
                        no label
                        display as radio buttons
                        display as required when (UseDefaultProcess)
                    visible when (UseDefaultProcess)
                        text of untranslatable:"*"
                            header3
                            bold
                            color of red
                    text of untranslatable:"Tip: Test in ION API before configuring this section."
                        italics
                text of untranslatable:"Parameters"
                visible when (Solution.DBExport)
                    paragraph
                        DbBusinessClass
                            display as required when (Solution.DBExport)
                        text of untranslatable:"Business Class To Retrieve Data From"
                            italics
                    paragraph
                        DbFields
                            display as required when (Solution.DBExport)
                        text of untranslatable:"Fields To Return (Use \\_all to return all fields).Tip: Use a derived field to contatenate all extract fields with a preferred delimiter"
                            italics
                    paragraph
                        DbFilter
                        text of untranslatable:"Complex Filter - LPL Filter String Example: (AirportCode like \"MSP*\")"
                            italics
                    paragraph
                        DbLimit
                            display zero
                        text of untranslatable:"How many records to return (Default value: 5. Use 0 to return all records.)"
                            italics
                    DbShowAdvanceControls
                        display as switch
                    visible when (DbShowAdvanceControls)
                        paragraph
                            DbPaging
                            text of untranslatable:"Name of paging operation"
                                italics
                        paragraph
                            DbLinks
                            text of untranslatable:"Show hypertext"
                                italics
                        paragraph
                            DbPageNav
                            text of untranslatable:"Include Paging Header"
                                italics
                        paragraph
                            DbOutputFormat
                            text of untranslatable:"Output Format"
                                italics
                        paragraph
                            DbFlatten
                            text of untranslatable:"Display all Group Field members as individual Fields in list output."
                                italics
                    paragraph
                        RemoveQuotes
                        text of untranslatable:"Remove quotes from the output data"
                            italics
                visible when (Solution.DataLake)
                    single column
                        paragraph
                            Replicate
                            text of untranslatable:"Auto-Replicate is not allowed for standard Replication Sets. This is also not recommended for custom Replication Sets that are used by multiple processes."
                        visible when (Replicate)
                            ReplicationSet
                                display as required when (Replicate)
                    UseDateOverride
                    visible when (UseDateOverride)
                        paragraph
                            OverrideDateVariable
                            OverrideDateValue
                                label is untranslatable:"Override Date (YYYYMMDD)"
                    DlSQLQueryLocked
                    valid when (not DlSQLQueryLocked)
                        DlSQLQuery
                            use text area of 20 lines
                            display as required when (Solution.DataLake)
                    text of untranslatable:"SQL Query Used In Data Lake Compass"
                        italics
                paragraph
                    Header
                paragraph
                    visible when (Header)
                        HeaderValue
                    visible when (Header)
                        text of untranslatable:"Custom Header (Leave Blank To Use Standard Field Labels)"
                            italics
                visible when (not Header)
                    paragraph
                        BlankFile
