SourcingEventAnalysis is a CompositeForm
    title is "SourcingEventAnalysisForEvent<EventVersionString>"
    context form is SEHeaderAnalysisContext
        background is theme color
    restrict action Open.Notified.FinalAward
    restrict action Open.Notified.CreateOutput
    restrict action GenerateIDMBidTabulation
    restrict action PrintBidTabulation
    Actions
        action is MarkResponsesAsViewed
            hide when invalid
        action is StartPricingProcess
            hide when invalid
        action is StartBestAndFinalProcess
            hide when invalid
        action is StartBestAndFinalStepOne
            hide when invalid
        navigation is InitialBestAndFinalResponses
            action icon is preview
            hide when invalid
        navigation is UnacceptedTechnicalProposalResponses
            action icon is preview
            hide when invalid
        navigation is EventPrint
            action icon is preview
        navigation is CompareQuestionAnswersNav
            action icon is validate
            label is "CompareQuestionResponses"
            hide when invalid
        action is refresh
            label is "_"
        navigation is MissingBuyerWeightingScores
            hide when invalid
        navigation is ResponseModificationRequests
            hide when invalid
        action is Open.Notified.ChangeToBestAndFinalOffer
            hide when invalid
    ResponsesNotViewed is a Panel
        visible when (ShowResponsesNotViewed)
        list is SourcingEventResponseNotViewedRel.ResponsesBySupplierNotViewed
    ResponsesBySupplier is a Panel
        title is "ResponsesBySupplier"
        visible when (MixedResponseLink)
        list is SourcingEventResponseAnalysisRel.ResponsesBySupplier
    ResponsesBySupplierForPurchaseOrders is a Panel
        title is "ResponsesBySupplier"
        visible when (PurchaseOrderOnlyResponseLink)
        list is SourcingEventResponseAnalysisRel.ResponsesBySupplierForPurchaseOrders
    ResponsesBySupplierForContracts is a Panel
        title is "ResponsesBySupplier"
        visible when (ContractOnlyResponseLink)
        list is SourcingEventResponseAnalysisRel.ResponsesBySupplierForContracts
    ResponsesBySupplierForNoOutput is a Panel
        title is "ResponsesBySupplier"
        visible when (NoOutputOnlyResponseLink)
        list is SourcingEventResponseAnalysisRel.ResponsesBySupplierForNoOutput
    ResponsesByLine is a Panel
        visible when (ResponseLinkNoSelectedLines)
        list is SourcingEventLine set.ResponseLines
    TechnicalProposalsBySupplier is a Panel
        visible when (TechnicalResponseStep1Link)
        list is SourcingEventResponseAnalysisRel.TechnicalProposalsBySupplier
    PricingResponsesBySupplierMixed is a Panel
        title is "PricingResponsesBySupplier"
        visible when (TechnicalResponseStep2LinkMixed)
        list is SourcingEventResponseAnalysisRel.PricingStepResponsesBySupplierMixed
    PricingResponsesBySupplier is a Panel
        visible when (TechnicalResponseStep2LinkPurchaseOrder)
        list is SourcingEventResponseAnalysisRel.PricingStepResponsesBySupplier
    PricingResponsesBySupplierForContracts is a Panel
        title is "PricingResponsesBySupplier"
        visible when (TechnicalResponseStep2LinkContract)
        list is SourcingEventResponseAnalysisRel.PricingStepResponsesBySupplierForContracts
    PricingResponsesByLine is a Panel
        visible when (TechnicalResponseStep2Link)
        list is SourcingEventLine set.ResponseLines
    BestAndFinalInitialResponsesBySupplier is a Panel
        visible when (BestAndFinalResponseStep1Link)
        list is SourcingEventResponseAnalysisRel.BestAndFinalResponsesBySupplierStep1
    BestAndFinalInitialResponsesByLine is a Panel
        visible when (BestAndFinalResponseStep1Link)
        list is SourcingEventLine set.ResponseLines
    BestAndFinalResponsesBySupplier is a Panel
        visible when (MixedBestAndFinalResponseLink)
        list is SourcingEventResponseAnalysisRel.BestAndFinalResponsesBySupplier
    BestAndFinalResponsesBySupplierForPurchaseOrders is a Panel
        title is "BestAndFinalResponsesBySupplier"
        visible when (PurchaseOrderOnlyBestAndFinalResponseLink)
        list is SourcingEventResponseAnalysisRel.BestAndFinalResponsesBySupplierForPurchaseOrders
    BestAndFinalResponsesBySupplierForContracts is a Panel
        title is "BestAndFinalResponsesBySupplier"
        visible when (ContractOnlyBestAndFinalResponseLink)
        list is SourcingEventResponseAnalysisRel.BestAndFinalResponsesBySupplierForContracts
    BestAndFinalResponsesByLine is a Panel
        visible when (BestAndFinalResponseLink)
        list is SourcingEventLine set.BestAndFinalResponseLines
    BestAndFinalStepStarted is a Panel
        visible when (CanDisplayFinalStartedPanel)
        list is SourcingEventResponseAnalysisRel.BestAndFinalResponsesBySupplierStep1
    BestAndFinalStepOneStarted is a Panel
        visible when (CanDisplayBestAndFinalStepOnePanel)
        list is SourcingEventResponseAnalysisRel.BestAndFinalResponsesBySupplierStep1
    PricingStepStarted is a Panel
        visible when (CanDisplayPricingStartedPanel)
        list is SourcingEventResponseAnalysisRel.TechnicalProposalsBySupplier
