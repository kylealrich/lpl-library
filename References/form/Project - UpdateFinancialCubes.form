UpdateFinancialCubes is an ActionForm
    action is UpdateFinancialCubes
    title is "UpdateFinancialCubes"
    Layout
        single column
            text of "SelectTheAnalyticCubesYouWouldLikeToUpdate:"
                header2
                bold
        blank line
        three column
            column1
                UpdateFinanceEnterpriseGroup
                    label is "FinanceEnterpriseGroup"
                UpdateGeneralLedgerTotal
                    label is "GlobalLedgerTotal"
            column2
                visible when (UpdateFinanceEnterpriseGroup)
                    FinanceEnterpriseGroupOption
                        no label
                        display as horizontal radio buttons
                visible when (not UpdateFinanceEnterpriseGroup)
                    BlankField
                        no label
                        display as text
                visible when (UpdateGeneralLedgerTotal)
                    GeneralLedgerTotalOption
                        no label
                        display as horizontal radio buttons
                visible when (not UpdateGeneralLedgerTotal)
                    BlankField
                        no label
                        display as text
            column3
                paragraph
                    FinanceEnterpriseGroupAnalyticCubeRel.LastRefreshDate
                        display as text
                    FinanceEnterpriseGroupAnalyticCubeRel.LastReloadDate
                        display as text
                paragraph
                    GeneralLedgerTotalAnalyticCubeRel.LastRefreshDate
                        display as text
                    GeneralLedgerTotalAnalyticCubeRel.LastReloadDate
                        display as text
        blank line
        single column
            text of "SelectRefreshInOrderToUpdateExistingCubeData.SelectReloadToDeleteExistingCubeDataAndRebuildData."
                italics
