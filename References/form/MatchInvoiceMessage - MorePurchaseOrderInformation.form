MorePurchaseOrderInformation is a Form
    title is "PurchaseOrderInformation"
    Layout
        display as text
            two column distributed
                paragraph
                    PurchaseOrderLine.PurchaseOrder
                        label is "PONumber"
                    PurchaseOrderLine.PurchaseOrder.PORelease
                        no label
                    PurchaseOrderLine.PurchaseOrder.POCode
                        no label
                PurchaseOrderLine.CostCode
            two column distributed
                PayablesInvoice.Vendor.DerivedVendorNameID
                PurchaseOrderLine.VendorItem
            paragraph
                PurchaseOrderLine.ManufacturerNumber
                PurchaseOrderLine.Manufacturer.ManufacturerCode
                    label is "Code"
                PurchaseOrderLine.Manufacturer.ManufacturerDivision
                    label is "Division"
            single column
                PurchaseOrderLine.ItemGTIN
                    label is "G\\T\\I\\N"
                visible when (PurchaseOrderLine.CostCode.Contract)
                    PurchaseOrderLine.Contract
                        label is "Contract"
                    PurchaseOrderLine.ContractLine
                        label is "ContractLine"
                    PurchaseOrderLine.ContractLine.EffectiveDate
                        label is "EffectiveDate"
                    PurchaseOrderLine.ContractLine.ExpirationDate
                        label is "ExpirationDate"
                DerivedPurchaseOrderLineCommentsConditionalMessage
                    no label
            three column table
                column headers
                    "Quantity"
                    "UOM"
                    "Multiplier"
                row headers
                    "Ordered"
                    "Received"
                    "Matched"
                column1
                    PurchaseOrderLine.Quantity
                        no label
                    PurchaseOrderLine.ReceivedQuantity
                        no label
                    PurchaseOrderLine.MatchedQuantity
                        no label
                column2
                    PurchaseOrderLine.EnteredBuyUOM
                        no label
                    PurchaseOrderLine.EnteredBuyUOM
                        no label
                    PurchaseOrderLine.EnteredBuyUOM
                        no label
                column3
                    PurchaseOrderLine.EnteredBuyUOMMultiplier
                        no label
                    PurchaseOrderLine.EnteredBuyUOMMultiplier
                        no label
                    PurchaseOrderLine.EnteredBuyUOMMultiplier
                        no label
