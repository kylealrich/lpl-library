FundingSourceCreateMainForm is a Form
    title is "Main"
    Layout
        visible when (false)
            single column
                FinanceDimension2Structure
        three column distributed
            column1
                CreateDimensionType
                    label is "DimensionType"
                    show up to 15 characters
            column2
                paragraph
                    FinanceDimension2
                        label is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
                        show up to 15 characters
                    visible when (CreateDimensionType.Summary)
                        DisplayDimension
                            display as text
                            show up to 15 characters
            column3
                Description
        visible when (CreateDimensionType.Summary)
            two column distributed
                column1
                    paragraph
                        BudgetIdentifier
                        BudgetIdentifier.Description
                            no label
                            display as text
        visible when (InEnterpriseStructure)
            visible when (CreateDimensionType.Posting)
                two column distributed
                    column1
                        paragraph
                            Active
                            SystemDimension
                                label is "System<FinanceEnterpriseGroup.FinanceDimension2Label>"
                            IncludeInBudgetEdit
                                label is "IncludeInBudgetEdit"
            visible when (CreateDimensionType.Summary)
                two column distributed
                    column1
                        paragraph
                            PersonResponsible
                            PersonResponsible.Name.PreferredFirstAndLastName
                                no label
                                display as text
                two column distributed
                    column1
                        paragraph
                            ProjectTeam
                                show up to 20 characters
                            ProjectTeam.Description
                                no label
                                display as text
        visible when (IsAchild)
            two column distributed
                column1
                    paragraph
                        DerivedParentDisplayDimension
                            label is "Parent<FinanceEnterpriseGroup.FinanceDimension2Label>"
                            display as text
                        DerivedParentDescription
                            no label
                            display as text
        header2 of "AddressInformation"
        two column distributed
            column1
                paragraph
                    AddressCode
                    AddressCode.Description
                        no label
                        display as text
        visible when (AddressCode entered)
            two column distributed
                column1
                    AddressCode.PersonResponsible
                        label is "PersonResponsible"
                        display as text
                column2
                    AddressCode.PostalAddress.Country
                        display as text
            two column distributed
                column1
                    paragraph
                        AddressCode.PostalAddress.DisplayAddressLine1
                            label is "StreetAddress"
                            display as text
                        AddressCode.PostalAddress.DisplayAddressLine2
                            no label
                            display as text
                        AddressCode.PostalAddress.DisplayAddressLine3
                            no label
                            display as text
                        AddressCode.PostalAddress.DisplayAddressLine4
                            no label
                            display as text
                column2
                    paragraph
                        AddressCode.PostalAddress.Municipality
                            label is "City"
                            display as text
                        AddressCode.PostalAddress.StateProvince
                            label is "State/Province"
                            display as text
                        AddressCode.PostalAddress.PostalCode
                            label is "PostalCode"
                            display as text
                        AddressCode.PostalAddress.County
                            label is "County/District"
                            display as text
                        AddressCode.PostalAddress.Type
                            label is "Type"
                            display as text
            two column distributed
                column1
                    AddressCode.TelephoneNumber
                        label is "TelephoneNumber"
                        display as text
                column2
                    AddressCode.FaxNumber
                        label is "FaxNumber"
                        display as text
            two column distributed
                column1
                    AddressCode.EmailAddress
                        label is "EmailAddress"
                        display as text
