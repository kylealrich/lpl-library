SupplierUpdateComposite is a CompositeForm
    title is "AccountInformation"
    context form is SupplierUpdateContext
    show panel control on left
    restrict action Delete
    restrict action InactivateContact
    restrict action MobileUpdateContactBankInformation
    restrict action MobileUpdateSupplierBankInformation
    Actions
        action is save
        action is UpdateSupplierPortalInformation
            hide when invalid
        action is UpdateSupplierBankInformation
            hide when invalid
        action is UpdateContactBankInformation
            label is "UpdateMyContactBankInformation"
            hide when invalid
        action is UpdateSupplierBankCountry
            hide when invalid
        action is UpdateContactBankCountry
            hide when invalid
        navigation is UpdateCompanyAddress
            hide when invalid
    Contacts is a Panel
        visible when (IsPrimaryAndPortalContact)
        list is Supplier.SupplierSourceId set.SupplierContactCreateUpdate
    ContactBankInformation is a Panel
        visible when (CanDisplayBankInformationIsPrimaryPortalContact)
        list is Supplier.SupplierSourceId set.SupplierContactBankInformation
    AllCertifications is a Panel
        visible when (CanDisplayAllCertificationsPortalContact)
        list is Supplier.AllSupplierCertificationsRel.SupplierSideCertifications
    MyContactInformation is a Panel
        visible when (PortalContact)
        form is ContactSummaryForm
    MyCommodityCodes is a Panel
        visible when (PortalContact)
        list is SupplierCommodityCode set.SupplierCommodityCodeUpdate
            helper list is CommodityCodes.CommodityCodeHelper
                action is AttachToContact
    MyCertifications is a Panel
        visible when (PortalContact)
        list is SupplierCertificationRel.SupplierSideContactCertifications
    MyContactBankInformation is a Panel
        visible when (CanDisplayBankInformationPortalContact)
        form is BankInformationForm
    MyProxyNotifications is a Panel
        visible when (PortalContact)
        list is SupplierContactProxy set.SupplierContactProxyList
    CompanyAddress is a Panel
        form is SupplierAddress
    Questions is a Panel
        visible when (PrimaryQuestionResponsePortalContact)
        form is SupplierQuestionLink
    DiversityCodes is a Panel
        visible when (PrimaryDiversityResponsePortalContact)
        list is Supplier.SupplierDiversityResponse set.DiversityResponseUpdate
