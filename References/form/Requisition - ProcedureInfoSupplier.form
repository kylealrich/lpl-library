ProcedureInfoSupplier is a Form
    title is "ProcedureInformation"
    disable field options
    Actions
        action is save
    Layout
        visible when (CanDisplayVisitOrProcedure)
            two column distributed
                column1
                    header2 of "VisitOrProcedure"
                    visible when (CanDisplayVisitNumber)
                        TransientVisitNumber
                            label is "VisitNumber"
                            select is SupplierPatientVisitList
                    visible when (CanDisplayPatientID)
                        TransientPatientID
                            label is "MedicalRecordNumber"
                            select is PatientsByNameForSupplier
                    visible when (CanDisplayCaseNumber)
                        TransientCaseNumber
                            label is "CaseID"
                    paragraph
                        visible when (CanDisplayProcedure)
                            DefaultProcedureInformation.DefaultProcedure
                                label is "Procedure"
                            DefaultProcedureInformation.DefaultProcedureModifier
                                label is "ProcedureModifier"
                        visible when (CanDisplayProcedure)
                            DefaultProcedureInformation.DefaultProcedureDate
                                label is "ProcedureDate"
                    visible when (CanDisplayPractitioner)
                        DefaultProcedureInformation.DefaultPractitioner
                            label is "Practitioner"
                    visible when (CanDisplayNurse)
                        DefaultProcedureInformation.DefaultNurseCoordinator
                            label is "CoordinatingNurse"
                column2
                    visible when (CanDisplayPatient)
                        header2 of "Patient"
                        TransientPatientLastName
                            label is "LastName"
                        TransientPatientFirstName
                            label is "FirstName"
                        TransientPatientMiddleName
                            label is "MiddleInitial"
                    paragraph
                        visible when (CanDisplayBirthdate)
                            TransientBirthdate
                                label is "Birthdate"
                        visible when (CanDisplayGender)
                            TransientGender
                                label is "Gender"
        visible when (not CanDisplayVisitOrProcedure)
            single column
                visible when (CanDisplayPatient)
                    header2 of "Patient"
                    TransientPatientLastName
                        label is "LastName"
                    TransientPatientFirstName
                        label is "FirstName"
                    TransientPatientMiddleName
                        label is "MiddleInitial"
                paragraph
                    visible when (CanDisplayBirthdate)
                        TransientBirthdate
                            label is "Birthdate"
                    visible when (CanDisplayGender)
                        TransientGender
                            label is "Gender"
        header2 of "Order"
        two column distributed
            column1
                DefaultProcedureInformation.DefaultSalesRepresentative
                    label is "SalesRepresentative"
                DefaultProcedureInformation.DefaultRequester
                    label is "Requester"
            visible when (CanDisplaySupplierOrder)
                column2
                    DefaultProcedureInformation.DefaultSupplierOrderNumber
                        label is "SupplierOrderNumber"
