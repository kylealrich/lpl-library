UnitsOfMeasure is a Form
    Layout
        two column distributed
            column1
                Item.first DefaultBuyUOMRel.UnitOfMeasure
                    label is "DefaultPurchaseUOM"
                    display as text
                visible when (Item.first DefaultBuyUOMRel.UnitOfMeasure entered)
                    Item.first DefaultBuyUOMRel.UOMConversion
                        label is "ConversionFactor"
                        display as text
                DefaultSourceUOM.UnitOfMeasure
                    label is "SourceUOM"
                    display as text
                visible when (DefaultSourceUOM.UnitOfMeasure entered)
                    DefaultSourceUOMRel.UOMConversion
                        label is "ConversionFactor"
                        display as text
                Item.StockUOM
                    label is "StockUOM"
                    display as text
            column2
                DefaultTransactionUOM.UnitOfMeasure
                    label is "TransactionUOM"
                    display as text
                visible when (DefaultTransactionUOM.UnitOfMeasure entered)
                    DefaultTransactionUOMRel.UOMConversion
                        label is "ConversionFactor"
                        display as text
                Item.first DefaultTransactionUOMRel.UnitOfMeasure
                    label is "DefaultTransactionUOM"
                    display as text
                visible when (Item.first DefaultTransactionUOMRel.UnitOfMeasure entered)
                    Item.first DefaultTransactionUOMRel.UOMConversion
                        label is "ConversionFactor"
                        display as text
