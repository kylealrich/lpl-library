EditAsset is an ActionForm
    action is EditAsset
    title is "EditAsset"
    Layout
        single column
            PrmDescription
                label is "Description"
            PrmTagNumber
                label is "TagNumber"
            PrmAssetOwner
                label is "Owner"
            visible when (DisplayAdjustmentDate)
                PrmAdjustmentDate
                    label is "AdjustmentDate"
