BuyerRegistrationUpdate is a Form
    title is "Registration"
    Layout
        display as text
            single column
                visible when (not SupplierExtEntityEnabled)
                    PrimaryContact.LoginName
            single column
                PrimaryContact.MainContact.GivenName
                    label is "FirstName"
                PrimaryContact.MainContact.FamilyName
                    label is "LastName"
                PrimaryContact.ContactTitle
                    label is "Title"
            paragraph
                PrimaryContact.TelephoneNumber
            paragraph
                PrimaryContact.MobilePhone
            paragraph
                PrimaryContact.FaxNumber
            paragraph
                PrimaryContact.EmailAddress
                PrimaryContact.ReceiveEmailNotification
