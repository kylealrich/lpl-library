CreateRequestCompositeForm is a WizardForm
    use for action CreateRequest
    use for desktop
    title is "Schedule"
    summary form is CreateRequestSummaryForm
    ScheduleAnActionToRun is a Panel
        form is CreateRequestForm
    ScheduleByFrequency is a Panel
        valid when (ScheduleType.ByFrequency)
        form is ScheduleFrequencyFormReq
    ScheduleByDateInMonth is a Panel
        valid when (ScheduleType.ByDateInMonth)
        form is ScheduleDateTimeFormReq
    ScheduleByDay is a Panel
        valid when (ScheduleType.ByDay)
        form is ScheduleWeekDayFormReq
    ScheduleByWeekNumber is a Panel
        valid when (ScheduleType.ByWeekNumber)
        form is ScheduleWeekNumberFormReq
    AdvancedScheduleOptionsToRunAction is a Panel
        valid when (ScheduleType.AdvancedSchedule)
        form is ScheduleAdvancedFormReq
    CopyScheduleFromTemplate is a Panel
        valid when (ScheduleType.CopyFromTemplate)
        form is ScheduleTemplateCopyFormReq
    OverrideActionFailureConfiguration is a Panel
        valid when (EnterMisFireData)
        form is MisfireFormReq
