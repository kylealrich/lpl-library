                "Before"
                "After"
            row headers
                "Status"
                "CurrencyRevalue"
                "RiskCode"
                "CreditReviewDays"
                "AuditChanges"
                "AgeDisputes"
                "DisputesFinanced"
                "PaymentGraceDays"
                "PrintChargebacks"
                "AutoTransactionRemoval"
