MassCustomerUpdate is an ActionForm
    action is MassCustomerUpdate
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        two column distributed
            column1
                UpdateOption
                    no label
                    display as horizontal radio buttons
            column2
                DefCdValue
                    label is "UseDefaultCodeValues"
        header2 of "Customers"
        single column
            AllCust
                label is "AllCustomers"
        visible when (AllCust.No)
            two column distributed
                column1
                    paragraph
                        CustFrom
                            label is "FromCustomer"
                            select is GroupSearchNameContextCompanyCustomerSelect
                        CustFrom.Name
                            no label
                            display as text
                column2
                    paragraph
                        CustTo
                            label is "ToCustomer"
                            select is GroupSearchNameContextCompanyCustomerSelect
                        CustTo.Name
                            no label
                            display as text
        blank line
        two column distributed
            column1
                paragraph
                    MajClass
                        label is "MajorClass"
                        select is MajorClass
                    MajClass.Description
                        no label
                        display as text
            column2
                paragraph
                    MinClass
                        label is "MinorClass"
                    MinClass.Description
                        no label
                        display as text
        two column distributed
            column1
                paragraph
                    CredAnalyst
                        label is "CreditAnalyst"
                        select is CreditAnalyst
                    CredAnalyst.Name
                        no label
                        display as text
            column2
                paragraph
                    Salesman
                        label is "SalesRepresentative"
                        select is SalesReps
                    Salesman.Name
                        no label
                        display as text
        paragraph
            DefaultCode
                select is DefaultCodes
            DefaultCode.Description
                no label
                display as text
        header2 of "Codes"
        two column table
            column headers
                "Before"
                "After"
            row headers
                "CreditAnalyst"
                "SalesRepresentative"
                "PaymentTerms"
                "MemoPaymentTerms"
                "AccountsReceivableCode"
                "PaymentCode"
                "AgingCode"
                "RemitLockbox"
                "Currency"
                "DiscountReasonCode"
            column1
                OldCredAnalyst
                    no label
                    select is CreditAnalyst
                OldSalesman
                    no label
                    select is SalesReps
                OldTermsCd
                    no label
                    select is TermsCodes
                OldMemoTerm
                    no label
                    select is TermsCodes
                OldArCode
                    no label
                    select is ARCodes
                OldBankInstCd
                    no label
                    select is CashPayments
                OldAgingCode
                    no label
                    select is AgingCodes
                OldLockBox
                    no label
                    select is LockBoxes
                OldCurrCd
                    no label
                OldDiscReasCd
                    no label
                    select is ReceivableReasonCodeDiscountList
            column2
                NewCredAnalyst
                    no label
                    select is CreditAnalyst
                NewSalesman
                    no label
                    select is SalesReps
                NewTermsCd
                    no label
                    select is TermsCodes
                NewMemoTerm
                    no label
                    select is TermsCodes
                NewArCode
                    no label
                    select is ARCodes
                NewBankInstCd
                    no label
                    select is CashPayments
                NewAgingCode
                    no label
                    select is AgingCodes
                NewLockBox
                    no label
                    select is LockBoxes
                NewCurrCd
                    no label
                NewDiscReasCd
                    no label
                    select is ReceivableReasonCodeDiscountList
