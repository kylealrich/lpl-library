CustodialDetailTransferUpdateForm is an ActionForm
    action is Unreleased.Update
        on completion navigate to TransferLink
    Layout
        paragraph
            CustodialAccountManagement.CustodialSummary
                display as text
            Status
                no label
                display as text
        two column distributed
            column1
                header3 of "TransferFrom"
                CustodialDetailAmount.CustodialPosting
                    display as text
                CustodialAccountManagement.CamCategory
                    display as text
                CustodialAccountManagement.TaxID
                    display as text
                visible when (HasTaxID)
                    text of "TaxIDEntered"
                        italics
                visible when (not HasTaxID)
                    text of "TaxIDNotEntered"
                        italics
                CustodialAccountManagement.CamCustodian
                    display as text
                FromFinanceCodeBlock
                    compact format
            column2
                header3 of "TransferTo"
                CustodialPosting
                    select is CustodialAccountManagement.CustodialSummary.DescendantsRel.FinanceDimension10Select
                CamCategory
                visible when (not CamCategory.SSNRequired.Yes)
                    text of "_"
                visible when (CamCategory.SSNRequired.Yes)
                    TaxID
                CamCustodian
                ToFinanceCodeBlock
                    compact format
                TransferAmount
                ApprovalCode
