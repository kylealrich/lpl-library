AccountingEntityDocument is a CompositeForm
    is primary
    title is "<FinanceEnterpriseGroup.AccountingEntityLabel>_<AccountingEntity>"
    AccountingEntityPanel is a Panel
        title is "<FinanceEnterpriseGroup.AccountingEntityLabel>"
        form is AccountingEntityDetails
    AccountingUnitStructures is a Panel
        title is "<FinanceEnterpriseGroup.AccountingUnitLabel>Structures"
        visible when (FinanceEnterpriseGroup.AccountingUnitSelected)
        list is FinanceEnterpriseGroup.AccountingUnitStructure set.AccountingUnitStructuresForEntity
    SystemClosingControl is a Panel
        title is "SystemControl"
        list is SystemClosingControl set.SystemClosingControlList
