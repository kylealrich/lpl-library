PaidPayablesSearchForm is a SearchForm
    title is "PaidPayablesSearch"
    Layout
        five column distributed
            InvoiceSearchDocument
                label is "InvoiceKeyword"
                mouse over text is "SearchOn_Invoice_Number,_System_Invoice_Number,_Voucher_Number,_Vendor_Name,_Pay_Vendor_Name,Or_Company_Name"
                use text area of 1 lines
            Company
                label is "Company"
            Vendor
                label is "Vendor"
            Invoice
                label is "InvoiceNumber"
            InvoiceAmount.CurrencyAmount
                label is "InvoiceAmount"
        five column distributed
            InvoiceDate
                label is "InvoiceDate"
            TransientPaymentDate
                label is "PaymentDate"
            Vendor.VendorSearchDocument
                label is "VendorKeyword"
                mouse over text is "SearchOn_Vendor_Number,_Name,_Search_Name,_Legal_Name,_Reference_Number,Or_Tax_ID"
                use text area of 1 lines
            APClerk
            CashCode
        header2 of "Additional"
            collapsible
                initially collapsed
                use show more or less affordance
            five column distributed
                DueDate
                    label is "DueDate"
                DerivedPaymentNumber
                    label is "PaymentNumber"
                Suffix
                TermsCode
                ServiceContract
            five column distributed
                InvoiceTypeForSearch
                PayCurrency
