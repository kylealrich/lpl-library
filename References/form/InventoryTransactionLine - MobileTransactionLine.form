MobileTransactionLine is a CompositeForm
    title is "TransactionLine"
    context form is MobileTransactionLineContext
    Actions
        action is Unreleased.Create
            action icon is create
            label is "_"
        action is save
            label is "_"
        action is refresh
            label is "_"
    Main is a Panel
        form is MobileInventoryTransactionLine
    Details is a Panel
        visible when (ShowDetailPanel)
        list is InventoryTransactionLineDetailsRel.MobileDetails
    Distributions is a Panel
        visible when (HasDistributions)
        list is InventoryTransactionLineDistribution set.MobileInventoryTransactionLineDistributions
