OrderEntryDailyShipmentJournal is an ActionForm
    action is OrderEntryDailyShipmentJournal
    Layout
        single column
            Company
        two column
            Location
            Location.Name
                no label
                display as text
        blank line
        two column
            ProcessLevel
            ProcessLevel.Name
                no label
                display as text
        single column
            paragraph
                SelectOption
                    label is "ShipmentType"
            text of "Or"
            paragraph
                FromShipment
                    label is "ShipmentRange"
                ToShipment
                    no label
        single column
            ShipDate
                label is "ShipDateCutoff"
        single column
            InvoiceDate
        single column
            visible when (not Company.ManualInvoiceNumbering)
                ReleaseOpt
                    label is "ReleaseOption"
        visible when (Company.UseIDM)
            header2 of "PrinterOption"
            paragraph
                IDMPrinter
                IDMPrinter.Description
                    no label
                    display as text
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                OrderEntryDailyShipmentJournalReport_RDG
                OrderEntryDailyShipmentJournalFreightInvoice_RDG
                OrderEntryDailyShipmentJournalError_RDG
            column2 of 25%
                OrderEntryDailyShipmentJournalReport_XPT
                OrderEntryDailyShipmentJournalFreightInvoice_XPT
                OrderEntryDailyShipmentJournalError_XPT
