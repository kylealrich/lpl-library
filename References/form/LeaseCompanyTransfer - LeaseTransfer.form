LeaseTransfer is a WizardForm
    use for action Unreleased.Create
    title is "LeaseTransfer"
    show panel navigation
        last panel navigation is LeaseLink
            next button title is "Finish"
    LeaseCompanyTransferDetails is a Panel
        panel text is "LeaseTransferDetails"
        form is LeaseTransferWizardMain
        save on next
    NonLeaseComponentAndTermination is a Panel
        panel text is "NonLeaseComponentsDetails"
        form is LeaseTransferNonLeaseComponentsAndTermination
        save on next
    LeaseCompanyTransferSummaryForm is a Panel
        panel text is "LeaseCompanyTransferSummary"
        form is LeaseCompanyTransferSummaryForm
    AssetTransfer is a Panel
        panel text is "TransferAsset"
        form is LeaseTransferAssetTransferForm
    CompleteTransfer is a Panel
        panel text is "CompleteTransfer"
        form is LeaseTransferCompleteTransferForm
