CodeBlockRelationRule is a Form
    title is "FinanceStructureRelation"
    Layout
        paragraph
            CodeBlockRelation.DerivedFromLabel
                no label
                display as text
            CodeBlockRelation.DerivedToLabel
                label is "RelatesTo"
                display as text
        single column
            Description
            Active
            DynamicCreate
        single column
            visible when (not ToRequired)
                CreateBlankTo
                    label is "CreateBlankToValueForEachValidFromValue"
        single column
            visible when (not FromRequired)
                CreateBlankFrom
                    label is "CreateBlankFromValueForEachValidToValue"
        single column
            DefaultToValue
        two column
            button of "PreviewLedgers"
                visible when (IsFromGroupLedger)
                link is LedgerGroupPreview
            button of "PreviewLedgers"
                visible when (IsFromSingleLedger)
                link is LedgerSinglePreview
            button of "Preview<FinanceEnterpriseGroup.AccountingEntityLabel>"
                visible when (IsFromToGroupEntity)
                link is EntityGroupPreview
            button of "Preview<FinanceEnterpriseGroup.AccountingEntityLabel>"
                visible when (IsFromToSingleEntity)
                link is EntitySinglePreview
            button of "Preview<FinanceEnterpriseGroup.AccountingUnitLabel>"
                visible when (IsFromToGroupAU)
                link is AccountingUnitGroupPreview
            button of "Preview<FinanceEnterpriseGroup.AccountingUnitLabel>"
                visible when (IsFromToSummaryAU)
                link is AccountingUnitSummaryPreview
            button of "Preview<FinanceEnterpriseGroup.AccountLabel>"
