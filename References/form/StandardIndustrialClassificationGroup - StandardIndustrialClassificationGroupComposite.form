StandardIndustrialClassificationGroupComposite is a CompositeForm
    is primary
    propagate as a drill
    title is "StandardIndustrialClassificationGroup"
    context form is StandardIndustrialClassificationGroup
    SICCodes is a Panel
        list is StandardIndustrialClassification set.primary
    AssociatedCountries is a Panel
        title is "AssociatedCountries/\\Jurisdictions"
        list is Country set.Countries
            helper list is CountryRel.Countries
                action is AssociateSICGroup
                    invoked.PrmSICGroup = StandardIndustrialClassificationGroup
