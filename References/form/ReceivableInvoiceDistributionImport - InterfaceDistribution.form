InterfaceDistribution is a Form
    is primary
    restrict action CreateFromConversion
    restrict action UpateFromConversion
    Layout
        paragraph
            RunGroup
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        paragraph
            InvoiceType
                label is "TransactionType"
        paragraph
            Invoice
                label is "Transaction"
        paragraph
            Sequence
                label is "SequenceNumber"
        paragraph
            Company
                label is "GlobalLedgerCompany"
            Company.Name
                no label
                display as text
        paragraph
            DistributionCategory
        paragraph
            DistributionAccount
                compact format
        paragraph
            TransactionAmount
        paragraph
            Units
                label is "Unit"
        paragraph
            Description
        paragraph
            TaxCode
                select is TaxCodes
        paragraph
            TransactionTaxableAmount
                label is "TaxableAmount"
        visible when (TaxEntityUsesOneSource)
            two column
                UNSPSCCode
                    compact format
                UNSPSCCode.ShortDescription
                    no label
                    display as text
        paragraph
            LineType
        visible when (IsHSNSACCodeEnabled)
            paragraph
                HSNSACCode
                HSNSACCode.Description
                    no label
                    display as text
        paragraph
            ICNCode
                select is IntrastatICNCode
                show up to 10 characters
        paragraph
            SupplementaryQuantity
        paragraph
            StockWeight
                label is "Weight"
        visible when (ShowTaxOriginCrossReference)
            paragraph
                TaxOriginCrossReference
