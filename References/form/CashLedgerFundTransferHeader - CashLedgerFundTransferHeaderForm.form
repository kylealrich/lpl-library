CashLedgerFundTransferHeaderForm is a CompositeForm
    is primary
    title is "<DerivedTransferTypeLabel>"
    context form is UpdateHeaderContext
        background is theme color
    Actions
        action is Release
            label is "Release"
            hide when invalid
        action is CreateBankFundTransferDeposits
            label is "CreateDeposits"
        action is CreateBankFundTransferWithdrawals
            label is "CreateWithdrawals"
        action is CreateCashCodeFundTransfer
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is save
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    Main is a Panel
        form is Main
    BankCharge is a Panel
        visible when (TransferType.MultipleDeposits)
        form is TransferBankCharges
    CurrencyAmounts is a Panel
        title is "CurrencyAmounts"
        visible when (not TransferType.CashCodeFundTransfer)
        form is CurrencyAmountsForm
    TransferDetail is a Panel
        visible when (not TransferType.CashCodeFundTransfer)
        list is CashLedgerFundTransferDetail set.primary
    CashCodeTransferDetail is a Panel
        visible when (TransferType.CashCodeFundTransfer)
        list is CashCodeFundTransferDetail set.primary
    Errors is a Panel
        title is "Errors<DerivedBankFundTransferErrorCount>"
        visible when (BankFundTransferErrorsExist)
        list is CashLedgerTransactionError set.CashLedgerFundTransferHeaderErrorList
