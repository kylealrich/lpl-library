CashLedgerAutoTranNumberForm is a Form
    is primary
    title is "CashLedgerAutoTransactionNumber"
    Layout
        two column distributed
            column1
                CashManagementGroup
                CashLedgerDocumentType
                paragraph
                    BankTransactionCode
                        label is "TransactionCode"
                    BankTransactionCode.Description
                        no label
                        display as text
                visible when (RecordExists)
                    CreateOperator
                        display as text
                visible when (DocumentDataExists)
                    DocumentCashCode
                        display as text
                visible when (DocumentDeleted)
                    DeleteOperator
                        label is "DocumentDeleteOperator"
                        display as text
            column2
                paragraph
                    Company
                    Company.Name
                        no label
                        display as text
                paragraph
                    CashCode
                    CashCode.Description
                        no label
                        display as text
                visible when (not RecordExists)
                    LastNumberUsed
                visible when (RecordExists)
                    AutoTransactionNumber
                visible when (RecordExists)
                    CreateDate
                        display as text
                visible when (DocumentDataExists)
                    DocumentBankTransactionCode
                        label is "DocumentTransactionCode"
                        display as text
                visible when (DocumentDeleted)
                    DeleteDate
                        label is "DocumentDeleteDateAndTime"
                        display as text
