UserFormChangesForm is a Form
    disable field options
    Layout
        visible when (TransactionRecordExists)
            paragraph
                UserFormRel.BusinessView
                    header2
                    display as text
                    mouse over text is "ConfiguredBusinessView"
                UserFormRel.BusinessForm
                    header2
                    display as text
                visible when (UserFormRel.Global)
                    text of "(Configuration)"
                        header3
                visible when (UserFormRel.IsFolder)
                    text of "(Folder)"
                        header3
                visible when (UserFormRel.IsActor)
                    text of "(Personalization)"
                        header3
                button of "<BusinessView>Details"
                    link is UserFormNav
                    display as link
        visible when (not TransactionRecordExists)
            paragraph
                text of "<BusinessView>RecordDoesNotExist"
