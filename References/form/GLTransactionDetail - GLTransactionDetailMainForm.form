GLTransactionDetailMainForm is a Form
    title is "Main"
    Layout
        two column distributed
            AccountingEntity
                label is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
            PostingDate
        two column distributed
            FinanceCodeBlock
                label is "FinanceStructure"
                compact format
            FinanceCodeBlock.GeneralLedgerChartAccount.AccountDescription
        two column distributed
            Description
            JournalCode
        two column distributed
            TransactionDate
            paragraph
                System
                AutoReverse
        two column distributed
            GeneralLedgerEvent
                label is "Event"
            Reference
        two column distributed
            DocumentNumber
            RelatedJournalTransaction.BusinessClassName
                label is "LinkedTo"
            RelatedJournalTransaction.SymbolicKeyText
                label is "KeyFields"
            OriginatingTransaction.BusinessClassName
                label is "SourceTransaction"
        two column distributed
            button of "ViewExpenseReport"
                visible when (ViewExpenseManagementLink)
                link is external "<DerivedExpenseManagementURL>"
                label is "ExpenseReport"
                display as link
