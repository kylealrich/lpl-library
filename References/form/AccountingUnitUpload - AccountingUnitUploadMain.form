        paragraph
            AccountingEntity
            AccountingEntity.Name
                no label
                display as text
        single column
            RecordType
                label is "Type"
        two column
            AccountingUnit
                label is "<FinanceEnterpriseGroup.AccountingUnitLabel>"
            Description
        single column
            visible when (UsedForFundAccounting)
                FundType
                    label is "FundType"
        single column
            ParentAccountingUnit
                label is "Parent<FinanceEnterpriseGroup.AccountingUnitLabel>"
        single column
            Active
        two column
            AddressCode
            AddressCode.Description
                no label
                display as text
        single column
            SystemAccountingUnit
            visible when (CanDefineZones)
                IsAZoneAccountingUnit
            visible when (CanDefineZones)
                ZoneAccountingUnit
            visible when (CanDefineZones)
                EntityZoneAccountingUnit
            visible when (CanDefineZones)
                DefaultZoneAccountingUnit
            InterEntityAccountingUnit
            visible when (RecordType.Summary)
                BudgetIdentifier
            visible when (RecordType.Posting)
                IncludeInBudgetEdit
            ErrorMessage
