BankServiceDetailForm is a Form
    is primary
    title is "FeeServiceDetail"
    Layout
        single column
            BankServiceStatement
                display as text
        paragraph
            FinancialInstitution
                display as text
            FinancialInstitution.Description
                no label
                display as text
        paragraph
            CashManagementAccount
                select is BankServiceStatementCashManagementAccounts
            CashManagementAccount.Title
                no label
                display as text
        header2 of "FeeDetail"
        paragraph
            BankServiceCode
                select is BankServiceCodeSelectList
            BankServiceCode.Description
                no label
                display as text
        two column
            Quantity
            QuantityType
        paragraph
            PricePerUnit
            BankServiceCode.ZeroDollarAmount
            BankServiceCode.UnitPrice
                label is "ContractedPrice"
                display as text
            FeeDifference
                display as text
        three column
            BankChargedAmount
            CalculatedChargedAmount
                display as text
            ChargedAmountDifference
                display as text
        two column
            Resolved
        single column
            Comment
            update stamp
                label is "LastUpdated"
                display as text
