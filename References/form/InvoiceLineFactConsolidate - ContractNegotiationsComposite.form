ContractNegotiationsComposite is a CompositeForm
    title is "ContractNegotiations"
    context form is ContractComparisionContext
    ContractLinesForItem is a Panel
        list is ContractLinesForNegotiationRel.ContractLinesForItemNegotiation
    ContractNegotiations is a Panel
        visible when (HasContractNegotiationsForContracts)
        list is ContractNegotiationsForContractsRel.ContractNegotiationWithContracts
