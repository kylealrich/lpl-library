CashCodeBankFundTransferForm is a Form
    is primary
    Layout
        paragraph
            CashCode.CashCode.CashCodeAndDescription
                label is "CashCode"
                display as text
            CashCode.CashCode.Currency
                no label
                display as text
        blank line
        two column
            column1
                FromBankTransactionCode
                    select is BankServices
                ToBankTransactionCode
                    select is BankServices
                ExpenseBankTransactionCode
                    select is BankServices
            column2
                FromBankTransactionCode.Description
                    no label
                    display as text
                ToBankTransactionCode.Description
                    no label
                    display as text
                ExpenseBankTransactionCode.Description
                    no label
                    display as text
