AcceptSuggestedItem is an ActionForm
    action is AcceptSuggestedItem
    Layout
        display as text
            single column
                ParmContractGroup
                text of "CurrentValues:"
                    bold
                DerivedContractLineItem
                    label is "ItemNumber"
                EnteredVendorItem
                    label is "VendorItem"
            paragraph
                EnteredManufacturer
                    label is "Manufacturer"
                EnteredManufacturerNumber
                    label is "ManufacturerNumber"
            single column
                text of "SuggestedValuesFor:"
                    bold
                SuggestedItem
                    label is "Item"
                SuggestedVendorItem
                    label is "VendorItem"
                SuggestedManufacturer
                    label is "Manufacturer"
                    compact format
                SuggestedManufacturerNumber
                    label is "ManufacturerNumber"
