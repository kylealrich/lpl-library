ContractTierDetails is a CompositeForm
    title is "ContractTier"
    context form is ContractTierContext
        background is theme color
    Actions
        navigation is TierContractsAndItems
            label is "QualifierInformation"
        action is save
    Tier is a Panel
        form is ContractTierMain
    TierQualifiers is a Panel
        list is ContractTierQualifier set.ContractTierQualifiers
