EFTNotificationPrint is an ActionForm
    action is PerformEFTNotificationPrint
    Layout
        paragraph
            FinanceEnterpriseGroup
                label is "FinanceEnterpriseGroup"
            FinanceEnterpriseGroup.EnterpriseGroup.Description
                no label
                display as text
        paragraph
            CustGroup
                label is "CustomerGroup"
            CustGroup.Name
                no label
                display as text
        single column
            text of "Or"
        paragraph
            Company
                label is "Company"
            Company.Name
                no label
                display as text
        paragraph
            BankTransactionCode
                label is "PaymentCode"
                select is CashPaymentAndOriginReceivablesList
            BankTransactionCode.Description
                no label
                display as text
        blank line
        single column
            DataFileName
                label is "DataFileName"
        header2 of "PrinterOption"
        paragraph
            IDMPrinter
                label is "IDMPrinter"
            IDMPrinter.Description
                no label
                display as text
