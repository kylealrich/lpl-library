PurgeWireTransferOutput is an ActionForm
    action is PurgeWireTransferOutput
    Layout
        header3 of "EnterTheWireTransferBatchOrCreateDateRangeYouWouldLikeToPurge"
        single column
            PrmWireTransferBatch
                label is "WireTransferBatch"
                select is ReleasedWireTransferBatch
            text of "Or"
        paragraph
            PrmCreateDateRange.Begin
                label is "CreateDateRange"
            text of "_-"
            PrmCreateDateRange.End
                no label
