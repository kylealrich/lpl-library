PriceBreakForm is a Form
    Layout
        visible when (PriceBreaksAllowed)
            visible when (not Contract.ServicePurchaseType)
                header2 of "PurchaseOrServicePricing"
            visible when (Contract.ServicePurchaseType)
                header2 of "ServicePricing"
            single column
                PricingMarkupOrDiscount
                    label is "MarkupOrDiscount"
                LinePriceBreakOn
                    label is "ByQuantityOrAmount"
            three column
                PriceBreak.QuantityPercentPrice[1].Quantity
                    label is "QuantityOrAmount"
                PriceBreak.QuantityPercentPrice[1].UnitPercent
                    label is "NewPercent"
                PriceBreak.QuantityPercentPrice[1].UnitPrice
                    label is "NewCost"
            three column
                PriceBreak.QuantityPercentPrice[2].Quantity
                    no label
                PriceBreak.QuantityPercentPrice[2].UnitPercent
                    no label
                PriceBreak.QuantityPercentPrice[2].UnitPrice
                    no label
            three column
                PriceBreak.QuantityPercentPrice[3].Quantity
                    no label
                PriceBreak.QuantityPercentPrice[3].UnitPercent
                    no label
                PriceBreak.QuantityPercentPrice[3].UnitPrice
                    no label
            three column
                PriceBreak.QuantityPercentPrice[4].Quantity
                    no label
                PriceBreak.QuantityPercentPrice[4].UnitPercent
                    no label
                PriceBreak.QuantityPercentPrice[4].UnitPrice
                    no label
            three column
                PriceBreak.QuantityPercentPrice[5].Quantity
                    no label
                PriceBreak.QuantityPercentPrice[5].UnitPercent
                    no label
                PriceBreak.QuantityPercentPrice[5].UnitPrice
                    no label
        visible when (CsnPriceBreaksAllowed)
            header2 of "ConsignmentPricing"
            single column
                ConsignmentPricingMarkupOrDiscount
                    label is "MarkupOrDiscount"
                ConsignmentLinePriceBreakOn
                    label is "ByQuantityOrAmount"
            three column
                ConsignmentPriceBreak.QuantityPercentPrice[1].Quantity
                    label is "QuantityOrAmount"
                ConsignmentPriceBreak.QuantityPercentPrice[1].UnitPercent
                    label is "NewPercent"
                ConsignmentPriceBreak.QuantityPercentPrice[1].UnitPrice
                    label is "NewCost"
            three column
                ConsignmentPriceBreak.QuantityPercentPrice[2].Quantity
                    no label
                ConsignmentPriceBreak.QuantityPercentPrice[2].UnitPercent
                    no label
                ConsignmentPriceBreak.QuantityPercentPrice[2].UnitPrice
                    no label
            three column
                ConsignmentPriceBreak.QuantityPercentPrice[3].Quantity
                    no label
                ConsignmentPriceBreak.QuantityPercentPrice[3].UnitPercent
                    no label
                ConsignmentPriceBreak.QuantityPercentPrice[3].UnitPrice
                    no label
            three column
                ConsignmentPriceBreak.QuantityPercentPrice[4].Quantity
                    no label
                ConsignmentPriceBreak.QuantityPercentPrice[4].UnitPercent
                    no label
                ConsignmentPriceBreak.QuantityPercentPrice[4].UnitPrice
                    no label
            three column
                ConsignmentPriceBreak.QuantityPercentPrice[5].Quantity
                    no label
                ConsignmentPriceBreak.QuantityPercentPrice[5].UnitPercent
                    no label
                ConsignmentPriceBreak.QuantityPercentPrice[5].UnitPrice
                    no label
