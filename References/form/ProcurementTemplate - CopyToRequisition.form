CopyToRequisition is an ActionForm
    action is CopyToRequisition
    Layout
        paragraph
            PrmCompany
                label is "Company"
        paragraph
            PrmRequisition
                label is "Requisition"
                select is UnreleasedRequisitionsForACompany
        paragraph
            PrmRequestingLocation
                label is "RequestingLocation"
            PrmRequestingLocation.Name
                no label
                display as text
        paragraph
            PrmFromCompanyLocation.FromCompany
                label is "FromCompany"
            PrmFromCompanyLocation.FromCompany.Name
                no label
                display as text
        paragraph
            PrmFromCompanyLocation.FromLocation
                label is "FromLocation"
            PrmFromCompanyLocation.FromLocation.Name
                no label
                display as text
        paragraph
            PrmDate
                label is "RequestedDeliveryDate"
        paragraph
            PrmVendor
                label is "Vendor"
        paragraph
            PrmBuyer
                label is "Buyer"
        paragraph
            PrmDeliverTo
                label is "DeliverTo"
        paragraph
            PrmPOCode
                label is "POCode"
        paragraph
            PrmAllocationPriority
                label is "AllocationPriority"
        paragraph
            PrmPurchaseFromLocation
                label is "PurchaseFromLocation"
        paragraph
            PrmPurchaseTaxCode
                label is "PurchaseTaxCode"
        paragraph
            PrmPurchaseTaxable
                label is "PurchaseTaxable"
        paragraph
            PrmCommodityCode
                label is "CommodityCode"
        visible when (false)
            paragraph
                PrmLimitCopy
