AssetInterface is an ActionForm
    action is AssetInterface
    title is "AssetImport"
    Layout
        paragraph
            RunGroup
        paragraph
            FinanceEnterpriseGroup
            FinanceEnterpriseGroup.EnterpriseGroup.Description
                no label
                display as text
        blank line
        paragraph
            Company
                label is "Company"
                select is Companies
            CompanyGroup
                label is "OrCompanyGroup"
        paragraph
            LeaseCompany
                label is "LeaseCompany"
                select is Companies
            Lease
                label is "Lease"
        blank line
        single column
            ErrorLimit
            ComputeOption
                when value changed
                    refresh ComputeOption
            visible when (ComputeOption not entered)
                TransientAllowZeroLifeRemaining
                    label is "AllowZeroLifeRemaining"
            visible when (ComputeOption entered)
                visible when (ComputeOption.No)
                    TransientAllowZeroLifeRemaining
                        label is "AllowZeroLifeRemaining"
        blank line
        single column
            FilesToConvert
                label is "Files_toConvert"
        paragraph
            FromRange
                label is "FromImportGroup"
            ToRange
                label is "ToImportGroup"
        blank line
        header2 of "ForTransactionsOnly"
        blank line
        single column
            AttributeBy
                label is "Conversion"
