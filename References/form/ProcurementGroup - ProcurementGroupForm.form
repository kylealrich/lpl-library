ProcurementGroupForm is a CompositeForm
    is primary
    context form is ProcurementGroupFormContext
        background is theme color
    Main is a Panel
        form is Main
    UserFields is a Panel
        form is UserFields
    ShipTerms is a Panel
        list is ShipTerm set.ShipTermsMain
    FreightTerms is a Panel
        list is FreightTerm set.FreightTermsLists
    POCodes is a Panel
        list is POCode set.POCodesLists
    BuyerGroups is a Panel
        list is BuyerGroup set.BuyerGroupsMain
    Buyers is a Panel
        list is BuyerRel.BuyerListMain
    VendorItems is a Panel
        list is VendorItem set.VendorItems
    ProcurementFieldMapping is a Panel
        title is "ExtrinsicFieldMapping"
        visible when (AribaEnabled)
        list is ProcurementFieldMapping set.ProcurementFieldMappingList
