RecurringInvoiceResultComposite is a CompositeForm
    title is "<RecurringFormTitle>"
    context form is RecurringInvoiceResultContext
        background is theme color
    InvoiceTemplates is a Panel
        list is CustomerRecurringInvoicesRel.CustomerRecurringInvoiceTemplatesForPreview
    Invoices is a Panel
        title is "ReceivableInvoices"
        visible when (ReceivableInvoicesExist)
        list is ReceivableInvoiceRel.RecurringReceivableInvoicesList
    Errors is a Panel
        title is "Errors"
        visible when (ErrorsExists)
        list is ReceivablesJobErrorResultRel.RecurringInvoiceErrors
