RetrieveNRACRecalls is an ActionForm
    action is RetrieveNRACRecalls
    Layout
        blank line
        paragraph
            PrmRecallGroup
                label is "Group"
            PrmRecallGroup.Description
                no label
                display as text
        blank line
        visible when (not RetrieveForCurrentDate)
            header5 of "DateRange"
            paragraph
                FromDate
                    label is "From"
                ToDate
                    label is "To"
                text of "(DefaultsToLastRetrievalDateUpToCurrentDate;CannotBeGreaterThanASevenDayRange)"
                    italics
        paragraph
            visible when (not RetrieveForCurrentDate)
                text of "Or"
            RetrieveForCurrentDate
                label is "RetrieveAllNoticesForCurrentDate"
        blank line
        single column
            PrmRecallCoordinator
                label is "NoticeCoordinator"
                select is RecallCoordinatorList
            PrmRecallType
                select is RecallTypesNotForItemProposal
            SetPriorityToHighForFDAClassINotices
