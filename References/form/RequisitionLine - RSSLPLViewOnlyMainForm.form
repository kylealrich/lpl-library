RSSLPLViewOnlyMainForm is a Form
    title is "Requisition<Requisition.Requisition>Line<RequisitionLine>"
    Layout
        display as text
            four column distributed
                column1
                    visible when (UseLinkedImage)
                        DerivedImageURL
                            display as image
                    visible when (not UseLinkedImage)
                        DerivedPicture
                            no label
                            align as label
                            display as full image up to 6 lines
                                missing image is random background
                                    foreground text is "<RequisitionLine.RequisitionLineItemAbbreviation>"
                column2
                    text of "<Item>_-_<Description>"
                    Manufacturer.DerivedCodeDivisionDescription
                        label is "Manufacturer"
                column3
                    Item.Description2
                        no label
                    paragraph
                        visible when (HasContractAndDisplayContract)
                            button of "<OnContractLabel>" with icon.new-solid
                                link is ContractLineNav
                                display as link
                        visible when (OriginalItem entered)
                            button of "<OriginalItemMessage>ForItem<OriginalItem>" with icon.yellowAlert
                                link is OriginalItemNav
                                display as link
                        visible when (RecallProductItemForItemRelExists)
                            button of "Recall" with icon.CSFRecall
                                link is RecallNav
                                display as link
                column4
                    visible when (Item.HasDesc3)
                        Item.Description3
                            no label
                    DerivedStatusMessage
                        label is "Status"
                        display as tag
                            color of blue when (IsUnreleased)
                            color of green when (Status.Processed)
                            color of red when (IsRejectedStatus)
                            color of yellow when (IsReleasedStatus)
                    button of "AddItem_toRequisition"
                        visible when (not IsUnreleased)
                        action is ModalCopyRequisitionLine
                        is primary
                    paragraph
                        visible when (LatexItem)
                            LatexLabel
                                no label
                                display as tag
                                    color of orange
                        visible when (MissingDistribution)
                            MissingDistribMessage
                                no label
                                display as tag
                                    color of orange
                        visible when (Item.Recipe)
                            RecipeItemLabel
                                no label
                                display as tag
                                    color of blue
            four column distributed
                paragraph
                    visible when (DisplayVendor)
                        Vendor
                        Vendor.VendorName
                            no label
                paragraph
                    visible when (DisplayVendorItem)
                        VendorItem
                        VendorItem.VendorItemDescription
                            no label
                ItemType
                visible when (DisplayServiceInfo)
                    ServiceCode
            four column distributed
                visible when (DisplayRequestedDeliveryDate)
                    RequestedDeliveryDate
                visible when (RSSLPLDisplayViewOnlyMainPriority)
                    AllocationPriority
                        label is "Priority"
                visible when (DisplayCostOption)
                    CostOption
                OverrideCost
            four column distributed
                LateDeliveryDate
                CreatePurchaseOrder
                CostCode
                DerivedTransactionTypeMessage
                    label is "TransactionType"
            visible when (DisplayGenericName)
                four column distributed
                    Item.GenericName
