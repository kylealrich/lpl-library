PhoneGeneralLedgerJournalControlForm is a Form
    propagate as a drill
    title is "JournalControl"
    Layout
        single column
            DerivedAccountingEntityNameID
                display as text
            DerivedJournalID
                display as text
        two column distributed
            PostingDate
                label is "PostDate"
                display as text
            TransactionDate
                display as text
        single column
            AccountingEntity.FunctionalCurrency
                label is "<FinanceEnterpriseGroup.AccountingEntityLabel>Currency"
                display as text
        two column distributed
            FunctionalDebitAmount
                label is "Debit"
                display as text
            FunctionalCreditAmount
                label is "Credit"
                display as text
        single column
            visible when (AlternateCurrencyExists)
                AccountingEntity.AlternateCurrency
                    label is "TransactionCurrency"
                    display as text
        two column distributed
            visible when (AlternateCurrencyExists)
                AlternateDebitAmount
                    label is "Debit"
                    display as text
                AlternateCreditAmount
                    label is "Credit"
                    display as text
        single column
            TransientUpdateStamp
                label is "LastUpdated"
                display as text
            create stamp.actor
                label is "actor"
                display as text
