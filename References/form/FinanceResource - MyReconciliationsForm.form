MyReconciliationsForm is a CompositeForm
    title is "Reconciliations_for<EmployeeRel.Name.PreferredFirstAndLastName>"
    restrict action Create
    restrict action Delete
    restrict action CreateAccountResource
    restrict action WorkRequest
    restrict action InactivateAndReassign
    restrict action AddToTeam
    restrict action Reload
    Actions
        action is save
            label is "_"
        action is refresh
            label is "_"
    OpenReconciliations is a Panel
        visible when (ShowMyPeriodEndReconciliations)
        list is MyPeriodEndReconciliationsRel.MyPeriodEndReconciliationList
    TeamReconciliations is a Panel
        visible when (ShowMyTeamPeriodEndReconciliations)
        list is MyTeamPeriodEndReconciliationsRel.MyTeamPeriodEndReconciliationList
    ClosedReconciliations is a Panel
        visible when (ShowMyClosedPeriodEndReconciliations)
        list is MyClosedPeriodEndReconciliationsRel.MyClosedPeriodEndReconciliationList
