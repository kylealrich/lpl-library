SendOffsite is an ActionForm
    action is InProduction.SendOffsite
    Layout
        paragraph
            Company
                display as text
            Company.Name
                no label
                display as text
        paragraph
            InventoryLocation
                label is "Location"
            InventoryLocation.Name
                no label
                display as text
        paragraph
            WorkOrder
        paragraph
            PrmHoldCode
                label is "HoldCode"
        paragraph
            PrmVendor
                label is "Vendor"
        paragraph
            PrmOffsiteDate
                label is "DateSent"
