MobileTransferWizardSummary is a Form
    title is "AssetTransfer"
    Layout
        header3 of "TransferTo"
            underline color of blue
        blank line
        blank line
        display as text
            two column table
                column1
                    TransferQuantity
                        label is "Quantity"
                    AssetTransferCompanyName
                        label is "Company"
                    AssetTransferToLocation
                        label is "Location"
                    AssetTransferTo.ToAssetType
                        label is "Type"
                        compact format
                    AssetTransferToDimensionGroup
                        label is "DimensionGroup"
                column2
                    TransferDate
                    AssetTranferToGroup
                        label is "AssetGroup"
                    AssetTransferToDivision
                        label is "Division"
                    AssetTransferToOwner
                        label is "Owner"
                    AssetTransferToAccountGroup
                        label is "AccountGroup"
        blank line
        single column
            button of "Release"
                valid when (Asset.UnreleasedAssetTransferExists)
                action is AssetReleaseTransfer
                is primary
