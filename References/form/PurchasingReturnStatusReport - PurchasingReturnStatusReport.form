PurchasingReturnStatusReport is an ActionForm
    action is PurchasingReturnStatusReport
    Layout
        paragraph
            CompanyGroup
                show up to 10 characters
            CompanyGroup.Description
                no label
                display as text
        single column
            text of "\\or"
        paragraph
            Company
            Company.Name
                no label
                display as text
        header2 of "ReturnCreatedDates"
        paragraph
            FrCreationDate
                label is "From"
            ToCreationDate
                label is "To"
        text of "_"
        header2 of "_"
        single column
            text of "_"
            PthUserId
                label is "CreatedBy"
                show up to 10 characters
        paragraph
            ReturnFrmLoc
                label is "ReturnFrom"
                show up to 10 characters
            ReturnFrmLoc.Name
                no label
                display as text
        paragraph
            VendorGroup
            Vendor
            Vendor.VendorName
                no label
                display as text
        paragraph
            PurchFrLoc
                label is "ReturnTo"
                show up to 10 characters
            PurchFrLoc.VendorName
                no label
                display as text
        paragraph
            SelectOption
                label is "Status"
            SortOption
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                PurchasingReturnStatusReportReport_RDG
            column2 of 25%
                PurchasingReturnStatusReportReport_XPT
