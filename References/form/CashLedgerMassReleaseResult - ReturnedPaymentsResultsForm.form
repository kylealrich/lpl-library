ReturnedPaymentsResultsForm is a CompositeForm
    title is "ReturnedPaymentMassReleaseResult<CashLedgerMassReleaseResult>"
    context form is ReturnedPaymentsResultsContext
        background is theme color
    Actions
        action is RerunMassReleaseForReturnedPaymentErrors
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    Parameters is a Panel
        form is ParametersForm
    ReturnedPaymentsReleased is a Panel
        title is "Released_(<ReleaseCount>)"
        list is ReturnedPaymentsReleasedRel.ReleasedReturnedPaymentsList
    ReturnedPaymentsInError is a Panel
        title is "Errors_(<ErrorCount>)"
        visible when (ReturnedPaymentErrorsExist)
        list is ReturnedPaymentsWithErrorsRel.MassReleaseReturnedPaymentErrorsList
