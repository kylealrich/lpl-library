GenerationOfBankRemittance is an ActionForm
    action is GenerationOfBankRemittance
    Layout
        paragraph
            Company
                label is "RemittanceCompany"
                select is ARCompanies
            Company.Name
                no label
                display as text
        paragraph
            ProcessLevel
                select is ProcessLevels
            ProcessLevel.Name
                no label
                display as text
        paragraph
            CashCode
            CashCode.Description
                no label
                display as text
        two column distributed
            column1
                RemitType
                    label is "RemittanceType"
            column2
                RemitDate
                    label is "RemittanceDate"
        two column distributed
            column1
                FrMaturityDate
                    label is "FromMaturityDate"
            column2
                ToMaturityDate
        blank line
        paragraph
            PrintOption
        paragraph
            ReprintRemitNbr
                label is "RemittanceNumber"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                GenerationOfBankRemittanceReport_RDG
            column2 of 25%
                GenerationOfBankRemittanceReport_XPT
