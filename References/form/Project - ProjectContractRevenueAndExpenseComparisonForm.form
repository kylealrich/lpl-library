ProjectContractRevenueAndExpenseComparisonForm is a SearchForm
    title is "RevenueToExpenseVariance"
    Layout
        four column
            SearchProjectContract
                label is "<FinanceEnterpriseGroup.ProjectLabel>Contract"
            SearchBillingMethod
                label is "BillingMethod"
            SearchRevenueRecognition
                label is "RevenueRecognition"
            SearchRevenueMethod
                label is "RevenueMethod"
        header4 of "GlobalLedgerTotalAmounts"
        four column
            SearchDateRange.Begin
                label is "BeginPeriodDate"
                display as required
            SearchDateRange.End
                label is "EndPeriodDate"
                display as required
            SuppressZeroVarianceRows
            ExpenseSelectionGroup
