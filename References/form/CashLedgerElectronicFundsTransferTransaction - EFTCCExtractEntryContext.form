EFTCCExtractEntryContext is a Form
    Layout
        visible when (RecordExists)
            three column distributed
                display as text
                    column1
                        paragraph
                            CashCode.CashCodeAndDescription
                                label is "CashCode"
                            CashCode.Currency
                                no label
                        Status
                        paragraph
                            ReceiptAmountUnsigned
                                label is "Amount"
                            visible when (IsUnreleased)
                                AppliedBankAmount
                                    label is "AppliedAmount"
                                BalanceAmount
                                    label is "Balance"
                        paragraph
                            visible when (BudgetEditInProgress)
                                text of "BudgetEditsInProgress"
                                    bold
                                    color of red
                            visible when (BudgetEditFailure)
                                text of "BudgetsExceeded;CouldNotRelease;SeeBudgetErrorsTab"
                                    bold
                                    color of red
                column2
                    display as text
                        CashLedgerElectronicFundsTransferTransaction.BankTransactionCode.BankTransactionCodeName
                            label is "TransactionCode"
                        paragraph
                            ElectronicFundsTransferDepositDate
                                label is "DepositDate"
                            visible when (VoidDate entered)
                                VoidDate
                                    label is "RejectDate"
                            visible when (ReturnedPaymentExists)
                                first CashLedgerReturnedPaymentRel.ReturnDate
                                    label is "ReturnDate"
                column3
                    display as text
                        paragraph
                            Reference
                                label is "ReferenceNumber"
                            CashLedgerTransactionIdentifier
                        visible when (TransactionNumberEntered)
                            TransactionNumber
                        visible when (IsBatchApproved)
                            CashLedgerBatchApproval
