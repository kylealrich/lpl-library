IntercompanyBillingSettlementDetailDistributionSetSelection is a CompositeForm
    title is "<DerivedDistributionSetSelectionTitle>"
    context form is IntercompanyBillingSettlementDetailLineSelectContextForm
        background is theme color
    restrict action RemoveInvoice
    Actions
        action is MatchPayableDistributionsToSelectedReceivables
            hide when invalid
        action is SelectInvoiceDetailForSettlementApply
            hide when invalid
        action is save
            label is "_"
        action is refresh
            label is "_"
    ReceivableDistributionSets is a Panel
        title is "Receivable"
        list is IntercompanyBillingDocumentHeader.ReceivableEntity.ReceivableInvoice.ReceivableInvoiceDetailRel.SettlementDetailSelectSequenceList
    PayableDistributionSets is a Panel
        title is "Payable"
        list is IntercompanyBillingDocumentHeader.PayableEntity.PayablesInvoice.PayablesInvoicePaymentRel.PayablesInvoicePaymentICBSettlementDetailLineSelectList
