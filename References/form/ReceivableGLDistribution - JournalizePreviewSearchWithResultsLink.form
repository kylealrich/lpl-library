JournalizePreviewSearchWithResultsLink is a SearchForm
    title is "JournalizePreview"
    Layout
        two column
            column1
                paragraph
                    ReceivableCompanyDataGroup.TransactionCompany
                    GeneralLedgerCompanyGroup
                        label is "CompanyGroup"
                    visible when (false)
                        GeneralLedgerDateRange.Begin
                            no label
                    GeneralLedgerDateRange.End
                        label is "ThroughPostDate"
            column2
                button of "ViewJournalizedDistributions"
                    link is JournalizeResultsNav
                    right align
                    display as link
