MobileMainForm is a Form
    title is "Main"
    Layout
        header3 of "_"
        paragraph
            WarehouseBatch
                label is "Batch"
                display as text
            ShipDate
        paragraph
            visible when (not IsShipped)
                Carrier
                    select is VendorByName
            visible when (not IsShipped)
                Carrier.VendorName
                    no label
                    display as text
        paragraph
            visible when (RequirePickingFeedback)
                PickDate
                    display as text
            visible when (RequirePackingFeedback)
                PackDate
                    display as text
        paragraph
            visible when (IsShipped)
                Carrier
                    display only
                    select is VendorByName
            visible when (IsShipped)
                Carrier.VendorName
                    no label
                    display as text
        paragraph
            visible when (IsShipped)
                FreightCode
                    label is "FreightCode"
                    display only
            visible when (IsShipped)
                FreightCharges
                    label is "Charges"
                    display only
        paragraph
            visible when (not IsShipped)
                FreightCode
                    label is "FreightCode"
            visible when (not IsShipped)
                FreightCharges
                    label is "Charges"
        header3 of "AdditionalShipmentInformation"
        single column
            visible when (IsShipped)
                GeneralLedgerDate
                    label is "PostingDate"
                    display only
                BillOfLading
                    label is "Bill_ofLadingNumber"
                    display only
                visible when (HasProofOfDeliveryRequired)
                    PODReturnDate
                        label is "P\\O\\DReturnedDate"
                visible when (not HasProofOfDeliveryRequired)
                    PODReturnDate
                        label is "P\\O\\DReturnedDate"
                        display only
                ShippingMethod
                    label is "Method"
                    display only
                TransactionId
                    label is "TransactionI\\D"
                    display only
                NumberOfCartons
                    label is "Cartons"
                    display only
            visible when (not IsShipped)
                GeneralLedgerDate
                    label is "PostingDate"
                BillOfLading
                    label is "Bill_ofLadingNumber"
                PODReturnDate
                    label is "P\\O\\DReturnedDate"
                ShippingMethod
                    label is "Method"
                TransactionId
                    label is "TransactionI\\D"
                NumberOfCartons
                    label is "Cartons"
        two column distributed
            visible when (IsShipped)
                StockVolume
                    label is "CubicFeet"
                    display only
            visible when (not IsShipped)
                StockVolume
                    label is "CubicFeet"
            visible when (IsShipped)
                StockWeight
                    label is "Weight"
                    display only
            visible when (not IsShipped)
                StockWeight
                    label is "Weight"
        header3 of "Global"
        paragraph
            visible when (IsShipped)
                ShipTerm
                    label is "DeliveryTerms"
                    display only
                PortCode
                    label is "LoadingPort"
                    display only
            visible when (not IsShipped)
                ShipTerm
                    label is "DeliveryTerms"
                TransientIntrastatPortCode
                    select is IntrastatPortCodeForInventoryLocationRel.IntrastatPortCodeList
        single column
            visible when (IsShipped)
                StatisticalProcedure
                    display only
                NatureOfTransactionCode
                    label is "N\\O\\T\\C"
                    display as text
                GlobalDocumentType
                    display only
            visible when (not IsShipped)
                StatisticalProcedure
                NatureOfTransactionCode
                    label is "N\\O\\T\\C"
                    display as text
                GlobalDocumentType
