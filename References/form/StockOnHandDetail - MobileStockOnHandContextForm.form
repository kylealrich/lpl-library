MobileStockOnHandContextForm is a Form
    title is "StockOnHandStatus"
    Layout
        header2 of "_"
        display as text
            two column table
                column1
                    ItemLocation.Company
                    Company.GeneralLedgerCompany.Currency
                        label is "Currency"
                    ItemLocation.Item
                        label is "Item"
                    Item.Description
                    ItemLocation.InventoryLocation
                        label is "Location"
                column2
                    Company.Name
                        label is "CompanyName"
                    Item.StockUOM
                        label is "StockUnit_ofMeasure"
                        display as text
                    blank line
                    InventoryLocation.Name
                        label is "LocationName"
