GarnishmentsForm is a Form
    title is "Garnishments"
    Layout
        three column distributed
            column1
                PayablesInvoiceSensitiveData.GarnFileNumber
                    label is "GarnFileNumber"
                PayablesInvoiceSensitiveData.GarnFIPSCode
                    label is "GarnFIPSCode"
                PayablesInvoiceSensitiveData.GarnParentName
                    label is "GarnParentName"
            column2
                PayablesInvoiceSensitiveData.GarnSocialSecurityNumber
                    label is "GarnSocialSecurityNumber"
                PayablesInvoiceSensitiveData.GarnTerminationIndicator
                    label is "GarnTerminationIndicator"
                button of "UpdateGarnishments"
                    visible when (IsUpdateGarnishments)
                    link is PayablesInvoiceSensitiveDataForm
            column3
                PayablesInvoiceSensitiveData.GarnCaseNumber
                    label is "GarnCaseNumber"
                PayablesInvoiceSensitiveData.GarnMedicalIndicator
                    label is "GarnMedicalIndicator"
