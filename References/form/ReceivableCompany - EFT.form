EFT is a Form
    Layout
        header2 of "CompanyProcessing"
        paragraph
            EnableElectronicFundsTransfer
                label is "EnableElectronicFundsTransfer"
                    left of field
            DiscountsNotAllowed
        paragraph
            EFTCalendar
                label is "SystemCalendar"
            EFTCalendar.Description
                no label
                display as text
        paragraph
            EFTDueDays
                label is "DueDays"
            EFTDueDaysType
                label is "Type"
            EFTGLDays
                label is "GlobalLedgerDays"
            EFTGLDaysType
                label is "Type"
            EFTDepositDays
                label is "DepositDays"
            EFTDepositDaysType
                label is "Type"
        blank line
        header2 of "Group_orCompanyProcessing"
        two column
            column1
                paragraph
                    EFTDebitCashCode
                        label is "DepositCashCode"
                        select is CashCodeForCompanyList
                    EFTDebitCashCode.Description
                        no label
                        display as text
                paragraph
                    EFTCreditCashCode
                        label is "CreditCashCode"
                        select is CashCodeForCompanyList
                    EFTCreditCashCode.Description
                        no label
                        display as text
            column2
                paragraph
                    EFTDepositBankCode
                        label is "DepositPaymentCode"
                        select is BankServices
                    EFTDepositBankCode.Description
                        no label
                        display as text
                paragraph
                    EFTCreditPaymentBankCode
                        label is "CreditPaymentCode"
                        select is BankServices
                    EFTCreditPaymentBankCode.Description
                        no label
                        display as text
        paragraph
            EFTACHCompanyID
                label is "E\\F\\TCompanyI\\D"
            OrganizationIdentifier
            RequirePaymentCashCodes
                label is "RequirePaymentCashCodes"
                    left of field
        visible when (UseIDM)
            header2 of "IDMOptions"
            single column
                paragraph
                    visible when (EnableElectronicFundsTransfer)
                        EFTNotificationPrintIDMTemplate
                            label is "EFTNotificationPrintIDMTemplate"
                            select is EFTNotificationPrintIDMTemplate
                        button of "<EFTNotificationPrintIDMTemplate.Name>"
                            link is external "<EFTNotificationPrintIDMTemplate.RetrieveUrl>"
                            display as link
                    visible when (not EnableElectronicFundsTransfer)
                        CustomerGroupField.CustomerGroup.EFTNotificationPrintIDMTemplate
                            label is "EFTNotificationPrintIDMTemplate"
                            display as text
                        text of "MaintainedOn_Customer_Group"
                        button of "<CustomerGroupField.CustomerGroup.EFTNotificationPrintIDMTemplate.Name>"
                            link is external "<CustomerGroupField.CustomerGroup.EFTNotificationPrintIDMTemplate.RetrieveUrl>"
                            display as link
                paragraph
                    EFTNotificationPrintIDMEmailTemplate
                        label is "EFTNotificationPrintEmailTemplate"
                        select is EFTNotificationPrintIDMEmailTemplate
                    button of "<EFTNotificationPrintIDMEmailTemplate.Name>"
                        link is external "<EFTNotificationPrintIDMEmailTemplate.RetrieveUrl>"
                        display as link
                paragraph
                    EFTNotificationPrintIDMEmailSubject
                        label is "EFTNotificationPrintEmailSubject"
                    visible when (EFTNotificationPrintIDMEmailSubject entered)
                        text of "Sample:<DerivedEFTNotificationPrintIDMEmailSubject>"
