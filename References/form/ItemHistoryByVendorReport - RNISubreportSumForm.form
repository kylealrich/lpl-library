RNISubreportSumForm is a SummaryForm
    title is "_"
    Layout
        visible when (not AllReceiptsInvoicedPerItem)
            visible when (not AllReceiptsInvoicedPerVendor)
                form is ItemHistoryByVendorReport.RNISubreportVendorNameHeaderForm
                list is PayablesInvoiceDetailTotalsRel.RNISubreportMainList
                form is ItemHistoryByVendorReport.RNISubreportTotalsForm
        visible when (AllReceiptsInvoicedPerItem)
            form is ItemHistoryByVendorReport.RNISubreportAllReceiptsInvoicedForm
