DiscontinuedItemAndContractProcessing is a CompositeForm
    context form is ContractContextForDiscontinued
        background is theme color
    restrict action CreateTemplate
    restrict action CloseContract
    restrict action RemoveMissingGPOItems
    restrict action CheckForNewAndUpdatedItemsForThisRecord
    restrict action CleanUpLineErrors
    restrict action CompareContractInterfaceCost
    restrict action FindTierChangesForThisRecord
    restrict action ProcessContractLineUpdates
    restrict action ResetAllUpdateFunctionFieldsToBlanks
    restrict action ResetAllUpdateFunctionFieldsToCreate
    restrict action ResetAllUpdateFunctionFieldsToUpdate
    restrict action SetCanUseForCostDefaultingNo
    restrict action SetCanUseForCostDefaultingToYes
    restrict action UpdateDatesOnContract
    restrict action UpdateNewDatesOnContract
    restrict action UpdateVendorItemsOnContract
    Actions
        action is ProcessDiscontinuedContractsAndItems
        navigation is SelectedLinesNav
            label is "ViewSelectedLines"
            hide when invalid
        navigation is ViewCopiedToContracts
            label is "ViewContractsCreatedOrUpdated"
            hide when invalid
        action is UnlinkFromContract
            label is "UnlinkFromGPOContract"
            hide when invalid
        navigation is GPOContractsForSameSupplier
            hide when invalid
        action is refresh
            label is "_"
    DiscontinuedLinesToProcess is a Panel
        visible when (CanViewSingleContract)
        list is NoGPOItemRel.LinesWithNoGPOData
    DiscontinuedLinesToProcessNoContract is a Panel
        title is "DiscontinuedLinesToProcess"
        visible when (not CanViewSingleContract)
        list is NoGPOItemRel.LinesWithNoGPODataNoContract
