DetailCurrencyAmountsForm is a Form
    title is "DetailCurrencyAmounts"
    Layout
        visible when (false)
            DetailBaseAmount.AssignToCurrency
        blank line
        four column table
            column headers
                "_"
                "Amount"
                "Currency"
                "Rate"
            column1
                text of "Functional"
                visible when (DetailCompany.OrigCompany.AccountingEntity.AlternateCurrencyExists)
                    text of "Alternate"
                visible when (DetailCompany.OrigCompany.AccountingEntity.AlternateCurrency2Exists)
                    text of "Alternate2"
                visible when (DetailCompany.OrigCompany.AccountingEntity.AlternateCurrency3Exists)
                    text of "Alternate3"
                visible when (DetailBaseAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                    text of "<DetailBaseAmount.FinanceEnterpriseGroup.ReportOne>"
                visible when (DetailBaseAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                    text of "<DetailBaseAmount.FinanceEnterpriseGroup.ReportTwo>"
                visible when (DetailBaseAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                    text of "<DetailBaseAmount.FinanceEnterpriseGroup.ReportThree>"
                visible when (DetailBaseAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                    text of "<DetailBaseAmount.FinanceEnterpriseGroup.ReportFour>"
                visible when (DetailBaseAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                    text of "<DetailBaseAmount.FinanceEnterpriseGroup.ReportFive>"
            column2
                display as text
                    DetailBaseAmount.FunctionalAmount.EnteredCurrencyAmount
                        no label
                        display only
                    visible when (DetailCompany.OrigCompany.AccountingEntity.AlternateCurrencyExists)
                        DetailBaseAmount.AlternateAmount.EnteredCurrencyAmount
                            no label
                    visible when (DetailCompany.OrigCompany.AccountingEntity.AlternateCurrency2Exists)
                        DetailBaseAmount.AlternateAmount2.EnteredCurrencyAmount
                            no label
                    visible when (DetailCompany.OrigCompany.AccountingEntity.AlternateCurrency3Exists)
                        DetailBaseAmount.AlternateAmount3.EnteredCurrencyAmount
                            no label
                    visible when (DetailBaseAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        DetailBaseAmount.ReportAmount1.EnteredCurrencyAmount
                            no label
                    visible when (DetailBaseAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        DetailBaseAmount.ReportAmount2.EnteredCurrencyAmount
                            no label
                    visible when (DetailBaseAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        DetailBaseAmount.ReportAmount3.EnteredCurrencyAmount
                            no label
                    visible when (DetailBaseAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        DetailBaseAmount.ReportAmount4.EnteredCurrencyAmount
                            no label
                    visible when (DetailBaseAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                        DetailBaseAmount.ReportAmount5.EnteredCurrencyAmount
                            no label
