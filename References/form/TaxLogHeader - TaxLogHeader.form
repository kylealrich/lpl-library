TaxLogHeader is a CompositeForm
    is primary
    context form is TaxLogHeaderForm
    show panel control on left
    TaxLogComments is a Panel
        form is MessageLogForm
    JurisdictionResults is a Panel
        visible when (JurisdictionResultsExist)
        list is JurisdictionResultsRel.InvoiceResponseShortList
    TaxLineItems is a Panel
        visible when (TaxLineItemsExist)
        list is TaxLineItemsRel.TaxLineItemLines
    Global is a Panel
        title is "GlobalFields"
        visible when (JurisdictionResultsExist)
        list is JurisdictionResultsRel.GlobalReturnValuesList
    FilingCurrencyAmounts is a Panel
        visible when (JurisdictionResultsExist)
        list is JurisdictionResultsRel.FilingCurrencyAmountsList
    TraceLog is a Panel
        title is "TaxMessages"
        visible when (TaxLogMessagesExist)
        list is TaxLogMessages.TaxLogMessageList
    HTTPInfo is a Panel
        title is "CapturedXML"
        visible when (HTTPInfo entered)
        form is HTTPInfoForm
    WebserviceError is a Panel
        visible when (WebServiceError)
        form is WebserviceErrorForm
