MobileShipmentLoadConsolidationComposite is a CompositeForm
    title is "ShipmentLoad"
    context form is MobileShipmentLoadContextForm
        background is theme color
    HasFromShipDateShipments is a Panel
        title is "Shipments"
        visible when (HasEnteredFromShipDateAndUnreleased)
        list is ShipmentLoadRel.MobileShipmentLoadList
            helper list is WarehouseShipmentRel.MobileDistributionManagementWarehouseShipmentList
                action is IncludeWarehouseShipment
    NoFromShipDateShipments is a Panel
        title is "Shipments"
        visible when (NoEnteredFromShipDateAndUnreleased)
        list is ShipmentLoadRel.MobileShipmentLoadList
            helper list is AllWarehouseShipmentRel.MobileDistributionManagementWarehouseShipmentList
                action is IncludeWarehouseShipment
    ShipmentLoadContainer is a Panel
        title is "ContainerUnits"
        list is ShipmentLoadContainerRel.MobileShipmentLoadContainerList
    ConsolidatedShipmentLine is a Panel
        list is ShipmentLoadLineRel.MobileShipmentLoadLineList
