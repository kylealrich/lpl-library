ElectronicPaymentTransaction is a Form
    Layout
        paragraph
            BillingElectronicPaymentTransactionID
                label is "TransactionI\\D"
            ElectronicPaymentAuthorizationCode
                label is "P\\N\\Ref"
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        paragraph
            Customer
                select is CustomerContextCompanySelectList
            Customer.Name
                no label
                display as text
        paragraph
            CustomerOrder
                label is "Order"
            InvcPrefix
                label is "Invoice"
            InvoiceNumber
                no label
        paragraph
            BillingElectronicPaymentType
                label is "ElectronicPayment"
            ElectronicPaymentTypeDesc
                no label
        paragraph
            ElectronicPaymentStatus
                label is "TransactionStatus"
            ElectronicPaymentStatus
                no label
        paragraph
            ElectronicPaymentErrorCode
                label is "Error"
        blank line
        blank line
        paragraph
            ElectronicPaymentAuthorizationDate
                label is "Date"
            ElectronicPaymentAuthorizationBatch
                label is "Batch"
            ElectronicPaymentAuthorizationAmount
                label is "Amount"
        blank line
        blank line
        paragraph
            ElectronicPaymentSettlementDate
                label is "Date"
            ElectronicPaymentSettlementBatch
                label is "Batch"
            ElectronicPaymentSettlementAmount
                label is "Amount"
        paragraph
            ElectronicPaymentSettlementFee
                label is "Fee"
