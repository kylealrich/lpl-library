PurgeAssetHistory is an ActionForm
    action is PurgeAssetHistory
    Layout
        two column table
            column1
                PrmFromDate
                paragraph
                    PrmCompany
                        select is AssetCompanyList
                    text of "Or"
                PrmAssetGroup
                    select is AssetGroups
                PrmAssetType
                    compact format
                    select is AssetTypesWithinCompany
            column2
                PrmToDate
                PrmCompanyGroup
                    select is CompanyGroupsInFinanceEnterpriseGroup
                blank line
                PrmAssetSubType
                    compact format
                    select is AssetTypesWithinCompany
        four column
            PrmAdjustmentHistory
            PrmTransferHistory
            PrmDisposalHistory
            PrmReinstateHistory
            PrmTransactions
            PrmComments
            PrmRepairs
            PrmImport
        single column
            PrmSimulated
