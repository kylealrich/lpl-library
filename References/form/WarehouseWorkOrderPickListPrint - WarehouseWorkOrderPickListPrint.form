WarehouseWorkOrderPickListPrint is an ActionForm
    action is WarehouseWorkOrderPickListPrint
    Layout
        two column distributed
            paragraph
                WorCompany
                    label is "Company"
                WorCompany.Name
                    no label
                    display as text
            paragraph
                WorLocation
                    label is "Location"
                    select is ProductionLocations
                    show up to 8 characters
                WorLocation.Name
                    no label
                    display as text
        blank line
        two column distributed
            FrWorkOrder
                label is "FromWorkOrder"
            ToWorkOrder
                label is "ToWorkOrder"
        two column distributed
            ReprintFl
                label is "Reprint"
            SortOptionFl
                label is "SortOption"
        single column
            FormSelection
                no label
                display as horizontal radio buttons
        visible when (FormSelection.ByDate)
            two column distributed
                FrStartDate
                    label is "FromDate"
                ToStartDate
                    label is "ToDate"
        visible when (FormSelection.ByNumberOfDays)
            single column
                paragraph
                    FromAddSubtract
                        label is "FromNumberOfDays"
                    FromDays
                        label is "Days"
            single column
                paragraph
                    ToAddSubtract
                        label is "ToNumberOfDays"
                    ToDays
                        label is "Days"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                WarehouseWorkOrderPickListPrintReport_RDG
            column2 of 25%
                WarehouseWorkOrderPickListPrintReport_XPT
