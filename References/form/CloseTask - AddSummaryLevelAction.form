AddSummaryLevelAction is an ActionForm
    action is CreateSubProcess
    title is "AddSubProcess"
    Layout
        header3 of "DefineSubProcessWithin:<CloseTask.TaskName>"
        single column
            ProcessName
                label is "ProcessName"
        header3 of "ProcessOwnership"
        two column
            Team
                label is "Team"
                select is FinanceTeamSelect
            Team.Description
                no label
                display as text
        two column
            ProcessOwner
                label is "ProcessOwner"
                compact format
                select is FinanceTeamMemberActiveSelect
            ProcessOwner.TeamMember.PreferredSimplePresentationName
                no label
                display as text
        header3 of "SelectValuesToDefaultOntoLowerLevelTasks"
        two column
            FinanceCompany
                label is "DefaultCompany"
            FinanceCompany.FinanceCompanyField.Name
                no label
                display as text
        two column
            FinanceCalendar
                label is "DefaultCalendar"
            FinanceCalendar.Description
                no label
                display as text
        two column
            FinancePeriodGroup
                label is "DefaultPeriodGroup"
            FinancePeriodGroup.Description
                no label
                display as text
        single column
            WorkDaysOnly
                label is "ScheduleTasksForWorkDaysOnly"
        two column
            Priority
                label is "Priority"
            Risk
                label is "Risk"
