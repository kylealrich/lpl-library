ManualEncumbranceJournalControl is a CompositeForm
    propagate as a drill
    title is "ManualEncumbrance_<AccountingEntity>_<Ledger>"
    context form is ManualCommitJournalControlForm
    AdditionalInfoPanel is a Panel
        title is "Main"
        form is ManualCommitAdditionalInfoForm
    CommentsPanel is a Panel
        title is "Comments"
        list is GeneralLedgerJournalComment set.Comment
    TransactionPanel is a Panel
        title is "Transactions"
        list is GeneralLedgerTransaction set.GeneralLedgerJournalTransactionList
    RelatedDocumentsPanel is a Panel
        title is "RelatedDocuments"
        list is GeneralLedgerJournalDocument set.GeneraLedgerJournalDocuments
    BudgetErrors is a Panel
        title is "BudgetErrors"
        visible when (BudgetErrorsExist)
        list is BudgetEditErrorRel.BudgetEditErrorList
