SupplierPortalAllContractLinesAnonymous is a CompositeForm
    title is "ContractLines"
    disable field options
    called out actions only
    context form is SupplierPortalContractLineContext
    show panel control on left
    Actions
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is refresh
    Line is a Panel
        form is SupplierPortalAllContractLine
    Comments is a Panel
        title is "Comments"
        visible when (HasSupplierContractLineComments)
        list is SupplierContractLineCommentsRel.SupplierPortalContractLineCommentList
    Attachments is a Panel
        title is "Attachments"
        visible when (HasSupplierContractLineAttachments)
        list is SupplierContractLineAttachmentsRel.SupplierPortalContractLineAttachmentList
    AOC is a Panel
        title is "AOC"
        visible when (AOCs)
        list is ContractLineAOC set.SupplierPortalContractLineAOCs
