ImportTierMembersFromGroup is an ActionForm
    action is ImportTierMembersFromGroup
    Layout
        display as text
            paragraph
                ContractGroup
                ContractGroup.BusinessGroup.Description
                    no label
            paragraph
                Contract
                Name
                    no label
        paragraph
            ParticipantGroup
                label is "MemberGroup"
                select is MemberSelect
            ParticipantGroup.Description
                no label
                display as text
        paragraph
            PrmTier
                label is "Tier"
                select is ContractTierSelect
            PrmTier.TierTitle
                no label
                display as text
