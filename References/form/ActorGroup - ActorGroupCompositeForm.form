ActorGroupCompositeForm is a CompositeForm
    is primary
    propagate as a drill
    title is "ActorGroup"
    context form is ActorGroupDefaultForm
    ActorGroupMemberActors is a Panel
        title is "ActorsIn<ActorGroup>Group"
        list is GroupMemberRel.ActorsForActorGroupList
            helper list is ActorsNotInGroupRel.ActorList
                action is AddActorToActorGroup
    ActorsInExpandedGroup is a Panel
        title is "ActorsIn<ActorGroup>AndDescendents"
        visible when (GroupOrDescendentHasActorChildren)
        list is ActorGroupMemberShadow set.ActorGroupMemberShadowResidenceGroupActorList
