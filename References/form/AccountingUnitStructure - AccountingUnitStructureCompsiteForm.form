AccountingUnitStructureCompsiteForm is a CompositeForm
    is primary
    title is "<FinanceEnterpriseGroup.AccountingUnitLabel>Structure_<AccountingUnitStructure>_<AccountingUnitStructure>_<Description>"
    restrict action Delete
    StructurePanel is a Panel
        title is "Structure"
        form is AccountingUnitStructureForm
    Dimensions is a Panel
        title is "<FinanceEnterpriseGroup.AccountingUnitLabel>Hierarchy"
        list is AccountingUnitTopNodeGroup.DimensionNode.AccountingUnitChildrenRel.AccountingUnitsForStructure
    AccountingUnitUpload is a Panel
        title is "<FinanceEnterpriseGroup.AccountingUnitLabel>Upload"
        visible when (not UnusedDimensionStructure)
        list is AccountingUnitUpload set.AccountingUnitUploadForStructure
