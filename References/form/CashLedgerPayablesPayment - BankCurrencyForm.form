BankCurrencyForm is a Form
    title is "BankCurrency"
    Layout
        header2 of "BankCurrencyInformation"
        single column
            PaymentAmountBankAccountCurrency.EnteredCurrencyAmount
                label is "PaymentAmount"
                display as text
            AppliedAmountBankAccountCurrency.BaseAmount.EnteredCurrencyAmount
                label is "AppliedAmount"
                display as text
            CashCode.Currency
                label is "BankAccount<CashCode>Currency"
                display as text
            AppliedAmountBankAccountCurrency.BaseAmount.EnteredCurrencyRate
                label is "ExchangeRate"
