DistributionDetailForm is a Form
    is primary
    title is "ChargeDistributionAccount<DisplayDistributionAccount>forTransaction<ProcurementCardStatementTransaction.TransactionNumber>"
    Layout
        header2 of "StatementTransactionInformation"
        display as text
            three column distributed
                column1
                    paragraph
                        ProcurementCardStatement
                            label is "Statement"
                        ProcurementCardStatementTransaction.TransactionNumber
                    ProcurementCardStatementTransaction.ProcurementCardNumber.DisplayProcurementCardNumber
                        label is "CardNumber"
                column2
                    ProcurementCardStatementTransaction.DerivedMerchantName
                        label is "Merchant"
                    ProcurementCardProgram.DerivedCardProgramName
                        label is "ProgramName"
                column3
                    ProcurementGroup.DerivedProcurementGroupName
                        label is "ProcurementGroup"
                    ProcurementCardStatementTransaction.Company.DerivedCompanyIDName
                        label is "Company"
        header2 of "DistributionInformation"
        two column distributed
            column1
                paragraph
                    ProcurementChargeDistributionType
                paragraph
                    DistributionAccount
                        label is "DistributionAccount"
                        compact format
                paragraph
                    DistributionAmount
                paragraph
                    AssetFlag
                paragraph
                    AssetDescription
                paragraph
                    TagNumber
                        show up to 15 characters
                paragraph
                    Item
                        display as text
                    ItemDescription
                        no label
                        display as text
                paragraph
                    ItemQuantity
            column2
                paragraph
                    InServiceDate
                    PurchaseDate
                paragraph
                    ModelNumber
                        show up to 15 characters
                    SerialNumber
                        show up to 15 characters
                paragraph
                    HoldAsset
                paragraph
                    AssetTemplate
                        show up to 15 characters
                    AssetTemplate.Description
                        no label
                        display as text
                paragraph
                    Asset
                    Asset.Description
                        no label
                        display as text
                paragraph
                    AssetGroup
                        show up to 15 characters
                    AssetGroup.Description
                        no label
                        display as text
                paragraph
                    Combine
                        show up to 15 characters
                    BarCode
                        show up to 15 characters
                visible when (ProcurementCardStatement.FrontEndSplitsEnabled)
                    paragraph
                        CreatedByFES
                        FESManuallyUpdated
