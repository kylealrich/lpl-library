SupplierSearchOpenEvents is a SearchForm
    title is "OpenEvents"
    Layout
        four column distributed
            column1
                EventSearchDocument
                    label is "Keyword"
                Category
                    label is "Category"
                    select is SupplierCategories
            column2
                SourcingEvent
                    label is "Event#"
                    select is OpenForBidSelect
                SubCategory
                    label is "SubCategory"
                    select is SupplierSubCategories
            column3
                Name
                EventReference
                    label is "Reference"
            column4
                SourcingEventType
                    label is "Type"
                    select is SupplierEventTypes
                CommoditySearch
                    label is "CommoditySearch"
                    use text area of 1 lines
