AccountReconciliationSearchForm is a SearchForm
    title is "TransactionSearch"
    Layout
        four column distributed
            BankStatement
            CashManagementAccount
                label is "CashAccount"
            CashManagementAccount.FinancialInstitution
            LineStatus
        four column distributed
            CashTransactionCategory
                select is CashTransactionCategorySelect
            Description
                use text area of 2 lines
            TransactionNumber
            DebitCreditIndicator
        four column distributed
            TransactionDateRange.Begin
                label is "FromTransactionDate"
            TransactionDateRange.End
                label is "ToTransactionDate"
            SearchAmountRange.Begin
                label is "FromTransactionAmount"
            SearchAmountRange.End
                label is "ToTransactionAmount"
