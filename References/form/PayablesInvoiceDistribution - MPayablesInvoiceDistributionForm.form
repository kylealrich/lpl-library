MPayablesInvoiceDistributionForm is a Form
    title is "Distribution"
    Layout
        header2 of "Options"
        single column
            UnitAmount
                label is "Units"
        two column distributed
            DistributionCode
            DistributionCode.Description
                no label
                display as text
        single column
            Description
            DistributionReference
                label is "Reference"
        header2 of "Intrastat"
        single column
            LineType
                label is "LineType"
        two column distributed
            ICNCode
                label is "I\\C\\NCode"
            ICNCode.Description
                no label
                display as text
        two column distributed
            SupplementaryQuantity
            StockWeight
                label is "Weight"
        visible when (TaxEntityUsesVertex)
            header2 of "TaxInformation"
            display as text
                single column
                    RateClassificationText
                        label is "RateClassification"
                        show up to 20 characters
                    Imposition
                        show up to 20 characters
                    ForeignOrDomestic
                    ReturnedTaxType
                        label is "VertexTaxType"
                    IsService
        header2 of "Miscellaneous"
        two column distributed
            AddOnCharge
            AddOnCharge.Description
                no label
                display as text
        single column
            TransientAOCAmount
        header2 of "Asset"
        single column
            AssetFlag
            button of "EnterAssetDetail"
                visible when (ShowAssetTemplateLink)
                form invoke is AssetDetail
                display as link
            button of "ViewAssetDetail"
                visible when (ShowAssetLink)
                form invoke is AssetDetail
                display as link
        visible when (not ShowAssetLink)
            single column
                TransientAssetTemplate
                TransientAsset
                TransientItemQuantity
        paragraph
        visible when (TaxEntityUsesVertex)
            three column distributed
                column1
                    header3 of "GlobalInput"
                    paragraph
                        PayablesInvoice.Vendor.VATRegistrationCountry
                            label is "VendorVATRegistration"
                            display as text
                        PayablesInvoice.Vendor.VATRegistrationNumber
                            no label
                            display as text
                    paragraph
                        PayablesInvoice.TaxEntityRel.VATRegistrationCountry
                            label is "EntityVATRegistration"
                            display as text
                        PayablesInvoice.TaxEntityRel.VATRegistrationNumber
                            no label
                            display as text
                    paragraph
                        ProductTaxCategory
                            display as text
                        ProductTaxCategory.Description
                            no label
                            display as text
                    paragraph
                        TaxUsageCode
                            display as text
                        TaxUsageCode.Description
                            no label
                            display as text
                    paragraph
                        PayablesInvoice.ShipTerm
                            label is "DeliveryTerms"
                        PayablesInvoice.ShipTerm.Description
                            no label
                            display as text
                    paragraph
                        VertexSimplificationCode
                            label is "SimplificationCode"
                            show up to 20 characters
                column2
                    visible when (DisplayVertexVatFields)
                        header3 of "InputTax"
                        paragraph
                            RecoverableImpositionType
                                display as text
                        paragraph
                            RecoverableTaxAreaId
                                display as text
                                show up to 20 characters
                        paragraph
                            RecoverableJurisLevel
                                display as text
                                show up to 20 characters
                        paragraph
                            VATInputTaxAmount
                                label is "InputVAT"
                                display as text
                        paragraph
                            VertexVATType
                                label is "VatType"
                                display as text
                                show up to 20 characters
                column3
                    visible when (DisplayVertexVatFields)
                        header3 of "GlobalOutput"
                        paragraph
                            RecoverablePercent
                                display as text
                        paragraph
                            RecoverableAmount
                                display as text
                        paragraph
                            UnrecoverableAmount
                                display as text
                        paragraph
                            PayablesInvoiceTaxRel.VATReverseCharge
                                display as text
                        paragraph
                            InvoiceTextCode.InvoiceTextCode
                                display as text
                            InvoiceTextCode.InvoiceTextCodeDesc
                                no label
                                display as text
                                show up to 20 characters
                        paragraph
                            SummaryInvoiceTextOut
                                display as text
                                show up to 20 characters
