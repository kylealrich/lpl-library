SelectTransactionsToReallocate is an ActionForm
    action is SelectTransactionsToReallocate
    Layout
        single column
            paragraph
                PostingDateRange.Begin
                    label is "PostingDateSelectionRange"
                PostingDateRange.End
                    label is "Through"
            paragraph
                TransactionDateRange.Begin
                    label is "TransactionDateSelectionRange"
                TransactionDateRange.End
                    label is "Through"
            PostingProject
                label is "Posting<FinanceEnterpriseGroup.ProjectLabel>"
                select is PostingProjectsRel.ContractPostingProjectSelect
            paragraph
                FundingSource1
                    select is ProjectContractFinanceDimension2Rel.FinanceDimension2FundingSourceSelect
                FundingSource2
                    select is ProjectContractFinanceDimension2Rel.FinanceDimension2FundingSourceSelect
            Billed
                label is "BilledStatus"
            System
            GeneralLedgerEvent
                label is "GlobalLedgerEvent"
            AppendToCurrentSelection
