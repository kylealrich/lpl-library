ProjectEmployeeAdminMain is a Form
    Layout
        visible when (not ProjectEmployeeExists)
            single column
                Employee
                    label is "EmploymentI\\D"
        single column
            Active
        header2 of "LaborDistribution"
        two column distributed
            paragraph
                ExcludeFromLabor
                visible when (not ExcludeFromLabor)
                    ExcludeFromCertification
                    button of "<DerivedLaborFTE>"
                        link is LaborFTEHistory
                        label is "CurrentF\\T\\E"
                        display as link
        visible when (not ExcludeFromLabor)
            two column distributed
                paragraph
                    ProjectLaborSchedule
                    ProjectLaborSchedule.Description
                        no label
                        display as text
                visible when (not ExcludeFromCertification)
                    display as text
                        paragraph
                            ProjectEffortSchedule
                            ProjectEffortSchedule.Description
                                no label
            three column distributed
                column1
                    paragraph
                        Approver
                            label is "LaborDistributionApprover"
                        Approver.PresentationNameSnapshot
                            no label
                            display as text
                column2
                    paragraph
                        RedistributionApprover
                            label is "RedistributionApprover"
                        RedistributionApprover.PresentationNameSnapshot
                            no label
                            display as text
                column3
                    visible when (not ExcludeFromCertification)
                        paragraph
                            Certifier
                                label is "Certifier"
                            Certifier.PresentationNameSnapshot
                                no label
                                display as text
            single column
                FinanceCodeBlock
                    compact format
                text of "DimensionsSetToBlank:"
                    align as label
                FinanceCodeBlockSetBlanks
                    no label
            two column distributed
                paragraph
                    BurdenCode
                    BurdenCode.Description
                        no label
                        display as text
                    BurdenCode.Rate
                        display as text
            single column
                BurdenCodeBlock
                    compact format
                text of "DimensionsSetToBlank:"
                    align as label
                BurdenCodeBlockSetBlanks
                    no label
            header2 of "SalaryEncumbrance"
            two column distributed
                column1
                    SalaryEncumbrance
            visible when (SalaryEncumbrance)
                three column
                    column1
                        EncumbranceSalaryAmount
                            label is "SalaryEncumbranceAmount"
                    column2
                        Currency
                            label is "SalaryEncumbranceCurrency"
                    column3
                        EncumbranceBasis
            single column
                visible when (SalaryEncumbrance)
                    SalaryEncumbranceCodeBlock
                        compact format
            two column distributed
                column1
                    FringeEncumbrance
            single column
                visible when (FringeEncumbrance)
                    FringeEncumbranceCodeBlock
                        compact format
