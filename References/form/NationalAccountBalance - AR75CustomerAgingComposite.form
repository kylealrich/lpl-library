AR75CustomerAgingComposite is a CompositeForm
    title is "NationalAccountCreditAnalystReviewDetail"
    context form is AR75Main
        background is theme color
    restrict action AddCustomer
    restrict action ClearFilter
    restrict action Create
    restrict action Delete
    restrict action UpdateFilters
    Actions
        action is refresh
            label is "_"
            mouse over text is "Refresh"
    Main is a Panel
        form is MainForm
    AR75ContactFinance is a Panel
        title is "Finance"
        form is NationalContactFinance
    AgePeriods is a Panel
        title is "Aging"
        visible when (IsNationalAccountCompanyCurrency)
        list is AgingPeriodRel.NationalAccountAgingList
    CompanyCustomerCurrencyAging is a Panel
        title is "Aging"
        visible when (IsNationalAccountCompanyCustomerCurrency)
        list is AgingPeriodRel.NationalAccountCompanyCustomerCurrencyAgingList
    Contacts is a Panel
        list is CompanyCustomerRel.CompanyCustomerContactRel.CustomerContacts
    Comments is a Panel
        list is CompanyCustomerCommentRel.CustomerCommentsList
    Address is a Panel
        form is AddressForm
