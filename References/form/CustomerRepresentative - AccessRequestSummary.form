AccessRequestSummary is a Form
    title is "Status"
    disable field options
    Layout
        visible when (PrimaryContact.RegistrationComplete)
            paragraph
                text of "<CustomerGroup.CompleteAccessRequestText>"
                    bold
                    italics
                    color of green
        visible when (not PrimaryContact.RegistrationComplete)
            paragraph
                text of "<CustomerGroup.IncompleteAccessRequestText>"
                    bold
                    italics
                    color of red
            paragraph
                text of "<PrimaryContact.NotCompleteQuestions>"
            paragraph
                text of "<PrimaryContact.NotCompleteQuestionAttachments>"
