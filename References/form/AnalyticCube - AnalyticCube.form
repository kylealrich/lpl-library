AnalyticCube is a Form
    is primary
    restrict action CreateDACubes
    restrict action DeleteDACubes
    restrict action ExportDACubes
    restrict action ExportDACubesFromBackup
    restrict action ImportDACubes
    Layout
        header1 of "Cube"
            show border color of black
        single column
            BusinessClass
                label is "CubeName"
                select is AnalyticBusinessClasses
            visible when (IsInforBICube)
                BusinessClass.Prefix
                    label is "InforBICubeName"
            BusinessClass.CubeType
                raise red alert when (BusinessClass.CubeType.NoLongerACube)
        header1 of "Status"
            show border color of black
        single column
            OLAPConnectionRel.Status
                label is "Connection"
                left align
                raise red alert when (OLAPConnectionRel.IsErrorStatus)
                    mouse over text is "ServerConnectionWasUnsuccessful"
                raise yellow alert when (OLAPConnectionRel.Status.Unknown)
                    mouse over text is "ServerConnectionWasUnsuccessful"
                raise green alert when (OLAPConnectionRel.Status.Healthy)
                    mouse over text is "ServerConnectionWasSuccessful"
            OLAPConnectionRel.StatusMessage
                display only
                use text area of 8 lines
            CubeStatus
                left align
                display only
                raise red alert when (IsCubeError)
                    mouse over text is "NeedToRunReloadOnThisCube"
                raise yellow alert when (CubeStatus.Unknown)
                    mouse over text is "NeedAHealthyConnectionToDetermineTheCubeStatus"
                raise green alert when (CubeStatus.Created)
                    mouse over text is "CubeHasBeenCreated"
            valid when (EnableRestrictedActions)
                LastReloadDate
                LastRefreshDate
            Mode
                display only
                raise red alert when (ShowModeRedAlert)
                    mouse over text is "<ModeStatusMessage>"
                raise yellow alert when (ShowModeYellowAlert)
                    mouse over text is "<ModeStatusMessage>"
        header1 of "Configurations"
            show border color of black
        two column
            HasUserCubeDeployment
                raise yellow alert when (ShowDeployedYellowAlert)
                    mouse over text is "<UserCubeDeploymentMessage>"
                raise green alert when (ActiveDeployedConfiguration)
                    mouse over text is "<UserCubeDeploymentMessage>"
            HasUserCube
        header1 of "Settings"
            show border color of black
        two column
            column1
                Pause
                FactRowThreadsForReload
                    label is "ReloadThreads"
            column2
                Verbose
