SabrixClientForm is a Form
    title is "SabrixRequest"
    Layout
        four column
            column1
                single column
                    SabrixClient
                    UniqueID
                        display as text
                paragraph
                    LegalEntity
                        label is "TaxEntity"
                    LegalEntity.OneSourceVersionType
                single column
                    InvoiceNumber
                    OneSourceVersionType
                paragraph
                    TaxCodeEntered
                        label is "TaxCode"
                    TaxCodeEntered.Description
                        label is "TaxCodeDescription"
                        show up to 25 characters
                single column
                    TaxAttributeCode
            column2
                single column
                    visible when (TaxLineItemsExist)
                        text of "<TaxLineItemsCount>..LineItemsExist..."
                            bold
                            color of blue
                    UserElementLevel
                    ReturnOneSourceTaxIn
                    TaxRate
                        display as text
                    TaxAmount
                        display as text
                    TaxingJurisdiction
                        display as text
                    IsSuccess
                        label is "IsSuccess"
                            left of field
                        display as text
                    visible when (SuccessfulExecution)
                        text of "Successful"
                            bold
                            color of green
                    visible when (FailedExecution)
                        text of "Failed..."
                            bold
                            color of red
            column3
                single column
                    TestTaxingJurisdiction
                        label is "TestTaxingJurisdiction"
                            left of field
                    DisplayWebserviceErrorXml
                        label is "DisplayWebserviceError_\\X\\M\\L"
                            left of field
                    StoreErrors
                        label is "StoreSevereErrors"
                            left of field
                    DontUseLineItems
                        label is "DontUseLineItems"
                            left of field
                    NoHaltOnErrorOfTaxLineItem
                        label is "NoHaltOnErrorOfTaxLineItem"
                            left of field
                    SendMultLineItems
                        label is "SendMultLineItems"
                            left of field
                    UpdateLineItems
                        label is "UpdateTaxLineItems"
                            left of field
                    UseNewResponseCode
                        label is "UseNewXpathResponseCode"
                            left of field
            column4
                TaxConfigurationRel.WriteToLogFile
                TaxConfigurationRel.DebugMode
                TaxConfigurationRel.DisplayWebserviceErrorXml
                FormatedTime
