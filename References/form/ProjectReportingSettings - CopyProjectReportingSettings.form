CopyProjectReportingSettings is an ActionForm
    action is CopyProjectReportingSettings
    title is "Copy<FinanceEnterpriseGroup.ProjectLabel>ReportingSettings"
    Layout
        single column
            ProjectReportingSettings
                label is "Copy<FinanceEnterpriseGroup.ProjectLabel>ReportingSettings"
                display as text
        two column distributed
            Actor
                display as text
            NewActor
                label is "NewActor"
                show up to 10 characters
        two column distributed
            NewProjectReportingSettings
                label is "<FinanceEnterpriseGroup.ProjectLabel>ReportingSettings"
            NewDescription
                label is "NewDescription"
        two column distributed
            column1
                NewProjectStructure
            column2
                NewProjectStructure.Description
                    no label
                    display as text
        two column distributed
            column1
                NewScenario
                    label is "NewBudgetScenario"
                    select is ProjectBudgets
            column2
                NewScenario.Description
                    no label
                    display as text
        two column distributed
            column1
                NewScenario.Basis
                    label is "NewReportingBasis"
                    display as text
            column2
                NewScenario.Basis.Description
                    no label
                    display as text
        two column distributed
            column1
                NewScenario.Basis.ReportingChart
                    label is "NewReportingChart"
                    display as text
            column2
                NewScenario.Basis.ReportingChart.Description
                    no label
                    display as text
        two column distributed
            column1
                NewReportingAccount
                    label is "NewAccount"
                    select is ProjectAccountList
                    show up to 10 characters
            column2
                NewReportingAccount.AccountDescription
                    no label
                    display as text
        two column distributed
            column1
                NewExcludeUndistributedRetainedEarnings
                    label is "NewExcludeUndistributedRetainedEarnings"
        two column distributed
            column1
                NewReportingPeriod
        two column distributed
            column1
                visible when (NewReportingPeriod.YearToDate)
                    NewReportingDate
                        label is "NewPeriodEndDate"
                        select is NewScenario.Basis.GeneralLedgerCalendar.YTDTopNodeRel.ChildrenRel.CalendarTreeView
                    NewComparisonPeriodEndDate
                        label is "NewComparisonPeriodEndDate"
                        select is NewScenario.Basis.GeneralLedgerCalendar.YTDTopNodeRel.ChildrenRel.CalendarTreeView
                visible when (NewReportingPeriod.LifeToDate)
                    NewReportingDate
                        label is "NewPeriodEndDate"
                        select is NewScenario.Basis.GeneralLedgerCalendar.LTDTopNodeRel.ChildrenRel.CalendarTreeView
                visible when (NewReportingPeriod.PeriodActivity)
                    NewReportingDate
                        label is "NewPeriodEndDate"
                        select is NewScenario.Basis.GeneralLedgerCalendar.TopNodeRel.ChildrenRel.CalendarTreeView
                    NewComparisonPeriodEndDate
                        label is "NewComparisonPeriodEndDate"
                        select is NewScenario.Basis.GeneralLedgerCalendar.TopNodeRel.ChildrenRel.CalendarTreeView
            column2
                visible when (NewReportingPeriod.YearToDate)
                    NewReportingDate.DisplayPeriod
                        label is "DisplayPeriodEndDate"
                        display as text
                    NewComparisonPeriodEndDate.DisplayPeriod
                        label is "DisplayComparisonPeriodEndDate"
                        display as text
                visible when (NewReportingPeriod.LifeToDate)
                    NewReportingDate.DisplayPeriod
                        label is "DisplayPeriodEndDate"
                        display as text
                visible when (NewReportingPeriod.PeriodActivity)
                    NewReportingDate.DisplayPeriod
                        label is "DisplayPeriodEndDate"
                        display as text
                    NewComparisonPeriodEndDate.DisplayPeriod
                        label is "DisplayComparisonPeriodEndDate"
                        display as text
        three column distributed
            NewReportingCurrency
                label is "NewReportCurrencyOption"
            DisplayReportingCurrency
                label is CurrencyDisplayLabel
                display as text
            NewSuppressZeroRows
                label is "SuppressZeroRows"
        header2 of "BudgetVariancePercentSettings"
        three column distributed
            NewPercentOfAvailableLimitRanges.RedAlertRangeMin
                label is "NewRedAlertRangeMin"
            NewPercentOfAvailableLimitRanges.RedAlertRangeMax
                label is "NewRedAlertRangeMax"
            NewPercentOfAvailableLimitRanges.RedMouseOverText
                label is "NewRedMouseOverText"
        three column distributed
            NewPercentOfAvailableLimitRanges.YellowAlertRangeMin
                label is "NewYellowAlertRangeMin"
            NewPercentOfAvailableLimitRanges.YellowAlertRangeMax
                label is "NewYellowAlertRangeMax"
            NewPercentOfAvailableLimitRanges.YellowMouseOverText
                label is "NewYellowMouseOverText"
        three column distributed
            NewPercentOfAvailableLimitRanges.GreenAlertRangeMin
                label is "NewGreenAlertRangeMin"
            NewPercentOfAvailableLimitRanges.GreenAlertRangeMax
                label is "NewGreenAlertRangeMax"
            NewPercentOfAvailableLimitRanges.GreenMouseOverText
                label is "NewGreenMouseOverText"
