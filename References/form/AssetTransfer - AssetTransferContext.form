AssetTransferContext is a Form
    Layout
        four column distributed
            column1
                paragraph
                    Asset
                        display as text
                    Asset.Description
                        no label
                        display as text
                paragraph
                    FullOrPartial
                        display as text
                visible when (Asset.LeasedAsset)
                    paragraph
                        Asset.AssetLease.LeaseCompany
                            label is "LeaseCompany"
                            display as text
                        Asset.AssetLease.LeaseCompany.Name
                            no label
                            display as text
            column2
                paragraph
                    TransferStatus
                        display as text
                paragraph
                    Asset.DerivedStatus
                        label is "AssetStatus"
                        display as text
                visible when (Asset.LeasedAsset)
                    paragraph
                        Asset.AssetLease.Lease
                            display as text
                        Asset.AssetLease.Lease.Description
                            no label
                            display as text
            column3
                paragraph
                    AssetRel.EnteredQuantity
                        label is "AssetEnteredQuantity"
                        display as text
                paragraph
                    AssetRel.LastAssetItemPurchaseDate
                        label is "LastItemPurchaseDate"
                        display as text
            column4
                paragraph
                    TransferQuantity
                        display as text
                paragraph
                    TransferDate
                        display as text
