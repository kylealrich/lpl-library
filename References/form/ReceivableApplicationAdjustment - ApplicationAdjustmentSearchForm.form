                    label is "Type"
            column2
                ReceivableApplication.Invoice
            column3
                SearchCurrency
                    label is "Currency"
            column4
                CurrencyType
        four column distributed
            column1
                SalesRepresentativeRange.BegSalesRepresentative
                    label is "FromSalesRepresentative"
            column2
                SalesRepresentativeRange.EndSalesRepresentative
                    label is "ToSalesRepresentative"
            column3
                TransUser1Range.FromTransUser1
                    label is "FromUserField1"
            column4
                TransUser1Range.ToTransUser1
                    label is "ToUserField1"
        four column distributed
            column1
                CreditAnalyst
            column2
                ReceivableApplication.ReceivableOperator
            column3
                GainLoss
