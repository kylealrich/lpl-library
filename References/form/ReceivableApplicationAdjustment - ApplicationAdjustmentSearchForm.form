                    label is "FromProcessLevel"
            column4
                ReceivableProcessLevelRange.ToReceivableProcessLevel
                    label is "ToProcessLevel"
        four column distributed
            column1
                ReceivableAdjustmentReasonType
                    label is "ReasonType"
            column2
                GeneralLedgerDateRange.Begin
                    label is "FromAdjustmentPostDate"
            column3
                GeneralLedgerDateRange.End
                    label is "ToAdjustmentPostDate"
