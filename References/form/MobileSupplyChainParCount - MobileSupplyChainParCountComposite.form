MobileSupplyChainParCountComposite is a CompositeForm
    title is "ParCount"
    context form is MobileSupplyChainParCountContextForm
        background is theme color
    Actions
        action is Reprocess
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is refresh
            label is "_"
    Main is a Panel
        form is Main
    ParCountLines is a Panel
        list is MobileSupplyChainParCountLine set.primary
    PickForPar is a Panel
        visible when (ShowPickForParPanel)
        list is PickForParTicketsRel.ParCountPickForParTickets
