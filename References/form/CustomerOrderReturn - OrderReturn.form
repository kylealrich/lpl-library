OrderReturn is a CompositeForm
    is primary
    title is "CustomerOrderReturn"
    context form is ReturnContext
        background is theme color
    Actions
        action is Entered.Release
            action icon is release
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is Entered.Create
            action icon is create
            label is "_"
        action is save
            label is "_"
        action is Entered.Delete
            action icon is delete
            label is "_"
        action is refresh
            label is "_"
    Main is a Panel
        form is Main
    LinesOrderNumber is a Panel
        title is "Lines"
        visible when (OrderNumber entered)
        list is CustomerOrderReturnLinesRel.ReturnLines
            helper list is CustomerOrderLinesRel.OrderLinesForReturnOrderLines
                action is CreateCustomerOrderReturnLine
                valid when (OrderNumber entered)
    Lines is a Panel
        title is "Lines"
        visible when (OrderNumber not entered)
        list is CustomerOrderReturnLinesRel.ReturnLines2
    AddOnCharges is a Panel
        list is CustomerOrderReturnLineAddOnChargesRel.ReturnHeaderAddOnChargesDetailList
    Attachments is a Panel
        list is CustomerOrderReturnAttachment set.CustomerOrderReturnAttachmentList
