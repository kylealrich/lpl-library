ReturnFromOffsite is an ActionForm
    action is InProduction.ReturnFromOffsite
    Layout
        paragraph
            Company
                display as text
            Company.Name
                no label
                display as text
        paragraph
            InventoryLocation
                label is "Location"
            InventoryLocation.Name
                no label
                display as text
        paragraph
            WorkOrder
        paragraph
            HoldCode
                label is "HoldCode"
            HoldCode.Description
                no label
                display as text
        paragraph
            Vendor
                display as text
        paragraph
            OffsiteDate
                label is "DateSent"
                display as text
        paragraph
            PrmReturnedDate
                label is "ReturnedDate"
