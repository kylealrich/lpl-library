ContractSubcontractorPortalComposite is a CompositeForm
    title is "ContractSubcontractor"
    context form is ContractSubcontractPortalContext
        background is theme color
    Actions
        action is save
            label is "_"
    SubcontractorPayments is a Panel
        list is ContractSubcontractorPaymentRels.ContractSubcontractorPaymentPortalList
    Comments is a Panel
        list is ContractSubcontractorCommentRels.ContractSubcontractorCommentPortalList
    Diversity is a Panel
        list is ContractSubcontractorDiversity set.primary
            helper list is SubcontractorDiversitiesRel.ActiveSubcontractorDiversities
                action is AttachDiversityToContract
