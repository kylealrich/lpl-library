CustomerOrderLinePricingInformation is a Form
    restrict action CreateCustomerOrderReturnLine
    Layout
        paragraph
            CustomerOrderLine
                label is "Line"
                display as text
        paragraph
            Item
                display as text
        paragraph
            ItemDescription
                display as text
        paragraph
            OrderQuantity
                label is "QuantityOrdered"
                display as text
            SellUOM
                display as text
        paragraph
            EnteredPrice
                label is "UnitPrice"
                display as text
            SellingPriceUOM
                display as text
        paragraph
            PriceStatus
                label is "PriceStatus"
                display as text
        paragraph
            NoCharge
                label is "NoCharge"
                display as text
        paragraph
            GrossAmountCurrency
                label is "GrossAmount"
                display as text
        paragraph
            CustomerOrder.Currency
                label is "Currency"
                display as text
        paragraph
            ListName
                label is "PriceList"
                display as text
        paragraph
            ContractNumber
                label is "Contract"
                display as text
        paragraph
            DiscountCode
                label is "DiscountCode"
                display as text
        paragraph
            text of "MultiUOM"
        paragraph
            PriceBaseItemRel.CurrentSellPriceByUOM.SellPriceByUOM[1].UnitOfMeasure
                display as text
            PriceBaseItemRel.CurrentSellPriceByUOM.SellPriceByUOM[1].SellPrice
                label is "Price"
                display as text
        paragraph
            PriceBaseItemRel.CurrentSellPriceByUOM.SellPriceByUOM[2].UnitOfMeasure
                display as text
            PriceBaseItemRel.CurrentSellPriceByUOM.SellPriceByUOM[2].SellPrice
                label is "Price"
                display as text
        paragraph
            PriceBaseItemRel.CurrentSellPriceByUOM.SellPriceByUOM[3].UnitOfMeasure
                display as text
            PriceBaseItemRel.CurrentSellPriceByUOM.SellPriceByUOM[3].SellPrice
                label is "Price"
                display as text
