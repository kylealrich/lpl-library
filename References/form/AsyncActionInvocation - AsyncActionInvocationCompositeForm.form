AsyncActionInvocationCompositeForm is a CompositeForm
    title is "Job"
    context form is AsyncActionInvocationContextForm
    InvokedByParentInvocation is a Panel
        title is "InvokedByParentJob"
        visible when (InvokingParentExists)
        list is InvokedByParentRel.AsyncActionInvocationSummaryList
    InvokedChildrenInvocations is a Panel
        title is "InvokedChildrenJobs"
        visible when (InvokedChildrenExist)
        list is InvokedChildrenRel.AsyncActionInvocationSummaryList
