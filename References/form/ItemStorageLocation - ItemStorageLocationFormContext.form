ItemStorageLocationFormContext is a Form
    Layout
        visible when (RecordExists)
            three column distributed
                display as text
                    column1
                        paragraph
                            Company.DerivedCompanyDisplay
                                label is "Company"
                        paragraph
                            InventoryLocation.DerivedInventoryLocationIDName
                                label is "Location"
                        paragraph
                            Item.RepresentativeText
                                label is "Item"
                    column2
                        paragraph
                            WarehouseStorageLocation.WarehouseStorageLocationIDName
                                label is "Storage_Location"
                        paragraph
                            WarehouseStorageLocation.StorageLocationType
                column3
                    paragraph
                        Bin
                            label is "DefaultBin"
                            select is ValidBinsForItemStorageLocationRel.WarehouseStorageBinList
                    paragraph
                        Active
