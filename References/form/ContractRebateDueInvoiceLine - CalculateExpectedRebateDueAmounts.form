CalculateExpectedRebateDueAmounts is an ActionForm
    action is CalculateExpectedRebateDueAmounts
    title is "CalculateExpectedRebateDueAmounts"
    Layout
        single column
            PrmContractGroup
                label is "ContractGroup"
                select is ContractGroups
            PrmContract
                label is "Contract"
                select is RebateCalculationContracts
        two column
            PrmBeginDueDate
                label is "BeginRebateDueDate"
            PrmEndDueDate
                label is "EndRebateDueDate"
        single column
            text of "EnterEitherContractOrDueDateRange,NotBoth"
            text of "IfContractIsEntered,ContractEffectiveAndExpirationDatesWillBeUsedForDueDateRange"
