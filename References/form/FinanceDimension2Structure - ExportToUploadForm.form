ExportToUploadForm is an ActionForm
    action is ExportToUpload
    title is "ExportToUpload"
    Layout
        single column
            PrmRunGroup
                label is "RunGroup"
            text of "LeaveTheFollowingParametersBlankToExportTheEntireStructure"
            PrmDimensionGroup
                label is "CustomGroup"
            text of "Or"
            PrmSummaryDimension
                label is "Summary<FinanceEnterpriseGroup.FinanceDimension2Label>"
                select is SummaryFinanceDimension2Records
            text of "Or"
            PrmPostingDimension
                label is "Posting<FinanceEnterpriseGroup.FinanceDimension2Label>"
                select is DimensionNode.DescendantsRel.FinanceDimension2Select
            visible when (FinanceEnterpriseGroup.ProjectInvoicing)
                PrmBypassEdits
                    label is "BypassEdits"
