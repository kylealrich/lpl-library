MobileSupplyChainForm is a Form
    Layout
        single column
            MobileSupplyChainEnabled
        visible when (ActualMobileSupplyChainEnabled)
            header3 of "ParCount"
            single column
                TransientReplenishDays
                    label is "ReplenishDays"
            four column distributed
                column1
                    DisplayQuantityOnOrder
                column2
                    DisplayParUtilizationLevel
                        label is "DisplayAverageParUtilizationAndSuggestedParLevel"
                column3
                    AllowEditParLevel
                column4
                    PrintParItemLabel
            header3 of "InventoryReplenishment"
            four column distributed
                column1
                    DisplayInventoryReplenishmentCompanyQOO
                        label is "DisplayQOO"
                column2
                    DisplayInventoryReplenishmentCompanyMinimumOrder
                        label is "DisplayMinimumOrder"
            header3 of "PurchasingOrderReceiving"
            four column distributed
                column1
                    CarrierOptions
                        label is "Carrier"
                    DisplayAddReceiptButton
                    CheckCatchWeightTolerance
                    DisplayPackageCondition
                    DisplayPurchaseOrderComment
                column2
                    CarrierTrackingNumberOptions
                        label is "CarrierTrackingNumber"
                    DisplayTrackingNumber
                    TemperatureScale
                    DisplayTemperature
                column3
                    ReferenceNumberOptions
                        label is "ReferenceNumber"
                    PrintPurchaseOrderDeliveryLabel
                        label is "PrintDeliveryLabel"
                    DisplayRejectOption
                    DisplayExpirationDate
                column4
                    BillOfLadingOptions
                        label is "BillOfLading"
                    PrintPurchaseOrderDeliveryTicket
                        label is "PrintDeliveryTicket"
                    DisplayStorageType
                    DisplayCapturePhotoOption
            header3 of "PickingForHandheld"
            four column distributed
                column1
                    TrackDeliveryOption
                column2
                    PrintPickingOptions
                column3
                    DownloadedShipmentOrder
                column4
                    ItemNumberValidation
            header3 of "InventoryIssuesAndReturns"
            single column
                PrintInventoryIssueTicket
                    label is "PrintTicket"
            header3 of "InventoryAdjustments"
            single column
                PrintInventoryAdjustmentTicket
                    label is "PrintTicket"
            header3 of "InventoryTransfers"
            single column
                PrintInventoryTransferTicket
                    label is "PrintTicket"
            header3 of "Inventory_Receiving_Transfers"
            single column
                PrintInventoryReceivingTransferTicket
