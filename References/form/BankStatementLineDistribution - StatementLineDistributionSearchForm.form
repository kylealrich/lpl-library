StatementLineDistributionSearchForm is a SearchForm
    title is "StatementLineDistributionSearch"
    Layout
        four column distributed
            BankStatement
            CashManagementAccount
                label is "CashAccount"
            DistributionAccount.GeneralLedgerChartAccount
                label is "Account"
            DistributionAccount.Project
                label is "Project"
        four column distributed
            column1
                StatementLineCategory
                    label is "LineCategory"
                    select is CashTransactionCategorySelect
                StatementDateRange.Begin
                    label is "FromStatementDate"
                SearchAmountRange.Begin
                    label is "FromDistributionAmount"
            column2
                DistributionAccount.ToAccountingEntity
                    label is "AccountingEntity"
                StatementDateRange.End
                    label is "ToStatementDate"
                SearchAmountRange.End
                    label is "ToDistributionAmount"
            column3
                DistributionAccount.AccountingUnit
                    label is "AccountingUnit"
                PostingDateRange.BeginDate
                    label is "FromPostingDate"
                Status
            column4
                DistributionAccount.Ledger
                    label is "Ledger"
                PostingDateRange.EndDate
                    label is "ToPostingDate"
