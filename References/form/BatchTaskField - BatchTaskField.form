BatchTaskField is a Form
    is primary
    title is "JobStreamStepField"
    Layout
        single column
            BatchTask
                label is "JobStream"
        three column
            BatchTaskStep
                label is "StepID"
            BatchTaskStep.ExecutionOrder
                label is "StepExecutionSequence"
            BatchTaskStep.TaskStep
        two column
            BatchTaskField.BusinessField
                label is "Field"
                select is PersistentBusinessFieldList
            BatchTaskField.BusinessField.Type
        single column
            UsedInStream
                display only
