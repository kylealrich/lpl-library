MobileResponderProposedItemForm is a CompositeForm
    title is "ProposedNewItem"
    restrict action Create
    restrict action CreateFromWizard
    restrict action ParseProductDescription
    restrict action Update
    restrict action SelectItem
    restrict action UnselectItem
    Actions
        action is save
            label is "_"
        action is refresh
            label is "_"
    AdditionalNewItemInformation is a Panel
        form is MobileProposedItemInformation
    RequisitionLineSummary is a Panel
        visible when (HasRequisitions)
        list is RecallNotice.RecallOrganizationRequisitionLineProductRel.MobileProductRequisitionLineSummary
    PurchaseOrderLineSummary is a Panel
        visible when (HasPurchaseOrders)
        list is RecallNotice.RecallOrganizationPurchaseOrderLineProductRel.MobileProductPurchaseOrderLineSummary
    RequisitionPurchaseOrderLineSummary is a Panel
        visible when (HasPolinesrc)
        list is RecallNotice.RecallOrganizationPolinesrcProductRel.MobileProductPolinesrcSummary
    Inventory is a Panel
        visible when (HasInventory)
        list is InventoryActivityRel.MobileRecallItemLocations
    ParLocations is a Panel
        visible when (HasParLocations)
        list is ParLocationActivityRel.MobileRecallParLocations
    StockOnHandLots is a Panel
        visible when (HasLotNumbers)
        list is LotNumberActivityRel.MobileRecallStockOnHandDetail
    StockOnHandSerialNumbers is a Panel
        visible when (HasSerialNumbers)
        list is SerialNumberActivityRel.MobileRecallItserial
    IssueSummary is a Panel
        visible when (HasIssues)
        list is RecallNotice.RecallOrganizationIssuesProductRel.MobileProductIssuesSummary
    IssueDetail is a Panel
        visible when (HasIssueDetail)
        list is IssueDetailActivityRel.MobileRecallIssuesDetail
    IncompleteTransfers is a Panel
        visible when (HasIntransitTransfers)
        list is IntransitTransferActivityRel.MobileRecallIntransitTransfers
    IncompleteTranferDetail is a Panel
        visible when (HasIntransitTransfersDetail)
        list is IntransitTransferDetailActivityRel.MobileRecallIntransitTransfersDetail
    ContractLines is a Panel
        visible when (HasContracts)
        list is ContractLineActivityRel.MobileRecallContractLines
    SourcingEventLines is a Panel
        visible when (HasSourcingEvents)
        list is SourcingEventLineActivityRel.MobileRecallSourcingEventLines
    ProcurementTemplateLines is a Panel
        visible when (HasProcurementTemplates)
        list is ProcurementTemplateLineActivityRel.MobileRecallProcurementTemplateLines
    PatientProcedureSerialAndLotNumbers is a Panel
        visible when (HasPatientProcedures)
        list is PatientProcedureActivityRel.MobileRecallPatientProcedures
