MobilePOReceivingLineDashboard is a CompositeForm
    title is "PurchaseOrderReceiptLineDetails"
    context form is MobilePOReceivingLineDashboardContext
    implements DashBoardView
    restrict action PopulateMissingUOM
    restrict action MobileUpdateReceiptLine
    Cost is a Panel
        visible when (PurchaseOrderReceipt.DisplayInvoiceAOCHdr)
        form is PO304InvoiceCost
    VendorItem is a Panel
        form is MobilePO304VendorItem
    Safety is a Panel
        form is PO304Safety
    CatchWeight is a Panel
        visible when (IsCatchWeightItemAndHasItemMaster)
        form is PO304CatchWeight
    SubstituteLines is a Panel
        list is PurchaseOrderReceiptLinesRel.MobilePurchaseOrderReceiptLineSubstitutes
