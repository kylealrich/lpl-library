BillingInterfaceResultContext is a Form
    propagate as a drill
    title is "BillingInterfaceResult"
    Layout
        display as text
            three column distributed
                column1
                    RunGroup
                    paragraph
                        visible when (HasOnlyCompany)
                            paragraph
                                Company.RepresentativeText
                                    label is "Company"
                        visible when (HasOnlyCompanyGroup)
                            paragraph
                                CompanyGroup.RepresentativeText
                                    label is "CompanyGroup"
                        visible when (HasOnlyRunGroup)
                            paragraph
                                Company
                                CompanyGroup
                    paragraph
                        visible when (BillingProcessLevel entered)
                            BillingProcessLevel.BillingProcessLevelDescriptionID
                                label is "ProcessLevel"
                        visible when (BillingProcessLevel not entered)
                            BillingProcessLevel
                                label is "ProcessLevel"
                column2
                    RunTime
                    paragraph
                        visible when (Currency entered)
                            Currency.CurrencyDescriptionID
                                label is "Currency"
                        visible when (Currency not entered)
                            Currency
                    Description
                column3
                    ReleaseOption
                    visible when (Status.InProcess)
                        InterfaceResultStatus
                            label is "Status"
                            display as tag
                                color of gray when (Status.InProcess)
                    visible when (not HasIncompleteImportRecords)
                        InterfaceResultStatus
                            label is "Status"
                            display as tag
                                color of green when (Status.Complete)
                    visible when (HasIncompleteImportRecords)
                        InterfaceResultStatus
                            label is "Status"
                            display as tag
                                color of red when (Status.Complete)
                    button of "RerunForErrors"
                        visible when (HasIncompleteImportRecords)
                        action is RerunForErrors
                    visible when (NoRecordsToProcess)
                        NoRecordsToProcessMsg
                            no label
                            display as tag
                                color of red when (NoRecordsToProcess)
