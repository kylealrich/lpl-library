BillingOverridesForm is a Form
    title is "<FinanceEnterpriseGroup.ProjectLabel>BillingOverrides"
    Layout
        two column
            column1
                paragraph
                    button of "<Project>"
                        link is ViewProject
                        label is "<FinanceEnterpriseGroup.ProjectLabel>"
                        display as link
                    Description
                        no label
                        display as text
        two column
            ProjectDateRange.BeginDate
                label is "DateRange"
                display as text
            ProjectDateRange.EndDate
                no label
                display as text
        two column
            ProjectStatus
                label is "<FinanceEnterpriseGroup.ProjectLabel>Status"
            ProjectStatus.Description
                no label
                display as text
        single column
            BillingMethod
            visible when (BillingMethod.CostPlus)
                MarkupPercent
            visible when (BillingMethod.TimeAndMaterials)
                MarkupPercent
            visible when (BillingMethod.RateTable)
                BillingRateTable
                MarkupPercent
            BillingMaximum
            RevenueCodeBlockOption
            RevenuePartialAccount
                compact format
            RevenueFullAccount
                compact format
            TaxExempt
            paragraph
                BuyerTaxCode
                BuyerTaxCode.Description
                    no label
                    display as text
            paragraph
                BuyerTaxLocation
                BuyerTaxLocation.Description
                    no label
                    display as text
                BuyerTaxLocation.TaxCode
                    display as text
                BuyerTaxLocation.AddressCode
                    display as text
            FeesExempt
            visible when (FinanceEnterpriseGroup.FrontEndSplits)
                ExcludeFromFES
                    label is "ExcludeFromFrontEndSplit"
            WorkType
            ServiceType
            UsageType
