CreateRefreshReport is an ActionForm
    action is CreateRefreshReport
        on completion navigate to RefreshReportNavigation
    title is "CreateRefreshReport"
    Layout
        single column
            ReportLastRefreshDate
                label is "LastRefreshDate_<PreviousRunDateParameterMessage>"
                display only
            ReportRefreshAsOfDate
                label is "RefreshAsOfDate_<AsOfDateParameterMessage>"
                display only
