InventoryTransactionLineDetail is a Form
    is primary
    title is "InventoryTransactionLineDetail"
    Layout
        header2 of "InventoryTransaction<InventoryTransaction>"
        display as text
            three column distributed
                column1
                    InventoryTransaction.Company.RepresentativeText
                        label is "Company"
                    InventoryTransaction.InventoryDocumentType
                        label is "DocumentType"
                column2
                    InventoryTransaction.InventoryLocation.RepresentativeText
                        label is "Location"
                    Company.GeneralLedgerCompany.Currency
                        label is "Currency"
                column3
                    InventoryTransaction.StatusMsgTagField
                        label is "Status"
                        display as tag
                            color of yellow when (InventoryTransaction.IsUnreleased)
                            color of green when (InventoryTransaction.IsReleased)
                    visible when (InventoryTransaction.IsInventoryTransfer)
                        single column
                            InventoryTransaction.IntransitTransfer
            three column distributed
                paragraph
                    visible when (InventoryTransaction.ShowToCompany)
                        InventoryTransaction.FromToCompanyLocation.FromToCompany.RepresentativeText
                            label is "ToCompany"
                    visible when (InventoryTransaction.IsIntransitReceivingOrReceivingTransfer)
                        InventoryTransaction.FromToCompanyLocation.FromToCompany.RepresentativeText
                            label is "FromCompany"
                paragraph
                    visible when (InventoryTransaction.IsDirectTransferOrInventoryTransfer)
                        InventoryTransaction.FromToCompanyLocation.FromToLocation.RepresentativeText
                            label is "ToLocation"
                    visible when (InventoryTransaction.IsInventoryIssues)
                        InventoryTransaction.FromToCompanyLocation.RequestingLocation.RepresentativeText
                            label is "RequestingLocation"
                    visible when (InventoryTransaction.IsIntransitReceivingOrReceivingTransfer)
                        InventoryTransaction.FromToCompanyLocation.FromToLocation.RepresentativeText
                            label is "FromLocation"
        header2 of "TransactionLine<InventoryTransactionLine.LineNumber>"
        single column
            display as text
                visible when (InventoryTransactionLine.IsBackOrderExist)
                    InventoryTransactionLine.BackOrderExistMessage
                        no label
                visible when (IsStorageCodeMismatch)
                    StorageCodeMismatchMessage
                        no label
                visible when (InventoryTransactionLine.DerivedToleranceMessage entered)
                    InventoryTransactionLine.DerivedToleranceMessage
                        no label
        display as text
            visible when (InventoryTransactionLine.IsReceiptAdjustmentsIssuesAndIntransitReceiving)
                three column distributed
                    paragraph
                        InventoryTransactionLine.Item
                        InventoryTransactionLine.Item.Description
                    InventoryTransactionLine.TransientQuantityValue
                        label is "Quantity"
                    InventoryTransactionLine.TransactionUOM
                    visible when (InventoryTransactionLine.Item.IsCatchWeightItem)
                        paragraph
                            InventoryTransactionLine.TransientSecondayQuantityValue
                                label is "SecondaryQuantity"
                            InventoryTransactionLine.SecondaryUOM
                                no label
                    visible when (not InventoryTransaction.IsInventoryBinTransfer)
                        visible when (not InventoryTransactionLine.IsNegativeAdjustmentOrNoCharge)
                            InventoryTransactionLine.TransientUnitCostValue
                                label is "UnitCost"
                    visible when (false)
                        InventoryTransactionLine.Item.CatchWeightCode
                    visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                        InventoryTransactionLine.OpenQuantity
                    visible when (InventoryTransaction.IsInventoryIntransitReceiving)
                        InventoryTransactionLine.OriginalQuantity
                    visible when (InventoryTransaction.IsInventoryBinTransfer)
                        visible when (IsMultipleBinEntered)
                            InventoryTransactionLine.MultipleBins
                                label is "MultipleBins"
                    visible when (IsIssueAdjustAndReceipt)
                        InventoryTransactionLine.Bin
                            label is "Bin"
                    visible when (IsMultipleBinEntered)
                        InventoryTransactionLine.MultipleBins
                            label is "MultipleBins"
            visible when (IsTransferWithToBinEntered)
                three column distributed
                    paragraph
                        InventoryTransactionLine.Item
                        InventoryTransactionLine.Item.Description
                    visible when (InventoryTransaction.IsInventoryBinTransfer)
                        InventoryTransactionLine.Bin
                            label is "FromBin"
                        InventoryTransactionLine.FromToCompanyLocationBin.FromToBin
                            label is "ToBin"
        header3 of "<DerivedTransactionLineDetailTitle>"
        paragraph
        visible when (IsLotRequired)
            paragraph
                visible when (InventoryTransaction.IsInventoryTransfer)
                    Lot
                        select is AvailableLotsWithQuantity
                        when value changed
                            LotExpirationDate =  DerivedLotExpirationDate
                            ShelfLife =  DerivedShelfLife
                            ManufacturingDate =  DerivedManufacturingDate
                visible when (not InventoryTransaction.IsInventoryTransfer)
                    Lot
                        when value changed
                            refresh DisallowUpdateLotExpirationDate
                            LotExpirationDate =  DerivedLotExpirationDate
                            ShelfLife =  DerivedShelfLife
                            ManufacturingDate =  DerivedManufacturingDate
                Sublot
                visible when (InventoryTransactionLine.ItemLocation.IsAWarehouseLocationItem)
                    ManufacturingDate
                    ShelfLife
                visible when (DisallowUpdateLotExpirationDate)
                    LotExpirationDate
                        display only
                visible when (not DisallowUpdateLotExpirationDate)
                    LotExpirationDate
                visible when (InventoryTransactionLine.ItemLocation.IsAWarehouseLocationItem)
                    DerivedRemainingShelfLife
                        display as text
                LotOnHold
        visible when (IsLotExpired)
            single column
                LotExpiredMessage
                    no label
                    display as text
        visible when (IsSerialRequired)
            single column
                Serial
                    label is "Serial"
        single column
            visible when (not InventoryTransaction.IsBinTransferOrInventoryTransfer)
                visible when (InventoryTransactionLine.IsItemLocationBinTracked)
                    visible when (not InventoryTransactionLine.IsAWarehouseLocationItem)
                        Bin
                            select is InventoryTransactionLine.BinsForItemLocationRel.BinListWithQuantity
                    visible when (InventoryTransactionLine.IsAWarehouseLocationItem)
                        Bin
                            select is InventoryTransactionLine.BinHasItemStorageLocationRel.DistributionManagementBinList
            visible when (InventoryTransaction.IsInventoryTransfer)
                visible when (InventoryTransactionLine.IsItemLocationBinTracked)
                    visible when (not InventoryTransactionLine.IsAWarehouseLocationItem)
                        Bin
                            select is InventoryTransactionLine.BinWithStockOnHandRel.BinListWithQuantity
                    visible when (InventoryTransactionLine.IsAWarehouseLocationItem)
                        Bin
                            select is InventoryTransactionLine.BinHasItemStorageLocationWithStockOnHandRel.DistributionManagementBinList
        visible when (IsQuantityVisibleInDetail)
            two column
                paragraph
                    DerivedDetailQuantityValue
                        label is "Quantity"
                    visible when (UnitOfMeasure entered)
                        UnitOfMeasure
                            no label
                            display as text
                    visible when (UnitOfMeasure not entered)
                        InventoryTransactionLine.TransactionUOM
                            no label
                            display as text
                paragraph
                    text of "Stock_On_Hand_Detail"
                    display as text
                        DerivedBeforeStockQuantity
                            label is "Before"
                        DerivedAfterStockQuantity
                            label is "After"
                        DerivedUOMDisplay
                            no label
        visible when (false)
            single column
                InventoryTransactionLine.Item.CatchWeightCode
        visible when (InventoryTransactionLine.Item.IsCatchWeightItem)
            two column
                paragraph
                    DerivedDetailQuantityValue
                        label is "Quantity"
                    UnitOfMeasure
                        no label
                        display as text
                paragraph
                    DerivedDetailSecondaryQuantityValue
                        label is "SecondaryQuantity"
                    InventoryTransactionLine.SecondaryUOM
                        no label
                        display as text
            text of "_"
            text of "_"
            four column table
                display as text
                    column1
                        text of "_"
                        text of "Quantity"
                        text of "Secondary_Quantity"
                    column2
                        text of "Before"
                        DerivedBeforeStockQuantity
                            no label
                        DerivedBeforeSecondaryQuantity
                            no label
                    column3
                        text of "After"
                        DerivedAfterStockQuantity
                            no label
                        DerivedAfterSecondaryQuantity
                            no label
                    column4
                        text of "_"
                        InventoryTransactionLine.DerivedUOM
                            no label
                        InventoryTransactionLine.SecondaryUOM
                            no label
        visible when (DisplayWarehouseStorageInformation)
            header3 of "Warehouse_Storage_Information"
            display as text
                visible when (InventoryTransaction.IsInventoryBinTransfer)
                    visible when (InventoryTransactionLine.Bin.WarehouseStorageLocation not entered)
                        paragraph
                            text of "NoAvailableStorageInformationFor_From_Bin<InventoryTransactionLine.Bin>"
                                bold
                                italics
                                color of blue
                    visible when (InventoryTransactionLine.Bin.WarehouseStorageLocation entered)
                        three column distributed
                            column1
                                InventoryTransactionLine.Bin.WarehouseStorageLocation.WarehouseStorageLocationIDName
                                    label is "From_Storage_Location"
                                InventoryTransactionLine.Bin.WarehouseStorageLocation.DerivedWarehouseZoneAndDescription
                            column2
                                InventoryTransactionLine.Bin.WarehouseStorageLocation.DerivedWarehouseZoneComponentTypeAndDescription
                                InventoryTransactionLine.Bin.WarehouseStorageLocation.DerivedZoneComponentAisle
                            column3
                                InventoryTransactionLine.Bin.WarehouseStorageLocation.DerivedWarehouseZoneComponentAndDescription
                                InventoryTransactionLine.Bin.WarehouseStorageLocation.Row
                        three column distributed
                            column1
                                InventoryTransactionLine.Bin.StorageCode.MaximumVolume
                            column2
                                InventoryTransactionLine.Bin.StorageCode.MaximumWeight
                            column3
                                InventoryTransactionLine.Bin.StorageCode.MaximumQuantity
                    visible when (InventoryTransactionLine.FromToCompanyLocationBin.FromToBin.WarehouseStorageLocation not entered)
                        paragraph
                            text of "NoAvailableStorageInformationFor_To_Bin<InventoryTransactionLine.FromToCompanyLocationBin.FromToBin>"
                                bold
                                italics
                                color of blue
                    visible when (InventoryTransactionLine.FromToCompanyLocationBin.FromToBin.WarehouseStorageLocation entered)
                        three column distributed
                            column1
                                InventoryTransactionLine.FromToCompanyLocationBin.FromToBin.WarehouseStorageLocation.WarehouseStorageLocationIDName
                                    label is "To_Storage_Location"
                                InventoryTransactionLine.FromToCompanyLocationBin.FromToBin.WarehouseStorageLocation.DerivedWarehouseZoneAndDescription
                            column2
                                InventoryTransactionLine.FromToCompanyLocationBin.FromToBin.WarehouseStorageLocation.DerivedWarehouseZoneComponentTypeAndDescription
                                InventoryTransactionLine.FromToCompanyLocationBin.FromToBin.WarehouseStorageLocation.DerivedZoneComponentAisle
                            column3
                                InventoryTransactionLine.FromToCompanyLocationBin.FromToBin.WarehouseStorageLocation.DerivedWarehouseZoneComponentAndDescription
                                InventoryTransactionLine.FromToCompanyLocationBin.FromToBin.WarehouseStorageLocation.Row
                        three column distributed
                            column1
                                InventoryTransactionLine.FromToCompanyLocationBin.FromToBin.StorageCode.MaximumVolume
                            column2
                                InventoryTransactionLine.FromToCompanyLocationBin.FromToBin.StorageCode.MaximumWeight
                            column3
                                InventoryTransactionLine.FromToCompanyLocationBin.FromToBin.StorageCode.MaximumQuantity
                visible when (not InventoryTransaction.IsInventoryBinTransfer)
                    three column distributed
                        column1
                            Bin.WarehouseStorageLocation.WarehouseStorageLocationIDName
                                label is "Storage_Location"
                            Bin.WarehouseStorageLocation.DerivedWarehouseZoneAndDescription
                        column2
                            Bin.WarehouseStorageLocation.DerivedWarehouseZoneComponentTypeAndDescription
                            Bin.WarehouseStorageLocation.DerivedZoneComponentAisle
                        column3
                            Bin.WarehouseStorageLocation.DerivedWarehouseZoneComponentAndDescription
                            Bin.WarehouseStorageLocation.Row
                    three column distributed
                        column1
                            Bin.StorageCode.MaximumVolume
                        column2
                            Bin.StorageCode.MaximumWeight
                        column3
                            Bin.StorageCode.MaximumQuantity
