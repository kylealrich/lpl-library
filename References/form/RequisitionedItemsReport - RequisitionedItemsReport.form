RequisitionedItemsReport is an ActionForm
    action is RequisitionedItemsReport
    Layout
        header2 of "Main"
        paragraph
            CompanyGroup
                label is "CompanyGroup"
                show up to 8 characters
            CompanyGroup.Description
                no label
                display as text
        text of "\\or"
            bold
        paragraph
            RqhCompany
                label is "Company"
                show up to 8 characters
            RqhCompany.Name
                no label
                display as text
        single column
            blank line
        paragraph
            FrCreationDate
                label is "FromCreateDate"
            ToCreationDate
                label is "ToCreateDate"
        paragraph
            ReqLocation
                label is "RequestingLocation"
            ReqLocation.Name
                no label
                display as text
        two column
            Status
                show up to 8 characters
            RlnItemType
                label is "ItemType"
                show up to 8 characters
        two column
            CreatePoOnly
                label is "CreatePOOnly"
                show up to 8 characters
            ReportOption
        single column
            ReportSeq
                label is "SortOption"
            PrintUnspscGtin
                label is "PrintUNSPSC/\\GTIN"
                show up to 8 characters
        header2 of "Options"
        two column
            paragraph
                PurchMajor
                    label is "MajorPurchaseClass"
                    show up to 8 characters
            paragraph
                PurchMinor
                    label is "MinorPurchaseClass"
                    show up to 8 characters
        paragraph
            VendorGroup
            VendorGroup.Description
                no label
                display as text
        paragraph
            Vendor
            Vendor.VendorName
                no label
                display as text
        paragraph
            PurchFrLoc
                label is "PurchaseFrom"
                show up to 8 characters
            PurchFrLoc.VendorName
                no label
                display as text
        two column
            paragraph
                Item
                    show up to 8 characters
                Item.Description
                    no label
                    display as text
                Item.Description2
                    no label
                    display as text
        single column
            paragraph
                CommodityCode
        two column
            paragraph
                InclExclPoCodes
                    label is "Include/\\ExcludePOCodes"
            paragraph
                InclExclMfrCd
                    label is "Include/\\ExcludeManufacturerCode"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                RequisitionedItemsReportReport_RDG
            column2 of 25%
                RequisitionedItemsReportReport_XPT
