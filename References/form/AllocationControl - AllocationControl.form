AllocationControl is a CompositeForm
    is primary
    title is "<DerivedCompositeFormTitle>"
    context form is Header
        background is theme color
    Actions
        action is Run
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is save
            action icon is save
            label is "_"
            hide when invalid
        action is save and new
            label is "_"
            hide when invalid
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is search
            label is "_"
            mouse over text is "Search"
        action is refresh
            label is "_"
    Allocations is a Panel
        list is Allocation.AllocationControlList
    AllocationLines is a Panel
        list is AllocationLine set.AllocationLinesForAllocationControlList
    DefaultStructures is a Panel
        visible when (Type.Actuals)
        form is Structures
    DefaultBudgetStructures is a Panel
        title is "DefaultStructures"
        visible when (IsScenarioEnteredAndBudgetType)
        form is BudgetStructures
    BudgetDimensions is a Panel
        visible when (IsScenarioEnteredAndBudgetType)
        form is BudgetDimensions
    DimensionOverrideAndWeightBuilder is a Panel
        list is AllocationWeightGenerator set.primary
    RunOptions is a Panel
        form is RunOption
    Runs is a Panel
        list is RunsRel.AllocationControlList
    GeneratedListingReport is a Panel
        list is AllocationListingReport set.primary
