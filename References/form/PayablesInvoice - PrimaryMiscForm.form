PrimaryMiscForm is a Form
    Layout
        two column distributed
            column1
                paragraph
                    ForeignEconomicRegulatoryCode
                    visible when (FercTypeEntered)
                        display as text
                            paragraph
                                ForeignEconomicRegulatoryCode.FercType
                                DerivedPaymentType
                                    label is "PaymentType"
                                DerivedPaymentTypeDesc
                                    label is "Description"
            column2
                display as text
                    paragraph
                        Vendor.LanguageCode
                            label is "Locale"
        two column distributed
            paragraph
                NatureOfTransactionCode
                NatureOfTransactionCode.Description
                    no label
                    display as text
                StatisticalProcedure
                StatisticalProcedure.Description
                    no label
                    display as text
            paragraph
                ShipTerm
                ShipTerm.Description
                    no label
                    display as text
                ShippingMethod
        two column distributed
            paragraph
                GlobalDocumentType
                    label is "DocumentTypeCode"
                GlobalDocumentType.Description
                    no label
                    display as text
                Dropship
            paragraph
                UnloadingPort
                UnloadingPort.Description
                    no label
                    display as text
        visible when (TaxEntityRel.IntrastatUsed)
            header2 of "IntrastatInformation"
            single column
                button of "IntrastatNumber:<IntrastatNumber>"
                    visible when (IntrastatRecordExist)
                    link is IntrastatHeaderNav
                    display as link
            display as text
                five column
                    column1
                        text of "From"
                        text of "To"
                    column2
                        OriginCountry
                            label is "Country/\\Jurisdiction"
                        DestinationCountry
                            no label
                    column3
                        OriginCountry.MemberOfEC
                            label is "MemberOfEuropeanUnion"
                        DestinationCountry.MemberOfEC
                            no label
                    column4
                        OriginRegion
                            label is "Region"
                        DestinationRegion
                            no label
                    column5
                        OriginRegion.ExcludeFromIntrastat
                            label is "ExcludeFromIntrastat"
                        DestinationRegion.ExcludeFromIntrastat
                            no label
        header2 of "NordicUsage"
        three column
            ReferenceType
                display as required when (ReferenceTypeRequired)
            CustomerID
                when value changed
                    refresh ReferenceTypeRequired
            paragraph
                PaymentCategoryCode
                    label is "PaymentCategory"
                PaymentCategoryCode.Description
                    no label
                    display as text
