AssetMassReleaseResultContext is a Form
    title is "AssetReleaseResult"
    Layout
        three column
            column1
                paragraph
                    visible when (CompanyEntered)
                        Company
                        Company.GeneralLedgerCompany.Name
                            no label
                            display as text
                    visible when (CompanyGroupEntered)
                        CompanyGroup
                        CompanyGroup.Description
                            no label
                            display as text
                visible when (ReleasedPostedTransactionsExists)
                    paragraph
                        JournalizeGroup
                    paragraph
                        button of "ViewJournals"
                            link is ViewJournal
                            display as link
            column2
                paragraph
                    CreateDate
                        label is "CreationDate"
                paragraph
                    CreatedBy
                paragraph
                    Status
                        raise red alert when (Incomplete)
                    Release
                        label is "Update"
