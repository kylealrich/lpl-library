AssetTransferOptions is a Form
    Layout
        two column
            column1
                paragraph
                    Asset
                        display as text
                    Asset.Description
                        no label
                        display as text
                paragraph
                    Asset.TagNumber
                        label is "Tag"
                        display as text
                visible when (DisplayOnlyLeaseFields)
                    paragraph
                        Asset.AssetLease.LeaseCompany
                            label is "LeaseCompany"
                        Asset.AssetLease.LeaseCompany.Name
                            no label
                            display as text
            column2
                paragraph
                    Asset.DerivedStatus
                        label is "AssetStatus"
                        display as text
                visible when (DisplayOnlyLeaseFields)
                    paragraph
                        Asset.AssetLease.Lease
                        Asset.AssetLease.Lease.Description
                            no label
                            display as text
        two column
            column1
                AssetRel.EnteredQuantity
                    label is "AssetEnteredQuantity"
                    display as text
                AssetRel.LastAssetItemPurchaseDate
                    label is "LastItemPurchaseDate"
                    display as text
            column2
                paragraph
                    visible when (Asset.AllowTransferOfLeaseCompany)
                        TransferQuantity
                            display as text
                    visible when (not Asset.AllowTransferOfLeaseCompany)
                        TransferQuantity
                TransferDate
                PostingDate
        blank line
        two column distributed
            column1
                paragraph
                    text of "TransferFrom"
                        bold
                display as text
                    visible when (Asset.AllowTransferOfLeaseCompany)
                        paragraph
                            Asset.AssetLease.Lease
                        paragraph
                            Asset.AssetLease.LeaseCompany
                                label is "LeaseCompany"
                            Asset.AssetLease.LeaseCompany.Name
                                no label
                    paragraph
                        Company
                        Company.GeneralLedgerCompany.Name
                            no label
                            show up to 20 characters
                    TagNumber
                    paragraph
                        AssetGroup
                        AssetGroup.Description
                            no label
                            show up to 20 characters
                    paragraph
                        AssetLocation
                        AssetLocation.Description
                            no label
                            show up to 20 characters
                    paragraph
                        AssetDivision
                        AssetDivision.Description
                            no label
                            show up to 20 characters
                    paragraph
                        AssetType
                            compact format
                        AssetType.Description
                            no label
                            show up to 20 characters
                    paragraph
                        AssetAccountGroup
                        AssetAccountGroup.Description
                            no label
                            show up to 20 characters
                    paragraph
                        AssetAccountingUnitGroup
                        AssetAccountingUnitGroup.Description
                            no label
                            show up to 20 characters
                    paragraph
                        AssetAccountingUnit
                            label is "DefaultTransactionDimensions"
                            compact format
                    paragraph
                        AssetOwner
                        AssetOwner.Name.FirstAndLastName
                            no label
                            show up to 20 characters
                paragraph
                    text of "Depreciation_Expense_Project"
                        color of gray
                        align as label
                    text of "<Asset.DepreciationExpenseProject>"
                paragraph
                    text of "AssetProject"
                        color of gray
                        align as label
                    text of "<Asset.AssetProject>"
            column2
                paragraph
                    text of "TransferTo"
                        bold
                visible when (Asset.AllowTransferOfLeaseCompany)
                    paragraph
                        AssetTransferTo.ToAssetLease.Lease
                            label is "Lease"
                            display as text
                    paragraph
                        AssetTransferTo.ToAssetLease.LeaseCompany
                            label is "LeaseCompany"
                        AssetTransferTo.ToAssetLease.LeaseCompany.Name
                            no label
                            display as text
                paragraph
                    AssetTransferTo.ToAssetCompany
                        label is "Company"
                        when value changed
                            refresh AssetTransferTo.ToAssetType
                            refresh AssetTransferTo.ToAssetAccountGroup
                            refresh AssetTransferTo.ToAssetAccountingUnitGroup
                    AssetTransferTo.ToAssetCompany.GeneralLedgerCompany.Name
                        no label
                        display as text
                        show up to 20 characters
                paragraph
                    AssetTransferTo.ToTagNumber
                        label is "TagNumber"
                paragraph
                    AssetTransferTo.ToAssetGroup
                        label is "AssetGroup"
                    AssetTransferTo.ToAssetGroup.Description
                        no label
                        display as text
                        show up to 20 characters
                paragraph
                    AssetTransferTo.ToAssetLocation
                        label is "Location"
                    AssetTransferTo.ToAssetLocation.Description
                        no label
                        display as text
                        show up to 20 characters
                    visible when (AssetTransferTo.ToAssetLocation entered)
                        AssetTransferTo.ToAssetLocationDetail
                            label is "LocationDetail"
                            select is ActiveAssetLocationDetails
                        AssetTransferTo.ToAssetLocationDetail
                            no label
                            display as text
                            show up to 20 characters
                paragraph
                    AssetTransferTo.ToAssetDivision
                        label is "Division"
                    AssetTransferTo.ToAssetDivision.Description
                        no label
                        display as text
                        show up to 20 characters
                paragraph
                    AssetTransferTo.ToAssetType
                        label is "AssetType"
                        compact format
                    AssetTransferTo.ToAssetType.Description
                        no label
                        display as text
                        show up to 20 characters
                paragraph
                    AssetTransferTo.ToAssetAccountGroup
                        label is "AssetAccountGroup"
                    AssetTransferTo.ToAssetAccountGroup.Description
                        no label
                        display as text
                        show up to 20 characters
                paragraph
                    AssetTransferTo.ToAssetAccountingUnitGroup
                        label is "AssetDimensionGroup"
                    AssetTransferTo.ToAssetAccountingUnitGroup.Description
                        no label
                        display as text
                        show up to 20 characters
                paragraph
                    AssetTransferTo.ToAssetAccountingUnit
                        label is "DefaultTransactionDimensions"
                        compact format
                paragraph
                    AssetTransferTo.ToAssetOwner
                        label is "AssetOwner"
                    AssetTransferTo.ToAssetOwner.Name.FirstAndLastName
                        no label
                        display as text
                        show up to 20 characters
                paragraph
                    text of "Depreciation_Expense"
                        color of gray
                        align as label
                    AssetTransferTo.ToDepreciationExpenseProject
                paragraph
                    text of "Asset"
                        color of gray
                        align as label
                    AssetTransferTo.ToAssetProject
