FinanceResourceForm is a CompositeForm
    title is "<DerivedFormTitle>"
    context form is EmployeeAtAGlance
        background is theme color
    restrict action Create
    restrict action Update
    restrict action AddToTeam
    restrict action SendMessage
    restrict action Reminder
    restrict action WorkRequest
    restrict action Reload
    Calendar is a Panel
        visible when (ShowMyReminders)
        list is MyRemindersRel.CalendarList
    Schedule is a Panel
        visible when (ShowMyReminders)
        list is MyRemindersRel.FinanceResourceScheduleList
    Teams is a Panel
        visible when (DisplayTeams)
        list is TeamsRel.MyTeamsList
