TransferPricingTransactionOneViewHeader is a Form
    title is "TransferPricingTransaction"
    Layout
        five column distributed
            column1
                TransferPricing
                    label is "RechargeName"
                    display as text
                TransferPricingRun
                    label is "RechargeCalculation"
                    display as text
                TransferPricingTransaction
                    label is "RechargeTransaction"
                    display as text
                TransactionNumber
                    label is "TransactionLine"
                    display as text
            column2
                TransferPricing.TransferPricingType
                    display as text
                FromCompany
                    label is "ReceivablesCompany"
                    display as text
                FromEntity
                    label is "BillFromEntity"
                    display as text
            column3
                DerivedRechargeAmount
                    display as text
                ToCompany
                    label is "PayablesCompany"
                    display as text
                ToEntity
                    label is "BillToEntity"
                    display as text
            column4
                visible when (IsUnprocessed)
                    TargetTransactionAmount
                        display as text
                    TargetCurrency
                visible when (not IsUnprocessed)
                    DerivedBillingDocumentTotal
                        display as text
                visible when (IsUnprocessed)
                    CurrencyTable
                visible when (not IsUnprocessed)
                    CurrencyTable
                        display as text
                TargetCurrencyExchangeRate
                    display as text
                button of "<DerivedCreatedICBDocumentID>"
                    visible when (IsBillingDocumentCreated)
                    link is ViewBillingDocument
                    label is "BillingDocument"
                    display as link
            column5
                Status
                    display as tag
                        color of blue when (Status.Unprocessed)
                        color of turquoise when (Status.Processing)
                        color of green when (Status.Processed)
                        color of red when (Status.Failed)
                        color of gray
                TransferPricingRun.TransactionDate
                    label is "TransactionDate"
                    display as text
                TransferPricingRun.PostingDate
                    label is "PostingDate"
                    display as text
        single column
            visible when (ErrorExists)
                text of "Error:<DerivedErrorMessage>"
                    display as tag
                        color of red
            visible when (not ToCompanyExists)
                text of "Error:<ToCompanyNotExistErrorMessage>"
                    display as tag
                        color of red
            visible when (IsRateUndefined)
                text of "Warning:<RateWarningMessage>"
                    display as tag
                        color of yellow
            visible when (FromCompanyWarningExists)
                text of "Warning:<FromCompanySetupWarningMessage>"
                    display as tag
                        color of yellow
            visible when (ToCompanyWarningExists)
                text of "Warning:<ToCompanySetupWarningMessage>"
                    display as tag
                        color of yellow
            visible when (BillFromCurrencyWarningExists)
                text of "Warning:<BillFromCurrencyWarningMessage>"
                    display as tag
                        color of yellow
            ReceivablesRevenueAccount
                compact format
            PayablesExpenseAccount
                compact format
