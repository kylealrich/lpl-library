MassPrintForm1099 is an ActionForm
    action is MassPrintForm1099
    Layout
        header3 of "PrintOptions"
        paragraph
            PrintCopyA
            visible when (PrintCopyA)
                CopyAPrinter
                    select is ActiveIDMPrinter
                CopyAPrinter.Description
                    no label
                    display as text
        paragraph
            PrintCopyB
            visible when (PrintCopyB)
                CopyBPrinter
                    select is ActiveIDMPrinter
                CopyBPrinter.Description
                    no label
                    display as text
        visible when (PrintCopyB)
            paragraph
                CopyBPrintOption
                    no label
                    indent
                    display as horizontal radio buttons
        paragraph
            PrintCopyC
            visible when (PrintCopyC)
                CopyCPrinter
                    select is ActiveIDMPrinter
                CopyCPrinter.Description
                    no label
                    display as text
