UpdateBankStatementTotals is an ActionForm
    action is UpdateBankStatementTotal
    Layout
        paragraph
            PrmDateRange.Begin
                label is "StatementDateRange"
            text of "_-"
            PrmDateRange.End
                no label
        single column
            PrmIncludeUnreconciled
                label is "IncludeUnreconciledBankStatements"
        blank line
        header3 of "Optional:"
        single column
            PrmCashManagementAccount
                label is "CashAccount"
