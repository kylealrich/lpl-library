ItemUsageContext is a Form
    title is "ItemUsage"
    Layout
        display as text
            paragraph
                Item.RepresentativeText
                Item.Description2
                    no label
                visible when (CompanyEntered)
                    Company
                    Company.Name
                        no label
                visible when (LocationEntered)
                    InventoryLocation.RepresentativeText
                visible when (ReqLocationEntered)
                    RequestingLocation.RepresentativeText
        paragraph
            ItemmastUsageDateRange
                label is "UsageDateRange"
