LocationContact is a Form
    Layout
        single column
            ContactName
            PhoneNumber
            MobileNumber
            FaxNumber
            TelexNumber
                label is "Telex"
            VendorAccount
                label is "AccountNumber"
        paragraph
            VATRegistrationCountry
                label is "V\\A\\TRegistrationCountry/\\Jurisdiction"
                display as required when (VATRegistrationCountryRequired)
            VATRegistrationNumber
                when value changed
                    refresh VATRegistrationCountryRequired
        visible when (VendorGroup.BusinessGroup.UseHSNSACCode)
            paragraph
                TaxRegistrationType
        two column
            column1
                EmailAddress
                    label is "EmailAddress"
                PayablesGLNNumber
                    label is "PayablesVendorLocationG\\L\\N"
                button of "VendorContacts"
                    list is VendorContactImportRel.VendorConversionContacts
                    display as link
            column2
                TwitterID
                SocialNetworkID2
                SocialNetworkID3
                SocialNetworkID4
                SocialNetworkID5
