RecallNoticeAnalysisDetail is a Form
    title is "_"
    Layout
        display as text
            five column distributed
                button of "PrintAnalysisInformation"
                    link is NoticeAnalysisReport
                    is primary
                Name
            three column distributed
                RecallDate
                    label is "AnnouncementDate"
                RecallReceivedDate
                visible when (NotApplicableTimestamp entered)
                    NotApplicableTimestamp
                        label is "DateSetToNotApplicable"
            five column table
                column1
                    create date
                        label is "CreationDate"
                column2
                    ReleasedTimestamp
                        label is "ReleaseDate"
                    DaysInPendingStatus
                        label is "DaysToReleaseAfterCreation"
                visible when (RecallGroup.RecallApprovalRequired)
                    column3
                        ApprovalTimestamp
                            label is "ApprovalDate"
                        DaysInAwaitingApprovalStatus
                            label is "DaysToApproveAfterClose"
                column4
                    ClosedTimestamp
                        label is "CloseDate"
                    DaysInActiveStatus
                        label is "DaysToCloseAfterRelease"
                column5
                    FinalizedDate
                    DaysUntilFinalized
                        label is "DaysToFinalizeAfterCreation"
            single column
                Description
                    use text area of 4 lines
            two column distributed
                visible when (ManufactureDateRange entered)
                    paragraph
                        ManufactureDateRange.Begin
                            label is "ManufactureDateFrom"
                        ManufactureDateRange.End
                            label is "ManufactureDateTo"
                visible when (DistributionDateRange entered)
                    paragraph
                        DistributionDateRange.Begin
                            label is "DistributionDateFrom"
                        DistributionDateRange.End
                            label is "DistributionDateTo"
            two column distributed
                ManufacturerName
                    label is "Manufacturer"
                paragraph
                    RecallCoordinator.RecallUserName
                        label is "Coordinator"
                    button of icon.mail
                        link is "mailto:<RecallCoordinator.EmployeeWorkEmailAddress>"
                    button of icon.phone
                        link is "tel:<RecallCoordinator.EmployeeWorkTelephone>"
