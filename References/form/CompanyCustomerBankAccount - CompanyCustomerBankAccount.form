CompanyCustomerBankAccount is a Form
    is primary
    title is "CustomerBankAccount"
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            CompanyCustomerBankAccount.CustomerBankEntity
                label is "Bank"
        paragraph
            CompanyCustomerBankAccount.CustomerBankAccountNumber
                label is "AccountNumber"
        paragraph
            Customer
                select is GroupSearchName
            Customer.Name
                no label
                display as text
        paragraph
            RIBKey
        paragraph
            BankAccountValidationType
                label is "AccountValidation"
