InvoiceDetailAllowSummarize is a CompositeForm
    title is "<DerivedInvoiceDetailAllowSummarizeFormTitle>"
    context form is InvoiceDetailContextAllowSummarize
        background is theme color
    AllInvoiceDetailsForThisInvoice is a Panel
        title is "InvoiceDetails"
        visible when (InvoiceDetailAllowSummarizeShowAllInvoiceDetailsForThisInvoice)
        list is ReceivableInvoiceRel.ReceivableInvoiceDetailRel.ListReceivableInvoiceDetail
    Main is a Panel
        visible when (InvoiceDetailAllowSummarizeShowMain)
        form is ReceivableInvoiceDetailForm
    CurrencyAmounts is a Panel
        visible when (InvoiceDetailAllowSummarizeShowCurrencyAmounts)
        form is CurrencyAmounts
    PriorSummarizeCreditApplications is a Panel
        title is "PriorSummarizeCreditApplications"
        visible when (SummarizeDetailInvoiceHasCreditApplicationInvoice)
        list is ReceivableCreditApplicationInvoiceRel.ReviewCreditMemoApplications
    PriorCreditApplications is a Panel
        title is "PriorCreditApplications"
        visible when (InvoiceDetailAllowSummarizeShowPriorCreditApplications)
        list is CreditMemoReceivableApplicationRel.ReviewCreditMemoApplications
    PriorSummarizedApplications is a Panel
        visible when (SummarizedDetailInvoiceHasPaymentApplication)
        list is ReceivableApplicationInvoiceRel.TransactionApplicationsList
    PriorApplications is a Panel
        visible when (InvoiceDetailAllowSummarizeShowPriorApplications)
        list is ReceivableApplicationRel.TransactionApplicationsList
    PendingSummarizeApplications is a Panel
        title is "SelectedSummarizeApplications"
        visible when (SummarizeDetailInvoiceHasApplicationSelect)
        list is ReceivableInvoiceRel.ReceivableApplicationSelectForThisInvoiceRel.ReviewReceivableApplicationSelectList
    PendingApplications is a Panel
        title is "SelectedApplications"
        visible when (HasReceivableApplicationSelect)
        list is ReceivableApplicationSelectRel.ReviewReceivableApplicationSelectList
