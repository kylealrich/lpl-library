TaxConfigurationContext is a Form
    title is "TaxConfiguration"
    Layout
        paragraph
            TaxConfiguration
            WriteToLogFile
            DisplayWebserviceErrorXml
                label is "IfWebserviceError,ShowCaptured_\\X\\M\\L"
            paragraph
                ValidateVAT
                visible when (ValidateVAT)
                    VATTaxEntity
                        select is TaxEntityVATSelectList
        paragraph
            ThirdParty
            visible when (ThirdParty.Sabrix)
                SabrixVersion.Version
                    label is "Version"
                SabrixVersion.Release
                    label is "Release"
                OneSourceVersionType
            visible when (ShowForVertex)
                VertexVersionText
                    no label
                    display as text
                    show up to 10 characters
                last VertexConfigVersion9Rel.VertexAccessType
