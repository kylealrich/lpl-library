MyResourceForm is a CompositeForm
    title is "<DerivedFormTitle>"
    restrict action Create
    restrict action Delete
    restrict action InactivateAndReassign
    restrict action AddToTeam
    restrict action Reload
    Actions
        action is SendMessage
        action is Reminder
        action is WorkRequest
        action is save
        action is refresh
    Overdue is a Panel
        visible when (ShowMyOverDue)
        list is ShowOverDueRel.FinanceResourceScheduleList
    RejectedOneTimeTasks is a Panel
        visible when (ShowOneTimeRejections)
        list is MyOneTimeTaskRejectedRel.RejectedOneTimeTasks
    Approvals is a MultiListPanel
        visible when (ShowApprovals)
        list is MyApproverClosePeriodTaskRel.MaintainApprovalsList
            visible when (ShowTaskApprovals)
            show 20 lines
        list is MyOneTimeTaskApproverRel.MaintainOneTimeApprovalsList
            visible when (ShowOneTimeApprovals)
            show 20 lines
    TeamApprovals is a MultiListPanel
        visible when (ShowTeamApprovals)
        list is MyApproverTeamTaskRel.MaintainApprovalsList
            visible when (ShowTeamTaskApprovals)
            show 20 lines
        list is MyOneTimeApproverTeamTaskRel.MaintainOneTimeApprovalsList
            visible when (ShowOneTimeTeamApprovals)
            show 20 lines
    Messages is a MultiListPanel
        list is MessagesForMeRel.ViewResourceMessageList
            visible when (ShowMessagesForMe)
        list is MessagesSentRel.ViewResourceMessageSentList
            visible when (ShowMessageSent)
    DueToday is a Panel
        visible when (ShowMyDueToday)
        list is ShowDueTodayRel.FinanceResourceScheduleList
    ToDo is a Panel
        visible when (ShowMyCurrentToDo)
        list is CurrentToDoRel.FinanceResourceScheduleList
    TeamToDo is a Panel
        visible when (HasUnassigned)
        list is UnassignedRel.UnassignedTasksList
    Reconciliations is a Panel
        visible when (ShowMyPeriodEndReconciliations)
        list is MyPeriodEndReconciliationsRel.MyPeriodEndReconciliationList
    TeamReconciliations is a Panel
        visible when (ShowMyTeamPeriodEndReconciliations)
        list is MyTeamPeriodEndReconciliationsRel.PeriodEndReconciliationList
    FutureTasks is a MultiListPanel
        visible when (ShowFutureTasks)
        list is FutureTasksRel.FutureFinanceResourceScheduleList
            show 20 lines
        list is UnassignedFutureRel.UnassignedFutureTasksList
            show 20 lines
    Calendar is a Panel
        visible when (ShowMyReminders)
        list is MyRemindersRel.CalendarList
    Profile is a Panel
        form is EmployeeAtAGlance
