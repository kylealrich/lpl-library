ParLocationCountWizard is a WizardForm
    use for action Unreleased.CreateFromParLocation
    title is "ParLocationCount"
    show steps
    restrict action Unreleased.Update
    ParLocation is a Panel
        mouse over text is "ParLocationCount"
        form is ParLocationItems
        save on next
    ParLocationItems is a Panel
        mouse over text is "EnterParLocationCount"
        list is ParItemsRel.ParItemsForParCountList
    FillOrKill is a Panel
        mouse over text is "Fill_orKill"
        form is FillOrKill
        save on next
    ParLocationSummary is a Panel
        form is ParLocationItemsSummary
