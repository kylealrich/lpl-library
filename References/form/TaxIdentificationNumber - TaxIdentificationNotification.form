TaxIdentificationNotification is an ActionForm
    action is TaxIdentificationNumber
    Layout
        paragraph
            VendorGroup
                select is VendorGroups
            VendorGroup.Description
                no label
                display as text
        paragraph
            VendorClass
                select is VendorClass
            VendorClass.Description
                no label
                display as text
        single column
            OneTimeVenFlag
                label is "ExcludeOne\\-\\TimeVendors"
        paragraph
            FromVendor
                select is VendorName
            ThruVendor
                label is "ToVendor"
                select is VendorName
        single column
            IncomeCode
        single column
            NameOption
        paragraph
            FromCreateDate
                label is "FromCreateDate"
            ThruCreateDate
                label is "ToCreateDate"
        paragraph
            FromPmtDate
                label is "FromPaymentDate"
            ThruPmtDate
                label is "ToPaymentDate"
        single column
            FileName
                label is "FileName"
            Update
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                TaxIdentificationNumberReport_RDG
            column2 of 25%
                TaxIdentificationNumberReport_XPT
