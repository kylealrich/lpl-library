CreateOffContractPurchasing is an ActionForm
    action is CreateOffContractPurchasing
    title is "OffContractPurchasing"
    Layout
        single column
            Description
                label is "ReportDescription"
            ItemGroup
            Item
            TransientManufacturer
                label is "Manufacturer"
                compact format
        single column
            CommodityCode
                label is "CommodityCode"
                compact format
        two column
            column1
                PurchasingMajorClass
                    label is "MajorPurchasingClass"
                    show up to 10 characters
            column2
                PurchasingMinorClass
                    label is "MinorPurchasingClass"
                    show up to 10 characters
        single column
            UNSPSCCode
                label is "UNSPSCCode"
                compact format
        two column
            column1
                FromDate
                    label is "FromPurchaseOrderDate"
            column2
                ThruDate
                    label is "ToPurchaseOrderDate"
        single column
            IncludeDetails
            PrintParameters
