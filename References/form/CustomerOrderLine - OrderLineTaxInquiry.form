OrderLineTaxInquiry is a Form
    restrict action CreateCustomerOrderReturnLine
    Layout
        paragraph
            GrossAmountCurrency
                label is "GrossAmount"
                display as text
        paragraph
            DiscountAmountCurrency
                label is "Discount"
                display as text
        paragraph
            EnteredDiscount
                label is "AdditionalDiscount"
                display as text
        paragraph
            NetAmountCurrency
                label is "NetAmount"
                display as text
        paragraph
            DerivedTaxTotal
                label is "LineTaxTotal"
                display as text
        paragraph
            LineTotal
                display as text
