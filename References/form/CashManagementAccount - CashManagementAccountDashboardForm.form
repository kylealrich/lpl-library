CashManagementAccountDashboardForm is a Form
    title is "AccountDetail"
    Layout
        three column distributed
            column1
                Description
                    header2
            column2
                paragraph
                    CashManagementLocation
                        select is CashManagementLocationsNoChildren
                    CashManagementLocation.Description
                        no label
                        display as text
                    visible when (ParentLocationStructure entered)
                        text of "(<ParentLocationStructure>)"
                            italics
            column3
                ActiveStatus
                    no label
                    display as tag
                        color of red when (not Active)
                        color of green when (Active)
        three column distributed
            column1
                AccountType
                paragraph
                    LegalEntity
                        select is CompaniesForFinanceEnterpriseGroup
                    LegalEntity.Company.Name
                        no label
                        display as text
                FundingStructure
                WriteCSVHeader
                visible when (StatementLineClassification.DefaultFromAccount)
                    paragraph
                        DefaultCategory
                        DefaultCategory.Description
                            no label
                            display as text
            column2
                paragraph
                    OpenDate
                    CloseDate
                InternationalBankAccountNumber
                CashPositionTarget
                ElectronicFileTransmission
            column3
                paragraph
                    Currency
                    Currency.Description
                        no label
                        display as text
                StatementLineClassification
                ChecksIssued
                paragraph
                    AccountRestriction
                    AccountRestrictionDescription
                        no label
