PositivePayOutputFileHeaderForm is a CompositeForm
    is primary
    title is "PositivePayOutputFileHeader"
    context form is PositivePayOutputFileHeaderContext
        background is theme color
    OutputDetail is a Panel
        list is PositivePayOutputFileDetail set.PositivePayOutputFileDetails
    CSV is a Panel
        visible when (CreateCsv.Yes)
        list is PositivePayOutputFileDetail set.CSVList
