RecurringJournalScheduleMainForm is a Form
    is primary
    title is "Schedule"
    Layout
        two column distributed
            column1
                AccountingEntity
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>"
                GeneralLedgerClosePeriod.GeneralLedgerCloseYear
                    label is "Year"
                GeneralLedgerClosePeriod.PeriodName
                    label is "PeriodName"
            column2
                RecurringJournalControl
                    label is "JournalNumber"
                GeneralLedgerClosePeriod
                    label is "Period"
                paragraph
                    visible when (not CannotChangePeriodSelect)
                        PeriodSelected
                    visible when (CannotChangePeriodSelect)
                        PeriodSelected
                            display only
                    PeriodProcessed
                        display only
