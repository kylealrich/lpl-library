PrintableMultiPeriodForm is a SummaryForm
    title is "PeriodTransactionDetailsSummaryReport"
    Layout
        paragraph
            StudentActivityDistrict
                label is "SchoolDistrict"
            StudentActivityDistrict.Name
                no label
        paragraph
            StudentActivityAccount
                label is "Activity"
            Description
                no label
                display only
        visible when (HasParent)
            paragraph
                ParentStudentActivityAccount
                    label is "ParentAccount"
                ParentStudentActivityAccount.Description
                    no label
        paragraph
            ReportingPeriods.FromPeriod
                label is "PeriodRange"
            ReportingPeriods.ToPeriod
                no label
        blank line
        form is MultiPeriodStudentActivityBalanceRel.PrintableMultiPeriodDetailsForm
