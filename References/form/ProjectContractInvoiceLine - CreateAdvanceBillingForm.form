CreateAdvanceBillingForm is an ActionForm
    action is CreateAdvanceBilling
    title is "CreateAdvanceBilling"
    Layout
        single column
            PrmAmount
                label is "Amount"
            Description
            PrmProject
                label is "<FinanceEnterpriseGroup.ProjectLabel>"
                select is ProjectContract.PostingProjectsRel.ContractPostingProjectSelect
            AdvanceFullAccount
                compact format
            visible when (FinanceDimension2.IsExternalGrantOrCustomer)
                Taxable
            PrmUnitsAmount
                label is "Units"
            Hold
