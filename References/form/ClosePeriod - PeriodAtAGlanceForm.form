PeriodAtAGlanceForm is a Form
    title is "<ClosePeriod.CloseCalendarPeriod>_<ClosePeriod.CloseCalendar>Is<CloseManagementStatus>"
    Layout
        header3 of "ScheduleDetails"
        single column
            visible when (RedAndYellowAlert)
                AlertDisplayMessage
                    no label
                    display as tag
                        color of red when (RedAlert)
                        color of yellow when (YellowAlert)
        visible when (ShowExpectedOpenDate)
            two column distributed
                column1
                    paragraph
                        ScheduledBeginDate
                            label is "ExpectedOpen"
                            display as text
                        ScheduledBeginTime
                            no label
                            display as text
                column2
                    paragraph
                        ScheduledDueDate
                            label is "ExpectedClose"
                            display as text
                        ScheduledDueTime
                            no label
                            display as text
        visible when (ShowOpenDate)
            two column distributed
                column1
                    paragraph
                        PeriodOpenDate
                            label is "Opened"
                            display as text
                column2
                    paragraph
                        ScheduledBeginDate
                            label is "ScheduledStartDate"
                            display as text
                        ScheduledBeginTime
                            no label
                            display as text
        visible when (ShowExpectedCloseDate)
            two column distributed
                column1
                    paragraph
                        DerivedDueDate
                            label is "ExpectedClose"
                            display as text
                        DerivedDueTime
                            no label
                            display as text
                column2
                    paragraph
                        ScheduledDueDate
                            label is "ScheduledEndDate"
                            display as text
                        ScheduledDueTime
                            no label
                            display as text
        visible when (ShowClosedDate)
            two column distributed
                column1
                    paragraph
                        PeriodCloseDate
                            label is "Closed"
                            display as text
                column2
                    paragraph
                        ScheduledDueDate
                            label is "ScheduledEndDate"
                            display as text
                        ScheduledDueTime
                            no label
                            display as text
        blank line
        visible when (PeriodReopened)
            single column
                text of "<ReopenedMessage>"
                    bold
                    color of red
                    align as label
                OriginalCloseDate
                    display as text
            blank line
        visible when (CloseManagementStatus.Scheduled)
            header3 of "AtRiskBasedOnToday'sDate"
            single column
                TotalCount
                    label is "TotalCountOfTasks"
                    display as text
            two column table
                column1
                    visible when (HasOneTimeTasks)
                        CountOneTimeTask
                            label is "OneTimeTasks"
                            display as text
                    visible when (HasStartToday)
                        CountStartToday
                            label is "StartToday"
                            display as text
                    visible when (HasLateToStart)
                        CountLateToStart
                            label is "LateToStart"
                            display as text
                    visible when (HasDueToday)
                        CountDueToday
                            label is "DueToday"
                            display as text
                    visible when (HasOverDue)
                        CountOverDue
                            label is "Overdue"
                            display as text
                column2
                    visible when (HasOneTimeTasks)
                        button of "View"
                            link is OneTimeTaskNav
                            display as link
                    visible when (HasStartToday)
                        button of "View"
                            link is StartTodayNav
                            display as link
                    visible when (HasLateToStart)
                        button of "View"
                            link is LateToStartNav
                            display as link
                    visible when (HasDueToday)
                        button of "View"
                            link is DueTodayNav
                            display as link
                    visible when (HasOverDue)
                        button of "View"
                            link is OverDueNav
                            display as link
        visible when (CloseManagementStatus.Open)
            header3 of "Progress"
            three column table
                column1
                    TotalCount
                        label is "TotalCountOfTasks"
                        display as text
                    text of "_"
                    visible when (HasOneTimeTasks)
                        CountOneTimeTask
                            label is "OneTimeTasks"
                            display as text
                    visible when (HasScheduled)
                        CountScheduled
                            label is "Scheduled"
                            display as text
                    visible when (HasClosed)
                        CountClosed
                            label is "Closed"
                            display as text
                    visible when (HasInProcess)
                        CountInProcess
                            label is "InProcess"
                            display as text
                    visible when (HasPendingApproval)
                        CountPendingApproval
                            label is "PendingApproval"
                            display as text
                    visible when (ShowAllOpen)
                        CountOpen
                            label is "TotalOpen"
                            display as text
                    visible when (HasVoided)
                        CountVoided
                            label is "Voided"
                            display as text
                    visible when (HasOverDue)
                        CountOverDue
                            label is "Overdue"
                            display as text
                    visible when (HasDueToday)
                        CountDueToday
                            label is "DueToday"
                            display as text
                    visible when (HasInJeopardy)
                        CountInJeopardy
                            label is "InJeopardy"
                            display as text
                    visible when (HasLateToStart)
                        CountLateToStart
                            label is "LateToStart"
                            display as text
                column2
                    text of "_"
                    text of "_"
                    visible when (HasOneTimeTasks)
                        button of "View"
                            link is OneTimeTaskNav
                            display as link
                    visible when (HasScheduled)
                        button of "View"
                            link is ScheduledNav
                            display as link
                    visible when (HasClosed)
                        button of "View"
                            link is ClosedNav
                            display as link
                    visible when (HasInProcess)
                        button of "View"
                            link is InProcessNav
                            display as link
                    visible when (HasPendingApproval)
                        button of "View"
                            link is PendingApprovalNav
                            display as link
                    visible when (ShowAllOpen)
                        button of "View"
                            link is AllOpenNav
                            display as link
                    visible when (HasVoided)
                        button of "View"
                            link is VoidedNav
                            display as link
                    visible when (HasOverDue)
                        button of "View"
                            link is OverDueNav
                            display as link
                    visible when (HasDueToday)
                        button of "View"
                            link is DueTodayNav
                            display as link
                    visible when (HasInJeopardy)
                        button of "View"
                            link is InJeopardyNav
                            display as link
                    visible when (HasLateToStart)
                        button of "View"
                            link is LateToStartNav
                            display as link
                column3
                    CompletePercent
                        label is "PercentComplete"
                        display as progress bar
                            red alert range is from 0% to 25%
                            yellow alert range is from 26% to 99%
                            green alert range is from 100% to 100%
                    text of "_"
                    visible when (HasOneTimeTasks)
                        PercentOneTimeTask
                            label is "OneTimeTasks"
                            display as progress bar
                                red alert range is from 0% to 25%
                                yellow alert range is from 26% to 99%
                                green alert range is from 100% to 100%
                    visible when (HasScheduled)
                        PercentScheduled
                            label is "Scheduled"
                            display as progress bar
                                red alert range is from 0% to 25%
                                yellow alert range is from 26% to 99%
                                green alert range is from 100% to 100%
                    visible when (HasClosed)
                        PercentClosed
                            label is "Closed"
                            display as progress bar
                                red alert range is from 0% to 25%
                                yellow alert range is from 26% to 99%
                                green alert range is from 100% to 100%
                    visible when (HasInProcess)
                        PercentInProcess
                            label is "InProcess"
                            display as progress bar
                                red alert range is from 0% to 25%
                                yellow alert range is from 26% to 99%
                                green alert range is from 100% to 100%
                    visible when (HasPendingApproval)
                        PercentPendingApproval
                            label is "PendingApproval"
                            display as progress bar
                                red alert range is from 0% to 25%
                                yellow alert range is from 26% to 99%
                                green alert range is from 100% to 100%
                    visible when (ShowAllOpen)
                        PercentOpen
                            label is "TotalOpen"
                            display as progress bar
                                red alert range is from 0% to 25%
                                yellow alert range is from 26% to 99%
                                green alert range is from 100% to 100%
                    visible when (HasVoided)
                        PercentVoided
                            label is "Voided"
                            display as progress bar
                                red alert range is from 0% to 100%
                    visible when (HasOverDue)
                        PercentOverDue
                            label is "Overdue"
                            display as progress bar
                                red alert range is from 0% to 25%
                                yellow alert range is from 26% to 99%
                                green alert range is from 100% to 100%
                    visible when (HasDueToday)
                        PercentDueToday
                            label is "DueToday"
                            display as progress bar
                                red alert range is from 0% to 25%
                                yellow alert range is from 26% to 99%
                                green alert range is from 100% to 100%
                    visible when (HasInJeopardy)
                        PercentInJeopardy
                            label is "InJeopardy"
                            display as progress bar
                                red alert range is from 0% to 25%
                                yellow alert range is from 26% to 99%
                                green alert range is from 100% to 100%
                    visible when (HasLateToStart)
                        PercentLateToStart
                            label is "LateToStart"
                            display as progress bar
                                red alert range is from 0% to 25%
                                yellow alert range is from 26% to 99%
                                green alert range is from 100% to 100%
        visible when (CloseManagementStatus.Closed)
            header3 of "ProcessingOverview"
            single column
                TotalCount
                    label is "TotalCountOfTasks"
                    display as text
                visible when (HasOneTimeTasks)
                    paragraph
                        CountOneTimeTask
                            label is "OneTimeTasks"
                            display as text
                        button of "View"
                            link is OneTimeTaskNav
                            display as link
            blank line
            three column table
                column1
                    visible when (HasClosed)
                        CountClosed
                            label is "Closed"
                            display as text
                    visible when (HasVoided)
                        CountVoided
                            label is "Voided"
                            display as text
                    visible when (HasReopened)
                        CountReopened
                            label is "Reopened"
                            display as text
                    visible when (HasStartEarly)
                        CountStartEarly
                            label is "StartedEarly"
                            display as text
                    visible when (HasStartLate)
                        CountStartLate
                            label is "StartedLate"
                            display as text
                    visible when (HasDoneEarly)
                        CountDoneEarly
                            label is "CompletedEarly"
                            display as text
                    visible when (HasDoneLate)
                        CountDoneLate
                            label is "CompletedLate"
                            display as text
                column2
                    visible when (HasClosed)
                        PercentClosed
                            no label
                            display as progress bar
                                red alert range is from 0% to 25%
                                yellow alert range is from 26% to 99%
                                green alert range is from 100% to 100%
                    visible when (HasVoided)
                        PercentVoided
                            no label
                            display as progress bar
                                red alert range is from 0% to 100%
                    visible when (HasReopened)
                        PercentReopened
                            no label
                            display as progress bar
                                yellow alert range is from 0% to 25%
                                red alert range is from 26% to 100%
                    visible when (HasStartEarly)
                        PercentStartEarly
                            no label
                            display as progress bar
                                green alert range is from 0% to 25%
                                yellow alert range is from 26% to 49%
                                red alert range is from 50% to 100%
                    visible when (HasStartLate)
                        PercentStartLate
                            no label
                            display as progress bar
                                green alert range is from 0% to 25%
                                yellow alert range is from 26% to 49%
                                red alert range is from 50% to 100%
                    visible when (HasDoneEarly)
                        PercentDoneEarly
                            no label
                            display as progress bar
                                green alert range is from 0% to 25%
                                yellow alert range is from 26% to 49%
                                red alert range is from 50% to 100%
                    visible when (HasDoneLate)
                        PercentDoneLate
                            no label
                            display as progress bar
                                green alert range is from 0% to 25%
                                yellow alert range is from 26% to 49%
                                red alert range is from 50% to 100%
                column3
                    visible when (HasClosed)
                        button of "View"
                            link is ClosedNav
                            display as link
                    visible when (HasVoided)
                        button of "View"
                            link is VoidedNav
                            display as link
                    visible when (HasReopened)
                        button of "View"
                            link is ReopenedNav
                            display as link
                    visible when (HasStartEarly)
                        button of "View"
                            link is StartEarlyNav
                            display as link
                    visible when (HasStartLate)
                        button of "View"
                            link is StartLateNav
                            display as link
                    visible when (HasDoneEarly)
                        button of "View"
                            link is DoneEarlyNav
                            display as link
                    visible when (HasDoneLate)
                        button of "View"
                            link is DoneLateNav
                            display as link
