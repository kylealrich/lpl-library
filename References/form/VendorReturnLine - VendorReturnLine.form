VendorReturnLine is a CompositeForm
    is primary
    title is "VendorReturnLine"
    context form is DetailContext
        background is theme color
    Main is a Panel
        form is Main
    Detail is a MultiListPanel
        visible when (IsNotDistributionManagement)
        list is VendorReturnLineDetailsRel.Details
            show 3 lines
        list is VendorReturnCancelDetailsRel.CancelDetails
            show 3 lines
    DMDetail is a MultiListPanel
        title is "Detail"
        visible when (IsDistributionManagement)
        list is VendorReturnLineDetailsRel.DMDetails
            show 3 lines
        list is VendorReturnCancelDetailsRel.CancelDetails
            show 3 lines
    UDIDetails is a MultiListPanel
        title is "UDI/DSCSADetail"
        visible when (IsUDIRequired)
        list is PurchaseOrderUDIDetailReturnRel.UDIEntrys
            show 3 lines
    Distributions is a Panel
        visible when (not InventoryItem)
        list is VendorReturnLineDistributionsRel.VendorReturnLineDistributions
    Comments is a Panel
        list is VendorReturnLineComment set.VendorReturnLineComments
