BudgetScenarioSearchForm is a Form
    title is "SearchBudgetScenario"
    Layout
        paragraph
            Scenario
                select is Scenario.AllChildrenRel.BudgetLeafScenarios
            AccountingEntity
                label is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
            visible when (Scenario.HasPostingAccountingUnit)
                GeneralLedgerTotal.AccountingUnit
                    label is "<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>"
            visible when (Scenario.HasPostingProject)
                GeneralLedgerTotal.Project
                    label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
            GeneralLedgerTotal.PrimaryLedger
                label is "PrimaryLedger"
                select is PrimaryLedgersList
            visible when (Scenario.HasPostingDimension1)
                FinanceDimension1
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension1Label>"
            visible when (Scenario.HasPostingDimension2)
                FinanceDimension2
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>"
            visible when (Scenario.HasPostingDimension3)
                FinanceDimension3
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension3Label>"
            visible when (Scenario.HasPostingDimension4)
                FinanceDimension4
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension4Label>"
            visible when (Scenario.HasPostingDimension5)
                FinanceDimension5
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension5Label>"
            visible when (Scenario.HasPostingDimension6)
                FinanceDimension6
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension6Label>"
            visible when (Scenario.HasPostingDimension7)
                FinanceDimension7
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension7Label>"
            visible when (Scenario.HasPostingDimension8)
                FinanceDimension8
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension8Label>"
            visible when (Scenario.HasPostingDimension9)
                FinanceDimension9
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension9Label>"
            visible when (Scenario.HasPostingDimension10)
                FinanceDimension10
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension10Label>"
            visible when (Scenario.HasPostingAccount)
                GeneralLedgerTotal.GeneralLedgerChartAccount
                    label is "<actor.context.FinanceEnterpriseGroup.AccountLabel>"
            GeneralLedgerTotal.EntityYearPeriod.Date
                label is "Date"
