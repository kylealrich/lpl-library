SchoolActivityAccountMultiPeriodContextForm is a Form
    title is "PeriodTransactionDetails"
    Layout
        paragraph
            StudentActivitySchool
                label is "School"
                display only
            StudentActivitySchool.SchoolName
                no label
        paragraph
            SchoolActivityAccount.StudentActivityAccount
                label is "Activity"
                display only
            SchoolActivityAccount.StudentActivityAccount.Description
                no label
            SchoolActivityAccount.StudentActivityAccount.StudentActivityType
                no label
        paragraph
            ReportingPeriods.FromPeriod
                label is "FromPeriod"
            ReportingPeriods.ToPeriod
                label is "ToPeriod"
            visible when (ReportPeriodRangeEntered)
                button of "PrintPeriodTransactionDetails"
                    link is PrintableMultiPeriodNavigation
