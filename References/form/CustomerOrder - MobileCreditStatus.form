MobileCreditStatus is a Form
    title is "CancellationOrderSummary"
    Layout
        paragraph
            Customer.Name
                label is "Name"
                display as text
        paragraph
            CompanyCustomer.Contact
                label is "Contact"
                display as text
        paragraph
            CompanyCustomer.PhoneNumber
                label is "Phone"
                display as text
        paragraph
            CompanyCustomer.TelexNbr
                label is "TelexNbr"
                display as text
        two column table
            column1
                HoldCode
                    label is "CustomerHoldcode"
                    display as text
                CompanyCustomer.CurrentBalance
                    label is "Balance"
                    display as text
                CompanyCustomer.OpenOrderAmount
                    label is "Orders"
                    display as text
                CompanyCustomer.Agings.Aging[1]
                    label is "Period1"
                    display as text
                CompanyCustomer.Agings.Aging[2]
                    label is "Period2"
                    display as text
                CompanyCustomer.Agings.Aging[3]
                    label is "Period3"
                    display as text
                CompanyCustomer.Agings.Aging[4]
                    label is "Period4"
                    display as text
                CompanyCustomer.Agings.Aging[5]
                    label is "Period5"
                    display as text
            column2
                CompanyCustomer.Agings.Aging[1]
                    no label
                    display as text
                CompanyCustomer.CreditLimit
                    label is "Limit"
                    display as text
                CompanyCustomer.OrderLimit
                    label is "Limit"
                    display as text
                Limit1
                    label is "Limit1"
                    display as text
                Limit2
                    label is "Limit2"
                    display as text
                Limit3
                    label is "Limit3"
                    display as text
                Limit4
                    label is "Limit4"
                    display as text
                Limit5
                    label is "Limit5"
                    display as text
                OrderEntryCustomer.SingleOrderLimit
                    label is "SingleOrderLimit"
                    display as text
                OrderEntryCustomer.TotalCODOrderLimit
                    label is "CashOnDeliveryLimit"
                    display as text
