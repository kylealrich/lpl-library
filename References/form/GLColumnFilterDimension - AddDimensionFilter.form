AddDimensionFilter is an ActionForm
    action is AddDimensionFilter
    Layout
        two column
            column1
                visible when (DisplayAccountingEntity)
                    ReportingBasis
                        show up to 10 characters
                        display as required
                    AccountingEntity
                        select is ReportingBasis.AccountingEntityTopNode.ChildrenRel.AccountingEntityHierarchyTreeView
                        show up to 10 characters
                visible when (HasAccountingUnitParam)
                    AccountingUnitStructure
                        show up to 10 characters
                        initial value is ReportingBasis.AccountingUnitStructure
                    AccountingUnit
                        select is AccountingEntity.ContextStructureAccountingUnitEntityNodeRel.AccountingUnitChildrenRel.AccountingUnitTreeSelect
                        show up to 10 characters
                visible when (HasGeneralLedgerChartAccountParam)
                    GLChartAccountTopNode
                        select is TopNodes
                        show up to 10 characters
                        initial value is ReportingBasis.ChartAccountTopNode
                        when value changed
                            refresh GeneralLedgerChartAccount
                    GeneralLedgerChartAccount
                        select is GLChartAccountTopNode.ChartAccountChildrenRel.ReportingChartAccountList
                        show up to 10 characters
                visible when (HasFinanceDimension1Param)
