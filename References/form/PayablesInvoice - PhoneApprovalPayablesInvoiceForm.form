PhoneApprovalPayablesInvoiceForm is a CompositeForm
    title is "InvoiceApproval"
    context form is PhonePayablesInvoiceContext
        background is theme color
    Invoice is a Panel
        form is PhonePayablesInvoiceGeneralInformationViewOnly
    Distributions is a Panel
        title is "Distributions"
        visible when (not Company.VendorGroup.DistributionEntryOnApproval)
        list is PayablesInvoiceDistribution set.PhonePayablesInvoiceDistributions
    DistributionsEdit is a Panel
        title is "Distributions"
        visible when (Company.VendorGroup.DistributionEntryOnApproval)
        list is PayablesInvoiceDistribution set.PhoneEditablePayablesInvoiceDistributions
    PurchaseOrders is a Panel
        title is "PurchaseOrders"
        visible when (PurchaseOrderInvoice)
        list is MatchPurchaseOrderInvoicesRel.PhoneMatchPurchaseOrderInvoice
    Details is a Panel
        title is "Details"
        visible when (HasInvoiceLines)
        list is PayablesInvoiceDetailRel.PhoneInvoiceDetailList
    ChargesAndAllowances is a Panel
        title is "ChargesAndAllowances"
        visible when (AddOnChargeExists)
        list is PayablesInvoiceAddOnChargeRel.PhonePayablesInvoiceAddOnChargeList
    Messages is a Panel
        title is "Messages"
        visible when (HasInvoiceMessages)
        list is MatchInvoiceMessageRel.PhonePayablesInvoiceCostMessageList
    RelatedDocuments is a Panel
        title is "Documents"
        list is PayablesInvoiceDocument set.PhonePayablesInvoiceDocuments
    Comments is a Panel
        title is "Comments"
        list is PayablesInvoiceCommentRel.PhonePayablesInvoiceComments
    Errors is a Panel
        title is "Errors"
        visible when (ErrorsExists)
        list is PayablesInvoiceError set.PhonePayablesInvoiceErrorList
    ApprovalProgress is a Panel
        visible when (first PfiWorkunitRel.HasProcessIndicator)
        form is ProcessIndicatorForm
    ApprovalHistory is a Panel
        visible when (AuditExists)
        list is PayablesInvoiceAudit set.PhonePayablesInvoiceHistoryList
