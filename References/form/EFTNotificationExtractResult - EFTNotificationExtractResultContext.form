EFTNotificationExtractResultContext is a Form
    propagate as a drill
    title is "EFTNotificationExtractResult"
    Layout
        two column
            column1
                visible when (Company not entered)
                    paragraph
                        CustomerGroup.RepresentativeText
                            display as text
                    paragraph
                        ProcessingCompany.RepresentativeText
                            display as text
                        EFTBaseCurrency
                            label is "Currency"
                            display as text
                visible when (Company entered)
                    paragraph
                        Company.RepresentativeText
                            display as text
                        Company.Currency
                            display as text
                paragraph
                    FromDate
                        label is "DueDateRange"
                        display as text
                    text of "-"
                    ToDate
                        no label
                        display as text
                blank line
                RunTime
                    display as text
                create stamp.actor
                    label is "User"
                    display as text
                Status
                    label is "Status"
                    display as tag
                        color of green when (Status.Complete)
                        color of gray when (Status.InProcess)
            column2
                text of "Parameters:"
                    bold
                    align as label
                paragraph
                    FinanceEnterpriseGroup.RepresentativeText
                        display as text
                ProcessDate
                    label is "ProcessingDate"
                    display as text
                paragraph
                    FromOverrideDate
                        display as text
                    text of "-"
                    ToOverrideDate
                        display as text
                paragraph
                    BankTransactionCode.BankTransactionCodeName
                        label is "PaymentCode"
                        display as text
                paragraph
                    UpdateRecords
                        label is "Update"
                        display as text
                    ExemptPrenoteForImmediates
                        label is "ExemptPrenotificationForImmediatelyDueTransactions"
        visible when (NoRecordsToProcess)
            text of "<NoRecordsToProcessMsg>"
                bold
            blank line
