CreditReviewDetailForm is a Form
    title is "CreditReviewDetailForm"
    Layout
        display as text
            four column distributed
                BatchNumber
                ReceivableInvoiceDetail.ReceivableInvoiceDetailType
                    label is "Type"
                ReceivableInvoiceDetail.Invoice
                    label is "Number"
                Status
                    display as tag
                        color of gray when (Status.Unreleased)
                        color of green when (Status.OpenTransaction)
                        color of gray
            four column distributed
                Description
                GeneralLedgerDate
                TransactionDate
                DueDate
            blank line
            four column distributed
                TransientSignedTransactionAmount
                TransientSignedOpenAmountSummarized
                OriginalCurrency
                visible when (IsNonBaseCurrency)
                    InvoiceAmount.FunctionalAmount.EnteredCurrencyRate
                        label is "FunctionalRate"
                        display as text
            four column distributed
                visible when (IsNonBaseCurrency)
                    column1
                        InvoiceAmount.FunctionalAmount.EnteredCurrencyAmount
                            label is "InvoiceFunctionalAmount"
                            display as text
                    column2
                        BaseOpenAmount
                            display as text
                visible when (IsDiscount)
                    column3
                        DiscountDate.DiscDate[1]
                            label is "DiscountDate"
                            display as text
                    column4
                        DerivedDiscount1Amount
                            label is "DiscountAmount"
                            display as text
            visible when (IsMultipleDiscount)
                four column distributed
                    DiscountDate.DiscDate[2]
                        label is "Discount2Date"
                        display as text
                    DerivedDiscount2Amount
                        label is "Discount2Amount"
                        display as text
                    DiscountDate.DiscDate[3]
                        label is "Discount3Date"
                        display as text
                    DerivedDiscount3Amount
                        label is "Discount3Amount"
                        display as text
            visible when (IsAppliedOrAdjusted)
                four column distributed
                    column1
                        ApplicationAmount.CurrencyAmount
                            label is "AppliedAmount"
                            display as text
                    column2
                        AdjustmentAmount.CurrencyAmount
                            label is "AdjustmentAmount"
                            display as text
                    visible when (IsNonBaseCurrency)
                        column3
                            ApplicationAmount.FunctionalAmount.EnteredCurrencyAmount
                                label is "ApplicationFunctionalAmount"
                                display as text
                        column4
                            AdjustmentAmount.FunctionalAmount.EnteredCurrencyAmount
                                label is "AdjustmentFunctionalAmount"
                                display as text
            four column distributed
                CustomerPurchaseOrder
                    label is "PurchaseOrder"
                    display as text
                FinanceDimension2
                    label is "<GeneralLedgerCompanyRel.FinanceEnterpriseGroup.FinanceDimension2Label>"
                ProjectContract.DisplayContract
                    label is "<GeneralLedgerCompanyRel.FinanceEnterpriseGroup.ProjectLabel>Contract"
                Project
                    label is "<GeneralLedgerCompanyRel.FinanceEnterpriseGroup.ProjectLabel>"
            blank line
            four column distributed
                LastFinanceChargeDate
                    label is "Last:_FinanceChargeDate"
                    display as text
                LastStatementDate
                    label is "StatementDate"
                    display as text
                LastDunningLetterDate
                    label is "DunningLetterDate"
                    display as text
                LastDunningLetter
                    label is "DunningLetter"
                    display as text
            four column distributed
                BankTransactionCode
                    label is "PaymentCode"
                BankTransactionCode.BankTransactionType
                    label is "TransactionCategory"
                Origin
                TermsCode
            four column distributed
                ReceivableInvoiceRel.System
                SalesRepresentative
                ItemReference
                ReceivableInvoiceRel.AlternateReceivableInvoiceType
                    label is "AlternateInvoiceType"
            CustomerBillTo
                label is "BillTo"
            text of "Cross_Reference:"
                header4
            four column distributed
                column1
                    ReceivableCrossReference.Company
                        label is "Company"
                    ReceivableCrossReference.ReceivableInvoiceDetail
                        label is "TransactionDetail"
                column2
                    ReceivableCrossReference.ReceivableInvoiceType
                        label is "Type"
                    ReceivableCrossReference.InvoiceDueDate
                        label is "TransactionDetailDueDate"
                column3
                    ReceivableCrossReference.ReceivableInvoice
                        label is "Transaction"
                column4
                    ReceivableCrossReference.Customer
                        label is "Customer"
