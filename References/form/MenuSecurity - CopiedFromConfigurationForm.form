CopiedFromConfigurationForm is a Form
    title is "Copied_vs_Current"
    called out actions only
    Layout
        two column distributed
            column1
                paragraph
                    MenuSecurity.WebApp
                        display as text
                        raise red alert when (Status.Invalid)
                            mouse over text is "Invalid"
                        raise yellow alert when (Status.Inactive)
                            mouse over text is "Inactive"
                        raise green alert when (Status.Active)
                            mouse over text is "Active"
                    MenuSecurity.Role
                        display as text
                    CopiedFrom.MenuSecurity.Role
                        label is "CopiedFrom"
                        display as text
            column2
                paragraph
                    button of "ResolveDifferences"
                        action is EditCopiedWithComparison
                        right align
                        is primary
                    button of "RevertToCopiedFrom"
                        action is RevertToCopiedFrom
                        right align
        six column distributed
            column2
                text of "Copied_From"
                    header3
                    bold
            column5
                text of "Current"
                    header3
                    bold
        single column
            ConfigurationCompare
                no label
                display only
                use text area of 30 lines
                compare with CopiedFromConfigurationCompare
