ReceiptsResultsForm is a CompositeForm
    title is "ReceiptMassReleaseResult<CashLedgerMassReleaseResult>"
    context form is ReceiptsResultsContext
        background is theme color
    Actions
        action is RerunMassReleaseForCashReceiptErrors
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    Parameters is a Panel
        form is ParametersForm
    CashReceiptsReleased is a Panel
        title is "Released_(<ReleaseCount>)"
        list is CashReceiptsReleasedRel.ReleasedCashReceiptsList
    CashReceiptsInError is a Panel
        title is "Errors_(<ErrorCount>)"
        visible when (CashReceiptErrorsExist)
        list is CashReceiptsWithErrorsRel.MassReleaseCashReceiptErrorsList
