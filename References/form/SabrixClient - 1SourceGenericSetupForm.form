1SourceGenericSetupForm is a Form
    title is "1SourceConfig"
    Layout
        four column distributed
            column1
                header4 of "SabrixClientTestParameter"
                visible when (OneSourceVersionType.Generic)
                    blank line
                    blank line
                CallingSystemNumber
                HostSystem
                CompanyRole
                ExternalCompany
                    label is "ExternalCompanyId"
                    show up to 30 characters
                ExternalCompanyName
                    show up to 30 characters
                FilterGroupName
                    show up to 30 characters
                FilterGroupOwner
                    show up to 30 characters
                MappingGroupName
                    show up to 30 characters
                MappingGroupOwner
                    show up to 30 characters
            column2
                single column
                    header4 of "TaxConfigurationDefaultParameter"
                    visible when (TaxConfigurationRel.OneSourceVersionType.Generic)
                        TaxConfigurationRel.SabrixVersion.UserName
                            show up to 30 characters
                paragraph
                    ShowPassword
                        label is "ShowTextPassword"
                            left of field
                paragraph
                    visible when (ShowPassword)
                        TaxConfigurationRel.SabrixVersion.OneSourcePassword
                            label is "OneSourceTextPassword"
                            show up to 20 characters
                    visible when (not ShowPassword)
                        ResourcePwdText
                            label is "OneSourceTextPassword"
                            show up to 20 characters
                    DerivedTextPasswordEntered
                        label is "Entered"
                paragraph
                    TaxConfigurationRel.SabrixVersion.Password
                        label is "EncryptedPassword"
                        show up to 25 characters
                    DerivedEncryptedPasswordEntered
                        label is "Entered"
                single column
                    TaxConfigurationRel.CallingSystem
                    TaxConfigurationRel.HostSystem
                    TaxConfigurationRel.CompanyRole
                    TaxConfigurationRel.ExternalCompanyId
                        label is "CompanyId"
                        show up to 30 characters
                    TaxConfigurationRel.ExternalCompanyIdName
                        label is "CompanyIdName"
                        show up to 30 characters
                    TaxConfigurationRel.FilterGroupName
                        show up to 30 characters
                    TaxConfigurationRel.FilterGroupOwner
                        show up to 30 characters
                    TaxConfigurationRel.ProductMappingGroupName
                        label is "MappingGroupName"
                        show up to 30 characters
                    TaxConfigurationRel.ProductMappingGroupOwner
                        label is "MappingGroupOwner"
                        show up to 30 characters
            column3
                header4 of "TaxConfigurationVersionSavedParameter"
                visible when (TaxConfigurationRel.OneSourceVersionType.Generic)
                    first TaxConfigurationVersionRel.UserName
                        show up to 30 characters
                    blank line
                first TaxConfigurationVersionRel.CallingSystem
                first TaxConfigurationVersionRel.HostSystem
                first TaxConfigurationVersionRel.CompanyRole
                first TaxConfigurationVersionRel.ExternalCompanyId
                    label is "CompanyId"
                    show up to 30 characters
                first TaxConfigurationVersionRel.ExternalCompanyIdName
                    label is "CompanyIdName"
                    show up to 30 characters
                first TaxConfigurationVersionRel.FilterGroupName
                    show up to 30 characters
                first TaxConfigurationVersionRel.FilterGroupOwner
                    show up to 30 characters
                first TaxConfigurationVersionRel.ProductMappingGroupName
                    label is "MappingGroupName"
                    show up to 30 characters
                first TaxConfigurationVersionRel.ProductMappingGroupOwner
                    label is "MappingGroupOwner"
                    show up to 30 characters
                first TaxConfigurationVersionRel.ShowValidationAddress
                    label is "ValidationAddress"
            column4
                header4 of "TaxEntity"
                visible when (TaxEntityExist)
                    single column
                        LegalEntity
                    paragraph
                        LegalEntity.ThirdParty
                            label is "TaxEntity"
                        LegalEntity.OneSourceVersionType
                            no label
                    single column
                        LegalEntity.CallingSystem
                        LegalEntity.HostSystem
                        LegalEntity.CompanyRole
                        LegalEntity.ExternalCompanyId
                            label is "CompanyId"
                            show up to 30 characters
                        LegalEntity.ExternalCompanyIdName
                            label is "CompanyIdName"
                            show up to 30 characters
                        LegalEntity.FilterGroupName
                            show up to 30 characters
                        LegalEntity.FilterGroupOwner
                            show up to 30 characters
                        LegalEntity.ProductMappingGroupName
                            label is "MappingGroupName"
                            show up to 30 characters
                        LegalEntity.ProductMappingGroupOwner
                            label is "MappingGroupOwner"
                            show up to 30 characters
