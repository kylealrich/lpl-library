AssetImportContext is a Form
    title is "AssetDetail"
    Layout
        two column
            column1 of 40%
                RunGroup
            column2 of 60%
                paragraph
                    RecordInError
                        display as text
                    ErrorMessage
                        display as text
                    Asset
                        display as text
        two column
            column1 of 40%
                FinanceEnterpriseGroup
            column2 of 60%
                AssetImport
                    label is "AssetImportGroup"
                    select is AssetInterfaceRecords
        two column
            column1 of 40%
                Description
                    label is "AssetDescription"
            column2 of 60%
                visible when (AseetGuidelineClassesExists)
                    paragraph
                        AssetGuidelineClass
                            label is "GuidelineClass"
        two column
            column1 of 40%
                Company
                    select is Companies
            column2 of 60%
                AssetLease
                    label is "Company\\,Lease"
                    compact format
        paragraph
            AssetOwner
            AssetOwner.Name.FirstAndLastName
                no label
                display as text
        two column
            column1 of 40%
                AssetType
                    label is "Type"
                    compact format
                    select is AssetTypesWithinCompany
            column2 of 60%
                AssetAccountingUnitGroup
                    select is AccountingUnitGroupsWithinCompany
        two column
            column1 of 40%
                AssetAccountGroup
                    select is AccountGroupsWithinCompany
            column2 of 60%
                AssetAccountingUnit
                    label is "DefaultTransactionDimensions"
                    compact format
        two column
            column1 of 40%
                AssetProject
            column2 of 60%
                AssetSummaryGroup
                    label is "SummaryGroup"
