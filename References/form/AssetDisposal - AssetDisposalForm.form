AssetDisposalForm is a CompositeForm
    is primary
    title is "AssetDisposal_-<AssetDisposal>"
    context form is AssetDisposalContext
        background is theme color
    restrict action Unreleased.Update
    Main is a Panel
        form is AssetDisposalMain
    AdditionalDetail is a Panel
        form is AdditionalDisposalDetail
    Items is a Panel
        list is AssetItemDisposal set.ItemDisposals
    GainLossFull is a Panel
        title is "GainLoss"
        visible when (NotPurgedFull)
        list is AssetBookDisposalsRel.AssetBookDisposals
    GainLossPartial is a Panel
        title is "GainLoss"
        visible when (NotPurgedPartial)
        list is PartialAssetBookDisposalRel.AssetBookDisposalWizard
    GainLossBook is a Panel
        title is "GainLoss"
        visible when (NotPurgedBook)
        list is AssetBookDisposalFullRel.AssetBookTypeDisposals
    Transactions is a Panel
        visible when (UnReleasedFull)
        list is DisposalAssetTransactionsRel.AssetTransactionListWizard
    BookTransactions is a Panel
        visible when (UnReleasedFull)
        list is DisposalBookTransactionsRel.AssetBookTransactionsWizard
    PartialTransactions is a Panel
        title is "Transactions"
        visible when (UnReleasedPartial)
        list is PartialDisposalAssetTransactionsRel.PartialTransactionListWizard
    PartialBookTransactions is a Panel
        title is "BookTransactions"
        visible when (UnReleasedPartial)
        list is PartialDisposalBookTransactionsRel.PartialDisposalBookTransactions
    ReinstatementTransactions is a Panel
        visible when (HasUnreleasedReinstatement)
        list is ReinstatementTransactionsRel.AssetTransactionListWizard
    ReinstatementBookTransactions is a Panel
        visible when (HasUnreleasedReinstatement)
        list is ReinstatementBookTransactionsRel.AssetBookTransactionsWizard
