Main is a Form
    Layout
        three column distributed
            column1
                RequireUnitOfMeasure
                    label is "RequireUOMEntryOnTransactions"
                AllowNegativeStockOnHand
                AllocatePartials
                AllowOverrideOfLastCost
                AllowOverrideOfSourceUOM
                visible when (Company.FinanceEnterpriseGroup.FrontEndSplits)
                    FrontEndSplitRequisition
                InventoryReturnsAtAverageCost
                OrderingCost
                visible when (DisplayPreferenceCard)
                    PreferenceCardOption
            column2
                AllocationMethod
                AllocationBasis
                AllocatePriorToSubstitution
                CarryingCostPercent
                RequisitionApprovalType
                DefaultCommentType
                    label is "DefaultRequisitionCommentType"
                ApplyMaximumRequisitionQuantityTo
                PricingGroup
                    select is ActivePricingGroupsForCompanies
            column3
                LotTrackingAtFromLocationOnly
                DoNotAllowExpiredLotOnTransaction
                DaysPriorToExpiringLots
                visible when (CostingMethod.AverageCosting)
                    UseGlobalAverageCost
                BypassAccountDefaultingForReplenishmentRequisitions
                RequisitionSelfServiceLPL
                    label is "AllowRequisitionSelfServiceLPL"
                TrackStockUOMAtItemLocation
                OrderUOMRounding
        single column
            DaysOnHandNoMovementMessage
        single column
            header2 of "DefaultDecimalsForSpecialsAndServices"
            paragraph
                NumberOfDecimalsQuantity
                    label is "Quantity"
                NumberOfDecimalsCost
                    label is "Cost"
        three column distributed
            column1
                header2 of "WorkOrder"
                WorkOrderInUse
                    label is default
                        left of field
                LastWorkOrderNumber
                    display as text
            column2
                visible when (IsRecipeManagementInUse)
                    header2 of "KitchenOrder"
                    KitchenOrderInUse
                        label is default
                            left of field
                    visible when (KitchenOrderInUse)
                        LastKitchenOrderNumber
                            display as text
            column3
                visible when (Company.FinanceEnterpriseGroup.FSMAdvancedWarehouseEnabled)
                    header2 of "DistributionManagement"
                    AdvancedWarehouseInUse
                        label is default
                            left of field
        blank line
        two column
            column1
                header2 of "AvailableToAllocate"
                AvailableToAllocateAllocated
                    label is "Allocated"
                    display as text
                AvailableToAllocateInProcess
                    label is "InProcess"
                    display as text
                AvailableToAllocateBackorder
                    label is "Backorder"
                AvailableToAllocateOnOrder
                    label is "OnOrder"
                AvailableToAllocateIntransit
                    label is "Intransit"
                AvailableToAllocateWIP
                    label is "WorkInProcess"
                AvailableToAllocateInspectionHold
                    label is "InspectionHold"
            column2
                header2 of "AvailableToSell"
                AvailableToSellAllocated
                    label is "Allocated"
                    display as text
                AvailableToSellInProcess
                    label is "InProcess"
                    display as text
                AvailableToSellBackorder
                    label is "Backorder"
                AvailableToSellOnOrder
                    label is "OnOrder"
                AvailableToSellIntransit
                    label is "Intransit"
                AvailableToSellWIP
                    label is "WorkInProcess"
                AvailableToSellInspectionHold
                    label is "InspectionHold"
        blank line
        two column distributed
            column1
                header2 of "DefaultPriorityFor"
                paragraph
                    DefaultRQPriority
                        label is "Requisition"
                    DefaultWOPriority
                        label is "WorkOrder"
                    DefaultOEPriority
                        label is "CustomerOrder"
            column2
                header2 of "ItemMasterSync"
                single column
                    ItemSyncOption
        blank line
        header2 of "ProcedureInformationOutput"
        two column table
            column headers
                "PrintOn_\\Warehouse_\\Pick_\\List_\\Report"
                "PrintOn_\\Warehouse_\\Packing_\\List_\\Report"
            row headers
                "Patient_\\Name"
            column1
                paragraph
                    text of "_"
                    text of "_"
                    text of "_"
                    PrintPatientNameOnPickList
                        no label
            column2
                paragraph
                    text of "_"
                    text of "_"
                    text of "_"
                    PrintPatientNameOnPackingList
                        no label
