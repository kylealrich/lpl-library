EntityTaxCodeForm is a CompositeForm
    is primary
    title is "EntityTaxCode"
    context form is EntityTaxCodeContext
        background is theme color
    Main is a Panel
        form is Main
    VatFields is a Panel
        title is "V\\A\\TFields"
        form is VatFields
    TaxTableCodes is a Panel
        visible when (TaxCode.TaxType.TaxTableCode)
        list is TaxTableRel.TaxTableDetailList
    TaxDistributionCodeDetail is a Panel
        visible when (TaxDistributionCodeValid)
        list is TaxDistributionoCodeDetailsRel.TaxDistributionCodeDetailStyle2
