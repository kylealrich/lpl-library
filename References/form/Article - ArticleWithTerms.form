ArticleWithTerms is a CompositeForm
    is primary
    title is "Article"
    context form is ArticleContext
        background is theme color
    Article is a Panel
        form is Article
    TermsAndConditions is a Panel
        list is TermAndCondition set.TermAndConditions
    ChangeDisplayOrder is a Panel
        visible when (HasTerms)
        list is TermAndCondition set.TermAndConditionsLevelOne
