InvoiceContext is a Form
    Layout
        three column distributed
            column1
                paragraph
                    Company
                    Company.Name
                        no label
                        display as text
            column2
                RunGroup
            column3
                ErrorMessage
                    display as text
        three column distributed
            column1
                paragraph
                    Vendor
                        select is ActiveVendorsByName
                    Vendor.VendorName
                        no label
                        display as text
            column2
                OldVendor
                    label is "InterfacedVendor"
            column3
                EDINumber
                    label is "E\\D\\INumber"
        three column distributed
            column1
                paragraph
                    visible when (not InvoiceType.MatchPrepayment)
                        Invoice
                            label is "Invoice"
                    visible when (InvoiceType.MatchPrepayment)
                        Invoice
                            label is "PrepaymentNumber"
                    Suffix
                        no label
            column2
                VoucherNumber
                    label is "Voucher"
                    compact format
            column3
                CreateDetails
        visible when (IsPartiallyImported)
            three column distributed
                column1
                    blank line
                column2
                    blank line
                column3
                    button of "ViewPartiallyInterfacedInvoice"
                        link is PayablesInvoiceNavigation
                        display as link
