ApplicationAudit is an ActionForm
    action is ApplicationAudit
    Layout
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        paragraph
            text of "Or"
        paragraph
            CompanyGroup
                label is "CompanyGroup"
            CompanyGroup.Description
                no label
                display as text
        paragraph
            ProcessLevel
                select is ProcessLevels
            ProcessLevel.Name
                no label
                display as text
        paragraph
            Operator
                show up to 15 characters
            Operator.Name
                no label
                display as text
        single column
            RiskCd
                label is "RiskCode"
        single column
            AcoCurrDisplay
                label is "CurrencyDisplay"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                ApplicationAuditReport_RDG
            column2 of 25%
                ApplicationAuditReport_XPT
