InventoryIssueSummaryReport is an ActionForm
    action is InventoryIssueSummaryReport
    Layout
        header2 of "Main"
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            Location
            Location.Name
                no label
                display as text
        single column
            text of "\\or"
                bold
        paragraph
            ReportGrp
                label is "ReportGroup"
            ReportGrp.Name
                no label
                display as text
        single column
            blank line
        paragraph
            FrUpdateDate
                label is "FromUpdateDate"
            ToUpdateDate
                label is "ToUpdateDate"
            text of "\\or"
                bold
            FrTranDate
                label is "FromTransactionDate"
            ToTranDate
                label is "ToTransactionDate"
        single column
            blank line
            paragraph
                InvenMajcl
                    label is "MajorInventoryClass"
                    show up to 10 characters
                InvenMajcl.Description
                    no label
                    display as text
            paragraph
                InvenMincl
                    label is "MinorInventoryClass"
                    show up to 10 characters
                InvenMincl.Description
                    no label
                    display as text
        single column
            PrtPatChrgTotal
                label is "PrintPatientChargeTotals"
        header2 of "Items"
        ItemOption
            label is "ItemOption"
        visible when (ItemOption.Range)
            two column
                paragraph
                    FrItem
                        label is "FromItem"
                    FrItem.Description
                        no label
                        display as text
                paragraph
                    ToItem
                        label is "ToItem"
                    ToItem.Description
                        no label
                        display as text
        visible when (ItemOption.Individual)
            two column
                ItemNbr.ItemNbr[1]
                    no label
                ItemNbr.ItemNbr[2]
                    no label
            two column
                ItemNbr.ItemNbr[3]
                    no label
                ItemNbr.ItemNbr[4]
                    no label
            two column
                ItemNbr.ItemNbr[5]
                    no label
                ItemNbr.ItemNbr[6]
                    no label
            two column
                ItemNbr.ItemNbr[7]
                    no label
                ItemNbr.ItemNbr[8]
                    no label
        header2 of "OffsetAccounts"
        text of "AccountUnit"
            header4
        two column
            MmdAcctUnit.MmdAcctUnit[1]
                no label
            MmdAcctUnit.MmdAcctUnit[2]
                no label
        two column
            MmdAcctUnit.MmdAcctUnit[3]
                no label
            MmdAcctUnit.MmdAcctUnit[4]
                no label
        two column
            MmdAcctUnit.MmdAcctUnit[5]
                no label
            MmdAcctUnit.MmdAcctUnit[6]
                no label
        two column
            MmdAcctUnit.MmdAcctUnit[7]
                no label
            MmdAcctUnit.MmdAcctUnit[8]
                no label
        text of "_"
        text of "Account"
            header4
        two column
            MmdAccount.MmdAccount[1]
                no label
            MmdAccount.MmdAccount[2]
                no label
        two column
            MmdAccount.MmdAccount[3]
                no label
            MmdAccount.MmdAccount[4]
                no label
        two column
            MmdAccount.MmdAccount[5]
                no label
            MmdAccount.MmdAccount[6]
                no label
        two column
            MmdAccount.MmdAccount[7]
                no label
            MmdAccount.MmdAccount[8]
                no label
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                InventoryIssueSummaryReportReport_RDG
            column2 of 25%
                InventoryIssueSummaryReportReport_XPT
