PurgeAuditLogVendorData is a Form
    is primary
    Layout
        header2 of "SelectAuditLogRecordTypeToPurge_(SelectOnlyOne)"
        single column
            VendorAuditLogPurge
                label is "Vendor"
            VendorContactAuditLogPurge
                label is "VendorContact"
        blank line
        header2 of "Parameters"
        paragraph
            VendorGroup
            VendorGroup.Description
                no label
                display as text
        paragraph
            Vendor
                select is AllByVendorNumber
            Vendor.VendorName
                no label
                display as text
        single column
            visible when (VendorContactAuditLogPurge)
                VendorLocation
        visible when (VendorContactAuditLogPurge)
            VendorContact.VendorContactCode
                label is "VendorContactCode"
            VendorContact.ContactLevel
                label is "ContactLevel"
