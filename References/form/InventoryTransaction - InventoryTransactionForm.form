InventoryTransactionForm is a CompositeForm
    is primary
    title is "<DerivedInventoryTransactionTitle>"
    context form is InventoryTranscationContext
        background is theme color
    Actions
        action is Unreleased.Release
            action icon is release
            hide when invalid
        action is Unreleased.Transfer
            action icon is release
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is Unreleased.Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    Main is a Panel
        form is Main
    Lines is a Panel
        visible when (not IsIntransitReceiving)
        list is InventoryTransactionLinesRel.Lines
    IntransitReceivingLines is a Panel
        title is "Lines"
        visible when (IsIntransitReceiving)
        list is InventoryTransactionLinesRel.IntransitReceivingLines
