MobileVendorItem is a Form
    title is "VendorItem"
    Layout
        display as text
            single column
                PurchaseOrderReceiptLine.PurchaseOrderLine.Description
                    label is "VendorItemDescription"
                PurchaseOrderReceiptLine.VendorBuyUOM
                    label is "BuyU\\O\\M"
        single column
            header3 of "ManufacturerInformation"
            PurchaseOrderReceiptLine.PurchaseOrderLine.VendorItem.Manufacturer
            PurchaseOrderReceiptLine.PurchaseOrderLine.VendorItem.ManufacturerNumber
            header3 of "_"
            PurchaseOrderReceiptLine.ItemGTIN
                label is "G\\T\\I\\N"
