RunAllocationWizard is a WizardForm
    use for action CreateTrueCostRunParameter
    title is "RunAllocationWizard"
    summary form is RunAllocationSummary
    show panel navigation
        last panel navigation is TrueCostAllocationControlLink
            next button title is "Finish"
    RunAllocation is a Panel
        form is RunAllocationFormEntry
        save on next
    SourceBalanceOptionNewExpenseSnapshot is a Panel
        title is "ExpenseSnapshots"
        valid when (SourceBalanceOption.NewExpenseSnapshot)
        list is TrueCostRunParameterPeriodRel.TrueCostRunParameterPeriodList
    SourceBalanceOptionExistingExpenseSnapshot is a Panel
        title is "ExpenseSnapshots"
        valid when (SourceBalanceOption.ExistingExpenseSnapshot)
        list is TrueCostRunParameterPeriodRel.TrueCostRunParameterExistingList
