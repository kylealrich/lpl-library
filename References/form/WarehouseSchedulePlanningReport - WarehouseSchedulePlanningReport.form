WarehouseSchedulePlanningReport is an ActionForm
    action is WarehouseSchedulePlanningReport
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            Location
                select is ProductionLocations
            Location.Name
                no label
                display as text
        paragraph
            Item
            Item.Description
                no label
                display as text
        blank line
        header3 of "PeriodEndDates"
        two column
            column1
                PrevDate
                    label is "Previous"
                Per2Date
                    label is "Period2"
                FutureDate
                    label is "Future"
            column2
                Per1Date
                    label is "Period1"
                Per3Date
                    label is "Period3"
        blank line
        two column
            DateUsed
            DocDmndView
                label is "DocumentDemandView"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                WarehouseSchedulePlanningReportReport_RDG
            column2 of 25%
                WarehouseSchedulePlanningReportReport_XPT
