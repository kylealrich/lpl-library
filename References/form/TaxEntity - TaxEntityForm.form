TaxEntityForm is a CompositeForm
    is primary
    title is "TaxEntity"
    context form is EntityContext
        background is theme color
    Main is a Panel
        form is Main
    Intrastat is a Panel
        title is "V\\A\\T/Intrastat"
        form is Intrastat
    EntityTaxCodeDetail is a Panel
        visible when (UseTaxCodeAccounts)
        list is TaxCodeRel.TaxCodeWithDetailList
    VertexConfiguration is a Panel
        visible when (ThirdParty.VertexOSeries)
        list is VertexConfigRel.VertexConfigList
    LookupTaxAreaId is a Panel
        title is "LookupTaxAreaID"
        visible when (ThirdParty.VertexOSeries)
        form is LookupTaxCodeForm
    JurisdictionResults is a Panel
        visible when (JurisResultsExists)
        list is JurisdictionResultsRel.InvoiceResponseShortList
    MultipleTaxAreaIDs is a Panel
        title is "MultipleTaxAreaI\\Ds"
        visible when (MultipleResultsExist)
        list is VertexTaxAreaIdResultsRel.TaxAreaResultList3
    TaxLog is a Panel
        visible when (LogFileRecordsExist)
        list is TaxLogHeaderRel.TaxLogHeaderList
    TaxTransactions is a Panel
        title is "TaxTransactions"
        visible when (TaxTransactionsExist)
        list is EntityTaxCodeTransactionsRel.TaxTransactionList
    SabrixForm is a Panel
        title is "OneSourceSetup"
        visible when (OneSourceOriginal)
        form is SabrixSetupForm
    OneSourceGenericForm is a Panel
        title is "OneSourceSetup"
        visible when (OneSourceGeneric)
        form is OneSourceGenericSetupForm
    VertexOptions is a Panel
        title is "VertexOptions"
        visible when (ThirdParty.VertexOSeries)
        form is VertexOptionsForm
    TaxDistributionCodeDetail is a Panel
        title is "TaxDistributionCode:<TaxDistributionCode>"
        visible when (TaxDistributionCodeValid)
        list is TaxDistributionoCodeDetailsRel.TaxDistributionCodeDetailStyle2
    OneSourceTaxAttributes is a Panel
        visible when (OneSourceTaxAttributesExist)
        list is TaxAttributesRel.OneSourceTaxAttributeRel.primary
    VertexTaxAttributes is a Panel
        visible when (VertexVersion9)
        list is TaxAttributesRel.VertexFlexibleFieldsRel.VertexFlexibleFieldsList
    JournalDetailPurgeReports is a Panel
        visible when (JournalDetailPurgeReportRelExist)
        list is JournalDetailPurgeReportRel.JournalDetailPurgeReportList
