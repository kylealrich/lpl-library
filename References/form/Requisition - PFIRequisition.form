PFIRequisition is a CompositeForm
    title is "Requisition<Requisition>"
    context form is PFIRequisitionContext
        background is theme color
    restrict action Unreleased.UpdateRequisitionLineFillOrKill
    RequisitionLinesList is a Panel
        title is "Lines"
        list is RequisitionLinesRel.PfiViewOnlyRequisitionLines
    Comments is a Panel
        visible when (CommentsExist)
        list is RequisitionCommentRel.PfiCommentsCardList
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
