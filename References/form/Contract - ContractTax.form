ContractTax is a CompositeForm
    context form is ContractContext
        background is theme color
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    HeaderLocations is a Panel
        visible when (HasHeaderTaxLocations)
        list is ContractHeaderLocationForTaxRel.ContractLocationTaxList
    LineLocations is a Panel
        visible when (HasLineTaxLocations)
        list is ContractLineLocationForTaxRel.ContractLineLocationList
    Lines is a Panel
        visible when (HasLineTax)
        list is ContractLineWithTaxRel.LineTax
