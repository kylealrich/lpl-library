FileCreationFileForm is a Form
    title is "File"
    Layout
        single column
            SystemTimeStamp
                display as text
            RowsOutputted
                display as text
                display zero
            RecordsOutputted
                display as text
                display zero
            visible when (not FileCreationSetup.UseForTesting)
                single column
                    ExportFile
        visible when (Regenerated)
            header3 of "FileHasBeenRegenerated"
            single column
                RegeneratedFileRel.ExportFile.File
                    label is "NewFile"
