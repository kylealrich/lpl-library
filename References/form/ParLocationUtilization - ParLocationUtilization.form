ParLocationUtilization is an ActionForm
    action is ParLocationUtilization
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        three column
            column1
                paragraph
                    ReportGrp
                        label is "ReportGroup"
                    ReportGrp.Name
                        no label
                        display as text
            column2
                text of "\\or"
                    bold
            column3
                paragraph
                    IclLocation
                        label is "ParLocation"
                        select is ParLocationsSelect
                        show up to 10 characters
                    IclLocation.Name
                        no label
                        display as text
        single column
            ReplenishDays
        paragraph
            DateRangeFrom
                label is "FromDate"
            DateRangeTo
                label is "ToDate"
        single column
            MinimumSamples
            SortOption
            PrintOption
            UpdateOption
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                ParLocationUtilizationReport_RDG
            column2 of 25%
                ParLocationUtilizationReport_XPT
