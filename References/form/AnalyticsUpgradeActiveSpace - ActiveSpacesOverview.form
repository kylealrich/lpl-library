ActiveSpacesOverview is a Form
    title is "UpgradeActiveSpacesDirectly"
    Actions
        action is refresh
    Layout
        single column
            blank line
            header2 of "Upgrade_Active_Spaces_Directly"
            blank line
            text of "This_process_will_cause_end_user_downtime_and_render_FSM_Analytics_not_operational."
                bold
            blank line
            text of "Overview"
                bold
            blank line
            paragraph
                text of "This_option_will_bring_the_Active_Environment_down_until_the_upgrade_is_completed,_making_the_system_unavailable_to_end_users_during_the_upgrade_process._This_is_an_option_for_non-production_tenants_or_when_data_size,_customizations,_and_validation_are_small._The_process_must_reliably_complete_during_non-working_hours;_otherwise,_time_must_be_factored_in_for_restoring_from_a_backup."
            blank line
            visible when (AnalyticsUpgradeActiveSpaceRel.IsDisableButtonText)
                text of "There_is_an_ongoing_upgrade_process._A_new_one_cannot_be_started_until_the_previous_one_is_completed_or_cancelled._Click_'Show_History'_to_see_more_details."
                    bold
                    color of red
            visible when (AnalyticsUpgradeActiveSpaceRel.IsStagingProcessRunning)
                text of "There_is_an_ongoing_upgrade_process_for_Analytics_Upgrade_Staging."
                    bold
                    color of red
            visible when (AnalyticsUpgradeActiveSpaceRel.IsCopyToAnotherTenantRunning)
                text of "There_is_an_ongoing_upgrade_process_for_Analytics_Copy_To_Another_Tenant."
                    bold
                    color of red
            visible when (last PostPfiWorkunitsRel.Status.Processing)
                text of "There_is_an_ongoing_process_for_Update_ION_Schemas."
                    bold
                    color of red
            visible when (last DataFlowToAnalyticsFLRel.Status.Processing)
                text of "There_is_an_ongoing_process_for_DataFlow_To_Analytics_Full_Load."
                    bold
                    color of red
            visible when (last DataFlowToAnalyticsILRel.Status.Processing)
                text of "There_is_an_ongoing_process_for_Data_Flow_To_Analytics_Incremental_Load."
                    bold
                    color of red
            blank line
        single column
            button of "Proceed_with_this_upgrade_process"
                valid when (AnalyticsUpgradeActiveSpaceRel.IsEnableButton)
                action is Create
            blank line
            button of "ShowHistory"
                list is AnalyticsUpgradeActiveSpace.AnalyticsUpgradeActiveSpaceList
        single column
            header2 of "FSM_Analytics_Version"
        two column
            column1
                text of "Latest_Metadata_Version_(SMD)"
                text of "Active_Environment_Version_(ATC)"
            column2
                last ProductVersionSMDRel.Value
                    label is "_"
                last ProductVersionATCRel.Value
                    label is "_"
                    raise red alert when (last AnalyticsUpgradeActiveSpaceRel.IsUpgradeCompleted)
                        mouse over text is "ActiveTenantContentVersion_isLower_than_theStandardMetaData"
        single column
            visible when (last AnalyticsUpgradeActiveSpaceRel.UpdateNotRequired)
                text of "Your_Active_Tenant_Version_<last ProductVersionATCRel.Value>_is_not_supported_to_be_upgraded_using_the_Upgrade_automation_workflows._Kindly_request_for_reprovisioning_of_new_ATC_spaces_in_your_tenant_through_ServiceNow"
                    color of red
        single column
            text of "_"
            button of "RefreshProductVersion"
                action is UpdateVersion
