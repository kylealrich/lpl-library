ContractTierWizardAction is an ActionForm
    action is CreateFromWizard
    title is "CreateTier_andQualifier"
    Layout
        single column
            ContractTier
                label is "Tier"
            TierTitle
                label is "Title"
            Description
            TierMarkupDiscount
                label is "Type"
            visible when (not TierMarkupDiscount.Fixed)
                TierPricingAmtPct.TierCost
                    label is "Cost"
                TierPricingAmtPct.TierPercent
                    label is "Percent"
            text of "QualifierInformation"
                header2
            paragraph
                TransientPurchaseType
                    label is "PurchaseType"
                TransientPurchaseBy
                    label is "PurchaseBy"
            single column
                TransientPurchaseOperator
                    label is "PurchaseOperator"
            paragraph
                TransientPurchaseAmtPct.PurchasePercent
                    label is "PurchasePercentOfAllPurchases"
                TransientPurchaseAmtPct.PurchaseAmount
                    label is "OrPurchaseAmount"
            three column
                TransientPurchaseQuantity
                    label is "Quantity"
                TransientPurchaseBeginRange
                    label is "BeginDateForQuantityQualifier"
                TransientPurchaseEndRange
                    label is "EndDate"
