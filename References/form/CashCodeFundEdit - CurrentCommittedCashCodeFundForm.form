CurrentCommittedCashCodeFundForm is a Form
    title is "CurrentCommittedCashCodeFunds"
    Layout
        paragraph
            CashManagementGroup
            CashManagementGroup.Description
                no label
                display as text
        paragraph
            CashCode
            CashCode.Description
                no label
                display as text
        paragraph
            AccountingEntity
            AccountingEntity.Name
                no label
                display as text
        paragraph
            visible when (CashManagementGroup.FinanceEnterpriseGroup.FundDimension.AccountingUnit)
                AccountingUnit
                AccountingUnit.Description
                    no label
                    display as text
            visible when (CashManagementGroup.FinanceEnterpriseGroup.FundDimension.Dimension1)
                FinanceDimension1
                FinanceDimension1.Description
                    no label
                    display as text
        single column
            CommittedAmount
            RemainingCommittedAmount
