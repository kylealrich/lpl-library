ProjectFundingSourceTotalsForm is a Form
    title is "Totals"
    Layout
        display as text
            six column
                column1
                    text of "Draft_Billing"
                        header3
                    TotalDraftBilledAmount
                        label is "InvoiceAmount"
                    visible when (FinanceDimension2.IsExternalGrantOrCustomer)
                        TotalDraftRetainedAmount
                            label is "InvoiceRetainage"
                    TotalDraftBilledFees
                        label is "InvoiceFees"
                    visible when (FinanceDimension2.IsExternalGrantOrCustomer)
                        TotalDraftBilledAllowance
                            label is "InvoiceAllowance"
                        TotalDraftBilledPassThrough
                            label is "InvoicePassThrough"
                column2
                    text of "Invoiced"
                        header3
                    TotalJournalizedBilledAmount
                        label is "Amount"
                    visible when (FinanceDimension2.IsExternalGrantOrCustomer)
                        TotalJournalizedRetainedAmount
                            label is "Retainage"
                    TotalJournalizedBilledFees
                        label is "Fees"
                    visible when (FinanceDimension2.IsExternalGrantOrCustomer)
                        TotalJournalizedBilledAllowance
                            label is "Allowance"
                        TotalJournalizedBilledPassThrough
                            label is "PassThrough"
                column3
                    text of "Remaining"
                        header3
                    BilledRemaining
                        label is "Balance"
                    visible when (TotalBillingAdjustments entered)
                        TotalBillingAdjustments
                            label is "Adjustments"
                column4
                    text of "Draft_Revenue"
                        header3
                    TotalDraftRecognizedAmount
                        label is "RecognizedAmount"
                    visible when (FinanceDimension2.IsExternalGrantOrCustomer)
                        TotalDraftRecognizedAllowance
                            label is "RecognizedAllowance"
                        TotalDraftRecognizedPassThrough
                            label is "RecognizedPassThrough"
                        text of "Retainer"
                            header3
                        TotalRetainerAmount
                            label is "Amount"
                    visible when (FinanceDimension2.FundingType.IntercompanyBilling)
                        text of "Retainer"
                            header3
                        TotalRetainerAmount
                            label is "Amount"
                column5
                    text of "Recognized"
                        header3
                    TotalJournalizedRecognizedAmount
                        label is "Amount"
                    visible when (FinanceDimension2.IsExternalGrantOrCustomer)
                        TotalJournalizedRecognizedAllowance
                            label is "Allowance"
                        TotalJournalizedRecognizedPassThrough
                            label is "PassThrough"
                        text of "Applied"
                            header3
                        TotalAppliedRetainerAmount
                            label is "Amount"
                    visible when (FinanceDimension2.FundingType.IntercompanyBilling)
                        text of "Applied"
                            header3
                        TotalAppliedRetainerAmount
                            label is "Amount"
                column6
                    text of "Remaining"
                        header3
                    RevenueRemaining
                        label is "Balance"
                    visible when (FinanceDimension2.IsExternalGrantOrCustomer)
                        BlankField
                            no label
                            display as text
                        BlankField
                            no label
                            display as text
                        BlankField
                            no label
                            display as text
                        text of "Remaining"
                            header3
                        TotalRemainingRetainerAmount
                            label is "Balance"
                    visible when (FinanceDimension2.FundingType.IntercompanyBilling)
                        text of "Remaining"
                            header3
                        TotalRemainingRetainerAmount
                            label is "Balance"
            header3 of "Billing_andRevenueForecast"
            paragraph
                BillingForecast
                BillingForecastDate
                    label is "AsOf"
                visible when (ProjectContract.RevenueRecognition.Separate)
                    RevenueForecast
                    RevenueForecastDate
                        label is "AsOf"
