ParametersForm is a Form
    title is "Parameters"
    Layout
        header2 of "Main"
        display as text
            paragraph
                CashCode
                CashCode.Description
                    no label
                    display as text
            single column
                paragraph
                    CashCodeGroup
                        label is "CashCodeGroup"
                    CashCodeGroup.Description
                        no label
                        display as text
                paragraph
                    BankTransactionCode
                    BankTransactionCode.Description
                        no label
                        display as text
        header2 of "Options"
        display as text
            paragraph
                UseSystemDate
                EnterDate
                Days
                ReleaseDateRange.Begin
                    label is "CalculatedDateRange"
                text of "_-"
                ReleaseDateRange.End
                    no label
            visible when (EventCodeValid)
                single column
                    GeneralLedgerSourceCode
                        label is "EventCode"
            visible when (PrenoteValid)
                single column
                    PrenoteStatus
                        label is "PrenotificationStatus"
            visible when (TransNbrRangeValid)
                paragraph
                    TransNbrRangeUsingLike.FromTransNbr
                        label is "TransactionNumber"
                    text of "_-"
                    TransNbrRangeUsingLike.ToTransNbr
                        no label
                    text of "TransactionNumberRangeInAlphaSortOrder"
                        color of blue
            visible when (TransferTypeValid)
                single column
                    TransferType
