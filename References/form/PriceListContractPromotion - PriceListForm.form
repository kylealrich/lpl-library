PriceListForm is a Form
    Layout
        paragraph
            Currency
            Currency.Description
                no label
                display as text
        paragraph
            InventoryLocation
                label is "Location"
            InventoryLocation.Name
                no label
                display as text
        paragraph
            BaseName
                label is "Base"
                select is PriceBaseList
            BaseName.Description
                no label
                display as text
        paragraph
            Percent
                label is "DefaultPercent"
        paragraph
            PriceDiscountCode
                label is "DiscountCode"
                select is DiscountCodes
            PriceDiscountCode.PriceDiscountCodeAccountArray.PriceDiscountCodeAccountGroup.InvoiceDescription
                no label
                display as text
        paragraph
            Miscellaneous
                label is "BrokenCaseCharge"
                compact format
            Miscellaneous.Description
                no label
                display as text
