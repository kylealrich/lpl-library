RebalanceFinanceDimension1ZonesForm is an ActionForm
    action is RebalanceFinanceDimension1Zones
    title is "<FinanceEnterpriseGroup.FinanceDimension1Label>ZoneRebalance"
    Layout
        single column
            PrmFinanceEnterpriseGroup
                label is "FinanceEnterpriseGroup"
            blank line
            PrmYearOption
                label is "YearOption"
            visible when (PrmYearOption.SpecificBeginYear)
                PrmGeneralLedgerCloseYear
                    label is "BeginFiscalYear"
            visible when (PrmYearOption.SpecificBeginYear)
                PrmCalendarPeriod
                    label is "BeginCalendarPeriod"
                    select is GeneralLedgerCalendarPeriodEndDateList
