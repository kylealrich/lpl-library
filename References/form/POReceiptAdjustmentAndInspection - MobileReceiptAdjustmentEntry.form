MobileReceiptAdjustmentEntry is a CompositeForm
    title is "AdjustmentEntry"
    context form is MobileReceiptAdjustmentEntryContext
    restrict action Unreleased.Delete
    Actions
        action is save
            label is "_"
            hide when invalid
        navigation is UnreleasedBuyerMessages
            hide when invalid
    Main is a Panel
        form is MobileMainReceiptAdjustment
    Lines is a Panel
        template is POReceiptAdjLineRels
            business class is POReceiptAdjustmentAndInspectionLine
                list is MobileReceiptAdjustmentLines
    Receiver is a Panel
        form is MobilePOReceiver
    Comments is a Panel
        title is "Comments<CommentCount>"
        list is POReceiptAdjustmentAndInspectionCommentRel.MobileReceiptAdjustmentCommentList
