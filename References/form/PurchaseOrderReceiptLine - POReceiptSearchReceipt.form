POReceiptSearchReceipt is a Form
    Layout
        display as text
            three column distributed
                paragraph
                    PurchaseOrderReceipt
                        label is "Receipt"
                    PurchaseOrderLine.LineNumber
                        label is "Line"
                OriginalUnitCost
                    label is "ReceivedUnitCost"
                paragraph
                    ExtendedAmount
                    PurchaseOrder.Currency
            three column distributed
                paragraph
                    Item
                    PurchaseOrderLine.Description
                        label is "Description"
                PurchaseOrderLine.EarlyDeliveryDate
                Status
            three column distributed
                paragraph
                    ItemType
                    Bin
                    ReceivedUOM
                        label is "UOM"
                paragraph
                    ShipToLocation
                        label is "Location"
                    CancelBackorder
                paragraph
                    MatchSequence
                    ReferenceNumber
