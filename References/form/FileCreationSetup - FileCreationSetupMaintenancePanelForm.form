FileCreationSetupMaintenancePanelForm is a Form
    title is "_"
    Layout
        single column
            text of "Records"
                header3
        two column distributed
            column1
                button of "<PreviewRecordCount>"
                    link is PreviewRecordsNav
                    label is "NotProcessed"
                    display as link
            column2
                button of "<ProcessedRecordCount>"
                    link is ProcessedRecordsNav
                    label is "Processed"
                    display as link
        single column
            OldestRecord
                label is "Oldest"
        single column
            text of "ScheduledJobs"
                header3
        visible when (ApplicationBusinessClass)
            paragraph
                button of "<NumberOfScheduledJobsGeneratePreviewRecords>"
                    link is AsyncGeneratePreviewRecordsNavigation
                    label is "GeneratePreviewRecord:"
                    display as link
