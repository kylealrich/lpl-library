IC598Sort_Translation is a Form
    is primary
    Layout
        paragraph
            Base_BusinessClass.UniqueID
                display as text
        single column
            IsoLocale
                compact format
                raise yellow alert when (VerifyTranslations)
                    mouse over text is untranslatable:"The base data has been changed but this translation has not been verified"
        single column
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.BinGroupDescription
                label is untranslatable:"Bin Group Description"
            BinGroupDescription
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.Desc
                label is untranslatable:"Desc"
            Desc
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.Description2
                label is untranslatable:"Description 2"
            Description2
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.VenItemDesc
                label is untranslatable:"Ven Item Desc"
            VenItemDesc
                label is untranslatable:"Translated"
