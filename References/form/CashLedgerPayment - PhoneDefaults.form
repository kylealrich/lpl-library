PhoneDefaults is a Form
    title is "Defaults"
    Layout
        two column distributed
            column1
                paragraph
                    PayablesProcessGroup
                        label is "ProcessGroup"
                    PayablesProcessGroup.Description
                        no label
                        display as text
                paragraph
                    GeneralLedgerSourceCode
                        label is "EventCode"
                        show up to 10 characters
                    GeneralLedgerSourceCode.Description
                        no label
                        display as text
                paragraph
                    PayablesData.PayablesIncomeCode
                        label is "IncomeCode"
                        select is IncomeCodes
                    PayablesData.PayablesIncomeCode.Description
                        no label
                        display as text
                paragraph
                    TaxID
                        label is "TaxID"
                    TaxIDType
                        label is "TaxIDType"
                paragraph
                    ReportableIncomeAmount
                        label is "ReportableAmount"
                blank line
            column2
                header3 of "Global"
                paragraph
                    ForeignEconomicRegulatoryCode
                visible when (FercTypeEntered)
                    display as text
                        ForeignEconomicRegulatoryCode.FercType
                        paragraph
                            DerivedPaymentType
                                label is "PaymentType"
                            DerivedPaymentTypeDesc
                                no label
