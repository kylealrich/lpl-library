Contact is a Form
    Layout
        two column distributed
            column1
                visible when (not HasLocation)
                    VendorSearchName
                        label is "SearchName"
                ContactName
            column2
                visible when (not HasLocation)
                    LegalName
        two column distributed
            column1
                PhoneNumber
                MobileNumber
                FaxNumber
            column2
                TwitterID
                SocialNetworkID2
                SocialNetworkID3
                SocialNetworkID4
                SocialNetworkID5
        two column distributed
            column1
                TelexNumber
                    label is "Telex"
                VATRegistrationCountry
                    label is "V\\A\\TRegistrationCountry/\\Jurisdiction"
                    display as required when (VATRegistrationCountryRequired)
            column2
                VendorAccount
                    label is "AccountNumber"
                VATRegistrationNumber
                    label is "V\\A\\TRegistrationNumber"
                    when value changed
                        refresh VATRegistrationCountryRequired
        visible when (VendorGroup.BusinessGroup.UseHSNSACCode)
            paragraph
                TaxRegistrationType
        two column distributed
            column1
                EmailAddress
                    label is "EmailAddress"
                visible when (not HasLocation)
                    PayablesGLNNumber
                        label is "PayablesG\\L\\N"
            column2
                URLAddress
                visible when (HasLocation)
                    PayablesGLNNumber
                        label is "PayablesVendorLocationG\\L\\N"
        single column
            button of "VendorContacts"
                list is VendorContactImportRel.VendorConversionContacts
                display as link
