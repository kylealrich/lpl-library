ReceivableGLDistributionForm is a Form
    title is "ReceivableDistribution"
    Layout
        display as text
            single column
                ReceivableCompanyDataGroup.TransactionCompany
                    label is "TransactionCompany"
            three column
                ReceivableCompanyGroup.GlCompany
                    label is "GlobalLedgerCompany"
                GeneralLedgerAccount
                    label is "GlobalLedgerAccount"
                Status
            three column
                GeneralLedgerDate
                    label is "GlobalLedgerDate"
                CreateDate
                ProgramSequence
        single column
            ReceivableCompanyDataGroup.ReceivableDistributionCategory
                label is "DistributionCategory"
        two column
            Description
            Units
        single column
            DistributionAmount.CurrencyAmount
        blank line
        display as text
            paragraph
                LastDistributionSequence
                DistributionAmount.CurrencyAmount
                TaxCode
                BaseTaxableAmount
            paragraph
                CurrencyDecimalsNumber
                ReceivableCompanyGroup.OriginCompany
                TransactionTaxableAmount
