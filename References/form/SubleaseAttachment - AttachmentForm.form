    is primary
    title is "RelatedDocument"
    Layout
        paragraph
            Company
                display as text
            Company.Name
                no label
                display as text
        paragraph
            Sublease
                display as text
        header2 of "_"
        single column
            Description
        paragraph
            visible when (HasAttachment)
                Attachment.Title
            Attachment.File
