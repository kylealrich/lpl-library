CustomerOrderLineSalesRepresentative is a Form
    restrict action CreateCustomerOrderReturnLine
    Layout
        paragraph
            CustomerOrderLine
                label is "Line"
                display as text
        paragraph
            Item
                display as text
        paragraph
            ItemDescription
                display as text
        paragraph
            Territory
                no label
                display as text
            Territory.Description
                no label
                display as text
        paragraph
            SalesRepresentative
                label is "SalesRep1"
                display as text
        paragraph
            CommissionRate1
                label is "CommissionPct"
                display as text
        paragraph
            SalesRepresentative2
                label is "SalesRep2"
                display as text
        paragraph
            CommissionRate2
                label is "CommissionPct"
                display as text
        paragraph
            CommissionSplitPercentage
                label is "SplitPercent"
                display as text
