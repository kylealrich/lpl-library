RequesterProposalWizard is a WizardForm
    title is "CreateContractProposal"
    summary form is RequesterProposalWizardFinish
    show steps
    Contract is a Panel
        form is RequesterProposalWizardMain
    Attachments is a Panel
        mouse over text is "UseCreateForLineItemsToAttachLineItemDocumentOrNextToEnterInformationInLineItems"
        panel header is "UseCreateForLineItemsToAttachLineItemDocumentOrNextToEnterInformationInLineItems"
        list is ContractAttachmentRel.RequesterProposalAttachmentList
    Lines is a Panel
        title is "LineItems"
        valid when (CanUpdateProposedContract)
        mouse over text is "EnterOrAttachADocumentUsingCreateForLineItems"
        panel header is "EnterOrAttachADocumentUsingCreateForLineItems"
        list is ContractLine set.RequesterWizardLines
    Questions is a Panel
        valid when (ContractQuestionsExist)
        mouse over text is "RequiredQuestionsMustBeAnsweredBeforeTheContractCanBeSubmitted."
        panel header is "RequiredQuestionsMustBeAnsweredBeforeTheContractCanBeSubmitted."
        template is ContractQuestionsDisplayRel
            business class is ContractQuestionAnswer
                form is ContractQuestionAnswerSummary
