MobileSupplyChainWarehouseShipmentFeedback is a CompositeForm
    title is "WarehouseShipment_<WarehouseShipment>"
    context form is MobileSupplyChainShippingFeedbackByDocumentContext
        background is theme color
    restrict action IncludeWarehouseShipment
    Actions
        action is Printed.Finish
            action icon is apply
            hide when invalid
        action is Picked.Finish
            action icon is apply
            hide when invalid
        action is Packed.Finish
            action icon is apply
            hide when invalid
        action is Shipped.Release
            action icon is release
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is refresh
            label is "_"
    Main is a Panel
        form is MainForm
    Lines is a Panel
        visible when (not HasCatchWeightItem)
        list is WarehouseShipmentLinesRel.MobileSupplyChainWarehouseShipmentLines
    LinesWithCatchWeight is a Panel
        title is "Lines"
        visible when (HasCatchWeightItem)
        list is WarehouseShipmentLinesRel.WarehouseShipmentLinesWithCatchWeight
    Printers is a Panel
        visible when (IsPrintShipment)
        form is MobileSupplyChainPrintersForm
    ToteIDs is a Panel
        title is "ToteI\\Ds"
        visible when (IsValidForShipmentDistribution)
        list is MobileSupplyChainShipmentTrackingNumberRel.MobileSupplyChainShipmentTrackingNumberList
