MMainForm is a Form
    title is "MainForm"
    Layout
        display as text
            single column
                text of "CurrentBalance:<CurrentBalance>"
                text of "HighBalance:<HighestBalance>"
                text of "B\\O\\EBalance:<DraftBalance>"
        display as text
            four column table
                column headers
                    "CurrentPeriod"
                    "YearToDate"
                    "LastYear"
                    "LastYearToDate"
                row headers
                    "Purchases"
                    "Payments"
                    "NumberOfPayments"
                    "DiscountsTaken"
                    "DiscountsLost"
                    "Gain\\,Loss"
                    "IncomeWithheld"
                column1
                    DerivedCurrentPerPurchases
                        no label
                    DerivedCurrentPerPayments
                        no label
                    DerivedCurrentPerNbrOfPayments
                        no label
                    DerivedCurrentPerDiscountTaken
                        no label
                    DerivedCurrentPerDiscountLost
                        no label
                    DerivedCurrentPerGainLoss
                        no label
                    CurrentYearIncomeWithholding
                        no label
                column2
                    DerivedYTDPeriodPurchases
                        no label
                    DerivedYTDPeriodPayments
                        no label
                    DerivedYTDNbrOfPayments
                        no label
                    LastYearIncomeWithholding
                        no label
                column3
                    DerivedLastYearPurchases
                        no label
                    DerivedLastYearPayments
                        no label
                    DerivedLastYearNbrOfPayments
                        no label
                column4
                    VendorBalanceDate.LastPurchaseDate
                        no label
                    VendorBalanceDate.LastPaymentDate
                        no label
