IDMAdditionalAttributesHeaderForm is a CompositeForm
    is primary
    title is "IDMAdditionalAttributes"
    context form is Main
        background is theme color
    Actions
        action is ImportToIDM
            action icon is release
            label is "ImportToIDM"
        action is NotImported.Create
            action icon is create
            label is "_"
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    Lines is a Panel
        visible when (IDMAdditionalAttributesHeaderExists)
        list is IDMAdditionalAttributesLinesRel.Lines
