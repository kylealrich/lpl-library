MobileShipment is a Form
    title is "Shipment"
    Layout
        single column
            paragraph
                VendorReturnShippingMethod
                    label is "ShipVia"
                LoadingPort
                    label is "Port"
            ShipmentBillOfLading
                label is "BillOfLading"
            paragraph
                ShipmentDate
                    label is "ShipDate"
                ShippedBy
                    display as text
            Carrier
                label is "CarrierNumber"
            ReturnStockToInventoryOnVendorReturnCancel
        header2 of "Global"
        paragraph
            NatureOfTransactionCode
                label is "NatureOfTransactionCode"
            StatisticalProcedure
        paragraph
            ShipTerm
                label is "ShipTerms"
                select is ShipTerms
            GlobalDocumentType
