More is a Form
    Layout
        three column distributed
            paragraph
                ProcessLevel
                ProcessLevel.Name
                    no label
                    display as text
            POCode
            ItemGTIN
                label is "GTIN"
        three column distributed
            MajorPurchasingClass
                show up to 10 characters
            MinorPurchasingClass
                show up to 10 characters
            LateDeliveryDate
        three column distributed
            CostOption
            InspectionRequired
                label is "Inspection"
            CertificationRequired
                label is "Certification"
        two column distributed
            header2 of "Tax"
            column1
                paragraph
                    Taxable
                    TaxCode
                    TaxCode.Description
                        no label
                        display as text
            header2 of "Global"
            column2
                GlobalLineType
                    label is "LineType"
        visible when (IsHSNSACCodeEnabled)
            paragraph
                HSNSACCode
                HSNSACCode.Description
                    no label
                    display as text
        blank line
        visible when (Dropship)
            header2 of "Dropship"
            single column
                display as text
                    Dropship
                    paragraph
                        DropshipName
                            label is "Name"
                        DropshipContact
                    DropshipPhoneNumber
                    DropshipAddress
