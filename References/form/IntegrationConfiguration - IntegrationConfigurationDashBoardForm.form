IntegrationConfigurationDashBoardForm is a CompositeForm
    title is "<IntegrationName>"
    ODS is a DashBoardPanel
        title is "ReplicationSet"
        visible when (ConfigureLakehouseOperationalDataStoreActive)
        ODSReplicationSets is a Panel
            title is "Replication"
            overview navigation is ODSReplicationSetNav
            form is ODSReplicationSet
        ODSReplicationSetCreationResults is a Panel
            title is "CreationResults"
            double width
            list is ODSDataCreationResultRel.primary
        ODSReplicationSetHistory is a Panel
            title is "RunHistory"
            double width
            list is ODSReplicationSetHistoryRel.primary
