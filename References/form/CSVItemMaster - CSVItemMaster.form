CSVItemMaster is an ActionForm
    action is CSVItemMaster
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        blank line
        paragraph
            Location
                label is "PickingLocation"
            Location.Name
                no label
                display as text
        blank line
        single column
            PatientChargeItem
                label is "RevenueCenter"
                compact format
        blank line
        single column
            IncludeInactive
                label is "IncludeInactiveItems"
        blank line
        single column
            IncludeAllItems
                label is "IncludeAllItems"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                CSVItemMasterReport_RDG
            column2 of 25%
                CSVItemMasterReport_XPT
