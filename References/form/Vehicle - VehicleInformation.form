VehicleInformation is a Form
    Layout
        paragraph
            Carrier
                select is CarrierVendorRel.AllVendorsCardList
            Carrier.VendorName
                no label
                display as text
        paragraph
            VehicleAttribute
            VehicleAttributeRel.Description
                no label
                display as text
        paragraph
            VehicleClassification
        paragraph
            VehicleIdentifier
        header1 of "VehicleLoadCapacity"
        three column distributed
            display as text
                column1
                    text of "Interior"
                    paragraph
                        DerivedVehicleInteriorLength
                            label is "Length"
                    paragraph
                        DerivedVehicleInteriorWidth
                            label is "Width"
                    paragraph
                        DerivedVehicleInteriorHeight
                            label is "Height"
                column2
                    text of "Exterior"
                    paragraph
                        DerivedVehicleExteriorLength
                            label is "Length"
                    paragraph
                        DerivedVehicleExteriorWidth
                            label is "Width"
                    paragraph
                        DerivedVehicleExteriorHeight
                            label is "Height"
                column3
                    text of "_"
                    paragraph
                        DerivedVehicleWeightCapacity
                            label is "Weight"
                    paragraph
                        DerivedVehicleVolumeCapacity
                            label is "Cubic_Volume"
