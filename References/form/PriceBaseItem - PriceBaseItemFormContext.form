PriceBaseItemFormContext is a Form
    Layout
        paragraph
            Company
                display as text
            Company.Name
                no label
                display as text
        paragraph
            InventoryLocation
                label is "Location"
                display as text
            InventoryLocation.Name
                no label
                display as text
        paragraph
            Currency
                label is "Currency"
                display as text
            Currency.Description
                no label
                display as text
        paragraph
            PriceBase
                label is "Base"
                display as text
        paragraph
            Item
                label is "Item"
            Item.Description
                no label
                display as text
        paragraph
