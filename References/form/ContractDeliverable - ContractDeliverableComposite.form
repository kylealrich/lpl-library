ContractDeliverableComposite is a CompositeForm
    is primary
    title is "ContractDeliverable"
    context form is ContractDeliverableContext
        background is theme color
    Deliverable is a Panel
        form is ContractDeliverableForm
    Comments is a Panel
        list is ContractDeliverableComment set.ContractDeliverableComments
    Contacts is a Panel
        list is ContractDeliverableContact set.ContractDeliverableContacts
    Emails is a Panel
        visible when (Contract.NotContractTemplate)
        list is DeliverableNotificationEmailsRel.ContractDeliverableNotificationEmails
