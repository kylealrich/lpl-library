PhoneInbasketHistoryForm is a Form
    title is "ActionDetails"
    Layout
        single column
            PfiActivity.ActivityID
            PfiActivity.ActStatus
        single column
            ActionTaken
                display as text
            PfiActivity.StartDate
            PfiActivity.EndDate
        single column
            ProLogicType
                display as text
            ProLogicVal1
                display as text
            visible when (not ProLogicType.AnyOneUser)
                ProLogicVal2
                    display as text
        single column
            visible when (not TimeOutType.NotSet)
                TimeOutType
                    display as text
            visible when (not TimeOutType.NotSet)
                TimeOutHour
                    display as text
            visible when (not TimeOutType.NotSet)
                TimeoutAction
                    display as text
