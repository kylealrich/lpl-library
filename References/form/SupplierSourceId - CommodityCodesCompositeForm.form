CommodityCodesCompositeForm is a CompositeForm
    title is "EditCommodityCodes"
    CommodityCodes is a Panel
        visible when (not SupplierGroup.CommodityRegistrationLevel.Level1)
        mouse over text is "<SupplierGroup.CommodityCodesText>"
        panel header is "<SupplierGroup.CommodityCodesText>"
        list is SupplierCommodityCode set.SupplierCommodityCode
            helper list is CommodityCodes.CommodityCodeHelper
                action is AttachToContact
    CommodityCodesNoTree is a Panel
        title is "CommodityCodes"
        visible when (SupplierGroup.CommodityRegistrationLevel.Level1)
        mouse over text is "<SupplierGroup.CommodityCodesText>"
        panel header is "<SupplierGroup.CommodityCodesText>"
        list is SupplierCommodityCode set.SupplierCommodityCode
            helper list is CommodityCodes.CommodityCodeHelperNoTree
                action is AttachToContact
