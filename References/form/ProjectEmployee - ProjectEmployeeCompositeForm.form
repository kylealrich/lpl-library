ProjectEmployeeCompositeForm is a CompositeForm
    is primary
    title is "<DerivedFormTitle>"
    context form is ProjectEmployeeAdminContext
        background is theme color
    ProjectEmployee is a Panel
        form is ProjectEmployeeForm
    ProjectAssignments is a Panel
        list is ProjectAssignmentRel.ProjectAssignmentsTalentProfile
    CurrentPeriodLaborDistribution is a Panel
        visible when (ProjectAssignmentsExist)
        list is CurrentProjectAssignmentLaborRel.ProjectLaborListTalentProfile
    ProjectPeriods is a Panel
        visible when (ProjectAssignmentsExist)
        list is OpenProjectEmployeeLaborPeriodRel.AdminProjectSchedulePeriodList
    UserFields is a Panel
        form is UserFieldsForm
