CustomerDraftForm is a Form
    is primary
    Layout
        display as text
            four column distributed
                Company
                Customer
                CustomerDraft.DraftNumber
                    label is "BillOfExchange"
                TransactionAmount
            four column distributed
                OriginalCurrency
                CurrencyMultiplicationOrDivision
                CurrencyDecimalsNumber
                CurrencyRate
            four column distributed
                ApplicationSequence
                ReceivableProcessLevel
                BankTransactionCode
            four column distributed
                RemitGroup.RemitCompany
                    label is "RemitCompany"
                RemitGroup.RemitProcessLevel
                RemitGroup.RemitType
                RemitGroup.RemitNumber
            four column distributed
                CurrencyRateToCustomerGroupCurrency
                CustomerGroupCurrencyMultiplicationOrDivision
                DraftDate
                CashCode
            four column distributed
                CashRate
                CashUpdate
                BaseAmount
                MaturityDate
            four column distributed
                AcceptDate
                AcceptPostDate
                RemitDate
                CashDate
            four column distributed
                paragraph
                    Status
                    DraftReference
                DishonorDate
                CancelDate
                ResolvedDate
            four column distributed
                BankEntity
                BankAccountNumber
                PrintFlag
                PostalCode
            four column distributed
                Resolution
                CommentCode
                Risk
                Processed
            four column distributed
                PayerReference
                PaymentBatchNumber
                AppliedAmount
                BaseAppliedAmount
            four column distributed
                BillOfExchangeAccrualCode
                ReceivableGeneralLedgerCode
                    label is "ReceivableGlobalLedgerCode"
                DishonorCode
                DishonorStatus
            header2 of "_"
            two column distributed
                paragraph
                    AdditionalRevenueAmount
                    BaseAdditionalRevenueAmount
                AdditionalRevenueAccount
                    label is "AdditionalRevenueAccount"
                    compact format
            two column distributed
                paragraph
                    BankExpenseAmount
                    BaseBankExpenseAmount
                BankExpenseAccount
                    label is "BankExpenseAccount"
                    compact format
            two column distributed
                paragraph
                    DishonorExpenseAmount
                    BaseDishonorExpenseAmount
                DishonorExpenseAccount
                    label is "DishonorExpenseAccount"
                    compact format
