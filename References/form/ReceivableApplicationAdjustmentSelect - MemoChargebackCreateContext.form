MemoChargebackCreateContext is a Form
    Layout
        four column
            column1 of 25%
                display as text
                    paragraph
                        Company.RepresentativeText
                            label is "Company"
                        visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.IsNotCompanyCurrency)
                            Company.Currency
                                no label
            column2 of 23%
                display as text
                    paragraph
                        ReceivablePaymentHeader
                            label is "Batch"
                        ReceivablePaymentHeader.Currency
                            label is "Currency"
                        ReceivablePaymentHeader.Status
                            label is "BatchStatus"
            column3 of 51%
                paragraph
                    visible when (not ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.UnreleasedOrUnassigned)
                        ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.PaymentNumber
                            label is "PaymentNumber"
                            display as text
                        visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.ReferenceNumberEntered)
                            ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.ReferenceNumber
                                label is "ReferenceNumber"
                                display as text
                        ReceivablePayment
                            label is "PaymentSequence"
                            display as text
                        visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.FinanceDimension2OrProjectContractOrProjectEntered)
                            paragraph
                                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.FinanceDimension2
                                    label is "<Company.CustomerBusinessGroup.FinanceEnterpriseGroup.FinanceDimension2Label>"
                                    display as text
                                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.ProjectContract.DisplayContract
                                    label is "<Company.CustomerBusinessGroup.FinanceEnterpriseGroup.ProjectLabel>Contract"
                                    display as text
                                visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.ProjectEntered)
                                    ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.Project
                                        label is "<Company.CustomerBusinessGroup.FinanceEnterpriseGroup.ProjectLabel>"
                                        display as text
                        visible when (not ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.FinanceDimension2OrProjectContractOrProjectEntered)
                            paragraph
                                text of "___"
                                visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.CompanyCustomerMaximumOveragePercentEntered)
                                    ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.CompanyCustomerMaximumOveragePercent
                                        label is "MaxOverage"
                                        display as text
                                visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.CompanyCustomerMaximumOverageAmountEntered)
                                    ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.CompanyCustomerMaximumOverageAmount
                                        label is "MaximumOverageAmount"
                                        display as text
                    visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.UnreleasedOrUnassigned)
                        ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.PaymentNumber
                            label is "PaymentNumber"
                            display as text
                        visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.ReferenceNumberEntered)
                            ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.ReferenceNumber
                                label is "ReferenceNumber"
                                display as text
                        ReceivablePayment
                            label is "PaymentSequence"
                            display as text
                        ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.Status
                            display as tag
                                color of gray when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.Status.Unreleased)
                                color of yellow when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.Status.ReleasedUnassigned)
                        visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.CompanyCustomerMaximumOveragePercentEntered)
                            ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.CompanyCustomerMaximumOveragePercent
                                label is "MaxOverage"
                                display as text
                        visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.CompanyCustomerMaximumOverageAmountEntered)
                            ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.CompanyCustomerMaximumOverageAmount
                                label is "MaximumOverageAmount"
                                display as text
                        visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.Prepayment.Prepayment)
                            ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.Prepayment
                                no label
            column4 of 1%
                paragraph
                    text of "_"
        four column
            display as text
                column1 of 25%
                    paragraph
                        ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.Customer.RepresentativeText
                            label is "Customer"
                        ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DerivedMessage
                            label is "_"
                            display as tag
                                color of gray
                column2 of 23%
                    paragraph
                        ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DepositDate
                            label is "DepositDate"
                        ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.GeneralLedgerDate
                            label is "GlobalLedgerDate"
                        visible when (not ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.UnreleasedOrUnassigned)
                            visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.Prepayment.Prepayment)
                                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.Prepayment
                                    no label
                column3 of 51%
                    paragraph
                        ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplaySignedTransactionAmount
                            label is "PaymentAmount"
                        visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.AppliedAmountEntered)
                            ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplaySignedAppliedAmount
                                label is "Applied"
                        visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.CreditAppliedAmountEntered)
                            ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplaySignedCreditAppliedAmount
                                label is "CreditApplied"
                        visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.HasAdjustments)
                            ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplaySignedAdjustAmountTransactionCurrency
                                label is "Adjustments"
                        visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplaySignedCashApplicationPaymentOpen entered)
                            ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplaySignedCashApplicationPaymentOpen
                                label is "Open"
                                    header4
                                    bold
                                    color of red
                                header4
                                bold
                                color of red
                        visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplaySignedCashApplicationPaymentOpen not entered)
                            ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplaySignedCashApplicationPaymentOpen
                                label is "Open"
                                    header4
                                    bold
                                    color of green
                                header4
                                bold
                                color of green
                        visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.FinanceDimension2OrProjectContractOrProjectEntered)
                            visible when (not ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.UnreleasedOrUnassigned)
                                paragraph
                                    visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.CompanyCustomerMaximumOveragePercentEntered)
                                        text of "__"
                                        ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.CompanyCustomerMaximumOveragePercent
                                            label is "MaxOverage"
                                    visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.CompanyCustomerMaximumOverageAmountEntered)
                                        text of "__"
                                        ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.CompanyCustomerMaximumOverageAmount
                                            label is "MaximumOverageAmount"
                column4 of 1%
                    paragraph
                        text of "_"
        two column
            column1 of 67%
                visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.HasPendingTransactions)
                    display as text
                        paragraph
                            paragraph
                                text of "Pending_\\Payment_\\Selections:"
                                    header4
                                    bold
                            visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.HasPendingAppliedNonCreditMemoRel)
                                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplaySignedPendingAppliedNonCreditAmount
                                    label is "Debit"
                            visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.HasPendingAppliedCreditMemoRel)
                                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplaySignedPendingAppliedCreditMemoAmount
                                    label is "Credit"
                            visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.HasPendingAdjustMemoRel)
                                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplaySignedPendingAdjustMemoAmount
                                    label is "Chargeback"
                            visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.HasPendingAdjustNonMemoRel)
                                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplaySignedPendingAdjustNonMemoAmount
                                    label is "PaymentWriteoff"
                            visible when (not ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.PendingDifferenceDisplayCurrencyEqualsZero)
                                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplaySignedPendingSelectionsDisplayCurrency
                                    label is "Net"
                                        header4
                                        bold
                                        color of red
                                    header4
                                    color of red
                            visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.PendingDifferenceDisplayCurrencyEqualsZero)
                                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplaySignedPendingSelectionsDisplayCurrency
                                    label is "Net"
                                        header4
                                        bold
                                        color of green
                                    header4
                                    color of green
                            visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.WithinCompanyCustomerMaximumOverageThreshold)
                                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplaySignedPendingDifferenceDisplayCurrency
                                    label is "Difference"
                                        header4
                                        bold
                                        color of black
                                    display as tag
                                        color of yellow
                                    raise yellow alert when (true)
                                        mouse over text is "Not_in_balance,_but_within_Company_Customer's_Payment_Overage_tolerance."
                            visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.PendingEqualsZeroAndNotWithinOverageThreshold)
                                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplaySignedPendingDifferenceDisplayCurrency
                                    label is "Difference"
                                        header4
                                        bold
                                        color of green
                                    display as tag
                                        color of green
                            visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.NotWithinOverageThresholdAndNotEqualsZero)
                                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplaySignedPendingDifferenceDisplayCurrency
                                    label is "Difference"
                                        header4
                                        bold
                                        color of red
                                    display as tag
                                        color of red
                            text of "_"
            column2 of 33%
                visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.PendingApplicationsWithAdjustmentsRelExists)
                    display as text
                        paragraph
                            text of "Pending_\\Transaction_\\Adjustments:"
                                header4
                                bold
                            visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.PendingAppliedNonCreditDiscountAmountNotEqualsZero)
                                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.PendingAppliedNonCreditDiscountAmount
                                    label is "Debit"
                            visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.PendingAppliedCreditMemoDiscountAmountNotEqualsZero)
                                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.SignedPendingAppliedCreditMemoDiscountAmount
                                    label is "Credit"
                            ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.NetPendingAppliedAdjustmentAmount
                                label is "Net"
                                    bold
        visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplayProcessSelectionsInProgress)
            three column distributed
                column1
                    visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplayProcessSelectionsInProgress)
                        text of "<ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.ProcessSelectionsInBackgroundMsg>"
                            bold
                            italics
                            color of red
                    visible when (not ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.DisplayProcessSelectionsInProgress)
                        text of "_"
        visible when (not ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.PaymentExists)
            paragraph
                Company
                ReceivablePaymentHeader
                    label is "Batch"
                ReceivablePaymentHeader.Currency
                    label is "BatchCurrency"
                    display as text
                ReceivablePayment
                    display as text
                ReceivablePaymentHeader.Status
                    label is "BatchStatus"
                    display as tag
                        color of gray when (ReceivablePaymentHeader.Status.OpenBatchUnreleased)
                        color of green
            paragraph
                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.Customer
                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.PaymentNumber
                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.TransactionAmount
                    label is "Amount"
                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.ReferenceNumber
                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.SearchName
                visible when (ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.Prepayment.Prepayment)
                    ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.Prepayment
                        no label
            paragraph
                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.FinanceDimension2
                    label is "<Company.CustomerBusinessGroup.FinanceEnterpriseGroup.FinanceDimension2Label>"
                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.ProjectContract.DisplayContract
                    label is "<Company.CustomerBusinessGroup.FinanceEnterpriseGroup.ProjectLabel>Contract"
                ReceivablePaymentMemoChargebackOrPaymentWriteoffRel.Project
                    label is "<Company.CustomerBusinessGroup.FinanceEnterpriseGroup.ProjectLabel>"
