BinForm is a Form
    title is "Bin"
    Layout
        visible when (not RecordExists)
            three column distributed
                column1 of 25%
                    paragraph
                        Company
                        Company.Name
                            no label
                            display as text
                column2 of 25%
                    paragraph
                        InventoryLocation
                            show up to 5 characters
                        InventoryLocation.Name
                            no label
                            display as text
                column3 of 50%
                    Active
        visible when (RecordExists)
            four column distributed
                Company.DerivedCompanyDisplay
                    label is "Company"
                    display as text
                InventoryLocation.RepresentativeText
                    display as text
                Active
        four column distributed
            column1
                visible when (RecordExists)
                    Bin
                        display as text
                visible when (not RecordExists)
                    Bin
                        show up to 15 characters
                visible when (InventoryLocation.UseAsWarehouseLocation)
                    visible when (RecordExists)
                        visible when (not UseAsStorageLocation)
                            WarehouseStorageLocation
                                select is DistributionManagementWarehouseStorageLocationsSelectList
                        visible when (UseAsStorageLocation)
                            WarehouseStorageLocation
                                label is "Storage_Location"
                                display as text
                    visible when (not RecordExists)
                        WarehouseStorageLocation
                            select is DistributionManagementWarehouseStorageLocationsSelectList
            column2
                BinType
                    show up to 15 characters
                visible when (InventoryLocation.UseAsWarehouseLocation)
                    UseAsWarehouseStorageLocation
            column3
                paragraph
                    StorageCode
                paragraph
                    visible when (UseAsStorageLocation)
                        text of "<BinAsAStorageLocationMessage>"
                            bold
                            italics
                            color of blue
            column4
                GlobalLocationNumber
                    display as text
                blank line
        blank line
        header3 of "AvailableBinTypes"
        four column table
            column headers
                "BinType"
                "Description"
                "AllowsReceipt"
                "AllowsIssue"
            column1
                display as text
                    visible when (Company.HasBinType1)
                        Company.BinTypeArray.BinTypeGroup[1].BinType
