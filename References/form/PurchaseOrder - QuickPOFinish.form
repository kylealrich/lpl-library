QuickPOFinish is a Form
    title is "ViewPurchaseOrder"
    Layout
        display as text
            paragraph
                Company
                Company.Name
            single column
                PurchaseOrder
        single column
            text of "ClickTheButtonBelowToReleaseThePurchaseOrder."
            button of "Release"
                valid when (PurchaseOrderInOpenStatus)
                action is Unreleased.Release
        single column
            text of "ClickTheLinkBelowToViewThePurchaseOrder."
            button of "PurchaseOrder"
                link is ViewPO
                display as link
