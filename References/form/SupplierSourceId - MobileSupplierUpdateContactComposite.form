MobileSupplierUpdateContactComposite is a CompositeForm
    title is "ContactProfile"
    context form is MobileContactProfileContext
    Actions
        action is save
            action icon is save
            label is "_"
            hide when invalid
    Profile is a Panel
        form is MobilContactProfileForm
    Banking is a Panel
        visible when (CanDisplayBankInformation)
        form is BankInformationForm
    CommodityCodes is a Panel
        list is SupplierCommodityCode set.MobileApprovedCommodityCodeCardList
            helper list is CommodityCodes.MobileCommodityCodeHelper
                action is AttachToSupplierContact
    Certifications is a Panel
        valid when (SupplierGroup.AllowCertificationSelect)
        list is SupplierCertificationRel.MobileCertificationCardList
    ProxyNotifications is a Panel
        list is SupplierContactProxy set.MobileSupplierContactProxyList
