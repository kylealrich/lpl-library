ElectronicPaymentConversion is a Form
    is primary
    title is "CreditCardInterface"
    Layout
        two column distributed
            paragraph
                Company
                Company.Name
                    no label
                    display as text
            paragraph
                Customer
                    select is GroupSearchName
                Customer.Name
                    no label
                    display as text
        two column distributed
            paragraph
                CustomerCreditCardImport.EpType
                    label is "ElectronicPaymentType"
                CustomerCreditCardImport.EpType.Description
                    no label
                    display as text
            paragraph
                CustomerCreditCardImport.EpAccount
                    label is "ElectronicPaymentAccount"
                    show up to 16 characters
        two column distributed
            ActionCd
                label is "ActionCode"
            DefaultAccount
                label is "DefaultPayment"
        two column distributed
            EpExpDate
                label is "ExpirationDate"
            EpExpDtChg
                label is "ExpirationDateChange"
        two column distributed
            DefaultPmtCh
                label is "DefaultPaymentChange"
            CustChg
                label is "CustomerChange"
        single column
            EpCustName
                label is "Name"
            ElectronicPaymentAddress
                form is PostalAddressMinimum
