Requisition is a CompositeForm
    is primary
    title is "<DerivedFormTitle>"
    context form is RequisitionContext
        background is theme color
    restrict action Unreleased.UpdateRequisitionLineFillOrKill
    restrict action Unreleased.MobileCreate
    restrict action Unreleased.RSSLPLCreate
    restrict action Unreleased.RSSLPLDelete
    restrict action Unreleased.RSSLPLShop
    restrict action Unreleased.RSSLPLCreateUsingCopyFromRequisition
    restrict action Unreleased.RSSLPLCreateUsingCopyFromTemplate
    restrict action Unreleased.RSSLPLCopyAllLinesFromRequisition
    Actions
        action is Release
            action icon is release
            hide when invalid
        action is IssueDraft
            hide when invalid
        action is IssueFinal
            action icon is print
            hide when invalid
        action is IssueFinalIDM
            action icon is print
            hide when invalid
        action is Released.ManualApprove
            hide when invalid
        action is Released.ManualReject
            hide when invalid
        action is Released.ManualUnrelease
            hide when invalid
        action is Processed.ReleaseReturns
            hide when invalid
        action is ApproveForSupplier
            hide when invalid
        action is ReturnForSupplier
            hide when invalid
        action is RejectForSupplier
            hide when invalid
        action is Processed.QuickCreatePOForBillOnly
            label is "CreatePO"
            hide when invalid
        navigation is PurchaseOrderForCapitatedReq
            hide when invalid
        navigation is ComponentLinesWithNoSerialOrLotNav
            hide when invalid
        navigation is SwapCapitatedComponentsNav
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is refresh
            label is "_"
    Lines is a Panel
        visible when (not ForCapitatedProcedure)
        list is RequisitionLinesRel.RequisitionLines
    CapitatedLines is a MultiListPanel
        title is "ProceduresAndConstructComponents"
        visible when (CapitatedUnreleased)
        list is RequisitionLinesRel.CapitatedProcedureLineList
            helper list is ContractLinesForVendorRel.CapitatedProceduresForASupplierRequisition
                action is AddProcedureToRequisition
                title is "Procedures"
        list is RequisitionLinesRel.CapitatedComponentsRequisitionLineList
            helper list is CapitatedComponentItemsRel.ContractCapitatedComponentGroupItemsSelect
                action is AddToRequisitionConstructComponent
                title is "NoChargeComponentsOnConstruct"
        list is RequisitionLinesRel.CapitatedChargedComponentsRequisitionLineList
    ChargedCapitatedLines is a Panel
        title is "ChargedLinesNotOnConstruct"
        visible when (CapitatedUnreleased)
        list is RequisitionLinesRel.NonCapitatedComponentsRequisitionLineList
            helper list is ContractLinesForVendorRel.ContractLinesForACapitatedRequisitionSelect
                action is AddToRequisition
                title is "OtherContractItems"
            helper list is ContractLinesForVendorRel.ContractLinesForACapitatedRequisitionSelect
                action is AddWasteItemToRequisition
                title is "WasteItems"
    CapitatedLinesSubmitted is a MultiListPanel
        title is "ProceduresAndConstructLines"
        visible when (CapitatedReleased)
        list is RequisitionLinesRel.CapitatedProcedureLineList
        list is RequisitionLinesRel.CapitatedComponentsRequisitionLineList
        list is RequisitionLinesRel.CapitatedChargedComponentsRequisitionLineForSupplierList
    ChargedCapitatedLinesSubmitted is a Panel
        title is "ChargedLinesNotOnConstruct"
        visible when (CapitatedReleased)
        list is RequisitionLinesRel.NonCapitatedComponentsRequisitionLineList
    HeaderDetails is a Panel
        title is "Header"
        form is RequisitionMain
    LineDefaults is a Panel
        form is Defaults
    ProcedureInformationDefaults is a Panel
        visible when (DisplayOfProcedureInfo)
        form is ProcedureInfoDefaults
    AddOnCharges is a MultiListPanel
        list is RequisitionAddOnChargesRel.MiscellaneousAddOnCostsForARequisition
        list is RequisitionSpreadAddOnCharge set.RequisitionSpreadAddOnChargeList
    Comments is a Panel
        title is "Comments<CommentCount>"
        list is RequisitionCommentRel.RequisitionComments
    UserFields is a Panel
        form is UserFields
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
    MatrixApprovalHistory is a Panel
        visible when (DisplayMatrixApprovalHistory)
        list is ResponsibilityMatrixApprovalProcessorRel.ApprovalProcessorAuditRel.ApprovalProcessorHistoryCardHistoryCardList
    MatrixProcessIndicator is a DashBoardPanel
        title is "MatrixApprovalHistory"
        visible when (ResponsibilityMatrixApprovalProcessorRel.DisplayApprovalInformationPanel)
        show 4 columns
        Progress is a Panel
            triple width
            form is MatrixProcessIndicatorForm
        History is a Panel
            list is ResponsibilityMatrixApprovalProcessorRel.ApprovalProcessorAuditRel.ApprovalProcessorHistoryCardHistoryCardList
    Returns is a Panel
        visible when (LineReturnsAllowed)
        list is RequisitionLinesRel.RequisitionReturns
    BudgetErrors is a Panel
        visible when (HeaderBudgetErrorsExist)
        list is HeaderBudgetEditErrorRel.primary
    WarningBudgetErrors is a Panel
        visible when (DistributionBudgetErrorsExist)
        list is DistributionBudgetEditErrorRel.primary
    CreateErrors is a Panel
        visible when (LineErrorsFromHeaderExists)
        list is RequisitionLineErrorsRel.RequisitionLineErrorFromHeader
