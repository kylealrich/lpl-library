LogisticDeliveryForm is a CompositeForm
    is primary
    title is "Delivery<DerivedFormTitle>"
    called out actions only
    context form is LogisticDeliveryContextForm
        background is theme color
    Actions
        action is ConfirmDelivery
            label is "Confirm_All_Delivery"
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is refresh
            label is "_"
    ShipmentManagement is a Panel
        title is "Shipments<DerivedDeliveryLineCount>"
        list is LogisticsDeliveryLineRel.LogisticDeliveryLineList
    Comments is a Panel
        title is "Comments<DerivedCommentCount>"
        list is LogisticDeliveryCommentsRel.LogisticDeliveryCommentList
