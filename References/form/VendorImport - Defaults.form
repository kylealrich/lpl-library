Defaults is a Form
    Layout
        two column
            column1
                header2 of "Invoice"
                visible when (not HasLocation)
                    AccrualCode
                    DistributionCode
                    AuthorityCode
                HoldCode
                PaymentPriority
                    label is "Priority"
                TaxCode
                TaxUsageCode
                visible when (not HasLocation)
                    Project
                    WorkflowGroup
                DiversityCode
                    label is "DefaultDiversityCode"
                visible when (not HasLocation)
                    ForeignEconomicRegulatoryCode
                        label is "ForeignEconRegulatoryCode"
                visible when (HasLocation)
                    header2 of "Currency"
                    InvoiceCurrency
                        label is "Invoice"
                    RevalueCurrency
            column2
                header2 of "Payment"
                TermsCode
                CashCode
                BankTransactionCode
                DiscountCode
                InvoiceGroup
                SeparatePayment
                Enclosure
                visible when (not HasLocation)
                    PayImmediately
                visible when (not HasLocation)
                    header2 of "Currency"
                    BalanceCurrency
                        label is "VendorBalance"
                    InvoiceCurrency
                        label is "Invoice"
                    DefaultVendorPaymentCurrency
                    RevalueCurrency
        visible when (not HasLocation)
            header2 of "Withholding"
            paragraph
                visible when (VendorGroup.GlobalWithholding)
                    EnrolledWithholding
                    LineLevelWithholding
            paragraph
                paragraph
                    IncomeCode
                    IncomeCode.Description
                        no label
                        display as text
                ExemptFromSecondaryWithholding
                    label is "SecondaryWithholdingExempt"
                IncomeWithholding
                    label is "IncomeWithholding"
            paragraph
                SecondaryWithholdingCode1
                    label is "SecondaryWithholdingCodes"
                SecondaryWithholdingCode2
                    no label
                SecondaryWithholdingCode3
                    no label
            paragraph
                AllowWithholdingCodeOverride
                    label is "AllowOverrideOfCompanyIncomeWithholdingCode"
                visible when (AllowWithholdingCodeOverride)
                    text of "Use_\\Withholding_\\CodesPanelToDefineOverrideWithholdingCodes"
                        italics
        header2 of "Returns"
        paragraph
            ReplaceGoods
                label is "ReplaceGoods"
            ShipOrHold
                label is "ShipOrHoldGoods"
            WriteOffAmount
                label is "MinimumClaimAmount"
            VendorClaimType
            visible when (VendorClaimType.Chargeback)
                VendorClaimHoldCode
                    label is "ChargebackHoldCode"
