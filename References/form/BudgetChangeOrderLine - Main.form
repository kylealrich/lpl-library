Main is a Form
    Layout
        visible when (BudgetChangeOrder.Status.Unreleased)
            three column distributed
                column1
                    GeneralLedgerCalendarPeriod
                        label is "Period"
                        select is Scenario.MonthsInCalendarRel.GeneralLedgerCalendarPeriodList
                    Amount
                    UnitsAmount
                column2
                    text of "_"
                    DerivedRemainingBalanceAmount
                        display as text
                        raise blue alert when (ShowBudgetDataAlert)
                            mouse over text is "<BudgetDataAlertMessage>"
                    DerivedRemainingUnits
                        display as text
                column3
                    text of "_"
                    DerivedNewRemainingBalanceAmount
                        display as text
                    DerivedNewRemainingUnits
                        display as text
            header2 of "FinanceStructure"
            four column distributed
                column1
                    visible when (Scenario.HasAccountingEntity)
                        BudgetCodeBlock.AccountingEntity
                            label is FinanceEnterpriseGroup.AccountingEntityLabel
                            select is BudgetCodeBlock.AccountingEntitySelectRel.FinanceEnterpriseAccountingEntitySelectList
                    visible when (Scenario.HasPostingAccountingUnit)
                        BudgetCodeBlock.AccountingUnit
                            label is FinanceEnterpriseGroup.AccountingUnitLabel
                            select is BudgetCodeBlock.AccountingUnitSelectRel.PostingAccountingUnitsSelect
                            show up to 15 characters
                    visible when (Scenario.HasPostingAccount)
                        BudgetCodeBlock.GeneralLedgerChartAccount
                            label is FinanceEnterpriseGroup.AccountLabel
                            select is BudgetCodeBlock.AccountSelectRel.ChartAccountSelect
                            show up to 15 characters
                    visible when (Scenario.HasPostingProject)
                        BudgetCodeBlock.Project
                            label is FinanceEnterpriseGroup.ProjectLabel
                            select is BudgetCodeBlock.ProjectSelectRel.ProjectSelect
                            show up to 15 characters
                column2
                    visible when (Scenario.HasPostingDimension1)
                        BudgetCodeBlock.FinanceDimension1
                            label is FinanceEnterpriseGroup.FinanceDimension1Label
                            select is BudgetCodeBlock.Dimension1SelectRel.FinanceDimension1Select
                    visible when (Scenario.HasPostingDimension2)
                        BudgetCodeBlock.FinanceDimension2
                            label is FinanceEnterpriseGroup.FinanceDimension2Label
                            select is BudgetCodeBlock.Dimension2SelectRel.FinanceDimension2Select
                    visible when (Scenario.HasPostingDimension3)
                        BudgetCodeBlock.FinanceDimension3
                            label is FinanceEnterpriseGroup.FinanceDimension3Label
                            select is BudgetCodeBlock.Dimension3SelectRel.FinanceDimension3Select
                    visible when (Scenario.HasPostingDimension4)
                        BudgetCodeBlock.FinanceDimension4
                            label is FinanceEnterpriseGroup.FinanceDimension4Label
                            select is BudgetCodeBlock.Dimension4SelectRel.FinanceDimension4Select
                column3
                    visible when (Scenario.HasPostingDimension5)
                        BudgetCodeBlock.FinanceDimension5
                            label is FinanceEnterpriseGroup.FinanceDimension5Label
                            select is BudgetCodeBlock.Dimension5SelectRel.FinanceDimension5Select
                    visible when (Scenario.HasPostingDimension6)
                        BudgetCodeBlock.FinanceDimension6
                            label is FinanceEnterpriseGroup.FinanceDimension6Label
                            select is BudgetCodeBlock.Dimension6SelectRel.FinanceDimension6Select
                    visible when (Scenario.HasPostingDimension7)
                        BudgetCodeBlock.FinanceDimension7
                            label is FinanceEnterpriseGroup.FinanceDimension7Label
                            select is BudgetCodeBlock.Dimension7SelectRel.FinanceDimension7Select
                column4
                    visible when (Scenario.HasPostingDimension8)
                        BudgetCodeBlock.FinanceDimension8
                            label is FinanceEnterpriseGroup.FinanceDimension8Label
                            select is BudgetCodeBlock.Dimension8SelectRel.FinanceDimension8Select
                    visible when (Scenario.HasPostingDimension9)
                        BudgetCodeBlock.FinanceDimension9
                            label is FinanceEnterpriseGroup.FinanceDimension9Label
                            select is BudgetCodeBlock.Dimension9SelectRel.FinanceDimension9Select
                    visible when (Scenario.HasPostingDimension10)
                        BudgetCodeBlock.FinanceDimension10
                            label is FinanceEnterpriseGroup.FinanceDimension10Label
                            select is BudgetCodeBlock.Dimension10SelectRel.FinanceDimension10Select
        visible when (not BudgetChangeOrder.Status.Unreleased)
            display as text
                three column distributed
                visible when (not BudgetChangeOrder.Status.Released)
                    three column distributed
                        column1
                            GeneralLedgerCalendarPeriod
                                label is "Period"
                                select is Scenario.MonthsInCalendarRel.GeneralLedgerCalendarPeriodList
                            Amount
                            UnitsAmount
                        column2
                            visible when (not ShowBudgetDataAlert)
                                text of "_"
                            visible when (ShowBudgetDataAlert)
                                text of "<BudgetDataAlertMessage>"
                                    color of blue
                            DerivedRemainingBalanceAmount
                            DerivedRemainingUnits
                        column3
                            text of "_"
                            DerivedNewRemainingBalanceAmount
                            DerivedNewRemainingUnits
                visible when (BudgetChangeOrder.Status.Released)
                    three column distributed
                        GeneralLedgerCalendarPeriod
                            label is "Period"
                            select is Scenario.MonthsInCalendarRel.GeneralLedgerCalendarPeriodList
                        Amount
                        UnitsAmount
                header2 of "FinanceStructure"
                four column distributed
                    column1
                        visible when (Scenario.HasAccountingEntity)
                            BudgetCodeBlock.AccountingEntity
                                label is FinanceEnterpriseGroup.AccountingEntityLabel
                        visible when (Scenario.HasPostingAccountingUnit)
                            BudgetCodeBlock.AccountingUnit
                                label is FinanceEnterpriseGroup.AccountingUnitLabel
                        visible when (Scenario.HasPostingAccount)
                            BudgetCodeBlock.GeneralLedgerChartAccount
                                label is FinanceEnterpriseGroup.AccountLabel
                        visible when (Scenario.HasPostingProject)
                            BudgetCodeBlock.Project
                                label is FinanceEnterpriseGroup.ProjectLabel
                    column2
                        visible when (Scenario.HasPostingDimension1)
                            BudgetCodeBlock.FinanceDimension1
                                label is FinanceEnterpriseGroup.FinanceDimension1Label
                        visible when (Scenario.HasPostingDimension2)
                            BudgetCodeBlock.FinanceDimension2
                                label is FinanceEnterpriseGroup.FinanceDimension2Label
                        visible when (Scenario.HasPostingDimension3)
                            BudgetCodeBlock.FinanceDimension3
                                label is FinanceEnterpriseGroup.FinanceDimension3Label
                        visible when (Scenario.HasPostingDimension4)
                            BudgetCodeBlock.FinanceDimension4
                                label is FinanceEnterpriseGroup.FinanceDimension4Label
                    column3
                        visible when (Scenario.HasPostingDimension5)
                            BudgetCodeBlock.FinanceDimension5
                                label is FinanceEnterpriseGroup.FinanceDimension5Label
                        visible when (Scenario.HasPostingDimension6)
                            BudgetCodeBlock.FinanceDimension6
                                label is FinanceEnterpriseGroup.FinanceDimension6Label
                        visible when (Scenario.HasPostingDimension7)
                            BudgetCodeBlock.FinanceDimension7
                                label is FinanceEnterpriseGroup.FinanceDimension7Label
                    column4
                        visible when (Scenario.HasPostingDimension8)
                            BudgetCodeBlock.FinanceDimension8
                                label is FinanceEnterpriseGroup.FinanceDimension8Label
                        visible when (Scenario.HasPostingDimension9)
                            BudgetCodeBlock.FinanceDimension9
                                label is FinanceEnterpriseGroup.FinanceDimension9Label
                        visible when (Scenario.HasPostingDimension10)
                            BudgetCodeBlock.FinanceDimension10
                                label is FinanceEnterpriseGroup.FinanceDimension10Label
