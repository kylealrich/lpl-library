ContractThresholdRebateDueComposite is a CompositeForm
    title is "<mode>ContractRebateDue"
    context form is ContractThresholdRebateDueContext
        background is theme color
    RebatesReceived is a Panel
        list is ContractRebateReceived set.ContractRebatesReceived
    InvoiceLineDetail is a Panel
        visible when (ShowInvoiceLineDetails)
        list is ContractRebateDueInvoiceLinesRel.ContractRebateDueInvoiceLineList
    InvoiceDetail is a Panel
        visible when (ShowInvoiceDetails)
        list is ContractRebateDueInvoiceLinesRel.ContractRebateDueInvoiceList
    QualifierProgress is a Panel
        visible when (ShowQualifierProgress)
        list is ContractRebateDueQualifiersRel.ContractRebateDueQualifierList
