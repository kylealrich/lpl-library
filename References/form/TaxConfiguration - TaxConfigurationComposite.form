TaxConfigurationComposite is a CompositeForm
    is primary
    title is "TaxConfiguration"
    context form is TaxConfigurationContext
    show panel control on left
    OneSourceDefaultInformation is a Panel
        visible when (OneSourceGeneric)
        form is OneSourceGenericDefaultInformation
    OneSourceGenericVersions is a Panel
        visible when (OneSourceGeneric)
        list is SabrixVersionsRel.TaxConfigVersionList
    DefaultProcessingInformation is a Panel
        visible when (NonOneSourceGeneric)
        form is DefaultProcessingInformation
    OneSourceVersions is a Panel
        visible when (NonOneSourceGeneric)
        list is SabrixVersionsRel.TaxConfigVersionList
    OneSourceClient is a Panel
        visible when (IsOneSourceAndTestingModeOn)
        list is SabrixClientRel.SabrixClientList
    ConfigurationParameters is a Panel
        title is "OneSourceConfigurationParameters"
        visible when (NonOneSourceGeneric)
        form is OneSourceConfigForm
    VertexConfiguration is a Panel
        visible when (ThirdParty.VertexOSeries)
        list is VertexConfigRel.VertexConfigList
    VertexDefaultProcessingInformation is a Panel
        visible when (VertexVersion9UT)
        form is VertexDefaultProcessingInformation
    ConfigParm7 is a Panel
        title is "Vertex70ConfigurationParameters"
        visible when (VertexVersion7)
        form is VertexConfigForm
    ConfigParm8 is a Panel
        title is "Vertex80ConfigurationParameters"
        visible when (VertexVersion8)
        form is VertexConfig80Form
    UserTemplateV7 is a Panel
        title is "V7UserTemplates"
        visible when (VertexVersion7UT)
        list is Both70UserTemplatesRel.primary
    UserTemplateV9 is a Panel
        title is "UserTemplatesForVersion9"
        visible when (VertexVersion9UT)
        list is Both90UserTemplatesRel.primary
    UserTemplateV7Config is a Panel
        title is "ConfigurationParameters"
        visible when (VertexVersion7UT)
        form is Vertex70UserTemplateConfigForm
    UserTemplateV9Config is a Panel
        title is "ConfigurationParameters"
        visible when (VertexVersion9Basic)
        form is Vertex90UserTemplateConfigForm
    TaxLog is a Panel
        visible when (LogFileRecordsExist)
        list is TaxLogHeaderRel.TaxLogHeaderList
    ErrorsExist is a Panel
        title is "SetupStatusForVersion7"
        visible when (Vertex7ErrorsExist)
        form is VertexErrorForm
    Version8ErrorsExist is a Panel
        title is "SetupStatusForVersion8"
        visible when (Vertex8ErrorsExist)
        form is VertexV8ErrorForm
    Version9ErrorsExist is a Panel
        title is "SetupStatusForVersion9"
        visible when (Vertex9UTErrorsExist)
        form is VertexV9ErrorForm
    OneSourceErrors is a Panel
        visible when (OneSourceOriginalWithErrors)
        form is OneSourceErrorsForm
    JurisdictionResults is a Panel
        visible when (VertexJurisdictionResultsExist)
        list is JurisdictionResultsRel.PurgeJurisdictionResultsList
    VertexTaxAreaIdResults is a Panel
        title is "VertexTaxAreaIdResults"
        visible when (VertexVertexTaxAreaIdResultsExist)
        list is VertexTaxAreaIdResultsRel.PurgeVertexTaxAreaIdResults
    TaxEntityTransactions is a Panel
        title is "TaxTransactions"
        visible when (not ThirdParty.VertexOSeries)
        list is TaxEntityTransactionsRel.TaxEntityStatisticsList
    VatPurchaseSaleTransaction is a Panel
        visible when (VatPurchaseSaleTransactionExist)
        list is VatPurchaseSaleTransactionRel.VatPurchaseSaleTransactionList2
    Statistics is a Panel
        form is StatisticsForm
    OneSourceGenericUserTemplateForm is a Panel
        title is "OneSourceGenericConfigurations"
        visible when (OneSourceGeneric)
        form is 1SourceUserTemplateGenericForm
    WebserviceErrorsForm is a Panel
        title is "CalculateTaxErrors"
        visible when (WebserviceErrorsExist)
        list is TaxLogWebserviceErrorsRel.CalculateTaxErrorList
    VertexLookupResults is a Panel
        visible when (VertexLookupResultsExist)
        list is VertexLookupResultsRel.VertexLookupResultsSearchList
    WebserviceDebug is a Panel
        title is "<WebserviceDebugToggleMsg>"
        visible when (UsingAWebserviceTaxEngine)
        form is WebserviceDebugForm
    JournalDetailPurgeReport is a Panel
        title is "VatPurchaseSaleTransactionReport"
        visible when (JournalDetailPurgeReportExist)
        list is JournalDetailPurgeReportRel.JournalDetailPurgeReportList
