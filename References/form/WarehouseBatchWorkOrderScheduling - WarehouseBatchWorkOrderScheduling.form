WarehouseBatchWorkOrderScheduling is an ActionForm
    action is WarehouseBatchWorkOrderScheduling
    Layout
        two column distributed
            paragraph
                Company
                Company.Name
                    no label
                    display as text
            paragraph
                Location
                    select is ProductionLocations
                    show up to 12 characters
                Location.Name
                    no label
                    display as text
        header2 of "Main"
        two column distributed
            WohDateUsed
                label is "DateUsed"
            paragraph
                ThruDate
                    label is "ThroughDate"
                text of "Or"
                    bold
                BuildAddDays
                    label is "PlusNumberDays"
        two column distributed
            WohStatus
                label is "WorkOrderStatus"
            WohFirmFl
                label is "FirmAllocation"
        single column
            FormSelection
                no label
                display as horizontal radio buttons
        visible when (FormSelection.ByDate)
            two column distributed
                WohStartDate
                    label is "FromProductionDate"
                WohEndDate
                    label is "ToProductionDate"
        visible when (FormSelection.ByNumberOfDays)
            two column distributed
                ProdAddDaysFr
                    label is "FromPlusNumberDays"
                ProdAddDaysTo
                    label is "ToPlusNumberDays"
        single column
            GroupWorkOrders
                label is "WorkOrderPerDemand"
        two column distributed
            CapacityImpact
            WohWorkOrder
                label is "WorkOrder"
        header2 of "Items"
        two column table
            column1
                Item.Item[1]
                    label is "__1"
                Item.Item[3]
                    label is "__3"
                Item.Item[5]
                    label is "__5"
                Item.Item[7]
                    label is "__7"
                Item.Item[9]
                    label is "__9"
                Item.Item[11]
                    label is "11"
                Item.Item[13]
                    label is "13"
                Item.Item[15]
                    label is "15"
                Item.Item[17]
                    label is "17"
            column2
                Item.Item[2]
                    label is "__2"
                Item.Item[4]
                    label is "__4"
                Item.Item[6]
                    label is "__6"
                Item.Item[8]
                    label is "__8"
                Item.Item[10]
                    label is "10"
                Item.Item[12]
                    label is "12"
                Item.Item[14]
                    label is "14"
                Item.Item[16]
                    label is "16"
                Item.Item[18]
                    label is "18"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                WarehouseBatchWorkOrderSchedulingReport_RDG
            column2 of 25%
                WarehouseBatchWorkOrderSchedulingReport_XPT
