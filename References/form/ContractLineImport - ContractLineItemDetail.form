ContractLineItemDetail is a Form
    title is "ContractLineImportItemDetail"
    Layout
        header2 of "ItemCodes"
        single column
            Upc
            Sku
                label is "SKU"
            UccEanUpn
        visible when (not ItemMasterItem)
            header2 of "Detail"
            single column
                UNSPSCCode
                    label is "UNSPSC"
            paragraph
                MajorPurchasingClass
                    label is "PurchasingClass"
                    show up to 10 characters
                MinorPurchasingClass
                    no label
                    show up to 10 characters
            paragraph
                MajorInventoryClass
                    label is "InventoryClass"
                    show up to 10 characters
                MinorInventoryClass
                    no label
                    show up to 10 characters
            single column
                GenericName
            paragraph
                CriticalItem
                PersonalProtectiveEquipment
                Latex
            paragraph
                MajorPPEClass
                    show up to 10 characters
                MajorPPEClass.Description
                    no label
                    display as text
                MinorPPEClass
                    show up to 10 characters
                MinorPPEClass.Description
                    no label
                    display as text
            three column distributed
                Sterile
                Reprocessed
                Pharmaceutical
            three column distributed
                SingleUse
                Dressing
                Reusable
            single column
                Analytics
        visible when (ItemMasterItem)
            header2 of "Detail"
            display as text
                single column
                    UNSPSCCode
                        label is "GPOUNSPSC"
                    ItemNumber.UNSPSCCode
                        label is "ItemUNSPSC"
                        compact format
                paragraph
                    ItemNumber.MajorPurchasingClass
                        label is "PurchasingClass"
                    ItemNumber.MinorPurchasingClass
                        no label
                paragraph
                    ItemNumber.MajorInventoryClass
                        label is "InventoryClass"
                    ItemNumber.MinorInventoryClass
                        no label
                single column
                    ItemNumber.GenericName
                paragraph
                    ItemNumber.CriticalItem
                    ItemNumber.PersonalProtectiveEquipment
                    ItemNumber.Latex
                paragraph
                    ItemNumber.MajorPPEClass
                        show up to 10 characters
                    ItemNumber.MajorPPEClass.Description
                        no label
                        display as text
                    ItemNumber.MinorPPEClass
                        show up to 10 characters
                    ItemNumber.MinorPPEClass.Description
                        no label
                        display as text
                three column distributed
                    ItemNumber.Sterile
                    ItemNumber.Reprocessed
                    ItemNumber.Pharmaceutical
                three column distributed
                    ItemNumber.SingleUse
                    ItemNumber.Dressing
                    ItemNumber.Reusable
                single column
                    ItemNumber.Analytics
            visible when (CanDisplayHealthcareInformation)
                header2 of "Healthcare"
                three column distributed
                    ItemNumber.Implantable
                    ItemNumber.Drugs
                    ItemNumber.Instrument
                three column distributed
                    ItemNumber.Narcotic
                    ItemNumber.ContainsHumanTissue
                three column
                    ItemNumber.PatientChargeable
                    ItemNumber.ClinicalSystemUse
                    ItemNumber.ChargeNumber
                two column
                    visible when (HCPCSCodeEntered)
                        paragraph
                            ItemNumber.HCPCSCode.HCPCS
                            visible when (ItemNumber.HSPCSCodeSequenceEntered)
                                ItemNumber.HCPCSCode.AlphaHCPCSSequence
                                    label is "HCPCSSequence"
                    paragraph
                        ItemNumber.HCPCSCode
                            label is "HCPCSAndSequence"
                            compact format
                        ItemNumber.HCPCSCode.ShortDescription
                            no label
                paragraph
                    ItemNumber.GMDNPTCode
                        select is ActiveGMDNPTCodes
                    ItemNumber.GMDNPTCode.Name
                        no label
        visible when (CanDisplayHealthcareInformation)
            visible when (not ItemMasterItem)
                header2 of "Healthcare"
                three column distributed
                    Implantable
                    Drugs
                    Instrument
                three column distributed
                    Narcotic
                    ContainsHumanTissue
                three column
                    PatientChargeable
                    ClinicalSystemUse
                    ChargeNumber
                two column
                    visible when (HCPCSCodeEntered)
                        paragraph
                            HCPCSCode.HCPCS
                                display as text
                            visible when (ItemNumber.HSPCSCodeSequenceEntered)
                                HCPCSCode.AlphaHCPCSSequence
                                    label is "HCPCSSequence"
                                    display as text
                    paragraph
                        HCPCSCode
                            label is "HCPCSAndSequence"
                            compact format
                        HCPCSCode.ShortDescription
                            no label
                            display as text
                paragraph
                    GMDNPTCode
                        select is ActiveGMDNPTCodes
                    GMDNPTCode.Name
                        no label
                        display as text
                single column
                    NdcNbr
                        label is "NDC"
                    HibccUpn
                    LabelerIdentificationCode
        header2 of "UserFields"
        single column
            UserField1
                label is "ItemAlpha1"
            UserField2
                label is "ItemAlpha2"
            UserField3
                label is "ItemAlpha3"
            UserField4
                label is "ItemAlpha4"
            UserField5
                label is "ItemAlpha5"
            UserFieldN1
                label is "ItemNumeric1"
            UserFieldN2
                label is "ItemNumeric2"
            UserFieldN3
                label is "ItemNumeric3"
