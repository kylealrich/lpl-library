ContractLineMainNew is a Form
    title is "ContractLine"
    Layout
        visible when (not Contract.ServicePurchaseType)
            visible when (not LineExists)
                single column
                    TransientItemType
                visible when (TransientItemType.Service)
                    single column
                        ServiceCode
                    paragraph
                        ItemDescription
                            label is "ItemDescription"
                        ItemDescription2
                            no label
                    single column
                        ItemDescription3
                            label is "LongItemDescription"
                            use text area of 2 lines
                    paragraph
                        VendorItem
                        VendorItemDescription
                    paragraph
                        BaseCost
                        UOM.UnitOfMeasure
                        CostOption
                    paragraph
                        EffectiveDate
                        ExpirationDate
                    paragraph
                        Manufacturer
                            label is "ManufacturerCode"
                            compact format
                        ManufacturerNumber
                    single column
                        CommodityCode
                    single column
                        visible when (ServiceCode.Quantity)
                            LifetimeMaximumQuantity
                        Priority
                    single column
                        PayablesDistributionCode
                            select is DistributionCodesByPercentage
                    single column
                        StandardCommentCode
                            label is "CommentCode"
                visible when (TransientItemType.Special)
                    paragraph
                        ItemDescription
                            label is "ItemDescription"
                        ItemDescription2
                            no label
                    single column
                        ItemDescription3
                            label is "LongItemDescription"
                            use text area of 2 lines
                    paragraph
                        VendorItem
                        VendorItemDescription
                    paragraph
                        BaseCost
                        UOM.UnitOfMeasure
                        CostOption
                    paragraph
                        Manufacturer
                            label is "*ManufacturerCode"
                            compact format
                        ManufacturerNumber
                            label is "*ManufacturerNumber"
                    paragraph
                        EffectiveDate
                        ExpirationDate
                    single column
                        GlobalTradeItemNumber
                        CommodityCode
                    paragraph
                        LifetimeMaximumQuantity
                        Priority
                    single column
                        PayablesDistributionCode
                            select is DistributionCodesByPercentage
                    single column
                        StandardCommentCode
                            label is "CommentCode"
                visible when (TransientItemType.ItemMaster)
                    paragraph
                        ItemNumber
                        ItemNumber.Description
                            no label
                    paragraph
                        VendorItem
                        VendorItemDescription
                    paragraph
                        BaseCost
                        UOM.UnitOfMeasure
                        CostOption
                    paragraph
                        EffectiveDate
                        ExpirationDate
                    single column
                        GlobalTradeItemNumber
                    paragraph
                        LifetimeMaximumQuantity
                        Priority
                    single column
                        PayablesDistributionCode
                            select is DistributionCodesByPercentage
                    single column
                        StandardCommentCode
                            label is "CommentCode"
            visible when (LineExists)
                visible when (ServiceItemType)
                    single column
                        text of "Service"
                            bold
                        ServiceCode
                    paragraph
                        ItemDescription
                            label is "ItemDescription"
                        ItemDescription2
                            no label
                    single column
                        ItemDescription3
                            label is "LongItemDescription"
                            use text area of 2 lines
                    paragraph
                        VendorItem
                        VendorItemDescription
                    paragraph
                        BaseCost
                        UOM.UnitOfMeasure
                        CostOption
                    paragraph
                        EffectiveDate
                        ExpirationDate
                    visible when (not HasVendorItem)
                    paragraph
                        Manufacturer
                            label is "ManufacturerCode"
                            compact format
                        ManufacturerNumber
                    paragraph
                        CommodityCode
                            compact format
                            select is CommodityCodeSelect
                    paragraph
                        LifetimeMaximumQuantity
                        visible when (not Contract.StandingOrService)
                            Priority
                        visible when (Contract.StandingPurchaseType)
                            Quantity
                                label is "StandingQuantity"
                    paragraph
                        visible when (not Contract.UseHeaderCommitments)
                            single column
                                PayablesDistributionCode
                                    select is DistributionCodesForServiceContracts
                        RemainingPercentToBeDistributed
                            label is "PercentOfLineYetToBeDistributed"
                            display as text
                    single column
                        StandardCommentCode
                            label is "CommentCode"
                    paragraph
                        visible when (Contract.BlanketOrStanding)
                            BlanketStandingRequestingLocation
                    visible when (Contract.HasAPurchaseType)
                        single column
                            Contract.ContractType.PurchaseType
                                display as text
                    visible when (FromEvent)
                        paragraph
                            SourcingEventNumber
                                label is "OriginatingSourcingEventNumberAndLineNumber"
                                display as text
                            SourcingEventLineNumber
                                no label
                                display as text
                    visible when (OnEvent)
                        single column
                            text of "EventLineCreatedFromContractLine:"
                        paragraph
                            ToEventCompany
                                label is "Company"
                                display as text
                            ToEventNumber
                                label is "Event"
                                display as text
                        paragraph
                            ToEventLine
                                label is "Line"
                                display as text
                            button of "ViewEvent"
                                link is SourcingEventFromContractLine
                visible when (SpecialItem)
                    single column
                        text of "Special"
                    paragraph
                        ItemDescription
                            label is "ItemDescription"
                        ItemDescription2
                            no label
                    single column
                        ItemDescription3
                            label is "LongItemDescription"
                            use text area of 2 lines
                    paragraph
                        VendorItem
                        VendorItemDescription
                    visible when (IsDistributorWithManufacturerContract)
                        paragraph
                            MfgContractLineRel.VendorItem
                                label is "ManufacturerVendorItem"
                                display as text
                            MfgContractLineRel.VendorItemDescription
                                label is "Description"
                                display as text
                    paragraph
                        visible when (not FromManufacturerContract)
                            BaseCost
                        visible when (FromManufacturerContract)
                            BaseCost
                                display as text
                        UOM.UnitOfMeasure
                        CostOption
                    visible when (ContractLineTierCostExists)
                        single column
                            text of "SeeTierCostsTabForCosts"
                                bold
                    visible when (not Contract.ContractTemplate)
                        paragraph
                            visible when (not FromManufacturerContract)
                                EffectiveDate
                                    label is "CurrentEffectiveDate"
                                ExpirationDate
                                    label is "CurrentExpirationDate"
                            visible when (FromManufacturerContract)
                                EffectiveDate
                                    label is "CurrentEffectiveDate"
                                    display as text
                                ExpirationDate
                                    label is "CurrentExpirationDate"
                                    display as text
                    visible when (not HasVendorItem)
                        paragraph
                            Manufacturer
                                label is "ManufacturerCode"
                                compact format
                            ManufacturerNumber
                    visible when (MismatchWithManufacturerContractLine)
                        paragraph
                            MfgContractLineRel.Manufacturer
                                label is "ManufacturerContractManufacturerCode"
                                display as text
                                compact format
                            MfgContractLineRel.ManufacturerNumber
                                display as text
                    paragraph
                        visible when (not FromManufacturerContract)
                            GlobalTradeItemNumber
                                select is ActiveGTINNumbers
                        visible when (FromManufacturerContract)
                            GlobalTradeItemNumber
                                display as text
                    paragraph
                        visible when (NotFromManufacturerContractOrItemmast)
                            CommodityCode
                                compact format
                                select is CommodityCodeSelect
                        visible when (FromManufacturerContractNotItemmast)
                            CommodityCode
                                display as text
                                compact format
                            CommodityCode.Description
                                no label
                            display as text
                    paragraph
                        LifetimeMaximumQuantity
                        visible when (not Contract.StandingOrService)
                            Priority
                        visible when (Contract.StandingPurchaseType)
                            Quantity
                                label is "StandingQuantity"
                    paragraph
                        visible when (not Contract.UseHeaderCommitments)
                            PayablesDistributionCode
                                select is DistributionCodesByPercentage
                        RemainingPercentToBeDistributed
                            label is "PercentOfLineYetToBeDistributed"
                            display as text
                    single column
                        StandardCommentCode
                            label is "CommentCode"
                    paragraph
                        visible when (Contract.BlanketOrStanding)
                            BlanketStandingRequestingLocation
                    visible when (Contract.HasAPurchaseType)
                        single column
                            Contract.ContractType.PurchaseType
                                display as text
                    visible when (FromEvent)
                        paragraph
                            SourcingEventNumber
                                label is "OriginatingSourcingEventNumberAndLineNumber"
                                display as text
                            SourcingEventLineNumber
                                no label
                                display as text
                    visible when (OnEvent)
                        single column
                            text of "EventLineCreatedFromContractLine:"
                        paragraph
                            ToEventCompany
                                label is "Company"
                                display as text
                            ToEventNumber
                                label is "Event"
                                display as text
                        paragraph
                            ToEventLine
                                label is "Line"
                                display as text
                            button of "ViewEvent"
                                link is SourcingEventFromContractLine
                visible when (ItemNumberExists)
                    single column
                        text of "ItemMaster"
                            bold
                        visible when (not ItemmastAndFromManufacturerContractOrHasBeenActivated)
                            ItemNumber
                                label is "Item"
                        visible when (ItemmastAndFromManufacturerContractOrHasBeenActivated)
                            ItemNumber
                                label is "Item"
                                display as text
                    paragraph
                        ItemNumber.Description
                            label is "ItemDescription"
                            display as text
                        ItemNumber.Description2
                            no label
                            display as text
                    visible when (HasLongDescription)
                        single column
                            ItemNumber.Description3
                                label is "LongItemDescription"
                                display as text
                    paragraph
                        visible when (not HasBeenActivated)
                            VendorItem
                                select is ActiveVendorItemsForItem
                        visible when (HasBeenActivated)
                            VendorItem
                                display as text
                        visible when (not HasVendorItem)
                            VendorItemDescription
                                label is "Description"
                        visible when (HasVendorItem)
                            VendorItem.VendorItemDescription
                                label is "Description"
                                display as text
                    visible when (IsDistributorWithManufacturerContract)
                        paragraph
                            MfgContractLineRel.VendorItem
                                label is "ManufacturerVendorItem"
                                display as text
                            MfgContractLineRel.VendorItemDescription
                                label is "Description"
                                display as text
                    paragraph
                        visible when (not FromManufacturerContract)
                            BaseCost
                        visible when (FromManufacturerContract)
                            BaseCost
                                display as text
                        UOM.UnitOfMeasure
                        CostOption
                    visible when (ContractLineTierCostExists)
                        single column
                            text of "SeeTierCostsTabForCosts"
                                bold
                    visible when (not HasVendorItem)
                        paragraph
                            Manufacturer
                                label is "ManufacturerCode"
                                compact format
                            ManufacturerNumber
                    visible when (HasVendorItem)
                        paragraph
                            VendorItem.Manufacturer
                                label is "ManufacturerCode"
                                display as text
                                compact format
                            VendorItem.ManufacturerNumber
                                display as text
                        visible when (MismatchWithVendorItem)
                            single column
                                text of "EnteredManufacturerInformationDoesNotMatch_Vendor_Item"
                                    italics
                            paragraph
                                Manufacturer
                                    label is "EnteredManufacturerCode_andNumber"
                                    compact format
                                ManufacturerNumber
                    visible when (MismatchWithManufacturerContractLine)
                        paragraph
                            MfgContractLineRel.Manufacturer
                                label is "ManufacturerContractManufacturerCode"
                                display as text
                                compact format
                            MfgContractLineRel.ManufacturerNumber
                                display as text
                    paragraph
                        visible when (not FromManufacturerContract)
                            GlobalTradeItemNumber
                                select is ActiveGTINNumbers
                        visible when (FromManufacturerContract)
                            GlobalTradeItemNumber
                                display as text
                        visible when (GlobalTradeItemNumberExists)
                            GlobalTradeItemNumber.FullDescription
                                no label
                                display as text
                    paragraph
                        visible when (NotFromManufacturerContractOrItemmast)
                            CommodityCode
                                compact format
                                select is CommodityCodeSelect
                        visible when (FromManufacturerContractNotItemmast)
                            CommodityCode
                                display as text
                                compact format
                            CommodityCode.Description
                                no label
                                display as text
                        visible when (ItemmastForCommodityCode)
                            ItemNumber.CommodityCode
                                display as text
                            ItemNumber.CommodityCode.CcDescription
                                no label
                                display as text
                    paragraph
                        LifetimeMaximumQuantity
                        visible when (not Contract.StandingOrService)
                            Priority
                        visible when (Contract.StandingPurchaseType)
                            Quantity
                                label is "StandingQuantity"
                    paragraph
                        visible when (not Contract.UseHeaderCommitments)
                            PayablesDistributionCode
                                select is DistributionCodesByPercentage
                        RemainingPercentToBeDistributed
                            label is "PercentOfLineYetToBeDistributed"
                            display as text
                    single column
                        StandardCommentCode
                            label is "CommentCode"
                    paragraph
                        visible when (Contract.BlanketOrStanding)
                            BlanketStandingRequestingLocation
                    visible when (Contract.HasAPurchaseType)
                        single column
                            Contract.ContractType.PurchaseType
                                display as text
                    visible when (FromEvent)
                        paragraph
                            SourcingEventNumber
                                label is "OriginatingSourcingEventNumberAndLineNumber"
                                display as text
                            SourcingEventLineNumber
                                no label
                                display as text
                    visible when (OnEvent)
                        single column
                            text of "EventLineCreatedFromContractLine:"
                        paragraph
                            ToEventCompany
                                label is "Company"
                                display as text
                            ToEventNumber
                                label is "Event"
                                display as text
                        paragraph
                            ToEventLine
                                label is "Line"
                                display as text
                            button of "ViewEvent"
                                link is SourcingEventFromContractLine
        visible when (Contract.ServicePurchaseType)
            visible when (not LineExists)
                single column
                    ServiceCode
                paragraph
                    ItemDescription
                        label is "ItemDescription"
                    ItemDescription2
                        no label
                paragraph
                    BaseCost
                    UOM.UnitOfMeasure
                visible when (ServiceCode.Quantity)
                    single column
                        LifetimeMaximumQuantity
                paragraph
                    EffectiveDate
                    ExpirationDate
                single column
                    UNSPSCCode
                        label is "UNSPSC"
                        compact format
                single column
                    CommodityCode
                single column
                    StandardCommentCode
                        label is "CommentCode"
            visible when (LineExists)
                single column
                    ServiceCode
                paragraph
                    ItemDescription
                        label is "ItemDescription"
                    ItemDescription2
                        no label
                paragraph
                    BaseCost
                    UOM.UnitOfMeasure
                single column
                    LifetimeMaximumQuantity
                paragraph
                    LifetimeMaximumAmount
                        display as text
                    visible when (ServiceCode.Amount)
                        text of "LifetimeMaximumAmountIsEqualToBaseCostForAmountTypeService"
                            italics
                    visible when (ServiceCode.Quantity)
                        visible when (LifetimeMaximumQuantityEntered)
                            visible when (not HasOrderedQuantity)
                                text of "LifetimeMaximumAmountIsEqualToBaseCostTimesMaximumQuantityForQuantityTypeService"
                                    italics
                            visible when (HasOrderedQuantity)
                                visible when (LifetimeMaximumQuantityEntered)
                                    text of "LifetimeMaximumAmountIsEqualToBaseCostTimesMaximumQuantityForQuantityTypeService;AmountMayReflectOldBaseCostForQuantityOrdered"
                                        bold
                                        italics
                paragraph
                    EffectiveDate
                    ExpirationDate
                single column
                    CommodityCode
                single column
                    UNSPSCCode
                        label is "UNSPSC"
                        compact format
                single column
                    StandardCommentCode
                        label is "CommentCode"
