EFTExtractResultComposite is a CompositeForm
    is primary
    title is "EFTExtractResult"
    context form is EFTExtractResultContext
        background is theme color
    Exceptions is a Panel
        visible when (ExceptionsRecordExists)
        list is EFTExtractErrorResultRel.EFTExtractErrorResultList
    CompanyManualAdjustments is a Panel
        title is "ManualAdjustments"
        visible when (IsCompanyLevelAdjustments)
        list is ExtractedCompanyAdjustmentTotalDetailsRel.EFTExtractCompanyAdjustmentsList
    GroupManualAdjustments is a Panel
        title is "ManualAdjustments"
        visible when (IsGroupLevelAdjustments)
        list is ExtractedGroupAdjustmentTotalDetailsRel.EFTExtractGroupAdjustmentsList
    ExtractedCompanyLevel is a Panel
        title is "Extracted"
        visible when (IsCompanyLevel)
        list is ExtractedCompanyTotalDetailsRel.EFTExtractedCompanyList
    ExtractedGroupLevel is a Panel
        title is "Extracted"
        visible when (IsGroupLevel)
        list is ExtractedGroupTotalDetailsRel.EFTExtractedGroupList
    EFTExtractOutput is a Panel
        visible when (IsEFTExtractOutputCreated)
        list is EFTExtractOutputRel.EFTExtractOutputAndDetailList
    Totals is a Panel
        visible when (not NoRecordsToProcess)
        form is Totals
