AssetType is a CompositeForm
    is primary
    title is "AssetType"
    context form is AssetTypeContext
        background is theme color
    Main is a Panel
        form is MainForm
    ImpairmentAccounts is a Panel
        visible when (Impairment)
        form is ImpairmentAccounts
    RetirementObligationAccounts is a Panel
        visible when (AssetRetirementObligation)
        form is AssetRetirementObligationAccounts
    EventAccounts is a Panel
        visible when (AssetCompanyRel.UseEventAccounts)
        form is EventAccountForm
    ProductCategories is a Panel
        list is AssetProductCategory set.ProductCategorysList
    ClassDepreciationRates is a Panel
        list is AssetBookTypesRel.AssetBookTypesPanelList
    Allocations is a Panel
        list is AssetTypeAllocation set.AssetTypeAllocationPanelList
    Assets is a Panel
        list is AssetsByTypeRel.AllAssetsByTypeSubType
