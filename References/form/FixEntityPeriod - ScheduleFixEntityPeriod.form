ScheduleFixEntityPeriod is a Form
    is primary
    title is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>PeriodRebuild"
    Layout
        single column
            GeneralLedgerCloseConfiguration
                label is "CloseConfiguration"
            YearOption
            visible when (YearOption.Single)
                GeneralLedgerCloseYear
                    label is "Year"
                    select is GeneralLedgerCloseYearList
            visible when (YearOption.Range)
                YearRange
                    label is "YearRange"
                    compact format
