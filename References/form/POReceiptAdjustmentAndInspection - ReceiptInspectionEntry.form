ReceiptInspectionEntry is a CompositeForm
    use for action Unreleased.CreateInspection
    use for action Unreleased.AddAcceptAll
    context form is ReceivingInspectionContext
        background is theme color
    UnreleasedNonCatchweightLines is a Panel
        title is "Lines"
        visible when (UnreleasedAndNoCatchWeightLines)
        template is POReceiptLinesForInspectionRel
            business class is POReceiptAdjustmentAndInspectionLine
                list is ReceiptInspectionLines
    UnreleasedCatchweightLines is a Panel
        title is "Lines"
        visible when (UnreleasedAndHasCatchWeightLines)
        template is POReceiptLinesForInspectionRel
            business class is POReceiptAdjustmentAndInspectionLine
                list is ReceiptInspectionLinesCatchWeight
    ReleasedNonCatchweightLines is a Panel
        title is "Lines"
        visible when (ReleasedAndNoCatchWeightLines)
        list is POReceiptAdjustmentAndInspectionLine set.ReceiptInspectionLines
    ReleasedCatchweightLines is a Panel
        title is "Lines"
        visible when (ReleasedAndHasCatchWeightLines)
        list is POReceiptAdjustmentAndInspectionLine set.ReceiptInspectionLinesCatchWeight
    RejectOptions is a Panel
        form is RejectOptions
    Receiver is a Panel
        form is PO33Receiver
    Comments is a Panel
        title is "Comments<CommentCount>"
        list is POReceiptAdjustmentAndInspectionCommentRel.ReceiptInspectionCommentList
