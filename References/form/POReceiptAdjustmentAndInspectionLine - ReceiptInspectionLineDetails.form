ReceiptInspectionLineDetails is a CompositeForm
    context form is POReceivingInspectionLineDetailListContext
        background is theme color
    Actions
        action is open
            label is "_"
        action is save
            label is "_"
        action is refresh
            label is "_"
        action is search
            label is "_"
    Detail is a Panel
        form is Detail
    Source is a Panel
        visible when (POLineSourceExists)
        list is POLineSourceRels.POReceiptLineSourceList
    CatchWeight is a Panel
        visible when (ShowCatchWeightPanel)
        form is CatchWeight
    VendorItem is a Panel
        form is VendorItem
    Comments is a Panel
        title is "Comments<CommentCount>"
        list is POReceiptAdjustmentAndInspectionLineCommentRel.ReceiptInspectionLineCommentList
    AdjustmentOrRejectionDetails is a Panel
        title is "<AdjustmentOrRejectionDetails>"
        visible when (IsInventoryBinLotSerialTracked)
        list is PurchaseOrderTransactionDetailsRels.AdditionalAdjustmentOrRejectionEntrys
    UDIDetails is a Panel
        title is "UDI/DSCSADetail"
        visible when (IsUDITracked)
        list is PurchaseOrderUDIDetailRejectionRel.UDIEntrys
