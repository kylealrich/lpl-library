Main is a Form
    Layout
        visible when (not IsCreated)
            paragraph
                Company
                    select is CompaniesForFinanceEnterpriseGroup
                Company.Name
                    no label
                    display as text
                Company.Currency
                    no label
                    display as text
            paragraph
                CashCode
                CashCode.Description
                    no label
                    display as text
                CashCode.Currency
                    no label
                    display as text
        paragraph
            CashAccount
                compact format
        paragraph
            TransferClearingAccount
                compact format
        paragraph
            PaymentClearingAccount
                compact format
        paragraph
            ElectronicPaymentAccount
                compact format
