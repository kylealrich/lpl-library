RebateQualifierContractSetup is a CompositeForm
    is primary
    title is "RebateQualifierContract"
    context form is RebateQualifierContractContext
        background is theme color
    QualifierItems is a Panel
        visible when (ContractRebateQualifier.ShowSelectAllItemsOption)
        list is ContractRebateQualifierItemsRel.ContractRebateQualifierItems
            helper list is DirectBuyContractLinesRel.AllContractItemsForRebates
                action is Draft.SelectDraftItemForRebate
                    invoked.PrmContractWithRebate = Contract
                    invoked.PrmContractRebateType = ContractRebateType
                    invoked.PrmContractRebateQualifier = ContractRebateQualifier
                action is Active.SelectActiveItemForRebate
                    invoked.PrmContractWithRebate = Contract
                    invoked.PrmContractRebateType = ContractRebateType
                    invoked.PrmContractRebateQualifier = ContractRebateQualifier
                action is Amendment.SelectAmendmentItemForRebate
                    invoked.PrmContractWithRebate = Contract
                    invoked.PrmContractRebateType = ContractRebateType
                    invoked.PrmContractRebateQualifier = ContractRebateQualifier
    SpecificQualifierItems is a Panel
        visible when (ContractRebateQualifier.ShowOnlyQualifierItems)
        list is ContractRebateQualifierItemsForQualifierWithoutNonQualifyingItemsRel.ContractRebateQualifierItems
            helper list is DirectBuyContractLinesRel.AllContractItemsForRebates
                action is Draft.SelectDraftItemForRebate
                    invoked.PrmContractWithRebate = Contract
                    invoked.PrmContractRebateType = ContractRebateType
                    invoked.PrmContractRebateQualifier = ContractRebateQualifier
                action is Active.SelectActiveItemForRebate
                    invoked.PrmContractWithRebate = Contract
                    invoked.PrmContractRebateType = ContractRebateType
                    invoked.PrmContractRebateQualifier = ContractRebateQualifier
                action is Amendment.SelectAmendmentItemForRebate
                    invoked.PrmContractWithRebate = Contract
                    invoked.PrmContractRebateType = ContractRebateType
                    invoked.PrmContractRebateQualifier = ContractRebateQualifier
    RebateQualifierContract is a Panel
        form is RebateQualifierContractMain
