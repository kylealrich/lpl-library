AssetTransferForm is a CompositeForm
    is primary
    title is "AssetTransfer_-<AssetTransfer>"
    context form is AssetTransferContext
        background is theme color
    Main is a Panel
        form is AssetTransferMain
    TransferFunds is a Panel
        visible when (TransferFundsExist)
        list is AssetTransferFund set.AssetFunds
    Items is a Panel
        list is AssetItemTransfer set.AssetItemTransfers
    Books is a Panel
        list is AssetBookTransfersRel.AssetBookTransfers
