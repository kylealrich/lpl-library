FinanceDimension2CompositeUX1 is a CompositeForm
    title is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
    context form is FinanceDimension2ContextUX1
        background is theme color
    restrict action Create
    restrict action CreateFundingSource
    MainPanel is a Panel
        title is "<MainLocationTabMessage>"
        form is FinanceDimension2MainForm
    ProjectFunding is a Panel
        title is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
        visible when (IsProjectInvoicing)
        form is ProjectFundingFormUX1
    ReceivableCustomer is a Panel
        title is "ReceivableCustomer"
        visible when (IsExternalGrantOrCustomer)
        form is ReceivableCustomerFormUX1
    FundedProjectContracts is a Panel
        title is "<FinanceEnterpriseGroup.ProjectLabel>Contracts"
        visible when (IsProjectInvoicing)
        list is ProjectFundingSource(FinanceDimension2) set.ProjectFundingSources
    FundingSourceProject is a Panel
        visible when (IsProjectInvoicing)
        list is FundingSourceProject set.FundingSourceProjectList
            helper list is FundingSourcePostingProjectsRel.FundingSourceProjectSelect
                action is IncludeFundingSourceProject
                    invoked.PrmFinanceDimension2 = FinanceDimension2
    ReceivablePrepayments is a Panel
        title is "ReceivablePrepayments"
        visible when (HasReceivablePrepayments)
        list is ReceivablePrepaymentRel.ProjectFundingSourcePrepayments
    Deliverables is a Panel
        title is "Deliverables"
        visible when (IsProjectInvoicing)
        list is ProjectFundingSourceDeliverable set.DeliverableList
    GrantReporting is a Panel
        title is "GrantReporting"
        visible when (IsExternalOrIntercompanyBilling)
        form is GrantReportingFormUX1
    FederalFinancialReports is a Panel
        title is "FederalFinancialReports"
        visible when (IsExternalOrIntercompanyBilling)
        list is GrantReportingSettings set.FederalFinancialReportsListList
    Comments is a Panel
        title is "Comments"
        visible when (FinanceEnterpriseGroup.ProjectInvoicing)
        list is FinanceDimension2Comment set.FinanceDimension2CommentList
    UserFieldsPanel is a Panel
        title is "UserFields"
        form is UserFieldsForm
