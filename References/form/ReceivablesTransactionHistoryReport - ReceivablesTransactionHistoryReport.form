    action is ReceivablesTransactionHistoryReport
    Layout
        header2 of "Selection"
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        single column
            text of "Or"
        paragraph
            CompanyGroup
                label is "CompanyGroup"
            CompanyGroup.Description
                no label
                display as text
        paragraph
            FrGlDate
                label is "FromPostDate"
            ToGlDate
                label is "ToPostDate"
        single column
            Status
        paragraph
            ProcessLevel
                select is ProcessLevels
            ProcessLevel.Name
                no label
                display as text
        paragraph
            Customer
                select is GroupSearchName
            Customer.Name
                no label
                display as text
        paragraph
            CurrencyCd
                label is "Currency"
            CurrencyCd.Description
                no label
                display as text
        single column
            AcoCurrDisplay
                label is "CurrencyDisplay"
        single column
            column1
                PrintDetail
        header2 of "Transactions"
        two column distributed
            column1
                Payments
                NonArPay
                    label is "Non\\-\\A\\RPayments"
                Credits
                    label is "CreditMemos"
                Invoice
                    label is "Invoices\\,B\\O\\E"
            column2
                DebitMemo
                    label is "DebitMemos"
                RetPay
                    label is "R\\T\\MDebitMemos"
                FinChg
                    label is "FinanceChargeDebitMemos"
                ChgBack
                    label is "ChargebackDebitMemos"
        single column
            AllTrans
                label is "AllTransactions"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                ReceivablesTransactionHistoryReportReport_RDG
                ReceivablesTransactionHistoryErrorReport_RDG
            column2 of 25%
                ReceivablesTransactionHistoryReportReport_XPT
                ReceivablesTransactionHistoryErrorReport_XPT
