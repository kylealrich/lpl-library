CommitmentEncumbrancePOVarianceResult is a Form
    is primary
    title is "CommitmentsAndEncumbrancesVarianceReport"
    Layout
        paragraph
            FinanceEnterpriseGroup
                display only
            FinanceEnterpriseGroup.EnterpriseGroup.Description
                no label
                display as text
        two column distributed
            PurchasingCompany
            PurchaseOrder
            PurchaseOrderLine
            Item
            ItemType
            ServiceCode
            PurchaseOrderLifeCycleState
                label is "POStatus"
            PurchaseOrderLineLifeCycleState
                label is "POLineStatus"
            GLCommitStatus
                label is "GLCommitStatus"
            GeneralLedgerChartAccount
            visible when (AccountingUnitUsed)
                AccountingUnit
                    label is "<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>"
            visible when (ProjectUsed)
                Project
                    label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
            visible when (Dimension1Used)
                FinanceDimension1
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension1Label>"
        single column
            PODistributionAmount
                label is "PurchaseOrderLineAmount"
            GLCommitFunctionalAmount
            GLCommitFunctionalVariance
                label is "Variance"
