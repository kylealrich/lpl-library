InventoryInterfaceMaintenance is a Form
    is primary
    Layout
        three column distributed
            column1
                AssetItemInventory.RunGroup
                    label is "RunGroup"
                Company
                paragraph
                    InventoryLocation
                    InventoryLocation.Description
                        no label
                        display as text
                ItemCondition
                    label is "Condition"
            column2
                AssetItemInventory.BarCode
                    label is "BarCode"
                InventoryQuantity
                InventoryLocationDetail
                    select is ActiveAssetLocationDetails
                ItemStatus
                    label is "InventoryStatus"
            column3
                AssetItemInventory.CreationDate
                    label is "InventoryDate"
                InventorySerialNumber
                paragraph
                    InventoryLocationDetail.Level1
                    InventoryLocationDetail.Level2
                    InventoryLocationDetail.Level3
