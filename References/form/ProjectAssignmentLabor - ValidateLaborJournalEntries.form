ValidateLaborJournalEntries is an ActionForm
    action is ValidateLaborJournalEntries
    Layout
        single column
            header2 of "SelectionCriteria"
            paragraph
                EmployeeGroup
            paragraph
                PrmAccountingEntity
                    label is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
                PrmAccountingEntityGroup
                    label is "Or<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>Group"
            PrmProjectLaborSchedule
            paragraph
                PrmProjectSchedulePeriod
                    select is OpenPeriodList
                PrmProjectSchedulePeriod.DateRange.Begin
                    no label
                    display as text
                PrmProjectSchedulePeriod.DateRange.End
                    no label
                    display as text
            paragraph
                SelectionGroup
                ProcessAll
                    label is "ProcessAllTransactions"
            header2 of "DistributionCriteria"
            PrmPostingDate
            FromAccountingEntity
                label is "OptionalOverrideFrom<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
        two column
            column1
                header4 of "OptionalOverrideLaborOffsetFinanceStructure"
                OffsetCodeBlock
            column2
                header4 of "OptionalOverrideFringeOffsetFinanceStructure"
                FringeOffsetCodeBlock
