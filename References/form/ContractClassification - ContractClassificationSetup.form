ContractClassificationSetup is a CompositeForm
    is primary
    title is "ContractClassification"
    context form is ContractClassificationContext
    Approvals is a Panel
        visible when (BasicApprovals)
        form is ApprovalsWithNoRedlining
    ContractRedlineApprovals is a Panel
        visible when (ContractRedlining)
        form is ApprovalsWithContractRedlining
    DocumentRedlineApprovals is a Panel
        visible when (DocumentRedlining)
        form is ApprovalsWithDocumentRedlining
    ClassificationDetail is a Panel
        form is ContractClassification
    Subclassifications is a Panel
        list is ContractSubclassification set.ContractSubclassifications
