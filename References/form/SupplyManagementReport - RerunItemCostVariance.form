RerunItemCostVariance is an ActionForm
    action is RerunItemCostVariance
        on completion navigate to ItemCostVariance
    title is "RerunItemCostVariance"
    Layout
        Description
            label is "ReportDescription"
        display as text
            single column
                SupplyManagementReportVendorGroupField.VendorGroup
                SupplyManagementReportVendorGroupField.Vendor
                paragraph
                    FromDate
                        label is "FromPODate"
                    ThruDate
                        label is "ToPODate"
                Item
                PurchasingMajorClass
                    label is "MajorPurchasingClass"
                PurchasingMinorClass
                    label is "MinorPurchasingClass"
                ManufacturerCode
                ManufacturerDivision
                CommodityCode
                UNSPSCCode
                    label is "UNSPSCSegment"
                    compact format
                IncludeOnlyPurchasesWithVendorAgreement
                AbovePercentageVariance
                    label is "VariancePercentage"
                AboveAmountVariance
                    label is "VarianceAmount"
                IncludeDetails
                    label is "IncludeDetail"
                PrintParameters
                    label is "PrintParameters"
