EFTExtractReportOnlyContext is a Form
    propagate as a drill
    title is "EFTExtractReportOnly"
    Layout
        three column
            column1 of 25%
                visible when (Company not entered)
                    paragraph
                        CustomerGroup
                        CustomerGroup.Name
                            no label
                            display as text
                    paragraph
                        ProcessingCompany
                        ProcessingCompany.Name
                            no label
                            display as text
                        EFTBaseCurrency
                            label is "Currency"
                visible when (Company entered)
                    paragraph
                        Company
                        Company.Name
                            no label
                            display as text
                        Company.Currency
                            label is "Currency"
                paragraph
                    FromDate
                        label is "DueDateRange"
                    text of "-"
                    ToDate
                        no label
                blank line
                RunTime
                create stamp.actor
                    label is "User"
                Status
                    label is "Status"
            column2 of 30%
                text of "Parameters:"
                    bold
                    align as label
                paragraph
                    FinanceEnterpriseGroup
                    FinanceEnterpriseGroup.EnterpriseGroup.Description
                        no label
                        display as text
                ProcessDate
                    label is "ProcessingDate"
                paragraph
                    FromOverrideDate
                    text of "-"
                    ToOverrideDate
                paragraph
                    BankTransactionCode
                        label is "PaymentCode"
                    BankTransactionCode.Description
                        no label
                        display as text
                paragraph
                    TermsCode
                        label is "PaymentTerms"
                    TermsCode.Description.Desc[1]
                        no label
                        display as text
            column3 of 25%
                UpdateRecords
                    label is "Update"
                DataFileName
