VatUsageCodeContext is a Form
    Layout
        paragraph
            VatReportIDForCountry
                label is "VATReportID"
            VatReportingLine
                label is "VATReportingLine"
        visible when (UseTaxUsageCodes)
            paragraph
                TaxUsageCode
                TaxUsageCode.Description
                    display as text
        visible when (UseTaxCodes)
            paragraph
                TaxCodeSelect
                TaxCodeSelect.Description
                    display as text
        visible when (UseWithholdingCodes)
            paragraph
                WithholdingCodeSelect
                WithholdingCodeSelect.Description
                    display as text
