LeaseReportResultForm is a Form
    is primary
    Layout
        visible when (TypeOfReport.DisclosureSummary)
            LeaseAccountingStandard
                label is "AccountingStandard"
            LeaseAccountingStatement
                label is "AccountingStatement"
        Description
            label is "Description"
        PayablesCompany
            label is "Company"
        CompanyGroup
            label is "CompanyGroup"
        TypeOfReport
            label is "TypeOfReport"
        visible when (TypeOfReport.Rollforward)
            RollforwardReportType
                label is "ReportType"
        paragraph
            ReportDate
        create stamp
            label is "CreationDate"
        create stamp.actor
            label is "CreatedBy"
