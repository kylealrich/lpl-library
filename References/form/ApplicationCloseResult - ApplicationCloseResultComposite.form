ApplicationCloseResultComposite is a CompositeForm
    is primary
    propagate as a drill
    title is "ApplicationCloseResult<ApplicationCloseResult>"
    context form is ApplicationCloseResultContext
        background is theme color
    restrict action DeleteAllResults
    restrict action PerformApplicationClose
    Applications is a Panel
        list is ReceivableApplicationRel.ApplicationCloseList
    Distributions is a Panel
        list is ReceivableGLDistributionRel.ApplicationCloseList
    Chargebacks is a Panel
        list is ReceivableInvoiceDetailRel.ApplicationCloseChargebackList
    Disputes is a Panel
        list is ReceivableInvoiceDisputeRel.ApplicationCloseList
    OverApplicationMemos is a Panel
        visible when (ShowOverAppliedMemos)
        list is OverAppliedReceivableInvoiceDetailRel.OverApplicationMemos
    Exceptions is a Panel
        visible when (PrmCompany entered)
        list is ReceivableApplicationsWithExceptionsByCompanyRel.ApplicationCloseExceptionsList
    ExceptionsByCompanyGroup is a Panel
        title is "Exceptions"
        visible when (PrmCompanyGroup entered)
        list is ReceivableApplicationsWithExceptionsByCompanyGroupRel.ApplicationCloseExceptionsList
