GainLossResultCompositeForm is a CompositeForm
    is primary
    title is "GainLossResult_<ReportingBasis>_<GainLossResult>"
    GainLossResultPanel is a Panel
        title is "GainLossResult"
        form is GainLossResultContext
    GainLossResultDetailPanel is a Panel
        title is "GainLossResultError"
        visible when (DetailsExists)
        list is GainLossResultDetail set.primary
