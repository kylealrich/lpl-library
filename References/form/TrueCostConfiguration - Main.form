Main is a Form
    Layout
        two column
            column1
                paragraph
                    visible when (not TrueCostConfigurationExists)
                        TrueCostConfiguration
                            label is "True\\CostConfiguration"
                        visible when (TrueCostConfiguration entered)
                            TrueCostConfiguration.EnterpriseGroup.Description
                                no label
                                display as text
                    visible when (TrueCostConfigurationExists)
                        ConfigurationCodeAndDesciption
                            label is "True\\CostConfiguration"
                            display as text
            column2
                paragraph
                    Calendar
                    visible when (TrueCostConfigurationExists)
                        visible when (not HasCalendar)
                            text of "CalendarIsRequired"
                                color of red
                        visible when (HasCalendar)
                            Calendar.Description
                                no label
                                display as text
                            visible when (RebuildRequired)
                                visible when (TrueCostResourceUtilizationDriverEventExists)
                                    paragraph
                                        text of "Attention:_RunThe_Rebuild_Summarized_Driver_Events"
                                            bold
                                            color of red
                                        button of "RebuildSummarizedDriverEvents"
                                            action is RebuildSummarizedDriverEvents
                                            is primary
        header2 of "_"
