CreateFromBlanketContractMainForm is a Form
    title is "CreateFromBlanket"
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            BlanketOrStandingContract
                label is "BlanketContract"
                select is ActiveBlanketContracts
            BlanketOrStandingContract.Description
                no label
                display as text
        display as text
            paragraph
                visible when (not PurchaseOrderExists)
                    Company.DerivedPurchaseOrderToBeCreated
                        label is "PurchaseOrder"
                visible when (PurchaseOrderExists)
                    PurchaseOrder
                BlanketOrStandingContract.POCode
                    label is "POCode"
                BlanketOrStandingContract.LastPORelease
                    label is "CurrentRelease"
            three column
                paragraph
                    BlanketOrStandingContract.Vendor
                        label is "Vendor"
                    BlanketOrStandingContract.Vendor.VendorName
                        no label
                paragraph
                    BlanketOrStandingContract.ApPoPurchaseFrom.PurchaseFromLocation
                        label is "PurchaseFrom"
                paragraph
                    BlanketOrStandingContract.Buyer
                        label is "DefaultBuyer"
                    BlanketOrStandingContract.Buyer.Employee.PresentationNameSnapshot
                        no label
        paragraph
            Buyer
                label is "Buyer"
            Buyer.Employee.PresentationNameSnapshot
                no label
                display as text
        paragraph
            ShipToLocation
            ShipToLocation.Name
                no label
                display as text
        paragraph
            PurchaseOrderDate
            DefaultDeliveryDate
                label is "DeliveryDate"
        paragraph
            ProcessLevel
                display as required when (ProcessLevelRequired)
                initial value is BlanketOrStandingContract.ProcessLevelGroup.ProcessLevel
            ProcessLevel.Name
                no label
                display as text
        single column
            DefaultDistributionAccount
                compact format
