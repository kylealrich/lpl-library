ParametersForm is a Form
    Layout
        blank line
        blank line
        paragraph
            PickListPrintReportInventoryCompanyRel.DerivedCompanyDisplay
                label is "Company"
                left align
        paragraph
            PickListPrintReportInventoryLocationRel.DerivedInventoryLocationIDName
                label is "Location"
                left align
        blank line
        blank line
        header2 of "Main_Parameters"
        two column distributed
            column1
                DerivedExpectedShipDate
                    label is "ExpectedShipDate"
                SelectOption
                    label is "PrintOption"
                    show up to 20 characters
                visible when (PickListPrintReportInventoryLocationRel.UseAsWarehouseLocation)
                    UseZonePicking
                        label is "UseZonePicking"
                AllocateDetail
                    label is "AllocateDetails"
                IncludeCost
                    label is "IncludeCost"
                    show up to 20 characters
                PrintDeliveryData
                    label is "PrintDeliveryData"
                DerivedAlignForm
                    label is "AlignmentForms"
                DerivedReprintBatchNumber
                    label is "ReprintBatch"
                DerivedCreateFlatFile
                    label is "CreateFlatFile"
            column2
                DocumentSource
                    label is "DocumentSource"
                DerivedAllocationPurpose
                    label is "BinAllocationPurpose"
                DerivedPreferOption
                    label is "ForcePreferredBin"
                IncludeBackorders
                    label is "IncludeBackorders"
                IncludeKillDetails
                    label is "IncludeKillDetails"
                PrintRequester
                    label is "PrintRequester"
                DerivedReprintShipmentNumber
                    label is "ReprintShipment"
                DerivedFlatFileName
                    label is "FileName"
        blank line
        blank line
        header2 of "Filter"
        paragraph
            DerivedSourceDocuments
                label is "SourceDocument"
        paragraph
            DerivedRequestingLocation
                label is "RequestingLocation"
                show up to 20 characters
            DerivedRequestingReportGroup
                label is "RequestingLocationGroup"
        paragraph
            DerivedCustomer
                label is "Customer"
                show up to 20 characters
            visible when (PickListPrintReportInventoryLocationRel.UseAsWarehouseLocation)
                WarehouseWave
                    label is "Wave"
        paragraph
            DerivedItem
                label is "Item"
                show up to 20 characters
        paragraph
            DerivedProcessType
                label is "ProcessType"
                show up to 20 characters
        paragraph
            DerivedFromAllocationPriority
                label is "FromPriority"
            DerivedToAllocationPriority
                label is "ToPriority"
        paragraph
            DerivedShipTo
                label is "ShipTo"
                show up to 20 characters
        paragraph
            DerivedCarrier
                label is "Carrier"
                show up to 20 characters
        paragraph
            DerivedRoute
                label is "Route"
            AllRoute
                label is "AllRoute"
        paragraph
            SkipInsufBinDetails
                label is "SkipInsufficientBinDetails"
        blank line
        blank line
