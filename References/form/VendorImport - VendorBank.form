                        label is "VendorBankIdentification"
                paragraph
                    BankAccountValidationType
                        label is "ValidationType"
                paragraph
                    BankAccountNumber
                        label is "AccountNumber"
                single column
                    ACHPrenotification
                        label is "VendorPrenotificationStatus"
                    paragraph
                        BankAccountType
                            label is "VendorBankAccountType"
                        GlobalBankAccountType
                            label is "VendorGlobalBankAccountType"
                    BankCurrency
                single column
