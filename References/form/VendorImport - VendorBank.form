                            label is "BankCode"
                    paragraph
                        IntermediaryBankID
                            label is "BankID"
                    paragraph
                        IntermediaryAssignmentNumber
                            label is "AssignmentNumber"
        two column
            column1
                header2 of "OtherAccounts"
                GiroNumber
