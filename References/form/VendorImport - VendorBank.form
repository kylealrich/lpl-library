                    paragraph
                        PaymentForm
                    paragraph
                        ChargeCode
                    paragraph
                        DebitingSign
                    paragraph
                        BankingCode
                            label is "BankCode"
                    paragraph
                        BankID
                    paragraph
                        AssignmentNumber
            column2 of 50%
                header2 of "IntermediaryBank"
                paragraph
                    IntermediaryBankEntity
