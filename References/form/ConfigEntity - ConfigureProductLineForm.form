ConfigureProductLineForm is a CompositeForm
    use for action ConfigureProductLine
    title is "Configure<Name>"
    context form is ConfigureProductLineContextForm
    Actions
        action is refresh
    DataTranslation is a Panel
        visible when (CanGUIEditProdLine)
        panel header is "<Name>_Language_Translation_Options"
        panel text is "ConfigureYour_Product_LineToNotTranslateDataInTranslatableDataFields"
        form is DataTranslationForm
    Subjects is a Panel
        visible when (CanGUIEditProdLine)
        panel header is "<Name>_Reason_Code_Grouping_Options"
        panel text is "AddNewSubjectsToTailorActionReasonCodes"
        list is BusinessSubjectRel.ConfigConsoleList
        Special Actions
            action is AddSubject
            action is RemoveSubject
