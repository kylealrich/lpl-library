VendorImport is a CompositeForm
    is primary
    title is "VendorInterface"
    context form is VendorImportContext
        background is theme color
    Main is a Panel
        form is VendorImportMain
    Contact is a Panel
        form is Contact
    Defaults is a Panel
        form is Defaults
    Options is a Panel
        visible when (not HasLocation)
        form is Options
    VendorBank is a Panel
        form is VendorBank
    PurchasingVendor is a Panel
        form is POVendorMain
    PurchasingOptions is a Panel
        form is POVendorOptions
    Addresses is a Panel
        list is VendorAddressImportRel.VendorAddressImportList
    Contacts is a Panel
        list is VendorContactImportRel.VendorConversionContacts
    Balances is a Panel
        list is VendorBalanceImportRel.VendorBalanceImportList
    WithholdingCodes is a Panel
        visible when (AllowWithholdingCodeTab)
        list is VendorWithholdingCodeImportRel.VendorWithholdingCodeImportList
    Certifications is a Panel
        list is VendorCertificationImportsRel.VendorCertificationImportList
    ExcludedTemplates is a Panel
        list is VendorExcludeTemplateImportRel.VendorExcludeTemplateImportList
