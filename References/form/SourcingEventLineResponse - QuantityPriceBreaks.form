QuantityPriceBreaks is a Form
    title is "PriceBreaks"
    Layout
        header2 of "Line<SourcingEventLine>ResponseDetails"
        single column
            text of "ResponseUnitPrice:_1<UOM.UnitOfMeasure>@<CurrencyResponseUnitPrice>"
                italics
            visible when (SourcingEventResponse.DifferentResponseCurrency)
                text of "UnitPriceInEventCurrency:<ResponseAmount.UnitPrice>"
                    italics
        header2 of "EnterPriceBreakInformation"
        single column
            MarkupDiscount
                label is "MarkupOrDiscount"
            QuantityAmountBreakType
                label is "ByQuantityOrAmount"
            BasePercent
        blank line
        three column
            AdvancedPricing.BreakpointOccurs.QuantityPrice[1].Quantity
                label is "Quantity/Amount"
            AdvancedPricing.BreakpointOccurs.QuantityPrice[1].UnitPercent
                label is "NewPercent"
            AdvancedPricing.BreakpointOccurs.QuantityPrice[1].CurrencyUnitPrice
                label is "NewPrice"
        three column
            AdvancedPricing.BreakpointOccurs.QuantityPrice[2].Quantity
                no label
            AdvancedPricing.BreakpointOccurs.QuantityPrice[2].UnitPercent
                no label
            AdvancedPricing.BreakpointOccurs.QuantityPrice[2].CurrencyUnitPrice
                no label
        three column
            AdvancedPricing.BreakpointOccurs.QuantityPrice[3].Quantity
                no label
            AdvancedPricing.BreakpointOccurs.QuantityPrice[3].UnitPercent
                no label
            AdvancedPricing.BreakpointOccurs.QuantityPrice[3].CurrencyUnitPrice
                no label
        three column
            AdvancedPricing.BreakpointOccurs.QuantityPrice[4].Quantity
                no label
            AdvancedPricing.BreakpointOccurs.QuantityPrice[4].UnitPercent
                no label
            AdvancedPricing.BreakpointOccurs.QuantityPrice[4].CurrencyUnitPrice
                no label
        three column
            AdvancedPricing.BreakpointOccurs.QuantityPrice[5].Quantity
                no label
            AdvancedPricing.BreakpointOccurs.QuantityPrice[5].UnitPercent
                no label
            AdvancedPricing.BreakpointOccurs.QuantityPrice[5].CurrencyUnitPrice
                no label
