SubcontractorContactForm is a Form
    is primary
    title is "SubcontractorContact"
    Layout
        paragraph
            SupplierGroup
                display only
            Subcontractor
                display only
        two column
            MainContact.GivenName
                label is "FirstName"
            MainContact.FamilyName
                label is "LastName"
        paragraph
            ContactTitle
        paragraph
            TelephoneNumber
        paragraph
            FaxNumber
        paragraph
            EmailAddress
        header2 of "MailingAddress"
        single column
            LocationAddress
                form is PostalAddressMinimum
