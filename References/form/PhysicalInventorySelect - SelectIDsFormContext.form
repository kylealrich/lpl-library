SelectIDsFormContext is a Form
    Layout
        two column distributed
            column1
                paragraph
                    visible when (not RecordExists)
                        Company
                        Company.Name
                            no label
                            display as text
                    visible when (RecordExists)
                        Company.DerivedCompanyDisplay
                            label is "Company"
                            display as text
            column2
                Status
                    display as text
        paragraph
            visible when (IsNotInUseAndVarianceInProgress)
                PhysicalInventorySelect
                    label is "SelectID"
                Description
            visible when (IsSelectIdInUseOrUpdated)
                DisplaySelectID
                    label is "SelectID"
                    display as text
