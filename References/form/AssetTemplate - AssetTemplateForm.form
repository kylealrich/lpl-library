AssetTemplateForm is a CompositeForm
    is primary
    title is "AssetTemplate_-_<AssetTemplate>"
    context form is AssetTemplateContext
        background is theme color
    AssetOptions is a Panel
        form is AssetOptions
    Books is a Panel
        list is AssetTemplateBook set.AssetTemplateBooks
    InvoiceInterfaceRecords is a Panel
        list is InterfacingAssetsRel.PayablesInvoiceAssetInterface
    ProjectInterfaceRecords is a Panel
        list is InterfacingAssetsRel.ProjectAssetInterface
    AssetInterfaceRecords is a Panel
        list is InterfacingAssetsRel.ManuallyCreatedAssetInterfaceRecords
