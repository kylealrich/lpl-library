ReceiptAdjustmentLineDetails is a CompositeForm
    context form is ReceiptAdjustmentLineDetailsContext
        background is theme color
    Detail is a Panel
        form is PO33Detail
    Source is a Panel
        visible when (POLineSourceExists)
        list is POLineSourceRels.POReceiptLineSourceList
    CatchWeight is a Panel
        visible when (ShowCatchWeightPanel)
        form is PO33CatchWeight
    VendorItem is a Panel
        form is PO33VendorItem
    Comments is a Panel
        title is "Comments<CommentCount>"
        list is POReceiptAdjustmentAndInspectionLineCommentRel.ReceiptAdjustmentLineCommentList
    AdjustmentOrRejectionDetails is a Panel
        title is "<AdjustmentOrRejectionDetails>"
        visible when (IsInventoryBinLotSerialTracked)
        list is PurchaseOrderTransactionDetailsAdjRels.AdditionalAdjustmentEntrys
    UDIDetails is a Panel
        title is "UDI/DSCSADetail"
        visible when (IsUDITracked)
        list is PurchaseOrderUDIDetailAdjustmentRel.UDIEntrys
