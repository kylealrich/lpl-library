            three column grid distributed
                Item
                Quantity
                EnteredBuyUOM
            three column grid distributed
                Description
                    label is "Description"
                ItemType
                VendorBuyUnitCost
                    label is "UnitCost"
            three column grid distributed
                VendorItem
                ServiceCode
