PO33VendorItem is a Form
    Layout
        display as text
            visible when (LineExists)
                two column distributed
                    column1
                        paragraph
                            PurchaseOrderReceiptLine.VendorItem
                            PurchaseOrderReceiptLine.VendorItem.VendorItemDescription
                                no label
                        paragraph
                            PurchaseOrderReceiptLine.VendorItem.Manufacturer.ManufacturerCode
                                label is "ManufacturerCode"
                            PurchaseOrderReceiptLine.VendorItem.Manufacturer.ManufacturerDivision
                                label is "ManufacturerDivision"
                            PurchaseOrderReceiptLine.VendorItem.ManufacturerNumber
                                label is "ManufacturerNumber"
                        single column
                            PurchaseOrderReceiptLine.VendorItem.LabelerIdentificationCode
                                label is "L\\I\\CCode"
                    column2
                        paragraph
                            PurchaseOrderReceiptLine.VendorItem.VendorBuyUOM.UnitOfMeasure
                                label is "BuyU\\O\\M"
                            visible when (HasVendorItemBuyUOM)
                                paragraph
                                    text of "<PurchaseOrderReceiptLine.VendorItem.VendorBuyUOM.UOMConversion>_\\<PurchaseOrderReceiptLine.Item.StockUOM>_\\per_\\<PurchaseOrderReceiptLine.VendorItem.VendorBuyUOM.UnitOfMeasure>"
                        paragraph
                            PurchaseOrderReceiptLine.ItemGTIN
                            PurchaseOrderReceiptLine.ItemGTIN.AbbreviatedDescription
                                no label
            visible when (not LineExists)
                two column distributed
                    column1
                        paragraph
                            ReceiptLinesForTemplateRel.VendorItem
                            ReceiptLinesForTemplateRel.VendorItem.VendorItemDescription
                                no label
                        paragraph
                            ReceiptLinesForTemplateRel.VendorItem.Manufacturer.ManufacturerCode
                                label is "ManufacturerCode"
                            ReceiptLinesForTemplateRel.VendorItem.Manufacturer.ManufacturerDivision
                                label is "ManufacturerDivision"
                            ReceiptLinesForTemplateRel.VendorItem.ManufacturerNumber
                                label is "ManufacturerNumber"
                        single column
                            ReceiptLinesForTemplateRel.VendorItem.LabelerIdentificationCode
                                label is "L\\I\\CCode"
                    column2
                        paragraph
                            ReceiptLinesForTemplateRel.VendorItem.VendorBuyUOM.UnitOfMeasure
                                label is "BuyU\\O\\M"
                            visible when (HasTemplateVendorItemBuyUOM)
                                text of "<ReceiptLinesForTemplateRel.VendorItem.VendorBuyUOM.UOMConversion>_\\<ReceiptLinesForTemplateRel.Item.StockUOM>_\\per_\\<ReceiptLinesForTemplateRel.VendorItem.VendorBuyUOM.UnitOfMeasure>"
                        paragraph
                            ReceiptLinesForTemplateRel.ItemGTIN
                            ReceiptLinesForTemplateRel.ItemGTIN.AbbreviatedDescription
                                no label
