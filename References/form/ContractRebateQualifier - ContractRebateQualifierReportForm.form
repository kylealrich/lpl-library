ContractRebateQualifierReportForm is a SummaryForm
    title is "_"
    Layout
        three column
            column1
                blank line
            column2
                AllInvoicesForSupplier
                    display when blank
            column3
                blank line
        two column
            column1
                ContractRebateQualifier
                    label is "Qualifier"
                ContractRebateType
                    label is "Type"
                    display when blank
                paragraph
                    TargetedPurchasesFromDate
                        display when blank
                    TargetedPurchasesToDate
                        label is "To"
                        display when blank
                PercentOfTargetedPurchases
                    label is "PercentOfTargetedPurchases"
                    display when blank
            column2
                DerivedPurchaseTypeOperator
                    label is "TotalPurchasesByInvoicePrice"
                ContractRebate.RebatePercent
                    label is "RebatePercent"
