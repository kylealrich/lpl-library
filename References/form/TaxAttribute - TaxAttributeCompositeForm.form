TaxAttributeCompositeForm is a CompositeForm
    is primary
    title is "TaxAttribute"
    context form is TaxAttributeUserFieldForm
    show panel control on left
    OneSourceUsedAttributes is a Panel
        visible when (ShowAttributesThat.Used)
        list is OneSourceTaxAttributeUsedRel.primary
    OneSourceAvailableAttributes is a Panel
        visible when (ShowAttributesThat.Available)
        list is OneSourceTaxAttributeNotUsedRel.primary
    1AllSourceUserElement is a Panel
        title is "AllOneSourceAttributes"
        visible when (ShowAttributesThat.All)
        list is OneSourceTaxAttributeRel.primary
    VertexFlexibleFields is a Panel
        visible when (not HideVertexUserFields)
        list is VertexFlexibleFieldsRel.VertexFlexibleFieldsList
    DynamicFieldSearch is a Panel
        title is "DynamicFields"
        list is DynamicFieldRel.DynamicFieldSearchList
    Comments is a Panel
        title is "Comments<CommentsExist>"
        visible when (not HideComments)
        form is CommentsForm
