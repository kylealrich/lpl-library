ToForm is a Form
    title is "ToMappingOverride"
    Layout
        single column
            visible when (ManualToSplit)
                WeightDecimal
                Percent
            visible when (AllocationLine.ToMultipleAccountingEntityOrAccountingUnit)
                AccountingEntity
                    label is FinanceEnterpriseGroup.AccountingEntityLabel
            visible when (AllocationLine.ToMultipleAccountingUnit)
                AccountingUnit
                    label is FinanceEnterpriseGroup.AccountingUnitLabel
            visible when (AllocationLine.ToMultipleProject)
                Project
                    label is FinanceEnterpriseGroup.ProjectLabel
            visible when (AllocationLine.ToMultipleAccount)
                GeneralLedgerChartAccount
                    label is FinanceEnterpriseGroup.AccountLabel
            visible when (AllocationLine.ToMultipleDimension1)
                FinanceDimension1
                    label is FinanceEnterpriseGroup.FinanceDimension1Label
            visible when (AllocationLine.ToMultipleDimension2)
                FinanceDimension2
                    label is FinanceEnterpriseGroup.FinanceDimension2Label
            visible when (AllocationLine.ToMultipleDimension3)
                FinanceDimension3
                    label is FinanceEnterpriseGroup.FinanceDimension3Label
            visible when (AllocationLine.ToMultipleDimension4)
                FinanceDimension4
                    label is FinanceEnterpriseGroup.FinanceDimension4Label
            visible when (AllocationLine.ToMultipleDimension5)
                FinanceDimension5
                    label is FinanceEnterpriseGroup.FinanceDimension5Label
            visible when (AllocationLine.ToMultipleDimension6)
                FinanceDimension6
                    label is FinanceEnterpriseGroup.FinanceDimension6Label
            visible when (AllocationLine.ToMultipleDimension7)
                FinanceDimension7
                    label is FinanceEnterpriseGroup.FinanceDimension7Label
            visible when (AllocationLine.ToMultipleDimension8)
                FinanceDimension8
                    label is FinanceEnterpriseGroup.FinanceDimension8Label
            visible when (AllocationLine.ToMultipleDimension9)
                FinanceDimension9
                    label is FinanceEnterpriseGroup.FinanceDimension9Label
            visible when (AllocationLine.ToMultipleDimension10)
                FinanceDimension10
                    label is FinanceEnterpriseGroup.FinanceDimension10Label
