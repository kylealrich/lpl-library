            paragraph
                HasError
                    label is "HasError"
            paragraph
                text of "<ErrorMessage>"
                    color of red
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            Location
                label is "ParLocation"
            Location.Name
                no label
                display as text
        paragraph
            LocationType
        paragraph
            RequestingLocationFromCompany
            RequestingLocationFromLocation
        paragraph
            DefaultGLCategory
        paragraph
            BinSequence
            PreferredBin
        paragraph
            Item
        paragraph
            ReorderPoint
            UnitOfMeasure
            PatientChargeable
        paragraph
            GLCategory
            LeadtimeDays
        paragraph
            Vendor
            PurchaseFromLocation
        paragraph
            SuggestedReorderPoint
            ReplenishedFrom
        paragraph
            ReplenishFromCompany
            ReplenishFromLocation
            ReplenishFromBin
