TermsCodePreviewForm is a Form
    is primary
    title is "PaymentTermsPreview"
    Actions
        action is save
            label is "Preview"
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is refresh
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
    Layout
        paragraph
            TermsCode
            InvoiceDate
            InvoiceAmount
            AmountNumberOfDecimals
                label is "NumberOfDecimals"
        header2 of "PreviewInformation"
        visible when (HasDiscountPercent)
            header3 of "DiscountDates"
            display as text
                three column table
                    column headers
                        "DiscountDate"
                        "DiscountPercent"
                        "DiscountAmount"
                    column1
                        CalculatedTermsTable.CalculateTermsOutput[1].DiscountDate
                            no label
                    column2
                        CalculatedTermsTable.CalculateTermsOutput[1].DiscountPercent
                            no label
                    column3
                        CalculatedTermsTable.CalculateTermsOutput[1].DiscountAmount
                            no label
            visible when (HasDiscountPercent2)
