CreateEvent is an ActionForm
    action is CreateEvent
    Layout
        text of "EventLineQuantityWillBePopulatedWithTotalInvoiceQuantityOfAllTypesOfSpendFor"
        text of "<DerivedFromDate>-<DerivedThruDate>"
        blank line
        header4 of "NewEventValues"
        single column
            Company
            ApplyTemplate
                label is "ApplyEventTemplate"
                select is Templates
        blank line
        single column
            NewName
                label is "Name"
        single column
            NewDescription
                label is "Description"
                use text area
            NewEventReference
                label is "Reference"
            NewSourcingEventType
                label is "Type"
                select is SupplierEventTypes
            NewBuyer
                label is "Buyer"
        paragraph
            NewCategory
                label is "Category"
                select is BuyerCategories
            NewCategory.Description
                no label
        paragraph
            NewSubCategory
                label is "SubCategory"
                select is BuyerSubCategories
            NewSubCategory.Description
                no label
        single column
            NewPostingOptions
                label is "SupplierPortalPostingOptions"
        two column
            NewCreateAmendments
                label is "CreateAmendments"
            NewAllowSupplierQandA
                label is "AllowSupplierQandA"
        header4 of "NewEventDates"
        single column
            NewInformalQuote
                label is "DatesNotRequiredOnEvent"
        two column
            NewPreviewDate
                label is "Preview"
            NewOpenDate
                label is "Open"
            NewCloseDate
                label is "Close"
            NewQandAOpenDate
                label is "QAndAOpen"
            NewQandACloseDate
                label is "QAndAClose"
            NewDisputeCloseDate
                label is "DisputeClose"
        header4 of "NewLineDefaults"
        two column
            NewDefaultOutputType
                label is "OutputType"
            NewDefaultContractManagement
                label is "SendOutputToContractManagement"
