CreateCapitatedComponentForGroup is an ActionForm
    action is CreateCapitatedComponentForGroup
    Layout
        single column
            Contract.RepresentativeText
                display as text
            ItemDescription
                label is "Component"
                display as text
        paragraph
            ParmContractLine
                select is CapitatedProcedures
            ParmContractLine.VendorItem
                no label
                display as text
        paragraph
            ParmContractCapitatedComponentGroup
            ParmContractCapitatedComponentGroup.Name
                no label
                display as text
        paragraph
            ParmPrimaryDiscount
            text of "or"
            ParmPrimaryFinalCost
            text of "or"
            ParmUseBaseCostForPrimary
        paragraph
            ParmRevisionDiscount
            text of "or"
            ParmRevisionFinalCost
