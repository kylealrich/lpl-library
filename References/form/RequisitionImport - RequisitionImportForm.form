RequisitionImportForm is a Form
    title is "RequisitionInterface"
    Layout
        two column distributed
            column1
                FromCompanyLocation.FromCompany
                    label is "FromCompany"
                FromCompanyLocation.FromLocation
                    label is "FromLocation"
                    show up to 10 characters
            column2
                paragraph
                    Vendor
                    VendorName
                        show up to 10 characters
                PurchaseFromLocation
                    show up to 10 characters
        two column distributed
            paragraph
                OperatorID
                    show up to 10 characters
                paragraph
                    PrintRequisition
                    QuoteRequired
            paragraph
                DefaultBillCode
                    show up to 10 characters
                GlobalDocumentType
        two column distributed
            POCode
            paragraph
                CurrencyTable
                    show up to 10 characters
                TransactionCurrencyCode
        two column distributed
            paragraph
                AssetTemplate
                    show up to 10 characters
                Asset
                    show up to 10 characters
            paragraph
                PurchaseTaxable
                    label is "Taxable"
                PurchaseTaxCode
                    label is "TaxCode"
                    show up to 10 characters
        two column distributed
            DefaultDistributionAccount
                compact format
            Buyer
        two column distributed
            RequestedDeliveryDate
            AllocationPriority
                show up to 10 characters
            DeliverTo
                show up to 10 characters
            LocationRule
                show up to 10 characters
        single column
            header3 of "ProcedureInformation"
            PatientProcedure
        two column distributed
            DefaultProcedureInformation
