CashAllocationCodeComposite is a CompositeForm
    is primary
    title is "CashAllocationCode"
    context form is CashAllocationCode
        background is theme color
    Actions
        action is Activate
            action icon is hidden
            hide when invalid
        action is Inactivate
            action icon is hidden
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is save
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    Details is a Panel
        list is CashAllocationCodeDetail set.CashAllocationCodeDetails
