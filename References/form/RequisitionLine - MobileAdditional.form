MobileAdditional is a Form
    title is "Additional"
    Layout
        display as text
            visible when (Item.HasDesc3)
                paragraph
                    Item.Description3
                        label is "FullDescription"
            single column
                Manufacturer.DerivedCodeDivisionDescription
                    label is "Manufacturer"
            two column distributed
                Vendor
                paragraph
                    VendorItem
                    VendorItem.VendorItemDescription
                        no label
            two column distributed
                ItemType
                ServiceCode
            two column distributed
                CreatePurchaseOrder
                AllocationPriority
                    label is "Priority"
            two column distributed
                RequestedDeliveryDate
                LateDeliveryDate
            two column distributed
                CostOption
                OverrideCost
