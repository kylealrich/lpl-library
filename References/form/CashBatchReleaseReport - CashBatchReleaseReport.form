CashBatchReleaseReport is an ActionForm
    action is CashBatchReleaseReport
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            text of "Or"
                bold
        paragraph
            CompanyGroup
                label is "CompanyGroup"
            CompanyGroup.Description
                no label
                display as text
        paragraph
            ReleaseThirdParty
                label is "ReleaseThirdPartyBatchesOnly"
        paragraph
            text of "Or"
                bold
        paragraph
            AllBatches
                label is "ReleaseAllBatches"
        paragraph
            text of "Or"
                bold
        three column distributed
            column1
                GroupBatch.GroupBatch[1]
                    label is "Batch"
                    select is UnreleasedBatches
                GroupBatch.GroupBatch[4]
                    no label
                    select is UnreleasedBatches
            column2
                GroupBatch.GroupBatch[2]
                    no label
                    select is UnreleasedBatches
                GroupBatch.GroupBatch[5]
                    no label
                    select is UnreleasedBatches
            column3
                GroupBatch.GroupBatch[3]
                    no label
                    select is UnreleasedBatches
                GroupBatch.GroupBatch[6]
                    no label
                    select is UnreleasedBatches
        paragraph
            text of "Or"
                bold
        paragraph
            OprCode
                label is "Operator"
        paragraph
            text of "Or"
                bold
        three column distributed
            column1
                BatchDateFr
                    label is "FromBatchDate"
            column2
                BatchDateTo
                    label is "ToBatchDate"
        paragraph
            text of "Or"
                bold
        three column distributed
            column1
                BatchFrom
                    label is "FromBatch"
                    select is UnreleasedBatches
            column2
                BatchTo
                    label is "ToBatch"
                    select is UnreleasedBatches
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                CashBatchReleaseReportReport_RDG
            column2 of 25%
                CashBatchReleaseReportReport_XPT
