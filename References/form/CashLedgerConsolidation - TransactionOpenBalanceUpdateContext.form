TransactionOpenBalanceUpdateContext is a Form
    Layout
        two column distributed
            column1
                paragraph
                    CashCode
                    CashCode.Description
                        no label
                        display as text
                    CashCode.Currency
                        no label
                paragraph
                    CashLedgerConsolidation.LastMaintenanceDate
                        label is "Date"
            column2
                display as text
                    paragraph
                        OpenBeginningBalance
                            label is "BeginningBalance"
                    paragraph
                        OpenEndingBalance
                            label is "EndingBalance"
        visible when (not CashCodeLevelExists)
            paragraph
                CashLedgerConsolidation.Company
                CashLedgerConsolidation.Company.Name
                    no label
                    display as text
            paragraph
                CashLedgerConsolidation.BankTransactionCode
            paragraph
                OpenBeginningBalance
                    label is "BeginningBalance"
                OpenEndingBalance
                    label is "EndingBalance"
        blank line
        blank line
