ManageAssetDetailsContextForm is a Form
    title is "ReceiptLineDetails"
    Layout
        display as text
            two column distributed
                column1
                    Company.DerivedCompanyIDName
                        label is "Company"
                    paragraph
                        PurchaseOrder
                            label is "PurchaseOrder"
                        PurchaseOrderReceipt
                            label is "Receipt"
                        PurchaseOrderLine.LineNumber
                            label is "Line"
                    paragraph
                        EnteredReceivedQuantity
                            label is "CurrentReceivedQuantity"
                column2
                    paragraph
                        PurchaseOrderLineAssetRel.ReceivedQuantity
                            label is "TotalReceivedQuantityForPurchaseOrderLine"
                    paragraph
                        PurchaseOrderLineAssetRel.DerivedTotalAssetDetails
                            label is "TotalNumberOfReceivedAssetDetails"
                    paragraph
                        text of "Note:ToReleaseCurrentReceipt,TotalReceivedQuantityShouldMatchTotalReceivedAssetDetails"
                            bold
                            italics
