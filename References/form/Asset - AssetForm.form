AssetForm is a CompositeForm
    is primary
    title is "<AssetAndDescription>"
    context form is AssetContextForm
        background is theme color
    restrict action IncludeInMassSelection
    restrict action IncludeInMassSelectAll
    restrict action ExcludeFromMassSelectAll
    restrict action LoadToTrueCostAssetManagementInterface
    restrict action FullDispose
    Actions
        action is Unreleased.PortableCreateFromTemplate
            label is "CreateFromTemplate"
        action is Unreleased.Create
            action icon is create
            label is "_"
        action is save
            label is "_"
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is Unreleased.Delete
            action icon is delete
            label is "_"
        action is refresh
            label is "_"
    Main is a Panel
        form is AssetMain
    Funds1 is a Panel
        title is "Funds"
        visible when (FundOverrideSelected)
        list is AssetFund set.AssetFunds
    Funds2 is a Panel
        title is "Funds"
        visible when (AssetLease.Lease.CreateAssetFundRecords)
        list is AssetFund set.AssetFundsNoUpdate
    Items is a Panel
        visible when (not IsFundUnreleasedAndNotTransferInProgress)
        list is AssetItem set.AssetItems
    AssetItems is a Panel
        title is "Items"
        visible when (IsFundUnreleasedAndNotTransferInProgress)
        list is AssetItem set.AssetItemCreate
    UnreleasedAssetBooks is a Panel
        title is "Books"
        visible when (IsUnreleasedAndNotTransferInProgress)
        list is FunctionalBooksRel.AssetBookCreate
    AssetBooks is a Panel
        title is "Books"
        visible when (not IsUnreleasedAndNotTransferInProgress)
        list is AssetBooksForDisplayRel.NotDisposedAssetBooks
    Transactions is a MultiListPanel
        visible when (UnreleasedTransactionsExists)
        list is AssetTransactionsRel.AssetTransactions
            visible when (NotDisposedAndDisplayJournalEntries)
        list is AssetTransactionsRel.NewAssetCreateTransactions
            visible when (AssetCreatePostingTransactionList)
        list is UnreleasedBookTransactionsRel.BookTransactions
            visible when (NotPostingBookDisplayJournalEntries)
        list is AssetTransactionsRel.ReinstatementTransactions
            visible when (ReinstatedAndDisplayJournalEntries)
        list is UnreleasedBookTransactionsRel.ReinstatementTransactions
            visible when (ReinstatedBookEntries)
    ReleasedTransactions is a MultiListPanel
        title is "ReleasedTransactions"
        visible when (ReleasedTransactionsExists)
        list is ReleasedAssetTransactionsRel.ReleasedAssetTransactions
            visible when (HasReleasedTransactions)
        list is ReleasedBookTransactionsRel.ReleasedBookTransactions
            visible when (HasReleasedBookTransactions)
    Disposals is a Panel
        visible when (DisposalExists)
        list is AssetDisposalsRel.InContextOfAssetDisposals
    Transfers is a Panel
        visible when (AssetTransferExists)
        list is AssetTransfer set.AssetTransfers
    Adjustments is a Panel
        visible when (AssetAdjustmentExists)
        list is AssetAdjustment set.AssetAdjustments
    Revaluations is a Panel
        visible when (AssetRevaluationsExists)
        list is AssetRevaluationsRel.AssetRevaluations
    BookRevaluation is a Panel
        visible when (AssetBookRevaluationsExists)
        list is AssetBookRevaluationsRel.AssetBookRevaluationsList
    Impairment is a Panel
        visible when (AssetImpairmentsExists)
        list is AssetImpairmentsRel.AssetImpairments
    RetirementObligations is a Panel
        visible when (AssetRetirementObligationsExists)
        list is AssetRetirementObligationRel.RetirementObligations
    Repairs is a Panel
        list is AssetRepairsRel.AssetRepairs
    Interfacing is a MultiListPanel
        visible when (HasInterfacing)
        list is AssetRepairImport set.InterfacingAssetRepairs
            visible when (HasInterfacingRepairs)
        list is AssetItemAdjustmentImportsRel.AssetItemAdjustmentImportList
            visible when (HasInterfacingAdjustments)
    Comments is a Panel
        list is AssetComment set.primary
