ScheduleRebalanceAccountingUnitZones is a Form
    is primary
    Layout
        single column
            FinanceEnterpriseGroup
            EntityOption
                label is "<FinanceEnterpriseGroup.AccountingEntityLabel>Option"
            visible when (EntityOption.Single)
                AccountingEntity
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>"
            visible when (EntityOption.Group)
                AccountingEntityGroup
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>Group"
            blank line
            YearOption
                label is "YearOption"
            visible when (YearOption.SpecificBeginYear)
                GeneralLedgerCloseYear
                    label is "BeginFiscalYear"
            visible when (YearOption.SpecificBeginYear)
                GeneralLedgerCalendarPeriod
                    label is "FirstPeriodEndingDateForBeginYear"
                    select is GeneralLedgerCalendarPeriodEndDateList
