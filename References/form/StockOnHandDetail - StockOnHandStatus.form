StockOnHandStatus is a CompositeForm
    is primary
    context form is StockOnHandStatusContext
        background is theme color
    Actions
        action is DetailTransfer
            action icon is transfer
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
            hide when invalid
        action is refresh
            label is "_"
    LotBinTrackedItem is a Panel
        visible when (LotBinTracked)
        list is StockOnHandDetail.LotTransfer
    SerialTrackedItem is a Panel
        visible when (IsSerialTracked)
        list is StockOnHandDetail.SerialBinTransfer
    BinTrackedItem is a Panel
        visible when (IsOnlyBinTracked)
        list is StockOnHandDetail.IsBinTransfer
    CurrentStatus is a Panel
        form is CurrentStatus
