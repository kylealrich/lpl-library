PurchasingAndSalesJournal is an ActionForm
    action is PurchasingAndSalesJournal
    Layout
        two column distributed
            TaxEntity
            paragraph
                Company
                Company.Name
                    no label
                    display as text
        two column distributed
            ProcLevel
                label is "ProcessLevel"
            Source
        single column
            FinanceCodeBlock
                label is "Account"
                compact format
        two column distributed
            BegTaxYear
                label is "TaxPeriodBegin"
            BegTaxPeriod
                label is "BeginningTaxPeriod"
        two column distributed
            EndTaxYear
                label is "TaxPeriodEnd"
            EndTaxPeriod
        single column
            DistLines
                label is "DistributionLines"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                PurchasingAndSalesJournalReport_RDG
            column2 of 25%
                PurchasingAndSalesJournalReport_XPT
