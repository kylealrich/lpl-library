RequestNewItem is an ActionForm
    action is RequestNewItem
    title is "RequestNewItem"
    Layout
        section of Item
            visible when (actor.initiator)
                text of "This_request_will_be_routed_for_approval;_after_it_is_approved_this_record_will_be_created"
                    header4
                    italics
                    color of blue
            two column distributed
                column1
                    Description
                    Description2
                column2
                    Description3
                        label is "LongDescription"
                        use text area of 5 lines
            two column distributed
                column1
                    visible when (actor.initiator)
                        RequestPriority
                            show up to 30 characters
                        NeededBy
                    visible when (not actor.initiator)
                        paragraph
                            ItemGroup
                                display as required
                            ItemGroup.Description
                                no label
                                display as text
                        visible when (not ItemGroup.AutomaticItemNumbering)
                            Item
                                show up to 20 characters
                                display as required
                column2
                    action comment
                        label is "RequestComment"
                        use text area of 5 lines
            two column distributed
                Reference1
                Reference2
            visible when (not actor.initiator)
                two column distributed
                    paragraph
                        StockUOM
                            show up to 2 characters
                            display as required
                        StockUOM.Description
                            no label
                            display as text
                    GenericName
                        show up to 20 characters
                two column distributed
                    SerialTracked
                        show up to 30 characters
                    LotTracked
                        show up to 30 characters
                two column distributed
                    ReplacementItem
                        show up to 20 characters
                    visible when (HasReplacementItem)
                        ReplacementItem.Description
                            no label
                            display as text
                two column distributed
                    IssueAccount.GeneralLedgerChartAccount
                        label is "IssueAccount"
                        show up to 20 characters
                    visible when (HasGeneralLedgerChartAccount)
                        IssueAccount.GeneralLedgerChartAccount.AccountDescription
                            no label
                            display as text
                two column distributed
                    paragraph distributed
                        Manufacturer
                            compact format
                        Manufacturer.Description
                            no label
                            display as text
                    ManufacturerNumber
                two column distributed
                    AllowCustomerOrderReturns
                        show up to 30 characters
                two column distributed
                    StockWeight
                        label is "StockWeight"
                        show up to 30 characters
                    StockVolume
                        label is "StockCubic"
                        show up to 30 characters
            two column distributed
                visible when (actor.initiator)
                    CustomerOrderReturnDate
                        label is "ReturnDate"
                TransientAttachment.File
                    label is "Attachment"
            visible when (not actor.initiator)
                two column distributed
                    paragraph
                        MSDSRequired
                        CertificationRequired
                    paragraph
                        Consignment
                        BrokenCaseCharge
        section of Details
            header2 of "Number_ofDecimals"
            three column
                NumberOfDecimalsCost
                    label is "Cost"
                NumberOfDecimalsPrice
                    label is "Price"
                NumberOfDecimalsQuantity
                    label is "Quantity"
            header2 of "Codes"
            visible when (not SmartphoneDisplay)
                five column distributed
                    column1
                        FreightClass
                        HazardCode
                        CommodityCode
                            show up to 10 characters
                    column2
                        FreightClass.Description
                            no label
                            display as text
                        HazardCode.Description
                            no label
                            display as text
                        CommodityCode.CcDescription
                            no label
                            display as text
                    column3
                        ICNCode
                        UNSPSCCode
                            compact format
                        visible when (IsHSNSACCodeEnabled)
                            HSNSACCode
                    column4
                        ICNCode.Description
                            no label
                            display as text
                        UNSPSCCode.LongDescription
                            no label
                            display as text
                        visible when (IsHSNSACCodeEnabled)
                            HSNSACCode.Description
                                no label
                                display as text
                    column5
                        Sterile
                        Reprocessed
                        Pharmaceutical
                        SingleUse
                        Dressing
                        Reusable
                        Analytics
            visible when (SmartphoneDisplay)
                single column
                    paragraph
                        FreightClass
                        FreightClass.Description
                            no label
                            display as text
                    paragraph
                        ICNCode
                        ICNCode.Description
                            no label
                            display as text
                    paragraph
                        HazardCode
                        HazardCode.Description
                            no label
                            display as text
                    paragraph
                        UNSPSCCode
                            compact format
                        UNSPSCCode.LongDescription
                            no label
                            display as text
                    visible when (IsHSNSACCodeEnabled)
                        paragraph
                            HSNSACCode
                            HSNSACCode.Description
                                no label
                                display as text
                    paragraph
                        CommodityCode
                        CommodityCode.CcDescription
                            no label
                            display as text
                    Sterile
                    Reprocessed
                    Pharmaceutical
                    SingleUse
                    Dressing
                    Reusable
                    Analytics
            header2 of "Classes"
            visible when (not SmartphoneDisplay)
                two column table
                    column headers
                        "Major"
                        "Minor"
                    row headers
                        "Inventory"
                        "Purchasing"
                        "Sales"
                    column1
                        paragraph
                            InventoryClass.MajorInventoryClass
                                no label
                            InventoryClass.MajorInventoryClass.Description
                                no label
                                display as text
                        paragraph
                            PurchasingClass.MajorPurchasingClass
                                no label
                            PurchasingClass.MajorPurchasingClass.Description
                                no label
                                display as text
                        paragraph
                            SalesClass.MajorSalesClass
                                no label
                            SalesClass.MajorSalesClass.Description
                                no label
                                display as text
                    column2
                        paragraph
                            InventoryClass.MinorInventoryClass
                                no label
                            InventoryClass.MinorInventoryClass.Description
                                no label
                                display as text
                        paragraph
                            PurchasingClass.MinorPurchasingClass
                                no label
                            PurchasingClass.MinorPurchasingClass.Description
                                no label
                                display as text
                        paragraph
                            SalesClass.MinorSalesClass
                                no label
                            SalesClass.MinorSalesClass.Description
                                no label
                                display as text
            visible when (SmartphoneDisplay)
                two column table
                    column1
                        InventoryClass.MajorInventoryClass
                        InventoryClass.MinorInventoryClass
                        PurchasingClass.MajorPurchasingClass
                        PurchasingClass.MinorPurchasingClass
                        SalesClass.MajorSalesClass
                        SalesClass.MinorSalesClass
                    column2
                        InventoryClass.MajorInventoryClass.Description
                            display as text
                        InventoryClass.MinorInventoryClass.Description
                            no label
                            display as text
                        PurchasingClass.MajorPurchasingClass.Description
                            no label
                            display as text
                        PurchasingClass.MinorPurchasingClass.Description
                            no label
                            display as text
                        SalesClass.MajorSalesClass.Description
                            no label
                            display as text
                        SalesClass.MinorSalesClass.Description
                            no label
                            display as text
            header2 of "TaxInformation"
            single column
                PurchaseTaxable
                    label is "Taxable"
            two column table
                column1
                    PurchaseTaxCode
                        label is "TaxCode"
                    ProductTaxCategory
                column2
                    PurchaseTaxCode.Description
                        no label
                        display as text
                    ProductTaxCategory.Description
                        no label
                        display as text
        section of Healthcare
            visible when (CanShowHealthcareInformation)
                visible when (not SmartphoneDisplay)
                    two column table
                        column1
                            paragraph
                                HCPCSCode
                                    label is "HCPCS_andSequence"
                            ConsignCode
                                label is "BillCode"
                                show up to 30 characters
                            Latex
                            ClinicalSystemUse
                            PersonalProtectiveEquipment
                            Implantable
                            Drugs
                            Instrument
                        column2
                            HCPCSCode.ShortDescription
                                label is "Description"
                                display as text
                            ChargeNumber
                            HasExpirationDate
                            PatientChargeable
                            CriticalItem
                            Narcotic
                            ContainsHumanTissue
                visible when (SmartphoneDisplay)
                    paragraph
                        HCPCSCode
                            label is "HCPCS_andSequence"
                        HCPCSCode.ShortDescription
                            label is "Description"
                            display as text
                    single column
                        Latex
                        ConsignCode
                            label is "BillCode"
                            show up to 30 characters
                        ChargeNumber
                        HasExpirationDate
                        ClinicalSystemUse
                        PatientChargeable
                        PersonalProtectiveEquipment
                        CriticalItem
                        Narcotic
                        ContainsHumanTissue
                        Implantable
                        Drugs
                        Instrument
        section of Request visible when (not actor.initiator)
            display as text
                paragraph
                    RequestedBy
                    RequestPriority
                    NeededBy
                paragraph
                    button of "<RequesterEmail>"
                        link is "mailto:<RequesterEmail>"
                        label is "Email"
                        display as link
                    button of "<RequesterPhone.SubscriberNumber>"
                        link is "tel:<RequesterPhone>"
                        label is "PhoneNumber"
                        display as link
