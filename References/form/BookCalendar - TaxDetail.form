TaxDetail is a CompositeForm
    title is "TaxDetail"
    context form is TaxContext
        background is theme color
    restrict action SelectBookCalendars
    restrict action ApplyMidQuarterConvention
    restrict action MQProjection
    restrict action ResetAssetBooks
    restrict action ReleaseAssetBooks
    CalendarYearPurchases is a Panel
        title is "CalendarYearPurchases"
        visible when (IsCurrent)
        list is CalendarYearAdditionsRel.AssetBookTaxReport
    HistoricalPurchases is a Panel
        title is "CalendarYearPurchases"
        visible when (IsHistory)
        list is CalendarYearAdditionsRel.AssetBookHistoryTaxReport
