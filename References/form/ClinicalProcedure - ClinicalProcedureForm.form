ClinicalProcedureForm is a Form
    is primary
    title is "ClinicalProcedure"
    Layout
        visible when (RecordExists)
            display as text
                paragraph
                    ItemGroup.ItemGroupDescription
                        label is "ItemGroup"
                paragraph
                    ClinicalProcedure
        visible when (not RecordExists)
            paragraph
                ItemGroup
                    select is ItemGroupValidForPreferenceCardList
                ItemGroup.Description
                    no label
                    display as text
            paragraph
                ClinicalProcedure
        paragraph
            ClinicalProcedureName
        paragraph
            Active
