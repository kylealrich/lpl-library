MobileCashApplicationSelect is a CompositeForm
    title is "CashApplicationSelection"
    context form is MobileCashApplicationSelectContextForm
    show panel control on left
    restrict action SelectForAPInterface
    restrict action RefreshData
    restrict action Unreleased.CreateDistribution
    restrict action Unreleased.Delete
    restrict action Unreleased.ReleaseBatch
    MatchCashDetails is a Panel
        title is "MatchCashDetails"
        visible when (ShowMatchCashDetails)
        list is ReceivablePaymentCashDetail set.CashDetailsForApplication
    InvoiceSelectionNA is a Panel
        title is "InvoiceSelectionNA"
        visible when (not CompanyCustomerRel.NationalAccountIndicator.No)
        list is NationalAccountTransactionsRel.MobileCashApplicationSelectEntryList2
    InvoiceSelection is a Panel
        visible when (not ReceivableCompanyRel.IsApplyCashAcrossReceivableCompanies)
        list is TransactionsForCustomerRel.CashApplicationSelectEntryList2
    InvoiceSelectionAcrossCompanies is a Panel
        title is "InvoiceSelection"
        visible when (ReceivableCompanyRel.IsApplyCashAcrossReceivableCompanies)
        list is TransactionsForCustomerAcrossCompanyRel.CashApplicationSelectEntryList2
    SpeedEntryPreview is a Panel
        list is ReceivableApplicationSelectRel.MobileSelectPaymentApplicationsSpeedEntry
    WriteOffPreview is a Panel
        visible when (not IsZeroPayment)
        list is PendingAdjustNonMemoRel.MobilePaymentSelectWriteoffs
    ChargebackMemo is a Panel
        visible when (not IsZeroPayment)
        list is PendingAdjustMemoRel.MobileMemos
    ReceivableProjectTransactions is a Panel
        list is OpenReceivableProjectTransactionRel.ReceivableProjectTransactionCashApplicationList
    LineItemSelectionNA is a Panel
        title is "LineItemSelectionNA"
        visible when (not CompanyCustomerRel.NationalAccountIndicator.No)
        list is NationalAccountTransactionsRel.MobileLineItemCashApplicationSelectEntryList2
    LineItemSelection is a Panel
        visible when (not ReceivableCompanyRel.IsApplyCashAcrossReceivableCompanies)
        list is TransactionsForCustomerRel.LineItemCashApplicationSelectEntryList2
    LineItemSelectionAcrossCompanies is a Panel
        title is "LineItemSelection"
        visible when (ReceivableCompanyRel.IsApplyCashAcrossReceivableCompanies)
        list is TransactionsForCustomerAcrossCompanyRel.LineItemCashApplicationSelectEntryList2
