BudgetGroupTotal is a Form
    is primary
    title is "Obligations_(<BudgetGroup.DerivedCurrency>)"
    Layout
        paragraph
            GeneralLedgerCalendarPeriod
                label is "Period"
            CurrencyAmount
                label is "Amount"
            visible when (BudgetTemplate.IsYearToDate)
                YTD
                    label is YearToDate
            visible when (DisplayLTD)
                LTD
                    label is LifeToDate
