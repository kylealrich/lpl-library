ReclassifyUncategorizedLines is an ActionForm
    action is Unreconciled.ReclassifyUncategorizedLines
    title is "ReclassifyUncategorizedLines"
    Layout
        paragraph
            CashManagementAccount
                label is "Account"
                display as text
            CashManagementAccount.Title
                no label
                display as text
        single column
            BankStatement
                display as text
        header3 of "ReclassifyCategoryFrom:<first UncategorizedCategoryRel.CashTransactionCategory>To:"
        single column
            PrmCategory
                label is "Category"
                select is CashTransactionCategorySelect
