OptionsForm is a Form
    title is "Options"
    Layout
        blank line
        header2 of "Dimension"
        three column distributed
            FundAccounting
            visible when (FinanceDimension1Label entered)
                Dimension1ZoneBalancing
                    label is "<FinanceDimension1Label>ZoneBalancing"
            visible when (CustodialAccountManagement)
                CustodialAccountManagement
        three column distributed
            visible when (FrontEndSplits)
                FrontEndSplits
                    label is "FrontEndSplit"
                UseFundedAmounts
                    label is "UseContractFundedAmounts"
        three column distributed
            ProjectDateEdit
                label is "<ProjectLabel>DateEdit"
            ProjectBurdenDate
                label is "<ProjectLabel>BurdenDate"
        three column distributed
            ProjectInvoicing
                label is "<ProjectLabel>Invoicing"
            visible when (ProjectInvoicing)
                paragraph
                    BillingOverrideLevel
                    BillingOverrideLevelLabel
                        no label
                        display as text
                ThirdPartyInvoicePrint
        three column distributed
            paragraph
                StructureValidationDimension
                    label is "ProjectStructureValidationDimension"
                StructureValidationDimensionLabel
                    no label
                    display as text
            visible when (HasStructureValidationDimension)
                ValidationDimensionRequiresProject
            ProjectStructureValidation
                label is "ProjectStructureValidationEnabled"
        single column
            UnitControl
                raise blue alert when (StructureRelationRequired)
                    mouse over text is "Structure_Relations_With_Account_Are_Required"
                when value changed
                    refresh UnitControl
        three column distributed
            InterEntityDimension
            NettingJournalApproval
            EnableRelatedPartyElimination
        three column distributed
            HROrganization
                label is "HROrganization"
            ValidateFinanceStructure
                label is "StructureExceptions"
            paragraph
                EnableResponsibilityMatrix
                visible when (EnableResponsibilityMatrix)
                    ResponsibilityMatrixSecurity
        blank line
        header2 of "General"
        three column distributed
            column1
                RequireUniqueJournalID
                UpdateInterZoneBalance
                    label is "UpdateZonePayablesAndReceivablesBalance"
                FinanceStructureSmartSelect
                    label is "UseFinanceStructureRelationDetailForSelect"
                BypassCubeRefreshOnPost
                    label is "BypassCubeRefreshOnQuickPost"
                UseDEPMBudgeting
                    label is "Use_EPMBudgeting"
            column2
                BODTrigger
                    label is "BODTrigger"
                EnableIDMForAttachments
                    display only
                NoDimensionUREPosting
                AutoReverseWithinSamePeriod
            column3
                visible when (BODTrigger)
                    BODCostCenter
                visible when (IsArchivingEnabled)
                    ArchivingEnabledMessage
                        no label
                        display as tag
                            color of green
                visible when (not IsArchivingEnabled)
                    ArchivingDisabledMessage
                        no label
                        display as tag
                            color of red
                PurgeCutOffDate
                    display only
                    raise red alert when (IsPurgeCutOffDateAfter1Year)
                        mouse over text is "DateIsWithin1Year"
                    raise yellow alert when (IsPurgeCutOffDateAfter2Years)
                        mouse over text is "DateIsWithin2Years"
                    raise blue alert when (IsPurgeCutOffDateAfter3Years)
                        mouse over text is "DateIsWithin3Years"
        three column distributed
            DimensionForDepartmentJournal
            visible when (EnableFundDimension)
                FundDimension
        three column distributed
            UseLeaseRateTable
            ShortTermLiabilityAccounting
            LeaseAccountingStandard
        blank line
        header2 of "AverageDailyBalance"
        single column
            UseAverageDailyBalance
            ADBSelection
                raise blue alert when (true)
                    mouse over text is "This_is_for_selecting_fixed_dimensions_for_ADB"
        three column distributed
            visible when (not ADBSelection.None)
                AllBalanceSheetAccounts
                    display only
                visible when (AllBalanceSheetAccounts)
                    IncludeUREInAverageDailyBalance
                        display only
                visible when (NewAverageDailyBalanceProcess)
                    IncludeIncomeStatementAccounts
        three column distributed
            visible when (ADBSelection.AccountOnly)
                CalculateADBOption
                IncludeLedgerInAverageDailyBalance
                    display only
                ADBProcessingDate
            visible when (ADBSelection.AccountAndOneDimension)
                CalculateADBOption
                IncludeLedgerInAverageDailyBalance
                    display only
                ADBProcessingDate
            visible when (ADBSelection.AccountAndTwoDimensions)
                CalculateADBOption
                IncludeLedgerInAverageDailyBalance
                    display only
                ADBProcessingDate
            visible when (ADBSelection.AccountAndThreeDimensions)
                CalculateADBOption
                IncludeLedgerInAverageDailyBalance
                    display only
                ADBProcessingDate
        single column
            visible when (DisplayADBResetNotification)
                text of "<ADBResetNotification>"
                    color of blue
        three column distributed
            visible when (ADBSelection.AccountAndOneDimension)
                ADBDimensions.Dimension1
                    select is StructureRowDimensionListForADB
            visible when (ADBSelection.AccountAndTwoDimensions)
                ADBDimensions.Dimension1
                    select is StructureRowDimensionListForADB
                ADBDimensions.Dimension2
                    select is StructureRowDimensionListForADB
            visible when (ADBSelection.AccountAndThreeDimensions)
                ADBDimensions.Dimension1
                    select is StructureRowDimensionListForADB
                ADBDimensions.Dimension2
                    select is StructureRowDimensionListForADB
                ADBDimensions.Dimension3
                    select is StructureRowDimensionListForADB
        blank line
        header2 of "RegularExpression"
        three column distributed
            AccountingEntityRegularExpression
                label is "<AccountingEntityLabel>"
            AccountRegularExpression
                label is "<AccountLabel>"
            SubAccounRegularExpression
                label is "SubAccount"
            visible when (AccountingUnitLabel entered)
                AccountingUnitRegularExpression
                    label is "<AccountingUnitLabel>"
            visible when (ProjectLabel entered)
                ProjectRegularExpression
                    label is "<ProjectLabel>"
            visible when (FinanceDimension1Label entered)
                FinanceDimension1RegularExpression
                    label is "<FinanceDimension1Label>"
            visible when (FinanceDimension2Label entered)
                FinanceDimension2RegularExpression
                    label is "<FinanceDimension2Label>"
            visible when (FinanceDimension3Label entered)
                FinanceDimension3RegularExpression
                    label is "<FinanceDimension3Label>"
            visible when (FinanceDimension4Label entered)
                FinanceDimension4RegularExpression
                    label is "<FinanceDimension4Label>"
            visible when (FinanceDimension5Label entered)
                FinanceDimension5RegularExpression
                    label is "<FinanceDimension5Label>"
            visible when (FinanceDimension6Label entered)
                FinanceDimension6RegularExpression
                    label is "<FinanceDimension6Label>"
            visible when (FinanceDimension7Label entered)
                FinanceDimension7RegularExpression
                    label is "<FinanceDimension7Label>"
            visible when (FinanceDimension8Label entered)
                FinanceDimension8RegularExpression
                    label is "<FinanceDimension8Label>"
            visible when (FinanceDimension9Label entered)
                FinanceDimension9RegularExpression
                    label is "<FinanceDimension9Label>"
            visible when (FinanceDimension10Label entered)
                FinanceDimension10RegularExpression
                    label is "<FinanceDimension10Label>"
