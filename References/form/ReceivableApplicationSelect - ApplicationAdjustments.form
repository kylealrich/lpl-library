ApplicationAdjustments is a CompositeForm
    title is "SelectedApplication"
    context form is ReceivableApplicationSelectContextForm
        background is theme color
    restrict action Create
    restrict action CreateViaSpeedEntry
    DistributionSet is a Panel
        title is "Distributions"
        visible when (ShowDistributionSet)
        list is ReceivableInvoiceDetailRel.ReceivableInvoiceRel.ProrateDistributionsRel.ReceivableDistributionSetSelectList1
    Adjustments is a Panel
        visible when (ShowAdjustments)
        list is ReceivableApplicationAdjustmentSelectRel.AdjustmentsForApplication
