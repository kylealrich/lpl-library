BillingInterfaceResultComposite is a CompositeForm
    is primary
    title is "BillingInterfaceResult"
    context form is BillingInterfaceResultContext
        background is theme color
    ProcessingCounts is a Panel
        form is ProcessingCountsForm
    TransactionsInterfaced is a Panel
        visible when (HasTransactionsCreated)
        list is BillingInvoicesRel.BillingInvoiceInterfacedList
    TransactionsExceedingCreditLimit is a Panel
        title is "TransactionsInterfaced"
        visible when (HasTransactionsExceedingCreditLimit)
        list is BillingInvoicesRel.InterfacedInvoicesExceedingCreditLimitList
    UninterfacedTransactions is a Panel
        visible when (HasIncompleteImportRecords)
        list is BillingInvoiceImportRel.UnInterfacedInvoices
