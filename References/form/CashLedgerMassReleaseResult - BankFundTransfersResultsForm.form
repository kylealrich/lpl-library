BankFundTransfersResultsForm is a CompositeForm
    title is "BankFundTransferMassReleaseResult<CashLedgerMassReleaseResult>"
    context form is BankFundTransfersResultsContext
        background is theme color
    Actions
        action is RerunMassReleaseForBankFundTransferErrors
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    Parameters is a Panel
        form is ParametersForm
    BankFundTransfersReleased is a Panel
        title is "Released_(<ReleaseCount>)"
        list is BankFundTransfersReleasedRel.ReleasedFundTransfersList
    BankFundTransfersInError is a Panel
        title is "Errors_(<ErrorCount>)"
        visible when (BankFundTransferErrorsExist)
        list is BankFundTransfersWithErrorsRel.MassReleaseBankFundTransferErrorsList
