CompletePaymentCycle is an ActionForm
    action is Completed.CompletePaymentCycle
    title is "CompletePaymentCycle"
    Layout
        header3 of "FileName"
        single column
            text of "Payment_\\Output_\\File_\\NameWillDefaultFrom_\\Electronic_\\Transfer_\\Identification"
            text of "-Or-"
            text of "BuildThe_\\Payment_\\Output_\\File_\\NameBySelectingTheValuesFollowedByAdditionalText"
        three column
            PrmPayGroup
            visible when (CashCodeGroup entered)
                PrmCashCodeGroup
            visible when (CashCodeGroup not entered)
                PrmCashCode
            PrmBankTransCode
        single column
            PrmFileName
            text of "*Note:FullFileNameIsLimitedTo50Characters*"
                bold
            blank line
            PrmOverrideDefaults
        visible when (PrmOverrideDefaults)
            header3 of "PaymentCreationDefaultOverrides"
            two column independent
                column1
                    PrmInclComments
                    PrmDocCurrency
                    PrmBalancedTape
                column2
                    PrmUseAp150SortOpt
                    PrmInclFaxEmail
        blank line
        single column
            PrmRunCashPaymentRegister
            PrmRunPaymentClose
            PrmRunPositivePayFileCreation
