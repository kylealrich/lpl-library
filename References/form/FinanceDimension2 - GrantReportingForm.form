GrantReportingForm is a Form
    title is "GrantReporting"
    Layout
        visible when (InEnterpriseStructure)
            visible when (IsExternalOrIntercompanyBilling)
                three column distributed
                    column1
                        GrantID
                            label is "GrantI\\D"
                            show up to 20 characters
                    column2
                        PrimeFlag
                    column3
                        PreAwardID
                paragraph
                    PrimeSponsor.Company
                        label is "PrimeSponsor"
                    PrimeSponsor.Customer
                        label is "ReceivableCustomer"
                        select is CustomerContextCompanySelect
                    PrimeSponsor.Customer.SearchName
                        no label
                        display as text
                header2 of "S\\E\\F\\AReporting"
                three column distributed
                    column1
                        SEFAFlag
                            label is "IncludeInS\\E\\F\\A"
                    column2
                        PrimeSponsorAwardNumber
                            show up to 15 characters
                    column3
                        paragraph
                            MajorCategory
                                show up to 15 characters
                            MajorCategory.Description
                                no label
                                display as text
                                show up to 15 characters
                three column distributed
                    column1
                        paragraph
                            CFDANumber
                                show up to 15 characters
                            CFDANumber.Description
                                no label
                                display as text
                    column2
                        paragraph
                            ProgramTitle
                                show up to 15 characters
                            ProgramTitle.Description
                                no label
                                display as text
                header2 of "F\\F\\RReporting"
                text of "RecipientShare"
                    header3
                three column distributed
                    column1
                        RequiredRecipientShare
                    column2
                        RecipientShareOfExpenditures
                header2 of "FederalProgramIncome"
                three column distributed
                    column1
                        FederalProgramIncome
                    column2
                        ProgramIncomeExpendedDeductionAlternative
                            label is "ExpendedDeductionAlternative"
                    column3
                        ProgramIncomeExpendedAdditionAlternative
                            label is "ExpendedAdditionAlternative"
                header2 of "IndirectExpense"
                three column distributed
                    column1
                        IndirectExpenseType
                            label is "Type"
                    column2
                        IndirectExpenseRate
                            label is "Rate"
                    column3
                        IndirectExpenseBaseAmount
                            label is "BaseAmount"
                three column distributed
                    column1
                        IndirectExpenseAmountCharged
                            label is "AmountCharged"
                    column2
                        IndirectExpenseFederalShare
                            label is "FederalShare"
