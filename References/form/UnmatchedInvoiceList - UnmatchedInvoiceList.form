UnmatchedInvoiceList is an ActionForm
    action is UnmatchedInvoiceList
    Layout
        header2 of "Main"
        single column
            Company
                select is Companies
        paragraph
            CpyProcLevel
                label is "ProcessLevel"
                select is ProcessLevels
            VenClass
                label is "VendorClass"
                select is VendorClass
        paragraph
            FrVendor
                label is "FromVendor"
                select is VendorName
            ToVendor
                label is "ToVendor"
                select is VendorName
        header2 of "DetailListing"
        single column
            CutOffDate
                label is "ReconciliationCutoffDate"
        paragraph
            FrDueDate
                label is "DueDateBegin"
            ToDueDate
                label is "DueDateEnd"
        header2 of "SummaryAging"
        paragraph
            AgeOption
                label is "AgingOption"
            AgeDate
                label is "As_ofDate"
        two column table
            column headers
                "FromDay"
                "ToDay"
            row headers
                "1"
                "2"
                "3"
                "4"
                "5"
            column1
                FromDays.AP252FromDaysGroup[1].FromDays
                    no label
                FromDays.AP252FromDaysGroup[2].FromDays
                    no label
                FromDays.AP252FromDaysGroup[3].FromDays
                    no label
                FromDays.AP252FromDaysGroup[4].FromDays
                    no label
                FromDays.AP252FromDaysGroup[5].FromDays
                    no label
            column2
                FromDays.AP252FromDaysGroup[1].ToDays
                    no label
                FromDays.AP252FromDaysGroup[2].ToDays
                    no label
                FromDays.AP252FromDaysGroup[3].ToDays
                    no label
                FromDays.AP252FromDaysGroup[4].ToDays
                    no label
                FromDays.AP252FromDaysGroup[5].ToDays
                    no label
        header2 of "Options"
        paragraph
            CurrencyOption
            ReportSequence
        paragraph
            VendorSequence
            Comments
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                UnmatchedInvoiceListReport_RDG
            column2 of 25%
                UnmatchedInvoiceListReport_XPT
