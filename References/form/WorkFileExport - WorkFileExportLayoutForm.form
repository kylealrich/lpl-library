WorkFileExportLayoutForm is a Form
    is primary
    title is "Export_fromWorkfile"
    Layout
        WorkDirectory
        header3 of "Select_aWorkfile"
        single column
            WorkFile
                select is WorkFileExportList
        header3 of "OutputFile"
        single column
            FileFormat
                display as horizontal radio buttons
            FileName
        header3 of "ExportOptions"
        two column
            CreateTemplate
            visible when (not CreateTemplate)
                FilterString
        DelimitersAndPatterns
