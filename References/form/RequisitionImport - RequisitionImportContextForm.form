RequisitionImportContextForm is a Form
    title is "RequisitionInterface"
    Layout
        visible when (not RequisitionExists)
            four column distributed
                RunGroup
                    label is "RunGroup"
                    show up to 10 characters
                visible when (not RecordExists)
                    paragraph
                        Company
                            show up to 10 characters
                        Company.Name
                            no label
                            display as text
                    RequisitionImport
                        show up to 10 characters
                visible when (RecordExists)
                    display as text
                        Company.RepresentativeText
                            label is "Company"
                        RequisitionImport
            four column distributed
                paragraph
                    Requester
                        show up to 10 characters
                    Requester.Name
                        no label
                        display as text
                paragraph
                    RequestingLocation
                        show up to 10 characters
                    RequestingLocation.Name
                        no label
                        display as text
                RequisitionDescription
                    show up to 20 characters
                CreationDate
        visible when (RequisitionExists)
            display as text
                four column distributed
                    RunGroup
                        label is "RunGroup"
                    Company.RepresentativeText
                        label is "Company"
                    RequisitionImport
                    RequisitionNumber
                four column distributed
                    Requester.Employee.FirstLastNameID
                        label is "Requester"
                    RequestingLocation.RequestingLocationNameID
                        label is "RequestingLocation"
                    RequisitionDescription
                    CreationDate
