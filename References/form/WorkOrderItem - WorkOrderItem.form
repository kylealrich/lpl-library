WorkOrderItem is a CompositeForm
    is primary
    title is "WorkOrder_<WorkOrder>_Item_<DerivedWorkOrderItem>"
    context form is WorkOrderItemContextForm
        background is theme color
    WorkOrderItem is a Panel
        title is "Item"
        form is ItemForm
    Details is a Panel
        visible when (DetailTracked)
        list is WorkOrderItemBinLotSerialRel.ComponentDetails
    Comments is a Panel
        list is WorkOrderItemComment set.WorkOrderItemCommentList
