BillingScheduleComposite is a CompositeForm
    is primary
    title is "BillingSchedule"
    context form is BillingScheduleContext
        background is theme color
    restrict action Create
    restrict action Delete
    Actions
        action is RecurringBilling
            label is "RecurringBilling"
            mouse over text is "RecurringBilling"
            hide when invalid
        action is save
            label is "_"
        action is refresh
            label is "_"
    Details is a Panel
        list is SubleaseBillingScheduleDetailByInvoiceDateRel.SubleaseBillingScheduleDetailList
    Miscellaneous is a Panel
        form is Miscellaneous
