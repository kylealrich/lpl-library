PayablesInvoiceDistributionCompositeForm is a CompositeForm
    is primary
    title is "<DistributionDerivedFormTitle>"
    context form is PayablesInvoiceDistributionContextForm
        background is theme color
    Actions
        navigation is PayablesInvoice
            label is "Go_\\To_\\Invoice_\\Header"
            valid when (PayablesInvoice.MatchProcessType.Expense)
        action is ReverseDistributionForUpdate
            hide when invalid
        action is CreateAndReleaseInvoice
            hide when invalid
        action is Create
            hide when invalid
        action is previous record
            label is "_"
            hide when invalid
        action is next record
            label is "_"
            hide when invalid
        action is save
            label is "_"
            hide when invalid
        action is save and new
            label is "_"
            hide when invalid
        action is Delete
            action icon is delete
            label is "_"
            hide when invalid
        action is refresh
            label is "_"
    AmountsPanel is a Panel
        title is "Distribution"
        form is GLTransAmountsForm
    Options is a Panel
        title is "Options"
        form is PayablesInvoiceDistributionForm
    IncomeWithholdingCodes is a Panel
        visible when (GlobalInvoiceWithholding)
        list is PayablesInvoiceWithholdingRel.InvoiceWithholdingList
    Comments is a Panel
        form is InvoiceDistributionComments
    Rebate is a Panel
        visible when (RebateEligible)
        form is Rebates
    RecurringJournalPanel is a Panel
        title is "RecurringEntry"
        visible when (RecurringJournalForm)
        form is RecurringJournalForm
    RecurringJournalBridgePanel is a Panel
        title is "RecurringJournal"
        visible when (RecurringJournalExists)
        list is RecurringJournalControlRel.RecurringJournalForBridgeList
    AmortizationDetails is a Panel
        visible when (AmortizationDetailsExists)
        list is ReconciliationDetailRel.AmortizationDetail
    UserFieldsPanel is a Panel
        title is "UserFields"
        form is UserFieldForm
    TaxDistributions is a Panel
        title is "TaxDistributions"
        visible when (HasLinkedTaxDistributions)
        list is LinkedTaxDistributionsRel.TaxDistributionsViewList
    JurisdictionResults is a Panel
        visible when (JurisResultsExists)
        list is JurisdictionResultsRel.InvoiceResponseShortList
    TaxAttributes is a Panel
        visible when (ShowOneSourceAttributesPanel)
        list is OneSourceUserAttributesRel.OneSourceAttributesForPayablesInvoice
    TaxLineItem is a Panel
        visible when (ShowOneSourceTaxLineItemPanel)
        list is TaxLineItemRel.TaxLineItemsForPayablesInvoice
