PracticalCapacityVolume is a Form
    is primary
    Layout
        two column distributed
            display as text
                column1
                    TrueCostAllocationControl
                    TrueCostAllocationRun
                column2
                    TrueCostCapacity
                    AccountingEntity
        header3 of "_"
        two column distributed
            display as text
                column1
                    visible when (TrueCostCapacity.HasAccountingUnitEntered)
                        AccountingUnit
                    visible when (TrueCostCapacity.HasProjectEntered)
                        Project
                    visible when (TrueCostCapacity.HasDimension1Entered)
                        FinanceDimension1
                    visible when (TrueCostCapacity.HasDimension2Entered)
                        FinanceDimension2
                    visible when (TrueCostCapacity.HasDimension3Entered)
                        FinanceDimension3
                    visible when (TrueCostCapacity.HasDimension4Entered)
                        FinanceDimension4
                column2
                    visible when (TrueCostCapacity.HasDimension5Entered)
                        FinanceDimension5
                    visible when (TrueCostCapacity.HasDimension6Entered)
                        FinanceDimension6
                    visible when (TrueCostCapacity.HasDimension7Entered)
                        FinanceDimension7
                    visible when (TrueCostCapacity.HasDimension8Entered)
                        FinanceDimension8
                    visible when (TrueCostCapacity.HasDimension9Entered)
                        FinanceDimension9
                    visible when (TrueCostCapacity.HasDimension10Entered)
                        FinanceDimension10
        header3 of "_"
        two column distributed
            column1
                display as text
                    TrueCostResourceUtilizationDriver
                    TrueCostResourceIntensityFactor
                    CapacityVolume
                    ActualVolume
            column2
                VolumeOverride
                EffectiveDate
                Update
