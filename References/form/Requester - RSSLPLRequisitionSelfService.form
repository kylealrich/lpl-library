RSSLPLRequisitionSelfService is a Form
    Layout
        valid when (not Company.PreventRequesterRSSOptionsUpdates)
            three column distributed
                button of "UseCompanySettings"
                    action is RSSLPLInitializeRSSOptions
                button of "SetToBasicUserSettings"
                    action is RSSLPLSetRSSOptionsToBasicUser
                button of "SetToPowerUserSettings"
                    action is RSSLPLSetRSSOptionsToPowerUser
            header2 of "Settings"
            three column distributed
                AllowAllCommentTypes
                AllowQuantityMultiplierOnShoppingList
                DisplayOnlyTransactionUOM
                EnableFavorites
                ContractsOnly
                    label is "ListOnlyContractItems"
                InternalOnly
                    label is "ListOnlyInternalItems"
                RSSHomePageTitle
                    label is "RSSHomePageTitle"
                SpecialItemValidation
                DuplicateItemValidation
                    label is "ValidateDuplicateItem"
            header2 of "DisplayOptions"
            three column distributed
                AccountingDisplayOption
                    label is "Accounting"
                AOCDisplayOption
                    label is "AddOnCharges"
                AssetDisplayOption
                    label is "Asset"
                AvailableQuantityDisplayUOM
                    label is "AvailableQuantityUOM"
                ConsignCodeDisplayOption
                    label is "BillCode"
                BuyerDisplayOption
                    label is "Buyer"
                ShowCatalogSearch
                    label is "CatalogSearch"
                CertificationRequiredDisplayOption
                    label is "CertificationRequired"
                DefaultCommentTypeDisplayOption
                    label is "CommentType"
                CommodityCodeDisplayOption
                    label is "CommodityCode"
                CompanyDisplayOption
                    label is "Company"
                CostOptionDisplayOption
                    label is "CostOption"
                CurrencyDisplayOption
                    label is "Currency"
                DeliverToDisplayOption
                    label is "DeliverTo"
                DeliveryMethodDisplayOption
                    label is "DeliveryMethod"
                DistributionCodeDisplayOption
                    label is "DistributionCode"
                DropShipDisplayOption
                    label is "DropShip"
                FillOrKillFlagDisplayOption
                FromCompanyDisplayOption
                    label is "FromCompany"
                    raise yellow alert when (RSSLPLIsFromCompanyDisplayOptionRequired)
                        mouse over text is "From_CompanyIsAMandatoryFieldWhenCreatingRequisitions"
                FromLocationDisplayOption
                    label is "FromLocation"
                    raise yellow alert when (RSSLPLIsFromLocationDisplayOptionRequired)
                        mouse over text is "From_LocationIsAMandatoryFieldWhenCreatingRequisitions"
                GenericNameDisplayOption
                    label is "GenericName"
                GLEncumbranceReferenceDisplayOption
                    label is "GLEncumbranceReference"
                GTINDisplayOption
                    label is "GTIN"
                InspectionRequiredDisplayOption
                    label is "InspectionRequired"
                EAMDescriptionDisplayOption
                    label is "ItemNote"
                visible when (not IsManufacturerDisplayOptionHiddenOrRequired)
                    ManufacturerDisplayOption
                        label is "Manufacturer"
                visible when (IsManufacturerDisplayOptionHidden)
                    ManufacturerDisplayOption
                        label is "Manufacturer"
                        raise yellow alert when (IsManufacturerDisplayOptionHidden)
                            mouse over text is "The_\\HideOptionIsNotSupportedInTheNew_\\RSS"
                visible when (IsManufacturerDisplayOptionRequired)
                    ManufacturerDisplayOption
                        label is "Manufacturer"
                visible when (not IsManufacturerNumberDisplayOptionHiddenOrRequired)
                    ManufacturerNumberDisplayOption
                        label is "ManufacturerNumber"
                visible when (IsManufacturerNumberDisplayOptionHidden)
                    ManufacturerNumberDisplayOption
                        label is "ManufacturerNumber"
                        raise yellow alert when (IsManufacturerNumberDisplayOptionHidden)
                            mouse over text is "The_\\HideOptionIsNotSupportedInTheNew_\\RSS"
                visible when (IsManufacturerNumberDisplayOptionRequired)
                    ManufacturerNumberDisplayOption
                        label is "ManufacturerNumber"
                BillOnlyDetailDisplayOption
                    label is "PatientProcedure"
                POCodeDisplayOption
                    label is "POCode"
                PriorityDisplayOption
                    label is "Priority"
                    raise yellow alert when (RSSLPLIsPriorityDisplayOptionRequired)
                        mouse over text is "PriorityIsAMandatoryFieldWhenCreatingRequisitions"
                ShowPunchout
                    label is "Punchout"
                PurchaseFromDisplayOption
                    label is "PurchaseFrom"
                    raise yellow alert when (IsPurchaseFromDisplayOptionRequired)
                        mouse over text is "The_\\RequiredOptionIsNotSupportedInTheNew_\\RSS"
                QuoteRequiredDisplayOption
                    label is "QuoteRequired"
                ReceivingDisplayOption
                    label is "Receiving"
                RequestedDeliveryDateDisplayOption
                    label is "RequestedDeliveryDate"
                RequestingLocationDisplayOption
                    label is "RequestingLocation"
                    raise yellow alert when (RSSLPLIsRequestingLocationDisplayOptionRequired)
                        mouse over text is "RequestingLocationIsAMandatoryFieldWhenCreatingRequisitions"
                RequisitionDescriptionDisplayOption
                    label is "RequisitionDescription"
                InventoryReturnsDisplayOption
                    label is "Returns"
                ServiceInfoDisplayOption
                    label is "ServiceInformation"
                ShippingNotesDisplayOption
                    label is "ShippingNotes"
                ShowShoppingList
                    label is "ShoppingLists"
                SingleDocToPODisplayOption
                    label is "SingleDocToPO"
                ShowSourcingEvent
                    label is "SourcingEvent"
                ShowSpecialServices
                    label is "Special\\/\\ServicePage"
                PurchaseClassesDisplayOption
                    label is "Special\\/\\ServicePurchaseClasses"
                StockOnHandDisplayOption
                    label is "StockOnHand"
                TaxInfoDisplayOption
                    label is "TaxInformation"
                    raise yellow alert when (IsTaxInfoDisplayOptionRequired)
                        mouse over text is "The_\\RequiredOptionIsNotSupportedInTheNew_\\RSS"
                UserFieldsDisplayOption
                    label is "UserFields"
                VendorDisplayOption
                    label is "Vendor"
                VendorItemDisplayOption
                    label is "VendorItem"
