CreateUsingCopyFromTemplateMainForm is a Form
    title is "CreateFromTemplate"
    restrict action Unreleased.UpdateRequisitionLineFillOrKill
    Layout
        paragraph
            Company
                display as required when (true)
            Company.Name
                no label
                display as text
        paragraph
            Requester
                display as required when (RequesterRequired)
                when value changed
                    refresh RequestingLocationRequired
            Requester.Employee.PresentationNameSnapshot
                no label
                display as text
            BaseCurrencyCode
                label is "Currency"
                display as text
        paragraph
            ProcurementTemplate
                select is ProcurementTemplateListByParticipants
        blank line
        paragraph
            RequestingLocation
                select is ActiveRequestingLocations
                display as required when (RequestingLocationRequired)
                when value changed
                    refresh FromCompanyLocationRequired
            RequestingLocation.Name
                no label
                display as text
        paragraph
            FromCompanyLocation.FromCompany
                label is "FromCompany"
                display as required when (FromCompanyLocationRequired)
            FromCompanyLocation.FromCompany.Name
                no label
                display as text
        paragraph
            FromCompanyLocation.FromLocation
                label is "FromLocation"
                display as required when (FromCompanyLocationRequired)
            FromCompanyLocation.FromLocation.Name
                no label
                display as text
        paragraph
            RequestedDeliveryDate
        paragraph
            TransactionCurrencyCode
                label is "TransactionCurrency"
        paragraph
            Vendor
            Vendor.VendorName
                no label
                display as text
        paragraph
            PurchaseFromLocation
                label is "PurchaseFrom"
            PurchaseFromLocation.VendorName
                no label
                display as text
        paragraph
            TransientCopyAllLines
                label is "CopyAllLines"
        blank line
        paragraph
            TransientAttachment.File
                label is "Attachment"
                display as required when (true)
            TransientCommentTitle
                label is "Title"
                display as required when (true)
