RunAutoMatch is an ActionForm
    action is RunAutoMatch
    Layout
        single column
            paragraph
                PrmCompanyGroup
                    label is "CompanyGroup"
                text of "Or"
                PrmCompany
                    label is "Company"
        single column
            paragraph
                PrmVendorRange.Begin
                    label is "FromVendor"
                PrmVendorRange.End
                    label is "ToVendor"
        single column
            paragraph
                PrmDateType
                    label is "DateType"
                PrmRunDate
                    label is "RunDate"
                PrmUseSystemDate
                    label is "UseSystemDate"
                        left of field
        two column distributed
            column1
                PrmInvoiceProcess
                    label is "InvoiceProcess"
            column2
                PrmMatchPoint
                    label is "MatchPoint"
        text of "Note:IfRunDateIsNotEnteredAndSystemDateIsNotChecked,AllInvoicesWillBeSelected"
            italics
        two column
            column1 of 33%
                PrmDaysBeforeRunDate
                    label is "DaysBeforeRunDate"
            column2 of 66%
                PrmDaysAfterRunDate
                    label is "DaysAfterRunDate"
        visible when (PrmInvoiceProcess.IncludePrepayInvoices)
            paragraph
                PrmPrepaymentInvoiceMatchRule
                    label is "PrepayInvoiceMatchRule"
                text of "RuleUsedForInvoicesWithPrepaymentMethodOfPrepayFromInvoice"
            paragraph
                PrmPrepaymentDetailMatchRule
                    label is "PrepayDetailMatchRule"
                text of "RuleUsedForInvoicesWithPrepaymentMethodOfEligibleForPrepay"
        visible when (PrmInvoiceProcess.PrepayInvoicesOnly)
            paragraph
                PrmPrepaymentInvoiceMatchRule
                    label is "PrepayInvoiceMatchRule"
                text of "RuleUsedForInvoicesWithPrepaymentMethodOfPrepayFromInvoice"
            paragraph
                PrmPrepaymentDetailMatchRule
                    label is "PrepayDetailMatchRule"
                text of "RuleUsedForInvoicesWithPrepaymentMethodOfEligibleForPrepay"
        text of "ExcludeVendors"
            bold
        paragraph
            PrmExcludeVendors.Vendor[1]
                no label
            PrmExcludeVendors.Vendor[2]
                no label
            PrmExcludeVendors.Vendor[3]
                no label
        paragraph
            PrmExcludeVendors.Vendor[4]
                no label
            PrmExcludeVendors.Vendor[5]
                no label
            PrmExcludeVendors.Vendor[6]
                no label
        paragraph
            PrmExcludeVendors.Vendor[7]
                no label
            PrmExcludeVendors.Vendor[8]
                no label
            PrmExcludeVendors.Vendor[9]
                no label
        text of "Note:SkipDateIsComparedAgainstCurrentDateToDetermineIfInvoiceIsSkipped"
            italics
        paragraph
            PrmSkipDate
                label is "SkipDate"
            PrmSkipDays
                label is "SkipDays"
            PrmSkipOnlyIfNoReceipt
                label is "SkipOnlyIfNoReceipt"
        paragraph
            PrmSummarize
                label is "ErrorSummaryOption"
