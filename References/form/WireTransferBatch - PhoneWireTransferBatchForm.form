PhoneWireTransferBatchForm is a CompositeForm
    title is "WireTransfer<DerivedWireTransferBatch>"
    context form is PhoneWireTransferBatchContext
        background is theme color
    InternalDetails is a Panel
        title is "Details"
        visible when (TransferType.Internal)
        list is WireTransferBatchDetail set.PhoneWireTransferBatchDetailInternalList
    ExternalDetails is a Panel
        title is "Details"
        visible when (TransferType.External)
        list is WireTransferBatchDetail set.PhoneWireTransferBatchDetailExternalList
