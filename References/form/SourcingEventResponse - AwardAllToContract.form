AwardAllToContract is an ActionForm
    action is Submitted.AwardAllAppendToContract
    title is "AwardToExistingContract"
    Layout
        display as text
            two column
                Company
                SourcingEvent.EventVersionString
                    label is "Event#"
            two column
                NotifiedSupplier.Supplier
                NotifiedSupplier.Supplier.SupplierName
            two column
                NotifiedSupplier.SupplierSourceId
                    label is "SupplierContact"
                NotifiedSupplier.SupplierSourceId.MainContact.FirstAndLastName
                    label is "SupplierContactName"
            single column
                first SourcingEventLineResponse set.SourcingEventLine.OutputType
        single column
            text of "<SelectedEventLinesMessage>"
                header3
                color of green
        single column
            ReasonCode
                label is "AwardReason"
                select is BuyerAwardReasonsSelect
            visible when (RequestToNegotiateTerms)
                TermsHaveBeenNegotiated
        two column
            Contract
                select is ContractsToAttachToEvents
            Contract.Name
        single column
            AwardQtyToLifetimeMaxQty
                label is "CopyAndReplaceAwardQuantityToLifetimeMaximumQuantity"
