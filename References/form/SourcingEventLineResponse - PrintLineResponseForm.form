PrintLineResponseForm is a SummaryForm
    title is "_"
    Layout
        blank line
        header2 of "Line<SourcingEventLine>:_<SourcingEventLine.Name>"
            show border color of black
        visible when (SourcingEventLine.Status.Cancelled)
            single column
                text of "**ThisLineHasBeenCancelled**"
                    header2
                    bold
                blank line
        single column
            SourcingEventLine.Description
        paragraph
            SourcingEventLine.ItemNumber
                label is "Item"
            SourcingEventLine.ItemDescription
                no label
            SourcingEventLine.ItemDescription2
                no label
        visible when (SourcingEventLine.HasThirdDescription)
            SourcingEventLine.ItemDescription3
                label is "LongItemDescription"
        paragraph
            SourcingEventLine.GlobalTradeItemNumber
            SourcingEventLine.GlobalTradeItemNumber.FullDescription
                no label
        paragraph
            SourcingEventLine.CommodityCode
                label is "CommodityCode"
                compact format
            SourcingEventLine.CommodityCode.Description
                no label
        three column
            SourcingEventLine.Quantity
            SourcingEventLine.UOM.UnitOfMeasure
            SourcingEventLine.RequestedDeliveryDate
        header2 of "_"
        blank line
        three column
            Quantity
                label is "BidQuantity"
            CurrencyResponseUnitPrice
            CurrencyExtendedPrice
                label is "ExtendedAmount"
        three column
            ResponseAmount.NoCharge
            ResponseAmount.NoBid
            DeliveryDate
        paragraph
            visible when (SourcingEventLine.DisplayVendorItem)
                VendorItem
                VendorItemDescription
                    no label
            visible when (not SourcingEventLine.DisplayVendorItem)
                VendorItemDescription
                    label is "ServiceDescription"
        paragraph
            GlobalTradeItemNumber
            GlobalTradeItemNumber.FullDescription
                no label
        single column
            ResponseComments
                label is "Comments"
                use text area
        blank line
        visible when (LineHasAddOnCharge)
            list is SourcingEventLineResponseAOCRel.SourcingEventLineResponseAOCPrint
            blank line
        visible when (SELQuestionResponseExists)
            list is SourcingEventLineQuestionResponse set.LineQuestionsResponsePrint
            blank line
        visible when (AlternatesExist)
            header2 of "AlternateResponses"
            visible when (Alt1Exists)
                blank line
                three column
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].Quantity
                        label is "BidQuantity"
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].CurrencyResponseUnitPrice
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].CurrencyExtendedPrice
                        label is "ExtendedPrice"
                three column
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].DeliveryDate
                paragraph
                    visible when (SourcingEventLine.DisplayVendorItem)
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].VendorItem
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].VendorItemDescription
                            no label
                    visible when (not SourcingEventLine.DisplayVendorItem)
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].VendorItemDescription
                            label is "ServiceDescription"
                paragraph
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].GlobalTradeItemNumber
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].GlobalTradeItemNumber.FullDescription
                        no label
                single column
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].ResponseComments
                        label is "Comments"
                        use text area
                    blank line
            visible when (Alt2Exists)
                header2 of "_"
                blank line
                three column
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].Quantity
                        label is "BidQuantity"
