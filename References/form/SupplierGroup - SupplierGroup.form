SupplierGroup is a Form
    title is "SupplierGroup"
    Layout
        header2 of "SupplierSetup"
        paragraph
            AllowDuplicateTaxIds
                label is "AllowDuplicateTaxInformation"
            visible when (DisplayVAT)
                text of "VATWillBeTestedForDuplicationOnBothCountry/\\jurisdictionCodeAndRegistrationNumber"
                    italics
            text of "WarningsForDuplicatesAreForInternalSupplierMaintenanceOnly,NotSupplierPortal"
                italics
        three column
            CommodityRegistrationLevel
            RequireCommodityRegistration
                label is "RequireCommodityCode"
            RequireSupplierDescription
        visible when (UseSupplierPortal)
            visible when (SupplierDialogAllowed)
                single column
                    CanCreateDialogWithSupplier
            two column
                DisplayGTINAndGLNInSupplierPortal
                RequireCertificationAttachment
                    label is "RequireAttachmentsForAllCertifications"
            single column
                UseConditionalQuestions
        header2 of "ValidationProcess"
        two column
            RequireNewSupplierValidation
                label is "ValidationRequiredForNewSuppliers"
            RequireUpdatedSupplierValidation
                label is "ValidationRequiredForUpdatedSuppliers"
        single column
            RequireEmailConfirmation
                label is "RequireAConfirmationEmailAddressToBeEntered"
        header2 of "SupplierRegistration"
        visible when (UseSupplierPortal)
            paragraph
                QuickRegistration
                    label is "EnableQuickRegistration"
                text of "OnlyRequiredFieldsWillDisplayOnRegistrationForm"
            header4 of "DisplayDuringRegistration"
            three column
                column1
                    visible when (not QuickRegistration)
                        AllowDiversitySelect
                            label is "DiversityCodes"
                        AllowCertificationSelect
                            label is "Certifications"
                    TaxCodeDisplay
                        label is "TaxCodes"
                column2
                    AllowCommoditySelect
                        label is "CommodityCodes"
                    visible when (not QuickRegistration)
                        visible when (AllowCommoditySelect)
                            visible when (AllowCertificationSelect)
                                AllowCommodityCodeCertification
                                    label is "CertificationsForCommodityCodes"
                    visible when (not QuickRegistration)
                        DisplayTypeForRegistration
                            label is "TypeFieldOnSupplierAddress"
                column3
                    visible when (not QuickRegistration)
                        AllowProxySelect
                            label is "ProxyNotifications"
                    AllowQuestionSelect
                        label is "Questions"
                    valid when (AllowBankInformation)
                        AllowBankInformationSelect
                            label is "BankInformation"
            header4 of "Other"
            single column
                SendForBuyerCreatedSupplier
                    label is "SendEmailToSuppliersWhoDidNotRegisterThemselves"
                AutoCompare
                TermAndCondition
                    label is "TermsAndConditions"
                    use text area of 8 lines
        header2 of "BankInformation"
        single column
            AllowBankInformation
                label is "AllowSupplierBankInformation"
            valid when (AllowBankInformation)
                AllowBankInformationSelect
                    label is "DisplayBankInformationDuringRegistration"
                RequireValidBankEntity
        visible when (AllowBankInformation)
            single column
                text of "MaskBankAccountAndCreditCardNumber:"
                    bold
            paragraph
                MaskBankInformationEntry
                    label is "ForEntry"
                MaskBankInformationDisplay
                    label is "ForDisplay"
        header2 of "EventProcessingOptions"
        two column
            column1
                visible when (UseSupplierPortal)
                    AllowNonAwardedView
                        label is "AllowNonAwardedSuppliersToViewAwardSummary"
                RequireNoBidReason
                    label is "RequireNoBidReasonCodesOnNoBidResponses"
                UseBidTabulationReport
                visible when (UseBidTabulationReport)
                    BidTabulationDefault
            column2
                text of "_"
                visible when (not UseIDMAndSupplierPortal)
                    text of "_"
                visible when (UseIDMAndSupplierPortal)
                    AllowAwardDetailsView
                        label is "AllowSuppliersToViewAwardDetailsReport"
                paragraph
                    UseIDM
                        label is "UseIDMForBidTabulationAndAwardDetailsReports"
                    visible when (UseIDM)
                        text of "AttachIDMTemplatesToEventTypes"
                            italics
        header2 of "CertificationAndDiversityAlerts"
        paragraph
            YellowAlert
            text of "DaysBeforeExpirationDate"
        paragraph
            RedAlert
            text of "DaysBeforeExpirationDate"
        header2 of "VendorLocationDefaulting"
        single column
            DefaultSupplierContactLocation
                label is "DefaultSupplierContactVendorLocationToContracts"
        visible when (UseSupplierPortal)
            header2 of "VendorCredentialing"
            single column
                SupplierGroupExtension.UseVendormate
                    label is "AllowSuppliersToPopulateRegistrationInformationFromVendormate"
