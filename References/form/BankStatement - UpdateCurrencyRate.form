UpdateCurrencyRate is an ActionForm
    action is UpdateCurrencyRate
    Layout
        header3 of "ExchangeRateForStatementDateWillBeUsedUnlessOverriden"
        single column
            CashManagementGroup.FinanceEnterpriseGroup.CurrencyTable
                label is "CorporateCurrencyTable"
                display as text
            visible when (AccountCurrencyDifferentFromBase)
                PrmOverrideBaseCurrencyRate
                    label is "OverrideBaseRateFrom<StatementCurrency>To<CashManagementGroup.Currency>"
            visible when (AccountCurrencyDifferentFromCompany)
                PrmOverrideCompanyCurrencyRate
                    label is "OverrideCompanyRateFrom<StatementCurrency>To<CashManagementAccount.LegalEntity.Currency>"
            visible when (AccountCurrencyDifferentFromLocation)
                PrmOverrideLocationCurrencyRate
                    label is "OverrideLocationRateFrom<StatementCurrency>To<CashManagementAccount.CashManagementLocation.Currency>"
            PrmRevalueUsingPeriodEndRate
                label is "RevalueUsingPeriodEndRate"
