OrderEntryContractStatusReport is an ActionForm
    action is OrderEntryContractStatusReport
    Layout
        single column
            Company
                select is Companies
        blank line
        paragraph
            FrOrderDate
                label is "FromDate"
            ToOrderDate
                label is "ToDate"
        two column
            Customer
                select is GroupSearchName
            Customer.Name
                no label
                display as text
        two column
            ContractNbr
                label is "Contract"
                compact format
                select is Contracts
            ContractNbr.Description
                no label
                display as text
        single column
            CutoffDate
                label is "ExpirationDateCutoff"
        single column
            ReportOption
                label is "CurrencyOption"
        two column
            ProcessLevel
                select is ProcessLevels
            ProcessLevel.Name
                no label
                display as text
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                OrderEntryContractStatusReportReport_RDG
            column2 of 25%
                OrderEntryContractStatusReportReport_XPT
