ManualEventLineResponse is a CompositeForm
    use for action Draft.ManualLineCreate
    use for action Draft.ManualLineUpdate
    title is "EventLineResponse"
    context form is ManualLineResponseContext
        background is theme color
    LineResponse is a Panel
        form is ManualLineResponse
    PriceBreaks is a Panel
        visible when (AllowQuantityPriceBreak)
        form is QuantityPriceBreaks
    AddOnCharges is a Panel
        visible when (LineHasAddOnCharge)
        list is SourcingEventLineResponseAOCRel.SourcingEventLineResponseAOCs
    LineQuestions is a Panel
        visible when (SELQuestionsOldExist)
        template is SourcingEventLine.SourcingEventLineQuestion set
            business class is SourcingEventLineQuestionResponse
                form is Question
