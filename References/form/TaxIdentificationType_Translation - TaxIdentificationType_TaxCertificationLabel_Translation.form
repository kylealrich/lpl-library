                display as text
        single column
            IsoLocale
                compact format
                raise yellow alert when (VerifyTranslations_TaxCertificationLabel)
                    mouse over text is untranslatable:"The base data has been changed but this translation has not been verified"
        single column
            blank line
            Base_BusinessClass.TaxCertificationLabel
                label is untranslatable:"Tax Certification Label"
            TaxCertificationLabel
                label is untranslatable:"Translated"
