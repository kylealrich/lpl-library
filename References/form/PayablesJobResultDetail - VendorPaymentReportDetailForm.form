        list is VendorPaymentSpendRel.VendorPaymentReportList
    Contracts is a Panel
        visible when (PayablesJobResult.ParametersVendorPaymentReportResult.IncludeContracts)
        list is VendorContractSpendRel.VendorPaymentReportList
