DataNavigationFlowDefinitionForm is a Form
    is primary
    title is "ProcessDefinitionForm"
    restrict action Delete
        when (FlowType.SystemDefined)
    Layout
        single column
            PfiFlowDefinition
            Description
            visible when (not FlowType.SystemDefined)
                FlowType
            IsFlowEnabled
            IsSubProcess
                display only
            Checkpoint
            ContinueWithCheckPointError
            LogBufferSize
            CaptureActivityTimes
            FlowLogLevel
            visible when (not FlowType.SystemDefined)
                CurrentFlowVersion
                    display only
            visible when (AutoRestartInXmlToggle)
                AutoRestartText
            NotifyByEmail
            Configuration
            EmailTo
            EmailCC
            EmailSubject
            EmailMessage
            VariableNames.VariableName[1]
                label is "VariableName1"
            VariableNames.VariableName[2]
                label is "VariableName2"
            VariableNames.VariableName[3]
                label is "VariableName3"
            VariableNames.VariableName[4]
                label is "VariableName4"
            VariableNames.VariableName[5]
                label is "VariableName5"
            VariableNames.VariableName[6]
                label is "VariableName6"
            VariableNames.VariableName[7]
                label is "VariableName7"
            VariableNames.VariableName[8]
                label is "VariableName8"
            VariableNames.VariableName[9]
                label is "VariableName9"
            VariableNames.VariableName[10]
                label is "VariableName10"
            ActCntSoftLimitEnabled
            ActCntSoftLimit
            ActCntHardLimitEnabled
            ActCntHardLimit
            ActCntEmailTo
            ActCntEmailFrom
            ErrorLimit
            visible when (AutoRestartInXmlToggle)
                ClassicAutoRestart
                    display only
                ClassicAutoRestartUpdateStatus
                    display only
            visible when (not AutoRestartInXmlToggle)
                ClassicAutoRestart
                    label is "AutoRestart"
