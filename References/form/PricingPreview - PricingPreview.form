PricingPreview is a CompositeForm
    is primary
    context form is PricingPreviewContext
        background is theme color
    Price is a Panel
        form is Price
    UnitOfMeasurePrices is a Panel
        visible when (IsMultipleUOM)
        form is UnitOfMeasurePrices
    Discounts is a Panel
        visible when (HasDiscountAndIsNoCharge)
        form is Discounts
    QuantityBreaksFromContract is a Panel
        title is "QuantityBreaks"
        visible when (DisplayPriceBreakFromContract)
        form is QuantityBreaksFromContract
    QuantityBreaksFromPromotion is a Panel
        title is "QuantityBreaks"
        visible when (DisplayPriceBreakFromPromotion)
        form is QuantityBreaksFromPromotion
    QuantityBreaksFromPriceList is a Panel
        title is "QuantityBreaks"
        visible when (DisplayPriceBreakFromPriceList)
        form is QuantityBreaksFromPriceList
