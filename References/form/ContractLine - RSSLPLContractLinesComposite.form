RSSLPLContractLinesComposite is a CompositeForm
    title is "ContractLine"
    called out actions only
    context form is RSSLPLContractLineContext
        background is theme color
    implements DashBoardView
    Actions
        navigation is RequesterShoppingPageNav
            action icon is undo
            label is "BackToShoppingPage"
    LineDetail is a Panel
        quad width
        double height
        form is ContractLineMain
