BuildSmartReconciliationQueue is an ActionForm
    action is BuildSmartReconciliationQueue
    title is "BuildSmartReconciliationQueue"
    Layout
        single column
            RunOption
            visible when (RunOption.Delete)
                text of "ThisOptionRemovesInvoicesOnTheMatchReconciliationMyWorkListPanel"
            visible when (RunOption.Update)
                text of "ThisOptionAddsInvoicesNotInMatchReconciliationListsToTheUnassignedPanel"
        visible when (RunOption.Delete)
            single column
                PrmAPClerk
                    label is "PayablesProcessor"
        single column
            PrmCompanyGroup
                label is "CompanyGroup"
            PrmCompany
                label is "Company"
            PrmVendorGroup
                label is "VendorGroup"
            PrmVendor
                label is "Vendor"
