TermAndConditionUpdateDraftView is a CompositeForm
    title is "RequestToUpdateTermAndCondition"
    context form is TermAndConditionUpdateDraftViewContext
    TermAndCondition is a Panel
        form is TermAndConditionUpdateDraftViewMain
    AdditionalTermLevels is a Panel
        visible when (NotLevelFour)
        list is ChildTermAndConditionRel.ChildTermsAndConditionsUpdateDraftViewApprove
    AttachmentsToBeAdded is a Panel
        visible when (DraftAttachmentsToBeAddedExist)
        list is TermAndConditionAddAttachmentDraftRel.DraftAddAttachmentUpdates
    AttachmentsToBeDeleted is a Panel
        visible when (DraftAttachmentsToBeDeletedExist)
        list is TermAndConditionDeleteAttachmentDraftRel.DraftDeleteAttachmentUpdates
    ExistingAttachments is a Panel
        visible when (TermAndConditionAttachmentExists)
        list is TermAndConditionAttachmentRel.PFTermAndConditionAttachments
