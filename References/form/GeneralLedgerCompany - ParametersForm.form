ParametersForm is a Form
    title is "Parameters"
    Layout
        two column distributed
            column1
                paragraph
                    first CashLedgerJobResultsPeriodEndDateRel.Company
                        label is "Company"
                    first CashLedgerJobResultsPeriodEndDateRel.Company.Name
                        no label
                        display as text
                paragraph
                    first CashLedgerJobResultsPeriodEndDateRel.CompanyGroup
                        label is "CompanyGroup"
                    first CashLedgerJobResultsPeriodEndDateRel.CompanyGroup.Description
                        no label
                        display as text
                first CashLedgerJobResultsPeriodEndDateRel.UpdateUnreleasedAndPendingApprovals
                    label is "UpdateUnreleasedAndPendingApprovals"
                        left of field
                first CashLedgerJobResultsPeriodEndDateRel.NewPostDate
                    label is "NewPostDate"
                first CashLedgerJobResultsPeriodEndDateRel.UpdateOption
                    label is "UpdateOption"
            column2
                first CashLedgerJobResultsPeriodEndDateRel.RunTime
                first CashLedgerJobResultDetailsRel.Status
                button of "<first CashLedgerJobResultDetailsRel.ReportMessage>"
                    view first CashLedgerJobResultDetailsRel.ErrorReport
                    display as link
