ParCartMassChanges is an ActionForm
    action is ParCartMassChanges
    Layout
        header2 of "Main"
        paragraph
            Company
            Company.Name
                no label
                display as text
        single column
            AllParLocs
                label is "AllParLocations"
            text of "\\or"
                bold
        paragraph
            ReportGrp
                label is "ReportGroup"
            ReportGrp.Name
                no label
                display as text
        paragraph
            ItlItem
                label is "Item"
            ItlItem.Description
                no label
                display as text
        single column
            UpdateFlag
                label is "Update"
        header2 of "ChangeField"
        single column
            BgrBinSeq
                label is "BinSequence"
            ItlPreferBin
                label is "ShelfLocation"
        paragraph
            ItlReorderPoint
                label is "ParLevel"
            text of "\\or"
                bold
            PercentChg
                label is "ChangePercent"
            PosNeg
                label is "%"
        paragraph
            ItlUom
                label is "UOM"
            ItlUom.Description
                no label
                display as text
        single column
            ItlChargeableFl
                label is "PatientCharge"
            ItlGlCat
                label is "GLCategory"
            ItlMinOrder
                label is "MinimumOrder"
        paragraph
            ItlManufacturer
                label is "ManufacturerInfo"
                compact format
            ItlManufacturerNumber
                label is "ManufacturerNumber"
        header2 of "LocationChange"
        two column
            column1
                header3 of "RecordSelection"
                paragraph
                    ItmVendorOld
                        label is "Vendor"
                    ItmVendorOld.VendorName
                        no label
                        display as text
                ItmPurchFrLocOld
                    label is "PurchaseFromLocation"
                paragraph
                    text of "\\or"
                        bold
                ItmReplFrCoOld
                    label is "ReplenishFromCompany"
                ItmReplFrLocOld
                    label is "ReplenishFromLocation"
                ItmReplFrBinOld
                    label is "ReplenishFromBin"
            column2
                header3 of "ChangeValue_to"
                ItmVendorNew
                    label is "Vendor"
                ItmPurchFrLocNew
                    label is "PurchaseFromLocation"
                paragraph
                    text of "\\or"
                        bold
                ItmReplFrCoNew
                    label is "ReplenishFromCompany"
                ItmReplFrLocNew
                    label is "ReplenishFromLocation"
                ItmReplFrBinNew
                    label is "ReplenishFromBin"
        header2 of "ItemChange"
        single column
            ItemChange
                label is "ChangeType"
                when value changed
                    ReplItemA =  blank
                    ReplItemB =  blank
                    AllCarts =  blank
                    AddItemA =  blank
                    WithItemB =  blank
                    AddItemB =  blank
                    AddBinSeq =  blank
                    AddPreferBin =  blank
                    AddReorderPoint =  blank
                    AddUom =  blank
                    AddChargeableFl =  blank
                    AddGlCat =  blank
                    AddPrimVendor =  blank
                    AddPurchFrLoc =  blank
                    AddFromCompany =  blank
                    AddFromLoc =  blank
                    AddFromBin =  blank
                    DelItem =  blank
                    FilterReplFromCompany =  blank
                    FilterReplFromLocation =  blank
                    FilterReplFromBin =  blank
                    FilterVendor =  blank
                    FilterPurchFrLoc =  blank
                    FilterFromCompany =  blank
                    FilterFromLocation =  blank
        visible when (ItemChange.Replace)
            four column independent
                text of "Replace"
                ReplItemA
                    label is "ItemA"
                text of "\\with"
                ReplItemB
                    label is "ItemB"
            single column
            blank line
            paragraph
                text of "Where"
                    bold
                FilterReplFromCompany
                    label is "ReplenishFromCompany"
                FilterReplFromLocation
                    label is "ReplenishFromLocation"
                FilterReplFromBin
                    label is "ReplenishFromBin"
            paragraph
                text of "\\Or_Where"
                    bold
                FilterVendor
                    label is "ReplenishmentVendor"
                FilterPurchFrLoc
                    label is "ReplenishmentPurchaseFromLocation"
                text of "\\And"
                    bold
                FilterFromCompany
                    label is "FromCompany"
                FilterFromLocation
                    label is "FromLocation"
        visible when (ItemChange.Add)
            two column distributed
                AllCarts
                    label is "AddItemA_toAllCarts"
                        left of field
                AddItemA
                    label is "ItemA"
            single column
                text of "\\or"
                    bold
            two column distributed
                WithItemB
                    label is "AddItemAEverywhereItemBExists"
                        left of field
                AddItemB
                    label is "Item_B"
            visible when (WithItemB.Yes)
                single column
                blank line
                paragraph
                    text of "Where"
                        bold
                    FilterReplFromCompany
                        label is "ReplenishFromCompany"
                    FilterReplFromLocation
                        label is "ReplenishFromLocation"
                    FilterReplFromBin
                        label is "ReplenishFromBin"
                paragraph
                    text of "\\Or_Where"
                        bold
                    FilterVendor
                        label is "ReplenishmentVendor"
                    FilterPurchFrLoc
                        label is "ReplenishmentPurchaseFromLocation"
                    text of "\\And"
                        bold
                    FilterFromCompany
                        label is "FromCompany"
                    FilterFromLocation
                        label is "FromLocation"
            three column distributed
                AddBinSeq
                    label is "Sequence"
                AddPreferBin
                    label is "ShelfLocation"
                AddReorderPoint
                    label is "ParLevel"
            three column distributed
                AddUom
                    label is "UOM"
                AddChargeableFl
                    label is "Charge"
                AddGlCat
                    label is "GLCategory"
                    show up to 10 characters
            single column
                blank line
            three column distributed
                AddPrimVendor
                    label is "PrimaryVendor"
                AddPurchFrLoc
                    label is "PurchaseFromLocation"
                    show up to 10 characters
            single column
                text of "\\or"
                    bold
            three column distributed
                AddFromCompany
                    label is "FromCompany"
                AddFromLoc
                    label is "FromLocation"
                    show up to 10 characters
                AddFromBin
                    label is "FromBin"
        visible when (ItemChange.Remove)
            single column
                blank line
                DelItem
                    label is "Delete_Item_A"
            single column
            blank line
            paragraph
                text of "Where"
                    bold
                FilterReplFromCompany
                    label is "ReplenishFromCompany"
                FilterReplFromLocation
                    label is "ReplenishFromLocation"
                FilterReplFromBin
                    label is "ReplenishFromBin"
            paragraph
                text of "\\Or_Where"
                    bold
                FilterVendor
                    label is "ReplenishmentVendor"
                FilterPurchFrLoc
                    label is "ReplenishmentPurchaseFromLocation"
                text of "\\And"
                    bold
                FilterFromCompany
                    label is "FromCompany"
                FilterFromLocation
                    label is "FromLocation"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                ParCartMassChangesReport_RDG
            column2 of 25%
                ParCartMassChangesReport_XPT
