MobileFinishedGoodsByproductForm is a Form
    title is "Byproducts"
    Layout
        two column table
            column1
                Company
                    display as text
                InventoryLocation
                    display as text
            column2
                Company.Name
                    label is "CompanyName"
                    display as text
                InventoryLocation.Name
                    label is "LocationName"
                    display as text
        paragraph
            FinishedGoodsByproduct.ByproductSequence
                label is "Sequence"
        paragraph
            Item
            Item.Description
                label is "Description"
                display as text
        paragraph
            Quantity
                label is "StandardQuantity"
