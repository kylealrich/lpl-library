DocumentInvoiceInterfaceResultContext is a Form
    title is "DocumentInvoiceInterfaceResult"
    Layout
        display as text
            three column distributed
                column1
                    PayablesInvoiceInterfaceResult
                        label is "ID"
                    RunTime
                    ValidateVendorItem
                    AutoCreateDetails
                column2
                    DerivedStatus
                        label is "Status"
                    Company
                    InvoiceProcess
                column3
                    PrmProcessType
                        label is "ProcessType"
                    PrmVendorClass
                    PrmVendor
                    visible when (IncompleteHasPendingRecords)
                        button of "Resubmit"
                            action is ResubmitDocumentInvoiceInterface
                    visible when (ShowAutoMatch)
                        text of "AutoMatchSubmittedForInterfacedInvoices"
                            bold
                            italics
                            color of green
                    visible when (NoRecordsToProcess)
                        text of "<NoRecordsToProcessMsg>"
                            bold
