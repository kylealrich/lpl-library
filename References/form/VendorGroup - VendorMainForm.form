                    paragraph
                        TransientPrepaymentHoldCode
                            label is "Prepayment"
                    paragraph
                        TransientReturnOnInvestmentHoldCode
                            label is "Return_onInvestment"
                column2
                    paragraph
                        TransientMaximumPaymentHoldCode
                            label is "MaximumPayment"
                    paragraph
                        TransientRetainageHoldCode
                            label is "Retainage"
                    paragraph
                        TransientMinimumPaymentHoldCode
                            label is "MinimumPayment"
                    paragraph
                        TransientTaxToleranceHoldCode
                            label is "TaxTolerance"
