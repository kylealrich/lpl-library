VendorMainForm is a Form
    Layout
        header2 of "General"
        four column distributed
            column1
                paragraph
                    visible when (OneTimeVendorClass entered)
                        OneTimeVendorClass
                        OneTimeVendorClass.Description
                            no label
                            display as text
                    visible when (OneTimeVendorClass not entered)
                        TransientOneTimeClass
                            label is "OneTimeVendorClass"
                ValidateOFAC
            column2
                LastVendor
                GlobalWithholding
            column3
                InvoiceNumberEdit
                VendorOffset
            column4
                effective date
                visible when (ValidateOFAC)
                    OFACCompareThreshold
                EditForPayToVendor
        header2 of "HoldCodes"
        visible when (RecordExists)
            two column distributed
                column1
                    paragraph
                        ManualPaymentHoldCode
                            label is "ManualPayment"
                        ManualPaymentHoldCode.Description
                            no label
                            display as text
                    paragraph
                        CreditBalanceHoldCode
