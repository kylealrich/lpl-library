CustomerInterfaceResultContext is a Form
    propagate as a drill
    title is "CustomerInterfaceResult"
    Layout
        two column distributed
            column1
                RunGroup
                    display as text
                visible when (not RunType.MassAdd)
                    Company
                        display as text
                visible when (RunType.MassAdd)
                    CustomerGroup
                        display as text
                RunTime
                    display as text
            column2
                CustomerInterfaceResult
                    label is "ResultID"
                Status
                    display as text
                visible when (NoRecordsToProcess)
                    text of "<NoRecordsToProcessMsg>"
                        bold
                visible when (IncompleteHasPendingRecords)
                    button of "Resubmit"
                        action is ResubmitCustomerInterface
                visible when (HasMassCustomersAddError)
                    text of "ErrorsExist;Check_Mass_Add_Customers_list_Error_Message_column"
                        bold
