TrueCostCapacityForm is a Form
    title is "PracticalCapacity"
    Layout
        two column distributed
            visible when (TrueCostCapacity entered)
                TrueCostCapacity
                    display as text
            visible when (TrueCostCapacity not entered)
                TrueCostCapacity
            Description
                show up to 60 characters
        header2 of "Dimensions"
        four column distributed
            column1
                DerivedAccountingEntity
                visible when (TrueCostConfiguration.HasAccountingUnit)
                    HasAccountingUnit
                visible when (TrueCostConfiguration.HasProject)
                    HasProject
                visible when (TrueCostConfiguration.HasDimension1)
                    HasDimension1
            column2
                visible when (TrueCostConfiguration.HasDimension2)
                    HasDimension2
                visible when (TrueCostConfiguration.HasDimension3)
                    HasDimension3
                visible when (TrueCostConfiguration.HasDimension4)
                    HasDimension4
            column3
                visible when (TrueCostConfiguration.HasDimension5)
                    HasDimension5
                visible when (TrueCostConfiguration.HasDimension6)
                    HasDimension6
                visible when (TrueCostConfiguration.HasDimension7)
                    HasDimension7
            column4
                visible when (TrueCostConfiguration.HasDimension8)
                    HasDimension8
                visible when (TrueCostConfiguration.HasDimension9)
                    HasDimension9
                visible when (TrueCostConfiguration.HasDimension10)
                    HasDimension10
        paragraph
            VolumeDimension
                select is TrueCostConfiguration.StructureRowRel.StructureRowSelectList
