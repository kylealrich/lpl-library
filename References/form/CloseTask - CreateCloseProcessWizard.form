CreateCloseProcessWizard is a WizardForm
    use for action CreateProcessWithWizard
    title is "DefiningANewProcess"
    summary form is CloseProcessWizardSummary
    show panel navigation
        last panel navigation is CloseManageProcessPageNav
            next button title is "Finish"
    show steps
    ProcessDetails is a Panel
        title is "Details"
        mouse over text is "ProvideProcessDetails"
        form is ProcessDetailsForm
    ProcessOwner is a Panel
        title is "Ownership"
        mouse over text is "DefineProcessOwnership"
        form is ProcessOwnerForm
    ProcessCompanyDefaults is a Panel
        title is "Company"
        valid when (PrmSpecificToCompany)
        mouse over text is "ProvideTheDefaultValuesForDeterminingPeriodEndDates"
        form is ProcessCompanyDefaultsForm
    ProcessCalendarDefaults is a Panel
        title is "Calendar"
        valid when (not PrmSpecificToCompany)
        mouse over text is "ProvideTheDefaultValuesForDeterminingPeriodEndDates"
        form is ProcessCalendarDefaultsForm
    ViewProcess is a Panel
        title is "ProcessDetails"
        valid when (IsProcess)
        list is RelatedCloseTaskRel.CloseTaskTreeViewWizardList
    ProcessDocuments is a Panel
        title is "Documentation"
        mouse over text is "AttachProcessDocumentation"
        list is ProcessDocumentationRel.ProcessDocumentationList
