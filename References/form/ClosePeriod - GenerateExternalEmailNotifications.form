GenerateExternalEmailNotifications is an ActionForm
    action is ExternalEmailNotifications
    title is "GenerateExternalEmailNotifications"
    Layout
        single column
            CloseManagementGroup
                label is "CloseManagementGroup"
                display only
            PrmClosePeriod
                label is "Period"
                compact format
                select is OpenPeriodSelectList
            blank line
            text of "ThisWillRunForThePeriodSpecifiedAndWillOnlyLookFor_ScheduledOr_In_ProcessTasksThatAreSetUpToUseExternalNotifications."
                italics
                align as label
