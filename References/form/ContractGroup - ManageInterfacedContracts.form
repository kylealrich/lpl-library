ManageInterfacedContracts is an ActionForm
    action is ManageInterfacedContractsFromGPOInformation
    title is "ProcessInterfaceUpdates"
    Layout
        paragraph
            ParmContractGroup
                select is ContractGroups
            ParmContractGroup.BusinessGroup.Description
                no label
                display as text
        paragraph
            ParmGPOSupplier
                select is GPOSuppliersSelect
            ParmGPOSupplier.SupplierName
                no label
                display as text
        two column
            column1
                text of "_"
                AutoUpdateContracts
                FindNewContractDates
                    label is "FindUpdatedContractHeaderDates"
                text of "Find_Updated_Contract_Header_DatesWillAutomaticallyBeRunIfEitherOfTheNextTwoOptionsAreSelected"
                    italics
                ProcessNewAndUpdatedItems
                    label is "FindAndUpdateItemCostAndDateChangesAndNewItems"
                visible when (ProcessNewAndUpdatedItems)
                    FindDiscontinuedItems
                visible when (AutoUpdateContracts)
                    visible when (ProcessNewAndUpdatedItems)
                        paragraph
                            TolerancePercent
                            Connector
                            ToleranceAmount
                            RejectAllCostIncreases
                                label is "ReviewAllItemsWithCostDifferences"
                        ProcessItemMasterOnly
                        UpdateCostsOnly
                        DefaultTolerance
                            label is "DefaultTolerancesFromClassificationsIfNotEntered"
                ProcessParticipantTierChanges
                    label is "FindTierChangesAndProcessIfDateIsEffective"
                visible when (AutoUpdateContracts)
                    visible when (FindNewContractDates)
                        LineUpdateOption
                            label is "OptionsToUpdateLinesWhenUpdatingContractDates"
                FindNewParticipants
                    label is "FindMappedMembersThatHaveBeenAddedAsParticipantsOnExistingContracts"
                FindVendorItemChanges
                visible when (AutoUpdateContracts)
                    RemoveDeletedItems
                        label is "DeleteItemsThatHaveBeenRemovedFromGPOContracts"
                FindChangedDistributorMarkups
                FindNewDistributors
                    label is "FindDistributorsThatHaveLinesAndMembersAssociated"
                FindChangedContractSuppliers
