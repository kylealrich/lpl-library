DT903BankExpenses is a Form
    title is "BankExpenses"
    Layout
        paragraph
            ReceivableCompanyRel.Company
            ReceivableCompanyRel.Name
                no label
                display as text
            ReceivableCompanyRel.Currency
                no label
                display as text
        paragraph
            CashCode
                select is CashCode
            CashCode.Description
                no label
                display as text
        paragraph
            BankTransactionCode
                label is "PaymentCode"
        paragraph
            TransactionDate
                label is "PostingDate"
            CustomerDraftBankCashingCharges
                no label
        paragraph
            BankExpenseAmount
        paragraph
            BankExpenseAccount
                compact format
        paragraph
            AdministrationExpenseAmount
        paragraph
            AdministrationExpenseAccount
                compact format
