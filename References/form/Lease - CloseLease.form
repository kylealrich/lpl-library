CloseLease is an ActionForm
    action is Released.Close
    Layout
        paragraph
            Company
                display as text
            Company.Name
                no label
                display as text
        paragraph
            Lease
                display as text
            Description
                no label
                display as text
        paragraph
            CloseDate
                label is "CloseDate"
        paragraph
            visible when (PromptForRenewal)
                header2 of "TermRenewalInformation?"
                CreateTermRenewal
                    label is "CreateATermRenewal"
                RenewalTerm
                PrmRenewalLease
