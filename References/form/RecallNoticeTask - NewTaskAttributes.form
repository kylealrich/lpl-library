NewTaskAttributes is a Form
    Layout
        visible when (NewOwnerTaskExists)
            single column
                text of "NewOwnerAlreadyHasA(n)<first NewOwnerRecallNoticeTaskRel.Status>Task.TasksWillBeMergedAndChangedToStatus=New"
                visible when (HasProductQuestionAnswers)
                    text of "QuestionsHaveAlreadyBeenAnsweredOnTheTaskBeingReassignedAndWillBeDeleted."
                visible when (NewOwnerHasProductQuestionAnswers)
                    text of "QuestionsHaveAlreadyBeenAnsweredOnTheNewOwners'_TaskAndWillBeDeletedIfTheyAreForAMatchingProduct."
                blank line
            header2 of "PleaseConfirmValuesForTasksToBeMerged"
            five column
                text of "_"
                text of "FromTask"
                    bold
                text of "ToTask"
                    bold
                text of "______"
                text of "NewValues"
                    bold
            five column
                text of "Owner"
                    bold
                text of "<Owner.FirstLastName>"
                text of "<first NewOwnerRecallNoticeTaskRel.Owner.FirstLastName>"
                text of "______"
                text of "<first NewOwnerRecallNoticeTaskRel.Owner.FirstLastName>"
            five column
                text of "Priority"
                    bold
                text of "<Priority>"
                text of "<first NewOwnerRecallNoticeTaskRel.Priority>"
                text of "______"
                NewPriority
                    no label
            five column
                text of "DueDate"
                    bold
                DueDate
                    no label
                    display as text
                first NewOwnerRecallNoticeTaskRel.DueDate
                    no label
                    display as text
                text of "______"
                NewDueDate
                    no label
        visible when (not NewOwnerTaskExists)
            single column
                blank line
                NewOwner.Employee.FirstLastName
                    label is "NewOwner"
                DueDate
                Priority
