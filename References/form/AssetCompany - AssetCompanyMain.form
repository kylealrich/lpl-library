AssetCompanyMain is a Form
    Layout
        visible when (not CompanyEntered)
            paragraph
                Company
        blank line
        five column
            column1
                TransactionReference
                ItemNumbering
                ComputeOption
                    label is "CalculateDepreciation"
                CalculateRemainingLife
                DisplayJournalEntries
                RequireUniqueTags
                HoldAssetInterface
                    label is "InterfaceHold"
            column2
                HistoryPostingDate
                DefaultPostingDate
                DefaultDepreciationExpenseProject
                    label is "DepreciationExpense<Company.FinanceEnterpriseGroup.ProjectLabel>"
                AllocateDepreciationExpense
                ConsolidateDisposalJournalEntry
                UseEventAccounts
                CopyUserFields
            column3
                PayablesDistributionWithTax
                InterfacePath
                Revalue
                Impairment
                AllowDistributionSplitting
                CreateBarCodes
                    label is "CreateBarcodes"
            column4
                AdjustmentPostHistory
                visible when (DepreciationApproval)
                    ApprovalCode
                        label is "DepreciationApprovalCode"
                visible when (not DepreciationApproval)
                    text of " "
                NeedsApproval
                    label is "ApprovalRequired"
                DepreciationApproval
                AssetReview
                visible when (AssetReview)
                    button of "ReviewSetup"
                        sub form is ReviewSetUpSubform
                        label is "ReviewSetup"
            visible when (FundAccounting)
                column5
                    visible when (IsBalancingTransfers)
                        BalanceTransfers
                    visible when (not IsBalancingTransfers)
                        BalanceTransfers
                            display only
