ModalCopyMultipleRequisitionLines is an ActionForm
    action is ModalCopyMultipleRequisitionLines
    submit label is "Ok"
    title is "AddAllSelectedItems_toRequisition"
    Layout
        paragraph
            PrmAddToRequisition
                no label
                display as horizontal radio buttons
        visible when (PrmAddToRequisition.AddToExistingRequisition)
            paragraph
                PrmCompany
                    label is "Company"
                PrmCompany.Name
                    no label
                    display as text
                PrmRequisition
                    label is "Requisition"
                    select is MyUnreleasedRequisitionsSelect
                    initial value is actor.agent(Employee).Employee.Requester.CurrentRequisitionForRQC
            header2 of "CopyLineOption"
            single column
                paragraph
                    text of "Distributions:"
                        header4
                    PrmCopyLineDistributionOption
                        no label
                        display as horizontal radio buttons
        visible when (PrmAddToRequisition.StartNewRequisition)
            visible when (RSSLPLDisplayHeaderInfo)
                header2 of "HeaderInformation"
            paragraph
                visible when (RSSLPLDisplayCompany)
                    PrmCompany
                        label is "Company"
                        display as required
                        when value changed
                            PrmFromCompanyLocation.FromCompany =  PrmRequestingLocation.FromCompanyLocation.FromCompany
                            PrmFromCompanyLocation.FromLocation =  PrmRequestingLocation.FromCompanyLocation.FromLocation
                    PrmCompany.Name
                        no label
                        display as text
                visible when (RSSLPLDisplayRequestingLocation)
                    PrmRequestingLocation
                        label is "RequestingLocation"
                        display as required
                        initial value is actor.agent(Employee).Employee.Requester.RequestingLocation
                        when value changed
                            PrmFromCompanyLocation.FromCompany =  PrmRequestingLocation.FromCompanyLocation.FromCompany
                            PrmFromCompanyLocation.FromLocation =  PrmRequestingLocation.FromCompanyLocation.FromLocation
                    PrmRequestingLocation.Name
                        no label
                        display as text
            paragraph
                visible when (RSSLPLDisplayFromCompany)
                    PrmFromCompanyLocation.FromCompany
                        label is "DeliverFromCompany"
                        display as required
                        initial value is actor.agent(Employee).Employee.Requester.RequestingLocation.FromCompanyLocation.FromCompany
                    PrmFromCompanyLocation.FromCompany.Name
                        no label
                        display as text
                visible when (RSSLPLDisplayFromLocation)
                    PrmFromCompanyLocation.FromLocation
                        label is "DeliverFromLocation"
                        select is RQInventoryLocations
                        display as required
                        initial value is actor.agent(Employee).Employee.Requester.RequestingLocation.FromCompanyLocation.FromLocation
                    PrmFromCompanyLocation.FromLocation.Name
                        no label
                        display as text
            paragraph
                visible when (DisplayRequestedDeliveryDate)
                    PrmRequestedDeliveryDate
                        label is "RequestedDeliveryDate"
                        display as required when (RequesterRel.RequireRequestedDeliveryDate)
                visible when (RSSLPLDisplayAllocationPriorityLevel)
                    paragraph
                        PrmAllocationPriorityLevel
                            label is "Priority"
                            initial value is RSSLPLAllocationPriorityLevelRel.AllocationPriorityLevel
                        PrmAllocationPriorityLevel.Description
                            no label
                            display as text
                visible when (RSSLPLDisplayPriority)
                    PrmAllocationPriority
                        label is "Priority"
                visible when (DisplayRequisitionDescription)
                    PrmRequisitionDescription
                        label is "RequisitionDescription"
                        display as required when (RequesterRel.RequireRequisitionDescription)
            visible when (RequesterRel.RequireShippingNotes)
                PrmShippingNotes
                    label is "ShippingNotes"
                    display as required when (RequesterRel.RequireShippingNotes)
            header2 of "CopyLineOption"
            single column
                paragraph
                    text of "Distributions:"
                        header4
                    PrmCopyLineDistributionOption
                        no label
                        display as horizontal radio buttons
            visible when (RSSLPLShowRequiredFields)
                header2 of "LineDefaults"
                two column distributed
                    visible when (RequesterRel.RequireDeliverTo)
                        PrmDeliverTo
                            label is "DeliverTo"
                            display as required when (RequesterRel.RequireDeliverTo)
                    visible when (RSSLPLRequireCommodityCode)
                        PrmCommodityCode
                            label is "CommodityCode"
                            display as required when (RequesterRel.RequireCommodityCodeDisplay)
                    visible when (RequesterRel.RequireBuyer)
                        PrmBuyer
                            label is "Buyer"
                            display as required when (RequesterRel.RequireBuyer)
                    visible when (RequesterRel.RequirePOCode)
                        PrmPOCode
                            label is "POCode"
                            display as required when (RequesterRel.RequirePOCode)
                    visible when (RequesterRel.RequireVendor)
                        PrmVendor
                            label is "Vendor"
                            display as required when (RequesterRel.RequireVendor)
