AssetAccountActivityComposite is a CompositeForm
    is primary
    title is "AssetAccountActivity"
    context form is AssetAccountBalances
        background is theme color
    PostingActivity is a Panel
        title is "AccountActivity"
        visible when (HasPostingTrans)
        list is AssetTransactionsRel.AssetBalanceListTransactions
    BookActivity is a Panel
        title is "AccountActivity"
        visible when (HasBookTrans)
        list is AnnualBookTransactionsRel.AssetBookBalanceListTransactions
