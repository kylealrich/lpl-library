MobilePurchaseOrderDashboard is a CompositeForm
    title is "PurchaseOrderDetails"
    context form is MobilePurchaseOrderDashboardContext
    implements DashBoardView
    HeaderDetails is a Panel
        form is PurchaseOrderHeader
    Addresses is a Panel
        form is PurchaseOrderAddresses
    Totals is a Panel
        form is PurchaseOrderAmountTotals
    LineDefaults is a Panel
        form is PurchaseOrderLineDefaults
    ProcedureInfomationDefaults is a Panel
        visible when (DisplayOfProcedureInfo)
        form is ProcedureInfoDefaults
    Retainage is a Panel
        form is PurchaseOrderHeaderRetainage
    Invoices is a Panel
        list is MatchedPurchaseOrderInvoicesRel.InvoicesForAPurchaseOrder
    Chargebacks is a Panel
        list is AnyMatchPurchaseOrderInvoicesWithChargebackByPORel.ChargebackAnalysisList
    MiscellaneousAddOnCharges is a Panel
        list is PurchaseOrderAddOnChargesRel.AddOnCostsForAPurchaseOrderDisplay
    SpreadAddOnCharges is a Panel
        list is PurchaseOrderSpreadAddOnCharge set.PhonePurchaseOrderSpreadAddOnCharges
    Encumbrances is a Panel
        list is GLCommitDrillRel.PODrillGLCommitList
    IssuesAndAcknowledgements is a Panel
        visible when (HasRevision)
        list is PurchaseOrderRevisionsRel.RevisionsforaPurchaseOrder
