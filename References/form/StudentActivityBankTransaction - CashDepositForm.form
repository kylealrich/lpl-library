CashDepositForm is a CompositeForm
    use for action Entered.CreateDeposit
    title is "Deposit"
    show panel control on left
    Actions
        action is Entered.CreateDeposit
            action icon is create
            hide when invalid
        action is Entered.Release
            action icon is release
            hide when invalid
        action is save
        action is save and new
        action is Entered.Delete
            action icon is delete
            hide when invalid
        action is next record
        action is previous record
        action is refresh
    BankTransaction is a Panel
        form is CreateDepositContextForm
    AssignedReceipts is a Panel
        visible when (BankTransactionType.Deposit)
        list is StudentActivityReceipt set.StudentActivityReceiptSelectList
            helper list is UnassignedReceiptsRel.ActivityReceipts
                action is Released.AssignReceipt
                    invoked.PrmBankAccount = StudentActivityBankAccount
                    invoked.PrmDepositNumber = StudentActivityBankTransaction
    AssignedReceiptBatches is a Panel
        visible when (UnreleasedDeposit)
        list is StudentActivityReceiptBatch set.BatchSelectedForDepositList
            helper list is UnassignedBatchReceiptsRel.BatchSelectList
                action is Released.AssignBatchToDeposit
                    invoked.PrmBankAccount = StudentActivityBankAccount
                    invoked.PrmDepositNumber = StudentActivityBankTransaction
