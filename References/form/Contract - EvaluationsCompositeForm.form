EvaluationsCompositeForm is a CompositeForm
    title is "Evaluations"
    context form is EvaluationInformation
    Actions
        action is CreateContractEvaluation
        navigation is ViewContract
        action is save
            label is "_"
    Evaluations is a Panel
        visible when (EvaluationExists)
        list is ContractPerformanceEvaluationRel.ContractEvaluationsPrimaryList
    EmailNotifications is a Panel
        list is EvaluationNotificationEmailRel.ContractNotificationEmails
