BillOfExchangePrintTapeCreate is an ActionForm
    action is BillOfExchangePrintTapeCreate
    Layout
        paragraph
            Company
                select is ARCompanies
            Company.Name
                no label
                display as text
        paragraph
            BankInstCode
                label is "PaymentCode"
                select is BankTransactionCodeList
            BankInstCode.Description
                no label
                display as text
        paragraph
            ProcessLevel
                select is ProcessLevels
            ProcessLevel.Name
                no label
                display as text
        two column distributed
            column1
                FrArDraftNbr
                    label is "FromB\\O\\E"
                    compact format
                    select is BillOfExchange
            column2
                ToArDraftNbr
                    label is "ToB\\O\\E"
                    compact format
                    select is BillOfExchange
        two column distributed
            column1
                paragraph
                    FrCustomer
                        label is "FromCustomer"
                        select is CustomerContextCompanySelectList
                    FrCustomer.Name
                        no label
                        display as text
            column2
                paragraph
                    ToCustomer
                        label is "ToCustomer"
                        select is CustomerContextCompanySelectList
                    ToCustomer.Name
                        no label
                        display as text
        two column distributed
            column1
                FrMaturityDate
                    label is "FromDueDate"
            column2
                ToMaturityDate
                    label is "ToDueDate"
        paragraph
            CreateSource
                label is "Source"
        paragraph
            DraftRef
                label is "B\\O\\EReference"
        paragraph
            CommentCd
                label is "MessageCode"
                select is StandardComments
        paragraph
            PrintOption
        two column distributed
            column1
                paragraph
                    FrReprintDrafts
                        label is "FromB\\O\\E"
                    FrReprintSuffix
                        no label
            column2
                paragraph
                    ToReprintDrafts
                        label is "ToB\\O\\E"
                    ToReprintSuffix
                        no label
        paragraph
            InclOption
                label is "IncludeHistoric\\,Dishonor"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                BillOfExchangePrintTapeCreateReport_RDG
                BillOfExchangePrintTapeCreateForm_RDG
            column2 of 25%
                BillOfExchangePrintTapeCreateReport_XPT
                BillOfExchangePrintTapeCreateForm_XPT
