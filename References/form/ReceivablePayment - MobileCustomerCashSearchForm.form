MobileCustomerCashSearchForm is a SearchForm
    title is "CustomerCashSearchForm"
    Layout
        single column
            Company
            Customer
                select is CustomerSelect
            ReceivablePaymentHeader
                label is "Batch"
            PaymentNumber
            DepositDateRange.Begin
                label is "FromDepositDate"
            DepositDateRange.End
                label is "ToDepositDate"
