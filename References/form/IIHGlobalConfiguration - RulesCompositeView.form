RulesCompositeView is a CompositeForm
    title is "Rules"
    show panel control on left
    restrict action ApplySystemDefault
    restrict action DailyDataEngine
    restrict action RecommendationsEngine
    Actions
        action is refresh
    ServiceLevel is a Panel
        list is ServiceLevelRulesOrderedRel.RuleTextList
    LeadTime is a Panel
        list is LeadTimeRulesOrderedRel.RuleTextList
    OrderDays is a Panel
        list is OrderDaysRulesOrderedRel.RuleTextList
    ReceivingDays is a Panel
        list is ReceivingDaysRulesOrderedRel.RuleTextList
    LowerAndUpperBounds is a Panel
        list is CutoffRulesOrderedRel.RuleTextList
    Exclusions is a Panel
        list is ExclusionRulesOrderedRel.RuleTextList
    PerpetualOrderPolicy is a Panel
        list is OptimizationMethodRulesOrderedRel.RuleTextList
    ParTargetDays is a Panel
        list is ParTargetDaysRulesOrderedRel.RuleTextList
