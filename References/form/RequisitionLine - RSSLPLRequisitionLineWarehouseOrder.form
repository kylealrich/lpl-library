RSSLPLRequisitionLineWarehouseOrder is a Form
    title is "WarehouseOrder"
    Layout
        display as text
            three column distributed
                column1
                    first WarehouseDemandLines2Rel.WarehouseDemandLine.LineNumber
                        label is "DemandLineNumber"
                    first WarehouseDemandLines2Rel.BackorderedQuantity
                    InProcessQuantity
                        label is "InProcess"
                column2
                    first WarehouseDemandLines2Rel.Quantity
                        label is "OrderQuantity"
                    KilledQuantity
                    DeliveredQuantity
                        label is "Delivered"
                column3
                    first WarehouseDemandLines2Rel.AllocatedQuantity
                    first WarehouseShipmentLinesRel.ShippedQuantity
                    ReturnQuantity
                        label is "Return"
