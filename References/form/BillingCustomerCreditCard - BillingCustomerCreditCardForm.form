BillingCustomerCreditCardForm is a Form
    is primary
    propagate as a drill
    Layout
        single column
            paragraph
                Company
                Company.Name
                    no label
                    display as text
            paragraph
                Customer
                    select is CustomerContextCompanySelectList
                Customer.Name
                    no label
                    display as text
            paragraph
                BillingElectronicPaymentType
                    label is "Type"
                BillingElectronicPaymentType.Description
                    no label
                    display as text
            BillingCustomerCreditCard
                label is "AccountNumber"
            ExpirationDate
            Active
            DefaultAccount
            EpCsc
                label is "CSC"
