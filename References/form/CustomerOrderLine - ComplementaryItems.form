ComplementaryItems is a Form
    restrict action CreateCustomerOrderReturnLine
    Layout
        paragraph
            first ItemCompanionsRel.Item
                display as text
            first ItemCompanionsRel.Item.Description
                no label
                display as text
        paragraph
            first ItemCompanionsRel.AssociatedItem.RelatedItem
                label is "ComplementaryItem"
                display as text
            first ItemCompanionsRel.DerivedAssociatedItemDescription
                no label
                display as text
        paragraph
            first ItemLocationForAssociatedItemRel.AvailableQuantity
                label is "AvailableQuantity"
                display as text
        paragraph
            SellUOM
                label is "UOM"
                select is ValidSellUnitsOfMeasure
        paragraph
            TransientOrderQuantity
                label is "OrderQuantity"
        paragraph
            UnitCost
        paragraph
            ExpectedShipDate
                label is "ExpectedShipDate"
        paragraph
            first ItemCompanionsRel.EffectiveDate
                label is "EffectiveDate"
                display as text
        paragraph
            first ItemCompanionsRel.ExpirationDate
                label is "ExpirationDate"
                display as text
