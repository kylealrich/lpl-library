ContractTierQualifierSetup is a CompositeForm
    is primary
    title is "ContractTierQualifiers"
    context form is TierQualifierContext
        background is theme color
    Main is a Panel
        form is TierQualifiersMain
    GMDNTerms is a Panel
        visible when (CanDisplayCodes)
        list is ContractTierQualifierCode set.GMDNTTermCodes
            helper list is GMDNPTCodeRel.GMDNPTCodesForTiers
                action is AttachToTierQualifier
                    invoked.ParmContractGroup = ContractGroup
                    invoked.ParmContract = Contract
                    invoked.ParmContractTier = ContractTier
                    invoked.ParmContractTierQualifier = ContractTierQualifier
    ManufacturerInformation is a Panel
        visible when (CanDisplayCodes)
        list is ContractTierQualifierCode set.ManufacturerQualifierCodes
            helper list is ManufacturersRel.ManufacturerCodesForTiers
                action is AttachToTierQualifier
    UNSPSC is a Panel
        visible when (CanDisplayCodes)
        list is ContractTierQualifierCode set.UnspscQualifierCodes
            helper list is UNSPSCCodeRel.UNSPSCCodesForTiers
                action is AttachToTierQualifier
    CommodityCode is a Panel
        visible when (CanDisplayCodes)
        list is ContractTierQualifierCode set.CommodityQualifierCodes
            helper list is CommodityCodeRel.CmCommodityCodesForTiers
                action is AttachToTierQualifier
    MajorPurchasingClass is a Panel
        visible when (CanDisplayCodes)
        list is ContractTierQualifierCode set.PurchasingClassQualifierCodes
            helper list is MajorClassRel.CMMajorPurchasingClassesForTiers
                action is AttachToTierQualifier
