AdvancedDunning is a Form
    Layout
        visible when (not ActionCd.ChangeACustomer)
            two column distributed
                column1
                    DunningLetterIndicator
                        label is "DunningLetterIndicator"
                            left of field
                    DunningProcessCode
                        select is DunningProcessingCodes
                    LanguageCode
                        label is "Locale"
                        compact format
                column2
                    AssessDunningFee
                    MultipleDunningLetters
                        label is "MultipleDunningLetters"
                            left of field
                    CreditAgencyName
            CreditAgencyPostalAddress
        visible when (ActionCd.ChangeACustomer)
            two column distributed
                column1
                    paragraph
                        DunningLetterIndicatorChangeIndicator
                            no label
                        DunningLetterIndicator
                            label is "DunningLetterIndicator"
                                left of field
                    paragraph
                        DunningProcessCodeChangeIndicator
                            no label
                        DunningProcessCode
                            select is DunningProcessingCodes
                    paragraph
                        LanguageCodeChangeIndicator
                            no label
                        LanguageCode
                            label is "Locale"
                            compact format
                column2
                    paragraph
                        AssessDunningFeeChangeIndicator
                            no label
                        AssessDunningFee
                    paragraph
                        MultipleDunningLettersChangeIndicator
                            no label
                        MultipleDunningLetters
                            label is "MultipleDunningLetters"
                                left of field
                    paragraph
                        CreditAgencynameChangeIndicator
                            no label
                        CreditAgencyName
            paragraph
                CreditAgencyCountryChangeIndicator
                    no label
                CreditAgencyPostalAddress.Country
            visible when (CreditAgencyPostalAddress.Country entered)
                two column distributed
                    column1
                        paragraph
                            CreditAgencyAddressLine1ChangeIndicator
                                no label
                            CreditAgencyPostalAddress.DeliveryAddress.AddressLine1
                                label is "Address1"
                        paragraph
                            CreditAgencyAddressLine2ChangeIndicator
                                no label
                            CreditAgencyPostalAddress.DeliveryAddress.AddressLine2
                                label is "Address2"
                        paragraph
                            CreditAgencyAddressLine3ChangeIndicator
                                no label
                            CreditAgencyPostalAddress.DeliveryAddress.AddressLine3
                                label is "Address3"
                        paragraph
                            CreditAgencyAddressLine4ChangeIndicator
                                no label
                            CreditAgencyPostalAddress.DeliveryAddress.AddressLine4
                                label is "Address4"
                    column2
                        paragraph
                            CreditAgencyCityChangeIndicator
                                no label
                            CreditAgencyPostalAddress.Municipality
                                label is "City"
                        paragraph
                            CreditAgencyStateProvinceChangeIndicator
                                no label
                            CreditAgencyPostalAddress.StateProvince
                                label is "State"
                        paragraph
                            CreditAgencyPostalCodeChangeIndicator
                                no label
                            CreditAgencyPostalAddress.PostalCode
                                label is "PostalCode"
                        paragraph
                            CreditAgencyCountyChangeIndicator
                                no label
                            CreditAgencyPostalAddress.County
                                label is "County"
