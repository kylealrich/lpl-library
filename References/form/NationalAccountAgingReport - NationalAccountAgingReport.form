NationalAccountAgingReport is an ActionForm
    action is NationalAccountAgingReport
    Layout
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        paragraph
            text of "Or"
        paragraph
            CompanyGroup
            CompanyGroup.Description
                no label
                display as text
        paragraph
            Customer
                select is GroupSearchName
            Customer.Name
                no label
                display as text
        header2 of "AgingOptions"
        two column distributed
            AcgAgeType
                label is "AgingDateMethod"
            AcgAgeCredits
                label is "AgeCredits"
        two column distributed
            AcgAgePymnt
                label is "AgePayments"
            AcgAgeDisputes
                label is "AgeDisputes"
        two column distributed
            TransDate
                label is "As_ofDate"
            FutureApps
                label is "QualifyFutureApplications"
        single column
            PeriodOf
                label is "Audit\\,Period"
        two column
            paragraph
                Period1
                    label is "CurrentDays"
            paragraph
                Period2
                    label is "Periods1\\-\\4"
                Period3
                    no label
                Period4
                    no label
                Period5
                    no label
        two column
            paragraph
                FutureDays
            paragraph
                Period6
                    label is "Periods5\\-\\7"
                Period7
                    no label
                Period8
                    no label
        two column distributed
            PrintDetail
            PrintExpHeading
                label is "ExpandedHeading"
        two column distributed
            XGainLoss
                label is "Gain\\,Loss"
            CustomerSeq
                label is "ReportSequence"
        two column distributed
            FormOpt
                label is "FormatOption"
            AmtsOpt
                label is "TruncateAmounts"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                NationalAccountAgingReportReport_RDG
            column2 of 25%
                NationalAccountAgingReportReport_XPT
