ProcedureInformationForm is a Form
    title is "ProcedureInformation"
    restrict action Processed.UpdateLineReturn
    restrict action MobileCopyRequisitionLine
    restrict action ModalCopyMultipleRequisitionLines
    restrict action NonModalCopyRequisitionLine
    restrict action Unreleased.RSSLPLUpdateRequisitionLine
    restrict action RSSLPLAddNewSpecialServiceToNewRequisition
    restrict action RSSLPLAddNewSpecialServiceToRequisition
    restrict action ModalCopyMultipleRequisitionLinesForSpecialAndService
    restrict action NonModalCopyRequisitionLineForSpecialAndService
    restrict action ModalCopyRequisitionLineForSpecialAndService
    restrict action MobileCopyRequisitionLineForSpecialAndService
    restrict action RSSLPLModalCopyLineOptionPromptForRecentlyRequestedItems
    restrict action RSSLPLModalCopyLineOptionPromptForSpecialAndService
    Layout
        visible when (PreferenceCardTransaction)
            display as text
                two column distributed
                    column1
                        paragraph
                            DerivedProcedureDate
                            DerivedProcedureTime
                        DerivedProcedureCase
                        DerivedPreferenceCardIDAndName
                        DerivedPreferenceCardProcedures
                        paragraph
                            DerivedPreferenceCardAlias
                            DerivedPreferenceCardService
                    column2
                        paragraph
                            DerivedPatientID
                            DerivedPatientName
                        DerivedPreferenceCardProviders
                        DerivedOpenQuantity
                        DerivedHoldQuantity
                        DerivedTotalQuantity
        visible when (not PreferenceCardTransaction)
            two column distributed
                column1
                    header2 of "Visit"
                    visible when (false)
                        TransientUpdatePatientProcedure
                    TransientVisitNumber
                        label is "VisitNumber"
                    TransientPatientID
                        label is "MedicalRecordNumber"
                    TransientCaseNumber
                        label is "CaseID"
                    paragraph
                        TransientProcedure
                            label is "Procedure"
                        TransientProcedureModifier
                            label is "Modifier"
                    TransientProcedureDate
                        label is "ProcedureDate"
                    TransientPhysicianName
                        label is "Practitioner"
                column2
                    header2 of "Patient"
                    TransientPatientLastName
                        label is "LastName"
                    TransientPatientFirstName
                        label is "FirstName"
                    TransientPatientMiddleName
                        label is "MiddleInitial"
                    paragraph
                        TransientBirthdate
                            label is "Birthdate"
                        TransientGender
                            label is "Gender"
                    TransientSocialSecurityNumber
                        label is "SSN"
                    TransientNurseCoordinator
                        label is "CoordinatingNurse"
            header2 of "Order"
            two column distributed
                column1
                    TransientSalesRepresentative
                        label is "SalesRepresentative"
                    TransientRequestor
                        label is "Requester"
                column2
                    TransientOrderNumber
                        label is "SupplierOrderNumber"
            header2 of "Item"
            two column distributed
                column1
                    paragraph
                        TransientUniqueDeviceId
                            label is "UDI"
                            display as text
                        TransientExpirationDate
                            label is "ExpirationDate"
                    TransientLot
                        label is "Lot"
                    TransientManufacturer
                        label is "ManufacturerCode\\/\\Division"
                        compact format
                    TransientUserDefinedField1
                        label is "UserDefined1"
                column2
                    TransientGTIN
                        label is "GTIN"
                    TransientSerialNumber
                        label is "Serial"
                    paragraph
                        TransientManufacturerNumber
                            label is "ManufacturerNumber"
                        TransientManufacturingDate
                            label is "Date"
                    TransientUserDefinedField2
                        label is "UserDefined2"
            paragraph
                button of "Delete"
                    visible when (IsDeleteAllowed)
                    action is DeletePatientProcedure
