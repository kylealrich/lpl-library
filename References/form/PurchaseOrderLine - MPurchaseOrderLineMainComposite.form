MPurchaseOrderLineMainComposite is a CompositeForm
    title is "LineDetail"
    context form is MPurchaseOrderLineCompositeContext
    restrict action FixReceivedQuantity
    restrict action Unreleased.Create
    restrict action Delete
    Actions
        action is Unreleased.ReleasePurchaseOrder
            action icon is release
            hide when invalid
        action is CancelLine
            hide when invalid
        action is Released.CancelLineRemaining
            hide when invalid
        action is previous record
            label is "_"
            hide when invalid
        action is next record
            label is "_"
            hide when invalid
        action is save
            label is "_"
    Detail is a Panel
        form is MPurchaseOrderLineMainDetail
    Options is a Panel
        form is PurchaseOrderLineOptions
    QuantityDetail is a Panel
        form is POLineQuantityDetailForm
    CostDetail is a Panel
        form is POLineCostForm
    ProcedureInformation is a Panel
        visible when (DisplayOfProcedureInfo)
        form is ProcedureInformationForm
    UserFields is a Panel
        form is UserFieldsForm
    Messages is a Panel
        form is MessageReviewsForm
    ItemAddOnCost is a Panel
        title is "AddOnCharges"
        list is PurchaseOrderAndLineAddOnCharge set.MobileAddOnCostsForAPurchaseOrderLine
    Sources is a Panel
        list is PurchaseOrderLineSource set.MobilePurchaseOrderLineSourceList
    Comments is a Panel
        title is "Comments<CommentCount>"
        list is PurchaseOrderLineComment set.PurchaseOrderLineCommentList
    Distributions is a Panel
        title is "<DerivedDistributionsPanelLabel>"
        visible when (not IsInventoryType)
        list is PurchaseOrderLineDistributionsRel.primary
    AcknowledgementMessages is a Panel
        visible when (HasAcknowledgementMessages)
        list is PurchaseOrderAcknowledgementMessageRel.LineAcknowledgementMessages
    Errors is a Panel
        visible when (LineErrorsExist)
        list is PurchaseOrderLineError set.MobilePurchaseOrderLineErrorList
