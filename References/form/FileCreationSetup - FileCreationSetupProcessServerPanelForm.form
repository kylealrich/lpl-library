FileCreationSetupProcessServerPanelForm is a Form
    title is "_"
    Layout
        single column
            FlowToProcessFile
            button of "<last WorkUnitRel.ExecutionStartDate>"
                link is WorkUnitNav
                label is "Last_Work_Unit:"
                display as link
                raise red alert when (last WorkUnitRel.Status.Failed)
                    mouse over text is "WorkUnitFailed"
            button of "<FailedWorkUnitCount>"
                link is FailedWorkUnitNav
                label is "NumberOfFailedWorkUnits:"
                display as link
