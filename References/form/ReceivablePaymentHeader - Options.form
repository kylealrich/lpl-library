Options is a Form
    Layout
        visible when (not Status.ReleasedBatch)
            two column distributed
                column1
                    TransientReceivableOperator
                        label is "Operator"
                column2
                    paragraph
                        ReceivableLockBox
                            label is "ReceivableLockbox"
                            select is ReceivableLockBoxRel.ReceivableLockBoxList
                        ReceivableLockBox.Name
                            no label
                            display as text
            two column distributed
                column1
                    paragraph
                        ReceivedNotAppliedReceivableGeneralLedgerCode
                            label is "ReceivedNotAppliedCode"
                        ReceivedNotAppliedReceivableGeneralLedgerCode.Description
                            no label
                            display as text
                column2
                    paragraph
                        PrepaymentReceivableGeneralLedgerCode
                            label is "PrepaymentCode"
                        PrepaymentReceivableGeneralLedgerCode.Description
                            no label
                            display as text
            header3 of "OptionalBankCharges"
            two column distributed
                column1
                    AdditionalAmount
                        label is "BankCharges"
                column2
                    AdditionalAmountAccount
                        compact format
            header3 of "CashLedgerTransactionData"
            two column distributed
                column1
                    CbUserField1
                        label is "1"
                column2
                    CbUserField2
                        label is "2"
            two column distributed
                column1
                    CbUserField3
                        label is "3"
                column2
                    CbUserField4
                        label is "4"
        visible when (Status.ReleasedBatch)
            display as text
                two column distributed
                    column1
                        TransientReceivableOperator
                            label is "Operator"
                    column2
                        paragraph
                            ReceivableLockBox
                                label is "ReceivableLockbox"
                                select is ReceivableLockBoxRel.ReceivableLockBoxList
                            ReceivableLockBox.Name
                                no label
                                display as text
                two column distributed
                    column1
                        paragraph
                            ReceivedNotAppliedReceivableGeneralLedgerCode
                                label is "ReceivedNotAppliedCode"
                            ReceivedNotAppliedReceivableGeneralLedgerCode.Description
                                no label
                                display as text
                    column2
                        paragraph
                            PrepaymentReceivableGeneralLedgerCode
                                label is "PrepaymentCode"
                            PrepaymentReceivableGeneralLedgerCode.Description
                                no label
                                display as text
                header3 of "OptionalBankCharges"
                two column distributed
                    column1
                        AdditionalAmount
                            label is "BankCharges"
                    column2
                        AdditionalAmountAccount
                            compact format
                header3 of "CashLedgerTransactionData"
                two column distributed
                    column1
                        CbUserField1
                            label is "1"
                    column2
                        CbUserField2
                            label is "2"
                two column distributed
                    column1
                        CbUserField3
                            label is "3"
                    column2
                        CbUserField4
                            label is "4"
