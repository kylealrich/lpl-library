VatEuropeanSalesCodeForm is a Form
    is primary
    title is "EuropeanCommunitySalesTaxCode"
    Layout
        single column
            FinanceEnterpriseGroup
                display as text
            TaxEntity
                display as text
            VatReportIDForCountry
                label is "VATReportID"
                display as text
            TaxCode
            VatType
                label is "VATType"
                display as text
                raise red alert when (not VatTypeIsIntraEEC)
                    mouse over text is "V\\A\\TTypeMust=Intra-\\E\\E\\CForThisTaxCode"
            GoodsOrServices
                display as text
            TriangulationCountry
                display as text
