PayablesHistoryPurge is an ActionForm
    action is PayablesHistoryPurge
    Layout
        paragraph
            PrmCompany
                label is "Company"
                select is Companies
            PrmCompany.Name
                no label
                display as text
        blank line
        header2 of "InvoicePurge"
        single column
            PrmPurgeInvoices
                label is "PurgeInvoices"
        single column
            PrmCompany.FinanceEnterpriseGroup.PurgeCutOffDate
                label is "CutoffDate"
                display as text
        paragraph
            PrmFromVendorInv
                label is "FromVendor"
                select is AllVendorName
            PrmToVendorInv
                label is "ToVendor"
                select is AllVendorName
        header2 of "VendorPurge"
        two column
            PrmPurgeOnetimeVens
                label is "PurgeOne\\-\\TimeVendors"
        single column
            PrmVenCutoffDate
                label is "CutoffDate"
        blank line
        two column
            PrmPurgeNoActVens
                label is "PurgeNoActivityVendors"
        paragraph
            PrmNoActBegCreateDte
                label is "BeginCreateDate"
            PrmNoActEndCreateDte
                label is "EndCreateDate"
        paragraph
            PrmFromVendor
                label is "FromVendor"
                select is AllVendorName
            PrmToVendor
                label is "ToVendor"
                select is AllVendorName
