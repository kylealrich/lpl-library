MobileCustomerContractContext is a Form
    title is "CustomerContractContext"
    restrict action Create
    restrict action Delete
    Layout
        single column
            paragraph
                Company
                    display as text
                Company.BillingCompany.Name
                    no label
                    display as text
            paragraph
                Customer
                    display as text
                Customer.Name
                    no label
                    display as text
            paragraph
                CustomerShipTo
                    label is "ShipTo"
                    display as text
                CustomerShipTo.Name
                    no label
                    display as text
            paragraph
                FranchiseContract
                    label is "Contract"
                    display as text
            paragraph
                Currency
