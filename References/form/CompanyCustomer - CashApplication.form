CashApplication is a Form
    Layout
        three column distributed
            column1
                paragraph
                    DiscountAdjustmentReason
                        select is ReceivableReasonCodeDiscountList
                    DiscountAdjustmentReason.Description
                        no label
                        display as text
                ApplicationDiscountGraceDays
                    label is "GraceDays"
                DetailChargebackDate
                    label is "DetailChargebackDates"
                PrintChargebacks
            column2
                AutomaticCashApplication
                AutomaticApplicationMethod
                    label is "AutomaticApplicationMethod"
                paragraph
                    ApplicationCode
                        label is "AutomaticApplicationCode"
                    ApplicationCode.Description
                        no label
                        display as text
                ReceivableProcessingMethod
                    label is "ProcessingMethod"
            column3
                AutomaticTransactionRemoval
                    label is "AutomaticTransactionRemoval"
                ApplyMixedSignedLineItemsAutomatically
                    label is "AutomaticallyApplyMixedSigned"
                ThirdPartyPayer
                    label is "ThirdPartyPayer"
        header2 of "ApplicationTolerances"
        header3 of "Maximum_\\Invoice_\\Shortage"
        three column table
            column headers
                "Amount"
                "Percent"
                "ReasonCode"
            row headers
                "1"
                "2"
                "3"
            column1
                MaximumAutoAdjustAmount.MaxAutoAmt[1]
                    no label
                MaximumAutoAdjustAmount.MaxAutoAmt[2]
                    no label
                MaximumAutoAdjustAmount.MaxAutoAmt[3]
                    no label
            column2
                MaxAutoPcts.MaxAutoPct[1]
                    no label
                MaxAutoPcts.MaxAutoPct[2]
                    no label
                MaxAutoPcts.MaxAutoPct[3]
                    no label
            column3
                paragraph
                    MaximumShortReasonCode.AutoReasCd[1]
                        no label
                        select is ReceivableReasonCodeOverShortAndChargebackDetailAndChargebackSummaryList
                    MaximumShortReasonCode.AutoReasCd[1].Description
                        no label
                        display as text
                paragraph
                    MaximumShortReasonCode.AutoReasCd[2]
                        no label
                        select is ReceivableReasonCodeOverShortAndChargebackDetailAndChargebackSummaryList
                    MaximumShortReasonCode.AutoReasCd[2].Description
                        no label
                        display as text
                paragraph
                    MaximumShortReasonCode.AutoReasCd[3]
                        no label
                        select is ReceivableReasonCodeOverShortAndChargebackDetailAndChargebackSummaryList
                    MaximumShortReasonCode.AutoReasCd[3].Description
                        no label
                        display as text
        header3 of "Maximum_\\Payment_\\Overage"
        three column table
            column headers
                "Amount"
                "Percent"
                "ReasonCode"
            row headers
                "1"
                "2"
                "3"
            column1
                MaximumOverageAmount
                    no label
            column2
                MaximumOveragePercent
                    no label
            column3
                paragraph
                    MaximumOverageReasonCode
                        no label
                        select is ReceivableReasonCodeOSList
                    MaximumOverageReasonCode.Description
                        no label
                        display as text
