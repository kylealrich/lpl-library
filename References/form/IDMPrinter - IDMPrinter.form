IDMPrinter is a Form
    is primary
    title is "IDMPrinter<IDMPrinter>"
    Actions
        action is PrintTestPage
            action icon is print
        action is SetAsDefaultPrinter
            action icon is apply
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is save
            label is "_"
            mouse over text is "Save"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
            mouse over text is "Refresh"
    Layout
        single column
            visible when (Type.GCP)
                PrinterID
                    display as text
                Description
                    display as text
            visible when (Type.Email)
                PrinterName
                Email
                Description
            visible when (Type.IEP)
                PrinterName
                    display as text
                PrinterID
                    display as text
                LocationName
                    label is "Location"
                    display as text
            Type
                display as text
            Active
            ExistsInIDM
                display only
