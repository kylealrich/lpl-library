SELSupplierQuestionsPortalNav is a CompositeForm
    title is "Event<SourcingEvent.RepresentativeText>,Line<SourcingEventLine>"
    called out actions only
    Actions
        navigation is SourcingEventLineReturnPortal
            action icon is transfer
            label is "ReturnToEventLineResponse"
        action is save
            action icon is save
            hide when invalid
        action is save and close
            action icon is save
            hide when invalid
        action is refresh
            label is "_"
    QuestionResponses is a Panel
        visible when (SELQuestionsOldExist)
        mouse over text is "<SourcingEvent.Company.QuestionResponseText>"
        panel header is "<SourcingEvent.Company.QuestionResponseText>"
        template is SELSupplierQuestionRel
            business class is SourcingEventQuestionResponse
                form is SupplierQuestion
    QuestionResponses2SP is a Panel
        visible when (SELQuestionsNewExist)
        mouse over text is "<SourcingEvent.Company.QuestionResponseText>"
        panel header is "<SourcingEvent.Company.QuestionResponseText>"
        template is SELSupplierQuestionPortalRel
            business class is SELSupplierQuestionResponse
                form is Question
