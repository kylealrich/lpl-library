CashLedgerTransactionForm is a CompositeForm
    is primary
    title is "CashLedgerTransaction<TransactionNumber>"
    context form is CashLedgerTransactionContext
        background is theme color
    Actions
        action is Unreleased.Release
            label is "Release"
            hide when invalid
        action is Unreleased.SubmitForApproval
            label is "SubmitForApproval"
            hide when invalid
        action is Unreleased.RemoveFromBatch
            label is "RemoveFromBatch"
            hide when invalid
        action is ReleaseAmendedDistributions
            label is "ReleaseAmendedDistributions"
            hide when invalid
        action is Open.StopPayment
            label is "StopPayment"
            hide when invalid
        action is Open.Void
            label is "Void"
            hide when invalid
        action is Open.Reconcile
            label is "Reconcile"
            hide when invalid
        action is Open.ManualStaleDate
            label is "ManualStaleDate"
            hide when invalid
        action is Reconciled.Reopen
            label is "Reopen"
            hide when invalid
        action is Voided.Reopen
            label is "Reopen"
            hide when invalid
        action is StopPayment.Reopen
            label is "Reopen"
            hide when invalid
        action is Reconciled.ReopenTransferDetailOrHeader
            label is "Reopen"
            hide when invalid
        action is Voided.ReopenTransferDetailOrHeader
            label is "Reopen"
            hide when invalid
        action is StopPayment.ReopenTransferDetailOrHeader
            label is "Reopen"
            hide when invalid
        action is StaleDated.ManualEscheat
            label is "ManualEscheat"
            hide when invalid
        action is StaleDated.Reopen
            label is "Reopen"
            hide when invalid
        action is StaleDated.Void
            label is "Void"
            hide when invalid
        action is StaleDated.StopPayment
            label is "StopPayment"
            hide when invalid
        action is PendingStopPayConfirmation.Reopen
            label is "Reopen"
            hide when invalid
        action is PendingStopPayConfirmation.StopPayment
            label is "StopPayment"
            hide when invalid
        action is ApplyAdditionalAllocations
            label is "ApplyAdditionalCashAllocations"
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is Unreleased.Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is save
            label is "_"
        action is Unreleased.Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    Main is a Panel
        form is MainForm
    Comment is a Panel
        title is "Comments"
        form is Comment
    TransactionData is a Panel
        form is TransactionData
    CurrencyAmounts is a Panel
        title is "CurrencyAmounts"
        form is CurrencyAmountsForm
    UserFieldsPanel is a Panel
        title is "UserFields"
        form is UserFieldForm
    BankInformation is a Panel
        form is BankInformation
    CashLedgerGLDistribution is a Panel
        title is "Distributions"
        visible when (DistributionsAreApplicable)
        list is CashLedgerGLDistributionsRel.TransactionGLDistributionList
    FundPaymentDistributions is a Panel
        visible when (IsFundCashPaymentDistribution)
        list is FundCashPaymentDistributionsRel.TransactionGLDistributionList
    PaymentRecords is a Panel
        visible when (CanViewPaymentRecords)
        list is CashLedgerPayablesPaymentsPayGroupRel.PaymentRecordsRejectionList
    RelatedInvoices is a Panel
        visible when (HasApPayments)
        list is PayablesInvoicePaymentRel.PayablesInvoicePaymentInqueryList
    RelatedDeposits is a Panel
        visible when (IsReceivableDeposit)
        list is ReceivablePaymentHeaderDepositsRel.ReceivablePaymentHeaderList
    RelatedDepositAdjustment is a Panel
        visible when (IsRTMReceivablePayment)
        list is ReceivablePaymentHeaderAdjustmentsRel.ReceivablePaymentHeaderList
    EFTExtractRecords is a Panel
        visible when (IsElectroniFundsExtractType)
        list is CashLedgerElectronicFundsTransferTransactionRel.EFTPaymentRejectionList
    VarianceAdjustments is a Panel
        title is "VarianceAdjustments"
        visible when (IsCashLedgerReconciliationAdjustmentExist)
        list is CashLedgerReconciliationAdjustmentRel.primary
    ApprovalComments is a Panel
        visible when (HasApprovalComments)
        list is CashLedgerTransactionReasonCodeUsage set.CashLedgerTransactionReasonCodeUsageList
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
    BudgetErrors is a Panel
        title is "BudgetErrors<DerivedHeaderBudgetErrorCount>"
        visible when (HeaderBudgetEditErrorExists)
        list is HeaderBudgetEditErrorRel.primary
    WarningBudgetErrors is a Panel
        title is "WarningBudgetErrors<DerivedWarningBudgetErrorCount>"
        visible when (DistributionBudgetEditErrorExists)
        list is DistributionBudgetEditErrorRel.primary
    Errors is a Panel
        title is "Errors<DerivedCashLedgerTransactionErrorCount>"
        visible when (CashLedgerTransactionErrorsExist)
        list is CashLedgerTransactionError set.CashLedgerTransactionErrorList
    TransactionDetails is a Panel
        visible when (HasTransactionDetails)
        list is CashLedgerTransactionDetailRel.CashLedgerTransactionDetails
