SmartReconAssignments is a CompositeForm
    title is "<EmployeeRel.Name.PreferredFirstAndLastName>'sSmartReconciliationAssignments"
    context form is FinanceResourceAdminContextForm
        background is theme color
    Company is a MultiListPanel
        list is SmartReconCompanyGroupsRel.APClerkSmartReconCompanyGroups
        list is SmartReconCompaniesRel.APClerkSmartReconCompanies
            helper list is PayablesCompanyRel.APCompanies
                action is CreateAPClerkSmartReconCompany
    Vendor is a MultiListPanel
        list is SmartReconVendorsRel.APClerkSmartReconVendors
            helper list is VendorsRel.VendorSelect
                action is CreateAPClerkSmartReconVendor
        list is SmartReconVendorClassesRel.APClerkSmartReconVendorClasses
            helper list is VendorClassRel.VendorClasses
                action is CreateAPClerkSmartReconVendorClass
        list is SmartReconVendorRangesRel.APClerkSmartReconVendorRanges
