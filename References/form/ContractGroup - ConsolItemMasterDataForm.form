ConsolItemMasterDataForm is a Form
    title is "ContractLocationsAndConsolidatedItemLevelMasterDataSetup"
    Actions
        action is save
        action is CreateInitialConsolidatedItemLevelMasterDataForItems
            hide when invalid
        action is CreateInitialConsolidatedItemLevelMasterDataForContractLines
            hide when invalid
    Layout
        visible when (StoreConsolidatedItemAndLocation)
            visible when (not InitialConsolidatedDataCreatedForItem)
                text of "Run_Create_Initial_Consolidated_Item_Level_Master_Data_For_Items"
                    bold
                    color of green
            visible when (InitialConsolidatedDataCreatedForItem)
                visible when (not InitialConsolidatedDataCreatedForContractLine)
                    text of "Run_Create_Initial_Consolidated_Item_Level_Master_Data_For_Contract_Lines"
                        color of green
        three column distributed
            StoreConsolidatedItemAndLocation
            EnableClinicalSystemUseLocations
        visible when (EnableClinicalSystemUseLocations)
            paragraph
                DefaultParticipantGroupForContractLocations
                    select is ParticipantSelectForLocations
                LineLevelForContractLocation
            two column distributed
                AutoAddFromParticipantGroup
                AutoAddFromNewLocations
            three column distributed
                DoNotAutoSetToClinical
                DoNotUseRequestingLocations
                DoNotUseParLocations
