CreateEventFromTemplateOverrideCompany is an ActionForm
    action is Draft.CreateEventFromTemplateOverrideCompany
        on completion navigate to ViewNewEvents
    title is "CreateFromTemplateForOverrideCompany"
    Layout
        header2 of "NewTemplateValues"
        two column distributed
            paragraph
                Company
                Company.Name
                    no label
                    display as text
            paragraph
                TemplateEvent
                    label is "FromEventTemplate#"
                    select is TemplatesForSelect
                TemplateEvent.Name
                    no label
                    display as text
        paragraph
            OverrideCompany
                label is "OverrideCompany"
        single column
            NewName
                label is "Name"
            NewDescription
                label is "Description"
                use text area
            NewEventReference
                label is "Reference"
        paragraph
            NewSourcingEventType
                label is "Type"
                select is SupplierEventTypes
            NewSourcingEventType.Description
                no label
                display as text
        paragraph
            NewBuyer
                label is "Buyer"
            NewBuyer.Employee.PresentationNameSnapshot
                no label
                display as text
        paragraph
            NewCategory
                label is "Category"
                select is BuyerCategories
            NewCategory.Description
                no label
                display as text
        paragraph
            NewSubCategory
                label is "SubCategory"
                select is BuyerSubCategories
            NewSubCategory.Description
                no label
                display as text
        two column
            NewCurrencyCode
            NewAllowResponseInOtherCurrency
        two column
            NewPostingOptions
                label is "SupplierPortalPostingOptions"
            NewDisplayPlanHoldersList
                label is "DisplayPlanHoldersList"
        two column
            NewCreateAmendments
                label is "CreateAmendments"
            NewAllowSupplierQandA
        single column
            visible when (UseBidTabulation)
                NewDisplayBidTabulation
        two column
            NewRequireBidBond
                label is "RequireBidBond"
            NewBidBondPercent
                label is "BidBondPercent"
        two column
            NewTwoStepBidding
                label is "TwoStepBidding"
            NewBestAndFinalOffer
                label is "Best_andFinalOffer"
        single column
            NewVendorItemProcessing
                label is "VendorItemProcessing"
        single column
            NewOnlyNotifyResponders
                label is "SendAmendmentNotificationsOnly_toRespondingSuppliers"
        header2 of "NewEventDates"
        single column
            NewInformalQuote
                label is "DatesNotRequiredOnEvent"
        visible when (not NewInformalQuote)
            two column
                NewPreviewDate
                    label is "Preview"
                NewOpenDate
                    label is "Open"
                NewCloseDate
                    label is "Close"
                visible when (NewAllowSupplierQandA)
                    NewQandAOpenDate
                    NewQandACloseDate
                NewDisputeCloseDate
                    label is "DisputeClose"
        header2 of "NewEventLineDates"
        single column
            NewRequestedDeliveryDate
                label is "RequestedDeliveryDate"
        header2 of "DefaultLineAwardInformation"
        two column
            NewDefaultContractType
                label is "ContractType"
            NewDefaultContractSubType
                label is "Subtype"
        two column
            NewDefaultContractClass
                label is "ContractClassification"
            NewDefaultContractSubClass
                label is "Subclassification"
        two column
            NewDefaultEffectiveDate
                label is "EffectiveDate"
            NewDefaultExpirationDate
                label is "ExpirationDate"
        header3 of "FieldsForStandingOutputTypeOnly"
        two column
            NewDefaultScheduleFrequency
                label is "ScheduleFrequency"
            NewDefaultMaxReleaseNumber
                label is "MaximumReleaseNumber"
        paragraph
            NewDefaultParticipant.Company
                label is "ParticipantCompany"
            NewDefaultParticipant.Location
                label is "Location"
        header2 of "CopyOptions"
        single column
            CopySelectAll
                label is "SelectAll"
        three column
            CopyEventQuestions
                label is "Questions"
            CopyArticlesAndTerms
                label is "ArticlesAndTerms"
            CopyEventMeetings
                label is "Meetings"
            CopyEventAttachments
                label is "Attachments"
            CopyEventContacts
                label is "Contacts"
            CopyEventComments
                label is "Comments"
            CopyBuyerWeighting
                label is "BuyerWeighting"
            CopyEventLines
                label is "TemplateLines"
            CopyEventLineQuestions
                label is "LineQuestions"
            CopyEventLineDistributions
                label is "LineDistributions"
            CopyEventLineComments
                label is "LineComments"
            CopyEventLineAttachments
                label is "LineAttachments"
            CopyEventLineAOC
                label is "LineAOC"
