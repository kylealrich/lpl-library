MobileBankStatementDistributionLineForm is a Form
    title is "WireTransferDistribution"
    Layout
        display as text
            two column table
                column1 of 50%
                    WireTransferBatch.FinancialInstitution.FinancialInstitutionAndDescription
                        label is "FinancialInstitution"
                    WireTransferBatchDetail
                    WireTransferBatchDetail.RequiredDistributionAmount
                        label is "RequiredAmount"
                    WireTransferBatchDetail.DistributionDifference
                        label is "Balance"
                column2 of 50%
                    WireTransferBatch.FinancialInstitutionBranch.FinancialInstitutionBranchAndDescription
                        label is "Branch"
                    WireTransferBatchDetail.Reference
                        label is "Reference"
                    visible when (ExternalTransfer)
                    WireTransferBatchDetail.TotalDistributions
                        label is "AppliedAmount"
                        display as text
                    visible when (FeeAmountEntered)
                    WireTransferBatchDetail.TotalFeeDistributions
                        label is "AppliedFeeAmount"
                        display as text
        header2 of "_"
        single column
            DistributionAccount
                display as text
                compact format
        display as text
            two column table
                column1 of 50%
                    DistributionAmount
                    visible when (CanMarkAsFeeDistribution)
                        FeeDistribution
                column2 of 50%
        single column
            Comment
                display as text
