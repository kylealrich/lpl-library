DeleteFreezeRecords is an ActionForm
    action is DeleteFreezeRecords
    Layout
        paragraph
            PrmCompany
            PrmCompany.Name
                no label
                display as text
        paragraph
            PrmPhysicalInventorySelect
                label is "SelectI\\D"
                select is SelectIDsToUnfreeze
            PrmPhysicalInventorySelect.Description
                no label
                display as text
        paragraph
            PrmDoNotSendNotification
                label is "DoNotSendNotification"
        paragraph
            visible when (not PrmDoNotSendNotification)
                PrmActorGroup
                    label is "ReportNotificationGroup"
