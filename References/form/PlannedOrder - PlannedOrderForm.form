PlannedOrderForm is a Form
    is primary
    title is "PlannedOrder"
    Layout
        paragraph
            Company
                display as text
            Company.Name
                no label
                display as text
        paragraph
            InventoryLocation
                display as text
            InventoryLocation.Name
                no label
                display as text
        paragraph
            Item
                display as text
            Item.Description
                no label
                display as text
            Item.StockUOM
                label is "UnitOfMeasure"
        paragraph
            PlannedOrderPeriodDate
                display as text
        paragraph
            FromLocation
                display as text
        paragraph
            Vendor
                display as text
        paragraph
            CreatedDate
                display as text
        paragraph
            PlannedReceiveDate
        paragraph
            PlannedShipDate
        paragraph
            PlannedOrderQuantity
