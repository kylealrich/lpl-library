WorksheetForm is a CompositeForm
    is primary
    context form is WorksheetFormContext
        background is theme color
    UpdateOptions is a Panel
        visible when (Status.Edit)
        form is UpdateOptions
    ItemSelection is a Panel
        visible when (Status.Edit)
        form is ItemSelection
    Line is a Panel
        title is "Items"
        visible when (IsCreatedReleaseAndProcessed)
        list is PricingWorksheetItemRel.WorksheetItems
