FinanceDimension3MainForm is a Form
    title is "Main"
    Layout
        two column
            effective date
            has future changes
                label is "FutureChanges"
        visible when (false)
            single column
                FinanceDimension3Structure
        visible when (DimensionType.Summary)
            visible when (IsAchild)
                paragraph
                    BudgetIdentifier
                    BudgetIdentifier.Description
                        no label
                        display as text
            visible when (not IsAchild)
                paragraph
                    BudgetIdentifier
                        display only
                    BudgetIdentifier.Description
                        no label
                        display as text
        visible when (DimensionType.Posting)
            single column
                Active
                SystemDimension
                    label is "System<FinanceEnterpriseGroup.FinanceDimension3Label>"
                IncludeInBudgetEdit
        visible when (PostingInEnterpriseStructureBOLDimension3)
            single column
                paragraph
                    ProductTaxCategory
                    ProductTaxCategory.Description
                        no label
                        display as text
                RevenuePartialAccount
                    compact format
        visible when (IsAchild)
            two column
                DerivedParentDisplayDimension
                    label is "Parent<FinanceEnterpriseGroup.FinanceDimension3Label>"
                    display only
                DerivedParentDescription
                    no label
                    display as text
        blank line
        blank line
        header2 of "AddressInformation"
        two column
            AddressCode
            AddressCode.Description
                no label
                display as text
        visible when (AddressCode entered)
            single column
                AddressCode.PersonResponsible
                    label is "PersonResponsible"
                    display as text
                AddressCode.PostalAddress.Country
                    display as text
                AddressCode.PostalAddress.DisplayAddressLine1
                    label is "StreetAddress"
                    display as text
                AddressCode.PostalAddress.DisplayAddressLine2
                    no label
                    display as text
                AddressCode.PostalAddress.DisplayAddressLine3
                    no label
                    display as text
                AddressCode.PostalAddress.DisplayAddressLine4
                    no label
                    display as text
                AddressCode.PostalAddress.Municipality
                    label is "City"
                    display as text
                AddressCode.PostalAddress.StateProvince
                    label is "State/Province"
                    display as text
                AddressCode.PostalAddress.PostalCode
                    label is "PostalCode"
                    display as text
                AddressCode.PostalAddress.County
                    label is "County/District"
                    display as text
                AddressCode.PostalAddress.Type
                    label is "Type"
                    display as text
                AddressCode.TelephoneNumber
                    label is "TelephoneNumber"
                    display as text
                AddressCode.FaxNumber
                    label is "FaxNumber"
                    display as text
                AddressCode.EmailAddress
                    label is "EmailAddress"
                    display as text
