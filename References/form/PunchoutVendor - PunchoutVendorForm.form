PunchoutVendorForm is a CompositeForm
    is primary
    title is "PunchoutVendor<PunchoutVendor>_-_<PunchoutVendorName>"
    context form is PunchoutVendorFormContext
        background is theme color
    Main is a Panel
        form is Main
    FieldMappings is a Panel
        list is PunchoutVendorFieldMapping set.PunchoutVendorFieldMapping
    OtherData is a Panel
        list is PunchoutVendorData set.PunchoutVendorData
    Requesters is a Panel
        list is RequestersForPunchoutVendorRel.RequesterPunchoutVendorCardList
            helper list is RequesterRel.HelperRequesterSelect
                action is AttachRequester
