PurchaseOrderLine_Translation is a Form
    is primary
    Layout
        paragraph
            Base_BusinessClass.PurchaseOrderLine
                display as text
            Base_BusinessClass.PurchaseOrderLineDisplay
                no label
                display as text
        single column
            IsoLocale
                compact format
                raise yellow alert when (VerifyTranslations)
                    mouse over text is untranslatable:"The base data has been changed but this translation has not been verified"
        single column
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.Description
                label is untranslatable:"Description"
            Description
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.EAMDescription
                label is untranslatable:"EAM Description"
            EAMDescription
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.E2GCompCode
                label is untranslatable:"E 2 G Comp Code"
            E2GCompCode
                label is untranslatable:"Translated"
