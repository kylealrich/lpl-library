GenerateInvoices is an ActionForm
    action is GenerateInvoices
    Layout
        single column
            paragraph
                DateRange.Begin
                    label is "PostingDateSelectionRange"
                DateRange.End
                    label is "Through"
            ThroughDate
                label is "TransactionThroughDate"
            SelectionGroup
            FundingSourceDate
            InvoiceDate
            visible when (NonRetainerOpenInvoicesExist)
                AppendToRunGroup
                    select is NonRetainerInvoiceRunGroupSelect
            ApplyRetainers
            SummaryProject
                label is "<FinanceEnterpriseGroup.ProjectLabel>"
                select is ContractProjectsRel.ContractAllProjectSelect
            FundingSource
                select is ProjectContractFinanceDimension2Rel.FinanceDimension2FundingSourceSelect
            InvoiceMaximum
            visible when (IsThirdPartyInvoicePrint)
                PrintStatus
            paragraph
                BillingGroup
                BillingGroup.Description
                    no label
                    display as text
