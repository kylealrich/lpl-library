BatchTaskComposite is a CompositeForm
    is primary
    title is "Job"
    context form is BatchTaskContext
    show panel control on left
    Details is a Panel
        title is "JobDetails"
        visible when (not TypeTransientField.TaskStream)
        form is BatchTaskDetails
    TaskSteps is a Panel
        title is "JobStreamSteps"
        visible when (TypeTransientField.TaskStream)
        list is BatchTaskStep(BatchTask) set.BatchTaskDetailedStepList
    LocationInStreams is a Panel
        title is "LocationInJobStreams"
        list is BatchTaskStepsInStream.BatchTaskInStreamsList
    ActionResults is a Panel
        visible when (HasActionResults)
        list is ActionResultsRel.DataAreaActionResultHierarchy
