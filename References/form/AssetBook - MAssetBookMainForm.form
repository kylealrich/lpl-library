MAssetBookMainForm is a CompositeForm
    title is "AssetBook"
    context form is MAssetBookContextForm
    show panel control on left
    restrict action Create
    restrict action Delete
    restrict action RefreshData
    Main is a Panel
        form is MAssetBookForm
    ProjectionsResults is a Panel
        visible when (AssetProjectionExists)
        list is AssetProjectionRel.ProjectionResults
