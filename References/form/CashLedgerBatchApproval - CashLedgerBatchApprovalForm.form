CashLedgerBatchApprovalForm is a CompositeForm
    is primary
    title is "CashLedgerBatchApproval<CashLedgerBatchApproval>"
    context form is CashLedgerBatchApprovalContextForm
        background is theme color
    Actions
        action is Open.ClearAllRecords
            hide when invalid
        action is Open.SelectRecords
            hide when invalid
        action is Open.ReleaseRecords
            hide when invalid
        action is Open.SubmitForApproval
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is Open.Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is save
            label is "_"
            mouse over text is "Save"
        action is Open.Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    CashLedgerPayments is a Panel
        visible when (BatchApprovalType.CashLedgerPayment)
        list is CashLedgerPayment set.CashLedgerBatchApprovalPayments
    PaymentsWithReleaseErrors is a Panel
        visible when (ApprovedPaymentBatchWithReleaseErrors)
        list is UnreleasedCashLedgerPaymentsRel.CashLedgerBatchApprovalPayments
    CashLedgerReceipts is a Panel
        visible when (BatchApprovalType.CashLedgerReceipt)
        list is CashLedgerCashReceiptHeader set.CashLedgerBatchApprovalReceipts
    ElectronicFundsTransactions is a Panel
        visible when (BatchApprovalType.ElectronicFundsTransaction)
        list is CashLedgerElectronicFundsTransferTransaction set.CashLedgerBatchApprovalEFTTransactions
    CashLedgerTransactions is a Panel
        visible when (BatchApprovalType.CashLedgerTransaction)
        list is CashLedgerTransaction set.CashLedgerBatchApprovalTransactions
    BatchParameters is a Panel
        form is BatchParameters
    ApprovalComments is a Panel
        visible when (HasApprovalComments)
        list is CashLedgerBatchApprovalReasonCodeUsage set.CashLedgerBatchApprovalReasonCodeUsageList
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
