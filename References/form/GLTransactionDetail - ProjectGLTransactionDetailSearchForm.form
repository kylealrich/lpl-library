ProjectGLTransactionDetailSearchForm is a Form
    title is "SearchProjectTransactionDetail"
    restrict action PurgeHistoryDetail
    restrict action PurgeNotToBePostedDetail
    Layout
        four column distributed
            FinanceCodeBlock.Project
            FinanceCodeBlock.GeneralLedgerChartAccount
                label is "Account"
            FinanceCodeBlock.GeneralLedgerChartAccount.GeneralLedgerSubAccount
                label is "SubAccount"
            blank line
        four column distributed
            TransactionDate
            System
            GeneralLedgerEvent
                label is "Event"
            blank line
        four column distributed
            AccountingEntity
                label is "AccountingEntity"
            FinanceCodeBlock.AccountingUnit
                label is "AccountingUnit"
            Resource
                label is "EmploymentI\\D"
            Description
        four column distributed
            ReportCurrencyAmount.ProjectAmount.EnteredCurrencyAmount
                label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Amount"
            TransactionAmount
                label is "Amount"
            blank line
            blank line
