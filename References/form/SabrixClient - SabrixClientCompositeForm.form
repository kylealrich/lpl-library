SabrixClientCompositeForm is a CompositeForm
    is primary
    title is "SabrixClient"
    context form is SabrixClientForm
    show panel control on left
    Main is a Panel
        title is "Main"
        form is SabrixClientMainForm
    MoreForm is a Panel
        title is "More"
        form is MoreForm
    InUserElements is a Panel
        form is UserElementsForm
    InputUserAttributes is a Panel
        title is "InputUserAttributes"
        list is InputUserAttributesRel.OneSourceUserAttributeLines
    OutputUserAttributes is a Panel
        title is "OutputUserAttributes"
        visible when (OutputOneSourceUserAttributesExist)
        list is OutputUserAttributesRel.OneSourceUserAttributeLines2
    DisplayMonitor is a Panel
        title is "DisplayMonitor"
        form is DisplayForm
    TaxConfig is a Panel
        list is TaxConfigurationRel.primary
    LineItems is a Panel
        title is "<LintItemCount>LineItems"
        list is TaxLineItemsRel.TaxLineItemLines
    XMLInfo is a Panel
        visible when (CapturedXml entered)
        form is HttpInfoForm
    ConfigurationParameters is a Panel
        title is "OneSourceConfigurationParameters"
        form is OneSourcConfigForm
    1SourceOriginalSetupForm is a Panel
        title is "OriginalSetup"
        visible when (OneSourceVersionType.Original)
        form is 1SourceOriginalSetupForm
    1SourceGenericSetupForm is a Panel
        title is "GenericSetup"
        visible when (OneSourceVersionType.Generic)
        form is 1SourceGenericSetupForm
    OneSourceUserTemplate is a Panel
        title is "OneSourceGeneric"
        visible when (OneSourceVersionType.Generic)
        list is OneSourceUserTemplatesRel.primary
    WebserviceDebug is a Panel
        title is "<WebserviceDebugToggleMsg>"
        form is WebserviceDebugForm
