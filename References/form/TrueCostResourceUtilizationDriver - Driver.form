Driver is a CompositeForm
    is primary
    context form is TrueCostResourceUtilizationDriverForm
    DriverByEventDate is a Panel
        title is "DriverEvents"
        visible when (DriverType.ByEventDate)
        list is TrueCostResourceUtilizationDriverEvent set.TrueCostResourceUtilizationDriverEventList
    DriverByEffectiveDate is a Panel
        title is "DriverData"
        visible when (DriverType.ByEffectiveDate)
        list is TrueCostResourceUtilizationDriverEvent set.TrueCostResourceUtilizationDriverEventList
