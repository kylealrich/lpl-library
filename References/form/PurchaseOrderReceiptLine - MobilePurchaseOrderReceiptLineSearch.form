MobilePurchaseOrderReceiptLineSearch is a Form
    title is "SearchReceiptLines"
    Layout
        paragraph
            Company
                select is MobileAllPurchasingCompanyList
            PurchaseOrder
                select is MobilePurchaseOrderList
            Item
                select is MobileItemFilterSelection
            PurchaseOrder.DefaultSourceInformation.DefaultRequestingLocation
                label is "RequestingLocation"
                select is MobileActiveRequestingLocations
            ReceivedDateRange.Begin
                label is "FromReceivedDate"
            ReceivedDateRange.End
                label is "ToReceivedDate"
            Status
