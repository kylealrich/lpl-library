PayablesInvoiceInterfaceResultContext is a Form
    title is "PayablesInvoiceInterfaceResult"
    Layout
        display as text
            visible when (InvoicesStillProcessing)
                paragraph
                    text of "<StillProcessingMsg>"
                        bold
            four column distributed
                column1
                    PayablesInvoiceInterfaceResult
                        label is "ID"
                    RunGroup
                column2
                    JournalizeGroup
                        display as text
                    DerivedStatus
                        label is "Status"
                column3
                    RunTime
                    RunMessage
                column4
                    visible when (IncompleteHasPendingRecords)
                        button of "Resubmit"
                            action is ResubmitInvoiceInterface
                    visible when (MatchOrReleaseError)
                        text of "InterfaceSuccessfulButSomeInvoicesDidNotRelease"
                            bold
                            italics
                            color of red
            visible when (NoRecordsToProcess)
                text of "<NoRecordsToProcessMsg>"
                    bold
