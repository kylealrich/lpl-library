CashTransactionImportResultContext is a Form
    propagate as a drill
    title is "CashTransactionInterfaceResult"
    Layout
        three column distributed
            column1
                RunGroup
                    display as text
                RunTime
                    display as text
                JournalizeGroup
                    display as text
            column2
                Status
                    display as text
                OriginalRunGroup
                    display as text
                visible when (IncompleteErrorsMoved)
                    ErrorRunGroup
                        display as text
            column3
                visible when (RunType.Payments)
                    paragraph
                        InterfacedPaymentAmount
                            display as text
                        InterfacedPaymentCount
                            label is "Count"
                            display as text
                visible when (HasReceiptsCreated)
                    paragraph
                        InterfacedReceiptAmount
                            display as text
                        InterfacedReceiptCount
                            label is "Count"
                            display as text
                visible when (HasElectronicFundsTransfersCreated)
                    paragraph
                        InterfacedElectronicFundsAmount
                            display as text
                        InterfacedElectronicFundsCount
                            label is "Count"
                            display as text
                visible when (RunType.Transactions)
                    paragraph
                        InterfacedTransactionsAmount
                            display as text
                        InterfacedTransactionsCount
                            label is "Count"
                            display as text
                visible when (RunType.ReturnedPayments)
                    paragraph
                        InterfacedReturnedPaymentAmount
                            display as text
                        InterfacedReturnedPaymentCount
                            label is "Count"
                            display as text
