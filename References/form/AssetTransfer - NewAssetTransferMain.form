NewAssetTransferMain is a Form
    Layout
        two column
            column1 of 20%
                text of "Transfer_From"
                    bold
                Company
                    display as text
                visible when (IsTagTransfered)
                    TagNumber
                        display as text
                visible when (IsAssetGroupTransfered)
                    AssetGroup
                        display as text
                visible when (IsLocationTransfered)
                    AssetLocation
                        label is "AssetLocation"
                        display as text
                visible when (IsAssetDivTransfered)
                    AssetDivision
                        label is "AssetDivision"
                        display as text
                visible when (IsAssetTypeTransfered)
                    AssetType
                        display as text
                        compact format
                visible when (IsAcctGroupTransfered)
                    AssetAccountGroup
                        display as text
                visible when (IsAcctUnitGrpTransfered)
                    AssetAccountingUnitGroup
                        display as text
                visible when (IsAcctUnitTransfered)
                    AssetAccountingUnit
                        label is "DefaultTransactionDimensions"
                        display as text
                        compact format
                visible when (IsAssetOwnerTransfered)
                    AssetOwner
                        display as text
                visible when (IsExpenseProTransfered)
                    paragraph
                        text of "Depreciation_Expense_Project"
                            color of gray
                            align as label
                        text of "<Asset.DepreciationExpenseProject>"
                visible when (IsAstProTransfered)
                    paragraph
                        text of "Asset_Project"
                            color of gray
                            align as label
                        text of "<Asset.AssetProject>"
            column2 of 20%
                text of "Transfer_To"
                    bold
                AssetTransferTo.ToAssetCompany
                    label is "Company"
                    display as text
                visible when (IsTagTransfered)
                    AssetTransferTo.ToTagNumber
                        label is "TagNumber"
                        display as text
                visible when (IsAssetGroupTransfered)
                    AssetTransferTo.ToAssetGroup
                        label is "AssetGroup"
                        display as text
                visible when (IsLocationTransfered)
                    AssetTransferTo.ToAssetLocation
                        label is "AssetLocation"
                        display as text
                visible when (IsAssetDivTransfered)
                    AssetTransferTo.ToAssetDivision
                        label is "AssetDivision"
                        display as text
                visible when (IsAssetTypeTransfered)
                    AssetTransferTo.ToAssetType
                        label is "AssetType"
                        display as text
                        compact format
                visible when (IsAcctGroupTransfered)
                    AssetTransferTo.ToAssetAccountGroup
                        label is "AssetAccountGroup"
                        display as text
                visible when (IsAcctUnitGrpTransfered)
                    AssetTransferTo.ToAssetAccountingUnitGroup
                        label is "AssetDimensionGroup"
                        display as text
                visible when (IsAcctUnitTransfered)
                    AssetTransferTo.ToAssetAccountingUnit
                        label is "DefaultTransactionDimensions"
                        display as text
                        compact format
                visible when (IsAssetOwnerTransfered)
                    AssetTransferTo.ToAssetOwner
                        label is "AssetOwner"
                        display as text
                visible when (IsExpenseProTransfered)
                    paragraph
                        text of "Depreciation_Expense_Project"
                            color of gray
                            align as label
                        text of "<AssetTransferTo.ToDepreciationExpenseProject>"
                visible when (IsAstProTransfered)
                    paragraph
                        text of "Asset_Project"
                            color of gray
                            align as label
                        text of "<AssetTransferTo.ToAssetProject>"
