SupplierAndVendorChangeForm is a CompositeForm
    title is "ManageSupplierAndVendorChanges"
    context form is ContractContextShort
        background is theme color
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Actions
        action is ChangeSupplierAndVendor
        navigation is ContractParticipant
            hide when invalid
        navigation is ContractTierMembers
            hide when invalid
        navigation is ContractPricingMembers
            hide when invalid
    CurrentVendor is a Panel
        form is VendorInformationForm
    OpenPurchaseOrders is a Panel
        title is "OpenPurchaseOrders<OpenPurchaseOrderIndicator>"
        visible when (not ServicePurchaseType)
        list is OpenSupplierChangePurchaseOrderLinesRel.POLinesFromAContract
    OpenRequisitions is a Panel
        title is "OpenRequisitions<OpenRequisitionIndicator>"
        visible when (not ServicePurchaseType)
        list is OpenSupplierChangeRequisitionLinesRel.RequisitionLinesFromAContract
    UnreleasedInvoices is a Panel
        title is "UnreleasedInvoices<UnreleasedInvoiceIndicator>"
        visible when (ServicePurchaseType)
        list is UnreleasedServiceInvoicesRel.UnreleasedVendorInvoices
    ReplenishmentItems is a Panel
        title is "ReplenishmentItems"
        list is VendorItemContractLineRel.ReplenishmentItemsNoParticipants
