PayablesProcessLevel is a CompositeForm
    is primary
    title is "<DerivedFormTitle>"
    context form is ProcessLevelContext
        background is theme color
    Main is a Panel
        form is ProcessLevelAddress
    Codes is a Panel
        form is ProcessLevelCodes
    Accounts is a Panel
        form is Accounts
    MultipleTaxAreas is a Panel
        visible when (VertexResultsExist)
        list is VertexTaxAreaIdResultsRel.TaxAreaResultList
    VertexError is a Panel
        visible when (VertexErrorExist)
        list is VertexWebserviceErrorRel.VertexTaxAreaIdErrorList
