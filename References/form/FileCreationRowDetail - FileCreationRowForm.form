                RowCondition2.CompareOperator
                    no label
                    when value changed
                        refresh DisplayRowCondition2Alpha
                        refresh DisplayRowCondition2Numeric
                        refresh DisplayRowCondition2Decimal
                        refresh DisplayRowCondition2Date
                        refresh DisplayRowCondition2Time
                        refresh DisplayRowCondition2Boolean
                        refresh DisplayRowCondition2Text
                        refresh DisplayRowCondition2TimeStamp
                        refresh DisplayRowCondition2Field
                visible when (DisplayRowCondition2Alpha)
                    RowCondition2.AlphaValue
                        no label
                visible when (DisplayRowCondition2Numeric)
                    RowCondition2.NumericValue
                        no label
                visible when (DisplayRowCondition2Decimal)
                    RowCondition2.DecimalValue
                        no label
                visible when (DisplayRowCondition2Date)
                    RowCondition2.DateValue
                        no label
                visible when (DisplayRowCondition2Time)
