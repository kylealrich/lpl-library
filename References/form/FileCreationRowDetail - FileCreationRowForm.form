FileCreationRowForm is a Form
    title is "RowDetail"
    Actions
        action is save and close
            label is "Save"
    Layout
        single column
            FileCreationSetup.Description
                label is "Mapping"
                display as text
            Description
            RowOrder
        visible when (LinkedRowValid)
            paragraph
                visible when (FileTypeIsSeparated)
                    LinkedSetup
                        select is LinkedSetupRel.FileCreationLinkedSetupListSeparated
                visible when (FileTypeIsFixedLength)
                    LinkedSetup
                        select is LinkedSetupRel.FileCreationLinkedSetupListFixedLength
                LinkedSetup.Description
                    no label
                    display as text
        visible when (LinkedRowValid)
            paragraph
                LinkedRow
                LinkedRow.Description
                    no label
                    display as text
        single column
            Disable
        visible when (not Linked)
            visible when (not RowExtendsOneToManyRow)
                single column
                    Header
                    Trailer
        visible when (Linked)
            single column
                LinkedRow.Header
                LinkedRow.Trailer
        single column
            visible when (FileCreationSetup.UseAuditInformation)
                OnlyWriteRowIfMappedFieldChanged
        visible when (not RowExtendsOneToManyRow)
            header3 of "RowProcessingConditions"
            paragraph
                text of "Condition1"
                    header3
                    bold
                button of icon.delete
                    visible when (CompareField1 entered)
                    action is ClearCompareFieldOne
            blank line
            paragraph
                RowCondition1.CompareFile
                RowCondition1.CompareType
                    when value changed
                        refresh DisplayRowCondition1Alpha
                        refresh DisplayRowCondition1Numeric
                        refresh DisplayRowCondition1Decimal
                        refresh DisplayRowCondition1Date
                        refresh DisplayRowCondition1Time
                        refresh DisplayRowCondition1Boolean
                        refresh DisplayRowCondition1Text
                        refresh DisplayRowCondition1TimeStamp
                        refresh DisplayRowCondition1Field
                RowCondition1.FieldDataType
                    when value changed
                        refresh DisplayRowCondition1Alpha
                        refresh DisplayRowCondition1Numeric
                        refresh DisplayRowCondition1Decimal
                        refresh DisplayRowCondition1Date
                        refresh DisplayRowCondition1Time
                        refresh DisplayRowCondition1Boolean
                        refresh DisplayRowCondition1Text
                        refresh DisplayRowCondition1TimeStamp
            paragraph
                CompareField1
                RowCondition1.CompareOperator
                    no label
                    when value changed
                        refresh DisplayRowCondition1Alpha
                        refresh DisplayRowCondition1Numeric
                        refresh DisplayRowCondition1Decimal
                        refresh DisplayRowCondition1Date
                        refresh DisplayRowCondition1Time
                        refresh DisplayRowCondition1Boolean
                        refresh DisplayRowCondition1Text
                        refresh DisplayRowCondition1TimeStamp
                        refresh DisplayRowCondition1Field
                visible when (DisplayRowCondition1Alpha)
                    RowCondition1.AlphaValue
                        no label
                visible when (DisplayRowCondition1Numeric)
                    RowCondition1.NumericValue
                        no label
                visible when (DisplayRowCondition1Decimal)
                    RowCondition1.DecimalValue
                        no label
                visible when (DisplayRowCondition1Date)
                    RowCondition1.DateValue
                        no label
                visible when (DisplayRowCondition1Time)
                    RowCondition1.TimeValue
                        no label
                visible when (DisplayRowCondition1Boolean)
                    RowCondition1.BooleanValue
                        no label
                visible when (DisplayRowCondition1Text)
                    RowCondition1.TextValue
                        no label
                visible when (DisplayRowCondition1TimeStamp)
                    RowCondition1.TimeStampValue
                        no label
                visible when (DisplayRowCondition1Field)
                    ComparedToField1
                        no label
            single column
                blank line
                RowConditionOperator
                    no label
                    align as label
                blank line
            paragraph
                text of "Condition2"
                    header3
                    bold
                button of icon.delete
                    visible when (CompareField2 entered)
                    action is ClearCompareFieldTwo
            blank line
            paragraph
                RowCondition2.CompareFile
                RowCondition2.CompareType
                    when value changed
                        refresh DisplayRowCondition2Alpha
                        refresh DisplayRowCondition2Numeric
                        refresh DisplayRowCondition2Decimal
                        refresh DisplayRowCondition2Date
                        refresh DisplayRowCondition2Time
                        refresh DisplayRowCondition2Boolean
                        refresh DisplayRowCondition2Text
                        refresh DisplayRowCondition2TimeStamp
                        refresh DisplayRowCondition2Field
                RowCondition2.FieldDataType
                    when value changed
                        refresh DisplayRowCondition2Alpha
                        refresh DisplayRowCondition2Numeric
                        refresh DisplayRowCondition2Decimal
                        refresh DisplayRowCondition2Date
                        refresh DisplayRowCondition2Time
                        refresh DisplayRowCondition2Boolean
                        refresh DisplayRowCondition2Text
                        refresh DisplayRowCondition2TimeStamp
            paragraph
