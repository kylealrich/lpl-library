PrepaymentMainForm is a Form
    title is "Main"
    Layout
        two column
            column1
                paragraph
                    Vendor
                    RemitToCode
                paragraph
                    Invoice
                paragraph
                    InvoiceAmount.CurrencyAmount
                        label is "PrepaymentAmount"
                    InvoiceCurrency
                        no label
                paragraph
                    InvoiceDate
                        label is "PrepaymentDate"
                paragraph
                    DueDate
                blank line
                paragraph
                    TaxableAmount
                    TaxCode
                paragraph
                    TaxAmount
            column2
                Vendor.VendorName
                    no label
                    display as text
                Vendor.Vendor
                    display as text
                Vendor.CurrentAddressRel.PostalAddress
                    display as text
