TierQualifierItem is an ActionForm
    action is Create
    title is "CreateTierQualifierItem"
    Layout
        display as text
            single column
                Contract.RepresentativeText
        visible when (Contract.HasPercentTierQualifier)
            paragraph
                QualifiedOrCompetitiveItem
                text of "WillDefaultToCompetitiveIfNotSelected"
                    italics
        visible when (not Contract.HasPercentTierQualifier)
            single column
                text of "WillDefaultToQualified"
                    italics
        paragraph
            ItemNumber
                label is "Item"
                select is ItemsForTierQualifiers
            ItemNumber.Description
                no label
                display as text
