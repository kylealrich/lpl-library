TaxLineItemUserFieldForm is a Form
    title is "TaxLineItemUserField"
    Layout
        three column table
            column1
                TaxLineItem.FinanceEnterpriseGroup
                SequenceNumber
                    label is "SequenceNumber"
                    display as text
            column2
                TaxLineItem.UniqueID
                Reference
                    label is "Reference"
            column3
                TaxLineItem.LineItemNumber
                    label is "LineNumber"
                Status
                    label is "Status"
