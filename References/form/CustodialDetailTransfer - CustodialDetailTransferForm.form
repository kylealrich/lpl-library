CustodialDetailTransferForm is a Form
    is primary
    Layout
        display as text
            paragraph
                CustodialAccountManagement.CustodialSummary
                    display as text
                CustodialAccountManagement.Title
                    display as text
                Status
                    no label
                    display as text
            two column distributed
                column1
                    header3 of "TransferFrom"
                    CustodialDetailAmount.CustodialPosting
                        display as text
                    CustodialAccountManagement.CamCategory
                        display as text
                    CustodialAccountManagement.TaxID
                        display as text
                    visible when (HasTaxID)
                        text of "TaxIDEntered"
                            italics
                    visible when (not HasTaxID)
                        text of "TaxIDNotEntered"
                            italics
                    CustodialAccountManagement.CamCustodian
                        display as text
                    FromFinanceCodeBlock
                        compact format
                column2
                    header3 of "TransferTo"
                    CustodialPosting
                    CamCategory
                    TaxID
                        display as text
                    CamCustodian
                    ToFinanceCodeBlock
                        compact format
                    TransferAmount
                    JournalizeGroup
                        display as text
                    ApprovalCode
