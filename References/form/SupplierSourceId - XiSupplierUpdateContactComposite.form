XiSupplierUpdateContactComposite is a CompositeForm
    title is "<MainContact.FirstAndLastName>'sProfile_with<Supplier.SupplierName>"
    called out actions only
    context form is ContactProfileContext
        show context form on left
    Actions
        action is InactivateContact
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is refresh
            label is "_"
        action is Delete
            label is "_"
    ContactHome is a DashBoardPanel
        BankInformation is a Panel
            visible when (CanDisplayBankInformation)
            double width
            overview navigation is BankInformationNav
            form is BankInformationForm
        CommodityCodes is a Panel
            overview navigation is CommodityCodeContactNav
            list is SupplierCommodityCode set.ApprovedCommodityCodeCardList
        Certifications is a Panel
            valid when (SupplierGroup.AllowCertificationSelect)
            overview navigation is CertificationNav
            list is SupplierCertificationRel.CertificationCardList
        ProxyNotifications is a Panel
            visible when (UsesSupplierPortal)
            overview navigation is ProxyNotificationNav
            list is SupplierContactProxy set.ProxyNotificationCardList
