PurchaseOrderReceiptTrackingForm is a Form
    is primary
    title is "TrackingNumberDetail"
    restrict action Create
    restrict action Update
    restrict action Delete
    Layout
        display as text
            single column
                paragraph
                    Company
                    Company.Name
                        no label
                paragraph
                    PurchaseOrderReceipt
                        label is "Receipt"
                    PurchaseOrderReceiptLine
                        label is "Line"
                PurchaseOrderReceiptTracking.PackageTrackingNumber
                    label is "TrackingNumber"
                NumberOfLabels
                PurchaseOrder
                PurchaseOrder.PORelease
                    label is "P\\ORelease"
                PurchaseOrder.POCode
                    label is "P\\OCode"
                DerivedRequisitionNumber
                Requester.Name
                    label is "RequesterName"
                SignedBy
                DateSigned
                ArrivalID
