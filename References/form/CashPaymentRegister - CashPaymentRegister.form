CashPaymentRegister is an ActionForm
    action is CashPaymentRegister
    Layout
        two column distributed
            column1
                paragraph
                    VendorGroup
                    VendorGroup.Description
                        no label
                        display as text
            column2
                paragraph
                    Company
                        select is Companies
                    Company.Name
                        no label
                        display as text
        two column distributed
            column1
                paragraph
                    PayGroup
                        select is PayGroups
                    PayGroup.Name
                        no label
                        display as text
            column2
                paragraph
                    ProcLevel
                        label is "ProcessLevel"
                        select is ProcessLevels
                    ProcLevel.Name
                        no label
                        display as text
        paragraph
            CashCode
            CashCode.Description
                no label
                display as text
            text of "Or"
            CashCodeGrp
                label is "CashCodeGroup"
                select is CashCodeGroup
            CashCodeGrp.Description
                no label
                display as text
        paragraph
            FrCheckDate
                label is "FromPaymentDate"
            ToCheckDate
                label is "ToPaymentDate"
        two column
            ReportOption
            CheckCurrency
                label is "DocumentCurrency"
        paragraph
            PaymentCode
                select is CashPayments
            PaymentCode.Description
                no label
                display as text
        single column
            UseCashRequirementsSortOptions
            FormatOption
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                CashPaymentRegisterReport_RDG
            column2 of 25%
                CashPaymentRegisterReport_XPT
