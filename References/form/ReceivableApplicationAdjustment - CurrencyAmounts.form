                        label is "FunctionalRate"
                    visible when (AdjustmentAmount.AccountingEntity.AlternateCurrencyExists)
                        AdjustmentAmount.AlternateAmount.EnteredCurrencyRate
                            label is "AlternateRate"
                    visible when (AdjustmentAmount.AccountingEntity.AlternateCurrency2Exists)
