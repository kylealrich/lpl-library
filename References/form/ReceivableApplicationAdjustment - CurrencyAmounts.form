CurrencyAmounts is a Form
    title is "CurrencyAmounts"
    Layout
        visible when (false)
            AdjustmentAmount.AssignToCurrency
        display as text
            three column independent
                column1
                    CreditAdjustAmount
                        label is "CreditAdjustmentAmount"
                    AdjustmentAmount.CurrencyAmount
                        label is "AdjustmentAmount"
                    AdjustmentAmount.FunctionalAmount.EnteredCurrencyAmount
                        label is "FunctionalAmount"
                    visible when (AdjustmentAmount.AccountingEntity.AlternateCurrencyExists)
                        AdjustmentAmount.AlternateAmount.EnteredCurrencyAmount
                            label is "AlternateAmount"
                    visible when (AdjustmentAmount.AccountingEntity.AlternateCurrency2Exists)
                        AdjustmentAmount.AlternateAmount2.EnteredCurrencyAmount
                            label is "AlternateAmount2"
                    visible when (AdjustmentAmount.AccountingEntity.AlternateCurrency3Exists)
                        AdjustmentAmount.AlternateAmount3.EnteredCurrencyAmount
                            label is "AlternateAmount3"
                    visible when (AdjustmentAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        AdjustmentAmount.ReportAmount1.EnteredCurrencyAmount
                            label is "ReportAmount1"
                    visible when (AdjustmentAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        AdjustmentAmount.ReportAmount2.EnteredCurrencyAmount
                            label is "ReportAmount2"
                    visible when (AdjustmentAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        AdjustmentAmount.ReportAmount3.EnteredCurrencyAmount
                            label is "ReportAmount3"
                    visible when (AdjustmentAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        AdjustmentAmount.ReportAmount4.EnteredCurrencyAmount
                            label is "ReportAmount4"
                    visible when (AdjustmentAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                        AdjustmentAmount.ReportAmount5.EnteredCurrencyAmount
                            label is "ReportAmount5"
                column2
                    ReceivableApplication.DerivedCreditCurrencyDesc
                        no label
                    ReceivableApplication.DerivedDebitCurrencyDesc
                        no label
                    Company.AccountingEntity.FunctionalCurrency.Description
