InterfaceCashReceipts is an ActionForm
    action is InterfaceCashReceipts
    Layout
        single column
            PrmCashManagementGroup
                label is "CashManagementGroup"
                display as text
            PrmRunGroup
                label is "RunGroup"
            PrmMoveErrorsToNewRunGroup
                label is "MoveErrorsToNewRunGroup"
            visible when (PrmMoveErrorsToNewRunGroup)
                PrmErrorRunGroupPrefix
                    label is "ErrorRunGroupPrefix"
            PrmCurrencyTable
                label is "CurrencyTable"
            PrmJournalizeDistributions
            visible when (PrmCashManagementGroup.AutoTranNumberForReceiptForm)
                PrmAutoNumberingActionForRec
                    label is "AutoTransactionNumberingActionForReceipt"
            visible when (PrmCashManagementGroup.AutoReferenceNumberForEFTForm)
                PrmAutoNumberingActionForEFT
                    label is "AutoTransactionNumberingActionForEFT"
