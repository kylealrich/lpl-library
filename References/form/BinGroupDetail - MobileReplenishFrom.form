MobileReplenishFrom is a Form
    title is "ReplenishFrom"
    Layout
        paragraph
            TransientVendorAndPurchaseFrom.Vendor
                label is "Vendor"
                select is MobileVendorSelectSearch
            TransientVendorAndPurchaseFrom.PurchaseFromLocation
                label is "PurchaseFromLocation"
                select is MobilePurchaseFromLocationsSelect
        header4 of "_"
        single column
            TransientReplenishFrom.ReplenishFromCompany
                select is MobileInventoryCompanyList
            TransientReplenishFrom.ReplenishFromLocation
                select is MobileInventoryLocations
            TransientReplenishFrom.ReplenishFromBin
                select is MobileBinSelect
