CurrencyAmountsForm is a Form
    title is "CurrencyAmounts"
    Layout
        visible when (false)
            BaseAmount.AssignToCurrency
        blank line
        four column table
            column headers
                "_"
                "Amount"
                "Currency"
                "Rate"
            column1
                text of "Functional"
                visible when (Company.OrigCompany.AccountingEntity.AlternateCurrencyExists)
                    text of "Alternate"
                visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency2Exists)
                    text of "Alternate2"
                visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency3Exists)
                    text of "Alternate3"
                visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                    text of "<BaseAmount.FinanceEnterpriseGroup.ReportOne>"
                visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                    text of "<BaseAmount.FinanceEnterpriseGroup.ReportTwo>"
                visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                    text of "<BaseAmount.FinanceEnterpriseGroup.ReportThree>"
                visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                    text of "<BaseAmount.FinanceEnterpriseGroup.ReportFour>"
                visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                    text of "<BaseAmount.FinanceEnterpriseGroup.ReportFive>"
            column2
                display as text
                    BaseAmount.FunctionalAmount.EnteredCurrencyAmount
                        no label
                        display only
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrencyExists)
                        BaseAmount.AlternateAmount.EnteredCurrencyAmount
                            no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency2Exists)
                        BaseAmount.AlternateAmount2.EnteredCurrencyAmount
                            no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency3Exists)
                        BaseAmount.AlternateAmount3.EnteredCurrencyAmount
                            no label
                    visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        BaseAmount.ReportAmount1.EnteredCurrencyAmount
                            no label
                    visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        BaseAmount.ReportAmount2.EnteredCurrencyAmount
                            no label
                    visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        BaseAmount.ReportAmount3.EnteredCurrencyAmount
                            no label
                    visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        BaseAmount.ReportAmount4.EnteredCurrencyAmount
                            no label
                    visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                        BaseAmount.ReportAmount5.EnteredCurrencyAmount
                            no label
            column3
                display as text
                    Company.OrigCompany.AccountingEntity.FunctionalCurrency
                        no label
                        display only
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrencyExists)
                        Company.OrigCompany.AccountingEntity.AlternateCurrency
                            no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency2Exists)
                        Company.OrigCompany.AccountingEntity.AlternateCurrency2
                            no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency3Exists)
                        Company.OrigCompany.AccountingEntity.AlternateCurrency3
                            no label
                    visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        BaseAmount.FinanceEnterpriseGroup.ReportCurrencyOne
                            no label
                    visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        BaseAmount.FinanceEnterpriseGroup.ReportCurrencyTwo
                            no label
                    visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        BaseAmount.FinanceEnterpriseGroup.ReportCurrencyThree
                            no label
                    visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        BaseAmount.FinanceEnterpriseGroup.ReportCurrencyFour
                            no label
                    visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                        BaseAmount.FinanceEnterpriseGroup.ReportCurrencyFive
                            no label
            column4
                display as text
                    BaseAmount.FunctionalAmount.EnteredCurrencyRate
                        no label
                        display only
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrencyExists)
                        BaseAmount.AlternateAmount.EnteredCurrencyRate
                            no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency2Exists)
                        BaseAmount.AlternateAmount2.EnteredCurrencyRate
                            no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency3Exists)
                        BaseAmount.AlternateAmount3.EnteredCurrencyRate
                            no label
                    visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        BaseAmount.ReportAmount1.EnteredCurrencyRate
                            no label
                    visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        BaseAmount.ReportAmount2.EnteredCurrencyRate
                            no label
                    visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        BaseAmount.ReportAmount3.EnteredCurrencyRate
                            no label
                    visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        BaseAmount.ReportAmount4.EnteredCurrencyRate
                            no label
                    visible when (BaseAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                        BaseAmount.ReportAmount5.EnteredCurrencyRate
                            no label
