FinanceResourceDetailsForm is a Form
    title is "Resource"
    Layout
        visible when (not FinanceResourceEntered)
            paragraph
                FinanceResource
                    label is "Employee"
                EmployeeRel.Name.PreferredFirstAndLastName
                    no label
                    display as text
        three column distributed
            Reference1
            Reference2
            Active
        three column distributed
            Title
            USResident
            paragraph
                Department
                    display as text
                JobCode
                    display as text
        header2 of "PayablesInvoiceAutomationEmailNotifications"
        three column distributed
            NotifyWhenInvoiceSubmitted
                label is "InvoiceSubmittedForMyApproval"
            NotifyWhenInvoiceApproved
                label is "MyInvoiceApproved"
            NotifyWhenInvoiceRejected
                label is "MyInvoiceRejected"
        header2 of "Address"
        FinanceResourcePostalAddress
            label is "Address"
