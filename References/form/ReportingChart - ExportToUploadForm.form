ExportToUploadForm is an ActionForm
    action is ExportToUpload
    title is "ExportToUpload"
    Layout
        single column
            text of "LeaveParametersBlankToExportEntireChart"
            blank line
            visible when (false)
                FinanceEnterpriseGroup
                ReportingChart
            PrmGeneralLedgerChartAccountGroup
                label is "CustomGroup"
            text of "Or"
            PrmSummaryAccount
                label is "Summary<FinanceEnterpriseGroup.AccountLabel>"
                select is AnySummaryInChart
            text of "Or"
            PrmPostingAccount
                label is "Posting<FinanceEnterpriseGroup.AccountLabel>"
                select is ChartAccountNode.DescendantsRel.ChartAccountSelect
