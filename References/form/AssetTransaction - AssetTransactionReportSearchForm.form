AssetTransactionReportSearchForm is a SearchForm
    title is "AssetTransactionReport"
    Layout
        five column independent
            column1 of 22%
                Company
                    label is "Company"
                DateRangeFilter.Begin
                    label is "TransactionDateFrom"
                Asset.AssetLease.LeaseCompany
                    label is "LeaseCompany"
                    display as text
            column2 of 22%
                Book
                DateRangeFilter.End
                    label is "TransactionDateTo"
                Asset.AssetLease.Lease
                    display as text
            column3 of 22%
                TransactionStatus
                    label is "Status"
                    initial value is "Released"
                Asset.AssetLocation
                Asset.TagNumber
                    label is "Tag"
            column4 of 20%
                AssetProcess
                    label is "TransactionProcess"
                Asset.Classification
                AssetBookRel.AssetSummaryGroup
            column5 of 20%
                Asset.AssetGroup
                Asset
                    select is AllAssets
                BatchUpdateResult
        paragraph
            DistributionAccount
                label is "TransactionDimensions"
                compact format
