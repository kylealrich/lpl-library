ProjectContractInvoiceNavForm is a CompositeForm
    title is "<FinanceEnterpriseGroup.ProjectLabel>_Contract_<DisplayContract>"
    context form is ProjectContractContextForm
        background is theme color
    ContractDetails is a Panel
        title is "Main"
        form is ProjectContractDetailsForm
    Totals is a Panel
        form is ProjectContractTotalsForm
    InvoiceDetail is a Panel
        title is "Invoices"
        visible when (InvoicesExist)
        list is ProjectContractInvoiceRel.ProjectContractInvoices
