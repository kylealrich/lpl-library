RegistrationProfile is a WizardForm
    use for action CustomerCreate
    title is "RequestCustomerAccess"
    disable field options
    summary form is AccessRequestSummary
    show steps
    ContactInformation is a Panel
        mouse over text is "<CustomerGroup.AccessRequestLoginText>"
        form is CustomerPortalRegistration
    CompanyInformation is a Panel
        mouse over text is "<CustomerGroup.AccessRequestCompanyText>"
        form is RegisterCustomerRepresentative
    Questions is a Panel
        valid when (ActiveQuestionsForCustomerGroupFromSessionKeyExist)
        mouse over text is "<CustomerGroup.AccessRequestQuestionText>"
        template is CustomerGroup.ActiveCustomerGroupQuestionRel
            business class is CustomerRepresentativeQuestionResponse
                form is CustomerQuestionResponse
