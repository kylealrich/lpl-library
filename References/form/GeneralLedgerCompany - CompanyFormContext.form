CompanyFormContext is a Form
    title is "Company"
    Layout
        two column
            Company
            Name
        three column
            BusinessGroup
            CustomerBusinessGroup
            AccountingEntity
        two column
            Currency
                label is "FunctionalCurrency"
                display as text
            CurrencyTable
                display as text
        single column
            IntercompanyBilling
            AutoTransactionNumbering
                label is "CashLedgerAutoTransactionNumbering"
            visible when (IsHSNSACCodeEnabled)
                RequireHSNSACCode
