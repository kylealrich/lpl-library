PreviewContractWithLines is a SummaryForm
    title is "Contract<ContractNbr>"
    Page Header
        Layout
            header2 of "_"
                underline color of black
            single column
                text of "Contract<ContractNbr>:_<Name>"
                    header2
                    left align
            header2 of "_"
            blank line
    Page Footer
        Layout
            two column
                text of "<current timestamp>"
                    left align
                text of "Page<page number>"
                    right align
    Layout
        paragraph
            ContractGroup
            ContractGroup.ProcurementGroup.Description
                no label
                display as text
        two column
            Name
            Status
        two column
            WorkingContractID
            WorkingContractType
        single column
            OnHold
                label is "ContractOnHold"
        two column
            paragraph
                Supplier
                    label is "Supplier"
                Supplier.SupplierName
                    no label
                display as text
            paragraph
                SupplierSourceId
                    label is "SupplierContact"
                SupplierSourceId.MainContact.FirstAndLastName
                    no label
        single column
            Description
                use text area of 2 lines
        two column
            ManufacturerOrDistributor.ManufacturerContract
                label is "ManufacturerContract"
            ManufacturerOrDistributor.DistributorContract
                label is "DistributorContract"
        single column
            SupplierOriginatedContract
        two column
            GPONegotiated
            paragraph
                GPOSupplier
                    label is "GPOSupplier"
                GPOSupplier.Supplier.SupplierName
                    no label
                    display as text
        two column
            ContractDocumentTemplate
            ContractDocumentTemplate.ContractDocumentTemplateGroup.Title
                no label
                display as text
        two column
            ContractType
            ContractSubtype
        two column
            ContractClassification
            ContractSubclassification
        single column
            CurrencyCode
        paragraph
            CommodityCode
                compact format
            CommodityCode.Description
                no label
                display as text
        two column
            EffectiveDate
            ExpirationDate
        two column
            visible when (YellowAlertExists)
                paragraph
                    YellowAlert
                    text of "DaysBeforeContractExpirationDate"
            visible when (RedAlertExists)
                paragraph
                    RedAlert
                    text of "DaysBeforeContractExpirationDate"
        single column
            ContractReviewDate
                label is "ReviewDate"
        two column
            FirstInvoiceAllowedDate
            LastInvoiceAllowedDate
        two column
            ContractSource
            RenewalContract
        two column
            HasBeenActivated
            PreferredContract
        single column
            SourcingEligible
                label is "EligibleForEventCreation"
        two column
            ApprovalStatus
            NegotiationStatus
        two column
            FinalApprovalStatus
            PreviousState
        two column
            ProposedTotalContractAmount
                label is "ProposedTotalAmount"
            MaximumAmount
        paragraph
            ParentContract
                select is NotClosedContracts
            ParentContract.Name
                no label
                display as text
        single column
            visible when (PrimaryContactExists)
                PrimaryContact
                    label is "PrimaryInternalContact"
        blank line
        visible when (TermsExist)
            header2 of "ContractTerms_andConditions"
            form is ContractTermsAndConditions
            header2 of "_"
        visible when (HasAPurchaseType)
            header2 of "AdditionalInformation"
            form is VAPrint
            header2 of "_"
        visible when (UseRetainage)
            header2 of "Retainage"
            form is Retainage
            header2 of "_"
        list is ContractLine set.PreviewLines
        visible when (ContractQuestionsExist)
            list is ContractQuestionAnswerRels.ContractQuestionsResponse
        visible when (AllUnansweredQuestionsExist)
            list is AllUnansweredQuestionRels.AllUnansweredQuestions
        visible when (ContractParticipantsExist)
            list is ContractParticipant set.ContractParticipantsPrint
        visible when (ContractRebateExists)
            list is ContractRebate set.SummaryContractRebates
        visible when (RelatedContractExist)
            list is RelatedContract set.SummaryRelatedContracts
        visible when (ContractAttachedContactExist)
            list is ActiveContractContactsRel.SummaryAttachedContacts
        visible when (ContractComments)
            list is ContractCommentRel.PrintComments
        visible when (ContractMilestones)
            list is ContractMilestone set.SummaryContractMilestones
        visible when (ContractDeliverables)
            list is ContractDeliverable set.SummaryContractDeliverables
        visible when (HasSubcontractors)
            list is ContractSubcontractorRels.SummaryContractSubcontractorList
