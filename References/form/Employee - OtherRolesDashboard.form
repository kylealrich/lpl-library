OtherRolesDashboard is a Form
    Layout
        display as text
            two column table
                column headers
                    "Create"
                    "View"
                row headers
                    "Requester"
                    "Buyer"
                    "Finance_\\Resource"
                    "Recall_\\User"
                    "P-\\Card_\\User"
                    "Contract_\\Evaluator"
                    "Inventory_\\Processor"
                    "Project_\\Employee"
                    "Distribution_Clerk"
                column1
                    button of icon.add
                        valid when (not RequesterExists)
                        action is CreateRequesterForEmployee
                    button of icon.add
                        valid when (not BuyerExists)
                        action is CreateBuyerForEmployee
                    button of icon.add
                        valid when (not FinanceResourceExists)
                        action is CreateFinanceResourceForEmployee
                    button of icon.add
                        valid when (not RecallUserExists)
                        action is CreateRecallUserForEmployee
                    button of icon.add
                        valid when (not ProcurementCardUserExists)
                        action is CreateProcurementCardUserForEmployee
                    button of icon.add
                        valid when (not ContractEvaluatorAndApproverExists)
                        action is CreateContractEvaluatorAndApproverForEmployee
                    button of icon.add
                        valid when (not InventoryIntelligenceProcessorExists)
                        action is CreateInventoryIntelligenceProcessorForEmployee
                    button of icon.add
                        valid when (ProjectEmployeeNotExists)
                        action is CreateProjectEmployee
                    button of icon.add
                        valid when (not DistributionClerkExists)
                        action is CreateDistributionClerkForEmployee
                column2
                    button of icon.profile
                        valid when (RequesterExists)
                        link is ViewRequester
                    button of icon.profile
                        valid when (BuyerExists)
                        link is ViewBuyer
                    button of icon.profile
                        valid when (FinanceResourceExists)
                        link is ViewFinanceResource
                    button of icon.profile
                        valid when (RecallUserExists)
                        link is ViewRecallUser
                    button of icon.profile
                        valid when (ProcurementCardUserExists)
                        link is ViewProcurementCardUser
                    button of icon.profile
                        valid when (ContractEvaluatorAndApproverExists)
                        link is ViewContractEvaluatorAndApprover
                    button of icon.profile
                        valid when (InventoryIntelligenceProcessorExists)
                        link is ViewInventoryIntelligenceProcessor
                    button of icon.profile
                        valid when (ProjectEmployeeExists)
                        link is ViewProjectEmployee
                    button of icon.profile
                        valid when (DistributionClerkExists)
                        link is ViewDistributionClerk
