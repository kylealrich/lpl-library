LocationCompanyCode is a Form
    Layout
        three column distributed
            column1
                paragraph
                    visible when (not RecordExists)
                        VendorGroup
                    visible when (RecordExists)
                        VendorGroup
                            display as text
                    VendorGroup.Description
                        no label
                        display as text
            column2
                paragraph
                    visible when (not RecordExists)
                        Vendor
                    visible when (RecordExists)
                        Vendor
                            display as text
                    Vendor.VendorName
                        no label
                        display as text
            column3
                paragraph
                    visible when (not RecordExists)
                        VendorLocation
                            label is "Location"
                    visible when (RecordExists)
                        VendorLocation
                            label is "Location"
                            display as text
                    VendorLocation.VendorName
                        no label
                        display as text
        paragraph
            visible when (not RecordExists)
                Company
            visible when (RecordExists)
                Company
                    display as text
            Company.Name
                no label
                display as text
        two column distributed
            paragraph
                TermsCode
                TermsCode.Desc1
                    no label
                    display as text
            paragraph
                CashCode
                CashCode.Description
                    no label
                    display as text
        two column distributed
            paragraph
                BankTransactionCode
                BankTransactionCode.Description
                    no label
                    display as text
            paragraph
                TaxCode
                    select is TaxCodeFilteredList
                TaxCode.Description
                    no label
                    display as text
        two column distributed
            paragraph
                TaxUsageCode
                TaxUsageCode.Description
                    no label
                    display as text
            paragraph
                DiversityCode
                DiversityCode.Description
                    no label
                    display as text
        two column distributed
            PoEdiNumber
            RevisedEDINumber
