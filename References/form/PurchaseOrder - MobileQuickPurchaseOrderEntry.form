MobileQuickPurchaseOrderEntry is a Form
    Layout
        paragraph
            Company
                select is MobilePurchasingCompanySelect
            Company.Name
                no label
                display as text
        paragraph
            Buyer
                select is MobileBuyerCardSelect
                show up to 8 characters
                display as required when (true)
            Buyer.Employee.Name.PreferredFirstAndLastName
                no label
                display as text
        paragraph
            POCode
                select is MobilePOCodeCardSelect
            POCode.Description
                no label
                display as text
        paragraph
            Vendor
                select is MobileActiveVendorsByName
            Vendor.VendorName
                no label
                display as text
        paragraph
            PurchaseFromLocation
                label is "PurchaseFrom"
                select is MobileNonRemitToActivePurchaseFromLocationsCardList
                show up to 8 characters
            PurchaseFromLocation.VendorName
                no label
                display as text
        paragraph
            ShipToLocation
                label is "ShipTo"
                select is MobileInventoryLocationsCardList
                show up to 8 characters
            ShipToLocation.NameForPurchaseOrder
                no label
                display as text
        single column
            PurchaseOrderDate
            DefaultDeliveryDate
                label is "DeliveryDate"
