FSMBODConfigurationEntry is a CompositeForm
    is primary
    title is "BODSetup"
    context form is FSMBODConfigurationEntryContext
    Triggers is a Panel
        visible when (not IsDirection)
        list is FSMBODConfigurationDetailRel.BODConfigurationDetail
    PulseAlertList is a Panel
        visible when (IsPulseAlert)
        list is FSMBODConfigurationDetailRel.BODConfigurationAlertDetail
    Criteria is a Panel
        visible when (not IsDirection)
        list is FSMBODConfigurationCriteriaRel.BODConfigurationCriteria
    UserTemplate is a Panel
        list is FSMBODConfigurationUserTemplateRel.UserTemplateList
    ActorGroup is a Panel
        visible when (IsPulseAlert)
        list is ActorGroupRel.ActorGroupHierarchyList
