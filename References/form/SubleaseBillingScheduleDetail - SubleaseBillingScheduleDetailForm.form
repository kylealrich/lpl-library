SubleaseBillingScheduleDetailForm is a Form
    is primary
    title is "BillingScheduleDetail"
    Layout
        single column
            CompanyAndCustomer.Company
                label is "ReceivableCompany"
            CompanyAndCustomer.Customer
                label is "Sublessee"
                select is CustomerContextCompanyCustomerSelectList
            Sublease
            SubleaseBillingScheduleDetail.PaymentNumber
            SubleaseBillingScheduleDetail.ExecutoryCostCode
                label is "NonLeaseComponent"
            InvoiceDate
            PaymentAmount
            Description
