MobileCashReceiptDeposit is a CompositeForm
    title is "CashReceipt"
    context form is MobileCashReceiptDepositContext
        background is theme color
    Main is a Panel
        form is MobileMainForm
    CurrencyAmounts is a Panel
        title is "CurrencyAmounts"
        form is MobileCurrencyAmountsForm
    TransactionData is a Panel
        form is TransactionData
    UserFieldsPanel is a Panel
        title is "UserFields"
        form is UserFieldForm
    CashReceiptDetail is a Panel
        title is "ReceiptDetail"
        list is CashLedgerCashReceiptDetail set.MobileListCashLedgerCashReceiptDetail
    DetailsNotImported is a Panel
        visible when (ImportInProcess)
        list is CashLedgerReceiptImportInErrorRel.primary
    ApprovalComments is a Panel
        visible when (HasApprovalComments)
        list is CashLedgerReceiptReasonCodeUsage set.MobileCashLedgerReceiptReasonCodeUsageList
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
    DistributionList is a Panel
        title is "Distributions"
        visible when (HasDistributions)
        list is CashLedgerGLDistributionsRel.MobileReceiptGLDistributionList
    Errors is a Panel
        title is "Errors<DerivedCashReceiptErrorCount>"
        visible when (CashReceiptErrorsExist)
        list is CashLedgerTransactionError set.CashLedgerCashReceiptHeaderErrorList
