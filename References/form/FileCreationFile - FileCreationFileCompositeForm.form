FileCreationFileCompositeForm is a CompositeForm
    title is "<FileCreationSetup.Description>"
    implements IndependentFormPanels
    show panel control on left
    Details is a Panel
        form is FileCreationFileForm
    FileOutput is a Panel
        visible when (DisplayFileOutput)
        independent form is FileOutputForm
    BlankFileOutput is a Panel
        title is "FileOutput"
        visible when (not DisplayFileOutput)
        independent form is FileOutputBlankForm
    Records is a Panel
        list is FileCreationRecordRel.FileCreationRecordFileList
    FileGroup is a Panel
        visible when (GroupedSystemTimeStamp entered)
        list is GroupedFilesRel.FileCreationRecordList
