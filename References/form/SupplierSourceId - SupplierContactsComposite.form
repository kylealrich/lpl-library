SupplierContactsComposite is a CompositeForm
    use for action BuyerCreateContacts
    title is "ContactInformation"
    context form is ContactContext
        background is theme color
    restrict action MobileUpdateContactBankInformation
    restrict action MobileUpdateSupplierBankInformation
    restrict action AttachToEvent
    restrict action AttachToEventAsPlanHolder
    ContactInformation is a Panel
        form is SourceId
    CommodityCodes is a Panel
        title is "CommodityCodes<SupplierCommodityCodeCount>"
        list is SupplierCommodityCode set.ApprovedCommodityCode
            helper list is CommodityCodes.CommCodes
                action is AttachToSupplierContact
    Certifications is a Panel
        title is "Certifications<SupplierContactCertificationCount>"
        list is SupplierCertificationRel.SupplierContactCertifications
    BankInformation is a Panel
        visible when (CanViewAndDisplayBankInformation)
        form is BankInformationForm
    SupplierRequisitions is a Panel
        visible when (HasSomeRequisitionAccess)
        form is SupplierRequisitions
    ProxyNotifications is a Panel
        title is "ProxyNotifications<SupplierProxyCount>"
        list is SupplierContactProxy set.primary
    PortalMessages is a Panel
        visible when (UsesSupplierPortal)
        list is SupplierContactMessage set.SupplierContactMessages
    PortalAccess is a Panel
        visible when (UsesSupplierPortal)
        form is PortalAccessForm
