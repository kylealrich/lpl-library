AwardLineNewContract is an ActionForm
    action is Submitted.AwardLineNewContract
    Layout
        display as text
            four column distributed
                Company
                SourcingEvent.RepresentativeText
                NotifiedSupplier.Supplier.RepresentativeText
                NotifiedSupplier.SupplierSourceId.RepresentativeText
            four column distributed
                SourcingEventLine.OutputType
                visible when (not SourcingEventLine.OutputType.Service)
                    SourcingEventLine.ItemNumber
                        label is "Item"
                SourcingEventLine.ItemDescription
                    label is "Description"
                visible when (SourcingEventLine.HasSecondDescription)
                    SourcingEventLine.ItemDescription2
                        no label
            two column
                SourcingEventLine
                    label is "Line#"
                visible when (SourcingEventLine.HasThirdDescription)
                    SourcingEventLine.ItemDescription3
                        label is "LongItemDescription"
            four column distributed
                SourcingEvent.CurrencyCode
                    label is "EventCurrency"
                visible when (DifferentResponseCurrency)
                    SourcingEventResponse.EnteredCurrencyCode
                        label is "ResponseCurrency"
                visible when (SourcingEventLine.GTINEntered)
                    paragraph
                        SourcingEventLine.GlobalTradeItemNumber
                            label is "GTIN"
                        SourcingEventLine.GlobalTradeItemNumber.FullDescription
                            no label
                visible when (not ResponseExists)
                    text of "OnlyAlternateResponsesExist"
                        bold
                        color of green
        header3 of "EnterAwardInformation"
        three column distributed
            AwardReason
                select is BuyerAwardReasonsSelect
            visible when (DisplayNegotiatedTerms)
                TermsHaveBeenNegotiated
            button of "ViewDescriptionAndComments"
                link is ViewComments
                display as link
        seven column
            visible when (AlternatesExist)
                column1
                    visible when (NonNoBidResponseExists)
                        text of "_"
                        text of "MainResponse"
                            bold
                    visible when (not NonNoBidResponseExists)
                        text of "OnlyAlternateResponsesExist"
                            bold
                    text of "AlternateResponses"
                        bold
            visible when (not ServiceItemWithContractOutput)
                column2
                    text of "VendorItem"
                        bold
                    visible when (NonNoBidResponseExists)
                        VendorItem
                            no label
                    visible when (not NonNoBidResponseExists)
                        blank line
                    visible when (Alt1Exists)
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].VendorItem
                            no label
                            display as text
                    visible when (Alt2Exists)
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].VendorItem
                            no label
                            display as text
                    visible when (Alt3Exists)
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[3].VendorItem
                            no label
                            display as text
                    visible when (Alt4Exists)
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[4].VendorItem
                            no label
                            display as text
                    visible when (Alt5Exists)
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[5].VendorItem
                            no label
                            display as text
            column3
                text of "ResponseQuantity"
                    bold
                visible when (NonNoBidResponseExists)
                    Quantity
                        no label
                visible when (not NonNoBidResponseExists)
                    blank line
                visible when (Alt1Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].Quantity
                        no label
                        display as text
                visible when (Alt2Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].Quantity
                        no label
                        display as text
                visible when (Alt3Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[3].Quantity
                        no label
                        display as text
                visible when (Alt4Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[4].Quantity
                        no label
                        display as text
                visible when (Alt5Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[5].Quantity
                        no label
                        display as text
            column4
                text of "Award"
                    bold
                visible when (NonNoBidResponseExists)
                    AwardForContract
                        no label
                visible when (not NonNoBidResponseExists)
                    blank line
                visible when (Alt1Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].AwardForContract
                        no label
                visible when (Alt2Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].AwardForContract
                        no label
                visible when (Alt3Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[3].AwardForContract
                        no label
                visible when (Alt4Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[4].AwardForContract
                        no label
                visible when (Alt5Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[5].AwardForContract
                        no label
            column5
                text of "UOM"
                    bold
                visible when (NonNoBidResponseExists)
                    SourcingEventLine.UOM.UnitOfMeasure
                        no label
                        display as text
                visible when (not NonNoBidResponseExists)
                    blank line
                visible when (Alt1Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].UOM.UnitOfMeasure
                        no label
                        display as text
                visible when (Alt2Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].UOM.UnitOfMeasure
                        no label
                        display as text
                visible when (Alt3Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[3].UOM.UnitOfMeasure
                        no label
                        display as text
                visible when (Alt4Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[4].UOM.UnitOfMeasure
                        no label
                        display as text
                visible when (Alt5Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[5].UOM.UnitOfMeasure
                        no label
                        display as text
            column6
                text of "UnitAmount"
                    bold
                visible when (NonNoBidResponseExists)
                    ResponseAmountDisplay
                        no label
                        display as text
                visible when (not NonNoBidResponseExists)
                    blank line
                visible when (Alt1Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].ResponseAmountDisplay
                        no label
                        display as text
                visible when (Alt2Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].ResponseAmountDisplay
                        no label
                        display as text
                visible when (Alt3Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[3].ResponseAmountDisplay
                        no label
                        display as text
                visible when (Alt4Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[4].ResponseAmountDisplay
                        no label
                        display as text
                visible when (Alt5Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[5].ResponseAmountDisplay
                        no label
                        display as text
            visible when (SourcingEventResponse.DifferentResponseCurrency)
                column7
                    text of "ResponseAmount"
                        bold
                    visible when (NonNoBidResponseExists)
                        ResponseCurrencyAmountDisplay
                            no label
                            display as text
                    visible when (not NonNoBidResponseExists)
                        blank line
                    visible when (Alt1Exists)
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].ResponseCurrencyAmountDisplay
                            no label
                            display as text
                    visible when (Alt2Exists)
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].ResponseCurrencyAmountDisplay
                            no label
                            display as text
                    visible when (Alt3Exists)
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[3].ResponseCurrencyAmountDisplay
                            no label
                            display as text
                    visible when (Alt4Exists)
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[4].ResponseCurrencyAmountDisplay
                            no label
                            display as text
                    visible when (Alt5Exists)
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[5].ResponseCurrencyAmountDisplay
                            no label
                            display as text
        header3 of "ContractInformation"
        two column distributed
            paragraph
                ContractType
                    select is BuyerContractTypesSelect
                ContractSubtype
                    select is BuyerContractSubtypeSelect
            paragraph
                ContractClassification
                    select is BuyerContractClassificationSelect
                ContractSubclassification
                    select is BuyerContractSubclassificationSelect
        paragraph
            EffectiveDate
            ExpirationDate
        four column distributed
            visible when (not HeaderOutputCurrencyEntered)
                visible when (not SourcingEventResponse.VendorHasInvoiceCurrency)
                    OutputCurrency
                        label is "ContractCurrency"
                    text of "IfContractCurrencyNotEnteredResponseCurrencyOf<SourcingEventResponse.EnteredCurrencyCode>WillBeUsed"
                        italics
            visible when (HeaderOutputCurrencyEntered)
                OutputCurrency
                    label is "ContractCurrency"
                    display as text
        four column distributed
            visible when (SourcingEventResponse.POCommitments)
                visible when (not ServiceContract)
                    paragraph
                        LifetimeMaximumAmount
                        text of "EnterALifetimeMaximumAmountToCreateCommitmentsOnTheContractLine"
                            italics
                visible when (ServiceContract)
                    ServiceCommitment
                        label is "CreateCommitment"
            visible when (not SourcingEventLine.ServiceCode.Amount)
                LifetimeMaxQuantity
                AwardQtyToLifetimeMaxQty
                    label is "CopyAwardQuantityToLifetimeMaximumQuantity"
            visible when (SourcingEventLine.OutputType.Standing)
                ScheduleFrequency
                MaxReleaseNumber
        visible when (SourcingEventLine.DisplayVendorItem)
            header3 of "Overrides"
        two column distributed
            visible when (BuyerDisplayGTIN)
                paragraph
                    GlobalTradeItemNumber
                        label is "GTIN"
                    GlobalTradeItemNumber.FullDescription
                        no label
                        display as text
            visible when (AllowEntryOfManufacturerInfoOnAward)
                paragraph
                    ManufacturerInformation.Manufacturer
                        label is "ManufacturerCode"
                        compact format
                    ManufacturerInformation.ManufacturerNumber
                        label is "ManufacturerNumber"
            visible when (not AllowEntryOfManufacturerInfoOnAward)
                paragraph
                    DerivedManufacturerCode
                        label is "ManufacturerCode"
                        display as text
                    DerivedManufacturerDivision
                        label is "Division"
                        display as text
                    DerivedManufacturerNumber
                        label is "ManufacturerNumber"
                        display as text
        header3 of "ParticipantInformation"
        two column distributed
            visible when (SourcingEventLine.NonServiceContracts)
                paragraph
                    ParticipantGroup
                    text of "Or"
            paragraph
                Participant.Company
                visible when (SourcingEventLine.NonServiceContracts)
                    Participant.Location
                visible when (SourcingEventLine.NonStandingOrServiceContracts)
                    Participant.RequestingLocation
                Participant.DefaultAccountingInformation
                    compact format
