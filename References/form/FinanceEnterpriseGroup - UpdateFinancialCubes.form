UpdateFinancialCubes is an ActionForm
    action is UpdateFinancialCubes
    title is "UpdateFinancialCubes"
    Layout
        single column
            text of "SelectTheAnalyticCubesYouWouldLikeToUpdate:"
                header2
                bold
        four column table
            column headers
                "Cube"
                "Action"
                "LastRefreshDate"
                "LastReloadDate"
            column1
                UpdateFinanceEnterpriseGroup
                    label is "FinanceEnterpriseGroup"
                UpdateGeneralLedgerTotal
                    label is "GlobalLedgerTotal"
                UpdateDEPMDimensionMap
                    label is "EPMDimensionMap"
                UpdateAllocationTransactionDetail
                    label is "AllocationTransactionDetail"
                visible when (actor.context.FinanceEnterpriseGroup.UseAverageDailyBalance)
                    UpdateDailyPeriodTotal
            column2
                visible when (UpdateFinanceEnterpriseGroup)
                    FinanceEnterpriseGroupOption
                        no label
                        display as horizontal radio buttons
                visible when (UpdateGeneralLedgerTotal)
                    GeneralLedgerTotalOption
                        no label
                        display as horizontal radio buttons
                visible when (UpdateDEPMDimensionMap)
                    DEPMDimensionMapOption
                        no label
                        display as horizontal radio buttons
                visible when (UpdateAllocationTransactionDetail)
                    AllocationTransactionDetailOption
                        no label
                        display as horizontal radio buttons
                visible when (UpdateDailyPeriodTotal)
                    DailyPeriodTotalOption
                        no label
                        display as horizontal radio buttons
            column3
                display as text
                    FinanceEnterpriseGroupAnalyticCubeRel.LastRefreshDate
                        no label
                    GeneralLedgerTotalAnalyticCubeRel.LastRefreshDate
                        no label
                    DEPMDimensionMapAnalyticCubeRel.LastRefreshDate
                        no label
                    AllocationTransactionDetailAnalyticCubeRel.LastRefreshDate
                        no label
                    visible when (UpdateDailyPeriodTotal)
                        DailyPeriodTotalAnalyticCubeRel.LastRefreshDate
                            no label
            column4
                display as text
                    FinanceEnterpriseGroupAnalyticCubeRel.LastReloadDate
                        no label
                    GeneralLedgerTotalAnalyticCubeRel.LastReloadDate
                        no label
                    DEPMDimensionMapAnalyticCubeRel.LastReloadDate
                        no label
                    AllocationTransactionDetailAnalyticCubeRel.LastReloadDate
                        no label
                    visible when (UpdateDailyPeriodTotal)
                        DailyPeriodTotalAnalyticCubeRel.LastReloadDate
                            no label
        single column
            text of "SelectRefreshInOrderToUpdateExistingCubeData.SelectReloadToDeleteExistingCubeDataAndRebuildData."
                italics
