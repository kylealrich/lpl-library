MobileLineApprovalMainForm is a Form
    title is "Requisition<Requisition.Requisition>Line<RequisitionLine>"
    Layout
        display as text
            paragraph
                Item.RepresentativeText
            paragraph
                Item.Description2
            three column table
                column1
                    Quantity
                    TransactionUnitCost
                        label is "UnitCost"
                column2
                    EnteredUOM
                        label is "U\\O\\M"
                    ApprovalValue
                        label is "Amount"
                column3
                    RequestedDeliveryDate
                    TransactionCurrencyCode
                        label is "Currency"
            header2 of "_"
            single column
                Manufacturer.DerivedCodeDivisionDescription
                    label is "Manufacturer"
                Vendor
                DerivedVendorItemDisplay
                ItemType
            two column table
                column1
                    CreatePurchaseOrder
                    CostOption
                    LateDeliveryDate
                column2
                    ServiceCode
                    OverrideCost
                    AllocationPriority
                        label is "Priority"
