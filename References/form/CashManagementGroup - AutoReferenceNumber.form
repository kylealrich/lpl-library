AutoReferenceNumber is a Form
    Layout
        header2 of "AutoReferenceNumberBy_(SelectOnlyOne)"
        single column
            AutoReferenceNumberByCompany
                label is "CompanyLevel"
            AutoReferenceNumberByCashCode
                label is "CashCodeLevel"
            AutoReferenceNumberByBankTransactionCode
                label is "BankTransactionCodeLevel"
            AutoReferenceNumberByCashCodeBankTranCode
                label is "CashCodeAndBankTransactionCodeLevel"
        header2 of "FormsToApplyAutoNumberingTo_(SelectOneOrMore)"
        single column
            AutoReferenceNumberForPaymentForm
                label is "CashLedgerPayment"
            AutoReferenceNumberForEFTForm
                label is "CashLedgerEFT/CCExtracts"
