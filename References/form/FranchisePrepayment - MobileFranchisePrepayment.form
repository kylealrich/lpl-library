MobileFranchisePrepayment is a Form
    title is "FranchisePrepayment"
    restrict action Create
    restrict action Unreleased.Delete
    Actions
        action is Unreleased.Delete
            hide when invalid
        action is save
            label is "_"
    Layout
        paragraph
            Company
                display as text
            Company.BillingCompany.Name
                no label
                display as text
        paragraph
            Customer
                display as text
            Customer.Name
                no label
                display as text
        paragraph
            CustomerShipTo
                label is "ShipTo"
                display as text
            CustomerShipTo.Name
                no label
                display as text
        paragraph
            FranchiseContract
                label is "Contract"
                display as text
            FranchiseContractCharge.FranchiseStandardCharge.Currency
                label is "Currency"
                display as text
        paragraph
            FranchiseContractCharge
                display as text
            FranchiseContractCharge.Description
                no label
                display as text
        paragraph
            Charge
                display as text
        blank line
        paragraph
            Status
                display as text
        paragraph
            FranchisePrepayment
                label is "PrepaymentDate"
        paragraph
            TransactionAmount
                label is "Amount"
