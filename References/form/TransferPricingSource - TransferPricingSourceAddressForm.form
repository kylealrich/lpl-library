TransferPricingSourceAddressForm is a Form
    title is "SourceAddress"
    Layout
        paragraph
            SourceEntity
            visible when (SourceEntity.SpecificEntity)
                SourceAddress.AccountingEntity
        three column independent
            column1
                visible when (FinanceEnterpriseGroup.AccountingUnitSelected)
                    SourceAddress.AccountingUnit
                        label is FinanceEnterpriseGroup.AccountingUnitLabel
                        select is TransferPricing.Company.AccountingEntity.ContextStructureAccountingUnitEntityNodeRel.AccountingUnitChildrenRel.AccountingUnits
                SourceAddress.GeneralLedgerChartAccount
                    label is FinanceEnterpriseGroup.AccountLabel
                    select is TransientAccountStructure.ChartAccountNode.ChartAccountChildrenRel.ChartAccountList
                visible when (FinanceEnterpriseGroup.ProjectEntry)
                    SourceAddress.Project
                        label is FinanceEnterpriseGroup.ProjectLabel
                        select is TransientProjectStructure.ProjectNode.ProjectChildrenRel.ProjectList
                visible when (FinanceEnterpriseGroup.FinanceDimension1Entry)
                    SourceAddress.FinanceDimension1
                        label is FinanceEnterpriseGroup.FinanceDimension1Label
                        select is TransientFinanceDimension1Structure.DimensionNode.DimensionChildrenRel.FinanceDimension1List
                visible when (FinanceEnterpriseGroup.FinanceDimension2Entry)
                    SourceAddress.FinanceDimension2
                        label is FinanceEnterpriseGroup.FinanceDimension2Label
                        select is TransientFinanceDimension2Structure.DimensionNode.DimensionChildrenRel.FinanceDimension2List
                visible when (FinanceEnterpriseGroup.FinanceDimension3Entry)
                    SourceAddress.FinanceDimension3
                        label is FinanceEnterpriseGroup.FinanceDimension3Label
                        select is TransientFinanceDimension3Structure.DimensionNode.DimensionChildrenRel.FinanceDimension3List
                visible when (FinanceEnterpriseGroup.FinanceDimension4Entry)
                    SourceAddress.FinanceDimension4
                        label is FinanceEnterpriseGroup.FinanceDimension4Label
                        select is TransientFinanceDimension4Structure.DimensionNode.DimensionChildrenRel.FinanceDimension4List
                visible when (FinanceEnterpriseGroup.FinanceDimension5Entry)
                    SourceAddress.FinanceDimension5
                        label is FinanceEnterpriseGroup.FinanceDimension5Label
                        select is TransientFinanceDimension5Structure.DimensionNode.DimensionChildrenRel.FinanceDimension5List
                visible when (FinanceEnterpriseGroup.FinanceDimension6Entry)
                    SourceAddress.FinanceDimension6
                        label is FinanceEnterpriseGroup.FinanceDimension6Label
                        select is TransientFinanceDimension6Structure.DimensionNode.DimensionChildrenRel.FinanceDimension6List
                visible when (FinanceEnterpriseGroup.FinanceDimension7Entry)
                    SourceAddress.FinanceDimension7
                        label is FinanceEnterpriseGroup.FinanceDimension7Label
                        select is TransientFinanceDimension7Structure.DimensionNode.DimensionChildrenRel.FinanceDimension7List
                visible when (FinanceEnterpriseGroup.FinanceDimension8Entry)
                    SourceAddress.FinanceDimension8
                        label is FinanceEnterpriseGroup.FinanceDimension8Label
                        select is TransientFinanceDimension8Structure.DimensionNode.DimensionChildrenRel.FinanceDimension8List
                visible when (FinanceEnterpriseGroup.FinanceDimension9Entry)
                    SourceAddress.FinanceDimension9
                        label is FinanceEnterpriseGroup.FinanceDimension9Label
                        select is TransientFinanceDimension9Structure.DimensionNode.DimensionChildrenRel.FinanceDimension9List
                visible when (FinanceEnterpriseGroup.FinanceDimension10Entry)
                    SourceAddress.FinanceDimension10
                        label is FinanceEnterpriseGroup.FinanceDimension10Label
                        select is TransientFinanceDimension10Structure.DimensionNode.DimensionChildrenRel.FinanceDimension10List
            column2
                visible when (FinanceEnterpriseGroup.AccountingUnitSelected)
                    ProcessAccountingUnitPostingBalance
                ProcessAccountPostingBalance
                visible when (FinanceEnterpriseGroup.ProjectEntry)
                    ProcessProjectPostingBalance
                visible when (FinanceEnterpriseGroup.FinanceDimension1Entry)
                    ProcessDimension1PostingBalance
                visible when (FinanceEnterpriseGroup.FinanceDimension2Entry)
                    ProcessDimension2PostingBalance
                visible when (FinanceEnterpriseGroup.FinanceDimension3Entry)
                    ProcessDimension3PostingBalance
                visible when (FinanceEnterpriseGroup.FinanceDimension4Entry)
                    ProcessDimension4PostingBalance
                visible when (FinanceEnterpriseGroup.FinanceDimension5Entry)
                    ProcessDimension5PostingBalance
                visible when (FinanceEnterpriseGroup.FinanceDimension6Entry)
                    ProcessDimension6PostingBalance
                visible when (FinanceEnterpriseGroup.FinanceDimension7Entry)
                    ProcessDimension7PostingBalance
                visible when (FinanceEnterpriseGroup.FinanceDimension8Entry)
                    ProcessDimension8PostingBalance
                visible when (FinanceEnterpriseGroup.FinanceDimension9Entry)
                    ProcessDimension9PostingBalance
                visible when (FinanceEnterpriseGroup.FinanceDimension10Entry)
                    ProcessDimension10PostingBalance
            column3
                visible when (FinanceEnterpriseGroup.AccountingUnitSelected)
                    text of "_"
                text of "_"
                visible when (FinanceEnterpriseGroup.ProjectEntry)
                    visible when (BlankProject)
                        TreatBlankProjectAsEmpty
                            label is "TreatBlank<FinanceEnterpriseGroup.ProjectLabel>AsEmpty"
                    visible when (not BlankProject)
                        text of "_"
                visible when (FinanceEnterpriseGroup.FinanceDimension1Entry)
                    visible when (BlankFinanceDimension1)
                        TreatBlankDimension1AsEmpty
                            label is "TreatBlank<FinanceEnterpriseGroup.FinanceDimension1Label>AsEmpty"
                    visible when (not BlankFinanceDimension1)
                        text of "_"
        single column
            visible when (ProcessPostingPerformanceImpact)
                ProcessPostingPerformanceWarningForForm
                    no label
                    display as text
