ScheduleProcess is an ActionForm
    action is Schedule
    title is "Schedule"
    Layout
        header3 of "Schedule<CloseTask.TaskName>"
        paragraph
            CloseManagementGroup
                label is "CloseManagementGroup"
                display as text
            CloseManagementGroup.Description
                no label
                display as text
        paragraph
            CloseTask
                label is "Task"
                display as text
            TaskName
                no label
                display as text
        header3 of "Schedule"
        paragraph
            DefaultCalendar
                label is "BySystemCalendar"
                display as text
            DefaultCalendar.Description
                no label
                display as text
        paragraph
            DefaultPeriodGroup
                label is "PeriodGroup"
                display as text
            DefaultPeriodGroup.Description
                no label
                display as text
        two column
            PrmScheduleFrom
                label is "ScheduleFrom"
                compact format
                select is SystemCalendarPeriodsForPeriodGroup
            PrmScheduleThru
                label is "Thru"
                compact format
                select is SystemCalendarPeriodsForPeriodGroup
