                        no label
                        display as text
                paragraph
                    PaymentDate
                        display as text
                    PaymentAmount
                        display as text
                    SettlementCurrency
                        no label
                        display as text
            column2
                header2 of "Receivable"
                paragraph
                    ReceivableCompany
                        label is "Company"
                        display as text
                    ReceivableCompany.Name
                        no label
                        display as text
                    ReceivableCompany.Currency
                        no label
                        display as text
                paragraph
                    ReceivableCashCode
