CreateNewContractInterface is an ActionForm
    action is CreateNewContractForThisGPOContract
    Layout
        paragraph
            ContractGroup
                select is ContractGroups
            ContractGroup.BusinessGroup.Description
                no label
                display as text
        single column
            ParmGPOContractNumber
                display as text
        paragraph
            ParmGPOSupplier
                select is GPOSuppliersSelect
            ParmGPOSupplier.SupplierName
                no label
                display as text
        single column
            TypeOfContract
        paragraph
            UseTemplate
                select is ContractTemplates
            UseTemplate.Name
                no label
                display as text
        two column
            ContractType
            ContractType.Description
                no label
                display as text
        single column
            ContractGroup.DefaultGPOContractType
                label is "DefaultContractTypeFromContractGroup"
                display as text
        two column
            ContractSubtype
            ContractSubtype.Description
                no label
                display as text
        two column
            ContractClassification
            ContractClassification.Description
                no label
                display as text
        single column
            ContractGroup.DefaultGPOContractClassification
                label is "DefaultContractClassificationFromContractGroup"
                display as text
        two column
            ContractSubclassification
            ContractSubclassification.Description
                no label
                display as text
        single column
            ManufacturerCodeDivision
                compact format
            text of "MustEnterAManufacturerOrHaveManufacturerEnteredForGPOContractSupplierOrTemplate"
        two column
            Contact
            Contact.FirstLastName
                no label
                display as text
        single column
            ContractGroup.DefaultGPOContact
                label is "DefaultPrimaryInternalContactFromContractGroup"
                display as text
        single column
            UseGPOContractNumberAsWorkingContract
            MemberProcessing
            visible when (TypeOfContract.Manufacturer)
                DoNotCreateDistributorContract
                visible when (not DoNotCreateDistributorContract)
                    DistributorPriority
