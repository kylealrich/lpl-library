MobileMain is a Form
    title is "Main"
    Layout
        paragraph
            visible when (not VendorReturnExists)
                Company
                    select is MobilePurchasingCompanyList
                Company.Name
                    no label
                    display as text
        paragraph
            ReturnFromLocation
                select is MobileInventoryLocations
                show up to 5 characters
            ReturnFromLocation.Name
                no label
                display as text
        paragraph
            Vendor
                select is MobileVendorSelectSearch
            Vendor.VendorName
                no label
                display as text
        paragraph
            ReasonCode
                select is MobileProcurementReasonCodesSelect
            ReasonCode.Description
                no label
                display as text
        paragraph
            ReplacementPurchaseOrderBuyer
                label is "Buyer"
                select is MobileBuyerSelect
                show up to 5 characters
            ReplacementPurchaseOrderBuyer.Employee.FirstLastName
                no label
                display as text
        visible when (ReplacementPurchaseOrder.PurchaseOrder entered)
            paragraph
                ReplacementPurchaseOrder.PurchaseOrder
                    label is "ReplacementPurchaseOrder"
                    display as text
        paragraph
            ReferenceNumber
        paragraph
            VendorReturnPOCode
                select is MobilePOCodeSelect
        paragraph distributed
            display as text
                MobileReturnValue
                    label is "ReturnValue"
            CreditReceived
