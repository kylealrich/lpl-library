SupplierCreateForm is an ActionForm
    action is SupplierCreate
    title is "CreateRequisitionLine"
    disable field options
    Layout
        two column distributed
            column1
                VendorItem
                    select is VendorItemsForASupplier
                Description
                EnteredUOM
                    label is "UOM"
                    select is SupplierUnitsOfMeasure
                paragraph
                    ConsignCode
                        label is "BillCode"
                    text of "DefaultBillCodeIs<Requisition.DefaultBillCode>"
            column2
                ManufacturerNumber
                    label is "ManufacturerPart"
                Quantity
                visible when (not TransientNoChargeFromSupplier)
                    TransactionUnitCost
                        label is "UnitCost"
                visible when (ConsignCode.WasteOnly)
                    TransientNoChargeFromSupplier
                        label is "WasteNoCharge"
        paragraph
            TransientServiceFlag
                label is "IsAService"
        visible when (not ItemType.Service)
            visible when (ItemType.Service)
                TransientVendor
        two column distributed
            column1
                visible when (CanDisplaySerialNumber)
                    TransientSerialNumber
                        label is "Serial"
                visible when (CanDisplayGTIN)
                    TransientGTIN
                        label is "GTIN"
            column2
                visible when (CanDisplayManufacturingDate)
                    TransientManufacturingDate
                        label is "ManufacturingDate"
                visible when (CanDisplayExpirationDate)
                    TransientExpirationDate
                        label is "ExpirationDate"
                visible when (CanDisplayLot)
                    TransientLot
                        label is "Lot"
