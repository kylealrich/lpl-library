MatchInvoiceInterfaceResultComposite is a CompositeForm
    title is "MatchInvoiceInterfaceResult"
    context form is MatchInvoiceInterfaceResultContext
        background is theme color
    MatchParameters is a Panel
        title is "Parameters"
        form is MatchParameters
    InterfaceTotals is a Panel
        form is MatchInterfaceTotals
    InvoicesCreated is a Panel
        list is PayablesInvoicesRel.MatchInterfaceListing
    PendingInvoicesForRunGroup is a Panel
        visible when (IncompleteHasPendingRecords)
        list is MatchInvoiceImportRel.InterfaceInvoicesList
    Metrics is a Panel
        visible when (ShowMetricsMatch)
        form is MatchMetrics
    AutoMatchResults is a Panel
        visible when (ShowAutoMatch)
        list is AutoMatchResultsRel.AutoMatchResults
