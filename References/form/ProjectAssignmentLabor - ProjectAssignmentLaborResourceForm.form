ProjectAssignmentLaborResourceForm is a Form
    title is "<FinanceEnterpriseGroup.ProjectLabel>AssignmentLabor"
    Layout
        two column distributed
            column1
                paragraph
                    Project
                        label is "<FinanceEnterpriseGroup.ProjectLabel>"
                        show up to 15 characters
                    Project.Description
                        no label
                        display as text
            column2
                paragraph
                    DateRange.Begin
                        no label
                        display as text
                    DateRange.End
                        no label
                        display as text
                    Status
                        display as text
        two column distributed
            column1
                PlannedPercent
            column2
                ActualPercent
        two column distributed
            column1
                DerivedDisplayEncumbranceAmount
                    label is "SalaryEncumbranceAmount"
                    display as text
            column2
                DerivedDisplayFringeAmount
                    label is "FringeEncumbranceAmount"
                    display as text
