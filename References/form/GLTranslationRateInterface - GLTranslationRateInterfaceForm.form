GLTranslationRateInterfaceForm is a Form
    is primary
    propagate as a drill
    title is "PeriodRateInterface"
    Layout
        paragraph
            FinanceEnterpriseGroup
            FinanceEnterpriseGroup.EnterpriseGroup.Description
                no label
                display as text
        paragraph
            GLTranslationRateInterface
                label is "SequenceNumber"
        paragraph
            GeneralLedgerTranslationCode
                label is "GlobalLedgerTranslationCode"
            GeneralLedgerTranslationCode.Description
                no label
                display as text
        paragraph
            BudgetTranslationRate
                label is "Budget"
        paragraph
            Scenario
                select is ActualsAndBudgets
            Scenario.Description
                no label
                display as text
        paragraph
            FromCurrency
            FromCurrency.Description
                no label
                display as text
        paragraph
            ToCurrency
            ToCurrency.Description
                no label
                display as text
        paragraph
            visible when (ActionTypeCreate)
                TransientCalendarPeriod
                    label is "Period"
                    select is XlationEndDates
                DerivedEndDate
                    label is "PeriodDate"
            visible when (not ActionTypeCreate)
                TransUpdatedCalendarPeriod
                    label is "Period"
                    select is XlationEndDates
                TranslationDate
                    label is "PeriodDate"
        paragraph
            Rate
                label is "Rate"
        paragraph
            ErrorDescription
                display only
