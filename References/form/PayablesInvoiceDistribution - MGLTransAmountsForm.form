MGLTransAmountsForm is a Form
    title is "Amounts"
    Layout
        header2 of "Ledger"
        single column
            visible when (Status.Unreleased)
                GLTransactionAmount
                    label is "DistributionAmount"
            visible when (not Status.Unreleased)
                GLTransactionAmount
                    label is "DistributionAmount"
                    display as text
        paragraph
            visible when (Status.Unreleased)
                GLFinanceCodeBlock
                    label is "FinanceStructure"
                    compact format
            visible when (not Status.Unreleased)
                GLFinanceCodeBlock
                    label is "FinanceStructure"
                    display as text
                    compact format
            visible when (ShowVendorAccountRestrictions)
                button of "Restrictions"
                    list is PayablesInvoice.Vendor.VendorAccountRestrictionRel.VendorAccountRestrictionDisplayList
        single column
            GLFinanceCodeBlock.GeneralLedgerChartAccount.AccountDescription
                display as text
        two column distributed
            display as text
                InvoiceCurrency
                    label is "<DerivedCurrencyLabel>"
                Company.GeneralLedgerCompany.Currency
                    label is "FunctionalCurrency"
        visible when (PayablesInvoice.PurchaseOrderInvoice)
            text of "FieldsForLinkingDistributionToInvoiceDetails_(LeaveBlankForMiscellaneousDistribution)"
        visible when (PayablesInvoice.MatchProcessType.ServiceContract)
            text of "FieldsForLinkingDistributionToInvoiceDetails_(LeaveBlankForMiscellaneousDistribution)"
        visible when (PayablesInvoice.PurchaseOrderInvoice)
            two column distributed
                PurchaseOrderLine
                    select is PurchaseOrderLinesForInvoiceRel.PurchaseOrderLinesList
                PayablesInvoiceDetail.MatchDetailKey
                    label is "Item"
                    display as text
            single column
                TransientPercent
                    label is "Percent"
                OriginalPOLineDistribution
                    label is "SourceDistribution"
            visible when (PayablesInvoice.MatchProcessType.ServiceContract)
                two column distributed
                    ContractLine
                        select is ContractLinesForInvoiceRel.ContractLinesToInvoiceHelperList
                    PayablesInvoiceDetail.MatchDetailKey
                        label is "Item"
                        display as text
                single column
                    TransientPercent
                        label is "Percent"
                    OriginalContractLineDistribution
                        label is "SourceDistribution"
        single column
            visible when (false)
                PurchaseOrder
                    display as text
                PurchasingContract
                    display as text
        visible when (TestingOn)
            single column
                button of "TransactionDetail"
                    list is GLTransactionDetailRel.MGLTransactionDetailList
        header2 of "Tax"
        two column distributed
            column1
                TaxAccountingEntity
                    label is "TaxEntity"
            column2
                visible when (DisplayTaxIndicator)
                    TaxIndicator
        two column distributed
            TaxCode
                select is TaxCodeFilteredList
            TaxCode.Description
                no label
                display as text
        visible when (TaxCode entered)
            display as text
                two column distributed
                    TaxType
                        label is "Type"
                    TaxRate
        single column
            TaxableAmount
        two column distributed
            TaxUsageCode
            TaxUsageCode.Description
                no label
                display as text
        two column distributed
            ProductTaxCategory
            ProductTaxCategory.Description
                no label
                display as text
        two column distributed
            visible when (IsHSNSACCodeEnabled)
                HSNSACCode
                HSNSACCode.Description
                    no label
                    display as text
        visible when (PayablesInvoice.ForIntercompanyBilling)
            header2 of "IntercompanyBilling"
            two column distributed
                ICBillToCompany.Company
                    label is "BillToCompany"
                    select is PayablesCompanySelectForIntercompanyBilling
                ICBillToCompany.Company.Name
                    no label
                    display as text
            two column distributed
                ICBillingRechargeItem
                    label is "RechargeItem"
                    select is ActiveRechargeItemList
                ICBillingRechargeItem.Description
                    no label
                    display as text
        header2 of "CurrencyAmounts"
        display as text
            single column
                visible when (false)
                    TransientFinanceCurrencyAmt.AssignToCurrency
            four column table
                column headers
                    "_"
                    "Amount"
                    "Currency"
                    "Rate"
                column1
                    text of "Functional"
                    visible when (DistributionAmount.AccountingEntity.AlternateCurrencyExists)
                        text of "Alternate"
                    visible when (DistributionAmount.AccountingEntity.AlternateCurrency2Exists)
                        text of "Alternate2"
                    visible when (DistributionAmount.AccountingEntity.AlternateCurrency3Exists)
                        text of "Alternate3"
                    visible when (DistributionAmount.InterAccountingEntityTransaction)
                        text of "PostToFunctional"
                    visible when (DistributionAmount.InterAccountingEntityAlternate)
                        text of "PostToAlternate"
                    visible when (DistributionAmount.InterAccountingEntityAlternate2)
                        text of "PostToAlternate2"
                    visible when (DistributionAmount.InterAccountingEntityAlternate3)
                        text of "PostToAlternate3"
                    visible when (DistributionAmount.ProjectAmount entered)
                        text of "<TransientFinanceCurrencyAmt.FinanceEnterpriseGroup.ProjectLabel>"
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        text of "<TransientFinanceCurrencyAmt.FinanceEnterpriseGroup.ReportOne>"
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        text of "<TransientFinanceCurrencyAmt.FinanceEnterpriseGroup.ReportTwo>"
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        text of "<TransientFinanceCurrencyAmt.FinanceEnterpriseGroup.ReportThree>"
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        text of "<TransientFinanceCurrencyAmt.FinanceEnterpriseGroup.ReportFour>"
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                        text of "<TransientFinanceCurrencyAmt.FinanceEnterpriseGroup.ReportFive>"
                column2
                    TransientFinanceCurrencyAmt.FunctionalAmount.EnteredCurrencyAmount
                        no label
                    visible when (DistributionAmount.AccountingEntity.AlternateCurrencyExists)
                        TransientFinanceCurrencyAmt.AlternateAmount.EnteredCurrencyAmount
                            no label
                    visible when (DistributionAmount.AccountingEntity.AlternateCurrency2Exists)
                        TransientFinanceCurrencyAmt.AlternateAmount2.EnteredCurrencyAmount
                            no label
                    visible when (DistributionAmount.AccountingEntity.AlternateCurrency3Exists)
                        TransientFinanceCurrencyAmt.AlternateAmount3.EnteredCurrencyAmount
                            no label
                    visible when (DistributionAmount.InterAccountingEntityTransaction)
                        TransientFinanceCurrencyAmt.ToFunctionalAmount.EnteredCurrencyAmount
                            no label
                    visible when (DistributionAmount.InterAccountingEntityAlternate)
                        TransientFinanceCurrencyAmt.ToAlternateAmount.EnteredCurrencyAmount
                            no label
                    visible when (DistributionAmount.InterAccountingEntityAlternate2)
                        TransientFinanceCurrencyAmt.ToAlternateAmount2.EnteredCurrencyAmount
                            no label
                    visible when (DistributionAmount.InterAccountingEntityAlternate3)
                        TransientFinanceCurrencyAmt.ToAlternateAmount3.EnteredCurrencyAmount
                            no label
                    visible when (DistributionAmount.ProjectAmount entered)
                        TransientFinanceCurrencyAmt.ProjectAmount.EnteredCurrencyAmount
                            no label
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        TransientFinanceCurrencyAmt.ReportAmount1.EnteredCurrencyAmount
                            no label
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        TransientFinanceCurrencyAmt.ReportAmount2.EnteredCurrencyAmount
                            no label
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        TransientFinanceCurrencyAmt.ReportAmount3.EnteredCurrencyAmount
                            no label
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        TransientFinanceCurrencyAmt.ReportAmount4.EnteredCurrencyAmount
                            no label
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                        TransientFinanceCurrencyAmt.ReportAmount5.EnteredCurrencyAmount
                            no label
                column3
                    TransientFinanceCurrencyAmt.AccountingEntity.FunctionalCurrency
                        no label
                    visible when (DistributionAmount.AccountingEntity.AlternateCurrencyExists)
                        TransientFinanceCurrencyAmt.AccountingEntity.AlternateCurrency
                            no label
                    visible when (DistributionAmount.AccountingEntity.AlternateCurrency2Exists)
                        TransientFinanceCurrencyAmt.AccountingEntity.AlternateCurrency2
                            no label
                    visible when (DistributionAmount.AccountingEntity.AlternateCurrency3Exists)
                        TransientFinanceCurrencyAmt.AccountingEntity.AlternateCurrency3
                            no label
                    visible when (DistributionAmount.InterAccountingEntityTransaction)
                        TransientFinanceCurrencyAmt.TransactionCodeBlock.ToAccountingEntity.FunctionalCurrency
                            no label
                    visible when (DistributionAmount.InterAccountingEntityAlternate)
                        TransientFinanceCurrencyAmt.TransactionCodeBlock.ToAccountingEntity.AlternateCurrency
                            no label
                    visible when (DistributionAmount.InterAccountingEntityAlternate2)
                        TransientFinanceCurrencyAmt.TransactionCodeBlock.ToAccountingEntity.AlternateCurrency2
                            no label
                    visible when (DistributionAmount.InterAccountingEntityAlternate3)
                        TransientFinanceCurrencyAmt.TransactionCodeBlock.ToAccountingEntity.AlternateCurrency3
                            no label
                    visible when (DistributionAmount.ProjectAmount entered)
                        TransientFinanceCurrencyAmt.TransactionCodeBlock.Project.Currency
                            no label
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        TransientFinanceCurrencyAmt.FinanceEnterpriseGroup.ReportCurrencyOne
                            no label
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        TransientFinanceCurrencyAmt.FinanceEnterpriseGroup.ReportCurrencyTwo
                            no label
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        TransientFinanceCurrencyAmt.FinanceEnterpriseGroup.ReportCurrencyThree
                            no label
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        TransientFinanceCurrencyAmt.FinanceEnterpriseGroup.ReportCurrencyFour
                            no label
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                        TransientFinanceCurrencyAmt.FinanceEnterpriseGroup.ReportCurrencyFive
                            no label
                column4
                    TransientFinanceCurrencyAmt.FunctionalAmount.EnteredCurrencyRate
                        no label
                    visible when (DistributionAmount.AccountingEntity.AlternateCurrencyExists)
                        TransientFinanceCurrencyAmt.AlternateAmount.EnteredCurrencyRate
                            no label
                    visible when (DistributionAmount.AccountingEntity.AlternateCurrency2Exists)
                        TransientFinanceCurrencyAmt.AlternateAmount2.EnteredCurrencyRate
                            no label
                    visible when (DistributionAmount.AccountingEntity.AlternateCurrency3Exists)
                        TransientFinanceCurrencyAmt.AlternateAmount3.EnteredCurrencyRate
                            no label
                    visible when (DistributionAmount.InterAccountingEntityTransaction)
                        TransientFinanceCurrencyAmt.ToFunctionalAmount.EnteredCurrencyRate
                            no label
                    visible when (DistributionAmount.InterAccountingEntityAlternate)
                        TransientFinanceCurrencyAmt.ToAlternateAmount.EnteredCurrencyRate
                            no label
                    visible when (DistributionAmount.InterAccountingEntityAlternate2)
                        TransientFinanceCurrencyAmt.ToAlternateAmount2.EnteredCurrencyRate
                            no label
                    visible when (DistributionAmount.InterAccountingEntityAlternate3)
                        TransientFinanceCurrencyAmt.ToAlternateAmount3.EnteredCurrencyRate
                            no label
                    visible when (DistributionAmount.ProjectAmount entered)
                        TransientFinanceCurrencyAmt.ProjectAmount.EnteredCurrencyRate
                            no label
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        TransientFinanceCurrencyAmt.ReportAmount1.EnteredCurrencyRate
                            no label
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        TransientFinanceCurrencyAmt.ReportAmount2.EnteredCurrencyRate
                            no label
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        TransientFinanceCurrencyAmt.ReportAmount3.EnteredCurrencyRate
                            no label
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        TransientFinanceCurrencyAmt.ReportAmount4.EnteredCurrencyRate
                            no label
                    visible when (Company.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                        TransientFinanceCurrencyAmt.ReportAmount5.EnteredCurrencyRate
                            no label
        visible when (CanCreatePrepaidAmortization)
            blank line
            header2 of "PrepaidAmortization"
            single column
                TransientCreatePrepaidAmort
            visible when (TransientCreatePrepaidAmort)
                single column
                    TransientReconciliationAccountStructure
                        select is ForPayablesInvoiceDistributionList
                    TransientReconciliationAccountStructure.Description
                single column
                    TransientReconciliationAccount
                        select is ReconciliationAccountsByCodeBlockRel.PayablesInvoiceDistributionList
                    TransientReconciliationAccount.ReconciliationCodeBlock
                        label is "Account"
                        display as text
                blank line
                header2 of "AmortizationDetails"
                two column distributed
                    TransientReferenceNumber
                    TransientDescription
                    paragraph
                        PayablesInvoice.InvoiceDate
                            label is "TransactionDate"
                            display only
                        PayablesInvoice.DistributionDate
                            label is "PostingDate"
                            display only
                    TransientDetailAmount
                    paragraph
                        TransientStartPeriod
                            compact format
                        TransientStartPeriod.PeriodName
                            no label
                            display as text
                    paragraph
                        TransientEndPeriod
                            compact format
                        TransientEndPeriod.PeriodName
                            no label
                            display as text
                    paragraph
                        TransientCatchUpPeriod
                            compact format
                        TransientCatchUpPeriod.PeriodName
                            no label
                            display as text
                two column distributed
                    TransientPeriodTerm
                    TransientAmortizationSpread
                blank line
                header2 of "AmortizationExpenseAccount"
                paragraph
                    TransientAmortizationExpenseAccount
                        compact format
                single column
                    text of "-or-"
                two column distributed
                    TransientAllocationCode
                        select is PercentCashAllocationCodes
                    TransientAllocationCode.Description
                        no label
                        display as text
                single column
                    TransientDistributionDescription
