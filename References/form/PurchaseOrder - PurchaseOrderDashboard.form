PurchaseOrderDashboard is a CompositeForm
    title is "PurchaseOrder<PurchaseOrder.PurchaseOrder>Details"
    context form is PurchaseOrderDashboardContext
        background is theme color
    restrict action CreateInvoicePurchaseOrder
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    restrict action CreateInvoicePurchaseOrder
    Actions
        action is Unreleased.Release
            action icon is release
            hide when invalid
        action is IssueDraft
            hide when invalid
        action is Released.IssueFinal
            action icon is print
            hide when invalid
        action is Released.RegenerateDocument
            action icon is pdf
            hide when invalid
        action is Canceled.IssueFinal
            action icon is print
            hide when invalid
        action is Canceled.RegenerateDocument
            action icon is pdf
            hide when invalid
        action is Released.CancelPurchaseOrder
            action icon is reject
            hide when invalid
        action is Released.CancelRemainingPurchaseOrder
            action icon is reject
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is refresh
            label is "_"
    Lists is a MultiListPanel
        title is "Details"
        list is PurchaseOrderLinesRel.LinesForPurchaseOrder
            visible when (PurchaseOrderLinesExist)
        list is PurchaseOrderReceiptsRel.PurchaseOrderReceiptsForPO
            visible when (PurchaseOrderReceiptExists)
        list is MatchedPurchaseOrderInvoicesRel.InvoicesForAPurchaseOrder
            visible when (MatchPurchaseOrderInvoicesForAPurchaseOrderExist)
        list is AnyMatchPurchaseOrderInvoicesWithChargebackByPORel.ChargebackAnalysisList
            visible when (AnyMatchPurchaseOrderInvoicesWithChargebackExists)
        list is PurchaseOrderLinesRel.OpenToReceiveLines
            visible when (not ReceivedInFull)
        list is PurchaseOrderLinesRel.ReceivedNotInvoicedLines
            visible when (PurchaseOrderLinesReceivedNotInvoicedExist)
        list is ReceivingBuyerMessagesRel.ReceivingBuyerMessages
            visible when (BuyerMessagesExist)
        list is ReceivingAdjustmentRel.AdjustmentsForAPurchaseOrder
            visible when (AdjustmentsExist)
        list is ReceivingInspectionRel.InspectionsForAPurchaseOrder
            visible when (InspectionsExist)
        list is PurchaseOrderAddOnChargesRel.MiscellaneousAddOnCostsForAPurchaseOrder
            visible when (MiscellaneousAddOnChargeExists)
        list is PurchaseOrderSpreadAddOnCharge set.PurchaseOrderSpreadAddOnChargeList
            visible when (PurchaseOrderSpreadAddOnChargeExists)
        list is GLCommitDrillRel.PODrillGLCommitList
            visible when (CommitmentsExist)
        list is PurchaseOrderRevisionsRel.RevisionsforaPurchaseOrder
            visible when (HasRevision)
