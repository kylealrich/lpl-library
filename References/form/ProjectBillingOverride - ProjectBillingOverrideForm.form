                            label is "<FinanceEnterpriseGroup.FinanceDimension5Label>"
                            select is FinanceEnterpriseGroup.EnterpriseDim5Structure.DimensionNode.DimensionChildrenRel.FinanceDimension5TreeSelect
                            show up to 10 characters
                        ProjectCodeBlock.FinanceDimension5.Description
                            no label
                            display as text
                    visible when (FinanceEnterpriseGroup.BillingOverrideLevel.Dimension6)
                        ProjectCodeBlock.FinanceDimension6
                            label is "<FinanceEnterpriseGroup.FinanceDimension6Label>"
                            select is FinanceEnterpriseGroup.EnterpriseDim6Structure.DimensionNode.DimensionChildrenRel.FinanceDimension6TreeSelect
