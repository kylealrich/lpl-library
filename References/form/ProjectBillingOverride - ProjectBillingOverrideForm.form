ProjectBillingOverrideForm is a Form
    is primary
    title is "<FinanceEnterpriseGroup.ProjectLabel>BillingOverride"
    Layout
        two column distributed
            column1
                paragraph
                    Project
                        label is "<FinanceEnterpriseGroup.ProjectLabel>"
                        display as text
                    Project.Description
                        no label
                        display as text
        two column distributed
            column1
                paragraph
                    visible when (FinanceEnterpriseGroup.BillingOverrideLevel.Account)
                        ProjectCodeBlock.GeneralLedgerChartAccount
                            label is "<FinanceEnterpriseGroup.AccountLabel>"
                            select is FinanceEnterpriseGroup.EnterpriseChart.ChartAccountNode.ChartAccountChildrenRel.ChartAccountList
                            show up to 10 characters
                        ProjectCodeBlock.GeneralLedgerChartAccount.AccountDescription
                            no label
                            display as text
                    visible when (FinanceEnterpriseGroup.BillingOverrideLevel.Dimension1)
                        ProjectCodeBlock.FinanceDimension1
                            label is "<FinanceEnterpriseGroup.FinanceDimension1Label>"
                            select is FinanceEnterpriseGroup.EnterpriseDim1Structure.DimensionNode.DimensionChildrenRel.FinanceDimension1TreeSelect
                            show up to 10 characters
                        ProjectCodeBlock.FinanceDimension1.Description
                            no label
                            display as text
                    visible when (FinanceEnterpriseGroup.BillingOverrideLevel.Dimension3)
                        ProjectCodeBlock.FinanceDimension3
                            label is "<FinanceEnterpriseGroup.FinanceDimension3Label>"
                            select is FinanceEnterpriseGroup.EnterpriseDim3Structure.DimensionNode.DimensionChildrenRel.FinanceDimension3TreeSelect
                            show up to 10 characters
                        ProjectCodeBlock.FinanceDimension3.Description
                            no label
                            display as text
                    visible when (FinanceEnterpriseGroup.BillingOverrideLevel.Dimension4)
                        ProjectCodeBlock.FinanceDimension4
                            label is "<FinanceEnterpriseGroup.FinanceDimension4Label>"
                            select is FinanceEnterpriseGroup.EnterpriseDim4Structure.DimensionNode.DimensionChildrenRel.FinanceDimension4TreeSelect
                            show up to 10 characters
                        ProjectCodeBlock.FinanceDimension4.Description
                            no label
                            display as text
                    visible when (FinanceEnterpriseGroup.BillingOverrideLevel.Dimension5)
