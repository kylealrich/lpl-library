MassAssetAdditions is an ActionForm
    action is MassAssetAdditions
    title is "AssetInterfaceUpdate"
    Layout
        header2 of "SelectionOptions"
        paragraph
            Source
            Release
                label is "Update"
        paragraph
            Compute
            Posting
                label is "PostingEntries"
        paragraph
            Company
                label is "Company"
                select is Companies
            text of "Or"
            CompanyGroup
                label is "CompanyGroup"
        paragraph
            PostingDate
            InsrvDate
                label is "InServiceDate"
        paragraph
            VendorGroup
            Vendor
            Invoice
                select is InterfacedInvoicesbyCompanyr
        paragraph
            Activity
                label is "AssetProject"
            Combine
        header2 of "AdditionalOptions"
        paragraph
            Template
            Template.Description
                no label
                display as text
        blank line
        paragraph
            TagNbr
                label is "Tag"
            AssetGroup
                select is AssetGroups
        blank line
        paragraph
            LocationName
                label is "Location"
                select is Locations
            LocationName.Description
                no label
                display as text
        paragraph
            Division
                select is Divisions
            Division.Description
                no label
                display as text
        paragraph
            DefAcctUnit
                label is "TransactionDimensions"
                select is PostingAccountingUnits
            DefAcctUnit.Description
                no label
                display as text
        blank line
        single column
            HoldCode
                label is "OnHoldOnly"
        header2 of "OutputOptions"
        header3 of "PrintTotals"
        single column
            header4 of "Sequence"
            paragraph
                LocOption
                    label is "Location"
                DivOption
                    label is "Division"
                TypeOption
                    label is "Type\\,Sub"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                MassAssetAdditionsReport_RDG
            column2 of 25%
                MassAssetAdditionsReport_XPT
