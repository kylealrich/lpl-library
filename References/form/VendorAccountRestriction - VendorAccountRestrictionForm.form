VendorAccountRestrictionForm is a Form
    is primary
    title is "VendorFinanceStructureRestriction"
    Layout
        two column distributed
            column1
                paragraph
                    visible when (not RecordExists)
                        VendorGroup
                    visible when (RecordExists)
                        VendorGroup
                            display as text
                    VendorGroup.Description
                        no label
                        display as text
            column2
                paragraph
                    visible when (not RecordExists)
                        Vendor
                    visible when (RecordExists)
                        Vendor
                            display as text
                    Vendor.VendorName
                        no label
                        display as text
        paragraph
            GeneralLedgerChartAccount
                label is "<VendorGroup.BusinessGroup.FinanceEnterpriseGroup.AccountLabel>"
            GeneralLedgerChartAccount.AccountDescription
                no label
                display as text
        paragraph
            AccountingEntity
                label is "<VendorGroup.BusinessGroup.FinanceEnterpriseGroup.AccountingEntityLabel>"
            AccountingEntity.Name
                no label
                display as text
        paragraph
            AccountingUnit
                label is "<VendorGroup.BusinessGroup.FinanceEnterpriseGroup.AccountingUnitLabel>"
            AccountingUnit.Description
                no label
                display as text
