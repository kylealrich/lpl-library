VarianceReasonsOnly is a Form
    title is "EnterVarianceReason"
    Layout
        display as text
            paragraph
                PhysicalInventorySelect.Company.DerivedCompanyDisplay
                    label is "Company"
            paragraph
                PhysicalInventorySelect.DisplaySelectID
                    label is "SelectI\\D"
            paragraph
                PhysicalInventorySelect.Status
            blank line
            paragraph
                PhysicalInventoryFreeze.PhysicalInventoryFreezePage
                    label is "Page"
                PhysicalInventoryFreeze.PhysicalInventoryFreezeLine
                    label is "Line"
        paragraph
            InventoryReasonCode
                label is "ReasonCode"
                select is ActiveReasonCodes
            InventoryReasonCode.Description
                no label
                display as text
        display as text
            paragraph
                InventoryLocation.DerivedInventoryLocationIDName
                    label is "InventoryLocation"
            paragraph
                Item
                Item.Description
                    no label
                Item.Description2
                    no label
            paragraph
                UnitOfMeasure
                    label is "UOM"
            paragraph
                visible when (RecordExistsForBinTracked)
                    Bin
            paragraph
                visible when (RecordExistsForLotTracked)
                    Lot
                    Sublot
            paragraph
                visible when (RecordExistsForSerialTracked)
                    Serial
            paragraph
                CountQuantity
                DerivedVariance
            visible when (NotEnteredInStockUOM)
                paragraph
                    text of "StockUnitOfMeasure:"
                    CountQuantity
                    UnitOfMeasure
                    visible when (RecordExists)
                        DerivedVariance
            visible when (RecordExistsForCatchWeightItem)
                paragraph
                    SecondaryCountQuantity
                    SecondaryUOM
                    SecondaryVarianceQuantity
            paragraph
                visible when (DisplayUnitCost)
                    UnitCost
                        label is "UnitCostInStockUnitOfMeasure"
