FinanceDimension2MainForm is a Form
    title is "<MainLocationTabMessage>"
    Layout
        visible when (DimensionType.Summary)
            two column distributed
                column1
                    Description
        visible when (IsPostingNonProjectInvoicing)
            two column distributed
                column1
                    Description
        visible when (not FEGHasProjectInvoicing)
            two column
                column1
                    paragraph
                        effective date
                        has future changes
                            label is "FutureChanges"
        visible when (false)
            single column
                FinanceDimension2Structure
        visible when (DimensionType.Summary)
            visible when (IsAchild)
                paragraph
                    BudgetIdentifier
                    BudgetIdentifier.Description
                        no label
                        display as text
            visible when (not IsAchild)
                paragraph
                    BudgetIdentifier
                        display only
                    BudgetIdentifier.Description
                        no label
                        display as text
        visible when (DimensionType.Posting)
            two column distributed
                column1
                    paragraph
                        SystemDimension
                            label is "System<FinanceEnterpriseGroup.FinanceDimension2Label>"
                        IncludeInBudgetEdit
                            label is "IncludeInBudgetEdit"
        visible when (IsProjectInvoicingSummary)
            two column distributed
                column1
                    paragraph
                        PersonResponsible
                        PersonResponsible.Name.PreferredFirstAndLastName
                            no label
                            display as text
            two column distributed
                column1
                    paragraph
                        ProjectTeam
                            show up to 20 characters
                        ProjectTeam.Description
                            no label
                            display as text
        header2 of "Address"
        four column distributed
            column1
                paragraph
                    AddressCode
                    AddressCode.Description
                        no label
                        display as text
            visible when (AddressCode entered)
                column2
                    AddressCode.PostalAddress.DisplayAddressMultiline
                        label is "Address"
                        display as text
        visible when (AddressCode entered)
            two column distributed
                column1
                    paragraph
                        AddressCode.PersonResponsible
                            label is "PersonResponsible"
                            display as text
                        AddressCode.PersonResponsible.PresentationNameSnapshot
                            no label
                            display as text
            display as text
                single column
                    AddressCode.TelephoneNumber
                        label is "TelephoneNumber"
                    AddressCode.FaxNumber
                        label is "FaxNumber"
                    AddressCode.EmailAddress
                        label is "EmailAddress"
