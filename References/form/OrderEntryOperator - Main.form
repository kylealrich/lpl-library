Main is a Form
    Layout
        visible when (not RecordExists)
            paragraph
                Company
                Company.Name
                    no label
                    display as text
            two column distributed
                column1
                    OrderEntryOperator
                        label is "UserI\\D"
                column2
                    Name
                        label is "Name"
        header2 of "Rules"
        three column distributed
            column1
                RestrictToDefaults
                    label is "Restrict_toDefaults"
                AllowLocationTransfers
                    label is "AllowLocationTransfers"
            column2
                CurrencyOverride
                    label is "CurrencyOverride"
                CurrencyRateOverride
                    label is "CurrencyRateOverride"
        header2 of "Defaults"
        two column distributed
            column1
                DefaultOrderType
                    label is "OrderType"
            column2
                paragraph
                    BillingProcessLevel
                        label is "ProcessLevel"
                    BillingProcessLevel.Name
                        no label
                        display as text
        two column distributed
            column1
                paragraph
                    DefaultCustomer
                        label is "Customer"
                        select is CustomerContextCompanySelectList
                    DefaultCustomer.Name
                        no label
                        display as text
            column2
                DefaultOrderSource
                    label is "OrderSource"
        paragraph
            DefaultLocation
                label is "InventoryLocation"
            DefaultLocation.Name
                no label
                display as text
        two column distributed
            column1
                DefaultOrderDate
                    label is "OrderDate"
            column2
                DefaultShipDate
                    label is "ExpectedShipDate"
