InventoryAvailabilityReport is an ActionForm
    action is InventoryAvailabilityReport
    Layout
        three column table
            column1
                paragraph
                    Company
                    Company.Name
                        no label
                        display as text
                paragraph
                    Location
                        show up to 10 characters
                    Location.Name
                        no label
                        display as text
            column2
                text of "\\or"
                    bold
                text of "\\or"
                    bold
            column3
                paragraph
                    CompanyGroup
                        label is "CompanyGroup"
                    CompanyGroup.Description
                        no label
                        display as text
                paragraph
                    ReportGrp
                        label is "ReportGroup"
                    ReportGrp.Name
                        no label
                        display as text
        blank line
        two column table
            column1
                paragraph
                    InvenMajcl
                        label is "MajorInventoryClass"
                        show up to 10 characters
                    InvenMajcl.Description
                        no label
                        display as text
            column2
                paragraph
                    InvenMincl
                        label is "MinorInventoryClass"
                        show up to 10 characters
                    InvenMincl.Description
                        no label
                        display as text
        blank line
        single column
            Sequence
        two column distributed
            column1
                PrintZeroQtys
                    label is "ZeroQuantityItems"
            column2
                IncludeInactive
                    label is "IncludeInactiveItems"
        single column
            PrintSupplementalUOM
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                InventoryAvailabilityReportReport_RDG
            column2 of 25%
                InventoryAvailabilityReportReport_XPT
