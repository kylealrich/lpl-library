MobileCashManagementAccount is a CompositeForm
    title is "CashManagementAccount"
    context form is MobileCashManagementAccountContext
        background is theme color
    Detail is a Panel
        form is CashManagementAccountDetail
    CashCodes is a Panel
        visible when (HasCashCodes)
        list is CashCodeRel.MobileCashCodeSelectList
    Reconciliation is a Panel
        form is AccountReconciliationInformation
    Import is a Panel
        form is CashManagementAccountImportParameters
    Resources is a Panel
        list is CashManagementAccountResource set.MobileCashManagementAccountResourceList
            helper list is FinanceResourceRel.FinanceResourceHelperList
                action is CreateAccountResource
                    invoked.CashManagementGroup = CashManagementGroup
                    invoked.CashManagementAccount = CashManagementAccount
    Lockboxes is a Panel
        list is CashAccountLockbox set.MobileCashAccountLockboxList
    Correspondence is a Panel
        list is CashAccountCorrespondence set.MobileCashAccountCorrespondenceList
    Documents is a Panel
        list is CashManagementAccountDocument set.MobileCashManagementAccountDocumentList
    Comments is a Panel
        list is CashAccountComment set.MobileCashAccountCommentList
