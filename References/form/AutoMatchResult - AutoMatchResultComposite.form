AutoMatchResultComposite is a CompositeForm
    is primary
    title is "AutoMatchResult<AutoMatchResult>"
    context form is AutoMatchResultContext
    Actions
        navigation is SummaryNavigation
            label is "SummaryDashboard"
            hide when invalid
        action is refresh
            label is "_"
        action is previous record
            label is "_"
        action is next record
            label is "_"
    Main is a Panel
        form is AutoMatchResultMain
    InProcess is a Panel
        visible when (InvoicesInProcess)
        list is InProcessInvoicesRel.SelectedInvoices
    Matched is a Panel
        list is MatchedInvoicesRel.Invoices
    UnmatchedPayablesIssues is a Panel
        list is UnmatchedPayablesIssuesRel.UnmatchedPayablesInvoices
    UnmatchedProcurementIssues is a Panel
        list is UnmatchedProcurementIssuesRel.UnmatchedProcurementInvoices
    ProcessingErrors is a Panel
        visible when (TerminatedInvoicesExist)
        list is UnmatchedAsyncErrorRel.TerminatedInvoices
    All is a Panel
        list is AllInvoicesRel.AllInvoices
    Parameters is a Panel
        form is Parameters
    VendorSummary is a MultiListPanel
        title is "VendorSummary"
        visible when (not PrmSummarize.DoNotSummarize)
        list is VendorSummaryTotalRel.VendorSummaryTotal
        list is VendorSummaryRel.VendorSummary
    BuyerSummary is a MultiListPanel
        title is "BuyerSummary"
        visible when (not PrmSummarize.DoNotSummarize)
        list is BuyerSummaryTotalRel.BuyerSummaryTotal
        list is BuyerSummaryRel.BuyerSummary
    ErrorNumberSummary is a MultiListPanel
        title is "ErrorNumberSummary"
        visible when (not PrmSummarize.DoNotSummarize)
        list is ErrorNumberSummaryTotalRel.ErrorNumberSummaryTotal
        list is ErrorNumberSummaryRel.ErrorNumberSummary
