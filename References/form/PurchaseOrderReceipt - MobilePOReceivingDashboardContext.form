MobilePOReceivingDashboardContext is a Form
    title is "PurchaseOrderReceiptDetails"
    Layout
        paragraph
            Company
                display as text
            Company.Name
                no label
                display as text
        paragraph
            PurchaseOrder
                display as text
        paragraph
            PurchaseOrderReceipt
                label is "Receiver"
                display as text
        paragraph
            ReferenceNumber
                label is "Reference"
                display as text
        paragraph
            PurchaseOrder.PurchaseFromLocation
                label is "PurchaseFrom"
                display as text
            PurchaseFromLocation.VendorName
                no label
                display as text
        paragraph
            ReceivedDate
                label is "DateAndTimeReceived"
                display as text
        paragraph
            PurchaseOrder.Buyer
                label is "Buyer"
                display as text
            PurchaseOrder.BuyerEmployeeRel.PresentationNameSnapshot
                no label
                display as text
        paragraph
            ShipToLocation
                label is "ReceivedLocation"
                display as text
            ShipToLocation.Name
                no label
                display as text
        paragraph
            Status
                label is "Status"
                display as text
        paragraph
            Vendor
                display as text
            Vendor.VendorName
                no label
                display as text
        paragraph
            Vendor.Logo.File
                no label
                display as full image up to 5 lines
                    missing image is missingpicture96px
