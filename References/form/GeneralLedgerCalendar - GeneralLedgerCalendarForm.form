GeneralLedgerCalendarForm is a Form
    title is "GlobalLedgerCalendar"
    Layout
        paragraph
            FinanceEnterpriseGroup
            FinanceEnterpriseGroup.EnterpriseGroup.Description
                no label
                display as text
        single column
            GeneralLedgerCalendar
                label is "GlobalLedgerCalendar"
            Description
            CalendarType
            visible when (CalendarType.5253Week)
                YearEndMonth
                YearEndLastDay
                YearEndLastDayMethod
            visible when (CalendarType.Days)
                DaysHierarchy
            visible when (CalendarType.Weeks)
                WeeksMethod
                text of "ISO_Method:_AllWeeksBeginOnA_Monday._Week_OneStartsOn_MondayOfTheFirstWeekOfTheCalendarYearContainingA_Thursday._TheLastWeekEndsOnThe_SundayPriorTo_Week_OneOfTheNextYear."
                    bold
                text of "Excel_Method_1:_Week_OneBeginsOn_January_1st._Week_TwoBeginsOnTheFollowing_Sunday."
                    bold
                text of "Excel_Method_2:_Week_OneBeginsOn_January_1st._Week_TwoBeginsOnTheFollowing_Monday."
                    bold
                text of "Simple_Numbering_Method:_Week_OneBeginsOn_January_1st._Week_TwoBeginsOn_January_8th._Week_53HasOnlyOneOrTwoDays."
                    bold
            DefaultYearDisplayPeriod
                show up to 100 characters
            SampleYearDisplayPeriod
                label is "Sample"
                display as text
            DefaultQuarterDisplayPeriod
                show up to 100 characters
            SampleQuarterDisplayPeriod
                label is "Sample"
                display as text
            DefaultMonthDisplayPeriod
                show up to 100 characters
            SampleMonthDisplayPeriod
                label is "Sample"
                display as text
            visible when (ShowWeekTemplate)
                DefaultWeekDisplayPeriod
                    show up to 100 characters
                SampleWeekDisplayPeriod
                    label is "Sample"
                    display as text
            visible when (CalendarType.Days)
                DefaultDayDisplayPeriod
                    show up to 100 characters
                SampleDayDisplayPeriod
                    label is "Sample"
                    display as text
            DefaultEndDateDisplayPeriod
                show up to 100 characters
            SampleEndDateDisplayPeriod
                label is "Sample"
                display as text
