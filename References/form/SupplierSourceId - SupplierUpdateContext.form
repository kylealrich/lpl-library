SupplierUpdateContext is a Form
    Actions
        action is refresh
    Layout
        header2 of "SupplierInformation"
        visible when (not actor.agent(SupplierSourceId).InactiveAtSomeLevel)
            visible when (PortalContact)
                display as text
                    paragraph
                        Supplier
                            label is "SupplierNumber"
                            display as text
                        Supplier.SupplierName
                            no label
                    three column
                        Supplier.DoingBusinessAs
                        Supplier.DandBNbr
                            label is "Dun&Bradstreet"
                        Supplier.BusinessType
                    paragraph
                        Supplier.TaxIdGroup.TaxIdType
                        Supplier.TaxIdGroup.TaxIdType.Description
                            no label
                            display as text
                        visible when (Supplier.TaxCertificationExists)
                            button of "View<TaxCertificationLabel>"
                                view Supplier.TaxCertification.File
                                display as link
                    two column
                        Supplier.VATRegistrationCountry
                        Supplier.VATRegistrationNumber
                    two column
                        visible when (SupplierGroup.DisplayGTINAndGLNInSupplierPortal)
                            Supplier.GlobalLocationNumber
                        Supplier.WebAddress
                            label is "Website"
                visible when (IsPrimary)
                    text of "Use_Update_Supplier_InformationToUpdateSupplierInformation"
                        italics
                        color of blue
                visible when (SupplierGroup.AllowBankInformation)
                    header2 of "SupplierBankInformation"
                    display as text
                        visible when (IsPrimary)
                            two column
                                Supplier.BankInformationGroup.Country
                                text of "Use_Update_Supplier_Bank_Country/\\JurisdictionToUpdateBankCountry/\\Jurisdiction"
                                    italics
                                    color of blue
                        single column
                            visible when (Supplier.BankInformationGroup.DisplayBankEntity)
                                paragraph
                                    Supplier.BankInformationGroup.BankEntity
                                        label is "<Supplier.BankInformationGroup.DerivedBankEntityLabel>"
                                    visible when (Supplier.BankInformationGroup.BankEntityExists)
                                        Supplier.BankInformationGroup.BankEntity.BankName
                                            display as text
                                    visible when (not Supplier.BankInformationGroup.BankEntityExists)
                                        Supplier.BankInformationGroup.BankEntityName
                            visible when (Supplier.BankInformationGroup.DisplayBankAccountNumber)
                                visible when (not Supplier.BankInformationGroup.BankAccountNumberEntered)
                                    visible when (not Supplier.BankInformationGroup.MaskBankInformationEntry)
                                        Supplier.BankInformationGroup.BankAccountNumber
                                    visible when (Supplier.BankInformationGroup.MaskBankInformationEntry)
                                        Supplier.BankInformationGroup.BankAccountNumber
                                            hidden entry
                                visible when (Supplier.BankInformationGroup.BankAccountNumberEntered)
                                    Supplier.BankInformationGroup.DerivedDisplayBankAccountNumber
                                        label is "<Supplier.BankInformationGroup.DerivedBankAccountNumberLabel>"
                            visible when (Supplier.BankInformationGroup.DisplayBankAccountType)
                                Supplier.BankInformationGroup.BankAccountType
                            visible when (Supplier.BankInformationGroup.DisplayBankCurrency)
                                Supplier.BankInformationGroup.BankCurrency
                            visible when (Supplier.BankInformationGroup.DisplayCrossBorderIdentifier)
                                Supplier.BankInformationGroup.CrossBorderIdentifier
                            visible when (Supplier.BankInformationGroup.DisplaySwiftID)
                                Supplier.BankInformationGroup.SwiftID
                            visible when (Supplier.BankInformationGroup.DisplayBankingCode)
                                Supplier.BankInformationGroup.BankingCode
                            visible when (Supplier.BankInformationGroup.DisplayBankID)
                                Supplier.BankInformationGroup.BankID
                            visible when (Supplier.BankInformationGroup.DisplayAssignmentNumber)
                                Supplier.BankInformationGroup.AssignmentNumber
                            visible when (Supplier.BankInformationGroup.DisplayGiroNumber)
                                Supplier.BankInformationGroup.GiroNumber
                            visible when (Supplier.BankInformationGroup.DisplayBACSReference)
                                Supplier.BankInformationGroup.BACSReference
                            visible when (Supplier.BankInformationGroup.DisplayBACSName)
                                Supplier.BankInformationGroup.BACSName
                            visible when (Supplier.BankInformationGroup.DisplayCreditCardNumber)
                                visible when (not Supplier.BankInformationGroup.CreditCardNumberEntered)
                                    visible when (not Supplier.BankInformationGroup.MaskBankInformationEntry)
                                        Supplier.BankInformationGroup.CreditCardNumber
                                    visible when (Supplier.BankInformationGroup.MaskBankInformationEntry)
                                        Supplier.BankInformationGroup.CreditCardNumber
                                            hidden entry
                                visible when (Supplier.BankInformationGroup.CreditCardNumberEntered)
                                    Supplier.BankInformationGroup.DerivedDisplayCreditCardNumber
                                        label is "CreditCardNumber"
                    visible when (IsPrimary)
                        text of "Use_Update_Supplier_Bank_InformationToUpdateBankInformation"
                            italics
                            color of blue
            visible when (PortalContact)
                header2 of "RegistrationStatus"
                paragraph
                    text of "RegistrationStatus:<RegistrationStatus>"
                        center
                        bold
                paragraph
                    text of "<SummaryText>"
                        italics
                visible when (IncompleteQuestions)
                    paragraph
                        text of "<NotCompleteQuestions>"
                visible when (IncompleteQuestionAttachments)
                    paragraph
                        text of "<NotCompleteQuestionAttachments>"
                visible when (SomeRequiredQuestionsOrAttachmentsNotAnswered)
                    paragraph
                        button of "AnswerUnansweredRequiredQuestions"
                            link is SupplierUnansweredQuestions
                visible when (IncompleteCommodities)
                    paragraph
                        text of "<NotCompleteCommodities>"
                visible when (IncompleteCommodities)
                    paragraph
                        text of "<CompleteWarning>"
                            bold
