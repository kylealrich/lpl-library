CreditStatus is a Form
    title is "CancellationOrderSummary"
    Layout
        two column distributed
            display as text
                column1
                    Customer.Name
                        label is "Name"
                    CompanyCustomer.PhoneNumber
                        label is "Phone"
                column2
                    CompanyCustomer.Contact
                        label is "Contact"
                    CompanyCustomer.TelexNbr
                        label is "TelexNumber"
        two column distributed
            display as text
                column1
                    HoldCode
                        label is "CustomerHoldcode"
                    CompanyCustomer.CreditLimit
                        label is "CreditLimit"
                    CompanyCustomer.OrderLimit
                        label is "OrderLimit"
                column2
                    paragraph
                        CompanyCustomer.CurrentBalance
                            label is "CurrentBalance"
                        CompanyCustomer.OpenOrderAmount
                            label is "OrderAmount"
                    OrderEntryCustomer.SingleOrderLimit
                        label is "SingleOrderLimit"
                    OrderEntryCustomer.TotalCODOrderLimit
                        label is "CashOnDeliveryLimit"
        five column table
            column headers
                "Period1"
                "Period2"
                "Period3"
                "Period4"
                "Period5"
            column1
                CompanyCustomer.Agings.Aging[1]
                    display as text
            column2
                CompanyCustomer.Agings.Aging[2]
                    display as text
            column3
                CompanyCustomer.Agings.Aging[3]
                    display as text
            column4
                CompanyCustomer.Agings.Aging[4]
                    display as text
            column5
                CompanyCustomer.Agings.Aging[5]
                    display as text
        five column table
            column headers
                "Limit1"
                "Limit2"
                "Limit3"
                "Limit4"
                "Limit5"
            column1
                Limit1
                    display as text
            column2
                Limit2
                    display as text
            column3
                Limit3
                    display as text
            column4
                Limit4
                    display as text
            column5
                Limit5
                    display as text
