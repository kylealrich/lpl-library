Main is a Form
    Layout
        two column distributed
            visible when (not RecordExists)
                column1
                    CashManagementGroup
                    paragraph
                        CashLedgerReceiptImport.CashCode
                        CashLedgerReceiptImport.CashCode.Description
                            no label
                            display as text
                        CashLedgerReceiptImport.CashCode.Currency
                            no label
                            display as text
                    CashLedgerReceiptImport.TransactionNumber
                column2
                    CashLedgerReceiptImport.RunGroup
                    paragraph
                        CashLedgerReceiptImport.BankTransactionCode
                            label is "TransactionCode"
                            select is BankCreditTransactionCodes
                        CashLedgerReceiptImport.BankTransactionCode.Description
                            no label
                            display as text
                    CashLedgerReceiptImport.SequenceNumber
        two column distributed
            column1
                TransactionType
                PostDate
                paragraph
                    Company
                        select is Company
                    Company.Name
                        no label
                        display as text
                    Company.Currency
                        display as text
            column2
                TransactionDate
                NumberOfFinancialInstruments
                Status
            paragraph
                InterfaceAsPosted
                    label is "InterfaceAsPosted"
                        left of field
                    when value changed
                        refresh BypassCashFundUpdate
                visible when (InterfaceAsPosted)
                    BypassCashFundUpdate
        blank line
        two column distributed
            column1
                CashProcessLevel
                    label is "ProcessLevel"
                GeneralLedgerSourceCode
                    label is "EventCode"
                ApprovalCostCenter
                CashLedgerProcessGroup
            column2
                ACHPrenotification
                    label is "PrenotificationStatus"
                CustomerName
                    label is "CustomerReference"
                ApprovalCategory
        blank line
        header3 of "CustomerAddress"
        CustomerPostalAddress
            form is PostalAddressMinimumWithType
