MobileCurrencyAmountsForm is a Form
    title is "CurrencyAmounts"
    Layout
        visible when (false)
            GLBaseAmount.AssignToCurrency
        blank line
        two column distributed
            GLBaseAmount.FunctionalAmount.EnteredCurrencyAmount
                label is "Functional"
                display as text
            GLBaseAmount.AccountingEntity.FunctionalCurrency
                no label
                display as text
        single column
            GLBaseAmount.FunctionalAmount.EnteredCurrencyRate
                no label
                display as text
        visible when (GLBaseAmount.AccountingEntity.AlternateCurrencyExists)
            two column distributed
                GLBaseAmount.AlternateAmount.EnteredCurrencyAmount
                    label is "Alternate"
                    display as text
                GLBaseAmount.AccountingEntity.AlternateCurrency
                    no label
                    display as text
            single column
                GLBaseAmount.AlternateAmount.EnteredCurrencyRate
                    no label
                    display as text
        visible when (GLBaseAmount.AccountingEntity.AlternateCurrency2Exists)
            two column distributed
                GLBaseAmount.AlternateAmount2.EnteredCurrencyAmount
                    label is "Alternate2"
                    display as text
                GLBaseAmount.AccountingEntity.AlternateCurrency2
                    no label
                    display as text
            single column
                GLBaseAmount.AlternateAmount2.EnteredCurrencyRate
                    no label
                    display as text
        visible when (GLBaseAmount.AccountingEntity.AlternateCurrency3Exists)
            two column distributed
                GLBaseAmount.AlternateAmount3.EnteredCurrencyAmount
                    label is "Alternate3"
                    display as text
                GLBaseAmount.AccountingEntity.AlternateCurrency3
                    no label
                    display as text
            single column
                GLBaseAmount.AlternateAmount3.EnteredCurrencyRate
                    no label
                    display as text
        visible when (GLBaseAmount.InterAccountingEntityTransaction)
            two column distributed
                GLBaseAmount.ToFunctionalAmount.EnteredCurrencyAmount
                    label is "PostToFunctional"
                    display as text
                GLBaseAmount.TransactionCodeBlock.ToAccountingEntity.FunctionalCurrency
                    no label
                    display as text
            single column
                GLBaseAmount.ToFunctionalAmount.EnteredCurrencyRate
                    no label
                    display as text
        visible when (GLBaseAmount.InterAccountingEntityAlternate)
            two column distributed
                GLBaseAmount.ToAlternateAmount.EnteredCurrencyAmount
                    label is "PostToAlternate"
                    display as text
                GLBaseAmount.TransactionCodeBlock.ToAccountingEntity.AlternateCurrency
                    no label
                    display as text
            single column
                GLBaseAmount.ToAlternateAmount.EnteredCurrencyRate
                    no label
                    display as text
        visible when (GLBaseAmount.InterAccountingEntityAlternate2)
            two column distributed
                GLBaseAmount.ToAlternateAmount2.EnteredCurrencyAmount
                    label is "PostToAlternate2"
                    display as text
                GLBaseAmount.TransactionCodeBlock.ToAccountingEntity.AlternateCurrency2
                    no label
                    display as text
            single column
                GLBaseAmount.ToAlternateAmount2.EnteredCurrencyRate
                    no label
                    display as text
        visible when (GLBaseAmount.InterAccountingEntityAlternate3)
            two column distributed
                GLBaseAmount.ToAlternateAmount3.EnteredCurrencyAmount
                    label is "PostToAlternate3"
                    display as text
                GLBaseAmount.TransactionCodeBlock.ToAccountingEntity.AlternateCurrency3
                    no label
                    display as text
            single column
                GLBaseAmount.ToAlternateAmount3.EnteredCurrencyRate
                    no label
                    display as text
        visible when (GLBaseAmount.ProjectTransaction)
            two column distributed
                GLBaseAmount.ProjectAmount.EnteredCurrencyAmount
                    label is "<GLBaseAmount.FinanceEnterpriseGroup.ProjectLabel>"
                    display as text
                GLBaseAmount.TransactionCodeBlock.Project.Currency
                    no label
                    display as text
            single column
                GLBaseAmount.ProjectAmount.EnteredCurrencyRate
                    no label
                    display as text
        visible when (GLBaseAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
            two column distributed
                GLBaseAmount.ReportAmount1.EnteredCurrencyAmount
                    label is "<GLBaseAmount.FinanceEnterpriseGroup.ReportOne>"
                    display as text
                GLBaseAmount.FinanceEnterpriseGroup.ReportCurrencyOne
                    no label
                    display as text
            single column
                GLBaseAmount.ReportAmount1.EnteredCurrencyRate
                    no label
                    display as text
        visible when (GLBaseAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
            two column distributed
                GLBaseAmount.ReportAmount2.EnteredCurrencyAmount
                    label is "<GLBaseAmount.FinanceEnterpriseGroup.ReportTwo>"
                    display as text
                GLBaseAmount.FinanceEnterpriseGroup.ReportCurrencyTwo
                    no label
                    display as text
            single column
                GLBaseAmount.ReportAmount2.EnteredCurrencyRate
                    no label
                    display as text
        visible when (GLBaseAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
            two column distributed
                GLBaseAmount.ReportAmount3.EnteredCurrencyAmount
                    label is "<GLBaseAmount.FinanceEnterpriseGroup.ReportThree>"
                    display as text
                GLBaseAmount.FinanceEnterpriseGroup.ReportCurrencyThree
                    no label
                    display as text
            single column
                GLBaseAmount.ReportAmount3.EnteredCurrencyRate
                    no label
                    display as text
        visible when (GLBaseAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
            two column distributed
                GLBaseAmount.ReportAmount4.EnteredCurrencyAmount
                    label is "<GLBaseAmount.FinanceEnterpriseGroup.ReportFour>"
                    display as text
                GLBaseAmount.FinanceEnterpriseGroup.ReportCurrencyFour
                    no label
                    display as text
            single column
                GLBaseAmount.ReportAmount4.EnteredCurrencyRate
                    no label
                    display as text
        visible when (GLBaseAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
            two column distributed
