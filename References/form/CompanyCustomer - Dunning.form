Dunning is a Form
    Layout
        header2 of "BasicDunningSettings"
        paragraph
            AutomaticDunning
            DunningCycle
        header2 of "AdvancedDunningSettings"
        paragraph
            DunningProcessCode
                select is DunningProcessingCodes
            DunningProcessCode.Description
                no label
                display as text
        three column distributed
            column1
                DunningLetterIndicator
            column2
                MultipleDunningLetters
            column3
                AssessDunningFee
        text of "LastDunningLetter"
            header4
        three column distributed
            column1
                LastLetterDate
                    label is "LastDunningLetterDate"
                    display as text
            column2
                LastDunningLetter
                    label is "LastDunningLetterLevel"
                    display as text
            column3
                LastDunningLetterTextCode
                    label is "LastDunningLetterTextCode"
                    display as text
        text of "CreditAgency"
            header4
        paragraph
            CreditAgencyName
            CreditAgencyPostalAddress
                form is PostalAddressShortRequired
