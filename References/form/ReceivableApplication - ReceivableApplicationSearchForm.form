ReceivableApplicationSearchForm is a SearchForm
    title is "SearchReceivableApplications"
    Layout
        four column distributed
            column1
                Company
            column2
                CreateAdjustProject
                    label is "Project"
            column3
                ReceivableOperator
                    label is "Operator"
            column4
                ContextCrossPaid
                    label is "CrossCustomer"
        four column distributed
            column1
                CustomerGroupRel.CustomerGroup
                    label is "CustomerGroup"
            column2
                ApplicationCurrency
                    label is "Currency"
            column3
                TransientFunctionalType
                    label is "TransactionType"
            column4
                ReceivableApplication.Invoice
        four column distributed
            column1
                ContextReceivableProcessLevel
                    label is "ProcessLevel"
            column2
                CreditTransaction.CreditCustomer
                    label is "Customer"
            column3
                DueDateRange.Begin
                    label is "FromDueDate"
            column4
                DueDateRange.End
                    label is "ToDueDate"
        paragraph
            ApplicationPostDateRange.Begin
                label is "FromApplicationPostDate"
            ApplicationPostDateRange.End
                label is "ToApplicationPostDate"
