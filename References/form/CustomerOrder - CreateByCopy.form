CreateByCopy is an ActionForm
    action is Unreleased.CreateByCopy
        on completion navigate to ViewCustomerOrder
    Layout
        header2 of "CopyFrom"
        paragraph
            PrmBillingCompany
            PrmBillingCompany.Name
                no label
                display as text
        paragraph
            visible when (PrmBillingCompany.ManuallyNumbered)
                PrmOrderNumber
                    label is "CustomerOrder"
        paragraph
            PrmCustomerOrder
                label is "PreviousCustomerOrder"
        paragraph
            PrmCopyAddOnCharge
                label is "CopyAddOnCharge"
        paragraph
            PrmCopyUnitCost
                label is "CopyUnitCost"
        paragraph
            PrmStandardOrder
                label is "StandardOrder"
        header2 of "CopyTo"
        two column
            column1
                paragraph
                    PrmCustomer
                        label is "Customer"
                        select is CustomerContextCompanySelectList
                    PrmCustomer.Name
                        no label
                        display as text
                paragraph
                    PrmBillingProcessLevel
                        label is "ProcessLevel"
                    PrmBillingProcessLevel.Name
                        no label
                        display as text
                paragraph
                    PrmOrderEntryOrderType
                        label is "OrderType"
                    PrmOrderEntryOrderType.Description
                        no label
                        display as text
                paragraph
                    PrmInventoryLocation
                        label is "Location"
                    PrmInventoryLocation.Name
                        no label
                        display as text
            column2
                paragraph
                    PrmCustomerBillTo
                        label is "BillTo"
                    PrmCustomerBillTo.Name
                        no label
                        display as text
                paragraph
                    PrmCustomerShipTo
                        label is "ShipTo"
                    PrmCustomerShipTo.Name
                        no label
                        display as text
                paragraph
                    PrmRequestedDate
                        label is "RequestedDate"
                paragraph
                    PrmExpectedShipDate
                        label is "ExpectedShipDate"
