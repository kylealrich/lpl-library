ERSInvoiceCreation is an ActionForm
    action is ERSInvoiceCreation
    Layout
        single column
            paragraph
                Company
                    select is Companies
                Company.Name
                    no label
                    display as text
            paragraph
                VenVendor
                    label is "Vendor"
                    select is ActiveVendorsByName
                VenVendor.VendorName
                    no label
                    display as text
            paragraph
                AprProcLevel
                    label is "DefaultProcessLevel"
                    select is ProcessLevels
                AprProcLevel.Name
                    no label
                    display as text
            paragraph
                AtrAuthCode
                    label is "DefaultAuthorityCode"
                    select is Authorities
                AtrAuthCode.Description
                    no label
                    display as text
            paragraph
                MatchRule
                InvProcess
                    label is "InvoiceProcess"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                ERSInvoiceCreationReport_RDG
                ERSInvoiceCreationUnreleasedExceptions_RDG
                ERSInvoiceCreationFatalErrors_RDG
                ERSInvoiceCreationReleasedExceptions_RDG
                ERSInvoiceCreationUnreleasedAudit_RDG
                ERSInvoiceCreationReleasedAudit_RDG
            column2 of 25%
                ERSInvoiceCreationReport_XPT
                ERSInvoiceCreationUnreleasedExceptions_XPT
                ERSInvoiceCreationFatalErrors_XPT
                ERSInvoiceCreationReleasedExceptions_XPT
                ERSInvoiceCreationUnreleasedAudit_XPT
                ERSInvoiceCreationReleasedAudit_XPT
