MobileStockOnHandCurrentStatus is a Form
    title is "CurrentStatus"
    Layout
        display as text
            three column table
                column1
                    ItemLocation.StockOnHandQuantity
                        label is "StockOnHand"
                    ItemLocation.UnallocatedQuantity
                        label is "Unallocated"
                    ItemLocation.AllocatedQuantity
                        label is "Allocated"
                column2
                    SecondaryQuantity
                        label is "SecondaryOnHand"
                    ItemLocation.InspectionHoldQuantity
                        label is "InspectionHold"
                    ItemLocation.BackorderedQuantity
                        label is "Backordered"
                column3
                    ItemLocation.WorkInProgressQuantity
                        label is "WorkInProcess"
                    ItemLocation.InProcessQuantity
                        label is "InProcess"
                    ItemLocation.IntransitQuantity
                        label is "Intransit"
