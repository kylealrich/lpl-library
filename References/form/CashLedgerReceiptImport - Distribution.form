Distribution is a Form
    Layout
        visible when (IsRebatePaymentType)
            blank line
            header2 of "Rebate"
            single column
                ContractRebateGroup.ContractGroup
                ContractRebateGroup.Contract
                    label is "CMContract"
                ContractRebateGroup.ContractRebateType
                    label is "RebateType"
                ContractRebateGroup.ContractRebateDue
                    label is "DueDate"
        blank line
        two column
            column1
                header2 of "Ledger"
                paragraph
                    ReceiptAmount
                        label is "Amount"
                paragraph
                    CatTaxEntity
                        label is "EntityForCategoryOrTaxCode"
                paragraph
                    CashCategory
                        label is "Category"
                paragraph
                    ReceiptAccount
                        label is "FinanceStructure"
                        compact format
                paragraph
                    CashAllocationCode
                        label is "CashAllocationCode"
                paragraph
                    UnitsAmount
                blank line
                paragraph
                    Reference
                paragraph
                    Description
            column2
                header2 of "Tax"
                paragraph
                    TaxCode
                paragraph
                    TaxableAmount
                        label is "TaxableAmount"
                paragraph
                    TaxAmount
                        label is "TaxAmount"
