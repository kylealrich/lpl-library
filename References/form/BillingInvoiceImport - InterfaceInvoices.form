InterfaceInvoices is an ActionForm
    action is InterfaceInvoices
    Layout
        single column
            PrmRunGroup
                label is "RunGroup"
            paragraph
                PrmCompany
                    label is "Company"
                PrmCompany.Name
                    no label
                    display as text
            paragraph
                PrmCompanyGroup
                    label is "OrCompanyGroup"
                PrmCompanyGroup.Description
                    no label
                    display as text
            paragraph
                PrmProcessLevel
                    label is "ProcessLevel"
                PrmProcessLevel.Name
                    no label
                    display as text
            paragraph
                PrmCurrencyCd
                    label is "Currency"
                PrmCurrencyCd.Description
                    no label
                    display as text
            PrmDescription
                label is "Description"
            visible when (not PrmCompany.ManualInvoiceNumbering)
                PrmReleaseOption
                    label is "ReleaseOption"
