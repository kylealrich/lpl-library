OAuthRecord is a Form
    is primary
    title is "O\\AuthRecord"
    Layout
        single column
            OAuth
            Type
            ApplicationKey
            visible when (EnableOauthEncryption)
                ApplicationSecretEncrypted
            visible when (not EnableOauthEncryption)
                TApplicationSecret
            ResourceUser
            visible when (EnableOauthEncryption)
                ResourcePasswordEncrypted
            visible when (not EnableOauthEncryption)
                TResourcePassword
            TokenName
            AuthorizeURL
            AccessTokenURL
            AuthenticationURL
            RequestTokenURL
            AuthenticationToken
            visible when (EnableOauthEncryption)
                AuthenticationTokenSecretEncrypted
            visible when (not EnableOauthEncryption)
                TAuthenticationTokenSecret
            RedirectURL
            TokenType
            TokenName
            GrantType
            Scope
            ClientAuthentication
            AuthorizeUsingBrowser
            button of "Authorize"
                link is "<AuthenticationURL>"
