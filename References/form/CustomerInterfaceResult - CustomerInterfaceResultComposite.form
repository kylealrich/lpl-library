CustomerInterfaceResultComposite is a CompositeForm
    is primary
    propagate as a drill
    title is "CustomerInterfaceResult"
    context form is CustomerInterfaceResultContext
        background is theme color
    restrict action DeleteAllInterfaceResults
    restrict action DeleteAllConversionResults
    ProcessingCounts is a Panel
        form is ProcessingCountsForm
    CustomersCreated is a Panel
        visible when (HasCustomersCreated)
        list is CompanyCustomerRel.InterfacedCustomers
    PendingConversionCustomers is a Panel
        title is "PendingCustomers"
        visible when (ShowPendingRunGroupCustomerConversion)
        list is RunGroupCustomerConversionRel.primary
    PendingErroredConversionCustomers is a Panel
        title is "PendingErroredCustomers"
        visible when (ShowPendingErrorRunGroupCustomerConversion)
        list is ErrorRunGroupCustomerConversionRel.primary
    PendingInterfaceCustomers2 is a Panel
        title is "PendingCustomers"
        visible when (ShowPendingRunGroupCustomerImport)
        list is RunGroupCustomerImportRel.primary
    PendingErroredInterfaceCustomers2 is a Panel
        title is "PendingErroredCustomers"
        visible when (ShowPendingErrorRunGroupCustomerImport)
        list is ErrorRunGroupCustomerImportRel.primary
