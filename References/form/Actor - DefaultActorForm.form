DefaultActorForm is a CompositeForm
    is primary
    title is "Actor"
    context form is ActorContext
    restrict action ValidateEmail
    Detail is a Panel
        form is Detail
    Account is a Panel
        form is AccountInformation
    Contact is a Panel
        form is DefaultContactInfo
    Identities is a Panel
        list is IdentityActor set.SecondaryIdentityActorList
    Roles is a Panel
        list is ActorRole set.SecondaryActorRoleList
            helper list is RoleRel.ActorRoleHelperList
                action is AssignRoleToActor
    ConfigurableFeatures is a Panel
        form is ConfigurableFeatures
    CustomFields is a Panel
        form is CustomActorDetails
    ActorGroupMembership is a Panel
        list is ActorGroupMember set.ActorGroupList
    ActorType is a Panel
        title is "ActorTypes"
        form is ActorTypeForm
