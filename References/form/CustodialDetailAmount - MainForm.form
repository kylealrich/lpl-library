MainForm is a Form
    Layout
        visible when (EditLevel.Group)
            paragraph
                Reserve
            paragraph
                GroupAvailableEndingBalance
                    label is "AvailableEndingBalance"
        visible when (EditLevel.Individual)
            display as text
            header2 of "Balances"
            five column distributed
                column1
                    BeginBalance
                        display as text
                    Deposits
                        display as text
                    CommittedAmount
                        display as text
                    visible when (ParentCustodialDetailAmount not entered)
                        paragraph
                            Reserve
                    AvailableEndingBalance
                        display as text
                column2
                    Disbursements
                        display as text
                    visible when (ParentCustodialDetailAmount entered)
                        Reserve
                column3
                    visible when (CustodialPosting.ReceiveInterest)
                    Interest
                        display as text
                column4
                    Transfers
                        display as text
                column5
                    AccountBalance
                        display as text
