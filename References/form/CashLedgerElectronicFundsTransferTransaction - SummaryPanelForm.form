SummaryPanelForm is a Form
    Layout
        display as text
            header2 of "Main"
            two column table
                column1
                    TransactionDate
                    BankAccountValidationType
                        label is "AccountValidationType"
                    BankAccountNumber
                        label is "AccountNumber"
                        show up to 10 characters
                    GeneralLedgerSourceCode
                        label is "EventCode"
                column2
                    TransactionType
                        show up to 10 characters
                    BankEntity
                        label is "BankIdentification"
                        select is Banks
                        show up to 10 characters
                    paragraph
                        BankAccountType
                            label is "AccountType"
                            show up to 10 characters
                        GlobalBankAccountType
                    ACHPrenotification
                        label is "PrenotificationStatus"
            single column
                Description
            header2 of "CustomerAddress"
            single column
                CustomerName
                    label is "CustomerReference"
            header2 of "BankInformation"
            two column table
                column1
                    BankEntity
                        label is "BankIdentification"
                        select is Banks
                    BankName
                column2
                    BankAccountValidationType
                        label is "AccountValidation"
                    CrossBorderIdentifier
            header2 of "TransactionData"
            single column
                visible when (TransactionData1 entered)
                    TransactionData1
                visible when (TransactionData2 entered)
                    TransactionData2
                visible when (TransactionData3 entered)
                    TransactionData3
                visible when (TransactionData4 entered)
                    TransactionData4
