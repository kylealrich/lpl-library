MobileInventoryTransactionForm is a CompositeForm
    title is "<InventoryTransactionType>"
    context form is MobileInventoryTransactionContext
    Actions
        action is save
            label is "_"
        action is refresh
            label is "_"
    Main is a Panel
        form is MobileMain
    Lines is a Panel
        visible when (not IsIntransitReceiving)
        list is InventoryTransactionLinesRel.MobileInventoryTransactionLines
