                "Addendum"
                "Amendment"
            row headers
                "RequireInternalContractApprovalUsingDocumentRedliningBeforeNegotiations"
                "RequireContractApprovalBeforeNegotiations"
                "RequireSupplierContractNegotiations"
                "RequireInternalContractApprovalAfterNegotiations"
                "RequireResponsibilityMatrixApprovalForServiceAndStandingContracts"
