MContractDashboardForm is a CompositeForm
    title is "ContractDetails"
    context form is MContractDashboardContext
        background is theme color
    implements DashBoardView
    restrict action Draft.Create
    restrict action Delete
    restrict action Draft.SupplierCreateProposedContract
    restrict action Draft.RequesterCreateProposedContract
    restrict action Draft.CreateContractFromContract
    restrict action Draft.CreateFromInterview
    restrict action Draft.CreateFromWizard
    restrict action Draft.CreateContractTemplate
    restrict action Draft.CreateContractProposalTemplate
    restrict action Draft.CreateContractFromTemplate
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Actions
        action is refresh
            label is "_"
        action is save
            label is "_"
            hide when invalid
    Lines is a Panel
        title is "Lines<LineIndicator>"
        visible when (NonServicePurchaseType)
        list is ContractLine set.MNonServiceContractLines
    ServiceLines is a Panel
        title is "Lines<LineIndicator>"
        visible when (ServicePurchaseType)
        list is ContractLine set.MServiceContractLines
    NoContractLines is a Panel
        title is "Lines<LineIndicator>"
        visible when (not HasAPurchaseType)
        list is ContractLine set.ContractLines
    RelatedPOLines is a Panel
        visible when (ContractPolinesByContract)
        list is ContractPolinesByContractRel.POLinesFromAContract
    CommittedPOLineDistributions is a Panel
        visible when (POCommitmentsExist)
        list is ContractPolineDistributionsByContractRel.DistributionsForAContract
    RelatedRequisitionLines is a Panel
        visible when (RequisitionLinesByContract)
        list is RequisitionLinesByContractRel.RequisitionLinesFromAContract
    CommittedRequisitionLineDistributions is a Panel
        visible when (RQCommitmentsExist)
        list is RequisitionLineDistributionsByContractRel.DistributionsForAContract
    BlanketPurchaseOrders is a Panel
        visible when (BlanketPurchaseOrdersExist)
        list is PurchaseOrderForBlanketStandingRel.BlanketStandingPurchaseOrderList
    StandingPurchaseOrders is a Panel
        visible when (StandingPurchaseOrdersExist)
        list is PurchaseOrderForBlanketStandingRel.BlanketStandingPurchaseOrderList
    Commitments is a Panel
        visible when (DrillCommitmentsExist)
        list is GLCommitDrillRel.DrillGLCommitList
    ProcurementTemplatesFromContract is a Panel
        visible when (ProcurementTemplatesExist)
        list is ProcurementTemplateRel.ProcurementTemplateList
    SupplierCertifications is a Panel
        valid when (SupplierCertificationExist)
        list is ContractSupplierCertificationRel.AllSupplierCertifications
    ProjectContracts is a Panel
        valid when (RelatedProjectContractsExist)
        list is ProjectContractsRel.ProjectContractList
    ParticipantGroupsUsedForContract is a Panel
        valid when (ParticipantGroupsExist)
        list is ParticipantGroupContractRel.ContractParticipantGroupList
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
    ApprovalHistory2 is a Panel
        title is "FinalApprovalHistory"
        visible when (HasWorkunit2)
        form is ApprovalHistory2
    ApprovalHistory3 is a Panel
        title is "FinalApprovalHistory"
        visible when (HasWorkunit3)
        form is ApprovalHistory3
    ApprovalHistory4 is a Panel
        title is "FinalApprovalHistory"
        visible when (HasWorkunit4)
        form is ApprovalHistory4
    ApprovalHistory5 is a Panel
        title is "RedliningApprovalHistory"
        visible when (HasWorkunit5)
        form is ApprovalHistory5
    Email is a Panel
        title is "EmailNotifications"
        visible when (EmailExists)
        list is ContractNotificationEmailRel.ContractNotificationEmails
    ProcessingTimes is a Panel
        list is audit log records.ProcessingTimes
    ContractDocumentHistory is a Panel
        visible when (ContractDocumentVersionsExist)
        list is ContractDocumentRel.ContractDocumentHistoryList
