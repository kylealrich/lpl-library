CreateEncumbranceIndividual is an ActionForm
    action is CreateEncumbranceIndividual
    Layout
        two column
            column1
                Employee
                    display as text
                ProjectLaborSchedule
                    display as text
            column2
                Employee.Employee.Name.PreferredFirstAndLastName
                    no label
                    display as text
                Employee.ProjectLaborSchedule.Description
                    no label
                    display as text
        two column
            FromPeriod
                select is OpenPeriodList
            paragraph
                FromPeriod.DateRange.Begin
                    no label
                    display as text
                FromPeriod.DateRange.End
                    label is "-"
                    display as text
        two column
            ThroughPeriod
                select is OpenPeriodList
            paragraph
                ThroughPeriod.DateRange.Begin
                    no label
                    display as text
                ThroughPeriod.DateRange.End
                    label is "-"
                    display as text
