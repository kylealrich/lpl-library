MatchRuleGroup is a Form
    is primary
    title is "RuleGroup"
    Layout
        paragraph
            FinanceEnterpriseGroup
                display as text
        paragraph
            MatchRule
            Description
        blank line
        blank line
        header2 of "LevelSequence"
        six column table
            column headers
                "MatchLevel"
                "Type"
                "Option"
                "Tolerances"
            row headers
                "1"
                "2"
                "3"
                "4"
                "5"
                "6"
            column1
                MatchRuleToleranceArray.MatchRuleToleranceGroup[1].InvoiceMatchLevel
                    no label
                MatchRuleToleranceArray.MatchRuleToleranceGroup[2].InvoiceMatchLevel
                    no label
                MatchRuleToleranceArray.MatchRuleToleranceGroup[3].InvoiceMatchLevel
                    no label
                MatchRuleToleranceArray.MatchRuleToleranceGroup[4].InvoiceMatchLevel
                    no label
                MatchRuleToleranceArray.MatchRuleToleranceGroup[5].InvoiceMatchLevel
                    no label
                MatchRuleToleranceArray.MatchRuleToleranceGroup[6].InvoiceMatchLevel
                    no label
            column2
                MatchRuleToleranceArray.MatchRuleToleranceGroup[1].MatchRuleType
                    no label
                MatchRuleToleranceArray.MatchRuleToleranceGroup[2].MatchRuleType
                    no label
                MatchRuleToleranceArray.MatchRuleToleranceGroup[3].MatchRuleType
                    no label
                MatchRuleToleranceArray.MatchRuleToleranceGroup[4].MatchRuleType
                    no label
                MatchRuleToleranceArray.MatchRuleToleranceGroup[5].MatchRuleType
                    no label
                MatchRuleToleranceArray.MatchRuleToleranceGroup[6].MatchRuleType
                    no label
            column3
                MatchRuleToleranceArray.MatchRuleToleranceGroup[1].MatchToleranceOption
                    no label
                MatchRuleToleranceArray.MatchRuleToleranceGroup[2].MatchToleranceOption
                    no label
                MatchRuleToleranceArray.MatchRuleToleranceGroup[3].MatchToleranceOption
                    no label
                MatchRuleToleranceArray.MatchRuleToleranceGroup[4].MatchToleranceOption
                    no label
                MatchRuleToleranceArray.MatchRuleToleranceGroup[5].MatchToleranceOption
                    no label
                MatchRuleToleranceArray.MatchRuleToleranceGroup[6].MatchToleranceOption
                    no label
            column4
                visible when (MatchRuleExists)
                    button of "Tolerances"
                        action is UpdateTolerance01
                    button of "Tolerances"
                        action is UpdateTolerance02
                    button of "Tolerances"
                        action is UpdateTolerance03
                    button of "Tolerances"
                        action is UpdateTolerance04
                    button of "Tolerances"
                        action is UpdateTolerance05
                    button of "Tolerances"
                        action is UpdateTolerance06
            column5
                blank line
                visible when (MatchRuleExists)
                    visible when (Copy1To2Valid)
                        button of "CopyFromPrevious"
                            action is CopyTolerancesFrom1To2
                    visible when (Copy2To3Valid)
                        button of "CopyFromPrevious"
                            action is CopyTolerancesFrom2To3
                    visible when (Copy3To4Valid)
                        button of "CopyFromPrevious"
                            action is CopyTolerancesFrom3To4
                    visible when (Copy4To5Valid)
                        button of "CopyFromPrevious"
                            action is CopyTolerancesFrom4To5
                    visible when (Copy5To6Valid)
                        button of "CopyFromPrevious"
                            action is CopyTolerancesFrom5To6
            column6
                blank line
                visible when (MatchRuleExists)
                    visible when (Copy1To2Valid)
                        button of "ClearTolerances"
                            action is ClearLevel2
                    visible when (Copy2To3Valid)
                        button of "ClearTolerances"
                            action is ClearLevel3
                    visible when (Copy3To4Valid)
                        button of "ClearTolerances"
                            action is ClearLevel4
                    visible when (Copy4To5Valid)
                        button of "ClearTolerances"
                            action is ClearLevel5
                    visible when (Copy5To6Valid)
                        button of "ClearTolerances"
                            action is ClearLevel6
