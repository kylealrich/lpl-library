FileCreationSetupMappingExportForm is a Form
    title is "Reports"
    Layout
        four column
            column1
                visible when (SetupExportFileEntered)
                    paragraph
                        button of "<SetupExportFile.Title>"
                            view SetupExportFile.File
                            label is "SetupExport"
                            display as link
                    paragraph
                        SetupExportTimeStamp
                            label is "ExportedOn"
                            display as text
            column2
                visible when (MappingsDocumentFileEntered)
                    paragraph
                        button of "<MappingsDocument.Title>"
                            link is MappingsDocumentLink
                            label is "MappingsDocument"
                            display as link
                    paragraph
                        MappingsDocumentTimeStamp
                            label is "AsOf"
                            display as text
            column3
                visible when (ValidationReportFileEntered)
                    paragraph
                        button of "<ValidationReport.Title>"
                            link is ValidationReportLink
                            label is "ValidationReport"
                            display as link
                    paragraph
                        ValidationReportTimeStamp
                            label is "AsOf"
                            display as text
            column4
                visible when (ComparisonReportFileEntered)
                    paragraph
                        button of "<CompareReport.Title>"
                            link is CompareReportLink
                            label is "ComparisonReport"
                            display as link
                    paragraph
                        CompareReportTimeStamp
                            label is "AsOf"
                            display as text
