ApprovalProcessorTransactionForm is a CompositeForm
    title is "<ProcessorTitle>"
    context form is ApprovalProcessorContextForm
        background is theme color
    restrict action Create
    ApprovalInformation is a DashBoardPanel
        visible when (DisplayApprovalInformationPanel)
        show 4 columns
        Progress is a Panel
            triple width
            form is ProcessIndicatorForm
        History is a Panel
            list is ApprovalProcessorAuditRel.ApprovalProcessorHistoryCardHistoryCardList
    ApprovalTracking is a Panel
        title is "ApprovalInformation"
        visible when (not DisplayApprovalInformationPanel)
        list is ApprovalProcessorAuditRel.ApprovalProcessorHistoryCardHistoryCardList
    GLTransactionPanel is a Panel
        title is "Transaction"
        visible when (ApprovalProcessor.ApprovalTransactionForm.GLJournalEntry)
        list is GeneralLedgerTransactionRel.PortalGeneralLedgerJournalTransactionList
    RJTransactionPanel is a Panel
        title is "Transaction"
        visible when (ApprovalProcessor.ApprovalTransactionForm.GLJournalEntry)
        list is RecurringJournalTransRel.RecurringJournalTransactionList
    Invoice is a Panel
        visible when (ApprovalProcessor.ApprovalTransactionForm.PayablesInvoice)
        form is PortalPayablesInvoiceGeneralInformationViewOnly
    PayablesInvoiceRelatedDistributions is a Panel
        title is "RelatedDistributions"
        visible when (ApprovalProcessor.ApprovalTransactionForm.PayablesInvoice)
        list is PayablesInvoiceDistributionRel.PortalViewOnlyPayablesInvoiceDistributions
    PayablesInvoiceMessages is a Panel
        title is "Messages"
        visible when (PayablesInvoiceHasMessages)
        list is PayablesInvoiceRel.MatchInvoiceMessageRel.PayablesInvoiceCostMessageList
    PayablesInvoiceRelatedDocuments is a Panel
        title is "RelatedDocuments"
        visible when (ApprovalProcessor.ApprovalTransactionForm.PayablesInvoice)
        list is PayablesInvoiceRel.PayablesInvoiceDocument set.PortalPayablesInvoiceDocuments
    PayablesInvoiceComments is a Panel
        title is "Comments"
        visible when (ApprovalProcessor.ApprovalTransactionForm.PayablesInvoice)
        list is PayablesInvoiceRel.PayablesInvoiceCommentRel.XiPayablesInvoiceCommentCardList
    PayablesInvoiceErrors is a Panel
        title is "Errors"
        visible when (PayablesInvoiceHasErrors)
        list is PayablesInvoiceRel.PayablesInvoiceError set.PayablesInvoiceErrorList
    Lines is a Panel
        visible when (CanDisplayContractLines)
        list is ContractAggregationRel.Contract.ContractLine set.NonServiceContractLinesPF
    ServiceLines is a Panel
        visible when (CanDisplayContractServiceLines)
        list is ContractAggregationRel.Contract.ContractLine set.ServiceContractLinesPF
    DetailDistributions is a Panel
        visible when (ApprovalProcessor.ApprovalTransactionForm.ContractManagement)
        list is ContractAggregationRel.ContractLineDistributionRel.MatrixApprovalDistributions
    Comments is a Panel
        visible when (CanDisplayContractComments)
        list is ContractAggregationRel.Contract.ContractComment set.PFContractComments
    Attachments is a Panel
        visible when (CanDisplayContractAttachments)
        list is ContractAggregationRel.Contract.ContractAttachment set.PFContractAttachments
    Contacts is a Panel
        visible when (ApprovalProcessor.ApprovalTransactionForm.ContractManagement)
        list is ContractAggregationRel.Contract.ContractAttachedContact set.PFAttachedContacts
    Questions is a Panel
        visible when (CanDisplayContractQuestions)
        list is ContractAggregationRel.Contract.ContractQuestion set.PFContractQuestions
    Participants is a Panel
        visible when (CanDisplayContractParticipants)
        list is ContractAggregationRel.Contract.ContractParticipant set.PFContractParticipant
    RequisitionLines is a Panel
        title is "Lines"
        visible when (ApprovalProcessor.ApprovalTransactionForm.Requisition)
        list is RequisitionLineRel.PfiViewOnlyRequisitionLines
    RequisitionComments is a Panel
        title is "Comments"
        visible when (DisplayRequisitionComments)
        list is RequisitionCommentRel.PfiCommentsCardList
    PurchaseOrderLines is a Panel
        title is "LinesForAggregation"
        visible when (ApprovalProcessor.ApprovalTransactionForm.PurchaseOrder)
        list is POLinesForAggregationRel.ViewOnlyPurchaseOrderLines
