FinanceDimension9StructureCompsiteForm is a CompositeForm
    is primary
    title is "<FinanceEnterpriseGroup.FinanceDimension9Label>Structure_<FinanceDimension9Structure>_<Description>"
    Actions
        action is previous record
            label is "_"
            mouse over text is "Previous"
        action is next record
            label is "_"
            mouse over text is "Next"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is save
            action icon is save
            label is "_"
            mouse over text is "Save"
        action is refresh
            label is "_"
    StructurePanel is a Panel
        title is "Structure"
        form is FinanceDimension9StructureForm
    Dimensions is a Panel
        title is "Hierarchy"
        list is DimensionNode.DimensionChildrenRel.FinanceDimension9List
    DimensionUpload is a Panel
        title is "Upload"
        visible when (not UnusedDimensionStructure)
        list is FinanceDimension9Upload set.FinanceDimension9UploadForStructure
