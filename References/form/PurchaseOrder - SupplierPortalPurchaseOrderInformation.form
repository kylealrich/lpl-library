SupplierPortalPurchaseOrderInformation is a CompositeForm
    title is "PurchaseOrder<PurchaseOrder>Details"
    called out actions only
    context form is SupplierPortalPurchaseOrderInformationContext
        background is theme color
    Actions
        action is previous record
            hide when invalid
        action is next record
            hide when invalid
    Lines is a Panel
        list is PurchaseOrderLinesRel.SupplierPortalHeaderListOfLines
    Comments is a Panel
        visible when (SupplierCommentsExist)
        list is PurchaseOrderCommentDisplayOnPortalRel.SupplierPortalPurchaseOrderCommentCardList
    AddOnCharges is a Panel
        visible when (HeaderAddOnChargesForSameVendorExist)
        list is PurchaseOrderAndLineAddOnChargesByVendorRel.SupplierPortalMiscellaneousAddOnCostsForAPurchaseOrder
    InvoicesAndPayments is a Panel
        visible when (SupplierShowInvoiceLinks)
        list is MatchPurchaseOrderInvoice set.SupplierPortalInvoices
    Receipts is a Panel
        visible when (SupplierShowReceipt)
        list is PurchaseOrderReceiptsRel.SupplierPortalReceipts
    IssuesAndAcknowledgements is a Panel
        visible when (SupplierShowRevisions)
        list is PurchaseOrderRevisionsRel.SupplierRevisionsforaPurchaseOrder
