ReassignNewItemProposalForm is a Form
    title is "ReassignReview"
    Layout
        paragraph
            Owner
                label is "CurrentReviewer"
                display as text
            Owner.Employee.FirstLastName
                no label
                display as text
        blank line
        paragraph
            NewOwner
                label is "NewReviewer"
                select is ItemReviewerList
            NewOwner.Employee.FirstLastName
                no label
                display as text
