ReceivableInvoiceTransferForm is a Form
    is primary
    propagate as a drill
    title is "Transferred_Invoice<ReceivableInvoiceDetail.Invoice>"
    Layout
        display as text
            three column distributed
                column1
                    Company
                    paragraph
                        Customer
                            label is "FromCustomer"
                        ReceivableInvoiceDetail.ReceivableInvoiceDetail.Customer
                            label is "ToCustomer"
                    Project
                column2
                    paragraph
                        ReceivableInvoiceDetail.ReceivableInvoiceDetailType
                            label is "Type"
                        ReceivableInvoiceDetail.Invoice
                            label is "Invoice"
                        ReceivableInvoiceDetail.PaymentSeq
                            label is "Sequence"
                    paragraph
                        TransactionAmount
                        TranAmt
                            label is "BaseAmount"
                    ReceivableOperator
                column3
                    DueDate
                    CancelDate
