RecallOrganization is a CompositeForm
    is primary
    title is "Organization"
    context form is RecallOrganizationContext
        background is theme color
    Users is a Panel
        list is RecallOrganizationUser set.RecallOrganizationUserList
            helper list is RecallUserHelperListRel.RecallOrganizationHelperList
                action is AttachToRecallOrganization
