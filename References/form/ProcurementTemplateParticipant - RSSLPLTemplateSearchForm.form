RSSLPLTemplateSearchForm is a SearchForm
    title is "SearchShoppingLists"
    Layout
        single column
            ProcurementTemplate
                label is "ShoppingList"
            ProcurementTemplate.Description
                disable translation filtering
            visible when (RequesterRel.FavoritesEnabled)
                ProcurementTemplate.IsRequesterFavoritePOTemplate
                    label is "MyFavorites"
