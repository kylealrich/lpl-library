ProcurementCardStatementInterfaceResultComposite is a CompositeForm
    is primary
    title is "ProcurementCardStatementInterfaceResult"
    context form is ProcurementCardStatementResultContext
        background is theme color
    Actions
        action is RerunForErrors
            hide when invalid
        action is Delete
            label is "_"
    StatementsCreated is a Panel
        visible when (not NoRecordsToProcess)
        list is ProcurementCardStatementsRel.InterfaceListing
    PendingStatements is a Panel
        visible when (ShowPendingPanel)
        list is ProcessedProcurementCardStatementImportRel.StatementImportListForResults
    Parameters is a Panel
        form is ParameterForm
