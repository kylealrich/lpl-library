ClinicalContractMaintenance is a CompositeForm
    context form is ContractContextShort
        background is theme color
    Actions
        action is MassCreateContractLocation
            label is "CreateLocationsForClinicalUse"
        action is ResetAllLineClinicalUseFlags
        action is MaintainClinicalLocations
        navigation is ViewContract
            label is "ReturnToMainForm"
        action is refresh
            label is "_"
    Locations is a Panel
        title is "Locations<LocationIndicator>"
        list is ContractLocationRel.LocationsForClinicalUse
    LinesForClinicalUse is a Panel
        title is "SpecialLinesForClinicalUse<LinesForClinicalUseIndicator>"
        list is ContractLine set.SpecialLinesForClinicalUse
    LinesNotForClinicalUse is a Panel
        title is "SpecialLinesNotForClinicalUse<LinesNotForClinicalUseIndicator>"
        list is ContractLine set.SpecialLinesNotForClinicalUse
    ItemMasterLines is a Panel
        title is "ItemMasterLines<ItemMasterLinesIndicator>"
        list is ContractLine set.ItemMasterLinesForClinical
    ParticipantGroups is a Panel
        visible when (ParticipantGroupsExist)
        list is ParticipantGroupContractRel.ContractParticipantGroupList
    LocationsNotForClinicalUse is a Panel
        visible when (HasLocationsNotForClinicalUse)
        list is ContractLocationRel.LocationsNotForClinicalUse
