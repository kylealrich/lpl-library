GeneralLedgerTrasactionByAccountSearchForm is a SearchForm
    title is "SearchTransactionsBy<actor.context.FinanceEnterpriseGroup.AccountLabel>"
    Layout
        paragraph
            FinanceCodeBlock.GeneralLedgerChartAccount
                label is "<actor.context.FinanceEnterpriseGroup.AccountLabel>"
            SearchDateRange.Begin
                label is "FromPostingDate"
            SearchDateRange.End
                label is "ToPostingDate"
            AccountingEntity
                label is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
            visible when (actor.context.FinanceEnterpriseGroup.AccountingUnitSelected)
                FinanceCodeBlock.AccountingUnit
                    label is "<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>"
            visible when (actor.context.FinanceEnterpriseGroup.ProjectEntry)
                FinanceCodeBlock.Project
                    label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
            GeneralLedgerCalendarPeriod
                label is "TotalsPeriod"
                select is GeneralLedgerCalendarPeriodEndDateList
            FinanceCodeBlock.Ledger
                label is "Ledger"
            System
            CurrencyCode
                label is "Currency"
            GeneralLedgerEvent
                label is "Event"
            GeneralLedgerJournalControl
                label is "Journal"
            Status
