ManualCommitAdditionalInfoForm is a Form
    title is "Main"
    Layout
        paragraph
            PostingDate
                label is "PostDate"
            GeneralLedgerClosePeriod.GeneralLedgerCloseYear
                label is "Year"
                display as text
            GeneralLedgerClosePeriod.PeriodName
                label is "Period"
                display as text
        paragraph
            JournalType
                display only
            GeneralLedgerJournalControl
                label is "Journal"
                display only
            Description
            System
                display only
            Status
                no label
                display as text
            TransientUpdateStamp
                label is "LastUpdated"
                display as text
            visible when (PendingBudgetEdit)
                PendingBudgetEditText
                    no label
                    display as text
        single column
            TransactionDate
            CurrencyTable
            Currency
            Reference
            ApprovalCode
