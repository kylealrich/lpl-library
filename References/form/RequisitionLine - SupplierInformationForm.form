SupplierInformationForm is a Form
    title is "RequisitionLine"
    disable field options
    Layout
        display as text
            two column distributed
                column1
                    VendorItem
                        select is VendorItemsForASupplier
                    Description
                    EnteredUOM
                        label is "UOM"
                        select is SupplierUnitsOfMeasure
                column2
                    ManufacturerNumber
                        label is "ManufacturerPart"
                    Quantity
                    TransactionUnitCost
                        label is "UnitCost"
                    visible when (ItemExists)
                        Item
                            label is "ItemMasterItem"
            paragraph
                ItemType
                    display as radio buttons
                visible when (ItemType.Service)
                    ServiceCode
                        display as radio buttons
            two column distributed
                column1
                    visible when (CanDisplaySerialNumber)
                        TransientSerialNumber
                            label is "Serial"
                    visible when (CanDisplayManufacturingDate)
                        paragraph
                            TransientManufacturingDate
                                label is "ManufacturingDate"
                            TransientUniqueDeviceId
                                label is "UDI"
                                display as text
                    visible when (CanDisplayExpirationDate)
                        TransientExpirationDate
                            label is "ExpirationDate"
                column2
                    visible when (CanDisplayLot)
                        TransientLot
                            label is "Lot"
                    visible when (CanDisplayGTIN)
                        TransientGTIN
                            label is "GTIN"
