Create is an ActionForm
    action is Create
    title is "ConsolidateSpendAnalysis"
    Layout
        paragraph
            ProcurementGroup
            ProcurementGroup.Description
                no label
                display as text
        header2 of "ConsolidationDateRangeByInvoiceDate"
        two column
            ConsolidationDateRange
        single column
            text of "IfABeginDateIsEnteredWithoutAnEndDate,TheEndDateWillDefaultToTheCurrentDate"
        header2 of "\\orConsolidateTheLastTwelveMonths"
        single column
            ConsolidateLastTwelveMonths
                no label
        header2 of "ConsolidationOptions"
        three column
            ConsolidationOptions.ConsolidateTotalSpend
                label is "TotalSpend"
            ConsolidationOptions.ConsolidateBuyerSpend
                label is "BuyerSpend"
            ConsolidationOptions.ConsolidateDiversitySpend
                label is "DiversitySpend"
            ConsolidationOptions.ConsolidateCommodityCodeSpend
                label is "CommodityCodeSpend"
            ConsolidationOptions.ConsolidateUNSPSCSpend
                label is "UNSPSCSpend"
        single column
            InvoiceList
                label is "CustomInvoice_andVendorSelection"
