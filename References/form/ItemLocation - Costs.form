Costs is a Form
    Layout
        visible when (false)
            TransientOnlineCost
        two column distributed
            column1
                paragraph
                    visible when (not HasItemLocationStockUOM)
                        StandardCost
                    visible when (HasItemLocationStockUOM)
                        TransientStandardCostInLocationStockUOM
                            label is "StandardCost"
                    visible when (HasSupplementalUOM)
                        Item.StockUOM
                            no label
                            display as text
                        DerivedStandardCostInSupplementalUOM
                            no label
                            display as text
                        SupplementalUOM.UnitOfMeasure
                            no label
                            display as text
            column2
                paragraph
                    visible when (not HasItemLocationStockUOM)
                        FutureStandardCost
                    visible when (HasItemLocationStockUOM)
                        TransientFutureStandardCostInLocationStockUOM
                            label is "FutureStandardCost"
                    visible when (HasSupplementalUOM)
                        Item.StockUOM
                            no label
                            display as text
                        DerivedFutureStandardCostInSupplementalUOM
                            no label
                            display as text
                        SupplementalUOM.UnitOfMeasure
                            no label
                            display as text
        two column distributed
            column1
                paragraph
                    visible when (not HasItemLocationStockUOM)
                        MarketPrice
                    visible when (HasItemLocationStockUOM)
                        TransientMarketPriceInLocationStockUOM
                            label is "MarketPrice"
                    visible when (HasSupplementalUOM)
                        Item.StockUOM
                            no label
                            display as text
                        DerivedMarketPriceInSupplementalUOM
                            no label
                            display as text
                        SupplementalUOM.UnitOfMeasure
                            no label
                            display as text
            column2
                paragraph
                    visible when (not HasItemLocationStockUOM)
                        OverheadUnitCost
                            label is "OverheadCost"
                    visible when (HasItemLocationStockUOM)
                        TransientOverheadUnitCostInLocationStockUOM
                            label is "OverheadCost"
                    visible when (HasSupplementalUOM)
                        Item.StockUOM
                            no label
                            display as text
                        DerivedOverheadUnitCostInSupplementalUOM
                            no label
                            display as text
                        SupplementalUOM.UnitOfMeasure
                            no label
                            display as text
        two column distributed
            column1
                DerivedAverageCostDisplay
                    display as text
            column2
                paragraph
                    visible when (CanChangeLastReceiptCost)
                        visible when (not HasItemLocationStockUOM)
                            LastReceiptCost
                        visible when (HasItemLocationStockUOM)
                            LastReceiptCostInLocationStockUOM
                        visible when (HasSupplementalUOM)
                            Item.StockUOM
                                no label
                                display as text
                            LastReceiptCostInSupplementalUOM
                                no label
                                display as text
                            SupplementalUOM.UnitOfMeasure
                                no label
                                display as text
                    visible when (not CanChangeLastReceiptCost)
                        DerivedLastReceiptCostDisplay
                            display as text
                    LastReceiptDate
                        label is "Date"
                        display as text
        two column distributed
            display as text
                column1
                    DerivedAverageAddOnCostDisplay
                column2
                    paragraph
                        DerivedLastIssueCostDisplay
                        LastIssueDate
                            label is "Date"
