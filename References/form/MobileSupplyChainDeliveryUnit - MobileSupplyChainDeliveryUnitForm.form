MobileSupplyChainDeliveryUnitForm is a CompositeForm
    is primary
    title is "DeliveryUnit"
    called out actions only
    context form is MobileSupplyChainDeliveryUnitFormContext
        background is theme color
    Actions
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is refresh
            label is "_"
    Lines is a Panel
        list is MobileSupplyChainDeliveryLine set.MobileSupplyChainDeliveryLine
    EventLines is a Panel
        title is "Events"
        visible when (not MobileSupplyChainDeliveryTicket.IsShipmentOrPOReceipt)
        list is MobileSupplyChainDeliveryEventLineRel.MobileSupplyChainDeliveryEventLine
    Events is a Panel
        visible when (MobileSupplyChainDeliveryTicket.IsShipmentOrPOReceipt)
        list is DeliveryEventPerTicketRel.MobileSupplyChainDeliveryEvent
    Notes is a Panel
        list is MobileSupplyChainDeliveryNoteRel.MobileSupplyChainDeliveryNote
