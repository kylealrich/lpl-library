TaxableCurrencyAmountsForm is a Form
    title is "TaxableCurrencyAmounts"
    Layout
        visible when (false)
            TaxableData.AssignToCurrency
        blank line
        paragraph
            TaxableData.FunctionalAmount.EnteredCurrencyAmount
                label is "Functional"
                display only
            TaxableData.AccountingEntity.FunctionalCurrency
                no label
                display only
            TaxableData.FunctionalAmount.EnteredCurrencyRate
                no label
                display only
        visible when (TaxableData.AccountingEntity.AlternateCurrencyExists)
            paragraph
                TaxableData.AlternateAmount.EnteredCurrencyAmount
                    label is "Alternate"
                    display only
                TaxableData.AccountingEntity.AlternateCurrency
                    no label
                    display only
                TaxableData.AlternateAmount.EnteredCurrencyRate
                    no label
                    display only
        visible when (TaxableData.AccountingEntity.AlternateCurrency2Exists)
            paragraph
                TaxableData.AlternateAmount2.EnteredCurrencyAmount
                    label is "Alternate2"
                    display only
                TaxableData.AccountingEntity.AlternateCurrency2
                    no label
                    display only
                TaxableData.AlternateAmount2.EnteredCurrencyRate
                    no label
                    display only
        visible when (TaxableData.AccountingEntity.AlternateCurrency3Exists)
            paragraph
                TaxableData.AlternateAmount3.EnteredCurrencyAmount
                    label is "Alternate3"
                    display only
                TaxableData.AccountingEntity.AlternateCurrency3
                    no label
                    display only
                TaxableData.AlternateAmount3.EnteredCurrencyRate
                    no label
                    display only
        visible when (TaxableData.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
            paragraph
                TaxableData.ReportAmount1.EnteredCurrencyAmount
                    label is "<TaxableData.FinanceEnterpriseGroup.ReportOne>"
                    display only
                TaxableData.FinanceEnterpriseGroup.ReportCurrencyOne
                    no label
                    display only
                TaxableData.ReportAmount1.EnteredCurrencyRate
                    no label
                    display only
        visible when (TaxableData.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
            paragraph
                TaxableData.ReportAmount2.EnteredCurrencyAmount
                    label is "<TaxableData.FinanceEnterpriseGroup.ReportTwo>"
                    display only
                TaxableData.FinanceEnterpriseGroup.ReportCurrencyTwo
                    no label
                    display only
                TaxableData.ReportAmount2.EnteredCurrencyRate
                    no label
                    display only
        visible when (TaxableData.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
            paragraph
                TaxableData.ReportAmount3.EnteredCurrencyAmount
                    label is "<TaxableData.FinanceEnterpriseGroup.ReportThree>"
                    display only
                TaxableData.FinanceEnterpriseGroup.ReportCurrencyThree
                    no label
                    display only
                TaxableData.ReportAmount3.EnteredCurrencyRate
                    no label
                    display only
        visible when (TaxableData.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
            paragraph
                TaxableData.ReportAmount4.EnteredCurrencyAmount
                    label is "<TaxableData.FinanceEnterpriseGroup.ReportFour>"
                    display only
                TaxableData.FinanceEnterpriseGroup.ReportCurrencyFour
                    no label
                    display only
                TaxableData.ReportAmount4.EnteredCurrencyRate
                    no label
                    display only
        visible when (TaxableData.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
            paragraph
                TaxableData.ReportAmount5.EnteredCurrencyAmount
                    label is "<TaxableData.FinanceEnterpriseGroup.ReportFive>"
                    display only
                TaxableData.FinanceEnterpriseGroup.ReportCurrencyFive
                    no label
                    display only
                TaxableData.ReportAmount5.EnteredCurrencyRate
                    no label
                    display only
