SETermAndConditionWithAttachment is a CompositeForm
    is primary
    title is "SourcingEventTermAndCondition"
    context form is TermAndConditionContextForm
        background is theme color
    TermAndConditions is a Panel
        form is TermAndConditionForm
    AdditionalTermLevels is a Panel
        visible when (NotLevelFour)
        list is ChildTermAndConditionRel.ChildTermsAndConditions
    Attachments is a Panel
        list is SourcingEventTermAndConditionAttachment set.SETermAndConditionAttachments
