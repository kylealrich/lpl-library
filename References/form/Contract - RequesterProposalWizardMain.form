RequesterProposalWizardMain is a Form
    title is "ProposedContractInformation"
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        visible when (ShowInputForRequesterProposedContract)
            two column
                paragraph
                    ContractGroup
                    ContractGroup.ProcurementGroup.Description
                        no label
                        display as text
                Name
                    label is "ProposedContractName"
            visible when (not ContractExists)
                two column
                    paragraph
                        ProposalTemplateNumber
                            select is ContractProposalTemplatesSelect
                        ProposalTemplateNumber.Name
                            no label
                            display as text
            visible when (ContractExists)
                display as text
                    two column
                        paragraph
                            ProposalTemplateNumber
                                select is ContractProposalTemplatesSelect
                            ProposalTemplateNumber.Name
                                no label
            two column
                Description
                visible when (not ContractExists)
                    ProposalTemplateNumber.Description
                        label is "TemplateDescription"
                        display as text
            visible when (not ProposedSupplierNameEntered)
                visible when (SupplierNotEntered)
                    single column
                        text of "MustEnterSupplierAndContactNumberOrDefaultFromTemplateOrEnterSupplierName"
                            bold
            four column
                paragraph
                    Supplier
                        label is "Supplier"
                    Supplier.SupplierName
                        no label
                        display as text
                visible when (not ContractExists)
                    paragraph
                        ProposalTemplateNumber.Supplier
                            label is "TemplateSupplier"
                            display as text
                        ProposalTemplateNumber.Supplier.SupplierName
                            no label
                            display as text
                paragraph
                    SupplierSourceId
                        label is "SupplierContact"
                    SupplierSourceId.MainContact.FirstAndLastName
                        no label
                        display as text
                visible when (not ContractExists)
                    paragraph
                        ProposalTemplateNumber.SupplierSourceId
                            label is "TemplateSupplierContact"
                            display as text
                        ProposalTemplateNumber.SupplierSourceId.MainContact.FirstAndLastName
                            no label
                            display as text
            text of "Proposed_Supplier_Information"
                bold
            four column
                ProposedSupplierName
                    label is "SupplierName"
                ProposedMailingAddress.Country
                ProposedTaxID.TaxIdType
                ProposedTaxID.TaxId
            four column
                ProposedMailingAddress.DeliveryAddress.AddressLine1
                    label is "Address"
                ProposedMailingAddress.Municipality
                ProposedMailingAddress.StateProvince
                    label is "State/Province"
                ProposedMailingAddress.PostalCode
            paragraph
                ProposalApprover
                    select is ProposalApproverSelect
                ProposalApprover.Employee.FirstLastName
                    no label
                    display as text
            two column
                DefaultAccount
                    compact format
                visible when (not ContractExists)
                    visible when (ProposalTemplate entered)
                        ProposalTemplateNumber.DefaultAccount
                            label is "TemplateDefaultAccount"
                            display as text
                            compact format
            three column
                ProposedTotalContractAmount
                paragraph
                    EffectiveDate
                    ExpirationDate
        visible when (not ShowInputForRequesterProposedContract)
            display as text
                single column
                    Name
                    Description
                paragraph
                    ProposalTemplateNumber
                        label is "Template"
                    ProposalTemplateNumber.Name
                        no label
                        display as text
                two column
                    paragraph
                        ContractType
                        ContractSubtype
                    paragraph
                        ContractClassification
                        ContractSubclassification
                visible when (not ProposedSupplierNameEntered)
                    two column distributed
                        paragraph
                            Supplier
                                label is "Supplier"
                            Supplier.SupplierName
                                no label
                        paragraph
                            SupplierSourceId
                                label is "SupplierContact"
                            SupplierSourceId.MainContact.FirstAndLastName
                                no label
                visible when (ProposedSupplierNameEntered)
                    text of "Proposed_Supplier_Information"
                        bold
                        color of blue
                    four column
                        ProposedSupplierName
                        ProposedTaxID.TaxIdType
                        ProposedTaxID.TaxId
                        ProposedMailingAddress.Country
                    four column
                        ProposedMailingAddress.DeliveryAddress.AddressLine1
                            label is "Address"
                        ProposedMailingAddress.Municipality
                        ProposedMailingAddress.StateProvince
                        ProposedMailingAddress.PostalCode
                two column
                    paragraph
                        ProposalApprover
                        ProposalApprover.Employee.FirstLastName
                            no label
                            display as text
                    ProposedTotalContractAmount
                two column
                    paragraph
                        EffectiveDate
                        ExpirationDate
                    DefaultAccount
                        compact format
