TransferFromTo is a Form
    title is "TransferFrom&ToDetails"
    Layout
        two column
            column1
                header3 of "TransferFrom"
                paragraph
                    Book
                        display as text
                    Currency
                        display as text
                Book.Description
                    label is "Description"
                    display as text
                Basis
                    display as text
            column2
                header3 of "TransferTo"
                paragraph
                    ToBook
                        display as text
                    ToBookCurrency
                        display as text
                ToBook.Description
                    label is "Description"
                    display as text
                ToBasis
                    display as text
