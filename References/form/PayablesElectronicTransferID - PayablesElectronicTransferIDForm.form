PayablesElectronicTransferIDForm is a Form
    is primary
    title is "ElectronicTransferIdentification"
    Layout
        paragraph
            VendorGroup
            VendorGroup.Description
                no label
                display as text
        paragraph
            PayGroup
            PayGroup.Name
                no label
                display as text
        paragraph
            BankTransactionCode
                label is "PaymentCode"
            BankTransactionCode.Description
                no label
                display as text
        paragraph
            ElectronicTransferID
                label is "IdentificationNumber"
        paragraph
            OrganizationIdentifier
        paragraph
            PaymentOutputFileName
