SourcingEventNotificationSumForm is a SummaryForm
    title is "_"
    Layout
        visible when (ContextSupplyManagementReport.SupplierDetail)
            header5 of "HistoryForSupplier\\:_<NotifiedSupplier.Supplier.SupplierName>_ForEventFinalizedFrom_<ContextSupplyManagementReport.FromDate>_\\to_<ContextSupplyManagementReport.ThruDate>"
            header5 of "SupplierGroup\\:_<NotifiedSupplier.SupplierGroup>"
            header5 of "Supplier\\:_<NotifiedSupplier.Supplier>"
            header5 of "DiversityCodesExist\\:_<NotifiedSupplier.Supplier.DiversityCodesExist>"
            header5 of "NotificationDetailForSupplierContact\\:_<NotifiedSupplier.SupplierSourceId>"
            header5 of "Name\\:_<NotifiedSupplier.SupplierSourceId.MainContact>"
                right align
            header5 of "TelephoneNumber\\:_<NotifiedSupplier.SupplierSourceId.TelephoneNumber>"
                right align
            header5 of "EmailAddress\\:_<NotifiedSupplier.SupplierSourceId.EmailAddress>"
                right align
            list is SourcingEventNotificationRel.EventNotificationAwardBySupplierList
            header5 of "NotificationsForSourceI\\D\\:_<DerivedTotalNotificationForContact>"
            list is EventLineResponsesByDiversitySupplierRel.EventLineResponsesByDiversitySupplierList
        header5 of "TotalsForContact_<NotifiedSupplier.SupplierSourceId>"
        three column
            column1
                DerivedTotalEventsBid
                    label is "TotalEventsBid"
                    display when blank
                DerivedTotalEventsAwarded
                    label is "TotalEventsAwarded"
                    display when blank
                DerivedEventsBidPercent
                    label is "EventBidSuccess"
                    display when blank
            column2
                DerivedTotalEventLinesBid
                    label is "TotalLinesBid"
                    display when blank
                DerivedTotalEventLinesAwarded
                    label is "TotalLinesAwarded"
                    display when blank
                DerivedEventLinesBidPercent
                    label is "LineBidSuccess"
                    display when blank
            column3
                DerivedBidTotal
                    label is "BidTotal"
                    display when blank
                DerivedAwardedTotal
                    label is "AwardedTotal"
                    display when blank
                DerivedTotalBidPercent
                    label is "AmountBidSuccess"
                    display when blank
