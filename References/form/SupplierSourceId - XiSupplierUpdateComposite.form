XiSupplierUpdateComposite is a CompositeForm
    title is "Supplier_-<Supplier.SupplierName>"
    called out actions only
    context form is SupplierContext
        show context form on left
    Actions
        action is refresh
            label is "_"
    Main is a DashBoardPanel
        Profile is a Panel
            overview navigation is SupplierNav
            form is SupplierUpdateMain
        MultifactorAuthentication is a Panel
            visible when (DerivedMFAEnabled)
            form is RegisteredMFADeviceDisplay
        Contacts is a Panel
            visible when (IsPrimaryAndPortalContact)
            list is Supplier.SupplierSourceId set.SupplierContactCardList
        CommodityCodes is a Panel
            overview navigation is CommodityCodeNav
            list is SupplierCommodityCode set.SupplierCommodityCodeUpdateCardList
        Certifications is a Panel
            visible when (CanDisplayAllCertificationsPortalContact)
            overview navigation is AllCertificationsNav
            list is Supplier.AllSupplierCertificationsRel.SupplierSideCertificationsCardList
        ContactCertifications is a Panel
            visible when (CanDisplayMyCertificationsPortalContact)
            overview navigation is CertificationNav
            list is SupplierCertificationRel.SupplierSideCertificationsCardList
        DiversityCodes is a Panel
            visible when (PrimaryDiversityResponsePortalContact)
            overview navigation is DiversityCodeNav
            list is Supplier.SupplierDiversityResponse set.DiversityResponseUpdateCardList
        ProxyNotifications is a Panel
            visible when (PortalContact)
            overview navigation is ProxyNotificationNav
            list is SupplierContactProxy set.ProxyNotificationCardList
        Questions is a Panel
            title is "Questions"
            visible when (IsPrimaryNotConditional)
            triple width
            overview navigation is SupplierAllQuestions
            list is SupplierGroup.ActiveSupplierGroupQuestions.SupplierQuestionsCardList
        QuestionsConditional is a Panel
            title is "Questions"
            visible when (IsPrimaryConditional)
            triple width
            overview navigation is SupplierAllQuestions
            list is Supplier.ActiveSupplierQuestions.BuyerSupplierQuestionsCardList
    Banking is a DashBoardPanel
        SupplierBank is a Panel
            visible when (SupplierGroup.AllowBankInformation)
            double width
            overview navigation is SupplierBankInformationNav
            form is SupplierBankForm
        MyBank is a Panel
            visible when (CanDisplayBankInformationPortalContact)
            double width
            overview navigation is BankInformationNav
            form is BankInformationForm
        AdditionalContactBankInformation is a Panel
            visible when (CanDisplayBankInformationIsPrimaryPortalContact)
            double height
            list is Supplier.SupplierSourceId set.SupplierContactBankCardList
    Evaluations is a Panel
        visible when (session.key.SupplierGroup.CanViewPerformance)
        list is SupplierPerformanceEvaluationRel.SupplierEvaluationsPortalList
    Metrics is a Panel
        visible when (session.key.SupplierGroup.CanViewMetrics)
        form is SupplierPortalMetrics
