BusinessClassForm is a CompositeForm
    is primary
    title is "BusinessClass"
    context form is BusinessClassHeader
    IncludeFields is a Panel
        visible when (FieldSelection.IncludeFields)
        list is ASBCField set.FieldList
    ExcludeFields is a Panel
        visible when (FieldSelection.ExcludeFields)
        list is ASBCField set.FieldList
    PrerequisiteBusinessClasses is a Panel
        list is ASDependentBC set.ASDependentBCList
    RelatedBusinessClassLink is a Panel
        list is ASRelatedBusinessClass set.ASRelatedBCLinksList
