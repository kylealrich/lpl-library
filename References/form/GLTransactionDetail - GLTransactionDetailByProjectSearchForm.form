GLTransactionDetailByProjectSearchForm is a SearchForm
    title is "SearchDetailsBy<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
    Layout
        four column distributed
            FinanceCodeBlock.Project
                label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
            FinanceCodeBlock.Project.ProjectStatus
                label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Status"
            FinanceCodeBlock.GeneralLedgerChartAccount
                label is "Account"
            FinanceCodeBlock.GeneralLedgerChartAccount.GeneralLedgerSubAccount
                label is "SubAccount"
        four column distributed
            SearchDateRange.Begin
                label is "FromTransactionDate"
            SearchDateRange.End
                label is "ToTransactionDate"
            PostingDateRange.BeginDate
                label is "FromPostingDate"
            PostingDateRange.EndDate
                label is "ToPostingDate"
        four column distributed
            System
            GeneralLedgerEvent
                label is "Event"
            JournalizeGroup
            SearchGroup
        four column distributed
            AccountingEntity
                label is "From<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
            FinanceCodeBlock.ToAccountingEntity
                label is "To<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
            FinanceCodeBlock.AccountingUnit
                label is "AccountingUnit"
            CurrencyCode
                label is "Currency"
        four column distributed
            Resource
                label is "EmploymentI\\D"
            Status
            LaborDistribution
            IndirectBurden
        four column distributed
            Billed
            RevenueRecognized
            Capitalize
            APPaid
        four column distributed
            HROrganizationUnit
            PayCode
            Job
            Position
        paragraph
            visible when (false)
                PostingDate
