TrueCostAllocationLine is a CompositeForm
    is primary
    title is "True\\CostAllocationLine<TrueCostAllocationLine>"
    context form is Main
        background is theme color
    Parameters is a Panel
        title is "Main"
        form is Parameters
    Capacity is a MultiListPanel
        title is "Capacity"
        visible when (HasPracticalCapacity)
        list is TrueCostCapacity.PracticalCapacityList
        list is PracticalCapacityVolumeRel.PracticalCapacityVolumeList
            Special Actions
                action is CreatePracticalCapacityVolume
                    label is "Create"
                    mouse over text is "CreatePracticalCapacityVolume"
        list is UnutilizedCapacityRel.UnutilizedCapacityList
            Special Actions
                action is CreateUnutilizedCapacity
                    label is "Create"
                    mouse over text is "CreateUnutilizedCapacity"
                    hide when invalid
