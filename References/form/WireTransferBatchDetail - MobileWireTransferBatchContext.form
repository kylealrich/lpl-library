MobileWireTransferBatchContext is a Form
    title is "WireTransfer"
    Layout
        display as text
            two column table
                column1 of 50%
                    WireTransferBatch.FinancialInstitution.FinancialInstitutionAndDescription
                        label is "FinancialInstitution"
                    TransactionDate
                    visible when (DisplayDistributionTotals)
                        visible when (FeeAmount entered)
                            TotalFeeDistributions
                                label is "FeeDistributions"
                    visible when (ExternalTransfer)
                        TotalDistributions
                column2 of 50%
                    WireTransferBatch.FinancialInstitutionBranch.FinancialInstitutionBranchAndDescription
                        label is "Branch"
                    PostingDate
                    visible when (DisplayDistributionTotals)
                        DistributionDifference
            single column
                visible when (DisplayDistributionTotals)
                    RequiredDistributionAmount
                        label is "RequiredDistribution"
