ServiceComposisteForm is a CompositeForm
    is primary
    title is "Service:<Service>"
    show panel control on left
    Actions
        action is save
        navigation is ViewQuickSummary
            action icon is export
            label is "ViewSummary"
    Details is a Panel
        mouse over text is "Details"
        form is DefaultServiceForm
    SecurityPolicies is a Panel
        visible when (not IsExternalEntityIdConfigured)
        mouse over text is "SecurityPolicies"
        form is SecurityPolicies
    ServiceProperties is a Panel
        mouse over text is "ServiceProperties"
        list is ServiceProperty set.ServicePropertyHelperList
    SSODomainServiceBelongsTo is a Panel
        title is "SSODomainService"
        mouse over text is "The_SSO_domain_that_this_service_belongs_to"
        list is ServiceSSODomain set.SecondaryServiceSSODomainHelperList
    HTTPEndpointsAssignedToService is a Panel
        title is "HTTPEndpoints"
        mouse over text is "HTTPEndpointsAssignedToService"
        list is ServiceHTTPEndpoint set.ServiceHTTPEndpointHelperList
    ServiceTransactionValue is a Panel
        title is "TransactionValue"
        visible when (IsGreyBox)
        mouse over text is "ServiceTransactionValue"
        list is ServiceTransactionValue set.ServiceTransactionValueHelperList
    ServiceUrlPattern is a Panel
        title is "ServiceUrlPattern"
        visible when (IsGreyBox)
        mouse over text is "ServiceUrlPattern"
        list is ServiceUrlPattern set.ServiceUrlPatternHelperList
