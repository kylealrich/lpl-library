WorkunitComposite is a CompositeForm
    title is "WorkUnit"
    context form is WorkunitForm
    Detail is a Panel
        title is "Detail"
        form is DetailForm
    Metric is a Panel
        title is "ResourceMetric"
        list is PfiWorkunitMetric set.WorkunitMetricList
    Info is a Panel
        title is "Information<InfoCount>"
        list is PfiWorkunitInfo set.InfoList
    ActivityNodes is a Panel
        title is "ActivityNodes<ActivityCount>"
        list is PfiActivity set.ActivityList
    Variables is a Panel
        title is "Variables<VariableCount>"
        list is PfiWorkunitVariable set.LiveWorkunitVariables
    ErrorMessagesOld is a Panel
        title is "ErrorMessages<ErrorMessageCount>"
        visible when (not RunIdExists)
        list is PfiErrorMessage set.ErrorMessageOldList
    ErrorMessages is a Panel
        title is "ErrorMessages<ErrorMessageNewCount>"
        visible when (RunIdExists)
        list is PfiErrorMessage set.ErrorMessageList
    InputData is a Panel
        title is "InputData"
        list is PfiWorkunitInputData set.WorkunitInputDataList
    ApprovalTracking is a Panel
        visible when (HasProcessIndicator)
        form is ApprovalTracking
    UserActions is a Panel
        title is "UserActions<QueueCount>"
        list is PfiQueueRel.QueueList
    Log is a Panel
        title is "Log"
        form is WorkunitLog
    Messages is a Panel
        title is "Messages<MessageCount>"
        list is PfiMessage set.MessageList
    RelatedLinks is a Panel
        title is "RelatedLinks<FolderCount>"
        list is PfiWorkunitFolder set.LiveWorkunitFolders
    RelatedActvityNodes is a Panel
        title is "RelatedWorkUnits<RelatedWorkunitCount>"
        list is PfiWorkunitChildrenRel.ByParentWorkunitWorkunit
    MetricCharts is a Panel
        title is "UAMetric"
        list is PfiActivity set.ActivityListChart
    LastActivity is a Panel
        title is "CurrentActivity"
        visible when (IsHoldingThread)
        form is CurrentActivityForm
    CheckpointActivity is a Panel
        visible when (CheckpointExists)
        list is PfiCheckpointActivityRel.ActivityList
