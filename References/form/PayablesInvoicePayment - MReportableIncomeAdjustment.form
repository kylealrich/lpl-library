MReportableIncomeAdjustment is a Form
    title is "ReportableIncomeAdjustment"
    Layout
        two column distributed
            column1
                paragraph
                    Company
                        display as text
                    Company.Name
                        no label
                        display as text
                PayablesInvoice
                    display as text
                IncomeCode
                    select is IncomeCodes
                PayGroup
                    display as text
                TransactionNetPayment
                    display as text
                ReportableIncomeAmount
                PaymentStatus
                    display as text
            column2
                paragraph
                    Vendor
                        display as text
                    Vendor.VendorName
                        no label
                        display as text
                PayablesInvoicePayment.PaymentSequence
                    display as text
                IncomeCode.Description
                    no label
                    display as text
                ProcessLevel
                    display as text
                InvoiceCurrency
                    display as text
                CheckDate
                    display as text
