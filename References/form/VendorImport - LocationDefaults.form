LocationDefaults is a Form
    Layout
        two column
            column1
                header2 of "Invoice"
                InvoiceRoutingCategory
                HoldCode
                PaymentPriority
                    label is "Priority"
                TaxCode
                TaxUsageCode
                DiversityCode
                    label is "DefaultDiversityCode"
                blank line
                ValidateCertificationDate
            column2
                header2 of "Payment"
                TermsCode
                CashCode
                BankTransactionCode
                DiscountCode
                InvoiceGroup
                SeparatePayment
                Enclosure
        header2 of "Currency"
        two column table
            column1
                InvoiceCurrency
                    label is "Invoice"
            column2
                RevalueCurrency
        header2 of "Returns"
        paragraph
            ReplaceGoods
                label is "ReplaceGoods"
            ShipOrHold
                label is "ShipOrHoldGoods"
            WriteOffAmount
                label is "MinimumClaimAmount"
            VendorClaimType
            visible when (VendorClaimType.Chargeback)
                VendorClaimHoldCode
                    label is "ChargebackHoldCode"
