FrameworkChangesForm is a Form
    title is "ChangeDetails"
    disable field options
    Layout
        paragraph
            BusinessView
                no label
                header1
                display as text
        visible when (IsUserList)
            form is UserListChangesForm
        visible when (IsUserForm)
            form is UserFormChangesForm
        visible when (IsUserCardView)
            form is UserCardViewChangesForm
        visible when (IsUserPage)
            form is UserPageChangesForm
        visible when (IsUserMenu)
            form is UserMenuChangesForm
        visible when (IsUserSJRptMap)
            form is UserSJRptMapChangesForm
        visible when (IsUserWebApp)
            form is UserWebAppChangesForm
        visible when (IsUserProd)
            form is UserProdChangesForm
        visible when (IsUserBusinessClass)
            form is UserBusinessClassChangesForm
        visible when (IsUserKeyField)
            form is UserKeyFieldChangesForm
        visible when (IsUserField)
            form is UserFieldChangesForm
        visible when (IsUserAction)
            form is UserActionChangesForm
        visible when (IsUserCube)
            form is UserCubeChangesForm
        visible when (IsUserRelation)
            form is UserRelationChangesForm
        visible when (IsUserImage)
            form is UserImageChangesForm
        visible when (IsUserListView)
            form is UserListViewChangesForm
        visible when (IsUserPhrase)
            form is UserPhraseChangesForm
        visible when (IsUserWebServiceInterface)
            form is UserWebServiceInterfaceChangesForm
        visible when (IsSecurityClass)
            form is SecurityClassChangesForm
        visible when (IsConfigurationParameter)
            form is ConfigurationParameterChangesForm
        visible when (IsRole)
            form is RoleChangesForm
        visible when (IsRoleSecurityClass)
            form is RoleSecurityClassChangesForm
        visible when (IsActorRole)
            form is ActorRoleChangesForm
        visible when (IsReplicationSet)
            form is ReplicationSetChangesForm
        visible when (IsRepSetBC)
            form is RepSetBCChangesForm
        visible when (IsRepSetBCField)
            form is RepSetBCFieldChangesForm
        visible when (IsRepSetBCRelatedReplicate)
            form is RepSetBCRelatedReplicateChangesForm
        visible when (IsRepSetBCClientField)
            form is RepSetBCClientFieldChangesForm
        visible when (IsPfiBciApiInfo)
            form is PfiBciApiInfoChangesForm
        visible when (IsPfiClassicApplication)
            form is PfiClassicApplicationChangesForm
        visible when (IsPfiClassicField)
            form is PfiClassicFieldChangesForm
        visible when (IsPfiDocument)
            form is PfiDocumentChangesForm
        visible when (IsPfiFilterKey)
            form is PfiFilterKeyChangesForm
        visible when (IsPfiFilterKey)
            form is PfiFilterKeyChangesForm
        visible when (IsPfiFlowDefinition)
            form is PfiFlowDefinitionChangesForm
        visible when (IsPfiFlowVersion)
            form is PfiFlowVersionChangesForm
        visible when (IsPfiForm)
            form is PfiFormChangesForm
        visible when (IsPfiFormField)
            form is PfiFormFieldChangesForm
        visible when (IsPfiServiceDefinition)
            form is PfiServiceDefinitionChangesForm
        visible when (IsPfiServiceFlowDefinition)
            form is PfiServiceFlowDefinitionChangesForm
        visible when (IsPfiServiceVariable)
            form is PfiServiceVariableChangesForm
        visible when (IsPfiTask)
            form is PfiTaskChangesForm
        visible when (IsPfiTrigger)
            form is PfiTriggerChangesForm
        visible when (IsPfiTriggerFolder)
            form is PfiTriggerFolderChangesForm
        visible when (IsPfiUserCategory)
            form is PfiUserCategoryChangesForm
        visible when (IsPfiUserPreference)
            form is PfiUserPreferenceChangesForm
        visible when (IsPfiUserProfile)
            form is PfiUserProfileChangesForm
        visible when (IsPfiUserTask)
            form is PfiUserTaskChangesForm
        visible when (InstanceSelection entered)
            paragraph
                InstanceSelection
                    display as text
        five column
            column1
                text of "Who"
                    header3
                Actor
                    display only
                    raise yellow alert when (AuthenticatedActorIsDifferent)
                        mouse over text is "Authenticated_Actor:<AuthenticatedActor>"
            column2
                text of "When"
                    header3
                SystemStamp
                    display only
                Period
                    display only
                EntryStamp
                    display only
                AppliedStamp
                    display only
                visible when (EffectiveDate entered)
                    EffectiveDate
                        display only
                visible when (EffectiveTimeZone entered)
                    EffectiveTimeZone
                        display only
                visible when (EffectiveThroughDate entered)
                    EffectiveThroughDate
                        display only
            column3
                text of "How"
                    header3
                ActionType
                    display only
                Action
                    display only
                    raise blue alert when (ActionAndInitiatingActionIsDifferent)
                        mouse over text is "Initiating_Action:<InitiatingAction>"
                visible when (InvokingAction entered)
                    InvokingAction
            column4
                text of "Why"
                    header3
                Subject
                    display only
                ReasonCode
                    display only
            column5
                text of "UniqueID"
                    header3
                BOUniqueID
                    label is "BusinessClassRecord"
                    display only
                AuditEntryID
                    label is "AuditEntryRecord"
                    display only
                visible when (TransactionID entered)
                    TransactionID
                        label is "Transaction"
                        display only
                visible when (ActionRequestID entered)
                    ActionRequestID
                        label is "ActionRequest"
                        display only
