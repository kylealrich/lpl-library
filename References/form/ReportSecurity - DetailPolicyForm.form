DetailPolicyForm is a CompositeForm
    title is "DeliveredReportPolicy"
    Policy is a Panel
        form is ReportSecurityConsoleDetailForm
    ReviewChanges is a Panel
        visible when (HasUnreviewedChanges)
        form is SuggestedDifferencePanelForm
    History is a Panel
        list is ReportSecurityHistory set.ConsoleConfigurationChangesList
    DeliveredHistory is a Panel
        visible when (BasedOnDelivered)
        list is ReportSecurityDeliveredHistory set.ConsoleConfigurationChangesList
