RequestNewAccountingEntity is an ActionForm
    action is RequestNewAccountingEntity
    title is "New_<FinanceEnterpriseGroup.AccountingEntityLabel>"
    Layout
        visible when (not actor.initiator)
            header3 of "ReviewNew<FinanceEnterpriseGroup.AccountingEntityLabel>"
        visible when (actor.initiator)
            header3 of "ThisRequestWillBeRoutedForApproval;AfterApprovalThe<FinanceEnterpriseGroup.AccountingEntityLabel>WillBeCreated."
        header3 of "RequestDetails"
        two column distributed
            column1
                visible when (not actor.initiator)
                    display as text
                        RequestedBy
                        button of "<RequesterPhone.SubscriberNumber>"
                            link is "tel:<RequesterPhone.SubscriberNumber>"
                            label is "Phone"
                            display as link
                        blank line
                        RequestPriority
                            label is "Priority"
                visible when (actor.initiator)
                    RequestPriority
                        label is "Priority"
            column2
                visible when (not actor.initiator)
                    display as text
                        RequesterActor
                            label is "ID"
                        button of "<RequesterEmail>"
                            link is "mailto:<RequesterEmail>"
                            label is "Email"
                            display as link
                        blank line
                        NeededBy
                visible when (actor.initiator)
                    NeededBy
        header3 of "<FinanceEnterpriseGroup.AccountingEntityLabel>Details"
        paragraph
            FinanceEnterpriseGroup
                display as text
            FinanceEnterpriseGroup.EnterpriseGroup.Description
                no label
                display as text
        two column distributed
            column1
                PrmAccountingEntity
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>"
            column2
                Name
        two column distributed
            Status
            PostingCategory
        visible when (FinanceEnterpriseGroup.InterEntityDimension.SubAccount)
            display as text
                paragraph
                    SubAccount
                        label is "<FinanceEnterpriseGroup.AccountingEntityLabel>RelationIs_\\SubAccount"
                    SubAccount.Description
                        no label
        two column table
            column headers
                "Currency"
                "AutoBalanceThreshold"
            row headers
                "FunctionalCurrency"
                "AlternateCurrency"
                "AlternateCurrency2"
                "AlternateCurrency3"
            column1
                FunctionalCurrency
                    no label
                AlternateCurrency
                    no label
                AlternateCurrency2
                    no label
                AlternateCurrency3
                    no label
            column2
                FunctionalAutoBalanceThreshold
                    no label
                AlternateAutoBalanceThreshold
                    no label
                Alternate2AutoBalanceThreshold
                    no label
                Alternate3AutoBalanceThreshold
                    no label
        visible when (FinanceEnterpriseGroup.InterEntityDimension.Dimension1)
            paragraph
                FinanceDimension1
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>RelationIs<FinanceEnterpriseGroup.FinanceDimension1Label>"
                    display only
                FinanceDimension1.Description
                    no label
                    display as text
        visible when (FinanceEnterpriseGroup.InterEntityDimension.Dimension2)
            paragraph
                FinanceDimension2
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>RelationIs<FinanceEnterpriseGroup.FinanceDimension2Label>"
                    display only
                FinanceDimension2.Description
                    no label
                    display as text
        visible when (FinanceEnterpriseGroup.InterEntityDimension.Dimension3)
            paragraph
                FinanceDimension3
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>RelationIs<FinanceEnterpriseGroup.FinanceDimension3Label>"
                    display only
                FinanceDimension3.Description
                    no label
                    display as text
        visible when (FinanceEnterpriseGroup.InterEntityDimension.Dimension4)
            paragraph
                FinanceDimension4
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>RelationIs<FinanceEnterpriseGroup.FinanceDimension4Label>"
                    display only
                FinanceDimension4.Description
                    no label
                    display as text
        visible when (FinanceEnterpriseGroup.InterEntityDimension.Dimension5)
            paragraph
                FinanceDimension5
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>RelationIs<FinanceEnterpriseGroup.FinanceDimension5Label>"
                    display only
                FinanceDimension5.Description
                    no label
                    display as text
        visible when (FinanceEnterpriseGroup.InterEntityDimension.Dimension6)
            paragraph
                FinanceDimension6
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>RelationIs<FinanceEnterpriseGroup.FinanceDimension6Label>"
                    display only
                FinanceDimension6.Description
                    no label
                    display as text
        visible when (FinanceEnterpriseGroup.InterEntityDimension.Dimension7)
            paragraph
                FinanceDimension7
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>RelationIs<FinanceEnterpriseGroup.FinanceDimension7Label>"
                    display only
                FinanceDimension7.Description
                    no label
                    display as text
        visible when (FinanceEnterpriseGroup.InterEntityDimension.Dimension8)
            paragraph
                FinanceDimension8
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>RelationIs<FinanceEnterpriseGroup.FinanceDimension8Label>"
                    display only
                FinanceDimension8.Description
                    no label
                    display as text
        visible when (FinanceEnterpriseGroup.InterEntityDimension.Dimension9)
            paragraph
                FinanceDimension9
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>RelationIs<FinanceEnterpriseGroup.FinanceDimension9Label>"
                    display only
                FinanceDimension9.Description
                    no label
                    display as text
        visible when (FinanceEnterpriseGroup.InterEntityDimension.Dimension10)
            paragraph
                FinanceDimension10
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>RelationIs<FinanceEnterpriseGroup.FinanceDimension10Label>"
                    display only
                FinanceDimension10.Description
                    no label
                    display as text
        visible when (not FinanceEnterpriseGroup.ADBSelection.None)
            single column
                AverageDailyBalance
        blank line
        header3 of "CurrentYearAndPeriod"
        paragraph
            CloseConfiguration
            CloseConfiguration.Description
                no label
                display as text
        two column
            FiscalYear
                label is "CurrentYear"
                display as text
            CurrentPeriod.GeneralLedgerCalendarPeriod
                label is "CurrentPeriod"
                select is ClosePeriodsForContextConfiguration
        two column
            text of "_"
            CurrentPeriod.PeriodName
                no label
                display as text
        visible when (FinanceEnterpriseGroup.AccountingUnitLabelExists)
            header3 of "Zones"
            single column
                AccountingUnitZoneBalancing
                    label is "<FinanceEnterpriseGroup.AccountingUnitLabel>ZoneBalancing"
        header3 of "JournalOptions"
        two column distributed
            column1
                AutoBalance
            column2
                CurrencyLedger
                    label is "BalanceByTransactionCurrency"
        two column distributed
            column1
                UnreleaseOfReleasedEntries
                    label is "UnreleaseManualJournal"
            column2
                ValidSystems
        two column distributed
            column1
                JournalApproval
            column2
                JournalApprovalAmount
        single column
            BypassApprovalCode
        header3 of "NameAndAddressInformation"
        two column
            AddressCode
            AddressCode.Description
                no label
                display as text
        blank line
        visible when (AddressCode entered)
            single column
                AddressCode.PersonResponsible
                    label is "PersonResponsible"
                    display as text
                AddressCode.PostalAddress.Country
                    display as text
                AddressCode.PostalAddress.DisplayAddressLine1
                    label is "StreetAddress"
                    display as text
                AddressCode.PostalAddress.DisplayAddressLine2
                    no label
                    display as text
                AddressCode.PostalAddress.DisplayAddressLine3
                    no label
                    display as text
                AddressCode.PostalAddress.DisplayAddressLine4
                    no label
                    display as text
                AddressCode.PostalAddress.Municipality
                    label is "City"
                    display as text
                AddressCode.PostalAddress.StateProvince
                    label is "State/Province"
                    display as text
                AddressCode.PostalAddress.PostalCode
                    label is "PostalCode"
                    display as text
                AddressCode.PostalAddress.County
                    label is "County/District"
                    display as text
                AddressCode.PostalAddress.Type
                    label is "Type"
                    display as text
                AddressCode.TelephoneNumber
                    label is "TelephoneNumber"
                    display as text
                AddressCode.FaxNumber
                    label is "FaxNumber"
                    display as text
                AddressCode.EmailAddress
                    label is "EmailAddress"
                    display as text
        header3 of "LeaseAccounting"
        single column
            LeaseAccountingStandard
        header3 of "ActionComment"
        single column
            action comment
                no label
                use text area
