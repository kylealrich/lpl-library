StaleDateOutputFileHeaderForm is a CompositeForm
    is primary
    title is "StaleDateOutputFile"
    context form is StaleDateOutputFileHeaderContext
        background is theme color
    Actions
        action is PerformStaleDate
            hide when invalid
        action is DeleteStaleDateOutputFiles
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is refresh
            label is "_"
    Parameters is a Panel
        form is ParametersForm
    OutputFileDetailsSelected is a Panel
        title is "Selected"
        visible when (SelectedWithoutProcessLevelProcessing)
        list is StaleDateOutputFileDetailsByCashCodeSelectedRel.StaleDateOutputFileDetailByCashCodeList
    OutputFileDetailsByProcessLevelSelected is a Panel
        title is "Selected"
        visible when (SelectedWithProcessLevelProcessing)
        list is StaleDateOutputFileDetailsByProcessLevelSelectedRel.StaleDateOutputFileDetailByProcessLevelList
    OutputFileDetailsProcessed is a Panel
        title is "Processed"
        visible when (ProcessedWithoutProcessLevelProcessing)
        list is StaleDateOutputFileDetailsByCashCodeProcessedRel.StaleDateOutputFileDetailByCashCodeList
    OutputFileDetailsByProcessLevelProcessed is a Panel
        title is "Processed"
        visible when (ProcessedWithProcessLevelProcessing)
        list is StaleDateOutputFileDetailsByProcessLevelProcessedRel.StaleDateOutputFileDetailByProcessLevelList
    TransactionsInError is a Panel
        title is "Errors"
        visible when (ErrorsExist)
        list is CashLedgerTransactionErrorForStaleDateRel.StaleDateErrorList
