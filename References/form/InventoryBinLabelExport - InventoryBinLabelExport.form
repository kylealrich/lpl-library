InventoryBinLabelExport is an ActionForm
    action is InventoryBinLabelExport
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            Location
                show up to 10 characters
            Location.Name
                no label
                display as text
        blank line
        three column
            column1
                paragraph
                    BinGroup
                        show up to 10 characters
                    BinGroup.Description
                        no label
                        display as text
            column2
                text of "\\or"
                    bold
            column3
                paragraph
                    StartBin
                        label is "FromBin"
                    EndBin
                        label is "ToBin"
        blank line
        two column distributed
            column1
                BinType
                SortSequence
                    label is "OutputRecordSortSequence"
                InclItemSrcInfo
                    label is "IncludeItemSourceInformation"
                BarcodeType
                    label is "BarcodeType_toInclude"
            column2
                StorageCode
                LabelsPerRec
                    label is "Quantity_ofLabels_perRecord"
                InclVenItemInfo
                    label is "IncludeVendorItemInformation"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                InventoryBinLabelExportReport_RDG
            column2 of 25%
                InventoryBinLabelExportReport_XPT
