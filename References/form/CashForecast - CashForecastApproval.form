CashForecastApproval is a CompositeForm
    title is "CashForecast"
    context form is CashForecastApprovalContext
        background is theme color
    Accounts is a Panel
        list is CashForecastAccount set.CashForecastAccountApprovalList
    ApprovalComments is a Panel
        visible when (HasApprovalComments)
        list is CashForecastReasonCodeUsage set.CashForecastReasonCodeUsageList
