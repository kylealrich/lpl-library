ItemHistoryByVendorList is a SummaryForm
    title is "_"
    Layout
        three column distributed
            column1
                text of "Vendor"
                    center
                    bold
                DisplayVendorName
                    no label
                    display when blank
            column2
                text of "TotalForVendor"
                    center
                    bold
                DerivedTotalForVendor
                    no label
                    display when blank
            column3
                text of "_"
                DerivedTotalForVendorPercent
                    no label
                    display when blank
        form is VendorRel.VendorDetailSubreport
        visible when (SupplyManagementReport.IncludeInvoiceDetail)
            form is InvoiceDetailSumForm
