ProcessingResultComposite is a CompositeForm
    title is "<Source>"
    context form is ProcessingResultContext
    Actions
        action is save
            hide when invalid
        action is export to pdf
        action is refresh
    Parameters is a Panel
        form is ProcessingResultParameters
    Errors is a Panel
        visible when (HasNoGLErrors)
        list is AssetErrorResultRel.AssetPeriodCloseError
    ErrorsEdits is a Panel
        title is "Errors"
        visible when (HasEditsErrors)
        list is AssetEdistsErrorResultRel.AssetPeriodEditErrors
    ReleasedAssetsReport is a Panel
        title is "Assets"
        visible when (IsRelease)
        list is ReleasedAssetsForBatchRel.ReleaseAssetsReport
    AssetBooks is a Panel
        title is "AssetBooks"
        visible when (IsPeriodClose)
        list is AssetBooksForCloseRel.ClosedBooks
    PeriodCloseAssetTransactions is a Panel
        title is "AssetTransactions"
        visible when (DisplayDetailModeAssetTransactions)
        list is AssetTransactionGroupRel.TransactionDetailList
    PeriodCloseAssetTransaction is a Panel
        title is "AssetTransactions"
        visible when (DisplayDetailModeWithNoTransactionAmountsAssetTransactions)
        list is AssetTransactionGroupRel.TransactionBookDetailList
    PeriodCloseAssetBookTransactions is a Panel
        title is "AssetBookTransactions"
        visible when (DisplayDetailModeAssetBookTransactions)
        list is AssetBookTransactionGroupRel.TransactionDetailList
    PeriodCloseAssetBookTransaction is a Panel
        title is "AssetBookTransactions"
        visible when (DisplayDetailModeWithNoTransactionAmountsAssetBookTransactions)
        list is AssetBookTransactionGroupRel.TransactionBookDetailList
    PeriodCloseNonUpdateResults is a Panel
        title is "AssetBooks"
        visible when (NonUpdateModePeriodClose)
        list is PeriodCloseBookRel.NonUpdatePeriodCloseBooksList
    ErrorList is a Panel
        title is "Errors"
        visible when (DisplayErrors)
        list is AssetErrorResultRel.AssetBatchErrorList
    ReleaseAssetTransactions is a Panel
        title is "AssetTransactions"
        visible when (ReleasedIncludeTransactionAmount)
        list is ReleasedTransactionsRel.BatchTransactionsList
    ReleaseAssetTransaction is a Panel
        title is "AssetTransactions"
        visible when (ReleasedExcludeTransactionAmount)
        list is ReleasedTransactionsRel.BatchBookAmountTransactionsList
    ReleaseAssetBookTransactions is a Panel
        title is "AssetBookTransactions"
        visible when (ReleasedIncludeBookTransactionAmount)
        list is ReleasedBookTransactionsRel.BatchTransactionsList
    ReleaseAssetBookTransaction is a Panel
        title is "AssetBookTransactions"
        visible when (ReleasedExcludeBookTransactionAmount)
        list is ReleasedBookTransactionsRel.BatchBookAmountTransactionsList
    UnreleasedTransactions is a Panel
        title is "AssetTransactions"
        visible when (UnreleasedIncludeTransactionAmount)
        list is UnreleasedTransactionsRel.BatchTransactionsList
    UnreleasedTransaction is a Panel
        title is "AssetTransactions"
        visible when (UnreleasedExcludeTransactionAmount)
        list is UnreleasedTransactionsRel.BatchBookAmountTransactionsList
    UnreleasedBookTransactions is a Panel
        title is "AssetBookTransactions"
        visible when (UnreleasedIncludeBookTransactionAmount)
        list is UnreleasedBookTransactionsRel.BatchTransactionsList
    UnreleasedBookTransaction is a Panel
        title is "AssetBookTransactions"
        visible when (UnreleasedExcludeBookTransactionAmount)
        list is UnreleasedBookTransactionsRel.BatchBookAmountTransactionsList
    DepreciationAssetBooks is a Panel
        title is "AssetBooks"
        visible when (DepreciationCalculationSourceCompany)
        list is DepreciationAssetBookRel.AssetBookDepreciationList
    DepreciationAssetBooksCG is a Panel
        title is "AssetBooks"
        visible when (DepreciationCalculationSourceCG)
        list is DepreciationAssetBookRel.AssetBookDepreciationCGList
    DepreciationAssetBooksCompanyAndBG is a Panel
        title is "AssetBooks"
        visible when (DepreciationCalculationSourceCompanyAndBG)
        list is DepreciationAssetBookRel.AssetBookDepreciationCompanyAndBGList
    DepreciationAssetBooksCGAndBG is a Panel
        title is "AssetBooks"
        visible when (DepreciationCalculationSourceCGAndBG)
        list is DepreciationAssetBookRel.AssetBookDepreciationCGAndBGList
    DepreciationAssetBooksListOnly is a Panel
        title is "AssetBooks"
        visible when (DepreciationCalculationSourceListOnly)
        list is DepreciationAssetBookRel.AssetBookDepreciationListOnly
    DepreciationErrorList is a Panel
        title is "Errors"
        visible when (DepreciationErrorsExists)
        list is AssetErrorResultRel.DepreciationCompanyGroupErrorList
