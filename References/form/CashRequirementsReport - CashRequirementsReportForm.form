CashRequirementsReportForm is a Form
    is primary
    title is "CashRequirementsReport"
    Layout
        single column
            VendorGroup
            PayGroup
            CashRequirementsResult
            CashRequirementsReport.CashCode
            CashRequirementsReport.BankTransactionCode
            CashRequirementsReport.InvoiceCurrency
            CashRequirementsReport.PayVendor
            CashRequirementsReport.RemitToCode
        paragraph
            TotalPaymentAmount
            CashRequirementsReport.InvoiceCurrency
                no label
        paragraph
            TotalDiscountAmount
            CashRequirementsReport.InvoiceCurrency
                no label
        paragraph
            TotalNetPaymentAmount
            CashRequirementsReport.InvoiceCurrency
                no label
        paragraph
            TotalBankCheckAmount
                label is "TotalBankPaymentAmount"
            CheckCurrency
                no label
