PhoneMainForm is a Form
    title is "MainForm"
    Layout
        two column table
            visible when (not RecordExists)
                column1
                    paragraph
                        CashCode
                        CashCode.Description
                            no label
                            display as text
                        CashCode.Currency
                            label is "Currency"
                            display as text
                    paragraph
                        Company.OrigCompany
                            label is "Company"
                            select is Company
                        Company.OrigCompany.Name
                            no label
                            display as text
                        Company.OrigCompany.Currency
                            label is "Currency"
                            display as text
                    paragraph
                        CurrencyTable
                        CurrencyTable.Description
                            no label
                            display as text
                column2
                    paragraph
                        CashLedgerElectronicFundsTransferTransaction.BankTransactionCode
                            label is "TransactionCode"
                            select is BankCreditTransactionCodes
                        CashLedgerElectronicFundsTransferTransaction.BankTransactionCode.Description
                            no label
                            display as text
                    paragraph
                        Reference
                            label is "ReferenceNumber"
                        Status
                            label is "Status"
                            display as text
                        visible when (VoidDate entered)
                            VoidDate
                                label is "RejectDate"
                                display as text
                    paragraph
                        ReceiptAmountUnsigned
                            label is "Amount"
                        visible when (IsUnreleased)
                            AppliedBankAmount
                                label is "AppliedAmount"
                                display as text
                            BalanceAmount
                                label is "Balance"
                                display as text
                        visible when (IsBatchApproved)
                            CashLedgerBatchApproval
            visible when (UnreleasedRecordExists)
                column1
                    paragraph
                        Company.OrigCompany
                            label is "Company"
                            select is Company
                        Company.OrigCompany.Name
                            no label
                            display as text
                        Company.OrigCompany.Currency
                            label is "Currency"
                            display as text
                    paragraph
                        CurrencyTable
                        CurrencyTable.Description
                            no label
                            display as text
                column2
                    paragraph
                        Reference
                            label is "ReferenceNumber"
                        Status
                            label is "Status"
                        visible when (VoidDate entered)
                            VoidDate
                                label is "RejectDate"
                    paragraph
                        ReceiptAmountUnsigned
                            label is "Amount"
                        visible when (IsUnreleased)
                            display as text
                                AppliedBankAmount
                                    label is "AppliedAmount"
                                BalanceAmount
                                    label is "Balance"
                        visible when (IsBatchApproved)
                            CashLedgerBatchApproval
        visible when (IsPendingApproval)
            two column table
                column1
                    CurrencyTable
                        show up to 10 characters
                column2
                    Reference
                        label is "ReferenceNumber"
                        show up to 10 characters
        two column table
            column1
                TransactionDate
            column2
                TransactionType
                    show up to 10 characters
        two column table
            column1
                BankEntity
                    label is "BankIdentification"
                    select is Banks
                    show up to 10 characters
            column2
                BankAccountValidationType
                    label is "AccountValidationType"
                    show up to 10 characters
        two column table
            column1
                ConfirmationNumber
                    show up to 10 characters
            column2
                ApprovalCostCenter
                    show up to 10 characters
        paragraph
            GeneralLedgerSourceCode
                label is "EventCode"
                show up to 10 characters
            GeneralLedgerSourceCode.Description
                no label
                display as text
        paragraph
            CashLedgerProcessGroup
            CashLedgerProcessGroup.Description
                no label
                display as text
        single column
            Description
            CustomerName
                label is "CustomerReference"
        two column table
            column1
                BankAccountNumber
                    label is "AccountNumber"
                    show up to 10 characters
            column2
                paragraph
                    BankAccountType
                        label is "AccountType"
                        show up to 10 characters
                    GlobalBankAccountType
        two column table
            column1
                ACHPrenotification
                    label is "PrenotificationStatus"
                    show up to 10 characters
                ApprovalCategory
                    show up to 10 characters
            column2
                RIBKey
                EFTProcessingHold
                    label is "ProcessingHold"
        paragraph
            visible when (HasAttachment)
                EFTTransferAttachment.Title
            EFTTransferAttachment.File
        paragraph
            CommentText
                display as text
                use text area of 5 lines
        header2 of "TransactionData"
        single column
            TransactionData1
            TransactionData2
            TransactionData3
            TransactionData4
