MobileCurrencyAmountsForm is a Form
    title is "CurrencyAmounts"
    Layout
        visible when (false)
            ReceiptBaseAmount.AssignToCurrency
        blank line
        two column distributed
            ReceiptBaseAmount.FunctionalAmount.EnteredCurrencyAmount
                label is "Functional"
                display as text
            Company.OrigCompany.AccountingEntity.FunctionalCurrency
                no label
                display as text
        single column
            ReceiptBaseAmount.FunctionalAmount.EnteredCurrencyRate
                no label
                display as text
        visible when (Company.OrigCompany.AccountingEntity.AlternateCurrencyExists)
            two column distributed
                ReceiptBaseAmount.AlternateAmount.EnteredCurrencyAmount
                    label is "Alternate"
                    display as text
                Company.OrigCompany.AccountingEntity.AlternateCurrency
                    no label
                    display as text
            single column
                ReceiptBaseAmount.AlternateAmount.EnteredCurrencyRate
                    no label
                    display as text
        visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency2Exists)
            two column distributed
                ReceiptBaseAmount.AlternateAmount2.EnteredCurrencyAmount
                    label is "Alternate2"
                    display as text
                Company.OrigCompany.AccountingEntity.AlternateCurrency2
                    no label
                    display as text
            single column
                ReceiptBaseAmount.AlternateAmount2.EnteredCurrencyRate
                    no label
                    display as text
        visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency3Exists)
