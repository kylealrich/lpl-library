MobilePurchaseOrderLineDashboard is a CompositeForm
    title is "PurchaseOrderLineDetails"
    context form is MobilePurchaseOrderLineDashboardContext
    implements DashBoardView
    Options is a Panel
        form is PurchaseOrderLineOptions
    Quantity is a Panel
        form is POLineQuantityDetailForm
    CostDetail is a Panel
        form is POLineCostForm
    ProcedureInformation is a Panel
        visible when (DisplayOfProcedureInfo)
        form is ProcedureInformationForm
    Messages is a Panel
        form is MessageReviewsForm
    SourceLines is a Panel
        list is PurchaseOrderLineSourcesRel.PurchaseOrderLineSourcesDisplay
    Invoices is a Panel
        list is PayablesInvoiceDetailsForPOLineRel.ListViewInvoiceDetailLines
    AddOnCharges is a Panel
        list is PurchaseOrderAndLineAddOnCharge set.AddOnCostsForAPurchaseOrderDisplay
    VendorReturns is a Panel
        list is VendorReturnLinesRel.LinesForPODashboard
