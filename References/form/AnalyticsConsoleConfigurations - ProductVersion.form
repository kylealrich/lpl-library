ProductVersion is a Form
    title is "ProductVersion"
    disable all actions
    Layout
        button of "GetVersion"
            action is UpdateProductVersion
        two column
            column1
                text of "Type"
                    header4
                text of "Active_\\Tenant_\\Content(ATC)"
                text of "Standard_\\Meta_\\Data(SMD)"
                text of "Standard_\\Tenant_\\Content(STC)"
            column2
                text of "Version"
                    header4
                last ProductVersionATCRel.Value
                    label is "<blank>"
                    raise red alert when (last AnalyticsConsoleConfigurationsRel.IsUpgradeCompleted)
                        mouse over text is "Update_\\Available"
                last ProductVersionSMDRel.Value
                    label is "<blank>"
                last ProductVersionSTCRel.Value
                    label is "<blank>"
