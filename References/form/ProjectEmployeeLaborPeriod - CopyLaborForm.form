CopyLaborForm is an ActionForm
    action is CopyLabor
    title is "CopyLabor"
    Layout
        two column
            Employee
                display as text
            Employee.Employee.Name.PreferredFirstAndLastName
                no label
                display as text
        two column
            ProjectSchedulePeriod
                display as text
            paragraph
                DateRange.Begin
                    no label
                    display as text
                DateRange.End
                    no label
                    display as text
        two column
            ToPeriod
                select is OpenPeriodList
            paragraph
                ToPeriod.DateRange.Begin
                    no label
                    display as text
                ToPeriod.DateRange.End
                    no label
                    display as text
        two column
            ThroughPeriod
                select is OpenPeriodList
            paragraph
                ThroughPeriod.DateRange.Begin
                    no label
                    display as text
                ThroughPeriod.DateRange.End
                    no label
                    display as text
        single column
            CopyFinanceStructure
            CopyPlannedToActual
