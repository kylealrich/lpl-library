RSSLPLProcurementTemplateLineDetail is a CompositeForm
    title is "ShoppingList<ProcurementTemplate>Line<DisplayOrder>Details"
    called out actions only
    context form is RSSLPLProcurementTemplateLineContext
        background is theme color
    Actions
        action is RSSLPLAddToRequisition
            action icon is add
            label is "AddToRequisition"
            mouse over text is "AddToRequisition"
            hide when invalid
            valid when (actor.agent(Employee).Employee.Requester.CurrentRequisitionForRQC entered)
        action is RSSLPLAddRequisition
            action icon is add
            label is "AddToRequisition"
            mouse over text is "AddToRequisition"
            hide when invalid
            valid when (actor.agent(Employee).Employee.Requester.CurrentRequisitionForRQC not entered)
        navigation is RequesterShoppingPageNav
            action icon is undo
            label is "BackToShoppingLists"
    LineDefaultDetails is a Panel
        title is "LineDefaultDetails"
        form is RSSLPLProcurementTemplateLineMain
