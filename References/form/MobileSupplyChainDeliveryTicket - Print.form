Print is an ActionForm
    action is Print
    title is "PrintTicket"
    Layout
        visible when (IsPrintDeliveryReport)
            PrmReportPrinter
                label is "ReportPrinter"
                select is MSCMReportCompanyPrinterSelect
        visible when (IsPrintDeliveryLabel)
            PrmLabelPrinter
                label is "LabelPrinter"
                select is MSCMLabelCompanyPrinterSelect
