EventLineResponseComposite is a CompositeForm
    title is "Event<SourcingEvent.RepresentativeText>,Line<SourcingEventLine>:_ResponseAndAwardInformation"
    context form is LineResponseContext
        background is theme color
    LineResponse is a Panel
        form is EventLineResponseInformation
    AOC is a Panel
        visible when (LineHasAddOnCharge)
        list is SourcingEventLineResponseAOCRel.SourcingEventLineResponseAOCs
    QuestionResponses is a Panel
        visible when (SELQuestionResponseExists)
        list is LineQuestions.LineQuestionsResponseDisplay
    AlternateResponses is a Panel
        visible when (AlternatesExist)
        form is AlternateResponses
