EFTCCExtractEntryForm is a CompositeForm
    is primary
    title is "ElectronicFundsTransfer_/CreditCardExtractEntry<Reference>"
    context form is EFTCCExtractEntryContext
        background is theme color
    Actions
        action is Release
            label is "Release"
            hide when invalid
        action is SubmitForApproval
            label is "SubmitForApproval"
            hide when invalid
        action is CancelUnreleasedEFT
            label is "Cancel"
            hide when invalid
        action is CancelReleasedEFT
            label is "Cancel"
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is save
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    Main is a Panel
        form is MainForm
    CustomerAddress is a Panel
        form is CustomerAddress
    BankInformation is a Panel
        form is BankInformation
    TransactionData is a Panel
        form is TransactionData
    CurrencyAmounts is a Panel
        title is "CurrencyAmounts"
        form is CurrencyAmountsForm
    UserFieldsPanel is a Panel
        title is "UserFields"
        form is UserFieldForm
    DistributionList is a Panel
        title is "Distributions"
        list is CashLedgerGLDistributionsAllRel.EFTCCExtractEntrys
    DistributionsNotImported is a Panel
        visible when (ImportInProcess)
        list is CashLedgerReceiptImportInErrorRel.primary
    ApprovalComments is a Panel
        visible when (HasApprovalComments)
        list is CashLedgerEFTExtractReasonCodeUsage set.CashLedgerEFTExtractReasonCodeUsageList
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
    BudgetErrors is a Panel
        title is "BudgetErrors<DerivedHeaderBudgetErrorCount>"
        visible when (HeaderBudgetEditErrorExists)
        list is HeaderBudgetEditErrorRel.primary
    WarningBudgetErrors is a Panel
        title is "WarningBudgetErrors<DerivedWarningBudgetErrorCount>"
        visible when (DistributionBudgetEditErrorExists)
        list is DistributionBudgetEditErrorRel.primary
    Errors is a Panel
        title is "Errors<DerivedElectronicFundTransferErrorCount>"
        visible when (ElectronicFundTransferErrorsExist)
        list is CashLedgerTransactionError set.CashLedgerElectronicFundsTransferTransactionErrorList
