CreateDepositDetailContext is a Form
    Layout
        two column distributed
            column1
                display as text
                    paragraph
                        CashLedgerFundTransferHeader.CashCode.CashCodeAndDescription
                            label is "FromCashCode"
                        CashLedgerFundTransferHeader.CashCode.Currency
                    paragraph
                        CashLedgerFundTransferHeader.Company.OrigCompany.RepresentativeText
                            label is "Company"
                        CashLedgerFundTransferHeader.Company.OrigCompany.Currency
                            no label
                    paragraph
                        CashLedgerFundTransferHeader.TransferBankAmount
                            label is "Amount"
                        CashLedgerFundTransferHeader.CashCode.Currency
                            no label
            column2
                display as text
                    paragraph
                        CashLedgerFundTransferHeader
                            label is "TransferNumber"
                        CashLedgerFundTransferHeader.Status
                    paragraph
                        CashLedgerFundTransferHeader.CurrencyTable
                        CashLedgerFundTransferHeader.CurrencyTable.Description
                            no label
                    paragraph
                        visible when (CashLedgerFundTransferHeader.IsUnreleased)
                            CashLedgerFundTransferHeader.AppliedBankAmount
                                label is "AppliedBankAmount"
                        visible when (CashLedgerFundTransferHeader.IsUnreleased)
                            CashLedgerFundTransferHeader.BalanceAmount
                                label is "Balance"
                        visible when (CashLedgerFundTransferHeader.IsUnreleased)
                            CashLedgerFundTransferHeader.AppliedBaseAmount
                                label is "AppliedBaseAmount"
                    visible when (ShowDetailAccountingUnit)
                        paragraph
                            CashLedgerFundTransferHeader.HeaderAccountingUnit
                    visible when (ShowDetailFund)
                        paragraph
                            CashLedgerFundTransferHeader.HeaderFund
                            CashLedgerFundTransferHeader.HeaderFund.Description
                                no label
