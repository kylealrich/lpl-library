MobileDistributionDetailContextForm is a Form
    Layout
        header2 of "StatementInformation"
            collapsible
                initially collapsed
            display as text
                single column
                    ProcurementGroup.DerivedProcurementGroupName
                        label is "ProcurementGroup"
                    ProcurementCardStatementTransaction.Company.DerivedCompanyIDName
                        label is "CompanyName"
                    DerivedCardProgramNameDescription
                        label is "ProgramName"
                    DerivedCardTransaction
                        label is "Statement"
                    DerivedCardName
                        label is "CardNumber"
                    ProcurementCardStatement.ShipToPostalCode
                        label is "ShipToPostalCode"
                    ProcurementCardStatement.ShipFromPostalCode
                        label is "ShipFromPostalCode"
                    ProcurementCardStatementTransaction.TransactionDate
                        label is "TransactionDate"
                    ProcurementCardStatementTransaction.TaxAmount
                        label is "TaxAmount"
                    ProcurementCardStatementTransaction.UnitCost
                        label is "UnitCost"
                    ProcurementCardStatement.VatReferenceNumber
                        label is "V\\A\\TReferenceNumber"
                    ProcurementCardStatement.VatTaxRate
                        label is "V\\A\\TTaxRate"
                    ProcurementCardStatement.VatTaxAmount
                        label is "V\\A\\TTaxAmount"
                    DerivedMerchantName
                        label is "MerchantName"
        header2 of "DistributionInformation"
            collapsible
                initially expanded
            single column
                ProcurementChargeDistributionType
                DistributionAccount
                    label is "DistributionAccount"
                    compact format
                DistributionAmount
                AssetFlag
                AssetDescription
                TagNumber
                Item
                    display as text
                ItemDescription
                    no label
                    display as text
                ItemQuantity
            visible when (ProcurementCardStatement.FrontEndSplitsEnabled)
                CreatedByFES
                FESManuallyUpdated
                InServiceDate
                PurchaseDate
                ModelNumber
                SerialNumber
                AssetTemplate
                AssetTemplate.Description
                    display as text
                HoldAsset
                Asset
                Asset.Description
                    display as text
                AssetGroup
                AssetGroup.Description
                    display as text
                Combine
                BarCode
