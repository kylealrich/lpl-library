MobileTransactionSummaryDashboard is a CompositeForm
    title is "TransactionSummary"
    context form is MobileTransactionSummaryContext
    Actions
        action is save
            label is "_"
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is refresh
            label is "_"
    RequisitionLines is a Panel
        visible when (HasTransactionRequisitionLines)
        list is RequisitionLineTransactionActivityRel.MobileRecallRequisitionLines
    PurchaseOrderLines is a Panel
        visible when (HasTransactionPurchaseOrderLines)
        list is PurchaseOrderLineTransactionActivityRel.MobileRecallPurchaseOrderLines
    RequisitionPurchaseOrderLines is a Panel
        visible when (HasTransactionPolinesrc)
        list is PolinesrcTransactionActivityRel.MobileRecallPolinesrc
    Inventory is a Panel
        visible when (HasTransactionInventory)
        list is InventoryTransactionActivityRel.MobileRecallItemLocations
    ParLocations is a Panel
        visible when (HasTransactionParLocations)
        list is ParLocationTransactionActivityRel.MobileRecallParLocations
    StockOnHandLotNumbers is a Panel
        visible when (HasTransactionLotNumbers)
        list is LotNumberTransactionActivityRel.MobileRecallStockOnHandDetail
    StockOnHandSerialNumbers is a Panel
        visible when (HasTransactionSerialNumbers)
        list is SerialNumberTransactionActivityRel.MobileRecallItserial
    Issues is a Panel
        visible when (HasTransactionIssues)
        list is RecentIssueTransactionActivityRel.MobileRecallIssues
    RecallIssuesDetail is a Panel
        visible when (HasTransactionIssueDetail)
        list is IssueDetailTransactionActivityRel.MobileRecallIssuesDetail
    IncompleteTransfers is a Panel
        visible when (HasTransactionIntransitTransfers)
        list is IntransitTransferTransactionActivityRel.MobileRecallIntransitTransfers
    ContractLines is a Panel
        visible when (HasTransactionContractLines)
        list is ContractLineTransactionActivityRel.MobileRecallContractLines
    ProcurementTemplateLines is a Panel
        visible when (HasTransactionProcurementTemplateLines)
        list is ProcurementTemplateLineTransactionActivityRel.MobileRecallProcurementTemplateLines
    SourcingEventLines is a Panel
        visible when (HasTransactionSourcingEventLines)
        list is SourcingEventLineTransactionActivityRel.MobileRecallSourcingEventLines
    Equipment is a Panel
        visible when (HasTransactionEquipment)
        list is EquipmentTransactionActivityRel.MobileRecallAssetItems
