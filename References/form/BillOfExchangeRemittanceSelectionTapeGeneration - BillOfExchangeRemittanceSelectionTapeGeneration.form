BillOfExchangeRemittanceSelectionTapeGeneration is an ActionForm
    action is BillOfExchangeRemittanceSelectionTapeGeneration
    Layout
        paragraph
            Company
                select is ARCompanies
            Company.Name
                no label
                display as text
        paragraph
            ProcessLevel
                select is ProcessLevels
            ProcessLevel.Name
                no label
                display as text
        two column distributed
            column1
                paragraph
                    CashCode
                    CashCode.Description
                        no label
                        display as text
            column2
                paragraph
                    PayCode
                        label is "PaymentCode"
                        select is AllBankTransactionCodes
                    PayCode.Description
                        no label
                        display as text
        two column distributed
            column1
                FrMatureDate
                    label is "FromMaturityDate"
            column2
                ToMatureDate
                    label is "ToMaturityDate"
        paragraph
            RemitType
                label is "RemittanceType"
        two column distributed
            column1
                FinancialInstitution
            column2
                FinancialInstitutionBranch
        paragraph
            CreditLine
        blank line
        paragraph
            UpdateOption
        paragraph
            BankFileOption
                label is "GenerateBankFile"
        paragraph
            PresentOption
                label is "RemittancePresentation"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                BillOfExchangeRemittanceSelectionTapeGenerationReport_RDG
            column2 of 25%
                BillOfExchangeRemittanceSelectionTapeGenerationReport_XPT
