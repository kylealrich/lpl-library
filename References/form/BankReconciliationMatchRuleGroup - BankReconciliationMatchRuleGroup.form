BankReconciliationMatchRuleGroup is a CompositeForm
    is primary
    title is "ReconciliationMatchRuleGroup"
    context form is BankReconciliationMatchRuleGroupContext
        background is theme color
    ReconciliationMatchRules is a Panel
        list is BankReconciliationMatchRuleGroupDetail set.BankReconciliationMatchRuleGroupDetails
            helper list is BankReconciliationMatchRuleRel.BankReconciliationMatchRulesHelper
                action is AddMatchRule
                    invoked.PrmCashManagementGroup = CashManagementGroup
                    invoked.PrmBankReconciliationMatchRuleGroup = BankReconciliationMatchRuleGroup
