InterfaceLeasePaymentInvoicesToPayables is an ActionForm
    action is InterfaceLeasePaymentInvoicesToPayables
    Layout
        single column
            LeaseCompanyGroup
                no label
                display as horizontal radio buttons
            visible when (LeaseCompanyGroup.Lease)
                paragraph
                    Company
                        select is PayablesCompanyList
                    Company.Name
                        no label
                        display as text
                paragraph
                    Lease
                        select is AllLeases
                    Lease.Description
                        no label
                        display as text
            visible when (LeaseCompanyGroup.CompanyGroup)
                paragraph
                    CompanyGroup
                        label is "CompanyGroup"
                        select is CompanyGroupsInFinanceEnterpriseGroup
                    CompanyGroup.Description
                        no label
                        display as text
        paragraph
            LeaseClass
                label is "LeaseClassification"
        paragraph
            LeaseType
                select is LeaseTypes
            LeaseType.Description
                no label
                display as text
        blank line
        paragraph
            ThruInvDate
                label is "ThroughInvoiceDate"
        blank line
        paragraph
            UpdateOption
                label is "Update"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                InterfaceLeasePaymentInvoicesToPayablesReport_RDG
            column2 of 25%
                InterfaceLeasePaymentInvoicesToPayablesReport_XPT
