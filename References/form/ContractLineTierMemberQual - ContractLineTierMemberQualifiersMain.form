ContractLineTierMemberQualifiersMain is a Form
    is primary
    title is "ContractLineTierMemberQualifiers"
    Layout
        display as text
            four column distributed
                column1
                    paragraph
                        Contract.RepresentativeText
                        ContractLine
                            label is "Line"
                    paragraph
                        ContractLine.ItemNumber
                            label is "Item"
                        ContractLine.ItemDescription
                            label is "Description"
                        ContractLine.UOM.UnitOfMeasure
                            label is "UOM"
                    visible when (Contract.ManufacturerOrDistributor.DistributorContract)
                        visible when (ContractLineMember.MfgContractTierInfo.Tier entered)
                            paragraph
                                ContractLineMember.MfgContractTierInfo.MfgContract.RepresentativeText
                                    label is "ManufacturerContract"
                                visible when (ContractLineMember.MfgContractTierInfo.MfgContract.WorkingContractUnique)
                                    ContractLineMember.MfgContractTierInfo.MfgContract.WorkingContractID
                column2
                    Contract.Supplier.RepresentativeText
                        label is "Supplier"
                    visible when (ContractLine.VendorItem entered)
                        ContractLine.VendorItem
                column3
                    Contract.WorkingContractID
                column4
                    visible when (ContractLineMember.CompanyEntered)
                        ContractLineMember.Company.RepresentativeText
                            label is "Company"
                    visible when (ContractLineMember.PricingGroupEntered)
                        paragraph
                            ContractLineMember.PricingGroup.RepresentativeText
                                label is "PricingGroup"
                            visible when (ContractLineMember.PricingGroupInactive)
                                text of "PricingGroupIsInactive"
                                    color of red
                    visible when (ContractLineMember.LocationEntered)
                        paragraph
                            ContractLineMember.Location.RepresentativeText
                                label is "Location"
                            visible when (ContractLineMember.LocationInactive)
                                text of "LocationIsInactive"
                                    color of red
                    visible when (ContractLineMember.ReqLocEntered)
                        paragraph
                            ContractLineMember.RequestingLocation.RequestingLocationNameID
                                label is "RequestingLocation"
                            visible when (ContractLineMember.RequestingLocationInactive)
                                text of "RequestingLocationIsInactive"
                                    color of red
        header2 of "_"
        paragraph
            Tier
            Tier.TierTitle
                no label
                display as text
        single column
            OrGroup
                label is "Group"
        three column
            PurchaseType
            PurchaseBy
            PurchaseOperator
        two column
            column1
                PurchasePercentOrPurchaseAmount
                    display as horizontal radio buttons
            column2
                visible when (PurchasePercentOrPurchaseAmount.PurchasePercent)
                    PurchaseAmtPct.PurchasePercent
                        label is "PurchasePercent_ofAllPurchases"
                visible when (PurchasePercentOrPurchaseAmount.PurchaseAmount)
                    PurchaseAmtPct.PurchaseAmount
                        label is "PurchaseAmount"
        two column
            TargetedPurchasesFromDate
                label is "TargetedPurchasesFromDate"
            TargetedPurchasesToDate
                label is "TargetedPurchasesToDate"
        two column
            PurchaseQuantity
                label is "Quantity"
            PurchaseUomQualifier
                label is "UOM"
        two column
            PurchaseBeginRange
                label is "BeginRange"
            PurchaseEndRange
                label is "EndRange"
        single column
            Modified
                display as text
