ExpenseManagementInterfaceAllocationForm is a Form
    is primary
    Layout
        header2 of "ExpenseManagementInterfaceDetail"
        paragraph
            ExpenseManagementInterfaceHeader.Document
                label is "ExpenseReport"
                display as text
            ExpenseManagementInterfaceHeader.Description
                display as text
            ExpenseManagementInterfaceDetail
                label is "SequenceNumber"
                display as text
        paragraph
            ExpenseManagementInterfaceDetail.PaymentMethod
                display as text
            ExpenseManagementInterfaceDetail.ExpenseDate
                display as text
        paragraph
            ExpenseManagementInterfaceDetail.AccountingEntity
                display as text
            ExpenseManagementInterfaceDetail.AccountingEntity.Name
                no label
                display as text
            ExpenseManagementInterfaceDetail.ExpenseManagementInterfaceExpenseType
                label is "ExpenseType"
                display as text
            ExpenseManagementInterfaceDetail.ExpenseManagementInterfaceExpenseType.Description
                no label
                display as text
        paragraph
            ExpenseManagementInterfaceDetail.ReimbursementAmount
                display as text
            ExpenseManagementInterfaceDetail.ReimbursementCurrency
                no label
        header2 of "ExpenseManagementInterfaceAllocation"
        single column
            Percentage
            FinanceStructure
                compact format
            Billable
