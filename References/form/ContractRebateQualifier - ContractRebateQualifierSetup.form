ContractRebateQualifierSetup is a Form
    title is "<mode>ContractRebateQualifier"
    Layout
        single column
            OrGroup
                label is "Group"
        paragraph
            PurchaseType
        single column
            PurchaseOperator
        visible when (not Contract.ServicePurchaseType)
            paragraph
                RebatePurchaseAmtPct.PurchasePercent
                    label is "PurchasePercentOfAllPurchases"
                RebatePurchaseAmtPct.PurchaseAmount
                    label is "\\orPurchaseAmount"
                RebatePurchaseAmtPct.PurchaseUnits
                    label is "\\orPurchaseUnits"
        visible when (Contract.ServicePurchaseType)
            paragraph
                RebatePurchaseAmtPct.PurchaseAmount
                    label is "PurchaseAmount"
        paragraph
            TargetedPurchasesFromDate
                label is "TargetedPurchasesFrom"
            TargetedPurchasesToDate
                label is "Through"
        paragraph
            AllInvoicesForSupplier
        visible when (ContractRebate.CalculateRebateAmountOnAllContractSpend)
            paragraph
                CalculateRebateAmountOnAllContractSpend
                    no label
                    display as text
        header2 of "EstimatedRebateAmounts"
        paragraph
            PriorYearPurchaseAmount
            DerivedRebatePercent
                label is "_X_"
                display as text
            ProjectedRebateAmount
                label is "_=_Projected_Rebate"
