PurchaseOrderInterfaceResultComposite is a CompositeForm
    is primary
    title is "PurchaseOrderInterfaceResult"
    context form is PurchaseOrderInterfaceResultContext
        background is theme color
    Actions
        action is RerunForErrors
            hide when invalid
        action is Delete
            action icon is delete
            label is "_"
    ProcessingCounts is a Panel
        form is ProcessingCountsForm
    PurchaseOrdersCreatedUpdatedOrCanceled is a Panel
        visible when (not NoRecordsToProcess)
        list is PurchaseOrdersRel.InterfaceListing
    PendingPurchaseOrdersForRunGroup is a Panel
        visible when (ShowRunGroupPanel)
        list is RunGroupProcessedPurchaseOrderImportRel.PurchaseOrderImportListForResults
    PendingPurchaseOrdersForErrorRunGroup is a Panel
        visible when (ShowErrorRunGroupPanel)
        list is ErrorRunGroupPurchaseOrderImportRel.PurchaseOrderImportListForResults
    PendingPurchaseOrderLinesForRunGroup is a Panel
        visible when (ShowRunGroupPanelForLines)
        list is RunGroupPurchaseOrderImportStandaloneLineRel.StandalonePurchaseOrderImportLinesForResults
    PendingPurchaseOrderLinesForErrorRunGroup is a Panel
        visible when (ShowErrorRunGroupPanelForLines)
        list is ErrorRunGroupPurchaseOrderImportStandaloneLineRel.StandalonePurchaseOrderImportLinesForResults
    PendingPurchaseOrderAddOnChargesForRunGroup is a Panel
        visible when (ShowRunGroupPanelForAddOnCharges)
        list is RunGroupPurchaseOrderImportStandaloneAddOnChargeRel.StandalonePurchaseOrderImportAddOnChargesForResults
    PendingPurchaseOrderAddOnChargesForErrorRunGroup is a Panel
        visible when (ShowErrorRunGroupPanelForAddOnCharges)
        list is ErrorRunGroupPurchaseOrderImportStandaloneAddOnChargeRel.StandalonePurchaseOrderImportAddOnChargesForResults
    Parameters is a Panel
        form is ParameterForm
