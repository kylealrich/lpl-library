AdvancedSearchForm is a Form
    title is "Term_andConditionSearch"
    Layout
        four column distributed
            ContractGroup
            Contract.Supplier
                label is "Supplier"
            Contract.Supplier.Vendor
            Contract
        four column distributed
            Contract.Name
                label is "ContractName"
            Contract.WorkingContractType
            Contract.WorkingContractID
        four column distributed
            ContractArticle.HeaderText
                label is "ArticleHeaderText"
            HeaderText
                label is "TermHeaderText"
        two column
            Description
            ActiveApprovalRequestOnInProgressContractExists
                label is "InApprovalProcess"
