MobileCashLedgerTransactionContext is a Form
    title is "CashLedgerTransactionContext"
    Layout
        two column distributed
            Company.OrigCompany
                display as text
                select is Company
            Company.OrigCompany.Name
                no label
                display as text
        two column distributed
            BankTransactionCode
                label is "TransactionCode"
                display as text
            BankTransactionCode.Description
                display as text
        two column distributed
            CurrencyTable
            CurrencyTable.Description
                no label
                display as text
        two column distributed
            TransactionNumber
            Status
                label is "Status"
                display as text
        two column distributed
            Company.OrigCompany.Currency
                label is "Currency"
                display as text
            CashLedgerTransactionIdentifier
                display as text
        two column distributed
            paragraph
                CashCode
                CashCode.Description
                    no label
                    display as text
                CashCode.Currency
                    no label
                    display as text
        two column distributed
            paragraph
                visible when (IsUnreleasedImportedTran)
                    ImportStatus
                        display as text
                visible when (IsUnreleasedImportedTranWithMessage)
                    ImportErrorMessage
                        label is "ImportError"
                        display as text
                visible when (Status.PendingApproval)
                    ApprovalRequestType
                        label is "ApprovalType"
                        display as text
                visible when (IsUnreleasedCashLedgerReconciliationAdjustmentExist)
                    text of "UnreleasedAdjustmentExists"
            visible when (LastElectronicPaymentSerialNumber entered)
                LastElectronicPaymentSerialNumber
                    label is "ElectronicPaymentSerialNumber"
                    display as text
        two column distributed
            visible when (IsSourceCodeAP)
                IssuedBankAmount
                    label is "Amount"
            visible when (not IsSourceCodeAP)
                IssuedBankAmountUnsigned
                    label is "Amount"
            ReconciliationData.CurrencyAmount
                label is "ReconciledAmount"
                display as text
        paragraph
            visible when (VarianceExists)
                DerivedBankDifferenceAmount
                    label is "VarianceAmount"
                    display as text
            visible when (CanSeeDistributionTotals)
                AppliedDistributionBankAmount
                    label is "AppliedAmount"
                    display as text
                BalanceAmount
                    label is "Balance"
                    display as text
        paragraph
            visible when (ReconciledToBankStatement)
                ReconcilingBankStatement
                    display as text
                MatchNumber
                    display as text
                MatchField
                    display as text
                button of "ViewStatement"
                    link is BankStatementForm
        blank line
        paragraph
            visible when (BudgetEditInProgress)
                text of "BudgetEditsInProgress"
                    bold
                    color of red
            visible when (BudgetEditFailure)
                text of "BudgetsExceeded;CouldNotRelease;SeeBudgetErrorsTab"
                    bold
                    color of red
