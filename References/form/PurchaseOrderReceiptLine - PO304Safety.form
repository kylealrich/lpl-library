PO304Safety is a Form
    title is "Safety"
    Layout
        header3 of "MSDS"
        paragraph
            MSDSRequired
                label is "M\\S\\D\\S_\\Received"
        paragraph
            MSDSDocument
                label is "Document"
        paragraph
            MSDSVersion
                label is "Version"
        visible when (ItemHazardCodeEntered)
            header3 of "Hazard"
            paragraph
                ItemHazardCode
                    no label
                    display as text
            paragraph
                ItemHazardDescription
                    no label
                    display as text
