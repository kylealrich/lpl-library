Main is a Form
    Layout
        three column distributed
            column1
                paragraph
                    BillingInvoiceType
                        label is "InvoiceType"
                    BillingInvoiceType.Description
                        no label
                        display as text
            column2
                paragraph
                    BillingProcessLevel
                        label is "ProcessLevel"
                        select is ProcessLevels
                    BillingProcessLevel.Name
                        no label
                        display as text
            column3
                paragraph
                    Location
                    Location.Name
                        no label
                        display as text
        header2 of "Billing"
        three column distributed
            column1
                paragraph
                    TermCode
                    TermCode.Description.Desc[1]
                        no label
                        display as text
                    TermCode.Description.Desc[2]
                        no label
                        display as text
            column2
                paragraph
                    Currency
                    Currency.Description
                        no label
                        display as text
        paragraph
            ListName
                label is "PriceList"
                compact format
            ListName.Description
                label is "_"
                display as text
        header2 of "Payment"
        three column distributed
            column1
                TaxExemptCode
            column2
                paragraph
                    TaxCode
                    TaxCode.Description
                        no label
                        display as text
        header2 of "FrequencyControl"
        three column distributed
            column1
                Frequency
                CycleCode
                NumberOfInvoices
            column2
                BillMonth
                BillDay
                BillDayOfWeek
            column3
                StartDate
                StopDate
                CurrentBillDate
                    display as text
        header2 of "SalesData"
        three column distributed
            column1
                paragraph
                    Territory
                    Territory.Description
                        no label
                        display as text
                CommissionSplitPercentage
                    label is "CommissionSplit"
            column2
                paragraph
                    SalesRepresentative
                        label is "SalesRepresentative1"
                    SalesRepresentative.Name
                        no label
                        display as text
                CommissionRate1
                    label is "Commission"
            column3
                paragraph
                    SalesRepresentative2
                        label is "SalesRepresentative2"
                    SalesRepresentative2.Name
                        no label
                        display as text
                CommissionRate2
                    label is "Commission"
