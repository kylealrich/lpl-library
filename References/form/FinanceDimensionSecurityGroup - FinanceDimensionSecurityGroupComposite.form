FinanceDimensionSecurityGroupComposite is a CompositeForm
    is primary
    title is "<DimensionGroupType>SecurityGroup"
    context form is FinanceDimensionSecurityGroupForm
    Members is a Panel
        visible when (MemberDimension)
        list is FinanceDimensionSecurityGroupMember set.primary
    AccessibleDimensions is a Panel
        visible when (MemberDimension)
        list is FinanceDimensionSecurityGroupDetail set.primary
    AccessibleAccountingEntities is a Panel
        visible when (DimensionGroupType.AccountingEntity)
        list is AccountingEntityGroup.AccountingEntityGroupMember set.SecurityGroupEntities
    AccessibleCompanies is a Panel
        visible when (DimensionGroupType.Company)
        list is GeneralLedgerCompanyGroup.GeneralLedgerCompanyGroupMember set.SecurityGroupCompanies
    AccessibleCashCodes is a Panel
        visible when (DimensionGroupType.CashCode)
        list is CashCodeGroup.CashCodeGroupDetail set.SecurityGroupCashCodes
    AccessibleVendorClasses is a Panel
        visible when (DimensionGroupType.VendorClass)
        list is VendorClassGroup.VendorClassGroupDetail set.SecurityGroupVendorClasses
    AccessibleLedgers is a Panel
        visible when (DimensionGroupType.Ledger)
        list is LedgerBasisGroup.LedgerBasisGroupMember set.SecurityGroupLedgers
    ActorsAssignedToThisGroup is a Panel
        list is ActorContextRel.primary
