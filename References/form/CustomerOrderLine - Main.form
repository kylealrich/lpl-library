Main is a Form
    restrict action CreateCustomerOrderReturnLine
    Layout
        two column distributed
            column1
                paragraph
                    CustomerOrderLine
                        label is "Line"
                        display as text
                    CustomerItemEntered
                        label is "CustomerItemNumber"
                visible when (not CustomerItemEntered)
                    Item
                        show up to 25 characters
                        display as required
                visible when (CustomerItemEntered)
                    CustomerItem
                        show up to 25 characters
                        display as required
                ItemDescription
                    label is "Description"
                LineType
                    label is "ItemType"
                visible when (Status.Released)
                    paragraph
                        CancelQuantity
                        CancelledDate
                            label is "CancelDate"
                    paragraph
                        CancelledOperator
                            label is "CanceledBy"
                            show up to 10 characters
                        CancelledReason
                            label is "CancelReason"
            column2
                text of "_"
                paragraph
                    OrderQuantity
                        label is "Quantity"
                    SellUOM
                        label is "UOM"
                        select is ValidSellUnitsOfMeasure
                paragraph
                    EnteredPrice
                        label is "Price"
                        show up to 10 characters
                    SellingPriceUOM
                        select is ValidSellPriceUnitsOfMeasure
        two column
            column1
                header2 of "Processing"
                paragraph
                    ExpectedShipDate
                        label is "ExpectedShipDate"
                    EarlyDeliveryDate
                        label is "RequestedDate"
                paragraph
                    visible when (DisplayMaterialSafetyDataSheet)
                        MaterialSafetyDataSheet
                    visible when (not CustomerOrder.OrderEntryOrderType.ShipImmediate)
                        FutureOrder
                paragraph
                    HoldCode
                    HoldCode.Description
                        no label
                        display as text
                paragraph
                    ShipComplete
                        label is "ShipComplete"
                    PickingPriority
                        label is "Priority"
            column2
                header2 of "Shipping"
                paragraph
                    CustomerShipTo
                        label is "ShipTo"
                    CustomerShipTo.Name
                        no label
                        display as text
                paragraph
                    Carrier
                    Carrier.VendorName
                        no label
                        display as text
                paragraph
                    RouteHeader.Route
                        label is "Route"
                        display as text
                paragraph
                    RouteStop
                        label is "Stop"
                        display as text
        two column
            column1
                header2 of "Billing"
                paragraph
                    NoCharge
                        label is "NoCharge"
                    OrderDiscount
                        label is "LineDiscountable"
                paragraph
                    EnteredDiscount
                        label is "AdditionalDiscount"
                paragraph
                    TaxStatus
                    OverrideTaxable
                        label is "TaxableAmount"
                        display as text
                visible when (TaxStatus.Taxable)
                    paragraph
                        TaxCode
                            display as required
                ProductTaxCategory
                visible when (HasPointOfTitleTransfer)
                    paragraph
                        PointOfTitleTransfer
                            display as text
                visible when (TaxOutputDueAccount)
                    paragraph
                        CompanyTaxCodeRel.TaxOutputDueAccount
                            label is "TaxAccount"
                            display only
                            compact format
                visible when (TaxOutputAccount)
                    paragraph
                        CompanyTaxCodeRel.TaxOutputAccount
                            label is "TaxAccount"
                            display only
                            compact format
                visible when (IsHSNSACCodeEnabled)
                    paragraph
                        HSNSACCode
                        HSNSACCode.Description
                            no label
                            display as text
                paragraph
                    UNSPSCCode
                        compact format
            column2
                header2 of "PurchaseOrderInformation"
                paragraph
                    CreatePurchaseOrder
                paragraph
                    Dropship
                paragraph
                    Vendor
                paragraph
                    PurchaseFromLocation
                        label is "PurchaseFrom"
                    PurchaseFromLocation.VendorLocation.VendorName
                        no label
                        display as text
                paragraph
                    POCode
                        label is "PurchaseOrderCode"
                    POCode.Description
                        no label
                        display as text
        header2 of "Accounts"
        paragraph
            SalesAccount
                compact format
        paragraph
            ExpenseAccount
                compact format
        paragraph
            CostOfGoodsSoldAccount
                compact format
        paragraph
            UnitCost
        two column
            column1
                header2 of "SalesRepresentative"
                paragraph
                    Territory
                    Territory.Description
                        no label
                        display as text
                paragraph
                    SalesRepresentative
                        label is "SalesRepresentative1"
                    visible when (SalesRepresentative entered)
                        CommissionRate1
                            label is "Commission"
                blank line
                paragraph
                    SalesRepresentative2
                        label is "SalesRepresentative2"
                    visible when (SalesRepresentative2 entered)
                        CommissionRate2
                            label is "Commission"
                paragraph
                    CommissionSplitPercentage
                        label is "SplitPercent"
            column2
                header2 of "Global"
                paragraph
                    ShipTerm
                    ShipTerm.Description
                        no label
                        display as text
                paragraph
                    TransientIntrastatPortCode
                        select is IntrastatPortCodeForInventoryLocationRel.IntrastatPortCodeList
                paragraph
                    NatureOfTransactionCode
                        label is "No\\T\\C"
                paragraph
                    StatisticalProcedure
                paragraph
                    SupplementaryQuantity
                    SupplementaryUOM
                        no label
                paragraph
                    ICNCode
                        show up to 10 characters
                    ICNCode.Description
                        no label
                        display as text
                paragraph
                    GlobalLineType
