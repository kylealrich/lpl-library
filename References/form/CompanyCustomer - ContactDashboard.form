ContactDashboard is a Form
    title is "<CompanyCustomerPrimaryFormTitle>_-Contacts"
    disable all actions
    Layout
        display as text
            blank line
            two column distributed
                column1
                    paragraph
                        Contact
                    paragraph
                        PhoneNumber
                    paragraph
                        MobileNumber.MobilePhoneNumber.InternationalPrefix
                            label is "MobileCountry/\\JurisdictionCode"
                        MobileNumber.MobilePhoneNumber.SubscriberNumber
                            label is "MobileNumber"
                        MobileNumber.SmsEnabled
                            label is "SmsEnabled"
                    paragraph
                        FaxNumber.InternationalPrefix
                            label is "FaxCountry/\\JurisdictionCode"
                        FaxNumber.SubscriberNumber
                            label is "FaxNumber"
                        FaxNumber.Extension
                            label is "Extension"
                    paragraph
                        EmailAddress
                            label is "E\\-mail"
                    paragraph
                        URLAddress
                            label is "U\\R\\L"
                column2
                    TwitterID
                    SocialNetworkID2
                    SocialNetworkID3
                    SocialNetworkID4
                    SocialNetworkID5
            blank line
        paragraph
            button of "Contacts"
                list is CompanyCustomerContactRel.CustomerContacts
