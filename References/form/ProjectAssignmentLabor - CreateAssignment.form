CreateAssignment is an ActionForm
    action is CreateAssignment
    title is "New<FinanceEnterpriseGroup.ProjectLabel>Assignment"
    Layout
        two column
            column1
                Employee
                    display as text
                Employee.ProjectLaborSchedule
                    display as text
            column2
                Employee.Employee.Name.PreferredFirstAndLastName
                    no label
                    display as text
                Employee.ProjectLaborSchedule.Description
                    no label
                    display as text
        two column
            PrmProject
                label is "<FinanceEnterpriseGroup.ProjectLabel>"
                select is AssignmentProjectSelect
            PrmProject.Description
                no label
                display as text
        two column
            paragraph
                PrmProject.ProjectDateRange.BeginDate
                    label is "<FinanceEnterpriseGroup.ProjectLabel>BeginDate"
                    display as text
                PrmProject.ProjectDateRange.EndDate
                    label is "<FinanceEnterpriseGroup.ProjectLabel>EndDate"
                    display as text
        two column
            PrmProjectRole
                label is "Role"
            PrmProjectRole.Description
                display as text
        two column
            paragraph
                PrmProjectDateRange.BeginDate
                    label is "BeginDate"
                PrmProjectDateRange.EndDate
                    label is "EndDate"
        two column
            PrmCommittedEffort
                label is "CommittedEffort"
            PrmBillableRate
                label is "BillableRate"
        single column
            PrmFinanceCodeBlock
                label is "LaborDistributionFinanceStructure"
                compact format
            text of "DimensionsSetToBlank:"
                align as label
            PrmFinanceCodeBlockSetBlanks
                no label
        three column
            column1
                PrmBurdenCode
                    label is "FringeBurdenCode"
            column2
                PrmBurdenCode.Description
                    no label
                    display as text
            column3
                PrmBurdenCode.Rate
                    display as text
        single column
            PrmBurdenCodeBlock
                label is "FringeBurdenFinanceStructure"
                compact format
            text of "DimensionsSetToBlank:"
                align as label
            PrmBurdenCodeBlockSetBlanks
                no label
