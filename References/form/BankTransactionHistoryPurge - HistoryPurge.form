HistoryPurge is an ActionForm
    action is BankTransactionHistoryPurge
    Layout
        header2 of "MAIN"
        single column
            CashManagementGroup
        three column
            CbtCashCode
                label is "CashCode"
            CbtCashCode.Description
                no label
                display as text
            CbtCashCode.Currency
                no label
        two column
            CcgCashCodeGrp
                label is "CashCodeGroup"
                select is CashCodeGroup
            CcgCashCodeGrp.Description
                no label
                display as text
        blank line
        two column
            ArchiveCbtrans
                label is "ArchiveBankTransactions"
            CbtCutoffDate
                label is "CutoffDate"
        single column
            CbtBankInstType
                label is "TransactionType"
        two column
            visible when (CbtBankInstType not entered)
                CbtBankInstCode
                    label is "TransactionCode"
                    select is BankTransactionCodeList
            visible when (CbtBankInstType.CashPayments)
                CbtBankInstCode
                    label is "TransactionCode"
                    select is CashPayments
            visible when (CbtBankInstType.BoePayments)
                CbtBankInstCode
                    label is "TransactionCode"
                    select is BOEPayments
            visible when (CbtBankInstType.BankServices)
                CbtBankInstCode
                    label is "TransactionCode"
                    select is BankServices
            CbtBankInstCode.Description
                no label
                display as text
        two column
            ArchiveCbtrfhdr
                label is "ArchiveBankFundTransfers"
            CbhCutoffDate
                label is "CutoffDate"
        two column
            ArchiveCbrecpthdr
                label is "ArchiveCashReceiptDeposits"
            CbpCutoffDate
                label is "CutoffDate"
        two column
            ArchiveCbbalance
                label is "ArchiveBankAccountBalance"
            CbbCutoffDate
                label is "CutoffDate"
        two column
            ArchiveCbpayment
                label is "ArchiveCashLedgerPayments"
            CbmCutoffDate
                label is "CutoffDate"
        two column
            ArchiveCbfttrans
                label is "ArchiveCashLedgerE\\F\\TExtracts"
            CftCutoffDate
                label is "CutoffDate"
        two column
            ArchiveCbretpmt
                label is "ArchiveCashLedgerReturnPayments"
            CrpCutoffDate
                label is "CutoffDate"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                BankTransactionHistoryPurgeReport_RDG
            column2 of 25%
                BankTransactionHistoryPurgeReport_XPT
