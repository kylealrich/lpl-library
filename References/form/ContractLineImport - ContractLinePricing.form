ContractLinePricing is a Form
    title is "ContractLineImportPricing"
    Layout
        visible when (not ContractImport.ServicePurchaseType)
            two column
                CostOption
        two column
            EffectiveDate
                label is "CurrentEffectiveDate"
            ExpirationDate
                label is "CurrentExpirationDate"
        visible when (ContractImport.UseLineCommitments)
            single column
                UseCommitments
        visible when (PriceBreaksAllowed)
            header2 of "PurchaseOrServicePricing"
            single column
                PricingMarkupOrDiscount
                    label is "MarkupOrDiscount"
                LinePriceBreakOn
                    label is "ByQuantityOrAmount"
                PriceBreakImport
                    label is "PriceBreaks"
                    compact format
        visible when (CsnPriceBreaksAllowed)
            header2 of "ConsignmentPricing"
            single column
                ConsignmentPricingMarkupOrDiscount
                    label is "MarkupOrDiscount"
                ConsignmentLinePriceBreakOn
                    label is "ByQuantityOrAmount"
                ConsignmentPriceBreak
                    label is "PriceBreaks"
                    compact format
        header2 of "QuantitiesAndAmounts"
        visible when (not ContractImport.ServicePurchaseType)
            single column
                LifetimeMaximumAmount
        visible when (not ServiceCode.Amount)
            two column
                PerOrderMaximumQuantity
                    label is "PerOrderMaximumQuantity"
                PerOrderMinimumQuantity
                    label is "PerOrderMinimumQuantity"
        two column
            PerOrderMaximumAmount
                label is "PerOrderMaximumAmount"
            PerOrderMinimumAmount
                label is "PerOrderMinimumAmount"
        visible when (not ContractImport.ServicePurchaseType)
            paragraph
                PurchaseOrderUserField2
                    label is "POUserFields"
                PurchaseOrderUserField4
                    no label
                PurchaseOrderUserField6
                    no label
        paragraph
            Taxable
            TaxCode
            TaxUsageCode
