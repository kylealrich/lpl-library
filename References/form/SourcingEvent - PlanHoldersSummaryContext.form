PlanHoldersSummaryContext is a Form
    Layout
        single column
            button of "AddMeToPlanHoldersList"
                visible when (AddMeToPlanHoldersListValid)
                action is AddMeToPlanHoldersList
            button of "RemoveMeFromPlanHoldersList"
                visible when (RemoveMeFromPlanHoldersListValid)
                action is RemoveMeFromPlanHoldersList
