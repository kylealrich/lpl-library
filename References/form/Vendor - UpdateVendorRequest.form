                            when value changed
                                refresh SwiftIDRequired
                        BankEntity.BankName
                            no label
                            display as text
                    paragraph
                        BankAccountNumber
                            label is "AccountNumber"
                            display as required when (BankAccountNumberRequired)
                            when value changed
                                refresh SwiftIDRequired
                        DerivedAccountNumber
