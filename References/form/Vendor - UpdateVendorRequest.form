UpdateVendorRequest is an ActionForm
    action is RequestUpdateVendor
    title is "RequestVendorUpdate"
    Layout
        section of Main
            visible when (actor.initiator)
                header4 of "This_request_will_be_routed_for_approval;_after_it_is_approved_the_Vendor_will_be_updated."
            visible when (not actor.initiator)
                display as text
                    two column
                        column1 of 50%
                            RequestedBy
                            button of "<RequesterEmail>"
                                link is "mailto:<RequesterEmail>"
                                label is "Email"
                                display as link
                            NeededBy
                                raise red alert when (NeededByRedBoolean)
                                    mouse over text is "Overdue"
                                raise yellow alert when (NeededByYellowBoolean)
                                    mouse over text is "Needed_\\Today"
                                raise blue alert when (NeededByGreenBoolean)
                                    mouse over text is "Due_\\Within_<NoOfDays>_\\Days"
                        column2 of 50%
                            RequesterActor
                                label is "ID"
                            button of "<RequesterPhone.SubscriberNumber>"
                                link is "tel:<RequesterPhone.SubscriberNumber>"
                                label is "Phone"
                                display as link
                            RequestPriority
                                label is "Priority"
                                display as tag
                                    color of red when (RequestPriority.High)
                                    color of yellow when (RequestPriority.Medium)
                                    color of green when (RequestPriority.Low)
            visible when (not actor.initiator)
                two column
                    column1 of 50%
                        paragraph
                            VendorGroup
                                show up to 4 characters
                            VendorGroup.Description
                                no label
                                display as text
                    column2 of 50%
                        paragraph
                            VendorClass
                            VendorClass.Description
                                no label
                                display as text
            visible when (actor.initiator)
                paragraph distributed
                    NeededBy
                    RequestPriority
                        label is "Priority"
                two column distributed
                    column1 of 50%
                        paragraph
                            VendorClass
                            VendorClass.Description
                                no label
                                display as text
                    column2 of 50%
                        VendorStatus
                            label is "Status"
            two column
                column1 of 50%
                    VendorName
                    LegalName
                column2 of 50%
                    VendorSearchName
                        label is "SearchName"
                    visible when (not actor.initiator)
                        VendorStatus
                            label is "Status"
            three column
                column1 of 50%
                    Notes
                column2 of 25%
                    TaxIDType
                    VATRegistrationCountry
                        label is "V\\A\\TCountry/\\Jurisdiction"
                        display as required when (VATRegistrationNumber entered)
                column3 of 25%
                    paragraph
                        TaxID
                            label is "TaxID"
                            show up to 10 characters
                        visible when (TaxID entered)
                            ClearTaxID
                                label is "Clear"
                                mouse over text is "CheckToClearThe_\\Tax\\I\\DField"
                    VATRegistrationNumber
                        label is "V\\A\\TNumber"
                        show up to 10 characters
            two column table
                column1
                    Logo.File
                        label is "Logo"
                    URLAddress
                column2
                    TransientAttachment.File
                        label is "Attachment"
                    TransientAttachmentDescription
                        label is "AttachmentDescription"
            header2 of "CurrentAddress"
            RequestAddressChange
            visible when (RequestAddressChange)
                VendorAddress
                    form is PostalAddressShortRequired
        section of Banking
            header2 of "VendorBank"
            two column distributed
                column1 of 75%
                    paragraph
                        BankEntity
