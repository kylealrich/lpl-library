CreateForProcedureMainForm is a Form
    title is "CreateRequisition"
    disable field options
    Layout
        four column
            paragraph
                Company
                Company.Name
                    no label
                    display as text
            paragraph
                RequestingLocation
                RequestingLocation.Name
                    no label
                    display as text
            paragraph
                FromCompanyLocation.FromCompany
                    label is "DeliverFromCompany"
                FromCompanyLocation.FromCompany.Name
                    no label
                    display as text
            paragraph
                FromCompanyLocation.FromLocation
                    label is "DeliverFromLocation"
                    select is RQInventoryLocations
                FromCompanyLocation.FromLocation.Name
                    no label
                    display as text
        three column
            paragraph
                Requester
                Requester.RequesterFirstLastName
                    no label
                    display as text
            paragraph
                Vendor
                Vendor.VendorName
                    no label
                    display as text
            visible when (not ActorIsBuyer)
                paragraph
                    visible when (DisplayBuyer)
                        Buyer
                        Buyer.Employee.Name.PreferredFirstAndLastName
                            no label
                            display as text
                        text of "BuyerMustBeEnteredOrDefaultedBeforeReleaseIfPOIsToBeCreated"
                    visible when (DisplayOnlyBuyer)
                        Buyer
                        Buyer.Employee.Name.PreferredFirstAndLastName
                            no label
                            display as text
                        text of "BuyerMustBeEnteredOrDefaultedBeforeReleaseIfPOIsToBeCreated"
        two column
            visible when (DisplayBillCode)
                DefaultBillCode
            visible when (DisplayRequestedDeliveryDate)
                RequestedDeliveryDate
                    label is "DeliveryDate"
        two column
            paragraph
                TransientAttachment.File
                    label is "Attachment"
                TransientCommentTitle
                    label is "AttachmentTitle"
            RequisitionDescription
        header1 of "PatientProcedureInformation"
        two column distributed
            column1
                header2 of "Visit"
                TransientVisitNumber
                    label is "VisitNumber"
                TransientPatientID
                    label is "MedicalRecordNumber"
                TransientCaseNumber
                    label is "CaseID"
                paragraph
                    DefaultProcedureInformation.DefaultProcedure
                        label is "Procedure"
                    DefaultProcedureInformation.DefaultProcedureModifier
                        label is "ProcedureModifier"
                    DefaultProcedureInformation.DefaultProcedureDate
                        label is "ProcedureDate"
                DefaultProcedureInformation.DefaultPractitioner
                    label is "Practitioner"
                DefaultProcedureInformation.DefaultNurseCoordinator
                    label is "CoordinatingNurse"
            column2
                header2 of "Patient"
                TransientPatientLastName
                    label is "LastName"
                TransientPatientFirstName
                    label is "FirstName"
                TransientPatientMiddleName
                    label is "MiddleInitial"
                paragraph
                    TransientBirthdate
                        label is "Birthdate"
                    TransientGender
                        label is "Gender"
        header2 of "Order"
        single column
            DefaultProcedureInformation.DefaultSupplierOrderNumber
                label is "SupplierOrderNumber"
