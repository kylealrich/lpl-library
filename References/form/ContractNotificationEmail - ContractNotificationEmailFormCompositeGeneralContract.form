ContractNotificationEmailFormCompositeGeneralContract is a CompositeForm
    title is "ContractNotificationEmail"
    context form is ContractNotificationEmailForm
        background is theme color
    EmailedRedlinedDocument is a Panel
        visible when (GeneralRedlineContractEmailWithAttachment)
        list is RedlineAttachmentRel.ContractRedlineDocument
    EmailedContractDocument is a Panel
        visible when (GeneralContractDocumentEmailNotification)
        list is ContractDocumentRel.ContractDocument
    EmailedAttachments is a Panel
        visible when (GeneralContractEmailNotificationWithAttachment)
        list is ContractCommentAttachmentRel.ContractAttachments
    EmailedLineAttachments is a Panel
        visible when (GeneralContractLineEmailNotificationWithAttachment)
        list is ContractLineCommentAttachmentRel.ContractLineAttachments
