DistributionDetailForDrill is a Form
    title is "DistributionDetail"
    Layout
        paragraph
            Company
                display as text
        paragraph
            DistributionAccount
                display as text
        paragraph
            DistributionAmount
                label is "Amount"
                display as text
            Requisition.BaseCurrencyCode
                no label
                display as text
        paragraph
            ReportCurrencyAmount.FunctionalAmount
                display as text
        paragraph
            TransientPostingDate
                label is "PostingDate"
                display as text
        paragraph
            AssetInformation.Asset
                display as text
            AssetInformation.AssetTemplate
                display as text
        paragraph
            Contract
                display as text
