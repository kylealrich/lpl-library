CurrencyAmountsForm is a Form
    title is "CurrencyAmounts"
    Layout
        visible when (false)
            ReceiptBaseAmount.AssignToCurrency
        blank line
        four column table
            column headers
                "_"
                "Amount"
                "Currency"
                "Rate"
            column1
                text of "Functional"
                visible when (Company.OrigCompany.AccountingEntity.AlternateCurrencyExists)
                    text of "Alternate"
                visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency2Exists)
                    text of "Alternate2"
                visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency3Exists)
                    text of "Alternate3"
                visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                    text of "<ReceiptBaseAmount.FinanceEnterpriseGroup.ReportOne>"
                visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                    text of "<ReceiptBaseAmount.FinanceEnterpriseGroup.ReportTwo>"
                visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                    text of "<ReceiptBaseAmount.FinanceEnterpriseGroup.ReportThree>"
                visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                    text of "<ReceiptBaseAmount.FinanceEnterpriseGroup.ReportFour>"
                visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                    text of "<ReceiptBaseAmount.FinanceEnterpriseGroup.ReportFive>"
            column2
                display as text
                    ReceiptBaseAmount.FunctionalAmount.EnteredCurrencyAmount
                        no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrencyExists)
                        ReceiptBaseAmount.AlternateAmount.EnteredCurrencyAmount
                            no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency2Exists)
                        ReceiptBaseAmount.AlternateAmount2.EnteredCurrencyAmount
                            no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency3Exists)
                        ReceiptBaseAmount.AlternateAmount3.EnteredCurrencyAmount
                            no label
                    visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        ReceiptBaseAmount.ReportAmount1.EnteredCurrencyAmount
                            no label
                    visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        ReceiptBaseAmount.ReportAmount2.EnteredCurrencyAmount
                            no label
                    visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        ReceiptBaseAmount.ReportAmount3.EnteredCurrencyAmount
                            no label
                    visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        ReceiptBaseAmount.ReportAmount4.EnteredCurrencyAmount
                            no label
                    visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                        ReceiptBaseAmount.ReportAmount5.EnteredCurrencyAmount
                            no label
            column3
                display as text
                    Company.OrigCompany.AccountingEntity.FunctionalCurrency
                        no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrencyExists)
                        Company.OrigCompany.AccountingEntity.AlternateCurrency
                            no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency2Exists)
                        Company.OrigCompany.AccountingEntity.AlternateCurrency2
                            no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency3Exists)
                        Company.OrigCompany.AccountingEntity.AlternateCurrency3
                            no label
                    visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyOne
                            no label
                    visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyTwo
                            no label
                    visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyThree
                            no label
                    visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyFour
                            no label
                    visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                        ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyFive
                            no label
            column4
                display as text
                    ReceiptBaseAmount.FunctionalAmount.EnteredCurrencyRate
                        no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrencyExists)
                        ReceiptBaseAmount.AlternateAmount.EnteredCurrencyRate
                            no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency2Exists)
                        ReceiptBaseAmount.AlternateAmount2.EnteredCurrencyRate
                            no label
                    visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency3Exists)
                        ReceiptBaseAmount.AlternateAmount3.EnteredCurrencyRate
                            no label
                    visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        ReceiptBaseAmount.ReportAmount1.EnteredCurrencyRate
                            no label
                    visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        ReceiptBaseAmount.ReportAmount2.EnteredCurrencyRate
                            no label
                    visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        ReceiptBaseAmount.ReportAmount3.EnteredCurrencyRate
                            no label
                    visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        ReceiptBaseAmount.ReportAmount4.EnteredCurrencyRate
                            no label
                    visible when (ReceiptBaseAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                        ReceiptBaseAmount.ReportAmount5.EnteredCurrencyRate
                            no label
