MobileTransactionSummaryContext is a Form
    title is "TransactionSummary"
    Layout
        single column
            RecallProduct.ProductName
                display as text
            QuantityIdentified
                label is "QuantityFound"
                display as text
        display as text
            single column
                visible when (HasLocationActivity)
                    RecallProduct.TaskInstructionDescription
                        label is "TaskInstruction"
