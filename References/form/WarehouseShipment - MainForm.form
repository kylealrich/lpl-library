MainForm is a Form
    Layout
        visible when (not RecordExists)
            three column distributed
                paragraph
                    Company
                    Company.Name
                        no label
                        display as text
                paragraph
                    InventoryLocation
                        label is "Location"
                    InventoryLocation.Name
                        no label
                        display as text
                paragraph
                    WarehouseShipment
                        label is "ShipmentNumber"
        two column distributed
            column1
                paragraph
                    WarehouseBatch
                        label is "Batch"
                        display as text
                paragraph
                    visible when (IsShipped)
                        Carrier
                            display only
                            select is VendorByName
                        Carrier.VendorName
                            no label
                            display as text
                    visible when (not IsShipped)
                        Carrier
                            select is VendorByName
                        Carrier.VendorName
                            no label
                            display as text
            column2
                paragraph
                    ShipDate
                    visible when (RequirePickingFeedback)
                        PickDate
                            display as text
                    visible when (RequirePackingFeedback)
                        PackDate
                            display as text
                paragraph
                    visible when (IsShipped)
                        FreightCode
                            display only
                        FreightCharges
                            display only
                    visible when (not IsShipped)
                        FreightCode
                            label is "FreightCode"
                        FreightCharges
        two column distributed
            column1
                header2 of "AdditionalShipmentInformation"
                visible when (IsShipped)
                    GeneralLedgerDate
                        label is "PostingDate"
                        display only
                    BillOfLading
                        label is "Bill_ofLadingNumber"
                        display only
                        show up to 10 characters
                    visible when (HasProofOfDeliveryRequired)
                        PODReturnDate
                            label is "P\\O\\DReturnedDate"
                    visible when (not HasProofOfDeliveryRequired)
                        PODReturnDate
                            label is "P\\O\\DReturnedDate"
                            display only
                    ShippingMethod
                        label is "Method"
                        display only
                    TransactionId
                        label is "TransactionI\\D"
                        display only
                    paragraph
                        NumberOfCartons
                            label is "Cartons"
                            display only
                        StockVolume
                            label is "CubicFeet"
                            display only
                        StockWeight
                            label is "Weight"
                            display only
                visible when (not IsShipped)
                    GeneralLedgerDate
                        label is "PostingDate"
                    BillOfLading
                        label is "Bill_ofLadingNumber"
                        show up to 10 characters
                    PODReturnDate
                        label is "P\\O\\DReturnedDate"
                    ShippingMethod
                        label is "Method"
                    TransactionId
                        label is "TransactionI\\D"
                    paragraph
                        NumberOfCartons
                            label is "Cartons"
                        StockVolume
                            label is "CubicFeet"
                        StockWeight
                            label is "Weight"
            column2
                header2 of "Global"
                paragraph
                    visible when (IsShipped)
                        ShipTerm
                            label is "DeliveryTerms"
                            display only
                        PortCode
                            label is "LoadingPort"
                            display only
                    visible when (not IsShipped)
                        ShipTerm
                            label is "DeliveryTerms"
                        TransientIntrastatPortCode
                            select is IntrastatPortCodeForInventoryLocationRel.IntrastatPortCodeList
                visible when (IsShipped)
                    StatisticalProcedure
                        display only
                    GlobalDocumentType
                        display only
                visible when (not IsShipped)
                    StatisticalProcedure
                    GlobalDocumentType
                NatureOfTransactionCode
                    label is "N\\O\\T\\C"
                    display as text
