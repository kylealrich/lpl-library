TrialBalance is a CompositeForm
    title is "TrialBalance-<AccountAnalysisSettings>_<Description>"
    context form is AccountAnalysisSettingsContext
        show context form on left
    Actions
        action is save
            action icon is save
            label is "Save_andRefresh"
            mouse over text is "Save_SettingsAndRefreshData"
            hide when invalid
        action is export to pdf
            action icon is icon.export-to-pdf
            label is "Export"
            mouse over text is "ExportToPDF"
        action is ShareAccountAnalysis
            action icon is icon.share
            label is "Share"
            mouse over text is "Share_Account_Analysis_Settings"
            hide when invalid
        action is Create
            action icon is create
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
        action is refresh
            action icon is icon.refresh
            label is "_"
    SingleAccount is a Panel
        title is "<ReportingBasis.ReportingChart>_ChartIn<DisplayReportingCurrency>"
        visible when (AccountEntered)
        list is GeneralLedgerChartAccountRel.TrialBalance
    ChartAccounts is a Panel
        title is "<ReportingBasis.ReportingChart>_ChartIn<DisplayReportingCurrency>"
        visible when (not AccountEntered)
        list is ReportingBasis.ChartAccountTopNode.ChartAccountChildrenRel.TrialBalance
    PostingChartAccounts is a Panel
        title is "Posting<ReportingBasis.ReportingChart>_ChartIn<DisplayReportingCurrency>"
        visible when (not AccountEntered)
        list is ReportingBasis.ChartAccountTopNode.SettingsGLChartAccountPostingRel.PostingTrialBalance
    PostingAccounts is a Panel
        title is "Posting<ReportingBasis.ReportingChart>_ChartIn<DisplayReportingCurrency>"
        visible when (PostingAccountsUnderSummary)
        list is GeneralLedgerChartAccount.SettingsGLChartAccountPostingRel.PostingTrialBalance
    Structures is a Panel
        visible when (not IsOldBasis)
        form is Structures
