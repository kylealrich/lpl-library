InterfacePayment is a CompositeForm
    is primary
    title is "DocumentPaymentInterface"
    context form is InterfacePaymentContext
        background is theme color
    restrict action MovePaymentImportToDifferentCompanyCustomer
    Remittances is a Panel
        list is DocumentARPaymentRemitImportRel.InlineRemits
    Errors is a Panel
        visible when (RecordInError)
        list is DocumentARPaymentImportErrorRel.DocumentARPaymentImportErrorList
