PurgeActionResultBusClassCompForm is a CompositeForm
    is primary
    title is "BusinessClassPurgeResults"
    context form is PurgeActionResultBusClassContextForm
    Data is a Panel
        visible when (DisplayData)
        list is ArchivedDataRel.ArchivedDataNestedList
    ActionGroupMembers is a Panel
        visible when (DisplayActionGroupMembers)
        list is ActionGroupMembers.PurgeActionResultBusClassNoActionGroupList
    Purged is a Panel
        visible when (not IsUnderScoreBusinessClass)
        form is RecordCountCheckSumForm
    CreatedInArchive is a Panel
        visible when (DisplayOtherTabs)
        form is CreatedInArchiveCheckSumForm
    UpdatedInArchive is a Panel
        visible when (DisplayOtherTabs)
        form is UpdatedInArchiveCheckSumForm
    Restored is a Panel
        visible when (not IsUnderScoreBusinessClass)
        form is RestoredCheckSumForm
    PurgedFromArchive is a Panel
        visible when (DisplayOtherTabs)
        form is PurgedFromArchiveSumForm
