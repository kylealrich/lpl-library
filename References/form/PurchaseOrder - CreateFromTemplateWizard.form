CreateFromTemplateWizard is a WizardForm
    use for action Unreleased.CreateFromTemplate
    title is "CreatePurchaseOrder_fromTemplate"
    summary form is CreateFromTemplateFinish
    show steps
    PurchaseOrder is a Panel
        form is CreateFromTemplateForm
        save on next
    ProcurementTemplateLine is a Panel
        title is "PurchaseOrderLines"
        mouse over text is "SelectProcurementTemplateLinesForPurchaseOrder"
        list is PurchaseOrderLinesRel.PurchaseOrderLinesFromTemplate
            helper list is ProcurementTemplate.ValidProcurementTemplateLinesRel.ProcurementTemplateLinesList
                action is IncludeInProcurementTemplateLine
                    invoked.PrmCompany = Company
                    invoked.PrmPurchaseOrder = PurchaseOrder
