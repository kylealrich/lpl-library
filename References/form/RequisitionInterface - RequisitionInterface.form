RequisitionInterface is an ActionForm
    action is RequisitionInterface
    Layout
        single column
            PrmRunGroup
                label is "RunGroup"
            ProcessingOption
        single column
            ReleaseRequisitions
                label is "ReleaseRequisitions"
                    left of field
            visible when (ProcessingOption.DatabaseFile)
                IncludeRequisitionsInError
                    label is "IncludeRequisitionsInError"
                        left of field
            text of "IfRunForErrorRunGroupRequisitionsInErrorWillAutomaticallyBeIncluded"
                italics
            PrmMoveErrorsToNewRunGroup
                label is "MoveErrorsToNewRunGroup"
                    left of field
            visible when (PrmMoveErrorsToNewRunGroup)
                PrmErrorRunGroupPrefix
                    label is "ErrorRunGroupPrefix"
