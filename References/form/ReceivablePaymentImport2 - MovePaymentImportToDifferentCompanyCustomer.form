MovePaymentImportToDifferentCompanyCustomer is an ActionForm
    action is MovePaymentImportToDifferentCompanyCustomer
    title is "MovePaymentImportToDifferentCompanyCustomer"
    Layout
        two column distributed
            column1
                paragraph
                    text of "CURRENT"
                        header4
                        bold
                paragraph
                    Company
                        display as text
                        select is Companies
                    Company.Name
                        no label
                        display as text
                paragraph
                    ReceivablePaymentImport2.Customer
                        display as text
                        select is CustomerContextCompanyCustomerSelectList
                    ReceivablePaymentImport2.Customer.Name
                        no label
                        display as text
            column2
                paragraph
                    text of "NEW"
                        header4
                        bold
                paragraph
                    PrmNewReceivableCompany
                        select is Companies
                    PrmNewReceivableCompany.Name
                        no label
                        display as text
                paragraph
                    PrmNewCustomer
                        select is CustomerContextCompanyCustomerSelectList
                    PrmNewCustomer.Name
                        no label
                        display as text
