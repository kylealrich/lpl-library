ParameterForm is a Form
    Layout
        display as text
            two column distributed
                column1
                    text of "Parameters:"
                        bold
                    paragraph
                        ProcessByProcurementGroup
                        FinanceEnterpriseGroup
                            label is "ProcurementGroup"
                    Company
                    DefaultBuyer
                    DefaultDeliveryDays
                    IncludeAllCostOptions
                    MaximumPOLines
                    CombineOnPOLine
                    OneShipToPerPO
                    ReleasePurchaseOrder
                column2
                    text of "Filters:"
                        bold
                    SystemCode
                    VendorOption
                    paragraph
                        Vendor.VendorAndPurchaseFrom[1].Vendor
                            label is "Vendor\\,PurchaseFrom"
                        Vendor.VendorAndPurchaseFrom[1].PurchaseFromLocation
                            no label
                    visible when (Vendor.VendorAndPurchaseFrom[2].Vendor entered)
                        paragraph
                            Vendor.VendorAndPurchaseFrom[2].Vendor
                                no label
                            Vendor.VendorAndPurchaseFrom[2].PurchaseFromLocation
                                no label
                    visible when (Vendor.VendorAndPurchaseFrom[3].Vendor entered)
                        paragraph
                            Vendor.VendorAndPurchaseFrom[3].Vendor
                                no label
                            Vendor.VendorAndPurchaseFrom[3].PurchaseFromLocation
                                no label
                    visible when (Vendor.VendorAndPurchaseFrom[4].Vendor entered)
                        paragraph
                            Vendor.VendorAndPurchaseFrom[4].Vendor
                                no label
                            Vendor.VendorAndPurchaseFrom[4].PurchaseFromLocation
                                no label
                    FilterBuyer
                        label is "Buyer"
                    paragraph
                        BuyerOnPurchasingClasses
                        BuyerOnVendor
                    paragraph
                        BuyerOnVendor
                        BuyerOnRequestingLocation
                    FilterDeliveryDays
                        label is "DeliveryDays"
                    paragraph
                        FilterFromPriority
                            label is "FromPriority"
                        FilterToPriority
                            label is "ToPriority"
                    paragraph
                        FilterLocation
                            label is "Location"
                        text of "Or"
                        FilterReportGroup
                            label is "ReportGroup"
                    POCodeOption
                    FilterPOCode
                        label is "POCode"
                    FilterMajorPurchasingClass
                        label is "MajorPurchasingClass"
                    FilterMinorPurchasingClass
                        label is "MinorPurchasingClass"
                    FilterRequestingLocation
                        label is "RequestingLocation"
                    FilterItem
                        label is "Item"
                    FilterItemType
                        label is "ItemType"
                    FilterManufacturer
                        label is "ManufacturerCode"
                    FilterRequisition
                        label is "Requisition"
                    FilterProcurementTemplate
                        label is "ProcurementTemplate"
                    BillCodeOption
                    FilterConsignCode
                        label is "ConsignCode"
                    FilterAccountingUnit
                        label is "AccountingUnit"
                    FilterActivity
                        label is "Activity"
                    FilterCostCode
                        label is "CostCode"
