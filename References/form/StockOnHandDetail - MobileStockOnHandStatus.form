MobileStockOnHandStatus is a CompositeForm
    title is "StockOnHandStatus"
    context form is MobileStockOnHandContextForm
    LotBinTrackedItem is a Panel
        title is "LotAndBinTrackedItem"
        visible when (LotBinTracked)
        list is StockOnHandDetail.MobileLotTransferList
    SerialTrackedItem is a Panel
        visible when (IsSerialTracked)
        list is StockOnHandDetail.MobileSerialBinTransfer
    BinTrackedItem is a Panel
        visible when (IsOnlyBinTracked)
        list is StockOnHandDetail.MobileIsBinTransfer
    CurrentStatus is a Panel
        form is MobileStockOnHandCurrentStatus
