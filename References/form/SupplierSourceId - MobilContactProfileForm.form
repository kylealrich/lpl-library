MobilContactProfileForm is a Form
    title is "ContactProfile"
    Layout
        display as text
            header3 of "_"
            paragraph
                visible when (ContactTitle entered)
                    button of icon.roles
                    ContactTitle
                        no label
            paragraph
                visible when (PrimaryContact)
                    PrimaryTag
                        no label
                        display as tag
                            color of green
            paragraph
                visible when (not CurrentUser)
                    button of icon.mail
                    button of "<EmailAddress>"
                        link is "mailto:<EmailAddress>"
                        display as link
            paragraph
                visible when (CurrentUser)
                    button of icon.mail
                    EmailAddress
            paragraph
                visible when (TelephoneNumber entered)
                    button of icon.phone
                    button of "<DerivedTelephoneNumber>"
                        link is "tel:<TelephoneNumber.SubscriberNumber>"
                        display as link
            paragraph
                visible when (MobilePhone entered)
                    button of icon.mobile
                    button of "<DerivedMobileNumber>"
                        link is "tel:<MobilePhone.MobilePhoneNumber.SubscriberNumber>"
                        display as link
            paragraph
                visible when (FaxNumber entered)
                    paragraph
                        button of icon.print
                        FaxNumber.SubscriberNumber
                            no label
            paragraph
                button of icon.home
                FullAddressDisplay
                    no label
            single column
                visible when (not ReceiveEmailNotification)
                    NotReceivingNotifications
                        no label
                        display as tag
                            color of red
            paragraph
                button of icon.edit
                    link is ProfileNav
                button of "EditProfile"
                    link is ProfileNav
                    display as link
            visible when (not RegistrationComplete)
                header2 of "_"
                paragraph
                    text of "Registration_Incomplete"
                        header3
                        color of red
                paragraph
                    text of "TakeTheFollowingActionsToCompleteRegistration"
                        header4
                visible when (IncompleteCommodities)
                    paragraph
                        button of "AddCommodityCodes"
                            link is CommodityCodeNav
