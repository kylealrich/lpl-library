ItemInterfaceAdjustment is a Form
    is primary
    title is "AssetItemInterface"
    Layout
        paragraph
            RunGroup
            RecordInError
                display as text
            ErrorMessage
                display as text
        paragraph
            FinanceEnterpriseGroup
            AssetImport
                label is "AssetImportGroup"
            AssetImport.Description
                no label
                display as text
        paragraph
            AssetImport.Currency
        two column
            column1
                ItemNumber
                Description
                    label is "Description"
            column2
                PurchaseDate
                ItemQuantity
        two column
            column1
                TransactionItemCost
                    label is "Cost"
                TransactionItemTax
                    label is "Tax"
            column2
                BaseItemCost
                    label is "BaseCost"
                BaseItemTax
                    label is "Tax"
        two column
            column1
                ModelNumber
                SerialNumber
                BarCode
            column2
                LocationDetail
                    select is ActiveAssetLocationDetails
                ItemCondition
                visible when (DisplayItemImportFunds)
                    button of "Funds"
                        link is Funds
        blank line
        header3 of "SourceReferenceFields"
        two column independent
            column1
                text of "Internal"
                    bold
                    color of blue
                paragraph
                    InvoiceCompany
                        label is "PayablesCompany"
                    InvoiceCompany.Name
                        no label
                        display as text
                paragraph
                    Vendor
                    Vendor.VendorName
                        no label
                        display as text
                paragraph
                    PayablesInvoice
                    PayablesInvoice.Description
                        no label
                        display as text
                paragraph
                    PayablesInvoiceDistribution
                        label is "DistributionSequence"
                paragraph
                    PurchaseOrder
                paragraph
                    Project
                    Project.Project.Description
                        no label
                        display as text
            column2
                text of "External"
                    bold
                    color of blue
                AssetItemReferenceFields.ReferenceCompany
                AssetItemReferenceFields.ReferenceVendor
                AssetItemReferenceFields.ReferenceInvoice
                AssetItemReferenceFields.ReferencePurchaseOrder
                AssetItemReferenceFields.ReferenceSequenceNumber
                AssetItemReferenceFields.ReferenceProject
