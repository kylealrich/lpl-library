Parameters is a Form
    Layout
        display as text
            three column distributed
                column1
                    PrmCompany
                        label is "Company"
                    PrmCompanyGroup
                        label is "CompanyGroup"
                    PrmMatchPoint
                        label is "MatchPoint"
                    PrmVendorRange.Begin
                        label is "FromVendor"
                    PrmVendorRange.End
                        label is "ToVendor"
                    PrmInvoiceProcess
                        label is "InvoiceSelection"
                column2
                    PrmDateType
                        label is "DateType"
                    PrmRunDate
                        label is "RunDate"
                    PrmUseSystemDate
                        label is "SystemDate"
                            left of field
                    PrmDaysBeforeRunDate
                        label is "DaysBeforeRunDate"
                    PrmDaysAfterRunDate
                        label is "DaysAfterRunDate"
                    visible when (InterfaceRunEntered)
                        PrmInterfaceRun
                            label is "InterfaceRunID"
                column3
                    PrmPrepaymentInvoiceMatchRule
                        label is "PrepayInvoiceMatchRule"
                    PrmPrepaymentDetailMatchRule
                        label is "PrepayDetailMatchRule"
                    PrmSkipDays
                        label is "SkipDays"
                    PrmSkipDate
                        label is "SkipDate"
                    PrmSkipOnlyIfNoReceipt
                        label is "SkipOnlyIfNoReceipt"
