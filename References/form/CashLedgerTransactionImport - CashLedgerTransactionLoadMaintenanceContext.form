CashLedgerTransactionLoadMaintenanceContext is a Form
    title is "CashLedgerTransactionInterfaceMaintenance"
    Layout
        visible when (RecordExists)
            three column distributed
                column1
                    display as text
                        CashManagementGroup
                        CashLedgerTransactionImport.BankTransactionCode.BankTransactionCodeName
                            label is "TransactionCode"
                column2
                    display as text
                        CashLedgerTransactionImport.RunGroup
                            label is "RunGroup"
                        paragraph
                            CashLedgerTransactionImport.TransactionNumber
                column3
                    display as text
                        paragraph
                            CashLedgerTransactionImport.CashCode.CashCodeAndDescription
                                label is "CashCode"
                            CashLedgerTransactionImport.CashCode.Currency
                        CashLedgerTransactionImport.SequenceNumber
            single column
                display as text
                    ErrorMessage
            button of "InterfacedCashLedgerTransactionRecord"
                visible when (IsPartiallyImported)
                link is CashLedgerTransactionRecordNavigation
                display as link
