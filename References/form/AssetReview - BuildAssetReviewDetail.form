BuildAssetReviewDetail is an ActionForm
    action is BuildAssetReviewDetail
    title is "BuildAssetReviewDetailForCompany<Company>"
    Layout
        paragraph
            PrmAssetOwner
                label is "AssetOwner"
            PrmAssetOwner.Name.FirstAndLastName
                no label
                display as text
        paragraph
            PrmAssetType
                label is "AssetType"
                compact format
                select is AssetTypesForAssetReview
            PrmAssetType.Description
                no label
                display as text
        paragraph
            PrmAssetDimensionGroup
                label is "AssetDimensionGroup"
            PrmAssetDimensionGroup.Description
                no label
                display as text
        paragraph
            PrmThreshold
                label is "Threshold"
        paragraph
            PrmQueue
                label is "NumberOfThreads"
