PortalGeneralLedgerJournalControl is a CompositeForm
    title is "JournalControl_<AccountingEntity>_<GeneralLedgerJournalControl>"
    context form is PortalGeneralLedgerJournalControlForm
    restrict action Unreleased.Create
    TotalsPanel is a Panel
        title is "Totals"
        form is GeneralLedgerJournalControlTotalsForm
    TransactionPanel is a Panel
        title is "Transactions"
        list is GeneralLedgerTransaction set.PortalGeneralLedgerJournalTransactionList
    CommentsPanel is a Panel
        title is "Comments"
        list is GeneralLedgerJournalComment set.Comment
    RelatedDocumentsPanel is a Panel
        title is "RelatedDocuments"
        list is GeneralLedgerJournalDocument set.GeneraLedgerJournalDocuments
