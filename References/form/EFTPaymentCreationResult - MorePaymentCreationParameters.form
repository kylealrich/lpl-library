MorePaymentCreationParameters is a Form
    title is "EFTPaymentCreationParameters"
    Layout
        paragraph
            DebitCashCode.CashCodeAndDescription
                no label
                display as text
            CreditCashCode.CashCodeAndDescription
                no label
                display as text
        paragraph
            BankTransactionCode.BankTransactionCodeName
                label is "SelectPaymentCode"
                display as text
            TermsCode.RepresentativeText
                label is "SelectPaymentTerms"
                display as text
        paragraph
            SelectProcessLevel.RepresentativeText
                display as text
        single column
            NotAppldCd
                label is "ReceivedNotAppliedCode"
                display as text
            OprCode
                label is "Operator"
                display as text
            Reference
                display as text
            CompanyIdOption
                label is "CompanyI\\DOption"
                display as text
        paragraph
            FillLastBlock
                label is "FillA\\C\\HLastPhysicalBlock"
                display as text
            BalancedTape
                label is "BalancedA\\C\\HTape"
                display as text
            IncludeAddenda
                label is "IncludeA\\C\\HAddenda"
                display as text
        header2 of "OptionalBankCharges"
        single column
            BankCharges
        single column
            GLAccount
                label is "GlobalLedgerAccount"
                compact format
        header2 of "UserFields"
        single column
            CbUserField1
                label is "CashLedgerUserField1"
                display as text
            CbUserField2
                label is "CashLedgerUserField2"
                display as text
            CbUserField3
                label is "CashLedgerUserField3"
                display as text
            CbUserField4
                label is "CashLedgerUserField4"
                display as text
        header2 of "FileNames"
        single column
            ACHFileName
            BACSFileName
            NonTapeFileName
            SEPAFileName
