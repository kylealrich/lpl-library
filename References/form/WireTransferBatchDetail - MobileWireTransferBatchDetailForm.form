MobileWireTransferBatchDetailForm is a Form
    title is "Details"
    Layout
        header2 of "OriginatorInformation"
        display as text
            two column table
                column1 of 50%
                    FromCompany.GeneralLedgerCompany.RepresentativeText
                        label is "FromCompany"
                    FromCashCode.Currency
                        label is "CashCodeCurrency"
                        display as text
                    FeeAmount
                    Reference
                column2 of 50%
                    FromCashCode
                        select is CashCodeForFromCompanyFinancialInstitutionBranch
                    FromCashCode.BankAccountNumber
                        label is "AccountNumber"
                    TransactionAmount
                    FeeBankTransactionCode.BankTransactionCodeName
                        label is "FeeBankTransactionCode"
            visible when (ShowAccountingUnit)
                paragraph distributed
                    FromAccountingUnit.AccountingEntity
                        label is "FromAccountingEntity"
                    FromAccountingUnit.AccountingUnit
                        label is "FromAccountingUnit"
                    FromAccountingUnit.AccountingUnit.Description
                        label is "Description"
                        display as text
            single column
                FromBankTransactionCode.BankTransactionCodeName
                    label is "FromBankTransactionCode"
            single column
                visible when (ShowFinanceDimension1)
                    FromFund
                        label is "FromFund"
                    FromFund.Description
                        label is "FromFund"
                        display as text
            visible when (ExternalTransfer)
                single column
                    SpecifyIntermediaryBank
            visible when (InternalTransfer)
                header2 of "BeneficiaryInformation"
                display as text
                    two column table
                        column1 of 50%
                            ToCompany.GeneralLedgerCompany.RepresentativeText
                                label is "ToCompany"
                            ToCashCode.Currency
                                label is "CashCodeCurrency"
                        column2 of 50%
                            ToCashCode
                                select is ToCashCodeForCompanyList
                            ToCashCode.BankAccountNumber
                                label is "BankAccount"
                    visible when (ToAccountingUnitRequired)
                        paragraph distributed
                            ToAccountingUnit.AccountingEntity
                                label is "ToAccountingEntity"
                            ToAccountingUnit.AccountingUnit
                                label is "ToAccountingUnit"
                            ToAccountingUnit.AccountingUnit.Description
                                label is "Description"
                                display as text
                    single column
                        ToBankTransactionCode.BankTransactionCodeName
                            label is "ToBankTransactionCode"
                    single column
                        visible when (ShowFinanceDimension1)
                            ToFund
                                label is "ToFund"
                            ToFund.Description
                                label is "Description"
            visible when (ExternalTransfer)
                header2 of "BeneficiaryInformation"
                two column table
                    column1 of 50%
                        BeneficiaryBankIdentifier
                        BeneficiaryName
                        BeneficiaryBankAccount
                    column2 of 50%
                        visible when (BeneficiaryBankIdentifier.RoutingNumber)
                            BeneficiaryRoutingNumber
                        BeneficiaryBankName
                        BeneficiaryIBAN
                single column
                    visible when (BeneficiaryBankIdentifier.Swift)
                        BeneficiarySwiftID
                blank line
                text of "BankAddress"
                    italics
                BeneficiaryBankPostalAddress
                    form is PostalAddressMinimum
                blank line
                visible when (SpecifyIntermediaryBank)
                    header2 of "IntermediaryInformation"
                    paragraph distributed
                        IntermediaryBankIdentifier
                        visible when (IntermediaryBankIdentifier.RoutingNumber)
                            IntermediaryRoutingNumber
                        visible when (IntermediaryBankIdentifier.Swift)
                            IntermediarySwiftID
                    two column table
                        column1 of 50%
                            IntermediaryBankName
                            IntermediaryIBAN
                        column2 of 50%
                            IntermediaryBankAccount
                    single column
                        IntermediaryBankPostalAddress
                            form is PostalAddressMinimum
