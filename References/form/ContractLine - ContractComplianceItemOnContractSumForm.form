ContractComplianceItemOnContractSumForm is a SummaryForm
    title is "_"
    Layout
        two column
            column1
                header2 of "Item:_<ItemNumber>_<ItemNumber.Description>_<ItemNumber.Description2>"
                header2 of "ContractBaseCost:_<BaseCost>"
            column2
                header2 of "Contract_\\U\\O\\M:_<UOM.UnitOfMeasure>"
                blank line
        list is ContractComplianceInvoiceLinesOnContractRel.ContractComplianceInvoiceLineOnContractReportList
        text of "OnContract_<Contract>_Item_<ItemNumber>_U\\O\\M_<UOM.UnitOfMeasure>Total:_"
            center
        four column
            column1
                blank line
                blank line
            column2
                blank line
                blank line
            column3
                TotalContractComplianceInvoiceLinesOnContractQuantity
                    label is "TotalInvoiceQuantity"
                    display when blank
                blank line
            column4
                TotalContractComplianceInvoiceLinesOnContractAmount
                    label is "TotalInvoiceExtendedCost"
                    display when blank
                PercentOfOnContractTotal
                    label is "PercentOfOnContractTotal"
                    display when blank
        blank line
