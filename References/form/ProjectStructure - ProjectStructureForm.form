ProjectStructureForm is a Form
    title is "ProjectStructure"
    Layout
        two column distributed
            column1
                paragraph
                    FinanceEnterpriseGroup
                        show up to 5 characters
                    FinanceEnterpriseGroup.EnterpriseGroup.Description
                        no label
                        display as text
        two column distributed
            column1
                paragraph
                    ProjectStructure
                        show up to 10 characters
                    Description
            column2
                paragraph
                    visible when (not UnusedDimensionStructure)
                        Active
                            label is "Active"
                                bold
                        IsEnterpriseStructure
                            label is "EnterpriseStructure"
                                bold
                        DerivedSystemProject
                            label is "System<FinanceEnterpriseGroup.ProjectLabel>"
                            display as text
        visible when (IsEnterpriseStructure)
            two column distributed
                column1
                    paragraph
                        GeneralLedgerCalendar
                        GeneralLedgerCalendar.Description
                            no label
                            display as text
                column2
                    paragraph
                        Currency
                            select is ProjectCurrencies
                        Currency.Description
                            no label
                            display as text
        visible when (IsEnterpriseStructure)
            three column distributed
                column1
                    ProjectInvoiceFromEmailAddress
                        label is "<FinanceEnterpriseGroup.ProjectLabel>InvoiceFromEmailAddress"
                column2
                    InvoicePrinterEmailAddress
                        label is "<FinanceEnterpriseGroup.ProjectLabel>InvoicePrinterEmailAddress"
                column3
                    paragraph
                        IDMPrinter
                            label is "<FinanceEnterpriseGroup.ProjectLabel>IDMPrinter"
                        IDMPrinter.Description
                            no label
                            display as text
        visible when (IsEnterpriseStructure)
            paragraph
                ProjectContractChangeApproval
                    label is "<FinanceEnterpriseGroup.ProjectLabel>ContractChangeApproval"
                ProjectContractInvoiceApproval
                    label is "<FinanceEnterpriseGroup.ProjectLabel>ContractInvoiceJournalizeApproval"
                ProjectContractRevenueApproval
                    label is "<FinanceEnterpriseGroup.ProjectLabel>ContractRevenueJournalizeApproval"
                LaborDistributionApproval
                Approve100PercentLaborActualOnly
            single column
                OverageAccountDefaultOption
            three column distributed
                column1
                    DistributionApprovalGroup
                        label is "LaborDistributionApprovalGroup"
                column2
                    SalaryEncumbranceCodeBlock
                        label is "DefaultSalaryEncumbranceFinanceStructure"
                        compact format
                column3
                    FringeEncumbranceCodeBlock
                        label is "DefaultFringeEncumbranceFinanceStructure"
                        compact format
            single column
                LaborDistributionConfirmation
                CertifyEffortConfirmation
