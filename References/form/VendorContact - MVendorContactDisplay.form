MVendorContactDisplay is a Form
    title is "VendorContact"
    Layout
        header2 of "ContactInformation"
        display as text
            single column
                ContactName
                    label is "Name"
                FormattedPhoneNumber
                FormattedFaxNumber
                EmailAddress
                FormattedMobilePhoneNumber
                VendorAddressLine1
                    label is "Address"
                    display when blank
                VendorAddressLine2
                    no label
                visible when (VendorAddressLine3Entered)
                    VendorAddressLine3
                        no label
                visible when (VendorAddressLine4Entered)
                    VendorAddressLine4
                        no label
                CityStateZIPDisplay
                    no label
                VendorCountry
                    no label
                CommentText
