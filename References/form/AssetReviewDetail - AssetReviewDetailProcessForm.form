AssetReviewDetailProcessForm is a Form
    title is "AssetReviewDetail"
    restrict action Delete
    Layout
        single column
            Company
                display as text
            AssetReview
                label is "ReviewDate"
                display as text
            Asset
                display as text
            Asset.Description
                label is "Description"
                display as text
            AssetOwner.Name.FirstAndLastName
                label is "Owner"
                display as text
            AssetDimensionGroup
                label is "DimensionGroup"
                display as text
            AssetType
                label is "Type"
                display as text
                compact format
            AssetLocation
                label is "Location"
                display as text
            AssetDivision
                label is "Division"
                display as text
            DerivedAssetProject
                label is "Project"
                display as text
            Asset.Currency
                label is "Currency"
                display as text
            AssetCost
                label is "Cost"
                display as text
            Asset.Reference1
                label is "Reference1"
                display as text
            ReviewCode
                display as text
