PaymentLoadMaintenanceContext is a Form
    title is "PaymentInterfaceMaintenance"
    Layout
        visible when (RecordExists)
            three column distributed
                column1
                    display as text
                        CashManagementGroup
                        CashLedgerPaymentImport.BankTransactionCode.BankTransactionCodeName
                            label is "TransactionCode"
                column2
                    display as text
                        CashLedgerPaymentImport.RunGroup
                            label is "RunGroup"
                        paragraph
                            CashLedgerPaymentImport.ReferenceNumber
                column3
                    display as text
                        paragraph
                            CashLedgerPaymentImport.CashCode.CashCodeAndDescription
                                label is "CashCode"
                            CashLedgerPaymentImport.CashCode.Currency
                        CashLedgerPaymentImport.SequenceNumber
            single column
                display as text
                    ErrorMessage
            button of "InterfacedPaymentRecord"
                visible when (IsPartiallyImported)
                link is CashLedgerPaymentRecordNavigation
                display as link
