BusinessViewCompositeForm is a CompositeForm
    is primary
    title is "BusinessView"
    context form is BusinessViewForm
    Actions is a Panel
        list is BusinessAction set.BusinessActionRelList
    Fields is a Panel
        list is BusinessField set.BusinessFieldList
    Relations is a Panel
        visible when (Type.BusinessClass)
        list is BusinessRelation set.BusinessRelationRelList
    Lists is a Panel
        list is BusinessList set.BusinessListRelList
    CardViews is a Panel
        list is BusinessCardView set.BusinessCardViewRelList
    Forms is a Panel
        list is BusinessForm set.BusinessFormRelList
    Charts is a Panel
        visible when (Type.BusinessClass)
        list is BusinessClassChart set.BusinessClassChartRelList
    DrillLists is a Panel
        list is BusinessDrillList set.BusinessDrillListRelList
    IncrementalIndicatorFields is a Panel
        visible when (HasIncrementalReplicationIndicator)
        list is BusinessClassIncrementalIndicatorField set.PartOfList
    States is a Panel
        visible when (HasStates)
        list is BusinessState set.BusinessStateRelList
