ReportingChartCompsiteForm is a CompositeForm
    is primary
    title is "<ReportingChartFormTitle>"
    context form is ReportingChartContextForm
        background is theme color
    FinanceEnterpriseGroupPanel is a Panel
        title is "Chart"
        form is ReportingChartForm
    ReportingChartAccounts is a Panel
        title is "Hierarchy"
        list is ChartAccountNode.ChartAccountChildrenRel.ChartAccountList
    ReportingChartAccountUpload is a Panel
        title is "Upload"
        visible when (not UnusedDimensionStructure)
        list is ReportingChartAccountUpload set.ReportingChartAccountUploadList
