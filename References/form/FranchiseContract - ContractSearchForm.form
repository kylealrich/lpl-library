ContractSearchForm is a SearchForm
    Layout
        header2 of "ContractCharges"
            center
        header3 of "Within:<DateRangeFilter>"
        blank line
        text of "Parameters"
        four column
            column1
                paragraph
                    Company
                    Company.BillingCompany.Name
                        no label
                        display only
                paragraph
                    Customer
                    Customer.Name
                        no label
                        display as text
                paragraph
                    FranchiseContract
                        label is "Contract"
            column2
                paragraph
                    CustomerShipTo
                        label is "ShipTo"
                    CustomerShipTo.Name
                        no label
                        display only
                paragraph
                    ChargeTypeFilter
                        label is "ChargeType"
                    DateRangeFilter
                        label is "DateRange"
