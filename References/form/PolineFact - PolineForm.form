PolineForm is a Form
    title is "POLineDetail"
    Layout
        paragraph
            PoNumber
                display as text
            PoRelease
                label is "Release"
                display as text
            PoCode
                label is "Code"
                display as text
            LineNbr
                label is "Line"
                display as text
        two column
            Location
                label is "ShipTo"
                display as text
            Location.Name
                label is "ShipToName"
                display as text
        two column
            Item
                display as text
            Item.Description
                label is "ItemDescription"
                display as text
        single column
            PurchaseOrderLineRel.ItemType
                display as text
        two column
            PurchaseOrderLineRel.Buyer
                display as text
            PurchaseOrderLineRel.Buyer.Name
                label is "BuyerName"
                display as text
        two column
            RequestingLocation
                label is "RequestingLocation"
                display as text
            RqLocRel.Name
                label is "RequestingLocationName"
                display as text
        header3 of "CostDetail"
        paragraph
            EntUnitCst
                label is "UnitCost"
                display as text
            PurchaseOrderLineRel.VendorPriceUOM
                no label
                display as text
            PurchaseOrderLineRel.PurchaseOrder.Currency
                label is "Currency"
                display as text
        paragraph
            PurchaseOrderLineRel.LandedUnitCost
                label is "LandedUnitCost"
                display as text
            PurchaseOrderLineRel.VendorPriceUOM
                no label
                display as text
        paragraph
            PurchaseOrderLineRel.ExtendedAmount
                label is "ExtendedLineAmount"
                display as text
        paragraph
            PurchaseOrderLineRel.CostOption
                display as text
            PurchaseOrderLineRel.TaxCode
                display as text
        paragraph
            PurchaseOrderLineRel.CostCode
                label is "CostSource"
                display as text
        paragraph
            CmContract
                label is "Contract"
                display as text
        header3 of "QuantityDetail"
        paragraph
            Quantity
                label is "OrderedQuantity"
                display as text
            PurchaseOrderLineRel.EnteredBuyUOM
                no label
                display as text
        paragraph
            PurchaseOrderLineRel.ReceivedQuantity
                display as text
        paragraph
            PurchaseOrderLineRel.MatchedQuantity
                display as text
        paragraph
            PurchaseOrderLineRel.SubstituteQuantity
                label is "SubstitutionQuantity"
                display as text
        paragraph
            PurchaseOrderLineRel.RejectedQuantity
                display as text
        paragraph
            PurchaseOrderLineRel.ArchivedQuantity
                display as text
        paragraph
            PurchaseOrderLineRel.MatchedNotReceivedQuantity
                display as text
        paragraph
            PurchaseOrderLineRel.CancelQuantity
                display as text
        paragraph
            PurchaseOrderLineRel.ChargebackQuantity
                display as text
        paragraph
            PurchaseOrderLineRel.WriteOffQuantity
                display as text
        paragraph
            PurchaseOrderLineRel.VendorPriceUOMQuantityToProcess
                label is "OrderedPriceQuantity"
                display as text
        paragraph
            PurchaseOrderLineRel.EarlyDeliveryDate
                label is "Deliver"
                display as text
        header3 of "VendorAndPurchaseFromDetail"
        two column
            Vendor
                display as text
            VendorRel.VendorName
                label is "VendorName"
                display as text
        two column
            PurchFrLoc
                label is "PurchaseFromLocation"
                display as text
            PurchaseOrderLineRel.PurchaseOrder.PurchaseFromLocation.VendorName
                label is "PurchaseFromLocationName"
                display as text
        header3 of "VendorItem"
        two column
            VendorItem
                label is "VendorItem"
                display as text
            PurchaseOrderLineRel.VendorItem.VendorItemDescription
                label is "VendorItemDescription"
                display as text
