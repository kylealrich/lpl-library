ReportGroupAgingReport is an ActionForm
    action is ReportGroupAgingReport
    Layout
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        paragraph
            ProcessLevel
                label is "ProcessLevelReportGroup"
                compact format
                select is ProcessLevelReportGroup
        header2 of "AgingOptions"
        two column distributed
            paragraph
                AcoAgeType
                    label is "AgingDateMethod"
            paragraph
                AcoAgeCredits
                    label is "AgeCredits"
        two column distributed
            paragraph
                AcoAgePymnt
                    label is "AgePayments"
            paragraph
                AcoAgeDisputes
                    label is "AgeDisputes"
        two column distributed
            paragraph
                TransDate
                    label is "As_ofDate"
            paragraph
                FutureApps
                    label is "QualifyFutureApplications"
        paragraph
            PeriodOf
                label is "Audit\\,Period"
        two column
            paragraph
                AgeCurrent
                    label is "CurrentDays"
            paragraph
                AgePeriods1
                    label is "Periods1\\-\\4"
                AgePeriods2
                    no label
                AgePeriods3
                    no label
                AgePeriods4
                    no label
        two column
            paragraph
                FutureDays
                    label is "FutureDays"
            paragraph
                AgePeriods5
                    label is "Periods5\\-\\7"
                AgePeriods6
                    no label
                AgePeriods7
                    no label
        paragraph
            XGainLoss
                label is "UnrealizedGain\\,Loss"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                ReportGroupAgingReportReport_RDG
            column2 of 25%
                ReportGroupAgingReportReport_XPT
