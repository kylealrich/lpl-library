CashLedgerReceiptImport_Translation is a Form
    is primary
    Layout
        paragraph
            Base_BusinessClass.CashLedgerReceiptImport
                display as text
            Base_BusinessClass.CashCodeDisplay
                no label
                display as text
            Base_BusinessClass.TransactionCodeDisplay
                no label
                display as text
            Base_BusinessClass.TransactionNumberDisplay
                no label
                display as text
            Base_BusinessClass.SequenceNumberDisplay
                no label
                display as text
        single column
            IsoLocale
                compact format
                raise yellow alert when (VerifyTranslations)
                    mouse over text is untranslatable:"The base data has been changed but this translation has not been verified"
        single column
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.Description
                label is untranslatable:"Description"
            Description
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.CustomerName
                label is untranslatable:"Customer Name"
            CustomerName
                label is untranslatable:"Translated"
