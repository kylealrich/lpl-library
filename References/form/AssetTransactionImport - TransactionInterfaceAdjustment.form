TransactionInterfaceAdjustment is a Form
    is primary
    title is "AssetTransactionInterface"
    Layout
        two column
            column1
                RunGroup
                FinanceEnterpriseGroup
                AssetTransactionImport.SequenceNumber
                    label is "SequenceNumber"
                paragraph
                    Book
                        select is BookSelectList
                    Book.Description
                        no label
                        display as text
                TransactionDate
                TransactionAmount
                BookAmount
                AssetAccountType
                    label is "AccountType"
                paragraph
                    AssetGroup
                        select is AssetGroups
                    AssetGroup.Description
                        no label
                        display as text
            column2
                paragraph
                    RecordInError
                        display as text
                    ErrorMessage
                        display as text
                AssetTransactionImport.ConversionNumber
                    label is "AssetImportGroup"
                Description
                paragraph
                    Company
                        select is Companies
                    Company.GeneralLedgerCompany.Name
                        no label
                        display as text
                PostingDate
                DerivedTransactionCurrency
                    label is "TransactionCurrency"
                DerivedBookCurrency
                    label is "BookCurrency"
                AssetProcess
                    label is "Process"
        paragraph
            AssetType
                compact format
                select is AssetTypesWithinCompany
            AssetType.Description
                no label
                display as text
        paragraph
            AssetAccountGroup
                select is AccountGroupsWithinCompany
            AssetAccountGroup.Description
                no label
                display as text
        paragraph
            DistributionAccount
                label is "TransactionDimensions"
                compact format
        paragraph
            AssetLease
                label is "Lease"
                compact format
            AssetLease.LeaseCompany.Name
                no label
                display as text
