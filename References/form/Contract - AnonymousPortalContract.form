AnonymousPortalContract is a SummaryForm
    title is "Contract<ContractNbr>"
    Layout
        single column
            Name
            Description
                use text area of 2 lines
            WorkingContractID
            SupplierSourceId.MainContact.FirstAndLastName
                label is "SupplierContact"
        single column
            CurrencyCode
        paragraph
            EffectiveDate
        paragraph
            ExpirationDate
        two column
            visible when (PrimaryContactExists)
                PrimaryContact
                    label is "PrimaryInternalContact"
        blank line
        visible when (TermsExist)
            header2 of "ContractTerms_andConditions"
            form is ContractTermsAndConditions
            header2 of "_"
        visible when (ContractAttachedContactExist)
            list is SupplierContactRel.PortalSummaryAttachedContactsPrint
        visible when (ContractComments)
            list is SupplierContractCommentsRel.PortalPrintComments
        list is ContractLine set.AllSupplierPrint
