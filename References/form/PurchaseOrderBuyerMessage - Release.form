Release is an ActionForm
    action is Unreleased.ReleaseAndCreateVendorReturn
    title is "ReleaseBuyerMessage"
    Layout
        paragraph
            ReturnQuantity
                display as text
            PurchaseOrderReceiptLine.ReceivedUOM
                no label
                display as text
        visible when (PurchaseOrderReceiptLine.IsCatchWeightItem)
            paragraph
                PrmCatchWeightReturnQuantity
                PurchaseOrderReceiptLine.Item.StockUOM
                    no label
                    display as text
        paragraph
            PrmVendorClaimType
                label is "ClaimType"
        paragraph
            PrmShipOrHold
                label is "ShipOrHold"
        paragraph
            PrmReferenceNumber
                label is "ReferenceNumber"
