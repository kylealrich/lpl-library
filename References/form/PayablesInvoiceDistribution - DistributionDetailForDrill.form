DistributionDetailForDrill is a Form
    title is "DistributionDetail"
    Layout
        paragraph
            Company
                label is "OriginatingCompany"
                display as text
        paragraph
            DistributionAccount
                label is "FinanceStructure"
                display as text
        paragraph
            DistributionAmount.CurrencyAmount
                label is "Amount"
                display as text
            InvoiceCurrency
                no label
                display as text
        paragraph
            UnitAmount
                display as text
        paragraph
            DistributionDate
                label is "PostingDate"
                display as text
        paragraph
            DistributionStatus
                label is "PostingStatus"
        paragraph
            PayablesInvoice.AccrualCode
                display as text
        paragraph
            DistributionReference
                label is "Reference"
                display as text
        paragraph
            Description
                display as text
