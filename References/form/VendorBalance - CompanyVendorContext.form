CompanyVendorContext is a Form
    Layout
        five column distributed
            column1
                Vendor.DerivedVendorNameID
                    display as text
                VendorGroup.RepresentativeText
                    label is "VendorGroup"
                    display as text
            column2
                Company.RepresentativeText
                    label is "Company"
                    display as text
                VendorLocation.RepresentativeText
                    label is "Location"
                    display as text
            column3
                display as text
                    paragraph
                        DerivedPeriod
                            label is "Period"
                        visible when (DerivedPeriodEndDate entered)
                            DerivedPeriodEndDate
                                label is "EndDate"
                EnteredPeriod
                    label is "DisplayPeriod"
                    mouse over text is "Update_Display_PeriodAndSaveToViewBalancesForADifferentPeriod."
            column4
                BalanceCurrency
                    label is "Currency"
                    display as text
                DraftBalance
                    label is "BillOfExchangeBalance"
                    display as text
            column5
                CurrentBalance
                    label is "CurrentBalance"
                    display as text
                HighestBalance
                    label is "HighBalance"
                    display as text
