AddBusinessClasses is an ActionForm
    action is AddBusinessClasses
    Layout
        text of "OnlyAuditedBusinessClassesWithDatabasePersistenceThatAreNotDeprecatedWillBeIncluded."
        two column
            column1
                Module
                OwnedBy
                NameIsLike
                visible when (not ExportFormat.COLUMNAR)
                    NamePrefix
                visible when (DeveloperMode)
                    MarkAsDelivered
            column2
                blank line
                blank line
                text of "LeaveBlankForAllOrEnterAValueToMatchOn"
                text of "ReplicationNamePrefixToAvoidDuplicateNames"
        two column
            visible when (not ExportFormat.COLUMNAR)
                column1
                    blank line
                    text of "AutomaticallyAddFields?"
                    AddPersistentFields
                    AddPersistentUserFields
                    blank line
                    AddPersistentFieldCubeDimensionValues
                    AddPersistentUserFieldCubeDimensionValues
                column2
                    blank line
                    blank line
                    ExplodeCompoundFields
                        mouse over text is "IfACompoundFieldAlreadyExistsUnexplodedItWillBeSkipped."
                    visible when (ExplodeCompoundFields)
                        UseSQLName
                    ReplacePeriodsWithUnderscores
