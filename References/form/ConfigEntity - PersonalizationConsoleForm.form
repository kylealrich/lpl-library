PersonalizationConsoleForm is a CompositeForm
    title is "Personalization"
    restrict action DisableDataTranslation
    restrict action EnableDataTranslation
    restrict action AddSubject
    restrict action RemoveSubject
    Edit is a Panel
        form is ConsoleDetailForm
    LPLHistory is a Panel
        list is ConfigEntityHistory set.ConsoleLPLChangesList
    StatusHistory is a Panel
        list is ConfigEntityHistory set.ConsoleStatusChangesList
    Tags is a Panel
        visible when (HasTags)
        list is TaggedItemsRel.TagOnlyList
