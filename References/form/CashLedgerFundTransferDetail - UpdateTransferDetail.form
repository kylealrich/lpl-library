UpdateTransferDetail is a Form
    Layout
        visible when (RecordExists)
            display as text
                paragraph
                    CashLedgerFundTransferDetail.DetailCashCode.CashCodeAndDescription
                        label is "<DerivedDetailTransactionTypeLabel>CashCode"
                    CashLedgerFundTransferDetail.DetailCashCode.Currency
                        no label
        paragraph
            DetailCompany
                label is "Company"
            DetailCompany.OrigCompany.Name
                no label
                display as text
            DetailCompany.OrigCompany.Currency
                no label
                display as text
        visible when (ShowDetailAccountingUnit)
            paragraph
                TransientAccountingUnit
                    label is "<DerivedDetailTransactionTypeLabel><DerivedTransientAccountingUnitLabel>"
                TransientAccountingUnit.Description
                    no label
                    display as text
        visible when (ShowDetailFund)
            paragraph
                DetailFund
                    label is "<DerivedDetailTransactionTypeLabel><DerivedDetailFund>"
                DetailFund.Description
                    no label
                    display as text
        paragraph
            CurrencyTable
            CurrencyTable.Description
                no label
                display as text
        paragraph
            DetailBankAmountUnsigned
                label is "DetailBankAmount"
            visible when (IsCurrencyExchangeOverrideAllowed)
                HeaderTranAmount
                    label is "HeaderBankAmount"
                CashCode.Currency
                    no label
                    display as text
            visible when (not IsCurrencyExchangeOverrideAllowed)
                HeaderTranAmountDisplayOnly
                    label is "HeaderBankAmount"
                CashCode.Currency
                    no label
                    display as text
