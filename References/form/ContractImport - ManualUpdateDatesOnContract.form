ManualUpdateDatesOnContract is an ActionForm
    action is UpdateDatesOnContract
    title is "UpdateDatesOnContractInterface"
    Layout
        display as text
            paragraph
                ContractGroup
            paragraph
                ContractImport
                Name
                    no label
            paragraph
                DerivedToContract
                    label is "ContractCreatedFromInterfaceContract"
            button of "LinesWithMismatchedDates"
                visible when (HasLinesWithMismatchedDates)
                link is LinesWithMismatchedDates
                display as link
        paragraph
            EffectiveDate
                label is "CurrentEffectiveDate"
                display as text
            NewEffectiveDate
        paragraph
            ExpirationDate
                label is "CurrentExpirationDate"
                display as text
            NewExpirationDate
        paragraph
            ContractReviewDate
                label is "CurrentReviewDate"
                display as text
            NewReviewDate
        paragraph
            LineUpdateOption
