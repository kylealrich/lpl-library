Main is a Form
    Layout
        header2 of "InterfacingStatements"
        two column distributed
            column1
                IntradayStatementProcessing
                AutomaticBankStatementImport
                ValidateImportedBankStatementBalance
            column2
                StatementCurrencyGainLossCategory
                    select is CashTransactionCategorySelect
                DisplayStatementCurrencyGainLossLine
        paragraph
            BankInterfaceAdministrator
            BankInterfaceAdministrator.PreferredSimplePresentationName
                no label
                display as text
        header2 of "PaymentCurrencyDifferenceGainLoss"
        paragraph
            CurrencyDifferenceGainAccount
            CurrencyDifferenceGainAccount.AccountDescription
                no label
                display as text
        paragraph
            CurrencyDifferenceLossAccount
            CurrencyDifferenceLossAccount.AccountDescription
                no label
                display as text
        header2 of "WireTransferFTPConfiguration"
        paragraph
            FTPConfiguration
            PfiConfigurationRel.Description
                no label
                display as text
        header2 of "InforTreasury"
        single column
            InforTreasuryIntegrationType
            InforTreasurySubscriptionKey
        header2 of "ApprovalRequiredFor"
        two column distributed
            column1
                CashForecastApprovalRequired
                    label is "CashForecasts"
                CashLedgerPaymentApprovalRequired
                    label is "CashLedgerPayments"
                CashLedgerTransactionApprovalRequired
                    label is "CashLedgerTransactions"
                CashLedgerEFTApprovalRequired
                    label is "CashLedgerEFT/CCExtracts"
                CashLedgerReturnedPaymentApprovalRequired
                    label is "CashLedgerReturnedPayments"
            column2
                BankStatementDistribApprovalRequired
                    label is "BankStatementDistributionsAndTransactions"
                BankStatementReconciliationApprovalRequired
                    label is "ReconciliationBalanceReport"
                VoidStopPaymentApprovalRequired
                    label is "VoidOrStopPayments"
                CashLedgerReceiptApprovalRequired
                    label is "CashReceipts"
                WireTransferApprovalRequired
                    label is "WireTransfers"
        header2 of "ReasonCodeRequiredWhen"
        two column distributed
            column1
                RejectCashForecastReasonCodeRequired
                    label is "RejectingCashForecasts"
                RejectCashLedgerPaymentReasonCodeRequired
                    label is "RejectingCashLedgerPayments"
                RejectLedgerTransactionReasonCodeRequired
                    label is "RejectingCashLedgerTransactions"
                RejectCashLedgerEFTReasonCodeRequired
                    label is "RejectingCashLedgerEFT/CCExtracts"
                RejectCashLedgerReturnedPaymentReasonCodeRequired
                    label is "RejectingReturnedPayments"
            column2
                RejectStatementDistribReasonCodeRequired
                    label is "RejectingBankStatementDistributions"
                RejectBankStatementReconReasonCodeRequired
                    label is "RejectingReconciliationBalanceReport"
                RejectVoidStopPaymentReasonCodeRequired
                    label is "RejectingVoidOrStopPayments"
                RejectCashLedgerReceiptReasonCodeRequired
                    label is "RejectingCashReceipts"
                RejectWireTransferReasonCodeRequired
                    label is "RejectingWireTransfers"
        header2 of "ApprovalCodesNotUsedFor"
        two column distributed
            column1
                ApprovalCodesNotUsedForCashForecasts
                    label is "CashForecasts"
                ApprovalCodesNotUsedForPayments
                    label is "CashLedgerPayments"
                ApprovalCodesNotUsedForCashTransactions
                    label is "CashLedgerTransactions"
                ApprovalCodesNotUsedForEFTTransactions
                    label is "CashLedgerEFT/CCExtracts"
                ApprovalCodesNotUsedForReturnedPayments
                    label is "CashLedgerReturnedPayments"
            column2
                ApprovalCodesNotUsedForBankStatementDist
                    label is "BankStatementDistributions"
                ApprovalCodesNotUsedForBankStatementReconciliation
                    label is "ReconciliationBalanceReport"
                ApprovalCodesNotUsedForVoidStopPayments
                    label is "VoidOrStopPayments"
                ApprovalCodesNotUsedForReceipts
                    label is "CashReceipts"
                ApprovalCodesNotUsedForWireTransfers
                    label is "WireTransfers"
        EnforceProcessorCannotBeApprover
            label is "EnforceProcessorCannotBeApprover"
        header2 of "AttachmentSize"
        three column grid
            column1
                text of "MaximumSize"
                    right align
                LogoImageMaximumSize
                    label is "FinancialInstitution"
            column2
                text of "UOM"
                    right align
                LogoImageUOM
                    no label
            column3
                text of "Bytes"
                    right align
                LogoImageMaximumSizeBytes
                    no label
                    display as text
