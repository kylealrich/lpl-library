Main is a Form
    Layout
        visible when (not RecordExists)
            two column distributed
                column1
                    paragraph
                        CashCode
                        CashCode.Description
                            no label
                            display as text
                column2
                    paragraph
                        CashLedgerPayment.BankTransactionCode
                            select is CashPayments
                        CashLedgerPayment.BankTransactionCode.Description
                            no label
                            display as text
                        CashLedgerTransactionIdentifier
                            display as text
        two column distributed
            column1
                Reference
                    label is "ReferenceNumber"
                BankCheckAmountUnsigned
                    label is "Amount"
                paragraph
                    PayablesData.PayablesCompany
                        select is Company
                    PayablesData.PayablesCompany.Name
                        no label
                        display as text
                paragraph
                    PayablesData.Vendor
                        label is "Vendor"
                    PayablesData.Vendor.VendorName
                        no label
                        display as text
                paragraph
                    CompanyBaseData.OverrideCompany
                        label is "OverrideCompany"
                        select is Company
                    CompanyBaseData.OverrideCompany.Name
                        no label
                        display as text
                    CompanyBaseData.OverrideCompany.Currency
                        no label
                        display as text
                paragraph
                    BankAmount
                    BankCurrency
                PostDate
            column2
                paragraph
                    ProcessLevel
                        label is "PayablesProcessLevel"
                        select is APProcessLevels
                    ProcessLevel.Name
                        no label
                        display as text
                paragraph
                    PayablesData.PayGroup
                        label is "PayGroup"
                        select is PayGroups
                    PayablesData.PayGroup.PostCompany
                        label is "PayGroupPostingCompany"
                        display as text
                paragraph
                    CurrencyTable
                    CurrencyTable.Description
                        no label
                        display as text
                Description
                paragraph
                    visible when (HasAttachment)
                        PaymentAttachment.Title
                    PaymentAttachment.File
                paragraph
                    ReissueOriginalReference
                    ReissueOriginalIdentifier
                paragraph
                    ApprovalCostCenter
                    ApprovalCategory
                paragraph
                    visible when (not CanApplyAdditionalDistributions)
                        UseCashAllocationCode
                            label is "ApplyCashAllocationCode"
                visible when (UseCashAllocationCode)
                    paragraph
                        TransientCashAllocationCode
                            label is "CashAllocationCode"
                    paragraph
                        TransientCashAllocationAmount
                            label is "CashAllocationAmount"
                    paragraph
                        TransientCashAllocationUnitsAmount
                            label is "UnitsAmount"
        single column
            PaymentProcessingHold
