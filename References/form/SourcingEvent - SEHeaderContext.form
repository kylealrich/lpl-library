SEHeaderContext is a Form
    Layout
        five column distributed
            column1
                visible when (not RecordExists)
                    Company
                visible when (RecordExists)
                    Company.RepresentativeText
                        display as text
            column2
                paragraph
                    SourcingEvent
                        label is "Event#"
                        display as text
                    LastVersionNumber
                        label is "Version"
                        display as text
            column3
                Name
            column4
                DerivedStatus
                    label is "Status"
                    display as tag
                        color of brown when (Status.Draft)
                        color of green when (Status.Open)
                        color of red when (Status.Cancelled)
                        color of gray when (Status.Closed)
                        color of yellow when (Status.Suspended)
            column5
                visible when (DisplayIDMBidTabulation)
                    button of "View"
                        link is external "<DerivedIDMDocumentLink>"
                            lazy url retrieval
                        label is "BidTabulation"
                        display as link
                visible when (HasAwardDetailsIDMDocument)
                    button of "View"
                        link is external "<DerivedIDMAwardDetailsDocumentLink>"
                            lazy url retrieval
                        label is "AwardDetails"
                        display as link
