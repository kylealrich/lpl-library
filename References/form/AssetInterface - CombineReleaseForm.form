CombineReleaseForm is an ActionForm
    action is CombineRelease
    title is "Combine"
    Layout
        header3 of "SelectInterfaceRecords"
        paragraph
            FinanceEnterpriseGroup
                display as text
        paragraph
            PrmCompany
                label is "Company"
            PrmCompany.GeneralLedgerCompany.Name
                no label
                display as text
        paragraph
            PrmAssetTemplate
                label is "AssetTemplate"
                select is ValidTemplatesRel.AssetTemplates
            PrmAssetTemplate.Description
                no label
                display as text
        paragraph
            PrmCombine
                label is "Combine"
            ComputeOption
        blank line
        header3 of "CombineOverrides"
        paragraph
            OvrdAssetDescription
                label is "AssetDescription"
        paragraph
            OvrdAssetGroup
                label is "AssetGroup"
            OvrdAssetGroup.Description
                no label
                display as text
        two column
            OvrdCurrency
                label is "Currency"
            OvrdTagNumber
                label is "TagNumber"
        paragraph
            OvrdAssetType
                label is "AssetType"
                compact format
            OvrdAssetType.Description
                no label
                display as text
        paragraph
            OvrdAssetCategory
                label is "Category"
        paragraph
            OvrdAssetAccountGroup
                label is "AccountGroup"
            OvrdAssetAccountGroup.Description
                no label
                display as text
        paragraph
            OvrdAssetLocation
                label is "Location"
            OvrdAssetLocation.Description
                no label
                display as text
        paragraph
            OvrdAssetDivision
                label is "Division"
            OvrdAssetDivision.Description
                no label
                display as text
        paragraph
            OvrdAssetAccountingUnitGroup
                label is "AssetDimensionGroup"
            OvrdAssetAccountingUnitGroup.Description
                no label
                display as text
        paragraph
            visible when (GuidelineClassesExists)
                OvrdAssetGuidelineClass
                    label is "GuidelineClass"
                OvrdAssetGuidelineClass.Description
                    no label
                    display as text
        single column
            OvrdAssetAccountingUnit
                label is "DefaultTransactionDimensions"
                compact format
        paragraph
            OvrdDepreciationExpenseProject
                label is "DepreciationExpenseProject"
            OvrdDepreciationExpenseProject.Project.Description
                no label
                display as text
        paragraph
            OvrdInServiceDate
                label is "InServiceDate"
        paragraph
            OvrdWorkInProcess
                label is "WorkInProcess"
            OvrdTaxExempt
                label is "TaxExempt"
        paragraph
            OvrdSimulated
                label is "Simulated"
            OvrdUsed
                label is "Used"
