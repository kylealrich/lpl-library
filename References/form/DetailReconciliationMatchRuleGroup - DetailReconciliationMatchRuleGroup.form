DetailReconciliationMatchRuleGroup is a CompositeForm
    is primary
    title is "DetailReconciliationMatchRuleGroup"
    context form is DetailReconciliationMatchRuleGroupContext
        background is theme color
    ReconciliationMatchRules is a Panel
        list is DetailReconciliationMatchRuleGroupDetail set.DetailReconciliationMatchRuleGroupDetails
            helper list is DetailReconciliationMatchRuleRel.DetailReconciliationMatchRulesHelper
                action is AddMatchRule
                    invoked.PrmReconciliationManagementGroup = ReconciliationManagementGroup
                    invoked.PrmDetailReconciliationMatchRuleGroup = DetailReconciliationMatchRuleGroup
