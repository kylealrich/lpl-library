EFTPaymentCreationResultForm is a CompositeForm
    is primary
    title is "EFTPaymentCreationResult"
    context form is EFTPaymentCreationContext
        background is theme color
    EFTTransactions is a Panel
        title is "Paid"
        list is EFTransactionRel.PaymentCreationList
    EFTSummarizeTransactions is a Panel
        title is "Paid-SummarizedDetail"
        visible when (TransientIncludeSummarizeDetailPanel)
        list is EFTransactionRel.SummarizeList
    EFTPaymentOutput is a Panel
        visible when (IsEFTPaymentOutputCreated)
        list is EFTPaymentOutputRel.EFTPayCreationList
    Totals is a Panel
        visible when (IsEFTPaymentOutputCreated)
        form is Totals
