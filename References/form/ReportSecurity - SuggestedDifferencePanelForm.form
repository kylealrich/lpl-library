SuggestedDifferencePanelForm is a Form
    title is "Current_vs_Suggested"
    Layout
        two column distributed
            column1
                paragraph
                    SuggestionMsg
                        no label
                        display as text
                        raise blue alert when (not HasSuggestion)
                            mouse over text is "NoDifferences"
            column2
                paragraph
                    button of "Configure"
                        action is Configure
                        right align
                        is primary
                    visible when (HasSuggestion)
                        button of "ResolveDifferences"
                            action is EditWithSuggestedComparison
                            right align
                        button of "AcceptSuggestion"
                            action is AcceptSuggested
                            right align
                    visible when (BasedOnDelivered)
                        button of "MarkReviewed"
                            action is MarkReviewed
                            right align
        visible when (HasSuggestion)
            six column distributed
                column2
                    text of "Current"
                        header3
                        bold
                column5
                    paragraph
                        text of "Suggested"
                            header3
                            bold
            single column
                Suggested
                    no label
                    display only
                    use text area of 30 lines
                    compare with ConfigurationCompare
