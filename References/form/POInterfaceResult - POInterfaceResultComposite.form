POInterfaceResultComposite is a CompositeForm
    is primary
    title is "ProcessPurchaseRequestResult"
    context form is POInterfaceResultContext
        background is theme color
    ProcessedRequests is a Panel
        visible when (HasProcessed)
        list is ProcessedPOInterfaceResultDetailRel.PurchaseOrderList
    RejectedRequests is a Panel
        visible when (HasRejected)
        list is RejectedPOInterfaceResultDetailRel.RequestsRejectedList
    PurchaseOrderErrors is a Panel
        visible when (HasError)
        list is ErrorPOInterfaceResultDetailRel.ErrorList
    RunSummary is a Panel
        visible when (RunComplete)
        list is SummaryPOInterfaceResultDetailRel.RunSummaryList
    ParametersAndFilters is a Panel
        form is ParameterForm
