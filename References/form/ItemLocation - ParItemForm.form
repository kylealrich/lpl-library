ParItemForm is a Form
    Layout
        header2 of "ParItem"
        two column distributed
            column1
                BinSequence
                TransientShelfLocation
                    label is "ShelfLocation"
                ReorderPoint
                    label is "ParLevel"
                DefaultTransactionUOM.UnitOfMeasure
                    raise yellow alert when (InvalidOnLocationItemUOM)
                        mouse over text is "<DerivedInvalidUOMText>"
                GeneralLedgerCategory
                    label is "InventoryLocationAccountGroup"
                paragraph
                    PatientChargeable
                    NoCharge
                    Active
                visible when (false)
                    TransientOnline
                    Vendor
                    PurchaseFromLocation
                    ReplenishFrom
            column2
                MinimumOrderQuantity
                OrderMultiple
                LeadtimeDays
                SuggestedReorderPoint
                    label is "SuggestedParLevel"
                paragraph
                    StandardCost
                    Company.GeneralLedgerCompany.Currency
                        no label
                        display as text
                paragraph
                    Manufacturer
                        compact format
                    Manufacturer.Description
                        no label
                        display as text
                ManufacturerNumber
                NumberOfBins
                    display as text
