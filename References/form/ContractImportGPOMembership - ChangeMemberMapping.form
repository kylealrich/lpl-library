ChangeMemberMapping is an ActionForm
    action is ChangeMemberMapping
    Layout
        display as text
            single column
                ParmContractGroup
                    label is "ContractGroup"
                paragraph
                    ContractImportGPOMembership
                        label is "MembershipID"
                    visible when (not ContractGroup.GPOIsHealthTrust2021)
                        Name
                text of "CurrentMapping"
                    bold
                ParticipantLocation.Company
                ParticipantLocation.Location
                ParticipantLocation.RequestingLocation
                ParticipantLocation.PricingGroup
        single column
            text of "NewMapping"
                bold
            ParmNewMapping.Company
                label is "Company"
            ParmNewMapping.Location
                label is "Location"
            ParmNewMapping.RequestingLocation
                label is "RequestingLocation"
            ParmNewMapping.PricingGroup
                label is "PricingGroup"
            UpdateExistingContracts
