POVendorForm is a CompositeForm
    is primary
    title is "Vendor<Vendor>PurchasingInformation"
    context form is POVendorFormContext
        background is theme color
    Main is a Panel
        form is Main
    Options is a Panel
        form is Options
    ProcedureOutput is a Panel
        form is ProcedureInfoOutput
    PurchaseFromLocations is a Panel
        list is PurchaseFromLocationsRel.POPurchaseFromLocations
    CrossReferenceVendor is a Panel
        list is CrossReferenceVendorRel.CrossReferenceVendorDetailList
    Attachments is a Panel
        title is "Attachments<DerivedPurchasingVendorAttachmentCount>"
        list is PurchasingVendorAttachment set.Attachments
    VendorAccounts is a Panel
        list is PurchasingVendorAccountsRel.PurchasingVendorAccountsForVendor
    DocumentTemplate is a Panel
        form is PurchaseOrderDocumentTemplate
