EFTPaymentCreation is an ActionForm
    action is PerformEFTPaymentCreation
    Layout
        header2 of "Main"
        paragraph
            EftCompany
                label is "E\\F\\TProcessingCompany"
            EftCompany.Name
                no label
                display as text
        single column
            PostingDate
                label is "PostDate"
            DepositDate
        paragraph
            DebitCashCode
            DebitCashCode.Description
                no label
                display as text
        paragraph
            CreditCashCode
            CreditCashCode.Description
                no label
                display as text
        paragraph
            ProcessLevel
                label is "PaymentProcessLevel"
            ProcessLevel.Name
                no label
                display as text
            BankTransactionCode
                label is "SelectPaymentCode"
                select is CashPaymentAndOriginReceivablesList
            BankTransactionCode.Description
                no label
                display as text
        paragraph
            SelectProcessLevel
            SelectProcessLevel.Name
                no label
                display as text
            TermsCode
                label is "SelectPaymentTerms"
            TermsCode.Description.Desc[1]
                no label
                display as text
        single column
            NotAppldCd
                label is "ReceivedNotAppliedCode"
            DiscReasCd
                label is "DiscountReasonCode"
                select is ReceivableReasonCodeDiscountList
            OprCode
                label is "Operator"
            Reference
            CompanyIdOption
                label is "CompanyI\\DOption"
        paragraph
            FillLastBlock
                label is "FillA\\C\\HLastPhysicalBlock"
            BalancedTape
                label is "BalancedA\\C\\HTape"
            IncludeAddenda
                label is "IncludeA\\C\\HAddenda"
        header2 of "OptionalBankCharges"
        single column
            BankCharges
        single column
            GLAccount
                label is "GlobalLedgerAccount"
                compact format
        header2 of "UserFields"
        single column
            CbUserField1
                label is "CashLedgerUserField1"
            CbUserField2
                label is "CashLedgerUserField2"
            CbUserField3
                label is "CashLedgerUserField3"
            CbUserField4
                label is "CashLedgerUserField4"
        header2 of "FileNames"
        single column
            ACHFileName
            BACSFileName
            NonTapeFileName
            SEPAFileName
