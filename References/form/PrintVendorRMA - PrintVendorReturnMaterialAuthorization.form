PrintVendorReturnMaterialAuthorization is an ActionForm
    action is PrintVendorRMA
    Layout
        two column
            Company
            Company.Name
                no label
                display as text
        two column
            ReturnFrmLoc
                label is "ReturnFromLocation"
            ReturnFrmLoc.Name
                no label
                display as text
        two column
            Vendor
            Vendor.VendorName
                no label
                display as text
        three column
            ReturnNumber.ReturnNumber[1]
                label is "ReturnNumber"
            ReturnNumber.ReturnNumber[2]
                no label
            ReturnNumber.ReturnNumber[3]
                no label
        three column
            ReturnNumber.ReturnNumber[4]
                no label
            ReturnNumber.ReturnNumber[5]
                no label
            ReturnNumber.ReturnNumber[6]
                no label
        single column
            PrintGtinOption
                label is "PrintG\\T\\I\\N"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                PrintVendorRMAReport_RDG
            column2 of 25%
                PrintVendorRMAReport_XPT
