PhoneEFTCCDistributionForm is a Form
    title is "EFT/CCExtractEntryDistribution"
    Layout
        header2 of "Ledger"
        display as text
            paragraph distributed
                GLTransactionAmount
                    label is "Amount"
                CashLedgerElectronicFundsTransferTransactionRel.Company.OrigCompany.RepresentativeText
                    label is "Company"
            single column
                GLFinanceCodeBlock
                    label is "FinanceStructure"
                    compact format only
            two column distributed
                Reference
                Description
            header2 of "Tax"
            paragraph distributed
                TaxCode
                TaxAccountingEntity
                    label is "TaxEntity"
                    select is TaxEntitiesRel.TaxCompanies
                TaxableBankAmount
                    label is "TaxableAmount"
