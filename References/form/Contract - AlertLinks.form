AlertLinks is a Form
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        two column distributed
            column1
                visible when (ShowErrorLinks)
                    header2 of "ItemsNeedingAttention"
                button of "ContractLineErrors"
                    visible when (ContractLineErrorsExistNotInactiveVendorItem)
                    link is ContractLineErrorLink
                    display as link
                button of "OpenInvoiceMessages"
                    visible when (HasOpenMatchInvoiceMessage)
                    link is OpenInvoiceMessageNav
                    display as link
                button of "QuestionsWhichNeedAttention"
                    visible when (QuestionsRequireAttention)
                    link is QuestionsLink
                    display as link
                button of "UnsupportedTerm_andConditionAttachments_file_type"
                    visible when (UnsupportedIDMAttachmentsExist)
                    link is UnsupportedContractTermAndConditionAttachmentsNav
                    display as link
                button of "CapitatedComponentsWhichNeedAFinalCost"
                    visible when (HasCapitatedComponentItemsWithNoFinalCost)
                    link is CapitatedComponentsWithNoCostNav
                    display as link
                button of "CapitatedCompanyLocationComponentsWhichNeedAFinalCost"
                    visible when (HasCapitatedCompanyLocationItemsWithNoFinalCost)
                    link is CapitatedCompanyLocationComponentsWithNoCostNav
                    display as link
            column2
                visible when (ShowAdditionalInformationLinks)
                    header2 of "AdditionalInformation"
                button of "InactiveItems"
                    visible when (HasInactiveVendorItems)
                    link is InactiveVendorItemLink
                    display as link
                button of "ResolvedInvoiceMessages"
                    visible when (HasResolvedMatchInvoiceMessage)
                    link is ResolvedInvoiceMessageNav
                    display as link
                button of "UnreleasedPOLines"
                    visible when (HasUnreleasedPOLines)
                    link is UnreleasedPOLinesNav
                    display as link
                button of "UnreleasedRequisitionLines"
                    visible when (HasUnreleasedReqLines)
                    link is UnreleasedReqLinesNav
                    display as link
                button of "UnprocessedPurchaseRequests"
                    visible when (HasUnprocessedPOInterface)
                    link is UnprocessedRequestsNav
                    display as link
                button of "ContractCostComparison"
                    visible when (CostComparisonExists)
                    link is CostComparisonLink
                    display as link
                button of "BidWorksheets"
                    visible when (BidWorksheetExists)
                    link is BidWorksheetLink
                    display as link
                button of "CostSavings"
                    visible when (CostSavingsExists)
                    link is CostSavingsLink
                    display as link
                button of "UnresolvedSuggestedLineChanges"
                    visible when (UnresolvedSuggestedLineItemsExist)
                    link is UnresolvedSuggestedItemsLink
                    display as link
                button of "ResolvedSuggestedLineChanges"
                    visible when (ResolvedSuggestedLineItemsExist)
                    link is ResolvedSuggestedItemsLink
                    display as link
