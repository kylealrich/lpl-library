BookDisposalWizard is a Form
    title is "AssetBookDisposal"
    restrict action Create
    restrict action Update
    restrict action Delete
    Layout
        paragraph
            Asset
                display as text
            Asset.Description
                no label
                display as text
            Book
        single column
            blank line
            AssetBookRel.PostingCurrency
                label is "Currency"
            ProceedsOverride
                label is "Proceeds"
            CostOfSaleOverride
                label is "CostOfSale"
            DepreciationExpenseOverride
                label is "DepreciationExpense"
            GainOrLoss
            DeferredGainOrLoss
