FinancialInstitutionBranchContactForm is a Form
    is primary
    title is "Contact"
    Layout
        paragraph
            FinancialInstitution
                display as text
            FinancialInstitutionBranch
                display as text
            FinancialInstitutionBranch.Description
                no label
                display as text
        paragraph
            ContactInformation.PersonName.GivenName
                label is "*FirstName"
            ContactInformation.PersonName.FamilyName
                label is "*LastName"
        single column
            ContactInformation.ContactTitle
                label is "Title"
            AddressDifferentFromBranchAddress
            visible when (AddressDifferentFromBranchAddress)
                header2 of "Address"
                single column
                ContactInformation.ContactInfo.PostalAddress
        header2 of "ContactInformation"
        paragraph
            ContactInformation.ContactInfo.TelephoneNumber.InternationalPrefix
                label is "PhoneNumber"
            ContactInformation.ContactInfo.TelephoneNumber.SubscriberNumber
                no label
            ContactInformation.ContactInfo.TelephoneNumber.Extension
                label is "Ext"
            text of "(InternationalPrefix,PhoneNumber,Extension)"
                italics
        paragraph
            ContactInformation.ContactInfo.FaxNumber.InternationalPrefix
                label is "FaxNumber"
            ContactInformation.ContactInfo.FaxNumber.SubscriberNumber
                no label
            ContactInformation.ContactInfo.FaxNumber.Extension
                label is "Ext"
            text of "(InternationalPrefix,FaxNumber,Extension)"
                italics
        paragraph
            ContactInformation.ContactInfo.EmailAddress
                label is "EmailAddress"
        paragraph
            visible when (HasAttachment)
                Attachment.Title
            Attachment.File
        paragraph
            Comments
