FileCreationSetupDetailForm is a Form
    is primary
    title is "Setup"
    Layout
        single column
            FinanceEnterpriseGroup
                display as text
            Description
        valid when (MappingsExist)
            single column
                Active
        single column
            ExportFile
        visible when (PayablesInvoiceType)
            single column
                PayablesInvoiceGroupSelection
        visible when (GeneralLedgerTransactionType)
            single column
                GeneralLedgerTransactionGroupSelection
        visible when (PurchaseOrderLineType)
            single column
                PurchaseOrderLineGroupSelection
        visible when (InventoryLocationType)
            single column
                InventoryLocationGroupSelection
        visible when (InventoryTransactionLineType)
            single column
                InventoryTransactionLineGroupSelection
        visible when (GeneralLedgerTotalType)
            single column
                GeneralLedgerTotalGroupSelection
        visible when (GLTransactionDetailType)
            single column
                GLTransactionDetailGroupSelection
        visible when (VendorType)
            single column
                VendorGroupSelection
        visible when (AssetType)
            single column
                AssetGroupSelection
        visible when (ScenarioType)
            single column
                ScenarioGroupSelection
        visible when (GeneralLedgerChartAccountType)
            single column
                GeneralLedgerChartAccountGroupSelection
        visible when (BinGroupType)
            single column
                BinGroupGroupSelection
        visible when (ContractType)
            single column
                ContractGroupSelection
        visible when (ItemType)
            single column
                ItemGroupSelection
        visible when (RequesterRequestingLocationType)
            single column
                RequesterRequestingLocationGroupSelection
        visible when (PurchaseOrderType)
            single column
                PurchaseOrderGroupSelection
        visible when (BillingInvoiceType)
            single column
                BillingInvoiceGroupSelection
        visible when (CustomerOrderType)
            single column
                CustomerOrderGroupSelection
        visible when (GeneralLedgerJournalControlType)
            single column
                GeneralLedgerJournalControlGroupSelection
        visible when (IntercompanyBillingSettlementHeaderType)
            single column
                IntercompanyBillingSettlementHeaderGroupSelection
        visible when (IntercompanyBillingInvoiceType)
            single column
                IntercompanyBillingInvoiceGroupSelection
        visible when (IntercompanyBillingDocumentHeaderType)
            single column
                IntercompanyBillingDocumentHeaderGroupSelection
        visible when (AccountingEntityType)
            single column
                AccountingEntityGroupSelection
        visible when (CashLedgerTransactionType)
            single column
                CashLedgerTransactionGroupSelection
        visible when (AssetTransactionSummaryType)
            single column
                AssetTransactionSummaryGroupSelection
        visible when (AssetBookHistoryType)
            single column
                AssetBookHistoryGroupSelection
        visible when (AssetBookPeriodHistoryType)
            single column
                AssetBookPeriodHistoryGroupSelection
        visible when (ReceivablePaymentType)
            single column
                ReceivablePaymentGroupSelection
        visible when (ReceivableInvoiceType)
            single column
                ReceivableInvoiceGroupSelection
        visible when (ReceivableGLDistributionType)
            single column
                ReceivableGLDistributionGroupSelection
        visible when (CompanyCustomerType)
            single column
                CompanyCustomerGroupSelection
        visible when (ReceivableApplicationType)
            single column
                ReceivableApplicationGroupSelection
        visible when (NationalAccountBalanceType)
            single column
                NationalAccountBalanceGroupSelection
        visible when (ElectronicFundsTransferTransactionType)
            single column
                ElectronicFundsTransferTransactionGroupSelection
        visible when (ApplicationBusinessClass)
            paragraph
                AllRecords
                valid when (not AllRecords)
                    LastTimeStamp
        single column
            TurnOffRecordAuditing
        header3 of "FileAttributes"
        single column
            FileType
        paragraph
            visible when (FileType.Separated)
                FieldSeparator
                valid when (FieldSeparatorEntered)
                    IncludeSeparatorAtBeginningOfRow
        single column
            visible when (FileType.Separated)
                FieldDelimiter
            FileSegmentLimit
            visible when (FileSegmentLimit.NumberOfRecords)
                NumberOfRecordsBeforeNewFile
            visible when (FileSegmentLimit.NumberOfMegabytes)
                NumberOfMegabytesBeforeNewFile
            GenerationBehaviorForNoOutput
                when value changed
                    refresh GenerateBlankDocument
            visible when (GenerateBlankDocument)
                OutputTextForBlankDocument
            ThreadingMethod
        header3 of "FileName"
        paragraph
            FileName
            AppendTimestampToFileName
        paragraph
            FileNameTimeStampFormat
            button of "BuildTimeStampFormat"
                valid when (RecordExists)
                action is BuildTimeStampFormat
        visible when (DisplayFileProcessing)
            header3 of "FileProcessing"
            single column
                FlowToProcessFile
                AutomaticallySendFileToION
                visible when (AutomaticallySendFileToION)
                    IONDocumentName
                    IONInstanceCount
                    paragraph
                        IONDocumentID
                        text of "File_NameWillBeUsedIfLeftBlank"
                    paragraph
                        IONSource
                        text of "File_NameWillBeUsedIfLeftBlank"
        header3 of "FileHeaderText"
        single column
            FileHeaderText
                no label
        visible when (UseForTesting)
            header3 of "TestingParameters"
            single column
                DoNotIncludeHeader
                DoNotIncludeTrailer
                header4 of "FailureNotifications"
                TestingEmailFrom
            paragraph
                TestingEmailTo
                    display as text
                button of "AddUser"
                    action is AddEmailForTesting
                button of "Clear"
                    action is ClearEmail
