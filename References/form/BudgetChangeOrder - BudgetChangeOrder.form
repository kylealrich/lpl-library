BudgetChangeOrder is a CompositeForm
    is primary
    title is "<ChangeRequestTitle>"
    context form is Main
        background is theme color
    Actions
        action is Unreleased.Release
            hide when invalid
        action is PendingApproval.ManualReject
            hide when invalid
        action is PendingApproval.ManualApprove
            hide when invalid
        action is CreateUsingWizard
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is save
            action icon is save
            label is "_"
            hide when invalid
        action is save and new
            label is "_"
            hide when invalid
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
            mouse over text is "Refresh"
    Lines is a Panel
        list is BudgetChangeOrderLine set.Lines
    Comments is a Panel
        list is BudgetChangeOrderComment set.CommentsForChangeOrder
    TemplateUpdates is a Panel
        title is "UnreleasedObligations"
        visible when (HasTemplateUpdates)
        list is BudgetTemplateGroupTotalsRel.BudgetTemplateGroupTotalsForBudgetChange
    EditErrors is a Panel
        visible when (HasBudgetErrors)
        list is BudgetEditErrorRel.BudgetEditErrorsForBudgetChange
