Invoice is a CompositeForm
    is primary
    title is "MatchInterfaceInvoice"
    context form is InvoiceContext
    Actions
        navigation is PayablesInvoiceNavigation
            action icon is preview
            label is "ViewInterfacedInvoice"
            hide when invalid
        navigation is InterfaceResultsNavigation
            action icon is undo
            label is "ReturnToResults"
            hide when invalid
            valid when (InterfaceRunEntered)
        action is ResetErrorMessage
            action icon is finalize
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is Update
        action is save
            label is "_"
            mouse over text is "Save"
        action is save and new
            label is "_"
            mouse over text is "SaveAnd_New"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
            mouse over text is "Refresh"
    Main is a Panel
        form is Main
    Additional is a Panel
        form is Additional
    MiscellaneousAOC is a Panel
        title is "MiscCharges<NumberOfMiscellaneousAOCsTitle>"
        list is MiscPayablesInvoiceAddOnChargeImportRel.MiscellaneousAddonChargeLineDetails
    AdditionalPurchaseOrders is a Panel
        title is "AdditionalPurchaseOrders<NumberOfPOsTitle>"
        list is MatchPurchaseOrderInvoiceImportRel.PurchaseOrders
    AdditionalServiceContracts is a Panel
        visible when (ContractEntered)
        list is MatchServiceContractInvoiceImportRel.Contracts
    InvoiceDetails is a Panel
        title is "Details<NumberOfDetailsTitle>"
        list is PayablesInvoiceDetailImportRel.InvoiceDetails
    Distributions is a Panel
        title is "Distributions<NumberOfDistributionsTitle>"
        list is MatchInvoiceDistributionImportRel.Distributions
    Global is a Panel
        form is Global
    Rebate is a Panel
        form is Rebate
    Comments is a Panel
        title is "Comments<NumberOfCommentsTitle>"
        list is MatchInvoiceImportCommentRel.Comment
