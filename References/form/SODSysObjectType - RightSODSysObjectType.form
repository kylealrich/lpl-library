RightSODSysObjectType is an ActionForm
    action is CreateRightCondition
    title is "SegregationOfDutiesObjectType"
    Layout
        three column
            SODSysTemplate
            SODSysObjectType.SecurableObjectName
                display only
            SODSysObjectType.SecurableObjectAction
                display only
        three column
            Conjunction
            SODSysObjectType.SecurableObjectType
                when value changed
                    BusinessViewName =  blank
                        when (BusinessViewName entered)
        single column
            visible when (SODSysObjectType.SecurableObjectType entered)
                BusinessViewName
                    label is SODSysObjectType.SecurableObjectType
                    select is BusinessViewRel.BusinessViewList
                    when value changed
                        BusinessViewAction =  blank
                            when (BusinessViewAction entered)
                        CommonAction =  blank
                            when (CommonAction entered)
        two column
            CommonAction
                when value changed
                    BusinessViewAction =  blank
                        when (BusinessViewAction entered)
            BusinessViewAction
                when value changed
                    CommonAction =  blank
                        when (CommonAction entered)
