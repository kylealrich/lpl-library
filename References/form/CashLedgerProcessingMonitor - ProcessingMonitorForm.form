ProcessingMonitorForm is a Form
    is primary
    title is "CashLedgerProcessingMonitor"
    Layout
        single column
            CashLedgerProcessingMonitor.RunProgram
                label is "Program"
            Status
                label is "Status"
            CashLedgerProcessingMonitor.CashCode
                label is "CashCode"
            CashLedgerProcessingMonitor.CashLedgerProcessGroup
                label is "CashLedgerProcessGroup"
            CreateDate
                display as text
