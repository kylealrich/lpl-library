InterfaceCashLedgerTransactions is an ActionForm
    action is InterfaceCashLedgerTransactions
    Layout
        single column
            PrmCashManagementGroup
                label is "CashManagementGroup"
                display as text
            PrmRunGroup
                label is "RunGroup"
            PrmMoveErrorsToNewRunGroup
                label is "MoveErrorsToNewRunGroup"
            visible when (PrmMoveErrorsToNewRunGroup)
                PrmErrorRunGroupPrefix
                    label is "ErrorRunGroupPrefix"
            PrmCurrencyTable
                label is "CurrencyTable"
            PrmJournalizeDistributions
            visible when (PrmCashManagementGroup.AutoTranNumberForTransactionForm)
                PrmAutoNumberingAction
                    label is "AutoTransactionNumberingAction"
            PrmKeepEnteredSignForBankCredits
