AssetTransactionSearch is a SearchForm
    title is "TransactionFilter"
    Layout
        four column distributed
            column1
                Company
                Book
                DistributionAccount.GeneralLedgerChartAccount
                    label is "Account"
                AssetProcess
            column2
                Asset
                DistributionAccount.Ledger
                DistributionAccount.Project
                AssetAccountType
                    label is "AccountType"
            column3
                TransactionDate
                DistributionAccount.ToAccountingEntity
                    label is "AccountingEntity"
                DistributionAccount.FinanceDimension1
                    label is "Fund"
                BatchUpdateResult
            column4
                PostingDate
                DistributionAccount.AccountingUnit
                DistributionAccount.FinanceDimension2
                    label is "ProjectFunding"
