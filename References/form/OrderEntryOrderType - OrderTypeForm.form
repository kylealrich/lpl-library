OrderTypeForm is a Form
    is primary
    title is "<OrderEntryOrderTypeFormTitle>"
    Layout
        three column distributed
            column1
                paragraph
                    Company
                    Company.BillingCompany.Name
                        no label
                        display as text
            column2
                paragraph
                    OrderEntryOrderType
                        label is "OrderType"
                    Description
                        label is "Description"
                        show up to 16 characters
            column3
                Active
        header2 of "ProcessingOptions"
        three column distributed
            column1
                paragraph
                    WarehouseProcessType
                        label is "ProcessType"
                        select is ProcessTypes
                    WarehouseProcessType.Description
                        no label
                        display as text
            column2
                paragraph
                    BillingInvoiceType
                        label is "InvoiceType"
                        select is InvoiceTypes
                    BillingInvoiceType.Description
                        no label
                        display as text
            column3
                RushOrder
                    label is "RushOrder"
        header2 of "LineDefaults"
        three column distributed
            column1
                CreatePurchaseOrder
                    label is "CreatePurchaseOrder"
            column2
                Dropship
                    label is "DropShipment"
        header2 of "ShipImmediateOptions"
        three column distributed
            column1
                ShipImmediate
                    label is "ShipImmediate"
                PrintPickList
                    label is "PrintPickList"
            column2
                PrintPackList
                    label is "PrintPackList"
                PrintInvoice
                    label is "PrintInvoice"
