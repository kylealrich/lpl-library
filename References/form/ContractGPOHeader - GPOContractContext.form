GPOContractContext is a Form
    title is "GPOContractStatus"
    Layout
        display as text
            two column
                GPOContractNumber
                ContractDescription
            two column
                ManufacturerSupplier
                ContractVendorName
            two column
                StartDate
                EndDate
            single column
                visible when (ContractExists)
                    text of "HasInterfaceContract;Use_View_Interface_ContractToSeeDetails"
                        bold
                        color of green
                visible when (ContractDoesNotExist)
                    visible when (CanCreateNewContract)
                        text of "DoesNotHaveInterfaceContract;Use_Create_New_ContractToCreateAnInterfaceContract"
                            bold
                            color of green
                    visible when (not CanCreateNewContract)
                        text of "DoesNotHaveInterfaceContract;CannotCreateInterfaceContractWithDataProvided"
                            bold
                            color of yellow
