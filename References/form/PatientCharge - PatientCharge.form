PatientCharge is a Form
    is primary
    Layout
        paragraph
            display as text
                Company
                Company.InventoryCompany.Name
                    no label
        single column
            PatientVisit
            PatientCharge.RevenueCenter
                label is "RevenueCenter"
        paragraph
            PatientCharge.FromLocation
                label is "SourceLocation"
            PatientCharge.FromLocation.Name
                no label
                display as text
        single column
            PatientCharge.DateOfCharge
                label is "ChargeDate"
        blank line
        paragraph
            PatientCharge.Item
                label is "Item"
            PatientCharge.Item.Description
                no label
                display as text
            Quantity
        single column
            BatchNumber
                display as text
