ProjectAssetInterfaceForm is a Form
    title is "ProjectAssetInterface"
    restrict action Create
    Layout
        paragraph
            Company
            Company.GeneralLedgerCompany.Name
                no label
                display as text
        paragraph
            AssetTemplate
                select is ValidTemplatesRel.AssetTemplates
            AssetTemplate.Description
                no label
                display as text
        paragraph
            AssetProject
                compact format
        paragraph
            InvoiceAndPurchaseOrderInformation.InvoiceCompany
                label is "InvoiceCompany"
            InvoiceAndPurchaseOrderInformation.Vendor
        paragraph
            InvoiceAndPurchaseOrderInformation.PayablesInvoice
            button of "<InvoiceAndPurchaseOrderInformation.PayablesInvoice.Invoice>"
                visible when (HasInvoice)
                link is PayablesInvoiceLink
        two column
            ItemQuantity
                label is "Quantity"
            TransactionItemCost
                label is "Cost"
        two column
            TransactionItemTax
                label is "Tax"
            PurchaseDate
        two column
            AssetDescription
            AssetGroup
        two column
            TagNumber
            Combine
        two column
            Currency
            PostingDate
                label is "WIP_/ClearingPostingDate"
                display only
        paragraph
            HoldAsset
        header3 of "Item"
        two column
            ItemNumber
                label is "Item"
            Description
        two column
            ModelNumber
            SerialNumber
        single column
            InServiceDate
        header3 of "TemplateOverrides"
        single column
            paragraph
                text of "Default_Transaction_Dimensions"
                    right align
                AssetAccountingUnit
                    no label
                    compact format
            paragraph
                text of "Depreciation_Expense"
                    right align
                DepreciationExpenseProject
                    no label
        blank line
        three column
            AssetLocation
                label is "Location"
            AssetDivision
                label is "Division"
            AssetType
                compact format
            AssetAccountingUnitGroup
                label is "AssetDimensionGroup"
            AssetAccountGroup
                label is "AccountGroup"
            AssetCategory
                label is "Category"
            visible when (AssetGuidelineClassesExists)
                AssetGuidelineClass
                    label is "GuidelineClass"
        paragraph
            WorkInProcess
            TaxExempt
            Simulated
            Used
