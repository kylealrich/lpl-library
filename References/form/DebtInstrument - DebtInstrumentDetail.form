DebtInstrumentDetail is a Form
    Layout
        paragraph
            InterestMethod
        paragraph
            DebtGroup
                select is DebtGroupSelect
            DebtGroup.Description
                no label
                display as text
            visible when (ParentDebtGroupStructure entered)
                text of "(<ParentDebtGroupStructure>)"
                    italics
        paragraph
            DebtType
                select is DebtTypeSelect
            DebtType.Description
                no label
                display as text
            visible when (ParentDebtTypeStructure entered)
                text of "(<ParentDebtTypeStructure>)"
                    italics
        visible when (DebtInstrumentType.LineOfCredit)
            paragraph
                LineOfCreditCapacityAmount
            paragraph
                LineOfCreditCommittedAmount
                    display as text
        paragraph
            Issuer
            Issuer.Description
                no label
                display as text
        paragraph
            IssuerBranch
                select is FinancialInstitutionBranchDebtSelect
            IssuerBranch.Description
                no label
                display as text
        two column
            IssueDate
            MaturityDate
                display as text
        two column
            TaxExempt
            paragraph
                CallProvision
                visible when (CallProvision)
                    CallProvisionDescription
                        no label
        paragraph
            DebtCurrency
            DebtCurrency.Description
                no label
                display as text
        paragraph
            DebtCalendar
            DebtCalendar.Description
                no label
                display as text
        paragraph
            DebtReportGroup
                select is DebtReportGroupSelect
            DebtReportGroup.Description
                no label
                display as text
            visible when (ParentDebtReportGroupStructure entered)
                text of "(<ParentDebtReportGroupStructure>)"
                    italics
        single column
            Obligor
        paragraph
            CashManagementLocation
                select is CashManagementLocationsNoChildren
            CashManagementLocation.Description
                no label
                display as text
            visible when (ParentLocationStructure entered)
                text of "(<ParentLocationStructure>)"
                    italics
        paragraph
            LegalEntityGroup.LegalEntity
                label is "LegalEntity"
            LegalEntityGroup.LegalEntity.Company.Name
                no label
                display as text
        paragraph
            CashForecastAccount
            CashForecastAccount.Description
                no label
                display as text
        single column
            Active
