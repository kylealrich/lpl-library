MobileBudgetGroup is a CompositeForm
    title is "BudgetGroup_(<DerivedCurrency>)"
    context form is MobileMain
    Obligations is a Panel
        valid when (BudgetTemplate.ReadyOrProcessing)
        list is BudgetGroupTotalsRel.BudgetGroupTotalList
    ChangedBudgets is a Panel
        list is BudgetGroupAmountsRel.BudgetGroupAmountList
