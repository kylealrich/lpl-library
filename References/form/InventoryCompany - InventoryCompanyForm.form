InventoryCompanyForm is a CompositeForm
    is primary
    title is "InventoryCompany"
    context form is InventoryCompanyFormContext
        background is theme color
    Main is a Panel
        form is Main
    DocumentTemplates is a Panel
        form is DocumentTemplates
    RequisitionSelfService is a Panel
        title is "RSS"
        form is RequisitionSelfService
    RSSLPLRequisitionSelfService is a Panel
        title is "RSS_LPL"
        visible when (RequisitionSelfServiceLPLAllowed)
        form is RSSLPLRequisitionSelfService
    PeriodEndDates is a Panel
        form is PeriodEndDates
    BinTypes is a Panel
        form is BinTypes
    UserFields is a Panel
        form is UserFields
    ReportOptions is a Panel
        form is ReportOptions
    InventoryLocations is a Panel
        list is InventoryLocation set.InventoryLocationsForACompany
    RequestingLocations is a Panel
        list is RequestingLocation set.RequestingLocationsForACompany
    ReasonCodes is a Panel
        list is InventoryReasonCode set.InventoryReasonCodes
    ReportGroups is a Panel
        list is InventoryReportGroup set.InventoryReportGroupList
