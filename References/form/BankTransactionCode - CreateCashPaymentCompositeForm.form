CreateCashPaymentCompositeForm is a CompositeForm
    use for action CreateCashPayment
    title is "CreateCashPaymentTransactionCode"
    context form is CashPaymentForm
        background is theme color
    Main is a Panel
        form is CashPaymentMain
    PayablesElectronicTransferID is a Panel
        visible when (PayablesElectronicTransferIDsAreApplicable)
        list is PayablesElectronicTransferID set.ElectronicTransferIdentifications
