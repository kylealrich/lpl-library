CreateContactCodeForm is an ActionForm
    action is CreateNewContactCode
    title is "CreateContactCode"
    Layout
        paragraph
            VendorGroup
            VendorGroup.Description
                no label
                display as text
        single column
            IsPrimaryContact
            valid when (not IsPrimaryContact)
                VendorContactCode
                    label is "ContactCode"
                    select is AllContactCodes
                Description
