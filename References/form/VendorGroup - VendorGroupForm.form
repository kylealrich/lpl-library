VendorGroupForm is a CompositeForm
    is primary
    title is "<DerivedFormTitle>"
    context form is VendorGroupContextForm
        background is theme color
    Main is a Panel
        title is "Main"
        form is VendorMainForm
    PayablesInvoiceApproval is a Panel
        form is APInvoiceApprovalForm
    SupplierCreatedInvoices is a Panel
        visible when (SupplierGroupExists)
        form is SupplierCreatedInvoicesForm
    DocumentTemplates is a Panel
        form is DocumentTemplatesForm
    VendorOffsetDefault is a Panel
        visible when (VendorOffset)
        list is VendorOffsetDefault set.primary
    VendorDebt is a Panel
        visible when (VendorOffset)
        list is VendorDebtRel.primary
