DBSessionDebugUnitForm is a Form
    is primary
    title is "DatabaseTierSessionDebugUnit_of_Work"
    Layout
        single column
            DBSessionDebug.SessionDescription
                label is "SessionDescription"
                display only
        single column
            DBSessionDebugUnit
                label is "Unit_of_Work"
                display only
        single column
            Start
                display only
        single column
            End
                display only
        single column
            TotalTimeMs
                label is "TotalUnit_of_WorkTime_(ms)"
                display only
                display zero
        two column
            TotalAPITimeMs
                label is "TotalUnit_of_WorkAPI_Time_(ms)"
                display only
                display zero
            PercentAPITime
                label is "Percent_API_Time"
                display only
                display zero
        single column
            DBSessionDebug.Actor
                label is "Actor"
                display only
        two column
            DebugLog
                label is "Log"
            button of "ViewLog"
                view DebugLog
        single column
            LogFileName
                label is "TemporaryLogFileName_(blank_if_loaded)"
                display only
