InventoryAccuracyProgress is an ActionForm
    action is InventoryAccuracyProgress
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        blank line
        blank line
        three column
            column1
                paragraph
                    Location
                        show up to 10 characters
                    Location.Name
                        no label
                        display as text
            column2
                text of "\\or"
                    bold
            column3
                paragraph
                    ReportGrp
                        label is "ReportGroup"
                    ReportGrp.Name
                        no label
                        display as text
        blank line
        two column distributed
            column1
                paragraph
                    Period
                        label is "BeginningPeriod"
            column2
                paragraph
                    Year
                        label is "Year"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                InventoryAccuracyProgressReport_RDG
            column2 of 25%
                InventoryAccuracyProgressReport_XPT
