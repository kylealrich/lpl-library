PhoneEFTCCExtractEntryContext is a Form
    title is "EFTCCExtractEntryContext"
    Layout
        visible when (RecordExists)
            two column table
                display as text
                    column1
                        CashCode.CashCodeAndDescription
                            label is "CashCode"
                        CashLedgerElectronicFundsTransferTransaction.BankTransactionCode.BankTransactionCodeName
                            label is "TransactionCode"
                        Company.OrigCompany.RepresentativeText
                            label is "Company"
                        visible when (ElectronicFundsTransferDepositDate entered)
                            ElectronicFundsTransferDepositDate
                                label is "DepositDate"
                    column2
                        Reference
                            label is "ReferenceNumber"
                        CashLedgerTransactionIdentifier
                        paragraph
                            ReceiptAmountUnsigned
                                label is "Amount"
                            CashCode.Currency
                                no label
