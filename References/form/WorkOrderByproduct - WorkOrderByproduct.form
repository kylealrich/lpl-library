WorkOrderByproduct is a CompositeForm
    is primary
    title is "WorkOrder_<WorkOrder>_Byproduct_<DerivedWorkOrderByproduct>"
    context form is WorkOrderByproductContextForm
        background is theme color
    Byproduct is a Panel
        form is ByproductForm
    Details is a Panel
        visible when (DetailTracked)
        list is WorkOrderItemBinLotSerialRel.Details
    Comments is a Panel
        list is WorkOrderByproductComment set.WorkOrderByproductCommentList
