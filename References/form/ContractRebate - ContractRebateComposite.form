ContractRebateComposite is a CompositeForm
    is primary
    title is "<mode>ContractRebate"
    context form is ContractRebateContext
        background is theme color
    Actions
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
            hide when invalid
        action is save
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
            hide when invalid
        action is refresh
            label is "_"
    RebateDueDates is a Panel
        visible when (StandardRebateOrFixedRebateAmountRebate)
        list is ContractRebateDue set.ContractRebatesDue
    GrowthRebateDueDates is a Panel
        title is "RebateDueDates"
        visible when (GrowthRebate)
        list is ContractRebateDue set.ContractGrowthRebatesDue
    LoyaltyRebateDueDates is a Panel
        title is "RebateDueDates"
        visible when (LoyaltyRebate)
        list is ContractRebateDue set.ContractLoyaltyRebatesDue
    ThresholdRebateDueDates is a Panel
        title is "RebateDueDates"
        visible when (MultipleThresholdRebate)
        list is ContractRebateDue set.ContractMultipleThresholdRebatesDue
    SupplementalRebateDueDates is a Panel
        title is "RebateDueDates"
        visible when (SupplementalRebate)
        list is ContractRebateDue set.ContractMultipleThresholdRebatesDue
    BasisRebateDueDates is a Panel
        title is "BasisRebateDueDates"
        visible when (SupplementalRebate)
        list is SupplementalBasisRebateContractRebateDuesRel.BasisContractRebatesDue
    MultipleThresholdBasisRebateQualifiers is a Panel
        title is "RebateQualifiers"
        visible when (SupplementalRebateForMultipleThresholdRebate)
        list is SupplementalBasisRebateContractRebateQualifiersRel.ContractRebateThresholdQualifiers
    StandardBasisRebateQualifiers is a Panel
        title is "RebateQualifiers"
        visible when (SupplementalRebateForStandardRebate)
        list is SupplementalBasisRebateContractRebateQualifiersRel.ContractRebateStandardQualifiers
    RebateQualifiers is a Panel
        visible when (DisplayQualifiers)
        list is ContractRebateQualifier set.ContractRebateQualifiers
    RebateGrowthQualifiers is a Panel
        title is "RebateQualifiers"
        visible when (GrowthRebate)
        list is ContractRebateQualifier set.ContractRebateGrowthQualifiers
    RebateLoyaltyQualifiers is a Panel
        title is "RebateQualifiers"
        visible when (LoyaltyRebate)
        list is ContractRebateQualifier set.ContractRebateLoyaltyQualifiers
    RebateThresholdQualifiers is a Panel
        title is "RebateQualifiers"
        visible when (MultipleThresholdRebate)
        list is ContractRebateQualifier set.ContractRebateThresholdQualifiers
    GrowthRebateQualifiers is a Panel
        visible when (GrowthRebate)
        list is ContractRebateGrowthQualifier set.ContractRebateGrowthQualifiers
    LoyaltyRebateQualifiers is a Panel
        visible when (LoyaltyRebate)
        list is ContractRebateLoyaltyQualifier set.ContractRebateLoyaltyQualifiers
    ThresholdRebateQualifiers is a Panel
        visible when (MultipleThresholdRebate)
        list is ContractRebateThresholdQualifier set.ContractRebateThresholdQualifiers
    SupplementalRebateQualifiers is a Panel
        visible when (SupplementalRebate)
        list is ContractRebateThresholdQualifier set.ContractRebateSupplementalQualifiers
    RebateAnalysis is a Panel
        visible when (AmountExists)
        list is ContractRebateDue set.ContractRebatesDueChart
