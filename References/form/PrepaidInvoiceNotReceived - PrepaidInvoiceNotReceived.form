PrepaidInvoiceNotReceived is an ActionForm
    action is PrepaidInvoiceNotReceived
    Layout
        two column
            Company
                select is Companies
            Company.Name
                no label
                display as text
        two column
            Vendor
                select is ActiveVendorsByName
            Vendor.VendorName
                no label
                display as text
        blank line
        single column
            CompanyGroup
                label is "_orCompanyGroup"
        single column
            CompanyGroupVendor
        blank line
        single column
            VloLocationCode
                label is "VendorLocation"
        blank line
        two column
            FromDate
                label is "FromPaymentDate"
            ToDate
                label is "ToPaymentDate"
        blank line
        single column
            InvProcess
                label is "UpdateOption"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                PrepaidInvoiceNotReceivedReport_RDG
                PrepaidInvoiceNotReceivedErrorReport_RDG
            column2 of 25%
                PrepaidInvoiceNotReceivedReport_XPT
                PrepaidInvoiceNotReceivedErrorReport_XPT
