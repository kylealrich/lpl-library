ConsolidatedItemLocationForm is a Form
    is primary
    title is "LocationInformation"
    Layout
        display as text
            paragraph
                ContractOrItemLocation
                InventoryCompany
                InventoryLocation
                RequestingLocation
            paragraph
                ConsolidatedItemMaster
                CreateDate
                UpdateDate
            paragraph
                ItemRel.GTINForStockUOM
                    label is "GTINForStockUOM"
                ItemRel.ItemGTINForTransactionUOM
            paragraph
                DefaultUnitCost
                visible when (ConsolidatedItemMaster.ItemExists)
                    DefaultUnitCostUOM
                visible when (not ConsolidatedItemMaster.ItemExists)
                    ContractLineForOutputCostRel.UOM.UnitOfMeasure
            paragraph
                visible when (ConsolidatedItemMaster.ItemExists)
                    DefaultUnitCostInStockUOM
                    ItemRel.StockUOM
            paragraph
                DefaultUnitCostForLowestDivisibleCost
                visible when (ConsolidatedItemMaster.ItemExists)
                    ItemRel.UOMConversion
                visible when (not ConsolidatedItemMaster.ItemExists)
                    ContractLineForOutputCostRel.UOMConversion
                DefaultUnitCostVendor
                DefaultUnitCostVendorName
            visible when (ConsolidatedItemMaster.ItemExists)
                visible when (HasItemLocation)
                    paragraph
                        InventoryCostInStockUOM
                        InventoryCostForLowestDivisibleCost
            paragraph
                DefaultUnitCostContract
                DefaultUnitCostContractLine
            paragraph
                DefaultUnitCostPurchaseFromLocation
                DefaultUnitCostPurchaseFromLocationName
            visible when (ConsolidatedItemMaster.ItemExists)
                ItemLocationRel.GTINForDefaultTransactionUOM
                ItemLocationRel.StockOnHandQuantity
                ItemLocationRel.Consignment
                ItemLocationRel.PreferredBin
            paragraph
                InactiveFlag
                DiscontinuedFlag
                DeletedFlag
                ItemLocationRel.Manufacturer.ManufacturerCode
                ItemLocationRel.Manufacturer.ManufacturerDivision
                ItemLocationRel.ManufacturerNumber
