WorkItemForm is a CompositeForm
    use for desktop
    use for browser
    title is "WorkItem"
    context form is WorkItemContextForm
    restrict action Create
    restrict action Update
    restrict action Delete
    restrict action ExecuteAction
    ApprovalTracking is a Panel
        visible when (HasProcessIndicator)
        form is ApprovalTracking
    DisplayExec1 is a Panel
        title is "Display"
        visible when (DisplayTypeIsContentOrFile)
        form is DisplayExec1Form
    DisplayExec2 is a Panel
        title is "Display"
        visible when (DisplayTypeIsURL)
        url is "<DisplayExec>"
    DisplayExec3 is a Panel
        title is "Display"
        visible when (DisplayTypeIsS3)
        url is "<DisplayExecUrl>"
    History is a Panel
        title is "History"
        list is PfiWorkunit.PfiQueueRel.InbasketHistoryList
    Messages is a Panel
        title is "Messages<PfiWorkunit.MessageCount>"
        list is PfiWorkunit.PfiMessage set.InbasketMessageList
    RelatedLinks is a Panel
        title is "RelatedLinks<PfiWorkunit.FolderCount>"
        list is PfiWorkunit.PfiWorkunitFolder set.InbasketWorkunitFolderList
