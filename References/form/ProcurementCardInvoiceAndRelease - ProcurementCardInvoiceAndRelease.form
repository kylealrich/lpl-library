ProcurementCardInvoiceAndRelease is an ActionForm
    action is ProcurementCardInvoiceAndRelease
    Layout
        paragraph
            PrmProcurementGroup
                label is "ProcurementGroup"
            PrmProcurementGroup.Description
                no label
                display as text
        paragraph
            PrmProgram
                label is "Program"
            PrmProgram.Description
                no label
                display as text
        single column
            PrmStatement
                label is "Statement"
                select is UninvoicedStatements
            PrmInvoice
                label is "Invoice"
            SendEmail
                label is "SendEmailToProcurementCardUser"
