MobileItemLocationDetails is a CompositeForm
    title is "ItemLocation"
    context form is MobileLocationContext
    restrict action Delete
    restrict action Create
    restrict action RequestNewItemLocation
    restrict action ParLocationUpdate
    restrict action MobileParLocationUpdate
    restrict action PrintLabel
    restrict action PrintBinLabel
    Actions
        action is previous record
        action is next record
    Main is a Panel
        visible when (not CartParLocation)
        form is MobileItemLocationHeader
    DetailTracking is a Panel
        visible when (not CartParLocation)
        form is MobileDetailTrackingForm
    ParDetail is a Panel
        title is "Main"
        visible when (CartParLocation)
        form is MobileItemLocationDetailForParItem
    Replenishment is a Panel
        visible when (CartParLocation)
        list is ItemReplenishmentSourcesRel.MobileReplenishmentPriority
    UnitOfMeasure is a Panel
        list is ItemUOMRel.MobileItemUOMsForView
    LocationItemUOM is a Panel
        visible when (DisplayLocationStockUOMList)
        list is AllLocationItemUOMRel.MobileLocationItemUOMList
    InventoryBalances is a Panel
        visible when (not CartParLocation)
        form is MobileInventoryBalance
    OnOrder is a Panel
        visible when (not CartParLocation)
        list is OpenPurchaseOrderLinesRel.MobilePurchaseOrderLinesForAnItem
    Intransit is a Panel
        visible when (HasOpenRQTransferLines)
        list is OpenRequisitionDTorITLinesRel.MobileRequisitionLineList
