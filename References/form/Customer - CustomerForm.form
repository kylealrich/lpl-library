CustomerForm is a CompositeForm
    is primary
    title is "<DerivedFormTitle>"
    context form is CustomerFormContext
        background is theme color
    Main is a Panel
        form is Main
    Address is a Panel
        form is Address
    Banks is a Panel
        visible when (ElectronicFundsOrBOEProcessing)
        list is CustomerOnlyDraftBankRel.BanksList
    BillTo is a Panel
        list is BilltoRel.BillToForms
    FundsTransfer is a Panel
        visible when (CustomerGroup.GroupEFTProcessing)
        list is ReceivableElectronicFundsTransferNotifyRel.EFTNotificationList
    FundsTransferCustomers is a Panel
        visible when (CustomerGroup.GroupEFTProcessing)
        list is FtcustgrpRel.GroupEFTCustomerList
    Attachments is a Panel
        list is CustomerAttachment set.Attachments
