            Vendor
                display as text
        paragraph
            Carrier
                display as text
        paragraph
            Carrier.VendorName
                display as text
        paragraph
            CreatePurchaseOrder
                label is "CreatePO"
                display as text
        paragraph
            Dropship
                label is "DropShip"
                display as text
        paragraph
            RouteHeader.Route
                label is "Route"
                display as text
        paragraph
            RouteStop
                label is "Stop"
                display as text
        paragraph
            RouteHeader.Date
                label is "RouteDeliveryDate"
                display as text
