CustomerOrderLineProcessingOptions is a Form
    restrict action CreateCustomerOrderReturnLine
    Layout
        paragraph
            CustomerOrderLine
                label is "Line"
                display as text
        paragraph
            Item
                display as text
        paragraph
            ItemDescription
                display as text
        paragraph
            InventoryLocation
                label is "ShipFromLocation"
                display as text
        paragraph
            ExpectedShipDate
                label is "ExpectedShipDate"
                display as text
        paragraph
            EarlyDeliveryDate
                label is "RequestedDateRange"
                display as text
        paragraph
            HoldCode
                display as text
        paragraph
            HoldCode.Description
                display as text
