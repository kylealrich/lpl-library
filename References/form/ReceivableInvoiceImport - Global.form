Global is a Form
    Layout
        three column distributed
            column1
                OriginIntrastatCountry
                    label is "ShipFromCountry/\\Jurisdiction"
                DestinationIntrastatCountry
                    label is "ShipToCountry/\\Jurisdiction"
                OriginIntrastatRegion
                    label is "ShipFromRegion"
                DestinationIntrastatRegion
                    label is "ShipToRegion"
                paragraph
                    ReceivableInvoiceShipToGroup.CustomerShipTo
                    visible when (StaticTaxEntityUsesOneSource)
                        PointOfTitleTransfer
            column2
                ShipTerm
                    label is "ShipTerms"
                    select is ShipTerms
                TransportMode
                    label is "ShipVia"
                IntrastatPortCode
                    label is "Port"
                DropShipIndicator
                    label is "Dropship"
                paragraph
                    CertificationNumber
                    CertificationDate
            column3
                NatureOfTransactionCode
                StatisticalProcedure
                ForeignEconomicRegulatoryCode
                    select is ForeignEconCodes
                GlobalDocumentType
                    label is "DocumentTypeCode"
                    select is DocumentTypeCodes
