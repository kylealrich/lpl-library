PropertyReport is an ActionForm
    action is PropertyReport
    Layout
        header2 of "SelectionOptions"
        paragraph
            Book
                label is "Book"
            Book.Description
                no label
                display as text
            PrelimClose
                label is "PreliminaryClose"
        paragraph
            FromDate
                label is "FromInServiceDate"
            ToDate
                label is "ToInServiceDate"
        single column
            AuthDistrict
                label is "District"
        single column
            AuthCtyLocal
                label is "County_orLocality"
        paragraph
            Country
            StateProv
                label is "State_orProvince"
        single column
            AuthCountry
                label is "Country/\\Jurisdiction"
        paragraph
            Company
                label is "Company"
                select is Companies
            Company.CompanyName
                no label
                display as text
            text of "Or"
            CompanyGroup
                label is "CompanyGroup"
            CompanyGroup.Description
                no label
                display as text
        paragraph
            Classification
            Category
        paragraph
            Property
            Disposals
        header2 of "AdditionalOptions"
        paragraph
            FullyDepr
                label is "FullyDepreciated"
            TaxExempt
        paragraph
            Simulated
            Used
        paragraph
            Asset
                select is AllAssets
            Asset.Description
                no label
                display as text
        paragraph
            AssetLease.LeaseCompany
                label is "OrCompany"
            AssetLease.LeaseCompany.Name
                no label
                display as text
            AssetLease.Lease
                label is "Lease"
        single column
            TagNumber
                label is "Tag"
        paragraph
            AssetGroup
                select is AssetGroups
            AssetGroup.Description
                no label
                display as text
        paragraph
            AssetType
                label is "Type"
                compact format
                select is AssetTypesWithinCompany
            AssetType.Description
                no label
                display as text
        header2 of "OutputOptions"
        single column
            header3 of "PrintTotals"
        single column
            header4 of "Sequence"
            blank line
            two column
                PropertyOption
                    label is "Property"
                CountryOption
                    label is "Country/\\Jurisdiction"
            two column
                StateOption
                    label is "State_orProvince"
                CountyOption
                    label is "County\\,Locality"
            two column
                DistrictOption
                    label is "District"
                TypeSubTypeOption
                    label is "Type\\,Sub"
            two column
                AstgrpOption
                    label is "AssetGroup"
                AssetOption
                    label is "Asset"
            header4 of "Miscellaneous"
            blank line
            paragraph
                CatOption
                    label is "Category"
            InservOption
                label is "In\\-\\Service"
        blank line
        single column
            PageBreak
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                PropertyReportReport_RDG
            column2 of 25%
                PropertyReportReport_XPT
