InvoiceBatchRelease is an ActionForm
    action is InvoiceBatchRelease
    Layout
        single column
            paragraph
                Company
                    select is Companies
                Company.Name
                    no label
                    display as text
            text of "Or"
            paragraph
                CompanyGroup
                    label is "CompanyGroup"
                CompanyGroup.Description
                    no label
                    display as text
            paragraph
                FrBatchNbr
                    label is "FromBatch"
                    select is InvoiceBatches
                ToBatchNbr
                    label is "ToBatch"
                    select is InvoiceBatches
            ReleaseOpt
                label is "ReleaseOption"
            CertNbrOption
                label is "PrintCertificationNumber"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                InvoiceBatchReleaseReport_RDG
                InvoiceBatchReleaseErrorReport_RDG
            column2 of 25%
                InvoiceBatchReleaseReport_XPT
                InvoiceBatchReleaseErrorReport_XPT
