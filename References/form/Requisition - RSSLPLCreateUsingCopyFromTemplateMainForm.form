RSSLPLCreateUsingCopyFromTemplateMainForm is a Form
    title is "CreateFromShoppingList"
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            Requester
            Requester.Employee.PresentationNameSnapshot
                no label
                display as text
            BaseCurrencyCode
                label is "Currency"
                display as text
        paragraph
            ProcurementTemplate
                label is "ShoppingList"
                select is RequesterProcurementTemplateListByParticipants
        blank line
        paragraph
            RequestingLocation
                select is ActiveRequestingLocations
            RequestingLocation.Name
                no label
                display as text
        paragraph
            FromCompanyLocation.FromCompany
                label is "DeliverFromCompany"
            FromCompanyLocation.FromCompany.Name
                no label
                display as text
        paragraph
            FromCompanyLocation.FromLocation
                label is "DeliverFromLocation"
                select is RQInventoryLocations
            FromCompanyLocation.FromLocation.Name
                no label
                display as text
        paragraph
            RequestedDeliveryDate
        paragraph
            TransactionCurrencyCode
                label is "TransactionCurrency"
        paragraph
            Vendor
            Vendor.VendorName
                no label
                display as text
        paragraph
            PurchaseFromLocation
                label is "PurchaseFrom"
            PurchaseFromLocation.VendorName
                no label
                display as text
        paragraph
            TransientCopyAllLines
                label is "CopyAllLines"
        blank line
        paragraph
            TransientAttachment.File
                label is "Attachment"
            TransientCommentTitle
                label is "Title"
