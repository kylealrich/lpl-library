ManualPaymentsContext is a Form
    title is "ManualPaymentsClosingResults"
    Layout
        three column distributed
            column1
                VendorGroup.RepresentativeText
                    display as text
                ProcessGroupRepresentativeText
                    display as text
            column2
                PayGroup.DerivedPayGroupTitle
                    display as text
                visible when (APJournalizeGroup entered)
                    APJournalizeGroup
                        label is "PayablesJournalizeGroup"
            column3
                Status
                    display as tag
                        color of green when (Status.Complete)
                        color of orange when (Status.InProcess)
                        color of red when (Status.Incomplete)
                visible when (CBJournalizeGroup entered)
                    CBJournalizeGroup
                        label is "CashLedgerJournalizeGroup"
