Address is a Form
    Layout
        visible when (not ActionCd.ChangeACustomer)
            OverideAddress
                label is "O\\EAddressOverride"
            PostalAddress
        visible when (ActionCd.ChangeACustomer)
            paragraph
                OverideAddressChangeIndicator
                    no label
                OverideAddress
                    label is "O\\EAddressOverride"
            paragraph
                CountryCodeChangeIndicator
                    no label
                PostalAddress.Country
            visible when (PostalAddress.Country entered)
                two column distributed
                    column1
                        paragraph
                            AddressLine1ChangeIndicator
                                no label
                            PostalAddress.DeliveryAddress.AddressLine1
                                label is "Address1"
                        paragraph
                            AddressLine2ChangeIndicator
                                no label
                            PostalAddress.DeliveryAddress.AddressLine2
                                label is "Address2"
                        paragraph
                            AddressLine3ChangeIndicator
                                no label
                            PostalAddress.DeliveryAddress.AddressLine3
                                label is "Address3"
                        paragraph
                            AddressLine4ChangeIndicator
                                no label
                            PostalAddress.DeliveryAddress.AddressLine4
                                label is "Address4"
                        paragraph
                            CityChangeIndicator
                                no label
                            PostalAddress.Municipality
                                label is "City"
                    column2
                        paragraph
                            StateChangeIndicator
                                no label
                            PostalAddress.StateProvince
                                label is "State_orProvince"
                        paragraph
                            PostalCodeChangeIndicator
                                no label
                            PostalAddress.PostalCode
                                label is "ZipCode"
                        paragraph
                            CountyChangeIndicator
                                no label
                            PostalAddress.County
                                label is "County/District"
                        paragraph
                            RegionCodeChangeIndicator
                                no label
                            PostalAddress.Region
                                label is "Region"
                        paragraph
                            AddressTypeChangeIndicator
                                no label
                            PostalAddress.Type
                                label is "Type"
