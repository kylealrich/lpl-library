ComputeDetail is a Form
    Layout
        header3 of "Depreciation"
        single column
            MethodFormula
                label is "Formula"
                select is DepreciationFormulasRel.MethodFormulaList
        paragraph
            DepreciationOptimizingMethod
                label is "OptimizationMethod"
            DepreciationOptimizingMethodDescription
                no label
                display as text
        paragraph
            StraightLineDepreciationMethod
                label is "StraightLineCalculation"
