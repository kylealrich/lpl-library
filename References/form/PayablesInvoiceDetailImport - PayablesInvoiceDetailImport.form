PayablesInvoiceDetailImport is a Form
    is primary
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            Location
            Location.Name
                no label
                display as text
        single column
            VendorItem
            Invoice
                label is "Invoice"
            Suffix
                label is "Suffix"
            EDINumber
                label is "E\\D\\INumber"
            Sequence
                label is "InterfaceInvoiceDetail"
            LineNumber
            Item
            VendorItem
            visible when (IsHSNSACCodeEnabled)
                HSNSACCode
                HSNSACCode.Description
                    no label
                    display as text
            EnteredQuantity
            EnteredUOM
                label is "U/O/M"
            UnitCost
            ExtendedAmount
            AddOnCharge
                label is "AddOnCharge"
            TotalAddOnChargeAmount
            RetailUnitCost
            TaxCode
            TaxableUnitCost
            InvoiceLineTaxable
            TaxUsageCode
            ICNCode
                show up to 10 characters
            SupplementaryQuantity
            StockWeight
            CrossReferenceVendor
            CatchWeightCost
            CommodityCode
            RetainagePercent
            ItemGTIN
            GlobalLineType
            DistributionAccount
                compact format
