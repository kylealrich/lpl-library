ColumnDetailForm is a Form
    title is "FieldDetail"
    Layout
        single column
            ColumnOrder
            FieldName
            Description
            Comments
            FieldDataType
                when value changed
                    refresh SupportsReplacement
                    refresh DoesNotSupportJavaFormat
                    refresh ColumnCanBeTotalled
                    JavaFormat =  blank
            InputOption
                when value changed
                    refresh InputOptionUsesSourceField
            visible when (false)
                JavaFormat
                SupportsReplacement
                DoesNotSupportJavaFormat
                ColumnCanBeTotalled
        visible when (InputOptionUsesSourceField)
            header3 of "SourceValue"
            single column
                visible when (FileCreationRowDetail.OneToMany)
                    OneToMany
                        display as switch
            single column
                SourceRelation
                SourceFields
                    display as required when (InputOptionUsesSourceField)
            visible when (ColumnCanBeTotalled)
                single column
                    TotalThisColumn
                        display as switch
        visible when (InputOption.Literal)
            header3 of "SourceValue"
            visible when (FieldDataType.Alpha)
                single column
                    FileName
                        label is "OutputFileName"
                        display as switch
                    DefaultAlpha
                        label is "LiteralValue"
            visible when (FieldDataType.Decimal)
                single column
                    DefaultDecimal
                        label is "LiteralValue"
            visible when (FieldDataType.Date)
                single column
                    UseCurrentSystemDate
                        display as switch
                    DefaultDate
                        label is "LiteralValue"
            visible when (FieldDataType.Time)
                single column
                    UseCurrentSystemTime
                        display as switch
                    DefaultTime
                        label is "LiteralValue"
            visible when (FieldDataType.TimeStamp)
                single column
                    UseCurrentSystemTimeStamp
                        display as switch
                    DefaultTimeStamp
                        label is "LiteralValue"
            visible when (FieldDataType.Boolean)
                single column
                    DefaultBoolean
                        label is "LiteralValue"
            visible when (FieldDataType.Text)
                single column
                    DefaultText
                        label is "LiteralValue"
            visible when (FieldDataType.Numeric)
                single column
                    visible when (FileCreationRowDetail.OneToMany)
                        OneToManySequenceNumber
                            display as switch
                    DefaultNumeric
                        label is "LiteralValue"
                text of "OutputCount"
                    header4
                paragraph
                    RecordCount1
                        label is "1"
                        display as switch
                        when value changed
                            RecordCount2 =  false
                            RecordCount3 =  false
                            RecordCount4 =  false
                            RecordCount5 =  false
                            RecordCount6 =  false
                                when (RecordCount1)
                    RecordCount2
                        label is "2"
                        display as switch
                        when value changed
                            RecordCount1 =  false
                            RecordCount3 =  false
                            RecordCount4 =  false
                            RecordCount5 =  false
                            RecordCount6 =  false
                                when (RecordCount2)
                    RecordCount3
                        label is "3"
                        display as switch
                        when value changed
                            RecordCount1 =  false
                            RecordCount2 =  false
                            RecordCount4 =  false
                            RecordCount5 =  false
                            RecordCount6 =  false
                                when (RecordCount3)
                paragraph
                    RecordCount4
                        label is "4"
                        display as switch
                        when value changed
                            RecordCount1 =  false
                            RecordCount2 =  false
                            RecordCount3 =  false
                            RecordCount5 =  false
                            RecordCount6 =  false
                                when (RecordCount4)
                    RecordCount5
                        label is "5"
                        display as switch
                        when value changed
                            RecordCount1 =  false
                            RecordCount2 =  false
                            RecordCount3 =  false
                            RecordCount4 =  false
                            RecordCount6 =  false
                                when (RecordCount5)
                    RecordCount6
                        label is "6"
                        display as switch
                        when value changed
                            RecordCount1 =  false
                            RecordCount2 =  false
                            RecordCount3 =  false
                            RecordCount4 =  false
                            RecordCount5 =  false
                                when (RecordCount6)
            visible when (HeaderTrailerOrSubtotal)
                visible when (SupportsTotals)
                    single column
                        text of "ColumnTotal"
                            header4
                    paragraph
                        TotalForRow
                            label is "Row"
                            select is RowsWithTotalsRel.FileCreationRowDetailSelect
                        TotalForColumn
                            label is "Column"
                            select is ColumnsWithTotalsRel.FileCreationColumnSelect
                        TotalForColumn.FieldName
                            no label
                            display as text
