APToARInterfaceCompositeForm is a CompositeForm
    title is "Vendor<Vendor>_<VendorName>PayablesToReceivablesInterface"
    context form is APToARInterfaceContextForm
    Actions
        action is ReleaseToARInterface
            action icon is submit
            label is "InterfaceReleasedReceivables"
            hide when invalid
        action is LinkWithARCustomer
            action icon is validate
            hide when invalid
        action is save
            label is "_"
        action is search
            label is "_"
        action is refresh
            label is "_"
    Payables is a Panel
        visible when (HasCustomer)
        list is APToARSelectedInvoicesRel.APToARInterfaceList
    SelectedItems is a Panel
        visible when (IsSelected)
        list is APToARInterfaceSelectedRecordsRel.APToARSelectList
