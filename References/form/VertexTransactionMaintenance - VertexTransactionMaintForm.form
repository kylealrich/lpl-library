VertexTransactionMaintForm is a Form
    title is "VertexRequest"
    Layout
        paragraph
            VertexTransactionMaintenance
                display as text
            CurrentTimestampLabel
                no label
        single column
            ProcessType
                display as text
            OriginalTransactionId
            TransactionId
            RelatedObject
                label is "BusinessObjectReference"
                display as text
            RelatedObject.BusinessClassName
                display as text
            RelatedObject.BusinessObjectKey
                display as text
            Company
                display as text
            Invoice
                display as text
            Vendor
                display as text
            TaxAreaId
                display as text
            TaxableAmount
                display as text
            TaxAmount
                display as text
            TaxPointDate
                display as text
            ReasonText
                display as text
            ResultMessage
                display as text
