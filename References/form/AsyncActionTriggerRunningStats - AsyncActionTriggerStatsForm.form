AsyncActionTriggerStatsForm is a Form
    is primary
    title is "RunningStatistics"
    Layout
        single column
            text of "TheInformationBelowIsASnapshotOfTheActionState.AFailureCanCauseThe_Records_ProcessedAnd_Completion_PercentageValuesToPartiallyRollBack."
        four column
            column1
                visible when (SetActionStats.EstimatedSetSizeComputed)
                    SetActionStats.DerEstimatedSetSize
                visible when (not SetActionStats.EstimatedSetSizeComputed)
                    SetActionStats.DerEstimatedSetSizeUnknown
                SetActionStats.SetRecords
                    display only
                SetActionStats.SetCommits
                    label is "NumberCommits"
                    display only
                SetActionStats.CheckPointDurationSeconds
                    display only
            column2
                visible when (SetActionStats.EstimatedSetSizeComputed)
                    SetActionStats.EstimatedSetComplete
                visible when (not SetActionStats.EstimatedSetSizeComputed)
                    SetActionStats.DerEstimatedSetSizeUnknown
                blank line
                SetActionStats.SetCommitObjects
                    label is "ObjectsCommitted"
                    display only
                SetActionStats.SetAverageCheckPointDurationSeconds
                    label is "AverageCheckpointDurationSeconds"
                    display only
            column3
                blank line
                blank line
                SetActionStats.SetAverageObjectPerCommit
                    label is "AverageObjectsPerCommit"
                    display only
                SetActionStats.CheckPointHighwaterSeconds
                    display only
            column4
                blank line
                blank line
                SetActionStats.SetCommitHighWater
                    label is "HighWater"
                    display only
                blank line
