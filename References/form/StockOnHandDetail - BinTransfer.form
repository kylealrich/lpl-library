BinTransfer is an ActionForm
    action is DetailTransfer
    Layout
        single column
            Company
                display as text
            InventoryLocation
                display as text
            Item
                label is "Item"
                display as text
            paragraph
                PrmQuantity
                    label is "Quantity"
                visible when (HasItemLocationStockUOM)
                    DerivedLocationStockUOM
                        no label
        blank line
        two column
            column1
                visible when (ItemLocation.IsInventoryBinTracked)
                    StockOnHandDetail.Bin
                        label is "FromBin"
                        display as text
                visible when (ItemLocation.TrackedInAlternateUnitsOfMeasure)
                    StockOnHandDetail.UnitOfMeasure
                        label is "FromUOM"
                        display as text
                visible when (ItemLocation.IsLotTracked)
                    StockOnHandDetail.Lot
                        label is "Lot"
                        display as text
                    StockOnHandDetail.Sublot
                        label is "Sublot"
                        display as text
                visible when (InventoryLocation.UseAsWarehouseLocation)
                    DerivedWarehouseStorageLocation
                        label is "From_Storage_Location"
                        display as text
            column2
                visible when (ItemLocation.IsInventoryBinTracked)
                    visible when (not InventoryLocation.UseAsWarehouseLocation)
                        PrmToBin
                            label is "ToBin"
                    visible when (InventoryLocation.UseAsWarehouseLocation)
                        PrmToBin
                            label is "ToBin"
                            select is BinsWithItemStorageLocationRel.DistributionManagementWithPreferredBinList
                visible when (ItemLocation.TrackedInAlternateUnitsOfMeasure)
                    PrmToUOM
                        label is "ToUOM"
                visible when (ItemLocation.IsLotTracked)
                    StockOnHandDetail.Lot
                        label is "Lot"
                        display as text
                    StockOnHandDetail.Sublot
                        label is "Sublot"
                        display as text
                visible when (InventoryLocation.UseAsWarehouseLocation)
                    PrmToBin.WarehouseStorageLocation
                        label is "To_Storage_Location"
                        display as text
