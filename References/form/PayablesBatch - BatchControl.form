BatchControl is a Form
    title is "PayablesBatch"
    Layout
        paragraph
            Company
                select is Companies
        paragraph
            PayablesBatch
                label is "Batch"
                select is Batches
        paragraph
            DistributionDate
                label is "PostDate"
        paragraph
            NetAmount
            ActualInvoiceTotal
                display as text
        paragraph
            InvoiceCurrency
                label is "Currency"
        paragraph
            NumberOfInvoices
            ActualNumberOfInvoices
                display as text
        paragraph
            VendorHashTotal
            ActualVendorHashTotal
                display as text
