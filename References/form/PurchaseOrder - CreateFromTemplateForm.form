CreateFromTemplateForm is a Form
    title is "CreatePurchaseOrderFromProcurementTemplate"
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            ProcurementTemplate
                label is "ProcurementTemplate"
                select is ProcurementTemplateListByParticipantsForPO
            ProcurementTemplate.Description
                no label
                display as text
        paragraph
            Vendor
                select is ActiveVendorsByName
            Vendor.VendorName
                no label
                display as text
        paragraph
            PurchaseFromLocation
                label is "PurchaseFrom"
                select is NonRemitToActivePurchaseFromLocations
            PurchaseFromLocation.VendorName
                no label
                display as text
        paragraph
            Buyer
            Buyer.Employee.PresentationNameSnapshot
                no label
                display as text
        paragraph
            ShipToLocation
                label is "ShipTo"
            ShipToLocation.Name
                no label
                display as text
        paragraph
            PurchaseOrderDate
            DefaultDeliveryDate
                label is "DeliveryDate"
        single column
            TransientCopyAllLines
                label is "CopyAllLines"
