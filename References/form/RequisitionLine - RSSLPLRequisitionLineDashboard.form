RSSLPLRequisitionLineDashboard is a CompositeForm
    title is "RequisitionLineStatusDetails"
    called out actions only
    context form is RSSLPLRequisitionLineDashboardContext
        background is theme color
    PurchaseOrderQuantities is a Panel
        visible when (PurchaseOrderAndWarehouse)
        form is PurchaseOrderQuantitiesDashboardForm
    POQuantities is a Panel
        title is "PurchaseOrderQuantities"
        visible when (IsCreatePurchaseOrderNotContract)
        form is PurchaseOrderQuantitiesDashboardForm
    WarehouseQuantities is a Panel
        visible when (PurchaseOrderAndWarehouse)
        form is RSSLPLRequisitionLineWarehouseOrder
    WHQuantities is a Panel
        title is "WarehouseQuantities"
        visible when (not IsCreatePurchaseOrder)
        form is RSSLPLRequisitionLineWarehouseOrder
    ShipmentLines is a Panel
        visible when (not CreatePurchaseOrder)
        list is WarehouseShipmentLinesRel.RSSShipmentLinesStatus
    PurchaseOrderLines is a Panel
        visible when (SourceRecordExists)
        list is PurchaseOrderLineSourcesRel.RSSPurchaseOrderLineForSourceList
