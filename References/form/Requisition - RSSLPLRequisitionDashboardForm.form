RSSLPLRequisitionDashboardForm is a CompositeForm
    title is "Requisition<Requisition>Details"
    called out actions only
    context form is RSSLPLRequisitionDashboardContext
        background is theme color
    Actions
        action is Unreleased.RSSLPLShop
            action icon is undo
            label is "ContinueShopping"
            hide when invalid
    Details is a MultiListPanel
        list is RequisitionLinesRel.RSSLPLDashboardRequisitionLines
            visible when (HasLines)
        list is PurchaseOrderLineSourcesRel.PurchaseOrderLineForSourceList
            visible when (HasPOLines)
        list is RequisitionCommentRel.DashboardRequisitionComments
            visible when (CommentsExist)
        list is GLCommitDrillRel.DrillGLCommitList
            visible when (CommitmentsExist)
        list is RequisitionAddOnChargesRel.DashboardMiscAddOnCosts
            visible when (AddOnChargesExist)
        list is RequisitionSpreadAddOnCharge set.DashboardSpreadAddOnCharges
            visible when (SpreadAddOnChargesExist)
    ApprovalInformation is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
    MatrixApprovalInformation is a Panel
        visible when (DisplayMatrixApprovalHistory)
        list is ResponsibilityMatrixApprovalProcessorRel.ApprovalProcessorAuditRel.ApprovalProcessorHistoryCardHistoryCardList
    MatrixProcessIndicator is a DashBoardPanel
        title is "MatrixApprovalInformation"
        visible when (ResponsibilityMatrixApprovalProcessorRel.DisplayApprovalInformationPanel)
        show 4 columns
        Progress is a Panel
            triple width
            form is MatrixProcessIndicatorForm
        History is a Panel
            list is ResponsibilityMatrixApprovalProcessorRel.ApprovalProcessorAuditRel.ApprovalProcessorHistoryCardHistoryCardList
