VertexConfigVersion is a Form
    title is "VertexVersion"
    Layout
        three column distributed
            column1
                header2 of "Main"
                single column
                    paragraph
                        FinanceEnterpriseGroup
                    paragraph
                        VertexConfigVersion.Version
                        visible when (not TaxConfigurationVersionMatch)
                            text of "*"
                                bold
                                color of red
                        VertexConfigVersion.ServiceRelease
                        visible when (not TaxConfigurationReleaseMatch)
                            text of "*"
                                bold
                                color of red
                        VertexConfigVersion.ServerName
                        visible when (not TaxConfigurationServiceNameMatch)
                            text of "*"
                                bold
                                color of red
                    paragraph
                        visible when (VertexVersion9)
                            paragraph
                                DerivedEnteredTrustedIdText
                                    label is "TrustedID"
                                    display only
                                VertexEncryptedTrustedIdEntered
                                    label is "<EnteredLabel>"
                        visible when (VersionSixSevenOrEight)
                            paragraph
                                TrustedId
                                    label is "TrustedID"
                        visible when (UnencryptedTrustedIdStillEntered)
                            text of "*"
                                bold
                                color of red
                        visible when (TrustedIdNotEntered)
                            text of "TrustedIdCannotBeBlank"
                                bold
                                color of red
                paragraph
                    VertexCentralURL
                    visible when (VertexCentralURLEntered)
                        button of "VertexCentral"
                            link is external "<VertexCentralURL>"
                            display as link
                paragraph
                    ConfidenceIndicatorLimit
                        label is "ConfidenceIndicatorAlert"
            column2
                single column
                    header2 of "ValidationInformation"
                    VertexLicense
                        label is "ValidationRequest"
                    VertexAccessType
            column3
                single column
        paragraph
            ReleaseNotes
                use text area of 2 lines
        paragraph
            ReleaseComments
                label is "Comments"
                use text area of 2 lines
        paragraph
        visible when (RequestDescEntered)
            RequestDesc
                label is "ValidationMessage"
                use text area of 40 lines
                show up to 120 characters
