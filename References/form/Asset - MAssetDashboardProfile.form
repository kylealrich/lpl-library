MAssetDashboardProfile is a Form
    title is "AssetDashBoad"
    Layout
        paragraph
            visible when (not RestrictLeaseUpdate)
                AssetLease.LeaseCompany
                    label is "LeaseCompany"
                    select is MPayablesCompanyList
                AssetLease.Lease
                    select is MLeaseList
                    show up to 20 characters
                AssetLease.Lease.Description
                    no label
                    display as text
                    show up to 20 characters
        paragraph
            visible when (RestrictLeaseUpdate)
                AssetLease.LeaseCompany
                    label is "LeaseCompany"
                    display as text
                AssetLease.Lease
                    display as text
                visible when (RestrictLeaseUpdate)
                    AssetLease.Lease.Description
                        no label
                        display as text
        two column table
            column1
                AssetCategory
                    label is "Category"
            column2
                Reference2
        single column
            AdjustmentDate
                label is "AssetAdjustmentDate"
