ContractQuestions is a CompositeForm
    context form is ContractContextShort
        background is theme color
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Actions
        action is refresh
            label is "_"
        action is save
            label is "_"
        action is Draft.CreateContractTerms
            hide when invalid
        action is Draft.SupplierSubmitProposedContract
            label is "Submit"
            hide when invalid
        navigation is ViewContract
            label is "ReturnToContract"
    UnansweredRequiredQuestions is a Panel
        visible when (AnyUnansweredRequiredQuestionsExistAndNotTemplate)
        mouse over text is "FieldsWithAn*AreRequiredBeforeTheContractCanBeActivated,OrBeforeTheContractCanBeReleasedIfTermsAreAssociatedWithTheQuestion.QuestionsWith**AreRequiredAndAlsoHaveACorrectAnswerDefined."
        panel header is "FieldsWithAn*AreRequiredBeforeTheContractCanBeActivated,OrBeforeTheContractCanBeReleasedIfTermsAreAssociatedWithTheQuestion.QuestionsWith**AreRequiredAndAlsoHaveACorrectAnswerDefined."
        template is AllUnansweredRequiredQuestionsRel
            business class is ContractQuestionAnswer
                form is ContractQuestionAnswerSummary
    QuestionsMissingRequiredAttachments is a Panel
        visible when (AnyQuestionsMissingRequiredAttachmentsExistAndNotTemplate)
        mouse over text is "FieldsWithAn*AreRequiredBeforeTheContractCanBeActivated,OrBeforeTheContractCanBeReleasedIfTermsAreAssociatedWithTheQuestion.QuestionsWith**AreRequiredAndAlsoHaveACorrectAnswerDefined."
        panel header is "FieldsWithAn*AreRequiredBeforeTheContractCanBeActivated,OrBeforeTheContractCanBeReleasedIfTermsAreAssociatedWithTheQuestion.QuestionsWith**AreRequiredAndAlsoHaveACorrectAnswerDefined."
        template is AllQuestionsMissingRequiredAttachmentsRel
            business class is ContractQuestionAnswer
                form is ContractQuestionAnswerSummary
    IncorrectlyAnsweredQuestions is a Panel
        visible when (AnyIncorrectlyAnsweredQuestionsExistAndNotTemplate)
        mouse over text is "TheseQuestionsHaveACorrectAnswerDefinedAndAreRequiredToBeAnsweredCorrectlyBeforeTheContractCanBeActivated,OrBeforeTheContractCanBeReleasedIfTermsAreAssociatedWithTheQuestion."
        panel header is "TheseQuestionsHaveACorrectAnswerDefinedAndAreRequiredToBeAnsweredCorrectlyBeforeTheContractCanBeActivated,OrBeforeTheContractCanBeReleasedIfTermsAreAssociatedWithTheQuestion."
        template is AllIncorrectlyAnsweredQuestionsRel
            business class is ContractQuestionAnswer
                form is ContractQuestionAnswerSummary
