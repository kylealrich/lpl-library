ExpenseManagementInterfaceDetailForm is a Form
    Layout
        paragraph
            ExpenseManagementInterfaceHeader.Document
                label is "ExpenseReport"
            ExpenseManagementInterfaceDetail
                label is "SequenceNumber"
        paragraph
            Vendor
                select is VendorNumber
            Vendor.VendorName
                no label
                display as text
        blank line
        paragraph
            PaymentMethod
        paragraph
            ExpenseDate
        blank line
        paragraph
            AccountingEntity
            AccountingEntity.Name
                no label
                display as text
            ExpenseManagementInterfaceExpenseType
                label is "ExpenseType"
            ExpenseManagementInterfaceExpenseType.Description
                no label
                display as text
        blank line
        paragraph
            ReimbursementAmount
            ReimbursementCurrency
        visible when (NonReimbursableExpense)
            paragraph
                TransactionAmount
                    label is "TransactionAmount"
                TransactionCurrency
                    label is "TransactionCurrency"
        paragraph
            TaxCode1
                label is "TaxCode"
            TaxCode1.Description
                no label
                display as text
        visible when (not AllocationsExist)
            blank line
            paragraph
                FinanceStructure
                    compact format
            paragraph
                BillingFinanceStructure
                    compact format
                Billable
        paragraph
            Receipt
        blank line
        paragraph
            DerivedStatus
                display as text
            SentDate
                display as text
        paragraph
            ExpenseManagementInterfaceResult
                label is "InterfaceResult"
                display as text
            ErrorMessage
                display as text
