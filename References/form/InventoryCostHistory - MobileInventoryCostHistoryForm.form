MobileInventoryCostHistoryForm is a Form
    title is "InventoryCostHistory"
    Actions
        action is refresh
            label is "_"
    Layout
        header3 of "_"
        display as text
            two column distributed
                Company
                InventoryLocation
                    label is "Location"
                Item
                InventoryCostHistory.TransactionSystemCode
                ReceivedDate
                    label is "UpdateDate"
                ReceivedQuantity
                    label is "TransactionQuantity"
                TransientNewUnitCost
                AdjustToZeroFlag
                    label is "AdjustToZero"
                Company.Name
                    label is "CompanyName"
                    display as text
                InventoryLocation.Name
                    label is "LocationName"
                    display as text
                Item.Description
                    label is "Description"
                    display as text
                InventoryCostHistory.LineNumber
                    label is "Line"
                GeneralLedgerDate
                    label is "PostDate"
                UnusedQuantity
                    label is "UnusedQuantity"
                UnitCost
                    label is "CurrentUnitCost"
            paragraph
                Company.GeneralLedgerCompany.Currency
                    label is "Currency"
                InventoryTransaction
                InventoryTransaction.InventoryDocumentType
            paragraph
                InventoryReasonCode
                    label is "ReasonCode"
                InventoryCostHistory.ComponentSequence
                    label is "ComponentSequenceNumber"
            paragraph
                ItemLocation.StandardCost
                    label is "StandardCost"
                ItemLocation.DerivedItemAverageCost
                    label is "AverageCost"
                ItemLocation.LastReceiptCost
                    label is "LastReceivedCost"
            header3 of "_"
            paragraph
                OffsetAccount
                    compact format
