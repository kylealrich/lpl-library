MassRelease is an ActionForm
    action is MassRelease
    title is "PaymentMassRelease"
    Layout
        single column
            PrmCashManagementGroup
                label is "CashManagementGroup"
        three column
            PrmCashCode
                label is "CashCode"
                select is CashCode
            PrmCashCode.Description
                no label
                display as text
            PrmCashCode.Currency
                no label
                display as text
        two column
            PrmCashCodeGroup
                label is "CashCodeGroup"
                select is CashCodeGroup
            PrmCashCodeGroup.Description
                no label
                display as text
        blank line
        two column
            PrmUseSystemDate
                label is "UseSystemDate"
            PrmEnterDate
                label is "EnterDate"
        single column
            PrmDays
                label is "Days"
            PrmBankTransactionCode
                label is "BankTransactionCode"
            PrmGeneralLedgerSourceCode
                label is "EventCode"
            PrmPrenoteStatus
                label is "PrenotificationStatus"
