InventoryVariancesUpdate is an ActionForm
    action is InventoryVariancesUpdate
    Layout
        header2 of "Main"
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            SelectId
                label is "SelectI\\D"
            SelectId.Description
                no label
                display as text
        two column distributed
            column1
                UpdateOpt
                    label is "Update"
                PrtVarOnly
                    label is "PrintVariances"
            column2
                GlPostingDate
                    label is "G\\LPostDate"
                DeleteAllRecs
                    label is "DeleteRecords"
        single column
            SortOption
        single column
            AddBlankLineBetweenItems
                label is "BlankLineBetweenItems"
        blank line
        header2 of "AcceptableAccuracyPercentage"
        three column distributed
            AcptPctClass1
                label is "Class1"
            AcptPctClass2
                label is "Class2"
            AcptPctClass3
                label is "Class3"
        header2 of "Recount"
        three column distributed
            PrintTags
                label is "PrintRecountTags"
            PrintRecountSheets
            PrintFreezeQty
                label is "PrintFreezeQuantity"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                InventoryVariancesUpdateReport_RDG
                RecountSheetsReport_RDG
                RecountTagsReport_RDG
            column2 of 25%
                InventoryVariancesUpdateReport_XPT
                RecountSheetsReport_XPT
                RecountTagsReport_XPT
