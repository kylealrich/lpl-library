BudgetGroup is a CompositeForm
    is primary
    title is "BudgetGroup_(<DerivedCurrency>)"
    context form is ContextMain
        background is theme color
    Main is a Panel
        form is Main
    Obligations is a Panel
        valid when (BudgetTemplate.ReadyOrProcessing)
        list is BudgetGroupTotalsRel.BudgetGroupTotalList
    ObligationsLog is a Panel
        title is "ObligationLogs"
        visible when (HasLogs)
        list is LogsForGroupRel.BudgetGroupList
    ChangedBudgets is a Panel
        list is BudgetGroupAmountsRel.BudgetGroupAmountList
    PendingUpdates is a Panel
        title is "UnreleasedObligations"
        list is BudgetTemplateGroupTotalsRel.TemplateUpdates
