ScheduledProcedureCompositeForm is a CompositeForm
    is primary
    title is "<DerivedScheduledProcedureTitle>"
    context form is ScheduledProcedureContext
        background is theme color
    Main is a Panel
        form is ScheduledProcedureForm
    Inventoried is a Panel
        list is ScheduledProcedureLinesRel.ScheduledProcedureLineInventoriedList
    NonInventoried is a Panel
        visible when (HasNonStockLines)
        list is ScheduledProcedureLinesRel.ScheduledProcedureLineNonInventoriedList
