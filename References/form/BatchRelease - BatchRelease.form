BatchRelease is an ActionForm
    action is BatchRelease
    Layout
        two column
            Company
                select is Companies
            Company.Name
                no label
                display as text
        single column
            CompanyGroup
                label is "_orCompanyGroup"
        blank line
        single column
            RelAllBatches
                label is "ReleaseAllBatches"
        two column
            FrBatchNum
                label is "ReleaseFromBatch"
                select is Batches
            ToBatchNum
                label is "ToBatch"
                select is Batches
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                BatchReleaseReport_RDG
                BatchReleaseErrorReport_RDG
            column2 of 25%
                BatchReleaseReport_XPT
                BatchReleaseErrorReport_XPT
