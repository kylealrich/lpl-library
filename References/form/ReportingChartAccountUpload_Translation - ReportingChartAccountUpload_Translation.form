ReportingChartAccountUpload_Translation is a Form
    is primary
    Layout
        paragraph
            Base_BusinessClass.ReportingChartAccountUpload
                display as text
        single column
            IsoLocale
                compact format
                raise yellow alert when (VerifyTranslations)
                    mouse over text is untranslatable:"The base data has been changed but this translation has not been verified"
        single column
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.AccountDescription
                label is untranslatable:"Account Description"
            AccountDescription
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.SubAccountDescription
                label is untranslatable:"Sub Account Description"
            SubAccountDescription
                label is untranslatable:"Translated"
