JournalizeInvoices is an ActionForm
    action is JournalizeInvoices
    Layout
        single column
            PostingDate
            PrmRunGroup
                select is InvoiceRunGroupSelect
            visible when (IsThirdPartyInvoicePrint)
                PrmPrintStatus
                    label is "PrintStatus"
            visible when (not UseIDM)
                PrinterEmailAddress
            visible when (UseIDM)
                paragraph
                    PrmIDMPrinter
                        select is IDMPrinterList
                    PrmIDMPrinter.Description
                        no label
                        display as text
