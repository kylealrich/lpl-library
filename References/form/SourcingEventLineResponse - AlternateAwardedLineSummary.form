AlternateAwardedLineSummary is a SummaryForm
    title is "AwardSummary"
    Layout
        header2 of "Supplier:<NotifiedSupplier.Supplier.SupplierName>"
            underline color of black
        header2 of "_"
        single column
            AwardReason
        visible when (Alt1Exists)
            visible when (AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].IsAwarded)
                paragraph
                    visible when (PoData)
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].AwardQuantity
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].ResponseAmount.UnitPrice
                two column
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].VendorItem
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].VendorItemDescription
                        label is "Description"
        visible when (Alt2Exists)
            visible when (AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].IsAwarded)
                paragraph
                    visible when (PoData)
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].AwardQuantity
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].ResponseAmount.UnitPrice
                two column
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].VendorItem
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].VendorItemDescription
                        label is "Description"
        visible when (Alt3Exists)
            visible when (AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[3].IsAwarded)
                paragraph
                    visible when (PoData)
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[3].AwardQuantity
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[3].ResponseAmount.UnitPrice
                two column
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[3].VendorItem
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[3].VendorItemDescription
                        label is "Description"
        visible when (Alt4Exists)
            visible when (AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[4].IsAwarded)
                paragraph
                    visible when (PoData)
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[4].AwardQuantity
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[4].ResponseAmount.UnitPrice
                two column
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[4].VendorItem
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[4].VendorItemDescription
                        label is "Description"
        visible when (Alt5Exists)
            visible when (AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[5].IsAwarded)
                paragraph
                    visible when (PoData)
                        AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[5].AwardQuantity
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[5].ResponseAmount.UnitPrice
                two column
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[5].VendorItem
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[5].VendorItemDescription
                        label is "Description"
        visible when (PoData)
            paragraph
                PurchaseOrder
                    label is "PONumber"
        visible when (CmData)
            two column
                Contract
                Contract.Name
                    label is "ContractName"
