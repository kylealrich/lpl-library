Main is a Form
    Layout
        three column distributed
            column1
                paragraph
                    ProcurementCardProgramAdministrator
                        label is "Administrator"
                        select is ProcurementCardAdministrators
                    ProcurementCardProgramAdministrator.Name
                        no label
                        display as text
                single column
                    ProcurementCardProgramAdministrator.Employee.EmployeeWorkTelephone
                        display as text
                    ProcurementCardProgramAdministrator.Employee.EmployeeWorkEmailAddress
                        display as text
                paragraph
                    Vendor
                        label is "PayToVendor"
                    Vendor.VendorName
                        no label
                        display as text
                single column
                    AutomaticallyCloseProcurementCardCharge
                        label is "AutomaticChargeClose"
                    AutomaticallyApproveProcurementCardCharge
                        label is "AutomaticChargeApproval"
                    RequireReasonOnDispute
                    MaskProcurementCardNumberDisplay
                    MaskProcurementCardNumberEntry
                    AutomaticallyAddMerchant
                    AutomaticallyAddProcurementCardUser
            column2
                NotifyAdministratorOnChargeRelease
                NotifyAdministratorOnChargeApprove
                NotifyAdministratorOnChargeDispute
