MobileCurrencyAmounts is a Form
    title is "CurrencyAmounts"
    Layout
        header2 of "BankCurrencyInformation"
        two column
            column1
                CashCode.Currency
                    label is "BankAccount<CashCode>Currency"
                    display as text
                PaymentAmountBankAccountCurrency.EnteredCurrencyRate
                    label is "ExchangeRateFrom<PayCurrency>"
                    display as text
            column2
                PaymentAmountBankAccountCurrency.EnteredCurrencyAmount
                    label is "PaymentAmount"
                    display as text
                AppliedAmountBankAccountCurrency.BaseAmount.EnteredCurrencyAmount
                    label is "AppliedAmount"
                    display as text
        header2 of "CompanyCurrencyInformation"
        display as text
            two column
                column1
                    Company.Currency
                        label is "Company<Company>BaseCurrency"
                    BasePaymentAmount.EnteredCurrencyRate
                        label is "ExchangeRateFrom<PayCurrency>"
                column2
                    BasePaymentAmount.EnteredCurrencyAmount
                        label is "PaymentAmount"
                    AppliedBaseAmount.BaseAmount.EnteredCurrencyAmount
                        label is "AppliedAmount"
