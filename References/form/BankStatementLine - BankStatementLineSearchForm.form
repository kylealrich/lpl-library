BankStatementLineSearchForm is a SearchForm
    title is "TransactionSearch"
    Layout
        four column distributed
            BankStatement
            CashManagementAccount
                label is "CashAccount"
            CashManagementAccount.FinancialInstitution
            CashTransactionCategory
                select is CashTransactionCategorySelect
        four column distributed
            Description
            RelatedReference
            LineStatus
            LineCurrency
        four column distributed
            TransactionType
            ISO20022DomainCode
            ISO20022FamilyCode
            ISO20022SubFamilyCode
        four column distributed
            StatementDateRange.Begin
                label is "FromStatementDate"
            StatementDateRange.End
                label is "ToStatementDate"
            SearchAmountRange.Begin
                label is "FromLineAmount"
            SearchAmountRange.End
                label is "ToLineAmount"
        four column distributed
            ValueDateRange.Begin
                label is "FromValueDate"
            ValueDateRange.End
                label is "ToValueDate"
            DebitCreditIndicator
            paragraph
                HasLineException
                StatementIntraday
