ChangeSupplierAndVendor is an ActionForm
    action is ChangeSupplierAndVendor
    Layout
        two column
            ParmNewSupplier
            ParmNewSupplier.SupplierName
                no label
                display as text
        two column
            ParmNewSupplierContact
            ParmNewSupplierContact.FullName
                no label
                display as text
        two column
            ParmNewPurchaseFrom
            ParmNewPurchaseFrom.VendorLocationRel.VendorName
                no label
                display as text
        display as text
            two column
                ParmNewVendor
                    label is "NewVendor"
                ParmNewSupplier.Vendor.VendorName
                    no label
            visible when (not SupplierNotEntered)
                single column
                    Supplier.RepresentativeText
                        label is "CurrentSupplier"
                single column
                    SupplierSourceId.RepresentativeText
                        label is "CurrentSupplierContact"
                single column
                    Vendor.RepresentativeText
                        label is "CurrentVendor"
                two column
                    ApPoPurchaseFrom.PurchaseFromLocation
                        label is "CurrentPurchaseFrom"
                    ApPoPurchaseFrom.PurchaseFromLocation.VendorLocationRel.VendorName
                        no label
        single column
            visible when (Supplier.Active)
                InactivateOldSupplier
                    label is "InactivateCurrentSupplier"
            visible when (HasBeenActivated)
                text of "VendorItemsWillBeCreatedForActiveItemMasterItemsForTheNewVendor"
            visible when (not SupplierNotEntered)
                InactivateOldVendorItems
                    label is "InactivateOldVendorItemsIfVendorChanged"
        visible when (OpenSupplierChangeReqLinesExist)
            single column
                UpdateVendorOnRequisitionLines
        visible when (not SupplierNotEntered)
            single column
                ChangeReplenishmentVendor
                    label is "ChangeReplenishmentVendorAndPurchaseFromForAllRelatedReplenishmentItems"
