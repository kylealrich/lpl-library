ProjectionParameterForm is a Form
    is primary
    title is "DepreciationProjection"
    Layout
        two column
            AssetProjectionParameter
                label is "Projection"
            Description
        paragraph
            Book
            Book.Description
                no label
                display as text
            Book.Currency
                no label
                display as text
        paragraph
            Company
            Company.Name
                no label
                display as text
        single column
            paragraph
                Method
                    label is "Method"
                    select is CustomMethodList
                Method.Description
                    no label
                    display as text
                text of "Or"
                MethodComputeProjection
                    label is "CustomCompute"
        blank line
        two column
            Convention
                label is "Convention"
            InServiceDate
                label is "InServiceDate"
        two column
            Basis
                label is "Basis"
            Life
                label is "Life"
        two column
            LifeRemaining
                label is "LifeRemaining"
            YearPlacedInService
                label is "YearPlacedInService"
        two column
            SalvageValue
                label is "SalvageValue"
            YearToDateDepreciation
                label is "YearToDateDepreciation"
        single column
            LifeToDateDepreciation
                label is "LifeToDateDepreciation"
