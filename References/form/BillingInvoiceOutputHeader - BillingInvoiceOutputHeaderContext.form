BillingInvoiceOutputHeaderContext is a Form
    title is "BillingInvoiceOutputHeader"
    Layout
        three column distributed
            column1
                FinanceEnterpriseGroup
                    display as text
                Company
                    display as text
                ProcessLevel
                    display as text
                InvoiceDate
                    display as text
                paragraph
                    AddSubtract
                        display as text
                    CutoffDays
                        no label
                        display as text
                SelectOption
                    display as text
            column2
                InvFormOption
                    display as text
                OscCommentCode
                    display as text
                Alignments
                    display as text
                paragraph
                    InvcPrefixGrp
                        display as text
                    FrInvcNumber
                        no label
                        display as text
                    ToInvcNumber
                        no label
                        display as text
                EdiInvcOption
                    display as text
                InvcFormType
                    display as text
            column3
                CreateDate
                    display as text
                TotalRecordCount
                    display as text
                paragraph
                    OutputFile
                        compact format
                    visible when (HasAttachment)
                        button of "View"
                            view OutputFile.File
                            display as link
