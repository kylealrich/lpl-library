EFTExtract is an ActionForm
    action is EFTExtract
    Layout
        paragraph
            FinanceEnterpriseGroup
            FinanceEnterpriseGroup.EnterpriseGroup.Description
                no label
                display as text
        paragraph
            CustGroup
                label is "CustomerGroup"
                select is CustomerGroups
            CustGroup.Name
                no label
                display as text
        single column
            text of "Or"
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        single column
            ProcessDate
                label is "ProcessingDate"
        paragraph
            OverrideDateFr
                label is "FromOverrideDate"
            OverrideDateTo
                label is "ToOverrideDate"
        paragraph
            BankTransactionCode
                label is "PaymentCode"
                select is CashPaymentAndOriginReceivablesList
            BankTransactionCode.Description
                no label
                display as text
        paragraph
            TermsCode
                label is "PaymentTerms"
            TermsCode.Description.Desc[1]
                no label
                display as text
        single column
            Update
            DataFileName
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                EFTExtractReport_RDG
                EFTExtractAdjustment_RDG
            column2 of 25%
                EFTExtractReport_XPT
                EFTExtractAdjustment_XPT
