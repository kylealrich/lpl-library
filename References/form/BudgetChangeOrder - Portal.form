Portal is a CompositeForm
    title is "BudgetChangeRequest"
    context form is PortalControlForm
    show panel control on left
    LinesPanel is a Panel
        title is "Lines"
        list is BudgetChangeOrderLine set.PortalLines
    CommentsPanel is a Panel
        title is "Comments"
        list is BudgetChangeOrderComment set.PortalCommentsForChangeOrder
