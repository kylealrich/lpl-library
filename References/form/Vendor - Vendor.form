Vendor is a CompositeForm
    is primary
    title is "<DerivedFormTitle>"
    context form is VendorContext
        background is theme color
    Main is a Panel
        form is Main
    VendorBank is a Panel
        title is "Banking"
        form is VendorBank
    Options is a Panel
        form is Options
    Balances is a Panel
        list is AllVendorBalanceMasterRel.VendorLocationBalancePanelList
    WithholdingBalances is a Panel
        visible when (HasWithholdingBalances)
        list is VendorWithholdingBalance set.VendorWithholdingBalanceList
    Contacts is a Panel
        list is VendorContactRel.VendorContactsList
    Locations is a Panel
        title is "Locations<DerivedLocationCount>"
        list is VendorLocation set.VendorLocationsPanelList
    RelatedVendors is a MultiListPanel
        list is PayToVendor set.PayToVendorCardList
        list is CrossReferenceVendor set.CrossReferenceVendorDetailList
        list is VendorMappingTableInterfacesRel.VendorMappingTableInterfaceList
            visible when (InterfacedVendorsExist)
    Certifications is a MultiListPanel
        list is VendorCertificationsRel.CertificationCodeAssignmentsVendorPanel
        list is VendorDiversityCodesRel.DiversityCodeCertificationsVendorPanel
    CompanyDefaults is a Panel
        list is VendorCompanyDefaultRel.VendorCompanyDefaultCodes
    RelatedMerchants is a Panel
        list is MerchantCrossReferenceRel.VendorMerchantCrossReferences
    Comments is a Panel
        title is "Comments<DerivedVendorCommentsCount>"
        list is VendorCommentsRel.VendorComments
    Attachments is a Panel
        title is "Attachments<DerivedVendorAttachmentsCount>"
        list is VendorAttachmentsRel.Attachments
    MultipleTaxAreas is a Panel
        visible when (VertexResultsExist)
        list is VertexTaxAreaIdResultsRel.TaxAreaResultList
    VertexError is a Panel
        visible when (VertexErrorExist)
        list is VertexWebserviceErrorRel.VertexTaxAreaIdErrorList
    VendorWithholdingCodes is a Panel
        title is "WithholdingCodes"
        visible when (AllowWithholdingCodeOverride)
        list is VendorWithholdingCodesRel.VendorWithholdingCodeFromVendorList
    GlobalWithholdingDefault is a Panel
        visible when (VendorGroup.GlobalWithholding)
        list is VendorWithholdingDefaultRel.VendorWithholdingDefaultList
    DistributionRestrictions is a Panel
        list is VendorAccountRestriction set.primary
    InvoicesAndPayments is a MultiListPanel
        visible when (HasInvoicesOrPayments)
        list is PayablesInvoiceRel.InvoicesForAVendor
        list is CashLedgerPayablesPaymentRel.PaymentsForAVendor
    UserFields is a Panel
        form is UserFields
