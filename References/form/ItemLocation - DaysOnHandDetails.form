DaysOnHandDetails is a Form
    Layout
        visible when (IsActiveAndNotDiscontinued)
            paragraph
                DaysOnHandAlert
                    no label
                    display as tag
                        color of yellow when (DaysOnHandYellowAlert)
                        color of red when (DaysOnHandRedAlert)
        display as text
            three column table
                column headers
                    "Usage"
                    "AverageDailyUsage"
                    "DaysOnHand"
                row headers
                    "7Days"
                    "30Days"
                    "90Days"
                    "365Days"
                column1
                    TransactionLinesColumnarRel.UsageIn7Days
                        no label
                        right align
                    TransactionLinesColumnarRel.UsageIn30Days
                        no label
                        right align
                    TransactionLinesColumnarRel.UsageIn90Days
                        no label
                        right align
                    TransactionLinesColumnarRel.UsageIn365Days
                        no label
                        right align
                column2
                    TransactionLinesColumnarRel.AverageDailyUsage7Days
                        no label
                        right align
                    TransactionLinesColumnarRel.AverageDailyUsage30Days
                        no label
                        right align
                    TransactionLinesColumnarRel.AverageDailyUsage90Days
                        no label
                        right align
                    TransactionLinesColumnarRel.AverageDailyUsage365Days
                        no label
                        right align
                column3
                    TransactionLinesColumnarRel.DaysOnHand7Days
                        no label
                        right align
                        display "<DaysOnHandNoMovementMessage>" when (UsageIn7DaysNoMovementForm)
                    TransactionLinesColumnarRel.DaysOnHand30Days
                        no label
                        right align
                        display "<DaysOnHandNoMovementMessage>" when (UsageIn30DaysNoMovementForm)
                    TransactionLinesColumnarRel.DaysOnHand90Days
                        no label
                        right align
                        display "<DaysOnHandNoMovementMessage>" when (UsageIn90DaysNoMovementForm)
                    TransactionLinesColumnarRel.DaysOnHand365Days
                        no label
                        right align
                        display "<DaysOnHandNoMovementMessage>" when (UsageIn365DaysNoMovementForm)
