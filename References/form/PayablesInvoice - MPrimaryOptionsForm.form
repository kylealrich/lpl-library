MPrimaryOptionsForm is a Form
    title is "PrimaryOptionsForm"
    Layout
        visible when (false)
            InvoiceAmount.AssignToCurrency
        header2 of "Codes"
        visible when (DisplayUpdateableReportableIncome)
            two column distributed
                column1
                    IncomeCode
                        label is "DefaultIncomeCode"
                    TransientReportableAmount
                column2
                    IncomeCode.Description
                        no label
                        display as text
        visible when (not DisplayUpdateableReportableIncome)
            display as text
                two column distributed
        visible when (IsOnHold)
            single column
                InvoiceHoldCode
                    label is "HoldCode"
                    display as text
        single column
            button of "PutOnHold"
                visible when (IsManualHoldable)
                action is PutInvoiceOnHold
                display as link
        two column distributed
            column1
                DiversityCode
                button of "UpdateDiversityCode"
                    visible when (UpdateDiversityCodeValid)
                    action is UpdateDiversityCode
                    display as link
            column2
                DiversityCode.Description
                    no label
                    display as text
        two column distributed
            column1
                AuthorityCode
                RetainageAccrualCode
            column2
                AuthorityCode.Description
                    no label
                    display as text
                RetainageAccrualCode.Description
                    no label
                    display as text
        header2 of "Miscellaneous"
        single column
            InvoiceReference
            Reference
            ReceiptOfInvoiceDate
                label is "InvoiceReceiptDate"
            BillFromLocation
                label is "BillFrom"
            BillFromLocation.VendorName
                no label
                display as text
            BillToProcessLevel
                label is "BillTo"
            BillToProcessLevel.Name
                no label
                display as text
            CertificationNumber
            CertificationDate
        header2 of "Currency"
        visible when (UpdateCurrencyTableValid)
            four column table
                column headers
                    "_"
                    "AmountOverride"
                    "RateOverride"
                    "Currency"
                column1
                    text of "Functional"
                    visible when (InvoiceAmount.AccountingEntity.AlternateCurrencyExists)
                        text of "Alternate"
                    visible when (InvoiceAmount.AccountingEntity.AlternateCurrency2Exists)
                        text of "Alternate2"
                    visible when (InvoiceAmount.AccountingEntity.AlternateCurrency3Exists)
                        text of "Alternate3"
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        text of "Report1"
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        text of "Report2"
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        text of "Report3"
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        text of "Report4"
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                        text of "Report5"
                column2
                    InvoiceAmount.FunctionalAmount.EnteredCurrencyAmount
                        no label
                    visible when (InvoiceAmount.AccountingEntity.AlternateCurrencyExists)
                        InvoiceAmount.AlternateAmount.EnteredCurrencyAmount
                            no label
                    visible when (InvoiceAmount.AccountingEntity.AlternateCurrency2Exists)
                        InvoiceAmount.AlternateAmount2.EnteredCurrencyAmount
                            no label
                    visible when (InvoiceAmount.AccountingEntity.AlternateCurrency3Exists)
                        InvoiceAmount.AlternateAmount3.EnteredCurrencyAmount
                            no label
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        InvoiceAmount.ReportAmount1.EnteredCurrencyAmount
                            no label
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        InvoiceAmount.ReportAmount2.EnteredCurrencyAmount
                            no label
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        InvoiceAmount.ReportAmount3.EnteredCurrencyAmount
                            no label
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        InvoiceAmount.ReportAmount4.EnteredCurrencyAmount
                            no label
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                        InvoiceAmount.ReportAmount5.EnteredCurrencyAmount
                            no label
                column3
                    InvoiceAmount.FunctionalAmount.EnteredCurrencyRate
                        no label
                    visible when (InvoiceAmount.AccountingEntity.AlternateCurrencyExists)
                        InvoiceAmount.AlternateAmount.EnteredCurrencyRate
                            no label
                    visible when (InvoiceAmount.AccountingEntity.AlternateCurrency2Exists)
                        InvoiceAmount.AlternateAmount2.EnteredCurrencyRate
                            no label
                    visible when (InvoiceAmount.AccountingEntity.AlternateCurrency3Exists)
                        InvoiceAmount.AlternateAmount3.EnteredCurrencyRate
                            no label
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        InvoiceAmount.ReportAmount1.EnteredCurrencyRate
                            no label
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        InvoiceAmount.ReportAmount2.EnteredCurrencyRate
                            no label
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        InvoiceAmount.ReportAmount3.EnteredCurrencyRate
                            no label
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        InvoiceAmount.ReportAmount4.EnteredCurrencyRate
                            no label
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                        InvoiceAmount.ReportAmount5.EnteredCurrencyRate
                            no label
                column4
                    Company.AccountingEntity.FunctionalCurrency.Description
                        no label
                        display as text
                    visible when (InvoiceAmount.AccountingEntity.AlternateCurrencyExists)
                        Company.AccountingEntity.AlternateCurrency.Description
                            no label
                            display as text
                    visible when (InvoiceAmount.AccountingEntity.AlternateCurrency2Exists)
                        Company.AccountingEntity.AlternateCurrency2.Description
                            no label
                            display as text
                    visible when (InvoiceAmount.AccountingEntity.AlternateCurrency3Exists)
                        Company.AccountingEntity.AlternateCurrency3.Description
                            no label
                            display as text
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                        Company.FinanceEnterpriseGroup.ReportCurrencyOne.Description
                            no label
                            display as text
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                        Company.FinanceEnterpriseGroup.ReportCurrencyTwo.Description
                            no label
                            display as text
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                        Company.FinanceEnterpriseGroup.ReportCurrencyThree.Description
                            no label
                            display as text
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                        Company.FinanceEnterpriseGroup.ReportCurrencyFour.Description
                            no label
                            display as text
                    visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                        Company.FinanceEnterpriseGroup.ReportCurrencyFive.Description
                            no label
                            display as text
