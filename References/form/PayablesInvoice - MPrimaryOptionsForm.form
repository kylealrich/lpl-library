                        visible when (InvoiceAmount.AccountingEntity.AlternateCurrencyExists)
                            InvoiceAmount.AlternateAmount.EnteredCurrencyAmount
                                no label
                        visible when (InvoiceAmount.AccountingEntity.AlternateCurrency2Exists)
                            InvoiceAmount.AlternateAmount2.EnteredCurrencyAmount
                                no label
                        visible when (InvoiceAmount.AccountingEntity.AlternateCurrency3Exists)
                            InvoiceAmount.AlternateAmount3.EnteredCurrencyAmount
                                no label
                        visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                            InvoiceAmount.ReportAmount1.EnteredCurrencyAmount
                                no label
                        visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                            InvoiceAmount.ReportAmount2.EnteredCurrencyAmount
                                no label
                        visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                            InvoiceAmount.ReportAmount3.EnteredCurrencyAmount
                                no label
                        visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                            InvoiceAmount.ReportAmount4.EnteredCurrencyAmount
                                no label
                        visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                            InvoiceAmount.ReportAmount5.EnteredCurrencyAmount
                                no label
                    column3
                        InvoiceAmount.FunctionalAmount.EnteredCurrencyRate
                            no label
                        visible when (InvoiceAmount.AccountingEntity.AlternateCurrencyExists)
                            InvoiceAmount.AlternateAmount.EnteredCurrencyRate
                                no label
                        visible when (InvoiceAmount.AccountingEntity.AlternateCurrency2Exists)
                            InvoiceAmount.AlternateAmount2.EnteredCurrencyRate
                                no label
                        visible when (InvoiceAmount.AccountingEntity.AlternateCurrency3Exists)
                            InvoiceAmount.AlternateAmount3.EnteredCurrencyRate
                                no label
                        visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                            InvoiceAmount.ReportAmount1.EnteredCurrencyRate
                                no label
                        visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                            InvoiceAmount.ReportAmount2.EnteredCurrencyRate
                                no label
                        visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                            InvoiceAmount.ReportAmount3.EnteredCurrencyRate
                                no label
                        visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                            InvoiceAmount.ReportAmount4.EnteredCurrencyRate
                                no label
                        visible when (InvoiceAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                            InvoiceAmount.ReportAmount5.EnteredCurrencyRate
                                no label
                    column4
                        Company.AccountingEntity.FunctionalCurrency.Description
                            no label
                        visible when (InvoiceAmount.AccountingEntity.AlternateCurrencyExists)
                            Company.AccountingEntity.AlternateCurrency.Description
                                no label
                        visible when (InvoiceAmount.AccountingEntity.AlternateCurrency2Exists)
                            Company.AccountingEntity.AlternateCurrency2.Description
                                no label
