        header2 of "Global"
        single column
            ForeignEconomicRegulatoryCode
        visible when (FercTypeEntered)
            display as text
                single column
                    ForeignEconomicRegulatoryCode.FercType
                    DerivedPaymentType
                        label is "PaymentType"
                    DerivedPaymentTypeDesc
                        label is "Description"
        display as text
            paragraph
                Vendor.LanguageCode
                    label is "Locale"
        visible when (IDMPrinterValidForSetup)
            header2 of "Printer"
            paragraph
                IDMPrinter
                IDMPrinter.Description
                    no label
                    display as text
