DebtInstrumentIdentifierDetail is a Form
    Layout
        two column
            DebtInstrument.IssueDate
                display as text
            MaturityDate
        paragraph
            LineOfCreditDraw
            visible when (LineOfCreditDraw)
                LineOfCredit
                    no label
                    select is LineOfCreditList
        paragraph
            BenchmarkGroup.Benchmark
            BenchmarkGroup.Benchmark.Description
                no label
                display as text
        paragraph
            visible when (BenchmarkGroup.Benchmark entered)
                BenchmarkInterestRate
                    display as text
                text of "\\asOf<current corporate date>"
                    italics
        single column
            Active
        header2 of "Principal"
        two column
            paragraph
                OriginalPrincipalAmount
                DebtInstrument.DebtCurrency
                    no label
                    display as text
            paragraph
                OutstandingPrincipal
                    display as text
                DebtInstrument.DebtCurrency
                    no label
                    display as text
        visible when (DebtInstrument.DebtCurrencyNotCorporateBaseCurrency)
            paragraph
                CorporateBasePrincipalAmount
                    display as text
                CashManagementGroup.Currency
                    no label
                    display as text
        visible when (DebtInstrument.DebtCurrencyNotLocationCurrency)
            paragraph
                LocationCurrencyPrincipalAmount
                    display as text
                DebtInstrument.CashManagementLocation.Currency
                    no label
                    display as text
        single column
            PrincipalCalculation
            visible when (PrincipalCalculation.Manual)
                button of "PrincipalPayments"
                    link is DebtInstrumentIdentifierPrincipalLink
                    display as link
            visible when (PrincipalCalculation.StraightLine)
                StraightLineMethod
                visible when (StraightLineMethod.ConstantPayment)
                    ConstantPaymentAmount
                        display as text
                visible when (StraightLineMethod.ConstantPrincipal)
                    ConstantPrincipalAmount
                        display as text
        header2 of "Interest"
        two column
            InterestFrequency
            paragraph
                FirstPaymentDate
                    label is "FrequencyStartDate"
        two column
            InterestPayment
            paragraph
                visible when (InterestPayment.SpecifiedDays)
                    SpecifiedDays
                FirstInterestPaymentDate
                    label is "FirstPaymentDate"
        two column
            DayCountConvention
            BusinessDayConvention
        two column
            FixedInterestRate
            FixedResetDate
        visible when (IsVariableRate)
            paragraph
                DebtRateTable
                DebtRateTable.Description
                    no label
                    display as text
            two column
                paragraph
                    SpreadRate
                    RateFactor
                paragraph
                    RateFloor
                    RateCap
            paragraph
                TotalCurrentDateInterestRate
                    display as text
                text of "\\asOf<current corporate date>"
                    italics
            two column
                RateDependentOnRating
                visible when (RateDependentOnRating)
                    paragraph
                        DebtNotification.FinanceResource
                            label is "Notification:_Resource"
                        text of "Or"
                        DebtNotification.FinanceTeam
                            label is "Team"
                        visible when (DebtNotification.FinanceResource entered)
                            DebtNotification.FinanceResource.PreferredFirstAndLastName
                                no label
                                display as text
                        visible when (DebtNotification.FinanceTeam entered)
                            DebtNotification.FinanceTeam.Description
                                no label
                                display as text
