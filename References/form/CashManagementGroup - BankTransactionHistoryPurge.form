BankTransactionHistoryPurge is an ActionForm
    action is BankTransactionHistoryPurge
    title is "BankTransactionHistoryPurge"
    Layout
        single column
            PrmCashManagementGroup
                label is "CashManagementGroup"
        three column
            PrmCashCode
                label is "CashCode"
            PrmCashCode.Description
                no label
                display as text
            PrmCashCode.Currency
                no label
        two column
            PrmCashCodeGroup
                label is "_orCashCodeGroup"
                select is CashCodeGroup
            PrmCashCodeGroup.Description
                no label
                display as text
        blank line
        single column
            PrmCashManagementGroup.FinanceEnterpriseGroup.PurgeCutOffDate
                label is "CutoffDate"
                display as text
        blank line
        single column
            PrmPurgeTransaction
                label is "PurgeCashLedgerTransaction"
            PrmCbtBankInstType
                label is "BankTransactionType"
        two column
            visible when (PrmCbtBankInstType not entered)
                PrmBankTransactionCode
                    label is "TransactionCode"
                    select is BankTransactionCodeList
            visible when (PrmCbtBankInstType.CashPayments)
                PrmBankTransactionCode
                    label is "TransactionCode"
                    select is CashPayments
            visible when (PrmCbtBankInstType.BoePayments)
                PrmBankTransactionCode
                    label is "TransactionCode"
                    select is BOEPayments
            visible when (PrmCbtBankInstType.BankServices)
                PrmBankTransactionCode
                    label is "TransactionCode"
                    select is BankServices
            PrmBankTransactionCode.Description
                no label
                display as text
        two column
            column1
                PrmCashManagementAccount
                    label is "BankAccount"
                PrmPurgeBankStatement
                    label is "PurgeBankStatement"
            column2
                PrmPurgeBankStatementReconciliation
                    label is "PurgeBankStatementReconciliation"
                PrmPurgeNonCashLedgerTransaction
                    label is "PurgeNonCashLedgerTransaction"
        two column
            column1
                PrmPurgeFundTransferHeader
                    label is "PurgeCashLedgerFundTransferHeader"
                PrmPurgeCashReceiptHeader
                    label is "PurgeCashLedgerCashReceiptHeader"
                PrmPurgeBankAccountBalance
                    label is "PurgeBankAccountBalance"
            column2
                PrmPurgePayment
                    label is "PurgeCashLedgerPayment"
                PrmPurgeEFTransferTransaction
                    label is "PurgeCashLedgerElectronicFundsTransferTransaction"
                PrmPurgeReturnedPayment
                    label is "PurgeCashLedgerReturnedPayment"
