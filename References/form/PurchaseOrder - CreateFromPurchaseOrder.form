CreateFromPurchaseOrder is an ActionForm
    action is Unreleased.CreateFromPurchaseOrder
        on completion navigate to ViewPO
    Layout
        two column
            PrmCompany
                label is "Company"
            PrmCopyFromPurchaseOrder
                label is "CopyFromPurchaseOrder"
                select is ReleasedPurchaseOrdersForSelect
        two column table
            column headers
                "OriginalValue"
                "NewValue"
            row headers
                "Company"
                "Buyer"
                "DeliveryDate"
                "Vendor"
                "PurchaseFrom"
                "ShipTo"
                "PurchaseOrderDate"
            column1
                PrmCopyFromPurchaseOrder.Company.Name
                    no label
                    display as text
                PrmCopyFromPurchaseOrder.Buyer.Employee.PresentationNameSnapshot
                    no label
                    display as text
                PrmCopyFromPurchaseOrder.DefaultDeliveryDate
                    no label
                    display as text
                PrmCopyFromPurchaseOrder.Vendor
                    no label
                    display as text
                PrmCopyFromPurchaseOrder.PurchaseFromLocation
                    no label
                    display as text
                PrmCopyFromPurchaseOrder.ShipToLocation
                    no label
                    display as text
                PrmCopyFromPurchaseOrder.PurchaseOrderDate
                    no label
                    display as text
            column2
                PrmCompany
                    no label
                PrmBuyer
                    no label
                PrmDefaultDeliveryDate
                    no label
                PrmVendor
                    no label
                PrmPurchaseFromLocation
                    no label
                    select is NonRemitToActivePurchaseFromLocations
                PrmShipToLocation
                    no label
                PrmPurchaseOrderDate
                    no label
        single column
            PrmIsReleased
                label is "Release"
                left align
                align as label
        header2 of "LineCopyOptions"
        paragraph
            PrmCopyUnitCost
                label is "UnitCost"
                left align
                align as label
            PrmCopyIgnoreCancelQuantity
                label is "IgnoreCancelQuantity"
                left align
                align as label
            PrmCopyDistributions
                label is "Distributions"
                left align
                align as label
            PrmCopyComments
                label is "Comments"
                left align
                align as label
            PrmCopyAddOnCosts
                label is "AddOnCosts"
                left align
                align as label
            PrmCopyTaxData
                label is "TaxData"
                left align
                align as label
            PrmCopySourceRecords
                label is "SourceRecords"
                left align
                align as label
        two column
            PrmNewDefaultTaxCode
                label is "NewDefaultTaxCode"
                select is TaxCodeFilteredList
            PrmNewRequestingLocation
                label is "NewRequestingLocation"
            PrmRequester
                label is "Requester"
            PrmSourceDocument
                label is "SourceDocument"
