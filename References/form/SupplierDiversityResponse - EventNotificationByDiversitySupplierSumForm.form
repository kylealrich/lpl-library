EventNotificationByDiversitySupplierSumForm is a SummaryForm
    title is "_"
    Layout
        visible when (ContextSupplyManagementReport.SupplierDetail)
        form is EventNotificationAwardByDiversityRel.SourcingEventNotificationSumForm
        header5 of "TotalsForSupplier\\:_<SupplierDiversityResponse.Supplier.SupplierName>"
        three column
            column1
                DerivedTotalEventsBidBySupplier
                    label is "TotalEventsBid"
                    display when blank
                DerivedTotalEventsAwardedBySupplier
                    label is "TotalEventsAwarded"
                    display when blank
                DerivedEventsBidPercentBySupplier
                    label is "EventBidSuccess"
                    display when blank
            column2
                DerivedTotalEventLinesBidBySupplier
                    label is "TotalLinesBid"
                    display when blank
                DerivedTotalEventLinesAwardedBySupplier
                    label is "TotalLinesAwarded"
                    display when blank
                DerivedEventLinesBidPercentBySupplier
                    label is "LineBidSuccess"
                    display when blank
            column3
                DerivedBidTotalBySupplier
                    label is "BidTotal"
                    display when blank
                DerivedAwardedTotalBySupplier
                    label is "TotalAwardedAmount"
                    display when blank
                DerivedTotalBidPercentBySupplier
                    label is "AmountBidSuccess"
                    display when blank
