PrintLabelActionForm is an ActionForm
    action is PrintLabel
    title is "PrintLabelFor<Item.ItemDescriptionAbbreviation>"
    Layout
        single column
            paragraph
                PrmMobileSupplyChainPrinter
                    select is MSCMLabelCompanyPrinterSelect
            PrmNumberOfCopies
            visible when (IsInventoryLocation)
                PrmLabelTemplate
                    select is StockItemLabelTemplateList
            visible when (not IsInventoryLocation)
                PrmLabelTemplate
                    select is ParItemLabelTemplateList
