GrantorForm is a Form
    title is "GrantReporting"
    Layout
        visible when (InEnterpriseStructure)
            visible when (FundingType.ExternalGrant)
                three column distributed
                    column1
                        GrantID
                            label is "GrantI\\D"
                            show up to 20 characters
                    column2
                        PrimeFlag
                    column3
                        PreAwardID
                paragraph
                    PrimeSponsor.Company
                        label is "PrimeSponsor"
                    PrimeSponsor.Customer
                        label is "ReceivableCustomer"
                        select is CustomerContextCompanySelect
                    PrimeSponsor.Customer.SearchName
                        no label
                        display as text
                header2 of "S\\E\\F\\AReporting"
                three column distributed
                    column1
                        SEFAFlag
                            label is "IncludeInS\\E\\F\\A"
                    column2
                        PrimeSponsorAwardNumber
                            show up to 15 characters
                    column3
                        paragraph
                            MajorCategory
                                show up to 15 characters
                            MajorCategory.Description
                                no label
                                display as text
                                show up to 15 characters
                three column distributed
                    column1
                        CFDANumber
                            show up to 15 characters
                    column2
                        ProgramTitle
                            show up to 15 characters
