MobileSupplyChainDeliveryLineFormContext is a Form
    Layout
        six column distributed
            column1
                MobileSupplyChainDeliveryTicket.DeliverToCompanyLocation.MobileSupplyChainCompany
                    label is "DeliverToCompany"
                    display as text
                MobileSupplyChainDeliveryTicket.DeliveryType
                    display as text
                DerivedItem
                    display as text
            column2
                MobileSupplyChainDeliveryTicket.DeliverToCompanyLocation.MobileSupplyChainLocation
                    label is "DeliverToLocation"
                    display as text
                visible when (MobileSupplyChainDeliveryTicket.DocumentNumber entered)
                    MobileSupplyChainDeliveryTicket.DocumentNumber
                        display as text
                visible when (MobileSupplyChainDeliveryTicket.DocumentNumber not entered)
                    DerivedBlankDisplay
                        label is "DocumentNumber"
                        display as text
                OrderedQuantity
                    display as text
            column3
                MobileSupplyChainDeliveryTicket.InforTrackingNumber
                    display as text
                visible when (MobileSupplyChainDeliveryTicket.PurchaseOrder entered)
                    MobileSupplyChainDeliveryTicket.PurchaseOrder
                        display as text
                visible when (MobileSupplyChainDeliveryTicket.PurchaseOrder not entered)
                    DerivedBlankDisplay
                        label is "PurchaseOrder"
                        display as text
                Quantity
                    label is "DeliveredQuantity"
                    display as text
            column4
                MobileSupplyChainDeliveryUnit.CarrierTrackingNumber
                    display as text
                visible when (Requisition entered)
                    Requisition
                        display as text
                visible when (Requisition not entered)
                    DerivedBlankDisplay
                        label is "Requisition"
                        display as text
            column5
                LastPerformedAction
                    display as text
                Requester
                    display as text
            column6
                MobileSupplyChainDeliveryTicket.create stamp
                    label is "CreateDate"
                    display as text
                MobileSupplyChainDeliveryTicket.DerivedVendorName
                    display as text
