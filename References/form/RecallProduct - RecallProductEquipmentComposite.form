RecallProductEquipmentComposite is a CompositeForm
    title is "Product"
    context form is RecallProductContext
        background is theme color
    ProductIdentifiers is a Panel
        visible when (not RecallNotice.Equipment)
        form is ProductIdentifiers
    SerialNumbers is a Panel
        title is "SerialNumbers<EquipmentSerialNumberCount>"
        list is RecallEquipmentSerial set.SerialNumbersList
    Questions is a Panel
        title is "Questions<ProductQuestionCount>"
        list is RecallProductQuestion set.RecallProductQuestions
