GLTransactionDetailSearchForm_Van is a SearchForm
    title is "SearchFinanceTransactions"
    Layout
        paragraph
            FinanceCodeBlock
                label is "FinanceStructure"
            FinanceCodeBlock.GeneralLedgerChartAccount.GeneralLedgerSubAccount
                label is "SubAccount"
        four column distributed
            AccountingEntity
                label is "From<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
            TransactionAmount
            CurrencyCode
            FinanceCodeBlock.Project.ProjectStatus
                label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Status"
        four column distributed
            SearchDateRange.Begin
                label is "FromTransactionDate"
            SearchDateRange.End
                label is "ToTransactionDate"
            PostingDateRange.BeginDate
                label is "FromPostingDate"
            PostingDateRange.EndDate
                label is "ToPostingDate"
        four column distributed
            System
            GeneralLedgerEvent
                label is "Event"
            JournalizeGroup
            SearchGroup
        four column distributed
            Status
                initial value is "1"
            Resource
                label is "EmploymentI\\D"
            LaborDistribution
            IndirectBurden
        four column distributed
            Billed
            RevenueRecognized
            Capitalize
            APPaid
        four column distributed
            HROrganizationUnit
            PayCode
            Job
            Position
