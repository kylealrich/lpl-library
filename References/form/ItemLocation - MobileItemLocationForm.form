MobileItemLocationForm is a CompositeForm
    title is "LocationDetails"
    context form is MobileItemLocationContext
    Actions
        action is save
        action is refresh
    Main is a Panel
        visible when (not CartParLocation)
        form is MobileItemLocationHeader
    DetailTracking is a Panel
        visible when (not CartParLocation)
        form is MobileDetailTrackingForm
    ItemLocationDetailForParItem is a Panel
        title is "Main"
        visible when (CartParLocation)
        form is MobileItemLocationDetailForParItem
    Reorder is a Panel
        visible when (not CartParLocation)
        form is Reorder
    Costs is a Panel
        visible when (not CartParLocation)
        form is Costs
    Sales is a Panel
        visible when (not CartParLocation)
        form is Sales
    Replenishment is a Panel
        list is ItemReplenishmentSourcesRel.MobileReplenishmentPriority
    UnitsOfMeasure is a Panel
        list is ItemUOMRel.MobileItemUOMsForView
    InventoryBalances is a Panel
        visible when (not CartParLocation)
        form is MobileInventoryBalance
    Transactions is a Panel
        visible when (IsInventoryLocation)
        list is InventoryTransactionLinesRel.MobileTransactionLinesForAnItemLocation
    CostHistory is a Panel
        visible when (IsInventoryLocation)
        list is InventoryCostHistoryRel.MobileCostHistoryForAnItemLocation
    DemandAndSupply is a Panel
        visible when (IsInventoryLocation)
        list is WarehouseDemandLinesRel.MobileDemandAndSupply
