TaxAnalysisReport is an ActionForm
    action is TaxAnalysisReport
    Layout
        two column distributed
            column1
                paragraph
                    TaxEntity
                    TaxEntity.Name
                        no label
                        display as text
                paragraph
                    Company
                    Company.Name
                        no label
                        display as text
                paragraph
                    TranCurrency
                        label is "Currency"
                    TranCurrency.Description
                        no label
                        display as text
                SourceCode
                    label is "Source"
                paragraph
                    CurrentTax
                    UpdateOption
                FromDate
                    label is "FromDate"
                DateOption
                SortOption
                    label is "ReportSequence"
                ReportFormat
                    label is "InvoiceAmountFormat"
            column2
                paragraph
                    AccountingEntityGroup
                        label is "TaxEntityGroup"
                paragraph
                    CompanyGroup
                        label is "CompanyGroup"
                    CompanyGroup.Description
                        no label
                        display as text
                paragraph
                    ProcLevel
                        label is "ProcessLevel"
                        select is APProcessLevels
                    ProcLevel.Name
                        no label
                        display as text
                TaxCode
                    select is TaxCodesForTaxAnalysisReport
                CutOffDate
                    label is "CutoffDate"
                ThruDate
                    label is "ToDate"
                PrintMemo
                    label is "PrintCurrencyMemoLines"
                ReportOption
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                TaxAnalysisReportReport_RDG
            column2 of 25%
                TaxAnalysisReportReport_XPT
