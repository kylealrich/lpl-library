AdvancedSearchAlertForm is a Form
    title is "ContractAlerts"
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        three column distributed
            ExpirationDateYellowAlert
            ExpirationDateRedAlert
            ContractLineErrorsExistNotInactiveVendorItem
                label is "HasErrors"
        three column distributed
            OnHold
            HasMembersToRelease
                label is "NeedToReleaseMembers"
            ExpiringContract
                label is "ExpiringContracts"
        three column distributed
            HasUnapprovedArticleOrTermChangesInProgress
                label is "TermsInApprovalProcess"
            ContractLineErrorsExistNotInactiveVendorItem
                label is "LineErrorsToFix"
            ManufacturerModificationsToPush
                label is "RunUpdateDistributorContract"
        single column
            UnsupportedIDMAttachmentsExist
