VendorBalanceRebuild is an ActionForm
    action is VendorBalanceRebuild
    Layout
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        single column
            UpdateOption
                label is "Update"
            VenClass
                label is "VendorClass"
                select is VendorClass
        paragraph
            FrVendor
                label is "FromVendor"
                select is VendorName
            ToVendor
                label is "ToVendor"
                select is VendorName
        paragraph
            RebuildOption
            UpdateCurrent
                label is "UpdateCurrentBalance"
        paragraph
            CyBegDate
                label is "CurrentYearBeginningDate"
            LyBegDate
                label is "LastYearBeginningDate"
        header3 of "LastYearPeriodEndingDates"
        four column table
            column1
                LyPeriodEnd.LyPeriodEnd[1]
                    label is "1"
                LyPeriodEnd.LyPeriodEnd[5]
                    label is "5"
                LyPeriodEnd.LyPeriodEnd[9]
                    label is "9"
                LyPeriodEnd.LyPeriodEnd[13]
                    label is "13"
            column2
                LyPeriodEnd.LyPeriodEnd[2]
                    label is "2"
                LyPeriodEnd.LyPeriodEnd[6]
                    label is "6"
                LyPeriodEnd.LyPeriodEnd[10]
                    label is "10"
            column3
                LyPeriodEnd.LyPeriodEnd[3]
                    label is "3"
                LyPeriodEnd.LyPeriodEnd[7]
                    label is "7"
                LyPeriodEnd.LyPeriodEnd[11]
                    label is "11"
            column4
                LyPeriodEnd.LyPeriodEnd[4]
                    label is "4"
                LyPeriodEnd.LyPeriodEnd[8]
                    label is "8"
                LyPeriodEnd.LyPeriodEnd[12]
                    label is "12"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                VendorBalanceRebuildReport_RDG
            column2 of 25%
                VendorBalanceRebuildReport_XPT
