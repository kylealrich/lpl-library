ContractLineSpecialItemWorkComposite is a CompositeForm
    is primary
    title is "ItemCreationInformation"
    context form is ContractLineSpecialItemWorkContext
    show panel control on left
    ItemInformation is a Panel
        form is ContractLineSpecialItemWorkForm
    ContractLineItemInformation is a Panel
        form is ContractLineItemInformationForm
    ItemMasterItemInformation is a Panel
        visible when (NewItemMasterExists)
        form is ItemMasterItemInformationForm
