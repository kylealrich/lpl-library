Main is a Form
    Layout
        two column distributed
            visible when (not RecordExists)
                column1
                    CashManagementGroup
                    paragraph
                        CashLedgerPaymentImport.CashCode
                        CashLedgerPaymentImport.CashCode.Description
                            no label
                            display as text
                        CashLedgerPaymentImport.CashCode.Currency
                            no label
                            display as text
                    CashLedgerPaymentImport.ReferenceNumber
                column2
                    CashLedgerPaymentImport.RunGroup
                    paragraph
                        CashLedgerPaymentImport.BankTransactionCode
                            label is "TransactionCode"
                        CashLedgerPaymentImport.BankTransactionCode.Description
                            no label
                            display as text
                    CashLedgerPaymentImport.SequenceNumber
        two column distributed
            column1
                paragraph
                    PayablesData.PayablesCompany
                        select is Company
                    PayablesData.PayablesCompany.Name
                        no label
                        display as text
                paragraph
                    PayablesData.Vendor
                        label is "Vendor"
                    PayablesData.Vendor.VendorName
                        no label
                        display as text
                paragraph
                    OverrideCompany.OrigOverrideCompany
                        label is "OverrideCompany"
                        select is Company
                    OverrideCompany.OrigOverrideCompany.Name
                        no label
                        display as text
                    OverrideCompany.OrigOverrideCompany.Currency
                        no label
                        display as text
                ApprovalCostCenter
            column2
                paragraph
                    ProcessLevel
                        label is "PayablesProcessLevel"
                        select is APProcessLevels
                    ProcessLevel.Name
                        no label
                        display as text
                paragraph
                    PayablesData.PayGroup
                        label is "PayGroup"
                        select is PayGroups
                    PayablesData.PayGroup.PostCompany
                        label is "PayGroupPostingCompany"
                        display as text
                TransactionDate
                    label is "Date"
                ApprovalCategory
        paragraph
            CommentText
                use text area of 5 lines
