MassIncomeCodeUpdate is an ActionForm
    action is MassIncomeCodeUpdate
    Layout
        header2 of "UpdatesInvoicesWithIncomeCodeSetOnVendor"
        paragraph
            PrmVendorGroup
                label is "VendorGroup"
            PrmVendorGroup.Description
                no label
                display as text
        paragraph
            PrmFromVendor
                label is "FromVendor"
            PrmToVendor
                label is "ToVendor"
        paragraph
            PrmVendorSelectionRule
                label is "VendorSelectionRule"
            PrmVendorSelectionRule.Condition
                no label
                display as text
        blank line
        paragraph
            PrmUpdatePaidInvoices
                label is "UpdatePaidInvoices"
            PrmPaidDateRange.Begin
                label is "FromPaidDate"
            PrmPaidDateRange.End
                label is "ToPaidDate"
        blank line
        paragraph
            PrmUpdateUnpaidInvoices
                label is "UpdateUnpaidInvoices"
            PrmInvoiceDateRange.Begin
                label is "FromInvoiceDate"
            PrmInvoiceDateRange.End
                label is "ToInvoiceDate"
        blank line
        paragraph
            PrmClearIncomeCode
                label is "ClearIncomeCode"
