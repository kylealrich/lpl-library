PayablesOnHoldForm is a Form
    is primary
    title is "InvoiceHoldSelection"
    Layout
        paragraph
            HoldCode
            HoldCode.Description
                no label
                display as text
        single column
            HoldRecordType
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            VendorGroup
            VendorGroup.Description
                no label
                display as text
        paragraph
            Vendor
            Vendor.VendorName
                no label
                display as text
        single column
            PayablesInvoice
                select is PayGroupInvoices
        paragraph
            ProcessLevel
            ProcessLevel.Name
                no label
                display as text
        single column
            PayablesInvoicePayment
                label is "Payment"
            create date
                label is "CreationDate"
                display as text
