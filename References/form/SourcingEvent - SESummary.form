SESummary is a Form
    title is "EventSummary"
    Layout
        display as text
            visible when (not InformalQuote)
                header2 of "Dates"
                visible when (PreviewDate entered)
                    single column
                        PreviewDate
                            label is "Preview"
                single column
                    DerivedTextOpenDate
                        label is "Open"
                visible when (not StepTwoStarted)
                    visible when (not BestAndFinalStarted)
                        single column
                            DerivedCloseDate
                                label is "Close"
                visible when (QandAOpenDate entered)
                    single column
                        DerivedTextQAOpen
                            label is "Q_andAOpen"
                        DerivedTextQAClose
                            label is "Q_andAClose"
                visible when (DisputeCloseDate entered)
                    single column
                        DerivedTextDisputeClose
                            label is "DisputeClose"
                single column
                    visible when (StepTwoStarted)
                        DerivedTextStepTwoCloseDate
                            label is "StepTwoCloseDate"
                    visible when (BestAndFinalStarted)
                        DerivedTextBestAndFinalCloseDate
                            label is "BestAndFinalCloseDate"
            header2 of "Description"
            Description
                use text area of 5 lines
            header2 of "Rules"
            two column
                SourcingEventType
                    label is "Type"
                Sealed
                    label is "SealedBid"
                CurrencyCode
                    label is "Currency"
                RequireAllLines
                    label is "AllLinesRequired"
            two column
                RequireBidBond
                    label is "BidBondRequired"
                visible when (RequireBidBond)
                    BidBondPercent
