ProcurementTemplatesForm is a CompositeForm
    is primary
    title is "ProcurementTemplate"
    context form is ProcurementTemplatesFormContext
        background is theme color
    Lines is a Panel
        visible when (not Locked)
        list is ProcurementTemplateLines2Rel.ProcurementTemplateLinesSecondary
            helper list is ContractLinesForProcurementTemplateRel.ContractLinesForProcurementTemplateHelperList
                action is IncludeInProcurementTemplateLine
                    invoked.PrmProcurementGroup = ProcurementGroup
                    invoked.PrmProcurementTemplate = ProcurementTemplate
                valid when (CanUpdateFromContract)
    LockedLines is a Panel
        title is "Lines"
        visible when (Locked)
        list is ProcurementTemplateLines2Rel.LockedProcurementTemplateLines
    Participants is a Panel
        visible when (not Locked)
        list is ProcurementTemplateParticipantsRel.ProcurementTemplateParticipantList
    LockedParticipants is a Panel
        title is "Participants"
        visible when (Locked)
        list is ProcurementTemplateParticipantsRel.LockedProcurementTemplateParticipantList
    PurchaseOrders is a Panel
        visible when (PurchaseOrdersExist)
        list is PurchaseOrdersCreatedRel.PurchaseOrdersFromTemplate
    Requisitions is a Panel
        visible when (RequisitionsExist)
        list is RequisitionsCreatedRel.RequisitionsFromTemplate
