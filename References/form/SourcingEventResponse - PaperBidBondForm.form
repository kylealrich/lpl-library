PaperBidBondForm is a Form
    title is "ViewPaperBidBond"
    Layout
        two column
            SourcingEvent.EventVersionString
                label is "Event#"
                display as text
            SourcingEvent.Name
                label is "EventName"
                display as text
        two column
            NotifiedSupplier.Supplier
                label is "Supplier"
                display as text
            NotifiedSupplier.Supplier.SupplierName
                label is "SupplierName"
                display as text
        two column
            PaperBidBond
                label is "PaperBidBond"
                display as text
                compact format
            button of "View"
                view PaperBidBond.File
                display as link
