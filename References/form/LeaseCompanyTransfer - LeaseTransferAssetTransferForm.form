LeaseTransferAssetTransferForm is a Form
    title is "AssetTransfer"
    Layout
        single column
            text of "PleaseCompleteTheAssetTransferBeforeTransferringLease"
                bold
            visible when (CanDisplayAssetTransferAction)
                button of "AssetTransfer"
                    visible when (not LeaseHasMultipleAssets)
                    link is LeasedSingleAssetNav
                button of "AssetTransfer"
                    visible when (LeaseHasMultipleAssets)
                    link is LeasedMultipleAssetsNav
            visible when (not CanDisplayAssetTransferAction)
                paragraph
                    text of "PleaseCompleteTheReleaseTransferActionToContinueTransferTheAssetAndThenTransferTheLease"
                        bold
