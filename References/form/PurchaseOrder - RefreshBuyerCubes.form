RefreshBuyerCubes is an ActionForm
    action is RefreshBuyerCubes
    title is "RefreshPurchasingCubes"
    Layout
        single column
            text of "SelectTheAnalyticCubesYouWouldLikeToRefresh:"
                header4
                italics
                color of blue
        blank line
        three column
            column1
                UpdatePurchaseOrder
                    label is "PurchaseOrder"
                UpdatePurchaseOrderLine
                    label is "PurchaseOrderLine"
                UpdatePurchaseOrderLineDistribution
                    label is "PurchaseOrderLineDistribution"
                UpdateVendorReturn
                    label is "VendorReturn"
                UpdateVendorReturnLine
                    label is "VendorReturnLine"
                UpdateMatchInvoiceMessage
                    label is "MatchInvoiceMessage"
            column2
                paragraph
                    PurchaseOrderAnalyticCubeRel.LastRefreshDate
                        display as text
                paragraph
                    PurchaseOrderLineAnalyticCubeRel.LastRefreshDate
                        display as text
                paragraph
                    PurchaseOrderLineDistributionAnalyticCubeRel.LastRefreshDate
                        display as text
                paragraph
                    VendorReturnAnalyticCubeRel.LastRefreshDate
                        display as text
                paragraph
                    VendorReturnLineAnalyticCubeRel.LastRefreshDate
                        display as text
                paragraph
                    MatchInvoiceMessageAnalyticCubeRel.LastRefreshDate
                        display as text
