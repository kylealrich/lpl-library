ContractImport is a Form
    title is "ContractInterface"
    Layout
        single column
            Description
                use text area of 3 lines
        two column
            column1 of 30%
                ContractType
                    select is BuyerContractTypesSelect
            column2 of 70%
                ContractSubtype
                    select is BuyerContractSubtypeSelect
        two column
            column1 of 30%
                ContractClassification
                    select is BuyerContractClassificationSelect
            column2 of 70%
                ContractSubclassification
                    select is BuyerContractSubclassificationSelect
        paragraph
            visible when (not ContractLinesExist)
                EffectiveDate
            visible when (ContractLinesExist)
                EffectiveDate
                    display as text
        two column
            column1 of 30%
                visible when (not ContractLinesExist)
                    ExpirationDate
                visible when (ContractLinesExist)
                    ExpirationDate
                        display as text
            column2 of 70%
                visible when (not ContractLinesExist)
                    ContractReviewDate
                        label is "ReviewDate"
                visible when (ContractLinesExist)
                    ContractReviewDate
                        label is "ReviewDate"
                        display as text
        visible when (FutureDatesEntered)
            two column
                FutureEffectiveDate
                    label is "NewEffectiveDate"
                    display as text
                FutureExpirationDate
                    label is "NewExpirationDate"
                    display as text
        paragraph
            Contact
                label is "PrimaryInternalContact"
            Contact.FirstLastName
                no label
                display as text
        two column
            column1 of 30%
                ManufacturerOrDistributor.ManufacturerContract
                    label is "ManufacturerContract"
                visible when (ManufacturerOrDistributor.ManufacturerContract)
                    visible when (MultiTierDistributor)
                        text of "MultiTierDistributorContract"
                            color of green
            column2 of 70%
                ManufacturerOrDistributor.DistributorContract
                    label is "DistributorContract"
                visible when (GPONegotiated)
                    visible when (ManufacturerOrDistributor.ManufacturerContract)
                        visible when (DoNotCreateDistributorContract)
                            DoNotCreateDistributorContractMessage
                                no label
                                bold
                                display as text
        two column
            column1 of 30%
                GPONegotiated
            column2 of 70%
                paragraph
                    GPOSupplier.Supplier
                        label is "GPOSupplier"
                        select is GPOSuppliersSelect
                    GPOSupplier.Supplier.SupplierName
                        no label
                        display as text
        two column
            column1 of 30%
                SupplierOriginatedContract
            column2 of 70%
                RenewalContract
        two column
            PreferredContract
            CurrencyCode
        single column
            ProposedTotalContractAmount
                label is "ProposedTotalAmount"
        two column
            column1 of 30%
                MaximumAmount
                    label is "LifetimeMaximumAmount"
            column2 of 70%
                TotalLineMaximumAmount
                    label is "TotalLineMaximumAmount"
                    display as text
        two column
            paragraph
                ManufacturerCodeDivision
                    compact format
                ManufacturerCodeDivision.Description
                    no label
                    display as text
            EnableClinicalSystemUseLocations
        paragraph
            ContractImportSource
                label is "Source"
                display as text
        visible when (GPOIsVizient)
            single column
                first ContractGPOHeaderRel.ContractTypeCode
                    display as text
