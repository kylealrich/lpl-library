FileCreationSetupCompositeFormNew is a CompositeForm
    title is "<Description>"
    Actions
        action is GeneratePreviewRecords
            hide when invalid
        action is GenerateFileThreaded
            hide when invalid
            valid when (DisplayGenerateFileThreaded)
        action is ExportSetup
            action icon is export
        action is save
        action is previous record
        action is next record
        action is PrintMappings
            action icon is pdf
            hide when invalid
        action is ValidateRecords
            action icon is pdf
        action is refresh
            label is "_"
    SetupDetail is a Panel
        form is SetupDetail
    FileAttributesAndProcessing is a Panel
        form is FileAttributesForm
    RecordSelectionOptions is a Panel
        visible when (ShowRecordSelectionOptionsTab)
        form is RecordSelectionOptionsForm
    Sorting is a Panel
        form is SortingForm
    TestingParameters is a Panel
        visible when (UseForTesting)
        form is TestingParametersForm
    Mappings is a Panel
        list is FileCreationRowDetail set.FileCreationRowDetailListNew
    UnprocessedExportRecords is a Panel
        title is "PreviewRecords"
        list is FileCreationRecordsNotProcessed.FileCreationRecordList
    AllRecords is a Panel
        list is FileCreationRecord set.FileCreationRecordFileList
    Files is a Panel
        list is FileCreationFileRel.FileCreationRecordList
    Reports is a Panel
        visible when (ExportFileExists)
        form is FileCreationSetupMappingExportForm
    VersionHistory is a Panel
        list is FileCreationVersion set.FileCreationVersionCardViewList
