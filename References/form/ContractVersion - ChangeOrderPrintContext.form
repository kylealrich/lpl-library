ChangeOrderPrintContext is a Form
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        display as text
            four column distributed
                Contract.RepresentativeText
                visible when (ChangeOrderNumberEntered)
                    ChangeOrderNumber
                ContractVersion
                ChangeOrderTitle
            four column distributed
                Contract.PrimaryContact
                RequesterName
                    label is "CreatedBy"
                visible when (ChangeOrderDateEntered)
                    ChangeOrderDate
                visible when (SubcontractorEntered)
                    ChangeOrderSubcontractor.SubName
                        label is "Subcontractor"
            visible when (ChangeOrderDescriptionEntered)
                single column
                    ChangeOrderDescription
            four column distributed
                ChangeOrderEffectiveDate
                ChangeOrderExpirationDate
                ChangeOrderReviewDate
                ChangeOrderLineUpdateOption
            visible when (ChangeOrderApproverEntered)
                paragraph
                    ChangeOrderApprover
                        display as text
                    ChangeOrderApprover.Employee.FirstLastName
                        no label
                        display as text
