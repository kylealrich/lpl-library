ASStoredConfigurationForm is a Form
    is primary
    title is "StoredConfigurations"
    Layout
        two column
            ASStoredConfiguration
                label is "FileName"
                display as text
            StoredConfigType
                display as text
        single column
            Description
                display as text
        two column
            ASConfigurationName
                label is "ApplicationSetupConfiguration"
                display as text
            VersionNumber
                display as text
        paragraph
            BaseConfiguration
                display as text
            BaseVersionNumber
                display as text
        paragraph
            button of "Download_File"
                view ConfigurationFile
                    using mime type "application/json"
            CreateTimestamp
                display as text
