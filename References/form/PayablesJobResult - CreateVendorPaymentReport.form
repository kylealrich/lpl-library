CreateVendorPaymentReport is a CompositeForm
    use for action CreateVendorPaymentReport
    context form is VendorReportHeaderContext
    restrict action PerformVendorBalanceYearEnd
    Actions
        action is CreateVendorPaymentReport
        action is RunVendorPaymentReport
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
            hide when invalid
        action is next record
            label is "_"
            mouse over text is "Next_Record"
            hide when invalid
        action is save
            action icon is save
            label is "_"
            mouse over text is "Save"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    ReportDetails is a Panel
        list is PayablesJobResultDetail set.VendorPaymentReportDetailList
