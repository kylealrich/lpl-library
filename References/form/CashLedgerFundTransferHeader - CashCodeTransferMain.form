CashCodeTransferMain is a Form
    title is "CashCodeTransfer"
    Layout
        two column distributed
            visible when (not RecordExists)
                column1
                    paragraph
                        CashCode
                        CashCode.Description
                            no label
                            display as text
                        CashCode.Currency
                            no label
                            display as text
                column2
                    paragraph
                        CashLedgerFundTransferHeader
                            label is "TransferNumber"
                            select is UnreleasedTransfers
                        Status
                            display as text
        two column distributed
            column1
                paragraph
                    Company.OrigCompany
                        label is "Company"
                        select is Company
                    Company.OrigCompany.Name
                        no label
                        display as text
                    Company.OrigCompany.Currency
                        no label
                        display as text
                paragraph
                    CurrencyTable
                    CurrencyTable.Description
                        no label
                        display as text
                Description
                TransferDate
                PostDate
