MobileReceiver is a Form
    title is "Receiver"
    Layout
        single column
            Vendor.DerivedVendorNameID
                display as text
            PurchaseOrder.PurchaseFromLocation
                label is "PurchaseFrom"
                display as text
            PurchaseFromLocation.VendorName
                label is "LocationName"
                display as text
        two column distributed
            PurchaseOrder.Buyer.DerivedBuyerNameID
                display as text
            visible when (WorkPhoneEntered)
                PurchaseOrder.Buyer.Employee.EmployeeWorkTelephone
                    label is "WorkPhone"
                    display as text
            blank line
        single column
            ReceivedDate
                label is "Date\\,TimeReceived"
            ShipToArriveDate
                label is "ShipToArrive"
                display as text
            Carrier
                select is MobileVendorSelectSearch
            visible when (Carrier entered)
                Carrier.VendorName
                    label is "CarrierName"
                    display as text
            TransientBillOfLading
                label is "Bill_ofLading"
            ReprintReceipt
            PurchaseOrder.FreightTerm.Description
                label is "FreightTerms"
                display as text
            PackageTrackingNumber
                label is "TrackingNumber"
            PackageTrackingLabel
                label is "Labels"
            GlobalDocumentType
                label is "DocumentTypeCode"
                display as text
            visible when (Vendor.RequireMatchReference)
                MatchReferenceNumber
            visible when (Company.UseIDM)
                paragraph
                    IDMPrinter
                        select is MobileIDMPrinterSelect
                    IDMPrinter.Description
                        no label
                        display as text
