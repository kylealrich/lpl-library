MobileRecallNoticeCoordinatorDetails is a Form
    title is "_"
    Layout
        header3 of "Details"
        single column
            create date
                label is "CreationDate"
        two column table
            column1
                ReleasedTimestamp date
                    label is "ReleaseDate"
                FinalizedDate date
                    label is "FinalizedDate"
            column2
                DaysInPendingStatus
                    label is "No.OfDaysToRelease"
                DaysUntilFinalized
                    label is "No.OfDaysToFinalize"
        single column
            NotApplicableTimestampDateOnly
                label is "DateSetToNotApplicable"
            ClosedTimestampDateOnly
                label is "CloseDate"
            visible when (RecallGroup.RecallApprovalRequired)
                ApprovalTimestamp date
                    label is "ApprovalDate"
            button of "PrintAnalysisInformation"
                link is NoticeAnalysisReport
