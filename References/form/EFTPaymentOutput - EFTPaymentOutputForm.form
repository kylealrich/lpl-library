EFTPaymentOutputForm is a CompositeForm
    is primary
    title is "EFTPaymentOutput"
    context form is EFTPaymentOutputContext
        background is theme color
    ACHTapeOutput is a Panel
        visible when (HasACHTapeData)
        list is ACHTapeDataRel.EFTPaymentOutputDetailList
    BACSTapeOutput is a Panel
        visible when (HasBACSTapeData)
        list is BACSTapeDataRel.EFTPaymentOutputDetailList
    NonTapeOutput is a Panel
        visible when (HasNonTapeData)
        list is NonTapeDataRel.EFTPaymentOutputDetailList
    SEPAOutput is a Panel
        visible when (HasSEPAData)
        list is SEPADataRel.EFTPaymentOutputDetailList
