ReceivablesTieBackReport is an ActionForm
    action is ReceivablesTieBackReport
    Layout
        paragraph
            FinanceEnterpriseGroup
            FinanceEnterpriseGroup.FinanceEnterpriseGroupDescription
                no label
                display as text
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        single column
            text of "Or"
        paragraph
            CompanyGroup
                label is "CompanyGroup"
            CompanyGroup.Description
                no label
                display as text
        paragraph
            Customer
                label is "Customer"
            Customer.Name
                no label
                display as text
        two column distributed
            column1
                TransDate
                    label is "As_ofDate"
            column2
                FutureApps
                    label is "QualifyFutureApplications"
        single column
            AsOfDate
                label is "Audit_orPeriod"
        paragraph
            ArCode
                label is "AccountsReceivableCode"
                select is ARCodes
            ArCode.Description
                no label
                display as text
        single column
            PrintDetail
        single column
            XGainLoss
                label is "Gain\\,Loss"
        single column
            AcoCurrDisplay
                label is "CurrencyDisplay"
            ReportAmounts
        single column
            visible when (actor.context.FinanceEnterpriseGroup.FundAccounting)
                FundAccountSort
            visible when (not actor.context.FinanceEnterpriseGroup.FundAccounting)
                DetailedAccountSort
        header2 of "Selection"
        paragraph
            ProcessLevel
            ProcessLevel.Name
                no label
                display as text
        two column distributed
            column1
                paragraph
                    MajorClass
                        select is MajorClass
                    MajorClass.Description
                        no label
                        display as text
            column2
                paragraph
                    MinorClass
                        select is MinorClass
                    MinorClass.Description
                        no label
                        display as text
        two column distributed
            column1
                paragraph
                    FrProject
                        label is "FromProject"
                        select is ProjectFlatList
                    FrProject.Description
                        no label
                        display as text
            column2
                paragraph
                    ToProject
                        label is "ToProject"
                        select is ProjectFlatList
                    ToProject.Description
                        no label
                        display as text
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                ReceivablesTieBackReportReport_RDG
                ReceivablesTieBackErrorReport_RDG
            column2 of 25%
                ReceivablesTieBackReportReport_XPT
                ReceivablesTieBackErrorReport_XPT
