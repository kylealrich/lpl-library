AssetDisposalWizard is a WizardForm
    use for action Unreleased.Dispose
    title is "AssetDisposal"
    summary form is DisposalWizardSummary
    show steps
    Actions
        action is save
    DisposalDetail is a Panel
        mouse over text is "EnterDisposalDetails"
        form is AssetDisposalWizardMain
        save on next
    Items is a Panel
        valid when (PartialDisposal)
        mouse over text is "EnterItemDisposalDetails"
        list is AssetItemDisposal set.AssetItemDisposalWizard
            helper list is AssetItemRel.SelectAssetItemsToDispose
                action is DisposeItem
    GainLossFull is a Panel
        title is "GainLoss"
        valid when (NotPurgedBookFull)
        mouse over text is "EnterBookDisposalDetails"
        list is AssetBookDisposalFullRel.AssetBookDisposalWizard
    GainLossPartial is a Panel
        title is "GainLoss"
        valid when (NotPurgedPartial)
        mouse over text is "EnterBookDisposalDetails"
        list is PartialAssetBookDisposalRel.AssetBookDisposalWizard
    Transactions is a Panel
        valid when (DisplayDisposalTransactions)
        list is DisposalAssetTransactionsRel.DisposalWizardAssetTransactionList
    PartialTransaction is a Panel
        title is "PartialTransactions"
        valid when (DisplayPartialTransactions)
        list is PartialDisposalAssetTransactionsRel.PartialDisposalTransactionList
    BookTransactions is a Panel
        valid when (DisplayDisposalBookTransactions)
        list is AssetBookTransactionsRel.DisposalWizardAssetBookTransactionList
    PartialBookTransaction is a Panel
        title is "PartialBookTransactions"
        valid when (DisplayPartialBookTransactions)
        list is PartialDisposalBookTransactionsRel.PartialDisposalBookTransactionList
