ReceivablesInterfaceResultContext is a Form
    propagate as a drill
    title is "ReceivablesInterfaceResult"
    Layout
        display as text
            three column distributed
                column1
                    RunGroup
                    RunTime
                    RunType
                    DerivedStatus
                        label is "Status"
                    blank line
                    visible when (NoRecordsToProcess)
                        text of "<NoRecordsToProcessMsg>"
                            bold
                column2
                    visible when (RunType.PaymentInterface)
                        text of "Parameters:"
                            bold
                            align as label
                        Company
                        PrmReference
                            label is "Reference"
                        ReleaseBatch
                        DepositBankCode
                    visible when (RunType.TransactionInterface)
                        text of "Parameters:"
                            bold
                            align as label
                        Company
                        paragraph
                            CustomerRange.FromCustomer
                                label is "CustomerRange"
                            CustomerRange.ToCustomer
                                no label
                        paragraph
                            InterfacedCustomerRange.FromCustomer
                                label is "ExternalCustomerRange"
                            InterfacedCustomerRange.ToCustomer
                                no label
                    visible when (RunType.LockBoxInterface)
                        text of "Parameters:"
                            bold
                            align as label
                        Company
                        LockBox
                        ReleaseBatch
                        DepositBankCode
                    visible when (RunType.Payment2Interface)
                        button of "Parameters"
                            sub form is Payment2InterfaceParameters
                    visible when (RunType.TransactionConversion)
                        Company
                    visible when (RunType.PaymentConversion)
                        Company
                    visible when (RunType.ApplicationConversion)
                        Company
                    visible when (RunType.DistributionConversion)
                        Company
                    visible when (RunType.DisputeConversion)
                        Company
                    visible when (RunType.CommentConversion)
                        Company
                    visible when (ResultError)
                        text of "Error:<ResultErrorMessage>"
                            bold
                            italics
                    visible when (RunType.NonARPaymentInterface)
                        text of "Parameters:"
                            bold
                            align as label
                        Company
                        PrmReference
                            label is "Reference"
                        Operator
                        ReleaseBatch
                    visible when (RunType.DocumentInterface)
                        button of "Parameters"
                            sub form is DocumentInterfaceParameters
                column3
                    visible when (RunType.LockBoxInterface)
                        CashLedgerUserField1
                        CashLedgerUserField2
                        CashLedgerUserField3
                        CashLedgerUserField4
                    button of "Resubmit"
                        visible when (IncompleteHasPendingRecords)
                        action is ResubmitInterface
