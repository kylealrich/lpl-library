MobileCashLedgerCashReceiptDetailContextForm is a Form
    title is "CashReceiptDetail"
    Layout
        header2 of "CashReceiptDeposit"
        two column table
            column1
                CashCode.CashCodeAndDescription
                    label is "CashCode"
                    display as text
                CashLedgerCashReceiptHeader.TransactionNumber
                    label is "DepositNumber"
                    display as text
                CashLedgerCashReceiptHeader.AppliedBankAmount
                    label is "AppliedDistributionBankAmt"
                    display as text
                CashCode.Currency
                    label is "Currency"
                    display as text
            column2
                CashLedgerCashReceiptHeader.Company.OrigCompany.Name
                    label is "Company"
                    display as text
                CashLedgerCashReceiptHeader.BankTransactionCode
                    label is "TransactionCode"
                    display as text
                CashLedgerCashReceiptHeader.BalanceAmount
                    label is "Balance"
                    display as text
                CashLedgerCashReceiptHeader.DepositAmount
                    display as text
