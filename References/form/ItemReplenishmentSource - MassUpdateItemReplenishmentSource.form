MassUpdateItemReplenishmentSource is an ActionForm
    action is MassUpdateItemReplenishmentSource
    Layout
        single column
            text of "SelectReplenishmentRecordsBasedOn:"
                bold
                color of green
            text of "InventoryLocationValues"
                italics
                color of green
            ParmCompany
            ParmInventoryLocation
                select is AllActiveLocations
            ParmItem
            text of "CurrentReplenishFromValues"
                italics
                color of green
            CurrentVendor
            CurrentPurchaseFrom
            CurrentFromCompany
            CurrentFromLocation
            CurrentBin
            blank line
            text of "ReplaceWithTheseNewValues"
                bold
                color of green
            NewVendor
            NewPurchaseFrom
            NewFromCompany
            NewFromLocation
            NewFromBin
