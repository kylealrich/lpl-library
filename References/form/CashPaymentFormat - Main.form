Main is a Form
    Layout
        visible when (not RecordExists)
            CashManagementGroup
            three column distributed
                column1
                    paragraph
                        CashCode
                        CashCode.Description
                            no label
                            display as text
                column2
                    paragraph
                        CashPaymentFormat.BankTransactionCode
                            label is "BankTransactionCode"
                            select is PaymentCodes
                        CashPaymentFormat.BankTransactionCode.Description
                            no label
                            display as text
                column3
                    paragraph
                        CashPaymentFormat.PayablesProcessGroup
                        CashPaymentFormat.PayablesProcessGroup.Description
                            no label
                            display as text
        single column
            paragraph
                PaymentFormat
                CountryCode
            visible when (not PaymentFormat.IDMOutputFile)
                BankIssuedPaymentNumbers
            visible when (not BankIssuedPaymentNumbers)
                StartingDocumentNumber
            visible when (PaymentFormat.IDMOutputFile)
                paragraph
                    FractionalRoutingNumber
                    PaymentFilename
                    CheckVersioning
                        mouse over text is "This_is_for_generating_check_version_with_watermark/s"
        visible when (PaymentFormat.CSVFile)
            two column distributed
                column1
                    WriteCSVHeader
                    ConsolidateDetailAndRemittance
                    PaymentOutputMapping
                column2
                    paragraph
                        FTPConfiguration
                        PfiConfigurationRel.Description
                            no label
                            display as text
                    FTPHeaderFile
                    FTPDetailFile
                    visible when (GeneratesRemittanceRecords)
                        FTPRemittanceFile
        visible when (PaymentFormat.IDMOutputFile)
            header2 of "TemplateOptions"
            two column distributed
                column1
                    paragraph
                        PaymentTemplate
                            select is PayablesPaymentOutputTemplate
                            display as required
                        visible when (PaymentTemplate entered)
                            button of "<PaymentTemplate.Name>"
                                link is external "<PaymentTemplate.RetrieveUrl>"
                                display as link
                column2
                    paragraph
                        visible when (CheckVersioning)
                            CheckVersionTemplate
                                mouse over text is "This_template_must_have_watermark/s"
                                select is PayablesPaymentOutputTemplate
                                display as required
                            visible when (CheckVersionTemplate entered)
                                button of "<CheckVersionTemplate.Name>"
                                    link is external "<CheckVersionTemplate.RetrieveUrl>"
                                    display as link
            header2 of "PrintOptions"
            two column distributed
                column1
                    paragraph
                        UsePaymentPrinter
                        visible when (DisplayPrintCheckAndRemittanceSequentially)
                            SequentialPrintCheckAndRemittance
                                label is "PrintCheckAndRemittanceSequentially"
                                mouse over text is "Requires_\\Payment_\\PrinterToBeTheSameWith_\\Remittance_\\Advice_\\PrinterButWithDifferentPrinterTraySetOn_\\Printer_\\Settings."
                                when value changed
                                    TransientBatchPrintCount =  15
                                        when (SequentialPrintCheckAndRemittance)
                                    TransientBatchPrintCount =  100
                                        when (not SequentialPrintCheckAndRemittance)
                    paragraph
                        TransientBatchPrintCount
                            label is "BatchPrintCount"
                        text of "<DerivedBatchPrintCountMessage>"
                column2
                    paragraph
                        PaymentPrinter
                            display as required when (SequentialPrintCheckAndRemittance)
                            when value changed
                                PaymentPrinterSetting =  blank
                        PaymentPrinter.Description
                            no label
                            display as text
                    visible when (PaymentPrinter.Type.IEP)
                        paragraph
                            PaymentPrinterSetting
                                display as required when (SequentialPrintCheckAndRemittance)
            header2 of "EmailOptions"
            two column distributed
                column1
                    paragraph
                        UsePaymentEMail
                        PaymentFromEmail
                    ExcludeFromEmailCcChecks
                column2
                    paragraph
                        CheckEmailTemplate
                            select is PayablesPaymentOutputIDMEmailTemplate
                        visible when (CheckEmailTemplate entered)
                            button of "<CheckEmailTemplate.Name>"
                                link is external "<CheckEmailTemplate.RetrieveUrl>"
                                display as link
                    paragraph
                        CheckEmailSubject
                        visible when (CheckEmailSubject entered)
                            text of "Sample:<DerivedCheckEmailSubject>"
            header2 of "IntelligentMailBarcode"
            paragraph
                IntelligentMailBarcode
                    label is "Type"
                    raise blue alert when (IntelligentMailBarcode entered)
                        mouse over text is "ToIncludeRoutingCode,EnsureThat5,9,Or11DigitPostCodeIsEnteredInAllVendorAddress"
                BarcodeID
                ServiceTypeID
                MailerID
            header2 of "PaymentCheckFormattingOptions"
            visible when (IsCanada)
                single column
                    LanguageTextOptionIDM
            two column
                column1 of 15%
                    IncludeCurrency
                column2 of 85%
                    paragraph
                        visible when (IncludeCurrency)
                            Currency
                            text of "Example:_\\Dollars,_Pesos,_Dirhams,_etc."
            visible when (not SpanishOrCanadianFrench)
                two column
                    column1 of 15%
                        IncludeCents
                    column2 of 85%
                        paragraph
                            visible when (IncludeCents)
                                CentsFormat
                                visible when (CentsFormat.WordWithMinorCurrency)
                                    MinorCurrencyUnit
                                    text of "Example:_\\Cents,_\\Fils,_etc."
            visible when (SpanishOrCanadianFrench)
                two column
                    column1 of 15%
                        IncludeCents
                    column2 of 85%
                        visible when (IncludeCents)
                            CentsFormat
                                display as text
            single column
                paragraph
                    PaymentInWordsFormat
                    TestAmount
                    visible when (ShowSamplePaymentInWordsOutput)
                        text of "Sample_Output:<DerivedSamplePaymentInWords>"
                paragraph
                    Signature1.File
                        label is "1stSignature"
                        mouse over text is "FileMustBeAnImageType"
                    Signature2.File
                        label is "2ndSignature"
                        mouse over text is "FileMustBeAnImageType"
                    SignatureThresholdAmount
                        label is "2ndSignatureThresholdAmount"
        visible when (not PaymentFormat.CSVFile)
            visible when (not PaymentFormat.BODOutput)
            two column distributed
                column1
                    blank line
                    visible when (PaymentFormat.InforTreasuryFile)
                        PaymentOutputMapping
                column2
                    paragraph
                        visible when (not PaymentFormat.IDMOutputFile)
                            FTPConfiguration
                            PfiConfigurationRel.Description
                                no label
                                display as text
                    single column
                        visible when (not PaymentFormat.IDMOutputFile)
                            FTPDetailFile
                        visible when (GeneratesRemittanceRecords)
                            visible when (not PaymentFormat.IDMOutputFile)
                                FTPRemittanceFile
        header2 of "ElectronicPayment"
        single column
            BankRejectionBankTransactionCode
                label is "RejectionBankTransactionCode"
                select is BankCreditTransactionCodes
