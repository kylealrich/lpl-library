MoreNonServiceContractInformation is a Form
    Layout
        display as text
            paragraph
                PayablesInvoiceDetail.Contract
                    label is "Contract"
                PayablesInvoiceDetail.Contract.Name
                    no label
                    display as text
                PayablesInvoiceDetail.Contract.Description
                    no label
                    display as text
            single column
                PurchaseOrderLine.CostCode
            blank line
            paragraph
                PurchaseOrderLine.Quantity
                    label is "OrderedQuantity"
                PurchaseOrderLine.EnteredBuyUOM
                    label is "UOM"
                PurchaseOrderLine.EnteredBuyUOMMultiplier
                    label is "Multiplier"
            paragraph
                PurchaseOrderLine.ReceivedQuantity
                    label is "ReceivedQuantity"
                PurchaseOrderLine.EnteredBuyUOM
                    label is "UOM"
                PurchaseOrderLine.EnteredBuyUOMMultiplier
                    label is "Multiplier"
            paragraph
                PurchaseOrderLine.MatchedQuantity
                PurchaseOrderLine.EnteredBuyUOM
                    label is "UOM"
                PurchaseOrderLine.EnteredBuyUOMMultiplier
                    label is "Multiplier"
            blank line
            paragraph
                PurchaseOrderLine.ManufacturerNumber
                PurchaseOrderLine.Manufacturer.ManufacturerCode
                    label is "Code"
                PurchaseOrderLine.Manufacturer.ManufacturerDivision
                    label is "Division"
            blank line
            paragraph
                PayablesInvoice.Vendor
                PurchaseOrderLine.VendorItem
            single column
                PurchaseOrderLine.ItemGTIN
                    label is "G\\T\\I\\N"
                PurchaseOrderLine.ContractLine.EffectiveDate
                    label is "EffectiveDate"
                PurchaseOrderLine.ContractLine.ExpirationDate
                    label is "ExpirationDate"
                DerivedServiceContractLineCommentsConditionalMessage
                    no label
