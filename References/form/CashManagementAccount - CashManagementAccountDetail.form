CashManagementAccountDetail is a Form
    title is "AccountDetail"
    Layout
        single column
            Description
                use text area of 3 lines
        two column distributed
            OpenDate
            CloseDate
        paragraph
            CashManagementLocation
                select is CashManagementLocationsNoChildren
            CashManagementLocation.Description
                no label
                display as text
            visible when (ParentLocationStructure entered)
                text of "(<ParentLocationStructure>)"
                    italics
        two column distributed
            paragraph
                Currency
                Currency.Description
                    no label
                    display as text
            InternationalBankAccountNumber
        two column distributed
            AccountType
            BankAccountValidationType
        two column distributed
            RIBKey
            Sepa
        single column
            SwiftID
        paragraph
            LegalEntity
                select is CompaniesForFinanceEnterpriseGroup
            LegalEntity.Company.Name
                no label
                display as text
        paragraph
            GLAccount
                label is "CashPostingAccount"
                compact format
        paragraph
            LedgerBalanceGLAccount
                compact format
            ReportingBasis
        single column
            DefaultJournalDescription
        visible when (ApprovalCodeRequired)
            paragraph
                ApprovalCode
                    select is ApprovalCodesWithLevels
                ApprovalCode.Description
                    no label
                    display as text
        single column
            StatementLineClassification
        paragraph
            visible when (StatementLineClassification.UseProcessingRules)
                CashTransactionProcessingRuleGroup
                CashTransactionProcessingRuleGroup.Description
                    no label
                    display as text
            visible when (StatementLineClassification.DefaultFromAccount)
                DefaultCategory
                DefaultCategory.Description
                    no label
                    display as text
        two column distributed
            column1
                BeginningAccountBalance
                FundingStructure
                ChecksIssued
                SendingFinancialInstitution
                BankClientIdentifier
                EditDuplicatesByAccount
            column2
                CashPositionTarget
                paragraph
                    AccountRestriction
                    visible when (AccountRestriction entered)
                        AccountRestrictionDescription
                            no label
                ElectronicFileTransmission
                SendingFinancialInstitutionBranch
                    label is "SendingBranch"
                Active
        header3 of "PaymentOutputOptions"
        two column distributed
            WriteCSVHeader
            ConsolidateDetailAndRemittance
        two column distributed
            paragraph
                FTPConfiguration
                PfiConfigurationRel.Description
                    no label
                    display as text
            FTPHeaderFile
        two column distributed
            FTPDetailFile
            FTPRemittanceFile
        visible when (HasIntermediaryBank)
            header3 of "IntermediaryBankInformation"
            display as text
                paragraph
                    first FinancialInstitutionBranchIntBankRel.Swift
                        label is "SWIFT"
                    first FinancialInstitutionBranchIntBankRel.Description
                        no label
                single column
                    first FinancialInstitutionBranchIntBankRel.Comments
