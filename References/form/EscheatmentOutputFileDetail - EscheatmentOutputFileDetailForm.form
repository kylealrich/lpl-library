EscheatmentOutputFileDetailForm is a Form
    is primary
    title is "EscheatmentOutputFileDetail"
    Actions
        action is RemoveFromSelectedList
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
    Layout
        display as text
            three column distributed
                column1
                    CashManagementGroup
                    Status
                    CashCode
                    BankTransactionCode
                    ProcessLevel
                column2
                    TransactionNumber
                    TransactionStatus
                    CashLedgerTransactionIdentifier
                    IssueDate
                    StaleDate
                    EscheatDate
                    PostDate
                    IssuedBankAmount
                column3
                    VendorGroup
                    Vendor
                    VendorName
                    PayeeFirstName
                    PayeeMiddleName
                    LegalName
                    Company
                    StaleDateAccount
                        compact format
                    EscheatmentAccount
                        compact format
            header2 of "Address"
            Address
                form is PostalAddressMinimum
            blank line
