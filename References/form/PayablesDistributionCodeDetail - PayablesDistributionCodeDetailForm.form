PayablesDistributionCodeDetailForm is a Form
    is primary
    title is "DistributionCodeDetail"
    Layout
        paragraph
            VendorGroup
            VendorGroup.Description
                no label
                display as text
        paragraph
            PayablesDistributionCode
                label is "DistributionCode"
            PayablesDistributionCode.Description
                no label
                display as text
        paragraph
            DistributionAmount
                label is "Amount"
            text of "Or"
            DistributionPercent
                label is "Percent"
        single column
            paragraph
                DistributionAccount
                    compact format
                DistributionAccount.GeneralLedgerChartAccount.AccountDescription
                    display as text
            UnitAmount
                label is "Units"
        header2 of "LineDetail"
        paragraph
            TaxCode
            TaxCode.Description
                no label
                display as text
        paragraph
            ICNCode
            ICNCode.Description
                no label
                display as text
        two column
            paragraph
                Description
                DistributionReference
                    label is "Reference"
        header2 of "AssetManagement"
        two column
            column1
                paragraph
                    AssetTemplate
                        label is "Template"
                        select is AssetTemplatesAPSelect
                    AssetTemplate.Description
                        no label
                        display as text
                AssetDescription
                TagNumber
                HoldAsset
                paragraph
                    AssetDivision
                    AssetDivision.Description
                        no label
                        display as text
            column2
                ItemQuantity
                    label is "Quantity"
                Combine
                AssetGroup
                paragraph
                    AssetLocation
                    AssetLocation.Description
                        no label
                        display as text
        header2 of "AssetItem"
        two column
            column1
                ItemNumber
                    label is "Item"
                ModelNumber
                BarCode
            column2
                ItemDescription
                SerialNumber
                paragraph
                    display as text
                        LocationDetail.Level1
                            label is "LocationDetail"
                        LocationDetail.Level2
                            no label
                        LocationDetail.Level3
                            no label
        header2 of "Account"
        paragraph
            Company
            AssetAccountingUnitGroup
            AssetAccountingUnitGroup.Description
                no label
                display as text
        header2 of "Repair"
        two column
            column1
                paragraph
                    Asset
                    Asset.Description
                        no label
                        display as text
            column2
                paragraph
                    Asset.TagNumber
                        label is "Tag"
                        display as text
