FundOptions is a Form
    Layout
        three column
            column1 of 20%
                text of "AllocateDimensions"
                    bold
                visible when (AccountingUnitIsValid)
                    AccountingUnit
                        label is "<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>"
                visible when (ProjectIsValid)
                    Project
                        label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
                visible when (FinanceDimension1IsValid)
                    FinanceDimension1
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension1Label>"
                visible when (FinanceDimension2IsValid)
                    FinanceDimension2
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>"
                visible when (FinanceDimension3IsValid)
                    FinanceDimension3
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension3Label>"
                visible when (FinanceDimension4IsValid)
                    FinanceDimension4
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension4Label>"
                visible when (FinanceDimension5IsValid)
                    FinanceDimension5
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension5Label>"
                visible when (FinanceDimension6IsValid)
                    FinanceDimension6
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension6Label>"
                visible when (FinanceDimension7IsValid)
                    FinanceDimension7
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension7Label>"
                visible when (FinanceDimension8IsValid)
                    FinanceDimension8
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension8Label>"
                visible when (FinanceDimension9IsValid)
                    FinanceDimension9
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension9Label>"
                visible when (FinanceDimension10IsValid)
                    FinanceDimension10
                        label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension10Label>"
            column2 of 20%
                text of "TransactionTypes"
                    bold
                FundAccountTypes.Asset
                FundAccountTypes.AccumulatedDepreciation
                FundAccountTypes.DepreciationExpense
                FundAccountTypes.DisposalProceedsLoss
                FundAccountTypes.GainLoss
                FundAccountTypes.DeferredGainLoss
                FundAccountTypes.ShortTermLiability
            column3 of 20%
                text of "_"
                FundAccountTypes.Clearing
                FundAccountTypes.Revaluation
                FundAccountTypes.Impairment
                FundAccountTypes.LeaseDebt
                    label is "LeaseLiability"
                FundAccountTypes.LeaseImpairment
                FundAccountTypes.LeaseAdjustment
                FundAccountTypes.ManualEntry
                FundAccountTypes.LongTermLiability
