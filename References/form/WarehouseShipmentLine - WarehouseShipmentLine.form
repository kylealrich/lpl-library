WarehouseShipmentLine is a CompositeForm
    is primary
    title is "WarehouseShipment_<WarehouseShipment>_Line_<WarehouseShipmentLine>"
    context form is WarehouseShipmentLineContext
        background is theme color
    Main is a Panel
        form is MainForm
    Details is a Panel
        visible when (ShowDetailPanel)
        list is WarehouseShipmentLineDetailsRel.Details
    UDIDetails is a Panel
        visible when (IsUDIDetailRequired)
        list is InventoryUDIDetailsRel.UDIEntrys
    TrackingLineDetails is a Panel
        title is "Details"
        visible when (IsTrackedByContainerOnly)
        list is MobileSupplyChainShipmentDetailTrackingRel.ShipmentDetailTrackingList
    Components is a Panel
        visible when (IsKitItem)
        list is WarehouseShipmentLineComponentsRel.Components
    UserFields is a Panel
        form is UserFields
