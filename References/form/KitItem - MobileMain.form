MobileMain is a Form
    title is "Main"
    Layout
        single column
            DerivedItemGroup
                label is "ItemGroup"
                display as text
            DerivedKitCompany
                label is "Company"
                display as text
            DerivedKitLocation
                label is "Location"
                display as text
            visible when (MakeToOrderExists)
                Notes
                ProcedureCode
                    label is "Procedure"
                OrderSize
                    label is "StandardOrderSize"
