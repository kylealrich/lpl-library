MAssetMain is a Form
    title is "AssetMain"
    Layout
        header3 of "AssetDetails"
            underline color of blue
        paragraph
            Description
                show up to 20 characters
        paragraph
            Company
                label is "AssetCompany"
                select is MAssetCompanyList
            Company.GeneralLedgerCompany.Name
                no label
                display as text
                show up to 20 characters
        paragraph
            TagNumber
        paragraph
            visible when (not RestrictLeaseUpdate)
                AssetLease.LeaseCompany
                    label is "LeaseCompany"
                    select is MPayablesCompanyList
                AssetLease.Lease
                    select is MLeaseList
                    show up to 20 characters
                AssetLease.Lease.Description
                    no label
                    display as text
                    show up to 20 characters
        paragraph
            visible when (RestrictLeaseUpdate)
                AssetLease.LeaseCompany
                    label is "LeaseCompany"
                    display as text
                AssetLease.Lease
                    display as text
                visible when (RestrictLeaseUpdate)
                    AssetLease.Lease.Description
                        no label
                        display as text
        visible when (AseetGuidelineClassesExist)
            paragraph
                AssetGuidelineClass
                    label is "GuidelineClass"
        paragraph
            AssetCategory
                label is "Category"
        single column
            Reference1
            Reference2
        single column
            visible when (DisplayAdjustmentDate)
                AdjustmentDate
                    label is "AssetAdjustmentDate"
        header3 of "AssetAccounts"
            underline color of blue
        single column
            paragraph
                AssetType
                    label is "Type/Subtype"
                    compact format
                    select is MAssetTypeList
            paragraph
                AssetGroup
                    label is "AssetGroup"
                    select is MAssetGroups
                    show up to 20 characters
            paragraph
                AssetAccountGroup
                    label is "AccountGroup"
                    select is MAssetAccountGroupList
                    show up to 20 characters
                AssetAccountGroup.Description
                    no label
                    display as text
                    show up to 20 characters
            paragraph
                AssetAccountingUnitGroup
                    select is MAssetAccountingUnitGroupList
                AssetAccountingUnitGroup.Description
                    no label
                    display as text
                    show up to 20 characters
        single column independent
            paragraph
                AssetAccountingUnit
                    label is "DefaultTransactionDimensions"
                    compact format
            paragraph
                visible when (ProjectsInUse)
                    DepreciationExpenseProject
                        label is "DepreciationExpenseProject"
        header3 of "LocationAndProcessingOptions"
            underline color of blue
        single column independent
            paragraph
                AssetLocation
                    label is "Location"
                    select is MActiveAssetLocations
                AssetLocation.Description
                    no label
                    display as text
                    show up to 20 characters
            paragraph
                AssetDivision
                    label is "Division"
                    select is MAssetDivisions
                AssetDivision.Description
                    no label
                    display as text
                    show up to 20 characters
            paragraph
                AssetOwner
                    label is "Owner"
                    show up to 15 characters
                AssetOwner.Name.FirstAndLastName
                    no label
                    display as text
                    show up to 20 characters
            paragraph
                visible when (ProjectsInUse)
                    AssetProject
                        left align
            paragraph
                CurrencyTable
                Currency
                    select is MCurrencies
            blank line
            paragraph
                GeographicalLatitude
                    label is "Latitude"
                GeographicalLongitude
                    label is "Longitude"
                GeographicalAltitude
                    label is "Altitude"
            paragraph
                IncludeInPhysicalInventory
                    label is "Inventory"
                WorkInProcess
            paragraph
                TaxExempt
                Simulated
                Used
            Classification
                label is "Classification"
                    left of field
                display as text
