VendorDashboardProfile is a Form
    Layout
        display as text
            three column distributed
                column1
                    Vendor
                        header2
                column2
                    VendorName
                        label is "Name"
                        header2
                column3
                    paragraph
                        VendorStatus
                            no label
                            display as tag
                                color of red when (VendorStatus.Inactive)
                                color of green when (VendorStatus.Active)
                        visible when (OnHold)
                            XiOnHoldMessage
                                no label
                                display as tag
                                    color of red when (OnHold)
            three column distributed
                column1
                    VendorGroup
                    VendorClass
                    VendorAccount
                        label is "AccountNumber"
                column2
                    Reference1
                    Reference2
                    ContactName
                        label is "PrimaryContact"
                column3
                    CurrentAddressRel.PostalAddress.DisplayAddressMultiline
                        label is "Address"
                    paragraph
                        FormattedPhoneNumber
                            no label
                        button of icon.phone
                            visible when (HasPhoneNumber)
                            link is "tel:<PhoneNumber.SubscriberNumber>"
                        visible when (FutureAddressExists)
                            FutureAddressMessage
                                no label
                                display as tag
                                    color of red when (FutureAddressExists)
                    paragraph
                        URLAddress
                            no label
                        button of icon.url
                            visible when (HasURLAddress)
                            link is external "<Vendor.URLAddress>"
