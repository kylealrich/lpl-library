DirectForecastPeriodComposite is a CompositeForm
    is primary
    title is "DirectForecastPeriod"
    context form is DirectForecastPeriodContext
        background is theme color
    CashInflowByCompanyAccount is a Panel
        title is "CashInflow"
        visible when (DirectForecast.InflowCompanyAccountForDueDate)
        list is ReceiptSummaryByDateRel.DirectForecastSummaryDetail
    CashInflowByCompanyAccountForDiscountDate is a Panel
        title is "CashInflow"
        visible when (DirectForecast.InflowCompanyAccountForDiscountDate)
        list is ReceiptSummaryByDiscountDateRel.DirectForecastSummaryDiscountDetail
    CashInflowByCompanyAccountGroup is a Panel
        title is "CashInflow"
        visible when (DirectForecast.InflowCompanyAccountGroupForDueDate)
        list is ReceiptSummaryAccountGroupByDateRel.DirectForecastSummaryDetail
    CashInflowByCompanyAccountGroupForDiscountDate is a Panel
        title is "CashInflow"
        visible when (DirectForecast.InflowCompanyAccountGroupForDiscountDate)
        list is ReceiptSummaryAccountGroupByDiscountDateRel.DirectForecastSummaryDiscountDetail
    CashInflowByCompany is a Panel
        title is "CashInflow"
        visible when (DirectForecast.InflowCompanyOnlyForDueDate)
        list is CompanyReceiptSummaryByDateRel.DirectForecastSummaryDetail
    CashInflowByCompanyForDiscountDate is a Panel
        title is "CashInflow"
        visible when (DirectForecast.InflowCompanyOnlyForDiscountDate)
        list is CompanyReceiptSummaryByDiscountDateRel.DirectForecastSummaryDiscountDetail
    CashInflowByAccount is a Panel
