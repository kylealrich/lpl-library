BillingOverridesPanel is a Form
    title is "<FinanceEnterpriseGroup.ProjectLabel>BillingOverrides"
    Layout
        visible when (InEnterpriseStructure)
            three column distributed
                column1
                    BillingMethod
                column2
                    paragraph
                        visible when (BillingMethod.CostPlus)
                            MarkupPercent
                        visible when (BillingMethod.TimeAndMaterials)
                            MarkupPercent
                        visible when (BillingMethod.RateTable)
                            BillingRateTable
                            MarkupPercent
                        BillingMaximum
            single column
                TaxExempt
            two column
                column1
                    paragraph
                        BuyerTaxCode
                        BuyerTaxCode.Description
                            no label
                            display as text
            two column
                column1
                    paragraph
                        BuyerTaxLocation
                        BuyerTaxLocation.Description
                            no label
                            display as text
                        BuyerTaxLocation.AddressCode
                            display as text
            single column
                FeesExempt
            single column
                RevenueCodeBlockOption
                RevenuePartialAccount
                    compact format
                RevenueFullAccount
                    compact format
        visible when (not InEnterpriseStructure)
            three column distributed
                column1
                    BillingMethod
                        display as text
                column2
                    paragraph
                        visible when (BillingMethod.CostPlus)
                            MarkupPercent
                                display as text
                        visible when (BillingMethod.TimeAndMaterials)
                            MarkupPercent
                                display as text
                        visible when (BillingMethod.RateTable)
                            BillingRateTable
                                display as text
                            MarkupPercent
                                display as text
                        BillingMaximum
                            display as text
            single column
                TaxExempt
                    display as text
            two column
                column1
                    paragraph
                        BuyerTaxCode
                            display as text
                        BuyerTaxCode.Description
                            no label
                            display as text
            two column
                column1
                    paragraph
                        BuyerTaxLocation
                            display as text
                        BuyerTaxLocation.Description
                            no label
                            display as text
                        BuyerTaxLocation.AddressCode
                            display as text
            single column
                FeesExempt
                    display as text
            single column
                RevenueCodeBlockOption
                    display as text
                RevenuePartialAccount
                    display as text
                    compact format
                RevenueFullAccount
                    display as text
                    compact format
