SystemInformation is a Form
    Layout
        two column distributed
            column1
                visible when (TaxCalculationSource.Sabrix)
                    TaxCalculationSource
                        display as text
                    OneSourceVersionType
                        display as text
                SystemCode
                ProcessLevel
                AccruedOrInvoiced
                    label is "AccrualFlag"
                VATRegistrationCountry
                CommodityCode
            column2
                ProductTaxCategory
                TaxID
                    label is "TaxIdentificationNumber"
                TransactionSource
                    label is "Input_orOutput"
                VATRegistrationNumber
            ICNCodeRel.ExcludeFromTaxEstimate
