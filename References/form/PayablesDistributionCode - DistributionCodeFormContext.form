DistributionCodeFormContext is a Form
    Layout
        two column
            column1
                paragraph
                    VendorGroup
                    VendorGroup.Description
                        no label
                        display as text
                paragraph
                    PayablesDistributionCode
                        label is "DistributionCode"
                    Description
                        no label
                paragraph
                    AccountEdit
                        label is "EditAccounts"
                paragraph
                    Active
            column2
                TotalDistributionAmount
                    label is "AmountTotal"
                    display as text
                paragraph
                    TotalPercent
                        label is "PercentTotal"
                        display as text
                    visible when (DisplayPercentWarning)
                        text of "Warning:_\\Percent_\\Total_\\Not_100%"
                            bold
                            color of red
                TotalUnit
                    label is "UnitTotal"
                    display as text
            column3
