SubcontractorDiversityPortalForm is a Form
    title is "SubcontractorDiversity"
    Actions
        action is save
    Layout
        paragraph
            Subcontractor.SubcontractorName
                no label
                display as text
        single column
            PayablesDiversityCode
                label is "DiversityCode"
            PayablesDiversityCode.Description
                label is "DiversityDescription"
                display as text
            EffectiveDate
            ExpirationDate
            ReviewDate
                display as text
            CertificationCompleteText
                no label
                display as text
        blank line
        paragraph
            visible when (HasAttachment)
                Attachment.Title
            Attachment.File
                label is "Attachment"
            visible when (HasAttachment)
                button of "View"
                    view Attachment.File
                    display as link
