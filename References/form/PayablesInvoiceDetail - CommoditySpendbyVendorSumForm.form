CommoditySpendbyVendorSumForm is a SummaryForm
    title is "_"
    Layout
        visible when (SupplyManagementReportContext.PrintGraph)
            list is InvoiceByVendorCommodityCodeRel.CommoditySpendbyVendorChartList
        header4 of "CommodityCode\\:_<CommodityCode>_<CommodityCode.Description>"
        list is InvoiceByVendorCommodityCodeRel.CommoditySpendbyVendorVendorList
        visible when (SupplyManagementReportContext.IncludeInvoiceDetail)
            list is InvoiceByVendorCommodityCodeRel.CommoditySpendbyVendorPayableInvoiceList
        list is PayablesInvoice.CommoditySpendbyVendorPurchaseOrderList
        single column
            PayablesInvoice.SumOfDerivedTotalInvoiceCostWithTax
                label is "PurchaseOrder_<PurchaseOrder>"
                display when blank
            PayablesInvoice.SumOfDerivedTotalInvoiceCostWithTax
                label is "TotalFor_<PayablesInvoice.Vendor.VendorName>"
                display when blank
            PayablesInvoice.SumOfDerivedTotalInvoiceCostWithTax
                label is "TotalSpendFor_<CommodityCode>"
                display when blank
            PayablesInvoice.SumOfDerivedTotalInvoiceCostWithTax
                label is "ReportTotal"
                display when blank
