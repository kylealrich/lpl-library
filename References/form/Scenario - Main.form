Main is a Form
    Layout
        four column distributed
            column1
                visible when (IsInIncrementalLoad)
                    IncrementalLoadStatus
                        label is "IncrementalLoad"
                        display as text
            column2
                visible when (IsInIncrementalLoad)
                    IncrementalLoadStartPeriod
                        label is "From"
                        display as text
            column3
                visible when (IsInIncrementalLoad)
                    IncrementalLoadEndPeriod
                        label is "To"
                        display as text
        visible when (CommitmentType.Budget)
            paragraph distributed
            three column distributed
                column1
                    IsDEPMBudget
                        label is "EPMBudget"
                    Cycle
                    Version
                column2
                    ValidateStructureRelations
                    ValidateUnitControl
                column3
                    AllowZeroBudget
                    visible when (AllowZeroBudget)
                        RetainZeroBudget
            header2 of "ChangeRequestOptions"
            three column distributed
                BalancedChanges
                    label is "BalancedAmountChanges"
                BalancedUnitChanges
                ApprovalCode
        visible when (CommitmentType.Budget)
