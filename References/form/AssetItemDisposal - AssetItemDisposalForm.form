AssetItemDisposalForm is a Form
    is primary
    title is "AssetItemDisposal"
    Layout
        two column
            column1
                paragraph
                    Asset
                        display as text
                    Asset.Description
                        no label
                        display as text
            column2
                Asset.DerivedStatus
                    no label
                    display as text
        visible when (HasTagNumber)
            paragraph
                TransientTagNumber
                    label is "TagNumber"
                    display as text
        visible when (Asset.LeasedAsset)
            paragraph
                Asset.AssetLease.LeaseCompany
                    label is "LeaseCompany"
                    display as text
                Asset.AssetLease.LeaseCompany.Name
                    no label
                    display as text
            paragraph
                Asset.AssetLease.Lease
                    display as text
                Asset.AssetLease.Lease.Description
                    no label
                    display as text
                TransientTagNumber
                    label is "TagNumber"
        paragraph
            TransientDisposalDate
                label is "DisposalDate"
                display as text
        paragraph
            TransientItem
                label is "Item"
                display as text
            TransientItemDescription
                label is "Description"
                display as text
        paragraph
            OriginalItemQuantity
                label is "CurrentItemQuantity"
                display as text
        paragraph
            DisposalQuantity
