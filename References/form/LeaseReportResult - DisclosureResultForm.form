DisclosureResultForm is a Form
    Layout
        visible when (LeaseAccountingStandard.GASB)
            visible when (LeaseAccountingStatement.87)
                two column table
                    row headers
                        "GASB87_Leases"
                        "All_Leases"
                    column1
                        paragraph
                            OperatingCashFLowFinanceLease
                                label is "OperatingCashFlow"
                            AdditionalRightOfUseAmountFinanceLease
                                label is "AdditionalRightOfUseAmount"
                            AmortizationRightOfUse
                            InterestOnLeaseLiability
                            FinancingCashFlowFinanceLease
                                label is "FinancingCashFlow"
                        paragraph
                            CashPaid
                            ShortTermLeaseCost
            visible when (LeaseAccountingStatement.96)
                two column table
                    row headers
                        "GASB96_Leases"
                        "All_Leases"
                    column1
                        paragraph
                            OperatingCashFLowFinanceLease
                                label is "OperatingCashFlow"
                            AdditionalRightOfUseAmountFinanceLease
                                label is "AdditionalRightOfUseAmount"
                            AmortizationRightOfUse
                            InterestOnLeaseLiability
                            FinancingCashFlowFinanceLease
                                label is "FinancingCashFlow"
                        paragraph
                            CashPaid
                            ShortTermLeaseCost
            visible when (LeaseAccountingStatement not entered)
                two column table
                    row headers
                        "GASB_Leases"
                        "All_Leases"
                    column1
                        paragraph
                            OperatingCashFLowFinanceLease
                                label is "OperatingCashFlow"
                            AdditionalRightOfUseAmountFinanceLease
                                label is "AdditionalRightOfUseAmount"
                            AmortizationRightOfUse
                            InterestOnLeaseLiability
                            FinancingCashFlowFinanceLease
                                label is "FinancingCashFlow"
                        paragraph
                            CashPaid
                            ShortTermLeaseCost
        visible when (LeaseAccountingStandard.ASC)
            two column table
                row headers
                    "Finance_Leases"
                    "Operating_Leases"
                    "All_Leases"
                column1
                    paragraph
                        OperatingCashFLowFinanceLease
                            label is "OperatingCashFlow"
                        AdditionalRightOfUseAmountFinanceLease
                            label is "AdditionalRightOfUseAmount"
                        AmortizationRightOfUse
                        InterestOnLeaseLiability
                        FinancingCashFlowFinanceLease
                            label is "FinancingCashFlow"
                    paragraph
                        OperatingCashFlowOperatingLease
                            label is "OperatingCashFlow"
                        AdditionalRightOfUseAmountOperatingLease
                            label is "AdditionalRightOfUseAmount"
                        OperatingCashFlowLiabilityReduction
                            label is "OperatingCashFlowLiability"
                        OperatingLeaseCost
                            label is "Cost"
                    paragraph
                        CashPaid
                        ShortTermLeaseCost
        visible when (LeaseAccountingStandard.IFRS)
            two column table
                row headers
                    "IFRS16_Leases"
                    "All_Leases"
                column1
                    paragraph
                        OperatingCashFLowFinanceLease
                            label is "OperatingCashFlow"
                        AdditionalRightOfUseAmountFinanceLease
                            label is "AdditionalRightOfUseAmount"
                        AmortizationRightOfUse
                        InterestOnLeaseLiability
                        FinancingCashFlowFinanceLease
                            label is "FinancingCashFlow"
                    paragraph
                        CashPaid
                        ShortTermLeaseCost
