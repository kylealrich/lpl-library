IntercompanyBillingGroupForm is a Form
    title is "IntercompanyBillingGroup"
    Layout
        blank line
        paragraph
            IntercompanyBillingGroup
            Description
        blank line
        header3 of "ApprovalSettings"
        single column independent
            ApprovalRequiredForPayablesAndReceivables
                indent
        blank line
        header3 of "UseTheExpenseClearingSystemAccountForDocumentsFrom"
        single column independent
            UseExpenseClearingAcctForGLDocuments
                label is "JournalControl"
                indent
            UseExpenseClearingAcctForAPDocuments
                label is "PayablesInvoice"
                indent
            UseExpenseClearingAcctForTPDocuments
                label is "TransferPricing"
                indent
            UseExpenseClearingAcctForExternalDocuments
                label is "ExternalSource"
                indent
        blank line
        header3 of "AutomaticallyReleaseDocumentsWhenCreatedFrom"
        single column independent
            AutoReleaseDocumentsFromGL
                label is "JournalControl"
                indent
            AutoReleaseDocumentsFromAP
                label is "PayablesInvoice"
                indent
            AutoReleaseDocumentsFromTP
                label is "TransferPricing"
                indent
            AutoReleaseDocumentsFromProjects
                label is "ProjectContractInvoice"
                indent
            AutoReleaseDocumentsFromExternal
                label is "ExternalSource"
                indent
        blank line
        header3 of "InvoiceCurrency"
        single column independent
            UseBillFromFunctionalCurrency
                label is "RequireReceivableCompanyCurrency"
                indent
        header3 of "IDMDocumentSettings"
        single column independent
            UseIDM
                indent
            visible when (UseIDM)
                IDMAutomaticPrint
                    indent
                IDMAutomaticEmail
                    indent
        header3 of "LineItems"
        single column independent
            CreateDetailedLineItems
                label is "SendDistributionsAsLineItemsToReceivables"
                indent
