PFContractLinesComposite is a CompositeForm
    title is "Contract<Contract.ContractNbr>Line<ContractLine>"
    context form is ContractLineContext
    Lines is a Panel
        form is ContractLineMain
    Tiers is a Panel
        visible when (IsManufacturerContractWithLineMembers)
        list is ContractLineMember set.PFContractLineMembers
    DistributorPricing is a Panel
        visible when (IsDistributorContractWithLineMembers)
        list is ContractLineMember set.PFContractLineDistPricingMembers
    NoTemplateAgreementLineDetail is a Panel
        title is "AgreementLineDetail"
        form is ContractLinePricing
    Comments is a Panel
        title is "Comments<LineCommentCount>"
        list is ContractLineComment set.PFContractLineComments
    Distributions is a Panel
        title is "Distributions<DistributionCount>"
        list is ContractLineDistribution set.PFContractLineDistributions
    AOC is a Panel
        title is "AOC<AOCCount>"
        visible when (not Contract.ServicePurchaseType)
        list is ContractLineAOC set.PFContractLineAOC
    ChangeOrdersAndAllowances is a MultiListPanel
        visible when (Contract.AllowChangeOrder)
        list is ChangeOrderLineDetailRel.ChangeOrdersForALine
            visible when (ChangeOrdersExist)
        list is ContractAllowance set.AllowancesForALine
