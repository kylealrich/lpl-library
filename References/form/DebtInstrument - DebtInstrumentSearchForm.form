DebtInstrumentSearchForm is a SearchForm
    title is "InstrumentSearch"
    Layout
        three column
            DebtInstrument
                label is "Instrument"
            DebtInstrumentType
            paragraph
                InterestMethod
                DebtStatus
        three column
            Description
            Issuer
            paragraph
                MaturityDateRange.Begin
                    label is "MaturityDateRange"
                text of "_-"
                MaturityDateRange.End
                    no label
        three column
            DebtGroup
                select is DebtGroupSelect
            DebtType
                select is DebtTypeSelect
            CashManagementLocation
                select is CashManagementLocationsNoChildren
        three column
            CashTransactionCategory
                select is CashTransactionCategorySelect
            TaxExempt
            CallProvision
        three column
            DebtCurrency
            DebtReportGroup
                select is DebtReportGroupSelect
            LegalEntityGroup.LegalEntity
                label is "LegalEntity"
