FolderForm is a Form
    is primary
    Layout
        single column
            create stamp
            Name
            Actor
            ActorGroup
            Role
            RoleGroup
            BusinessClass
            visible when (IsManualReport)
                MRPrintAction
                    display only
            visible when (not IsManualReport)
                Dynamic
                Search
                ListName
                ChartName
            SourceType
                when value changed
                    refresh IsManualReport
                    refresh IsValidForPDF
            visible when (IsListReportOrPage)
                paragraph
                    ReportCategory
            visible when (not IsManualReport)
                Temporal
                visible when (false)
                    FromType
                AsOfDate
                ViewArchiveData
                visible when (IsValidForPDF)
                    paragraph
                        PDFPageOptions.PDFPageSize
                            select is PDFPageSizeSelectList
                        visible when (PDFPageOptions.PDFPageSize.IsCustomPageSize)
                            PDFPageOptions.UnitOfMeasure
                            PDFPageOptions.CustomPageDimensions.PageWidth
                            PDFPageOptions.CustomPageDimensions.PageHeight
                    paragraph
                        PDFFontOffset
                        PDFOrientation
                visible when (IsListReportOrPage)
                    RetentionDuration
                ContextFields
                ExtendedInfo
            visible when (false)
                FromScratch
                TListLPL
                TNoInvalid
                TReportDescription
