ReceivingAdjustmentLog is an ActionForm
    action is ReceivingAdjustmentLog
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        blank line
        paragraph
            Location
                label is "ShipTo"
            Location.Name
                no label
                display as text
        blank line
        paragraph
            CurrAdjustFl
                label is "PrintCurrentAdjustments"
        blank line
        paragraph
            UnreleasedAdjFl
                label is "IncludeUnreleasedReceivingAdjustments"
        blank line
        paragraph
            FrTransDate
                label is "FromAdjustmentDate"
            ToTransDate
                label is "ToAdjustmentDate"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                ReceivingAdjustmentLogReport_RDG
            column2 of 25%
                ReceivingAdjustmentLogReport_XPT
