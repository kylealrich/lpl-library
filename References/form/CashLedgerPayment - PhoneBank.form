PhoneBank is a Form
    title is "BankInformation"
    restrict action CreateBatchApproval
    restrict action Unreleased.Release
    restrict action Unreleased.Create
    restrict action Unreleased.Delete
    restrict action ApplyComments
    restrict action PendingApproval.ManualReject
    restrict action PendingApproval.ManualApprove
    restrict action MaintainPaymentAttachment
    restrict action Unreleased.RemoveFromBatch
    restrict action PendingApproval.RemoveFromBatch
    restrict action Unreleased.SubmitForApproval
    restrict action Unreleased.Cancel
    restrict action Released.Cancel
    Layout
        header3 of "AccountDetails"
        display as text
            two column table
                column1
                    BankEntity
                        label is "BankIdentification"
                    BankAccountValidationType
                        label is "AccountValidation"
                    BankAccountNumber
                        label is "AccountNumber"
                    BACSBankAccountReference
                        label is "B\\A\\C\\SReference"
                column2
                    BankName
                    paragraph
                        BankAccountType
                            label is "AccountType"
                        GlobalBankAccountType
                    ACHPrenotification
                        label is "PrenotificationStatus"
                    BACSBankAccountName
                        label is "B\\A\\C\\SName_orAccountName"
        blank line
        header3 of "PaymentDetails"
        display as text
            two column table
                column1
                    CrossBorderIdentifier
                        label is "CrossBorder"
                    PaymentCategoryCode
                        label is "PaymentCategory"
                    CashLedgerPaymentForm
                    BankToBankInformation
                    FirstPartyReference
                column2
                    SwiftID
                    NormalOrExpressCode
                        label is "PriorityCode"
                    CashLedgerChargeCode
                    ElectronicPaymentCode
                    SecondPartyReference
            blank line
            header3 of "BankPostalAddress"
            BankPostalAddress
                form is PostalAddressMinimumWithType
            blank line
            header3 of "IntermediaryBank"
            single column
                display as text
                    IntermediaryBankEntity
                    IntermediaryCrossBorderIdentifier
                    IntermediarySwiftID
                    IntermediaryBankName
            blank line
            header3 of "IntermediaryBankAddress"
            IntermediaryBankPostalAddress
                form is PostalAddressMinimumWithType
