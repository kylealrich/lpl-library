PhonePayablesInvoiceMessage is a Form
    title is "CostMessage"
    Layout
        header2 of "MessageInformation"
        display as text
            single column
                create stamp
                    label is "Date"
                Status
                AuthorityCode
                MessageType
                    label is "Type"
                DerivedMessageSubType
                    label is "Difference"
        header2 of "InvoiceDetailInformation"
        display as text
            single column
                PayablesInvoiceDetail.Item
                    label is "Item"
                PayablesInvoiceDetail.Item.Description
                PayablesInvoiceDetail.VendorItem
                PayablesInvoiceDetail.EnteredQuantity
                    label is "Quantity"
                PayablesInvoiceDetail.EnteredUOM
                PayablesInvoiceDetail.EnteredUnitCost
                    label is "UnitCost"
                PayablesInvoiceDetail.TotalDistributionAmount
                    label is "Amount"
        display as text
            visible when (PurchaseOrderLineEntered)
                header2 of "PurchaseOrderLineInformation"
                single column
                    PurchaseOrderLine.Item
                    PurchaseOrderLine.Description
                    PurchaseOrderLine.VendorItem
                        label is "VendorItem"
                    PurchaseOrderLine.Quantity
                        label is "Quantity"
                    PurchaseOrderLine.EnteredBuyUOM
                        label is "UOM"
                    PurchaseOrderLine.EnteredUnitCost
                        label is "UnitCost"
                    PurchaseOrderLine.ExtendedAmount
                        label is "Amount"
            visible when (ContractLineEntered)
                header2 of "ContractLineInformation"
                single column
                    ContractLine.ItemNumber
                    ContractLine.ItemDescription
                    ContractLine.VendorItem
                        label is "VendorItem"
                    ContractLine.LifetimeMaximumQuantity
                        label is "LifetimeMaximumQuantity"
                    ContractLine.DerivedRemainingQuantityOrdered
                        label is "QuantityRemaining"
                    ContractLine.UOM.UnitOfMeasure
                        label is "UOM"
                    DerivedContractLineUnitCost
                        label is "UnitCost"
