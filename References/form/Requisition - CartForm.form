CartForm is a Form
    Layout
        display as text
            two column distributed
                column1
                    visible when (actor.agent(Employee).Employee.Requester.CurrentRequisitionForRQC entered)
                        visible when (RSSLPLDisplayCompany)
                            Company.Name
                                label is "Company"
                        visible when (RSSLPLDisplayFromCompany)
                            FromCompanyLocation.FromCompany.Name
                                label is "DeliverFromCompany"
                        Requisition
                            label is "Requisition"
                            header3
                    visible when (actor.agent(Employee).Employee.Requester.CurrentRequisitionForRQC not entered)
                        visible when (RSSLPLDisplayCompany)
                            actor.agent(Employee).Employee.Requester.Company.Name
                                label is "Company"
                        visible when (RSSLPLDisplayFromCompany)
                            actor.agent(Employee).Employee.Requester.RequestingLocation.FromCompanyLocation.FromCompany.Name
                                label is "DeliverFromCompany"
                    visible when (actor.agent(Employee).Employee.Requester.CurrentRequisitionForRQC entered)
                        DerivedTaxTotal
                            label is "TaxTotal"
                            header3
                column2
                    visible when (actor.agent(Employee).Employee.Requester.CurrentRequisitionForRQC entered)
                        visible when (RSSLPLDisplayRequestingLocation)
                            RequestingLocation.Name
                                label is "RequestingLocation"
                        visible when (RSSLPLDisplayFromLocation)
                            FromCompanyLocation.FromLocation.Name
                                label is "DeliverFromLocation"
                        RSSLPLDerivedApprovalValueCurrency
                            label is "RequestTotal"
                            header3
                    visible when (actor.agent(Employee).Employee.Requester.CurrentRequisitionForRQC not entered)
                        visible when (RSSLPLDisplayRequestingLocation)
                            actor.agent(Employee).Employee.Requester.RequestingLocation.Name
                                label is "RequestingLocation"
                        visible when (RSSLPLDisplayFromLocation)
                            actor.agent(Employee).Employee.Requester.RequestingLocation.FromCompanyLocation.FromLocation.Name
                                label is "DeliverFromLocation"
                    visible when (actor.agent(Employee).Employee.Requester.RequisitionRel.NumberOfLines entered)
                        button of "Submit" with icon.invoice-released
                            action is RSSLPLRelease
                            is primary
            visible when (actor.agent(Employee).Employee.Requester.HasLocationRule)
                paragraph
                    text of "<Company.LocationRuleExtendedMessage>" with icon.warning-azure
            visible when (LocationRule entered)
                paragraph
                    text of "<Company.LocationRuleExtendedMessage>" with icon.warning-azure
            visible when (RSSLPLMissingRequiredFieldsHeader)
                paragraph
                    text of "MissingRequiredFields" with icon.warning-ruby
            visible when (actor.agent(Employee).Employee.Requester.RequisitionRel.NumberOfLines not entered)
                header2 of "_"
