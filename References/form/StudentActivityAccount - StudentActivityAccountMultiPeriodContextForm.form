StudentActivityAccountMultiPeriodContextForm is a Form
    title is "PeriodTransactionDetails"
    Layout
        paragraph
            StudentActivityDistrict
                label is "SchoolDistrict"
                display only
            StudentActivityDistrict.Name
                no label
        paragraph
            StudentActivityAccount
                label is "Activity"
                display only
            Description
                no label
                display only
        visible when (HasParent)
            paragraph
                ParentStudentActivityAccount
                    label is "ParentAccount"
                ParentStudentActivityAccount.Description
                    no label
        paragraph
            ReportingPeriods.FromPeriod
                label is "FromPeriod"
            ReportingPeriods.ToPeriod
                label is "ToPeriod"
            visible when (ReportPeriodRangeEntered)
                button of "PrintPeriodTransactionDetails"
                    link is PrintableMultiPeriodNavigation
