SupplierDashboard is a CompositeForm
    title is "SupplierDetails"
    context form is SupplierDashboardContext
        background is theme color
    restrict action SupplierCreate
    restrict action SupplierUpdate
    restrict action MobileSupplierCreate
    restrict action SupplierCreateImport
    Actions
        action is refresh
            label is "_"
    Contracts is a Panel
        valid when (HasContracts)
        list is SupplierContractRel.SupplierContracts
    OpenPurchaseOrdersAndLines is a MultiListPanel
        valid when (HasPOsOrLines)
        list is OpenPurchaseOrdersRel.OpenPurchaseOrdersForAVendor
        list is OpenPurchaseOrderLinesRel.OpenPurchaseOrderLinesForAVendor
    Receipts is a Panel
        visible when (HasReceipts)
        list is PurchaseOrderReceiptRel.ReleasedReceiptsForAVendor
    Returns is a Panel
        visible when (HasReturns)
        list is VendorReturnRel.ReturnsForAVendor
    InvoicesAndPayments is a MultiListPanel
        visible when (HasInvoicesOrChecks)
        list is NumberOfInvoicesRel.InvoicesForAVendor
        list is TotalNumberOfChecksRel.PaymentsForAVendor
    Requisitions is a Panel
        visible when (HasSupplierRequisitions)
        list is SupplierRequisitionRel.AllSingleSupplierRequisitions
    Items is a Panel
        visible when (HasItems)
        list is NumberOfItemsRel.SupplierItemReplenishment
    Templates is a Panel
        visible when (TemplatesExist)
        list is ProcurementTemplateRel.primary
