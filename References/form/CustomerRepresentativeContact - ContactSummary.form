ContactSummary is a Form
    title is "Status"
    disable field options
    Layout
        visible when (RegistrationComplete)
            paragraph
                text of "<CustomerGroup.CompleteAccessRequestText>"
                    bold
                    italics
                    color of green
        visible when (not RegistrationComplete)
            paragraph
                text of "<CustomerGroup.IncompleteAccessRequestText>"
                    bold
                    italics
                    color of red
        paragraph
            text of "<NotCompleteQuestions>"
        paragraph
            text of "<NotCompleteQuestionAttachments>"
