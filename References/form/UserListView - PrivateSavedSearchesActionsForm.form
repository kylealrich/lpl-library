PrivateSavedSearchesActionsForm is a Form
    disable field options
    Layout
        two column grid
            visible when (not Status.Active)
                button of "Activate"
                    action is Activate
                    display as link
                text of "TurnsOnAPreviouslyDeactivatedSavedSearch."
            visible when (not Status.Inactive)
                button of "Deactivate"
                    action is Deactivate
                    display as link
                text of "InactiveSavedSearchesWillNotBeSelectable,ButWillStillAppearInThe_ManageModal."
            visible when (not IsDefault)
                button of "MakeDefault"
                    action is MakeDefault
                    display as link
                text of "AutomaticallyApplyThisSavedSearchWhenAccessingThisList."
            visible when (IsDefault)
                button of "RemoveDefault"
                    action is RemoveDefault
                    display as link
                text of "MakeThisSavedSearchNotTheDefault."
            visible when (not Subscription)
                button of "Rename"
                    action is Rename
                    display as link
                text of "RenameThisSavedSearchAndUpdateTheDescription."
            visible when (not Subscription)
                button of "Delete"
                    action is Delete
                    display as link
                text of "DeletesYourSavedSearch.CannotBeRetrieved."
            visible when (Subscription)
                button of "Unsubscribe"
                    action is Delete
                    display as link
                text of "DeleteYourSubscriptionToThisSavedSearch."
