MPayablesInvoiceDistributionCompositeForm is a CompositeForm
    title is "<DerivedFormTitle>"
    context form is MPayablesInvoiceDistributionContextForm
    restrict action Create
    restrict action CreateAndReleaseInvoice
    restrict action Delete
    Actions
        action is ReverseDistributionForUpdate
            hide when invalid
        action is previous record
            label is "_"
            hide when invalid
        action is next record
            label is "_"
            hide when invalid
        action is save
            label is "_"
            hide when invalid
        action is refresh
            label is "_"
    AmountsPanel is a Panel
        title is "Distribution"
        form is MGLTransAmountsForm
    Options is a Panel
        title is "Options"
        form is MPayablesInvoiceDistributionForm
    Comments is a Panel
        form is InvoiceDistributionComments
    Rebate is a Panel
        visible when (RebateEligible)
        form is MRebates
    RecurringJournalPanel is a Panel
        title is "RecurringEntry"
        visible when (RecurringJournalForm)
        form is MRecurringJournalForm
    RecurringJournalBridgePanel is a Panel
        title is "RecurringJournal"
        visible when (RecurringJournalExists)
        list is RecurringJournalControlRel.MRecurringJournalForBridgeList
    AmortizationDetails is a Panel
        visible when (AmortizationDetailsExists)
        list is ReconciliationDetailRel.MAmortizationDetail
    TaxDistributions is a Panel
        title is "TaxDistributions<DerivedTaxDistributionCount>"
        visible when (HasLinkedTaxDistributions)
        list is LinkedTaxDistributionsRel.MTaxDistributionsViewList
    JurisdictionResults is a Panel
        visible when (JurisResultsExists)
        list is JurisdictionResultsRel.MInvoiceResponseShortList
