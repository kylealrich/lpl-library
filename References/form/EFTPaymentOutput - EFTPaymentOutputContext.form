EFTPaymentOutputContext is a Form
    title is "EFTPaymentOutput"
    Layout
        two column
            column1
                Company
                CreateDate
            column2
                visible when (HasACHTapeFileAttachment)
                    paragraph
                        AchTapeFile
                            no label
                            compact format
                        button of "View"
                            view AchTapeFile.File
                            display as link
                visible when (HasBACSTapeFileAttachment)
                    paragraph
                        BacsTapeFile
                            no label
                            compact format
                        button of "View"
                            view BacsTapeFile.File
                            display as link
                visible when (HasNonTapeFileAttachment)
                    paragraph
                        NonTapeFile
                            no label
                            compact format
                        button of "View"
                            view NonTapeFile.File
                            display as link
                visible when (HasSEPATapeFileAttachment)
                    paragraph
                        SepaFile
                            no label
                            compact format
                        button of "View"
                            view SepaFile.File
                            display as link
