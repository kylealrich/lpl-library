PO33Detail is a Form
    Layout
        four column distributed
            column1
                display as text
                    DerivedEnteredReceivedQuantity
                    DisplayQuantityOrdered
                        label is "QuantityOrdered"
                    OvershipReturnQuantity
                        label is "OvershipmentReturnQuantity"
                    DisplayOriginalQuantity
                        label is "OriginalReceivedQuantity"
            column2
                display as text
                    DerivedRejectedQuantityWithUOM
                        label is "QuantityRejected"
                    DerivedDisplayUnitCostInReceivedUOM
                    DerivedDisplayBin
                    VendorReturn.ReplacementPurchaseOrder
                        label is "ReturnNumber"
            column3
                visible when (POReceiptAdjustmentAndInspection.Status.Unreleased)
                    paragraph
                        Quantity
                            label is "AdjustmentQuantity"
                        DerivedDisplayUnitOfMeasure
                            display as text
                    Bin
                        label is "AdjustmentBin"
                        select is BinsForALocation
                    paragraph
                        WriteOff
                            label is "Write\\-\\OffIfMatched"
                        AssetDetails
                            no label
                            display as text
                    GlobalLineType
                        label is "LineType"
                        select is LineTypeCodes
                visible when (POReceiptAdjustmentAndInspection.Status.Released)
                    paragraph
                        Quantity
                            label is "AdjustmentQuantity"
                            display only
                        DerivedDisplayUnitOfMeasure
                            display as text
                    Bin
                        label is "AdjustmentBin"
                        display only
                    paragraph
                        WriteOff
                            label is "Write\\-\\OffIfMatched"
                            display only
                        AssetDetails
                            no label
                            display as text
                    GlobalLineType
                        label is "LineType"
                        display only
            column4
                visible when (POReceiptAdjustmentAndInspection.Status.Unreleased)
                    CancelBackorder
                visible when (POReceiptAdjustmentAndInspection.Status.Released)
                    CancelBackorder
                        display only
                CatchWeightMessage
                    no label
                    display as text
                visible when (HasBuyerMessage)
                    BuyerMessageDisplay
                        no label
                        display as text
