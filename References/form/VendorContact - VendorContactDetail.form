VendorContactDetail is a Form
    is primary
    title is "VendorContact"
    Layout
        three column distributed
            column1
                paragraph
                    visible when (not RecordExists)
                        VendorGroup
                    visible when (RecordExists)
                        VendorGroup
                            display as text
                    VendorGroup.Description
                        no label
                        display as text
            column2
                paragraph
                    visible when (not RecordExists)
                        Vendor
                    visible when (RecordExists)
                        Vendor
                            display as text
                    Vendor.VendorName
                        no label
                        display as text
            column3
                paragraph
                    visible when (not RecordExists)
                        VendorLocation
                            label is "Location"
                    visible when (RecordExists)
                        VendorLocation
                            label is "Location"
                            display as text
                    VendorLocation.VendorName
                        no label
                        display as text
        three column distributed
            column1
                visible when (not RecordExists)
                    VendorContact.VendorContactCode
                        label is "ContactCode"
                visible when (RecordExists)
                    VendorContact.VendorContactCode
                        label is "ContactCode"
                        display as text
            column2
                visible when (not RecordExists)
                    VendorContact.ContactLevel
                visible when (RecordExists)
                    VendorContact.ContactLevel
                        display as text
        two column distributed
            column1
                ContactName
                    label is "Name"
                Description
                VendorAddress
            column2
                EmailAddress
                    label is "Email"
                visible when (IsPrimaryContact)
                    OverrideIDMEmailDistribution
                        mouse over text is "DocumentsWillBeEmailedTo_Primary_ContactByDefault.SelectToOverride."
                    visible when (OverrideIDMEmailDistribution)
                        EmailCreditMemo
                            mouse over text is "SelectToReceive_Credit_MemoEmails."
                        EmailDebitMemo
                            mouse over text is "SelectToReceive_Debit_MemoEmails."
                        EmailPaymentCheck
                            mouse over text is "SelectToReceive_Payment_CheckEmails."
                        EmailRemittanceAdvice
                            mouse over text is "SelectToReceive_Remittance_AdviceEmails."
                visible when (not IsPrimaryContact)
                    text of "IDMDocumentEmailDistribution"
                    EmailCreditMemo
                        mouse over text is "SelectToReceive_Credit_MemoEmails."
                    EmailDebitMemo
                        mouse over text is "SelectToReceive_Debit_MemoEmails."
                    EmailPaymentCheck
                        mouse over text is "SelectToReceive_Payment_CheckEmails."
                    EmailRemittanceAdvice
                        mouse over text is "SelectToReceive_Remittance_AdviceEmails."
        single column
            AddressEffectiveDate
            PhoneNumber
            MobileNumber
            FaxNumber
            TwitterID
        two column distributed
            column1
                SocialNetworkID2
            column2
                SocialNetworkID3
        two column distributed
            column1
                SocialNetworkID4
            column2
                SocialNetworkID5
        single column
            CommentText
                use text area of 12 lines
