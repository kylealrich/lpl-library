Unapprove is an ActionForm
    action is Unapprove
    Layout
        single column
            paragraph
                Company
                Company.Name
                    no label
                    display as text
            paragraph
                Vendor
                    label is "Vendor"
                Vendor.VendorName
                    no label
                    display as text
            paragraph
                Lease
                    label is "Lease"
                Lease.Description
                    no label
                    display as text
            paragraph
                Invoice
                    label is "Invoice"
                Description
                    no label
            paragraph
                button of "Comments"
                    list is LeaseInvoice.LeaseInvoiceComment set.LeaseInvoiceComments
                    display as link
            paragraph
                LeaseInvoice.PaymentNumber
                    label is "PaymentNumber"
            paragraph
                PrmAuthorityCode
                    label is "AuthorityCode"
