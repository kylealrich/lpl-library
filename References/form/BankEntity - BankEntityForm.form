BankEntityForm is a Form
    is primary
    title is "ExternalBankEntity"
    Layout
        two column distributed
            column1
                CashManagementGroup
                paragraph
                    BankEntity
                    ExcludeFromPaymentFile
            column2
                BankName
                BranchName
        two column
            column1
                header2 of "Address"
                PostalAddress
                    form is PostalAddressMinimumWithType
            column2
                header2 of "Contact"
                paragraph
                    Contact
                        label is "ContactName"
                paragraph
                    PhoneNumber
                paragraph
                    FaxNumber
                header2 of "S\\W\\I\\F\\T"
                paragraph
                    SWIFTCharge
                        label is "ChargeAccount"
                    SwiftID
                        label is "B\\I\\C"
