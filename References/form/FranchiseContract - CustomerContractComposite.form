CustomerContractComposite is a CompositeForm
    is primary
    title is "<FranchiseContractFormTitle>"
    context form is CustomerContractContext
        background is theme color
    ContractDetail is a Panel
        form is FranchiseContractMain
    SalesBudget is a Panel
        list is FranchiseContractBudget set.primary
    Licensee is a Panel
        form is LicenseeDetail
    FranchiseLocation is a Panel
        form is FranchiseLocationDetail
    ContractCharges is a Panel
        list is FranchiseContractChargesRel.ContractChargesWithoutContextKeys
    CommentsAndAttachments is a Panel
        title is "Comments_andAttachments"
        list is FranchiseContractComment set.FranchiseContractCommentList
