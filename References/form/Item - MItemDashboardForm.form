MItemDashboardForm is a CompositeForm
    title is "Items"
    context form is MItemDashboardContext
    implements DashBoardView
    restrict action Create
    restrict action CopyItem
    restrict action RequestNewItem
    restrict action Delete
    Actions
        action is save
            label is "_"
            hide when invalid
        action is refresh
            label is "_"
    VendorItems is a Panel
        list is AllVendorItemRel.MVendorItemsForItem
    ContractLines is a Panel
        list is ItemContractLinesRel.MContractItemContractLines
    LocationBalances is a Panel
        list is AllActiveInventoriedItemLocationsRel.ItemStockOnHandByLocation
    ParLocations is a Panel
        list is AllItemLocationsRel.ItemsAtParLocation
    ProcurementTemplates is a Panel
        list is ItemProcurementTemplateLinesRel.TemplateLinesForAnItem
    PurchaseOrderLines is a Panel
        list is InventoryOrNonStockPurchaseOrderLineRel.MPurchaseOrderLinesForAnItem
    ContractInterfaceLines is a Panel
        list is ContractInterfaceItemRel.MContractInterfaceLinesForAnItem
    UnitsOfMeasure is a Panel
        list is ItemUOM set.MUOMsForItem
    AssociatedItems is a Panel
        visible when (AssociatedItemsExist)
        list is AssociatedItemsRel.AssociatedItemDashboard
    NationalDrugCodes is a Panel
        visible when (DrugPharmacyOrNarcoticUsage)
        list is NationalDrugCode set.NationalDrugCodes
    UPCCodes is a Panel
        list is ItemUPC set.MobileItemUniversalProductCodes
    HIBCCCodes is a Panel
        visible when (not DrugPharmacyOrNarcoticUsage)
        list is HIBCCItemUPN set.MobileHIBCCItemUPNs
    UCCEANCodes is a Panel
        visible when (not DrugPharmacyOrNarcoticUsage)
        list is UCCEANItemUPN set.MobileUCCEANItemUPNs
    SKUCodes is a Panel
        visible when (not DrugPharmacyOrNarcoticUsage)
        list is ItemSKU set.MobileItemSKUs
    GTINs is a Panel
        title is "GTIN's"
        list is ItemGTIN set.MobileGlobalTradeItemNumbers
    Attachments is a Panel
        list is ItemAttachment set.MItemAttachmentList
    ConvertedContractSpecialItems is a Panel
        visible when (ContractSpecialItemExists)
        list is ContractSpecialItemsRel.MOldSpecialItemsforItems
