NewPurchaseOrderAcknowledgementResultContextForm is a Form
    title is "PurchaseOrderAcknowledgementResult"
    Layout
        four column distributed
            single column
                PurchaseOrderAcknowledgementResult
                    label is "ResultID"
                    display as text
                RunGroup
                    display as text
                visible when (Company entered)
                    PurchaseOrderCompanyRel.RepresentativeText
                        label is "Company"
                        display as text
                visible when (Company not entered)
                    Company
                        label is "Company"
                        display as text
                RunTime
                    display as text
                visible when (Vendor entered)
                    VendorRel.RepresentativeText
                        label is "Vendor"
                        display as text
                visible when (Vendor not entered)
                    Vendor
                        label is "Vendor"
                        display as text
                GLNProcessingText
                    label is "GLNProcessingEnabled"
                    display as text
                visible when (NoResultsForRunGroup)
                    NoRecordsFound
                        no label
                        display as tag
                            color of red
