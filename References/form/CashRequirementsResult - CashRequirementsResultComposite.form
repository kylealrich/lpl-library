CashRequirementsResultComposite is a CompositeForm
    is primary
    title is "<CashRequirementResultFormTitle>"
    context form is CashRequirementsResultContext
        background is theme color
    Actions
        action is ClosePaymentCycle
            hide when invalid
        action is Created.SelectPayments
            hide when invalid
        action is Selected.SchedulePayments
            hide when invalid
        action is Completed.CompletePaymentCycle
            hide when invalid
        action is Selected.ResetParameters
            hide when invalid
        action is Completed.ResetParameters
            hide when invalid
        action is export to pdf
        action is save
            label is "_"
            hide when invalid
        action is Created.Delete
            action icon is delete
            label is "_"
            hide when invalid
        action is refresh
            label is "_"
    Parameters is a Panel
        form is ParametersForm
    SelectedVendors is a Panel
        title is "SelectedVendors_(<NumCashRequirementsReportSet>)"
        visible when (ShowAPPayments)
        list is CashRequirementsReport set.CashRequirementsReports
    SelectedInvoicePayments is a Panel
        title is "SelectedInvoicePayments_(<NumSelectedPayablesInvoicePaymentRel>)"
        visible when (ShowAPPaymentsSelected)
        list is SelectedPayablesInvoicePaymentRel.CashRequirementsList
    AvailableInvoicePayments is a Panel
        title is "AvailableInvoicesForSelection"
        visible when (ShowInvoicePaymentsAvailable)
        list is AvailablePayablesInvoicePaymentRel.CashRequirementsHelperList
    CompletedPayablesPayments is a Panel
        title is "SelectedInvoicePayments_(<NumCompletedPayablesInvoicePaymentRel>)"
        visible when (ShowAPPaymentsCompleted)
        list is CompletedPayablesInvoicePaymentRel.CashRequirementsList
    SelectedInvoicesOnHold is a Panel
        title is "SelectedInvoicesOnHold_(<NumSelectedPayablesInvoicePaymentOnHoldRel>)"
        visible when (ShowAPPaymentsOnHold)
        list is SelectedPayablesInvoicePaymentOnHoldRel.CashRequirementsOnHoldList
    SelectedCashLedgerPayments is a Panel
        title is "SelectedCashLedgerPayments_(<NumCashLedgerPaymentRel>)"
        visible when (ShowCBPayments)
        list is CashLedgerPaymentRel.CashRequirementsList
    AvailableCashLedgerPayments is a Panel
        title is "AvailableCashLedgerPaymentsForSelection"
        visible when (ShowCashLedgerPaymentsAvailable)
        list is AvailableCashLedgerPaymentRel.AvailableCashRequirementsList
    Alerts is a Panel
        title is "Alerts_(<NumCashRequirementsErrorAlertRel>)"
        visible when (ShowAlerts)
        list is CashRequirementsErrorAlertRel.GeneralCashRequirementsList
    PayablesPaymentErrors is a Panel
        title is "PayablesPaymentErrors_(<NumCashRequirementsErrorPIPRel>)"
        visible when (ShowPayablesPaymentErrors)
        list is CashRequirementsErrorPIPRel.PayablesPaymentCashRequirementsList
    CashLedgerPaymentErrors is a Panel
        title is "CashLedgerPaymentErrors_(<NumCashRequirementsErrorCLPRel>)"
        visible when (ShowCashLedgerPaymentErrors)
        list is CashRequirementsErrorCLPRel.CashLedgerPaymentCashRequirementsList
    ScheduledPayments is a Panel
        title is "ScheduledPayments_(<NumCashLedgerPayablesPaymentRel>)"
        visible when (ShowPayments)
        list is CashLedgerPayablesPaymentRel.CashRequirementsList
    ScheduledPaymentsSummary is a Panel
        visible when (ShowPayments)
        list is CashLedgerPayablesPaymentRel.SumCashRequirementsList
