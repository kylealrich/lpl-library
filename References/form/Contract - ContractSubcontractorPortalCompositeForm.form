ContractSubcontractorPortalCompositeForm is a CompositeForm
    title is "SubcontractorsForContract<Contract>"
    context form is ContractSubcontractorPortalContext
        background is theme color
    Actions
        action is refresh
    Subcontractors is a Panel
        list is ContractSubcontractor set.ContractSubcontractorPortalList
            helper list is SubcontractorRels.SubcontractorActivePortalList
                action is SupplierAttachToContract
                valid when (not ContractInactiveClosed)
