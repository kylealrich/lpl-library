DisplayWarehouseDemandLine is a CompositeForm
    title is "WarehouseDemandLine"
    context form is WarehouseDemandLineContext
        background is theme color
    Main is a Panel
        form is Main
    Inventoried is a Panel
        visible when (IsDemandType)
        form is InventoriedAllocations
    InventoryBalances is a Panel
        form is ItemLocationBalances
    Bin is a Panel
        visible when (IsBinTrackedAndNotLotOrSerialTracked)
        list is WarehouseDemandLineDetailsRel.DisplayBinAllocations
    Lot is a Panel
        visible when (IsLotTracked)
        list is WarehouseDemandLineDetailsRel.DisplayLotAllocations
    Serial is a Panel
        visible when (IsSerialTracked)
        list is WarehouseDemandLineDetailsRel.DisplaySerialAllocations
    UOMAllocations is a Panel
        visible when (UnitOfMeasureTracked)
        list is WarehouseDemandLineDetailsRel.DisplayUOMAllocations
