Main is a Form
    Layout
        header3 of "InvoiceInformation"
        three column distributed
            column1
                ProcessLevel
                ServiceContract
            column2
                PurchaseOrder
                Location
                    label is "ShipTo"
            column3
                paragraph
                    InterfacedPurchaseOrder
                    InterfacedPOCode
        three column distributed
            column1
                InvoiceDate
                    display as required when (IsIIAInvoiceDate)
                DistributionDate
                    label is "PostDate"
            column2
                DueDate
                ScanDate
            column3
                DiscountDate
        header3 of "InvoiceAmounts"
        three column distributed
            column1
                InvoiceAmount
                    display as required when (IsIIAPO)
                TaxAmount
                LineTax
            column2
                AllowableAmount
                TaxCode
                    select is TaxCodeFilteredList
                InvoiceCurrency
            column3
                TaxableAmount
                ProductTaxCategory
                TaxAdjustment
        header3 of "VendorInformation"
        three column distributed
            TermsCode
            RemitToCode
        header3 of "Options"
        three column distributed
            column1
                MatchReferenceNumber
                APClerk
                    label is "Processor"
                ReturnNumber
                ReasonCode
            column2
                CreditMemoReferenceInvoice
                RoutingCategory
                WebAddress
                ExternalPurchaseOrder
            column3
                OriginalInvoice
                Description
                MatchPrepaymentMethod
                CreationSource
        header3 of "PaymentInformation"
        three column distributed
            column1
                PaymentCode
                SeparatePayment
            column2
                Enclosure
            column3
                HoldCode
        header2 of "Miscellaneous"
        three column distributed
            CertificationNumber
            CertificationDate
        header3 of "ExternalInformation"
        three column distributed
            ExternalReasonCode
            ExternalReason
            AlphaAttribute1
            ExternalCompanyName
            ExternalVendorID
            ExternalCurrency
            ExternalTerms
