InternalFundWizardForm is a Form
    title is "InternalFund"
    Layout
        visible when (InEnterpriseStructure)
            single column
                CustomerPoNumber
                    label is "CustomerP\\ONumber"
                ARInterfaceOption
                    label is "ARInterfaceOption"
                LetterOfCredit
                    label is "LetterOfCredit"
                DocumentNumber
                    label is "DocumentNumber"
        visible when (not InEnterpriseStructure)
            single column
                CustomerPoNumber
                    label is "CustomerP\\ONumber"
                    display as text
                ARInterfaceOption
                    label is "ARInterfaceOption"
                    display as text
                LetterOfCredit
                    label is "LetterOfCredit"
                    display as text
                DocumentNumber
                    label is "DocumentNumber"
                    display as text
