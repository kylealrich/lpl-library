WarehouseBillOfLadingPrint is an ActionForm
    action is WarehouseBillOfLadingPrint
    title is "PrintBillsOfLading"
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            Location
            Location.Name
                no label
                display as text
        single column
            WhsStatus
                label is "ShipmentStatus"
        single column
            Carrier
        single column
            ShipDate
        single column
            CodOrders
                label is "C\\O\\DOrdersOnly"
        blank line
        single column
            BolNbr
                label is "BillOfLadingNumber"
                select is CompanyBillofLadings
        blank line
        visible when (not Company.UseIDM)
            single column
                NbrAlignments
                    label is "AlignmentForms"
        visible when (Company.UseIDM)
            header2 of "PrinterOption"
            paragraph
                IDMPrinter
                IDMPrinter.Description
                    no label
                    display as text
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                WarehouseBillOfLadingPrintReport_RDG
                WarehouseBillOfLadingAlignmentPrint_RDG
            column2 of 25%
                WarehouseBillOfLadingPrintReport_XPT
                WarehouseBillOfLadingAlignmentPrint_XPT
