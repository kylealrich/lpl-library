AllocationSnapshotDetailForm is a Form
    is primary
    title is "ExpenseSnapshot<AllocationSnapshot>"
    Layout
        display as text
            three column distributed
                column1
                    paragraph
                        FinanceEnterpriseGroup
                    paragraph
                        AllocationSnapshot
                column2
                    paragraph
                        System
                    paragraph
                        AllocationSourceSystem
                column3
                    paragraph
                        EntityYearPeriod
            header3 of "FinanceStructure"
            four column distributed
                FinanceCodeBlock
            blank line
            header3 of "Amounts"
            two column
                column1
                    TransactionAmount
                        label is "Transaction"
                    ReportCurrencyAmount.FunctionalAmount.EnteredCurrencyAmount
                        label is "Functional"
                    visible when (AlternateCurrencyExists)
                        ReportCurrencyAmount.AlternateAmount.EnteredCurrencyAmount
                            label is "AlternateAmount"
                    visible when (AlternateCurrency2Exists)
                        ReportCurrencyAmount.AlternateAmount2.EnteredCurrencyAmount
                            label is "AlternateAmount2"
                    visible when (AlternateCurrency3Exists)
                        ReportCurrencyAmount.AlternateAmount3.EnteredCurrencyAmount
                            label is "AlternateAmount3"
                    visible when (ProjectExists)
                        ReportCurrencyAmount.ProjectAmount.EnteredCurrencyAmount
                            label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Amount"
                    visible when (ReportCurrencyOneExists)
                        ReportCurrencyAmount.ReportAmount1.EnteredCurrencyAmount
                            label is "<actor.context.FinanceEnterpriseGroup.ReportOne>"
                    visible when (ReportCurrencyTwoExists)
                        ReportCurrencyAmount.ReportAmount2.EnteredCurrencyAmount
                            label is "<actor.context.FinanceEnterpriseGroup.ReportTwo>"
                    visible when (ReportCurrencyThreeExists)
                        ReportCurrencyAmount.ReportAmount3.EnteredCurrencyAmount
                            label is "<actor.context.FinanceEnterpriseGroup.ReportThree>"
                    visible when (ReportCurrencyFourExists)
                        ReportCurrencyAmount.ReportAmount4.EnteredCurrencyAmount
                            label is "<actor.context.FinanceEnterpriseGroup.ReportFour>"
                    visible when (ReportCurrencyFiveExists)
                        ReportCurrencyAmount.ReportAmount5.EnteredCurrencyAmount
                            label is "<actor.context.FinanceEnterpriseGroup.ReportFive>"
                    visible when (UnitsAmountExists)
                        UnitsAmount
                            label is "UnitsAmount"
                column2
                    CurrencyCode
                        label is "_"
                    FinanceCodeBlock.ToAccountingEntity.FunctionalCurrency
                        label is "_"
                    visible when (AlternateCurrencyExists)
                        FinanceCodeBlock.ToAccountingEntity.AlternateCurrency
                            label is "_"
                    visible when (AlternateCurrency2Exists)
                        FinanceCodeBlock.ToAccountingEntity.AccountingEntity.AlternateCurrency2
                            label is "_"
                    visible when (AlternateCurrency3Exists)
                        FinanceCodeBlock.ToAccountingEntity.AccountingEntity.AlternateCurrency3
                            label is "_"
                    visible when (ProjectExists)
                        FinanceCodeBlock.Project.Currency
                            label is "_"
                    visible when (ReportCurrencyOneExists)
                        FinanceEnterpriseGroup.ReportCurrencyOne
                            label is "_"
                    visible when (ReportCurrencyTwoExists)
                        FinanceEnterpriseGroup.ReportCurrencyTwo
                            label is "_"
                    visible when (ReportCurrencyThreeExists)
                        FinanceEnterpriseGroup.ReportCurrencyThree
                            label is "_"
                    visible when (ReportCurrencyFourExists)
                        FinanceEnterpriseGroup.ReportCurrencyFour
                            label is "_"
                    visible when (ReportCurrencyFiveExists)
                        FinanceEnterpriseGroup.ReportCurrencyFive
                            label is "_"
