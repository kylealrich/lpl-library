FinancialInstitution is a CompositeForm
    is primary
    title is "FinancialInstitution"
    context form is FinancialInstitutionContext
        background is theme color
    Branches is a Panel
        list is FinancialInstitutionBranch set.FinancialInstitutionBranches
    FeeServiceCodes is a Panel
        list is BankServiceCode set.BankServiceCodeList
    InstitutionRatings is a Panel
        list is FinancialInstitutionRating set.FinancialInstitutionRatingList
    Documents is a Panel
        list is FinancialInstitutionDocument set.FinancialInstitutionDocumentList
