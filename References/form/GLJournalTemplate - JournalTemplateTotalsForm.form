JournalTemplateTotalsForm is a Form
    title is "Totals"
    Layout
        display as text
            two column table
                column headers
                    "TemplateTotals"
                    "WorkingTotals"
                row headers
                    "Debit"
                    "Credit"
                    "NetDifference"
                    "Unit"
                column1
                    DerivedTemplateTransactionDebitAmount
                        no label
                    DerivedTemplateTransactionCreditAmount
                        no label
                    DerivedTemplateNetDifferenceAmount
                        no label
                    DerivedTemplateTransactionUnitAmount
                        no label
                column2
                    DerivedWorkTransactionDebitAmount
                        no label
                    DerivedWorkTransactionCreditAmount
                        no label
                    DerivedWorkNetDifferenceAmount
                        no label
                    DerivedWorkTransactionUnitAmount
                        no label
