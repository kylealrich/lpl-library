AwardHistoryTotalSumForm is a SummaryForm
    title is "_"
    Layout
        header4 of "TotalDiversityAwardRates"
        three column
            column1
                DerivedGrandTotalEventsBid
                    label is "DiversityEventResponses"
                    display when blank
                DerivedGrandTotalEventsAwarded
                    label is "DiversityAwards"
                    display when blank
                DerivedTotalEventsBidPercent
                    label is "DiversityEventAwardRate"
                    display when blank
            column2
                DerivedGrandTotalEventLinesBid
                    label is "DiversityLinesResponded"
                    display when blank
                DerivedGrandTotalEventLinesAwarded
                    label is "DiversityLinesAwarded"
                    display when blank
                DerivedTotalEventLinesBidPercent
                    label is "LineBidSuccess"
                    display when blank
            column3
                DerivedBidGrandTotal
                    label is "DiversityAmountBid"
                    display when blank
                DerivedAwardedGrandTotal
                    label is "DiversityAwardedAmount"
                    display when blank
                DerivedGrandTotalBidPercent
                    label is "AmountAwardRate"
                    display when blank
