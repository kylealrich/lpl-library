CashCodeFundTransferForm is a CompositeForm
    use for action CreateCashCodeFundTransfer
    title is "CashCodeFundTransfer"
    context form is CreateCashCodeFundTransferContext
        background is theme color
    Actions
        action is Release
            label is "Release"
            hide when invalid
        action is CreateBankFundTransferDeposits
            label is "CreateDeposits"
        action is CreateBankFundTransferWithdrawals
            label is "CreateWithdrawals"
        action is CreateCashCodeFundTransfer
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is save
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    Main is a Panel
        title is "CashCodeTransfer"
        form is CashCodeTransferMain
    CashCodeTransferDetail is a Panel
        list is CashCodeFundTransferDetail set.primary
    Errors is a Panel
        title is "Errors<DerivedBankFundTransferErrorCount>"
        visible when (BankFundTransferErrorsExist)
        list is CashLedgerTransactionError set.CashLedgerFundTransferHeaderErrorList
