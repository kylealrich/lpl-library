                    InventoryLocation
                        label is "Location"
                column2
                    create stamp.actor
                        label is "CountedBy"
                    ParCycleCount
                        label is "TransactionNumber"
                    DerivedLinesProcessed
                        label is "LinesCounted"
                column3
                    TimeSubmitted
                        label is "TimeSubmitted"
                    TimeStarted
                        label is "TimeStarted"
                    DerivedDurationName
                        label is "Duration"
