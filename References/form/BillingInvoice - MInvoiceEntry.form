MInvoiceEntry is a CompositeForm
    title is "Invoice"
    context form is MInvoiceEntryContext
    restrict action Unreleased.Create
    restrict action Unreleased.CreateInvoiceFromInvoice
    restrict action Unreleased.CreateInvoiceFromOrder
    restrict action Unreleased.Delete
    Actions
        action is refresh
            label is "_"
        action is save
            label is "_"
            hide when invalid
        action is previous record
            label is "_"
            hide when invalid
        action is next record
            label is "_"
            hide when invalid
    Main is a Panel
        form is MMain
    InvoiceOptions is a Panel
        form is MInvoiceOptions
    Payment is a Panel
        form is MPayment
    Amounts is a Panel
        form is MTaxInquiry
    InvoiceLines is a Panel
        visible when (not AllowMultipleSalesAccountEntry)
        list is BillingInvoiceLine set.MInvoiceLines
    InvoiceLinesSales is a Panel
        title is "InvoiceLines"
        visible when (AllowMultipleSalesAccountEntry)
        list is BillingInvoiceLine set.MInvoiceLinesWithSalesAccount
    InvoiceCharges is a Panel
        list is BillingInvoiceAddOnChargesRel.MInvoiceAddOnCharges
    Comments is a Panel
        list is BillingInvoiceCommentsRel.MBillingInvoiceCommentsList
            helper list is BillingStandardCommentHeaderRel.BillingStandardCommentHeaderForCopyList
                action is CopyComments
    Attachments is a Panel
        list is BillingInvoiceAttachment set.MAttachments
