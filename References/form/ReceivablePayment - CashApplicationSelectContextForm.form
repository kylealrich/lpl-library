CashApplicationSelectContextForm is a Form
    title is "CashApplicationSelection"
    Layout
        visible when (false)
            TransientIsolatedPaymentCompany
        visible when (PaymentExists)
            four column
                column1 of 25%
                    display as text
                        paragraph
                            Company.RepresentativeText
                                label is "Company"
                            visible when (IsNotCompanyCurrency)
                                Company.Currency
                                    no label
                column2 of 23%
                    display as text
                        paragraph
                            ReceivablePaymentHeader
                                label is "Batch"
                            ReceivablePaymentHeader.Currency
                                label is "Currency"
                            ReceivablePaymentHeader.Status
                                label is "BatchStatus"
                column3 of 51%
                    paragraph
                        visible when (not UnreleasedOrUnassigned)
                            PaymentNumber
                                display as text
                            visible when (ReferenceNumberEntered)
                                ReferenceNumber
                                    display as text
                            ReceivablePayment
                                label is "PaymentSequence"
                                display as text
                            visible when (FinanceDimension2OrProjectContractOrProjectEntered)
                                paragraph
                                    FinanceDimension2
                                        label is "<Company.CustomerBusinessGroup.FinanceEnterpriseGroup.FinanceDimension2Label>"
                                        display as text
                                    ProjectContract.DisplayContract
                                        label is "<Company.CustomerBusinessGroup.FinanceEnterpriseGroup.ProjectLabel>Contract"
                                        display as text
                                    visible when (ProjectEntered)
                                        Project
                                            label is "<Company.CustomerBusinessGroup.FinanceEnterpriseGroup.ProjectLabel>"
                                            display as text
                            visible when (not FinanceDimension2OrProjectContractOrProjectEntered)
                                paragraph
                                    text of "___"
                                    visible when (CompanyCustomerMaximumOveragePercentEntered)
                                        CompanyCustomerMaximumOveragePercent
                                            label is "MaxOverage"
                                            display as text
                                    visible when (CompanyCustomerMaximumOverageAmountEntered)
                                        CompanyCustomerMaximumOverageAmount
                                            label is "MaximumOverageAmount"
                                            display as text
                        visible when (UnreleasedOrUnassigned)
                            PaymentNumber
                                display as text
                            visible when (ReferenceNumberEntered)
                                ReferenceNumber
                                    display as text
                            ReceivablePayment
                                label is "PaymentSequence"
                                display as text
                            Status
                                display as tag
                                    color of gray when (Status.Unreleased)
                                    color of yellow when (Status.ReleasedUnassigned)
                            visible when (CompanyCustomerMaximumOveragePercentEntered)
                                CompanyCustomerMaximumOveragePercent
                                    label is "MaxOverage"
                                    display as text
                            visible when (CompanyCustomerMaximumOverageAmountEntered)
                                CompanyCustomerMaximumOverageAmount
                                    label is "MaximumOverageAmount"
                                    display as text
                            visible when (Prepayment.Prepayment)
                                Prepayment
                                    no label
                column4 of 1%
                    paragraph
                        text of "_"
            four column
                display as text
                    column1 of 25%
                        paragraph
                            Customer.RepresentativeText
                            DerivedMessage
                                label is "_"
                                display as tag
                                    color of gray
                    column2 of 23%
                        paragraph
                            DepositDate
                            GeneralLedgerDate
                                label is "GlobalLedgerDate"
                            visible when (not UnreleasedOrUnassigned)
                                visible when (Prepayment.Prepayment)
                                    Prepayment
                                        no label
                    column3 of 51%
                        paragraph
                            DisplaySignedTransactionAmount
                                label is "PaymentAmount"
                            visible when (AppliedAmountEntered)
                                DisplaySignedAppliedAmount
                                    label is "Applied"
                            visible when (CreditAppliedAmountEntered)
                                DisplaySignedCreditAppliedAmount
                                    label is "CreditApplied"
                            visible when (HasAdjustments)
                                DisplaySignedAdjustAmountTransactionCurrency
                                    label is "Adjustments"
                            visible when (DisplaySignedCashApplicationPaymentOpen entered)
                                DisplaySignedCashApplicationPaymentOpen
                                    label is "Open"
                                        header4
                                        bold
                                        color of red
                                    header4
                                    bold
                                    color of red
                            visible when (DisplaySignedCashApplicationPaymentOpen not entered)
                                DisplaySignedCashApplicationPaymentOpen
                                    label is "Open"
                                        header4
                                        bold
                                        color of green
                                    header4
                                    bold
                                    color of green
                            visible when (FinanceDimension2OrProjectContractOrProjectEntered)
                                visible when (not UnreleasedOrUnassigned)
                                    paragraph
                                        visible when (CompanyCustomerMaximumOveragePercentEntered)
                                            text of "__"
                                            CompanyCustomerMaximumOveragePercent
                                                label is "MaxOverage"
                                        visible when (CompanyCustomerMaximumOverageAmountEntered)
                                            text of "__"
                                            CompanyCustomerMaximumOverageAmount
                                                label is "MaximumOverageAmount"
                    column4 of 1%
                        paragraph
                            text of "_"
            two column
                column1 of 67%
                    visible when (HasPendingTransactions)
                        display as text
                            paragraph
                                paragraph
                                    text of "Pending_\\Payment_\\Selections:"
                                        header4
                                        bold
                                visible when (HasPendingAppliedNonCreditMemoRel)
                                    DisplaySignedPendingAppliedNonCreditAmount
                                        label is "Debit"
                                visible when (HasPendingAppliedCreditMemoRel)
                                    DisplaySignedPendingAppliedCreditMemoAmount
                                        label is "Credit"
                                visible when (HasPendingAdjustMemoRel)
                                    DisplaySignedPendingAdjustMemoAmount
                                        label is "Chargeback"
                                visible when (HasPendingAdjustNonMemoRel)
                                    DisplaySignedPendingAdjustNonMemoAmount
                                        label is "PaymentWriteoff"
                                visible when (not PendingDifferenceDisplayCurrencyEqualsZero)
                                    DisplaySignedPendingSelectionsDisplayCurrency
                                        label is "Net"
                                            header4
                                            bold
                                            color of red
                                        header4
                                        color of red
                                visible when (PendingDifferenceDisplayCurrencyEqualsZero)
                                    DisplaySignedPendingSelectionsDisplayCurrency
                                        label is "Net"
                                            header4
                                            bold
                                            color of green
                                        header4
                                        color of green
                                visible when (WithinCompanyCustomerMaximumOverageThreshold)
                                    DisplaySignedPendingDifferenceDisplayCurrency
                                        label is "Difference"
                                            header4
                                            bold
                                            color of black
                                        display as tag
                                            color of yellow
                                        raise yellow alert when (true)
                                            mouse over text is "Not_in_balance,_but_within_Company_Customer's_Payment_Overage_tolerance."
                                visible when (PendingEqualsZeroAndNotWithinOverageThreshold)
                                    DisplaySignedPendingDifferenceDisplayCurrency
                                        label is "Difference"
                                            header4
                                            bold
                                            color of green
                                        display as tag
                                            color of green
                                visible when (NotWithinOverageThresholdAndNotEqualsZero)
                                    DisplaySignedPendingDifferenceDisplayCurrency
                                        label is "Difference"
                                            header4
                                            bold
                                            color of red
                                        display as tag
                                            color of red
                                text of "_"
                column2 of 33%
                    visible when (PendingApplicationsWithAdjustmentsRelExists)
                        display as text
                            paragraph
                                text of "Pending_\\Transaction_\\Adjustments:"
                                    header4
                                    bold
                                visible when (PendingAppliedNonCreditDiscountAmountNotEqualsZero)
                                    PendingAppliedNonCreditDiscountAmount
                                        label is "Debit"
                                visible when (PendingAppliedCreditMemoDiscountAmountNotEqualsZero)
                                    SignedPendingAppliedCreditMemoDiscountAmount
                                        label is "Credit"
                                NetPendingAppliedAdjustmentAmount
                                    label is "Net"
                                        bold
            visible when (DisplayProcessSelectionsInProgress)
                three column distributed
                    column1
                        visible when (DisplayProcessSelectionsInProgress)
                            text of "<ProcessSelectionsInBackgroundMsg>"
                                bold
                                italics
                                color of red
                        visible when (not DisplayProcessSelectionsInProgress)
                            text of "_"
        visible when (not PaymentExists)
            paragraph
                Company
                ReceivablePaymentHeader
                    label is "Batch"
                ReceivablePaymentHeader.Currency
                    label is "BatchCurrency"
                    display as text
                ReceivablePayment
                    display as text
                ReceivablePaymentHeader.Status
                    label is "BatchStatus"
                    display as tag
                        color of gray when (ReceivablePaymentHeader.Status.OpenBatchUnreleased)
                        color of green
            paragraph
                Customer
                PaymentNumber
                TransactionAmount
                    label is "Amount"
                ReferenceNumber
                SearchName
                visible when (Prepayment.Prepayment)
                    Prepayment
                        no label
            paragraph
                FinanceDimension2
                    label is "<Company.CustomerBusinessGroup.FinanceEnterpriseGroup.FinanceDimension2Label>"
                ProjectContract.DisplayContract
                    label is "<Company.CustomerBusinessGroup.FinanceEnterpriseGroup.ProjectLabel>Contract"
                Project
                    label is "<Company.CustomerBusinessGroup.FinanceEnterpriseGroup.ProjectLabel>"
