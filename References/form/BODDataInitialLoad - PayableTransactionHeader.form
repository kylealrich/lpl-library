PayableTransactionHeader is a Form
    Layout
        paragraph
            PayableBODSelectType
                label is "PayableBODSelectType"
        visible when (IsPayableTransaction)
            paragraph
                PayableTransactionVendorGroup
                    label is "VendorGroup"
            paragraph
                PayableTransactionFromVendor
                    label is "Vendor"
                PayableTransactionToVendor
                    label is "-"
            paragraph
                PayableTransactionFromCompany
                    label is "Company"
                PayableTransactionToCompany
                    label is "-"
            paragraph
                PayableTransactionFromInvoice
                    label is "PayablesInvoice"
                PayableTransactionToInvoice
                    label is "-"
            paragraph
                PayableTransactionDateRange.Begin
                    label is "CheckDate"
                PayableTransactionDateRange.End
                    label is "-"
            paragraph
                PayableTransactionCreateDateRange.Begin
                    label is "CreateDate"
                PayableTransactionCreateDateRange.End
                    label is "-"
        visible when (IsPayableTracker)
            paragraph
                PayableTransactionVendorGroup
                    label is "VendorGroup"
            paragraph
                PayableTransactionFromVendor
                    label is "Vendor"
                PayableTransactionToVendor
                    label is "-"
            paragraph
                PayableTransactionFromCompany
                    label is "Company"
                PayableTransactionToCompany
                    label is "-"
            paragraph
                PayableTransactionCreateDateRange.Begin
                    label is "PaymentDate"
                PayableTransactionCreateDateRange.End
                    label is "-"
        visible when (IsPayableTrackerTransaction)
            single column
                PayableTransactionTrigger
                    label is "Trigger"
                        bold
                        color of green
