CopyRecord is an ActionForm
    action is CopyRecord
    title is "PracticalCapacity"
    Layout
        single column
            TrueCostConfiguration
                label is "True\\CostConfiguration"
            AccountingEntity
                label is "AccountingEntity"
            TrueCostPracticalCapacity.Structure
                label is "Structure"
            TrueCostPracticalCapacity.CostObject
                label is "Summary<TrueCostConfiguration.CostObjectLabel>"
            visible when (TrueCostConfiguration.DepartmentIsAccountingUnit)
                PrmAccountingUnit
            visible when (TrueCostConfiguration.DepartmentIsDimension1)
                PrmDepartmentDimension1
            visible when (TrueCostConfiguration.DepartmentIsDimension2)
                PrmDepartmentDimension2
            visible when (TrueCostConfiguration.DepartmentIsDimension3)
                PrmDepartmentDimension3
            visible when (TrueCostConfiguration.DepartmentIsDimension4)
                PrmDepartmentDimension4
            visible when (TrueCostConfiguration.DepartmentIsDimension5)
                PrmDepartmentDimension5
            visible when (TrueCostConfiguration.DepartmentIsDimension6)
                PrmDepartmentDimension6
            visible when (TrueCostConfiguration.DepartmentIsDimension7)
                PrmDepartmentDimension7
            visible when (TrueCostConfiguration.DepartmentIsDimension8)
                PrmDepartmentDimension8
            visible when (TrueCostConfiguration.DepartmentIsDimension9)
                PrmDepartmentDimension9
            visible when (TrueCostConfiguration.DepartmentIsDimension10)
                PrmDepartmentDimension10
            PrmDriver
            PrmWeight
            PrmCapacityVolume
            PrmStartDate
