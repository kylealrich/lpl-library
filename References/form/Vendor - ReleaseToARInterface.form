ReleaseToARInterface is an ActionForm
    action is ReleaseToARInterface
    title is "InterfaceReleasedReceivables"
    Layout
        paragraph
            PrmReceivableCompany
                label is "Company"
                when value changed
                    PrmDetailReceivablesInterface =  PrmReceivableCompany.Company.PayablesCompany.DetailReceivablesInterface
            PrmReceivableCompany.Name
                no label
                display as text
        paragraph
            PrmDetailReceivablesInterface
                label is "DetailReceivablesInterface"
                    left of field
        paragraph
            PrmARPostDate
                label is "PostDate"
        paragraph
            visible when (not PrmDetailReceivablesInterface)
                PrmInvoice
                    label is "ARInvoice"
        paragraph
            PrmARProcessLevel
                label is "ARProcessLevel"
            PrmARProcessLevel.Name
                no label
                display as text
        paragraph
            visible when (not PrmDetailReceivablesInterface)
                PrmInvoiceDate
                    label is "InvoiceDate"
        paragraph
            visible when (not PrmDetailReceivablesInterface)
                PrmTermsCode
                    label is "TermsCode"
        paragraph
            PrmMemoReason
                label is "MemoReason"
            PrmMemoReason.Description
                no label
                display as text
        paragraph
            PrmARAccrualCode
                label is "ARAccrualCode"
            PrmARAccrualCode.Description
                no label
                display as text
        paragraph
            visible when (not PrmDetailReceivablesInterface)
                PrmDueDate
                    label is "DueDate"
        paragraph
            PrmDistributionClearingAccount
                label is "*ClearingAccount"
                compact format
        paragraph
            visible when (PrmReceivableCompany.GeneralLedgerCompany.RequireHSNSACCode)
                PrmHSNSACCode
                    label is "HSNSACCode"
