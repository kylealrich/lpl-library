PlanHolderPrimary is a Form
    is primary
    title is "PlanHolder"
    Layout
        paragraph
            SourcingEvent
                label is "Event#"
                display as text
            SourcingEvent.LastVersionNumber
                no label
                display as text
            SourcingEvent.Name
                display as text
        paragraph
            NotifiedSupplier.SupplierGroup
            NotifiedSupplier.SupplierGroup.Description
                no label
                display as text
        paragraph
            NotifiedSupplier.Supplier
            NotifiedSupplier.Supplier.SupplierName
                no label
                display as text
        paragraph
            NotifiedSupplier.SupplierSourceId
                label is "SupplierContact"
            NotifiedSupplier.SupplierSourceId.MainContact.FirstAndLastName
                no label
                display as text
