CreateUserWizard is a WizardForm
    propagate as a drill
    use for action CreateUserWizard
    title is "CreateUserWizard"
    summary form is ActorSummary
    GeneralUserInformation is a Panel
        mouse over text is "CompleteGeneralUserInformation"
        form is GeneralInfoWizard
    ActorContactInfo is a Panel
        mouse over text is "CompleteUserContactInformation"
        form is ContactInfoWizard
    CreateIdentityForUser is a Panel
        mouse over text is "InformationRequiredToCreateIdentityForUser"
        form is CreateIdentityForm
    ActorIdentityAssignment is a Panel
        mouse over text is "AssignIdentityTo<Actor>:ClickNewAndSelectIdentityToAssign"
        list is IdentityActor set.SecondaryIdentityActorList
    ActorRoleAssignment is a Panel
        mouse over text is "AssignRoleTo<Actor>:ClickNewAndSelectRoleToAssign"
        list is ActorRole set.WizardActorRoleList
    CustomActorFields is a Panel
        mouse over text is "CompleteCustomActorFields"
        form is CustomActorFieldsWizard
