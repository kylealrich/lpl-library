PFlowSupplier is a CompositeForm
    title is "SupplierInformation"
    context form is PFlowContext
    Address is a Panel
        form is PFlowAddress
    Contacts is a Panel
        list is SupplierSourceId set.PFlowContactList
    ContactBankInformation is a Panel
        visible when (CanDisplayBankInformation)
        list is SupplierSourceId set.BuyerContactBankInformation
    Certifications is a Panel
        visible when (CertificationsExist)
        list is AllSupplierCertificationsRel.PFlowSupplierCertifications
    Diversity is a Panel
        visible when (DiversityCodesExist)
        list is SupplierDiversityResponse set.PFDiversityResponses
    Questions is a Panel
        visible when (HasQuestions)
        list is SupplierQuestionResponseRels.SupplierResponses
    Comments is a Panel
        visible when (CommentsExist)
        list is SupplierComment set.PFSupplierComment
