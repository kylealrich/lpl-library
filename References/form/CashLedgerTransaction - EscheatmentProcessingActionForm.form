EscheatmentProcessingActionForm is an ActionForm
    action is EscheatmentProcessing
    title is "EscheatmentProcessing"
    Layout
        single column
            PrmCashManagementGroup
                label is "CashManagementGroup"
        three column
            PrmCashCode
                label is "CashCode"
                select is CashCode
            PrmCashCode.Description
                no label
                display as text
            PrmCashCode.Currency
                no label
                display as text
        two column
            PrmCashCodeGroup
                label is "CashCodeGroup"
                select is CashCodeGroup
            PrmCashCodeGroup.Description
                no label
                display as text
        blank line
        single column
            PrmBankTransactionCode
                label is "BankTransactionCode"
            PrmProcessLevelProcessing
                label is "ProcessLevelProcessing"
                    left of field
            PrmProcessLevel
                label is "ProcessLevel"
            PrmIncludeAllTransactions
                label is "IncludeAllTransactions"
                    left of field
            PrmCountry
                label is "Country/\\Jurisdiction"
            PrmState
                label is "State"
            PrmFileName
                label is "Filename"
            PrmEscheatDate
                label is "EscheatDate."
            PrmDays
                label is "Days"
            PrmPostDate
                label is "PostDate"
