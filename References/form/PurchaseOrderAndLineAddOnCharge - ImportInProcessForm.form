ImportInProcessForm is a Form
    title is "PurchaseOrderLineAddOnCharge"
    Layout
        header2 of "AddOnCharge"
        display as text
            two column
                column1
                    paragraph
                        AddOnCharge
                        AddOnCharge.Description
                            no label
                            display as text
                column2
                    AddOnChargeEntryMethod
            two column
                visible when (AddOnChargeEntryMethod.TotalAmount)
                    column1
                        ZeroCost
                        EnteredUnitCost
                            label is "UnitCost"
                            display as text
                    column2
                        TotalAddOnChargeAmount
                            label is "TotalAddOnCost"
                visible when (AddOnChargeEntryMethod.RatePercentage)
                    column1
                        AddOnChargePercent
                            label is "Rate"
                        EnteredUnitCost
                            label is "UnitCost"
                            display as text
                    column2
                        TotalAddOnChargeAmount
                            label is "TotalAddOnCost"
                            display as text
                visible when (AddOnChargeEntryMethod.UnitCost)
                    column1
                        EnteredUnitCost
                            label is "UnitCost"
                    column2
                        TotalAddOnChargeAmount
                            label is "TotalAddOnCost"
                            display as text
            header2 of "ProcessFlags"
            two column
                column1
                    Taxable
                    visible when (HasInvoicedTax)
                        InvoicedTaxAmount
                            display as text
                    visible when (HasAccruedTax)
                        AccruedTaxAmount
                            display as text
                    Summarize
                        label is "Summary"
                    FromRequisition
                        display as text
                column2
                    TaxCode
                    AddOnCharge.LandedAddOnCharge
                        label is "LandedFlag"
                    PrintOnPO
                        label is "PrintOnPurchaseOrder"
