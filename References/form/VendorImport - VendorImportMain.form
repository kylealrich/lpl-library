VendorImportMain is a Form
    title is "Main"
    Layout
        visible when (not HasLocation)
            two column distributed
                column1
                    VendorClass
                    TaxID
                    TaxIDType
                    DefaultTaxIndicator
                        label is "TaxIndicator"
                    PayVendor
                column2
                    PayablesProcessGroup
                    CheckDigitType
                    Carrier
                    OldPayVendor
                    TinNot
                        label is "SecondT\\I\\NNoticeReceived"
        visible when (HasLocation)
            single column
                LocationType
                NormalRemitToLocation
                NormalPurchaseFromLocation
                RemitTo
                    label is "RemitPaymentTo"
        two column distributed
            PayablesGLNNumber
                label is "PayablesG\\L\\N"
            PurchaseOrderGLNNumber
                label is "PurchaseOrderG\\L\\N"
            ElectronicGovernmentID
            DunAndBradstreetNumber
        visible when (not HasLocation)
            header2 of "CustomerInformation"
            two column distributed
                column1
                    paragraph
                        CustomerGroup
                        CustomerGroup.Name
                            no label
                            display as text
                column2
                    paragraph
                        Customer
                        Customer.Name
                            no label
                            display as text
        header2 of "CurrentAddress"
        PostalAddress
        single column
            header2 of "GIIN_/FATCAInformation"
            paragraph
                FATCAId
                    label is "G\\I\\I\\N_\\ID"
                FinancialInstitutionType
                    label is "Type"
                GIINCategoryCode
                    label is "Category"
            paragraph
                W8Verified
                    label is "W\\8_\\Verified"
                ISOCountryNumber
                    label is "Country/\\Jurisdiction"
            paragraph
                FATCAPercentage
                    label is "F\\A\\T\\C\\A_\\Rate"
                    display as required when (W8Verified.No)
                FATCAIncomeCode
                    label is "F\\A\\T\\C\\A_\\Code"
                    display as required when (W8Verified.No)
                FATCAAccount
                    label is "F\\A\\T\\C\\A_\\Account"
                    compact format
                    display as required when (W8Verified.No)
            column2
                URLAddress
                visible when (HasLocation)
                    PayablesGLNNumber
                        label is "PayablesVendorLocationG\\L\\N"
