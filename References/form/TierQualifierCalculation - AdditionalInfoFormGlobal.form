AdditionalInfoFormGlobal is a Form
    title is "AdditionalInformation"
    Layout
        display as text
            visible when (HasQualifier)
                paragraph
                    ContractTier
                    ContractTierQualifier
            paragraph
                Company
                Location
            visible when (HasQuantity)
                paragraph
                    QualifiedQuantity
                    ContractTierQualifier.PurchaseQuantity
                        label is "QualifierQuantity"
                    visible when (ContractTierQualifierRel.PurchasePercentQualifier)
                        CompetitiveQuantity
            single column
                OnlyForTier
                FromInvoicesOrPurchaseOrders
                    label is "FromInvoicesOrPurchaseOrders"
