Main is a Form
    Layout
        four column distributed
            column1
                RechargeItem
                    select is ActiveRechargeItemList
                    show up to 10 characters
                Description
                    show up to 15 characters
            column2
                Quantity
                paragraph
                    ReceivablesDistributionCode
                        label is "ReceivablesDistributionCode"
                        select is IntercompanyBillingActiveReceivablesDistCodeSelectList
                        show up to 10 characters
                    ReceivablesDistributionCode.Description
                        no label
                        display as text
            column3
                UnitPrice
                paragraph
                    PayablesDistributionCode
                        label is "PayablesDistributionCode"
                        select is IntercompanyBillingActivePayablesDistCodeSelectList
                        show up to 10 characters
                    PayablesDistributionCode.Description
                        no label
                        display as text
            column4
                UnitOfMeasure
                    select is ActiveIntercompanyBillingUnitOfMeasureList
        four column distributed
            visible when (IsHSNSACCodeEnabled)
                paragraph
                    HSNSACCode
                    HSNSACCode.Description
                        no label
                        display as text
            visible when (ShowUNSPSCCode)
                paragraph
                    UNSPSCCodeGroup.UNSPSCCode
                        compact format
                    UNSPSCCodeGroup.UNSPSCCode.ShortDescription
                        label is "_"
                        display as text
        header2 of "TaxOptions"
        four column distributed
            Taxable
            paragraph
                TaxCode
                    show up to 10 characters
                TaxCode.Description
                    no label
                    display as text
            visible when (TaxEntityUsesInforTax)
                paragraph
                    AlternatePayablesTaxCode
                        show up to 10 characters
                    AlternatePayablesTaxCode.Description
                        no label
                        display as text
        four column distributed
            paragraph
                TaxCategory
                    show up to 10 characters
                TaxCategory.Description
                    no label
                    display as text
            paragraph
                TaxUsageCode
                    label is "TaxUsage"
                    show up to 10 characters
                TaxUsageCode.Description
                    no label
                    display as text
        visible when (HasWithholdingAmount)
            header2 of "WithholdingAccounts"
            two column distributed
                column1
                    visible when (HasWithholdingAmount)
                        ReceivableWithholdingAccount
                            compact format
                        PayableWithholdingAccount
                            compact format
                        visible when (HasCashBasisAccount)
                            CashBasisAccount
                                compact format
                column2
                    visible when (HasSecondaryWithholdingAmount)
                        SecondaryReceivableWithholdingAccount
                            compact format
                        SecondaryPayableWithholdingAccount
                            compact format
                        visible when (HasSecondaryCashBasisAccount)
                            SecondaryCashBasisAccount
                                compact format
