UpgradeActiveSpaces is a Form
    title is "UpgradeActiveSpaces"
    Layout
        single column
            text of "FSM_Analytics_Version"
                bold
        two column
            column1
                text of "Latest_Metadata_Version_(SMD)"
                text of "Active_Environment_Version_(ATC)"
            column2
                last ProductVersionSMDRel.Value
                    label is "_"
                last ProductVersionATCRel.Value
                    label is "_"
        single column
            text of "_"
            visible when (Status.InProgress)
                button of "RefreshProductVersion"
                    action is UpdateVersion
            blank line
        single column
            blank line
            header3 of "Replace_Active_Environment_from_Latest_Metadata"
            blank line
        paragraph
            text of "Any_customization_will_be_lost_in_the_active_environment_upon_upgrade,_please_make_sure_you_have_a_backup."
        single column
            visible when (Status.InProgress)
                button of "StartActiveEnvironmentUpgrade"
                    action is StartSMDToATCUpgrade
            last LatestSMDtoATCRel.Status
                label is "Status"
                raise red alert when (last LatestSMDtoATCRel.IsFailed)
                raise yellow alert when (last LatestSMDtoATCRel.IsCanceled)
                raise blue alert when (last LatestSMDtoATCRel.IsActionPendingOrActionPendingInServer)
                    mouse over text is "ActionPending"
            button of "<SMDToATCWorkUnit>"
                valid when (IsSMDtoATCWorkunitExists)
                link is SMDtoATCUpgradeWorkUnitNav
                label is "WorkUnit"
                display as link
            valid when (Status.InProgress)
                display as text
                UpgradeActiveSpacesIsCompleted
                    label is "Please_confirm_if_Status_of_workunit_is_completed_before_proceeding_the_next_steps."
