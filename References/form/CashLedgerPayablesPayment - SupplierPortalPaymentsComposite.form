SupplierPortalPaymentsComposite is a CompositeForm
    title is "Payment<TransactionNumber>Details"
    called out actions only
    context form is SupplierPortalContext
        background is theme color
    Actions
        action is previous record
        action is next record
    Invoices is a Panel
        list is AppaymentRel.SupplierPortalMultipleInvoicesDisplay
    RelatedInvoiceHistory is a Panel
        visible when (HasApAppHist)
        list is ApapphistRel.PayablesInvoicePaymentHistoryList
