ContractDates is a Form
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        display as text
            single column
                EffectiveDate
                ExpirationDate
                visible when (HasReviewDate)
                    ContractReviewDate
                        label is "ReviewDate"
                visible when (IsManufacturerContract)
                    visible when (HasDistributorContract)
                        text of "ThisContractHasAnAssociatedDistributorContractOrContracts"
                            italics
                        text of "DatesOnAssociatedDistributorContractLinesWillAlsoBeChanged"
                            italics
                        text of "LineEffectiveDatesWillBeChangedToLaterOfNewEffectiveDateOrDistributorContractHeaderEffectiveDate"
                            italics
                        text of "LineExpirationDatesWillBeChangedToEarlierOfNewExpirationDateOrDistributorContractHeaderExpirationDate"
                            italics
                visible when (IsDistributorContract)
                    visible when (HasManufacturerContract)
                        text of "ThisContractHasAssociatedManufacturerContracts"
                            italics
                        text of "LineEffectiveDatesWillBeChangedToLaterOfNewEffectiveDateOrManufacturerLineEffectiveDate"
                            italics
                        text of "LineExpirationDatesWillBeChangedToEarlierOfNewExpirationDateOrManufacturerExpirationDate"
                            italics
