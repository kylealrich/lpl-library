TransferredTransactionReport is an ActionForm
    action is TransferredTransactionReport
    Layout
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        single column
            text of "Or"
        paragraph
            CompanyGroup
                label is "CompanyGroup"
            CompanyGroup.Description
                no label
                display as text
        single column
            Type
        paragraph
            FrTxfrDate
                label is "FromTransferDate"
            ToTxfrDate
                label is "ToTransferDate"
        paragraph
            Customer
                select is GroupSearchName
            Customer.Name
                no label
                display as text
        paragraph
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                TransferredTransactionReportReport_RDG
                TransferredTransactionErrorReport_RDG
            column2 of 25%
                TransferredTransactionReportReport_XPT
                TransferredTransactionErrorReport_XPT
