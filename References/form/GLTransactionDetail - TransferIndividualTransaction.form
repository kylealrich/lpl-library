TransferIndividualTransaction is an ActionForm
    action is TransferIndividualTransaction
    title is "TransferIndividualTransaction"
    Layout
        two column
            column1
                FinanceCodeBlock
                    label is "FinanceStructure"
                    display as text
                    compact format
                PostingDate
                    display as text
                TransactionAmount
                    display as text
                UnitsAmount
                    display as text
                Description
                    display as text
            column2
                PrmFinanceCodeBlock
                    label is "ToFinanceStructure"
                    compact format
                PrmPostingDate
                    label is "ToPostingDate"
                PrmTransactionAmount
                    label is "ToTransactionAmount"
                PrmUnitsAmount
                    label is "ToUnitsAmount"
                PrmDescription
                    label is "ToDescription"
