ProcurementTemplateMassChanges is an ActionForm
    action is ProcurementTemplateMassChange
    Layout
        header2 of "Main"
        two column
            column1
                paragraph
                    ProcureGroup
                        label is "ProcurementGroup"
                        select is ProcurementGroup
                    ProcureGroup.Description
                        no label
                        display as text
            column2
                UpdateOption
                    label is "Update"
        single column
            paragraph
                blank line
        five column table
            column headers
                "Requesting_Company"
                "Buyer_Location"
                "Requester"
                "Group"
                "Buyer"
            row headers
                "1"
                "2"
                "3"
                "4"
                "5"
                "6"
            column1
                PoaCompany.PO115PoaCompanyGroup[1].PoaCompany
                    no label
                PoaCompany.PO115PoaCompanyGroup[2].PoaCompany
                    no label
                PoaCompany.PO115PoaCompanyGroup[3].PoaCompany
                    no label
                PoaCompany.PO115PoaCompanyGroup[4].PoaCompany
                    no label
                PoaCompany.PO115PoaCompanyGroup[5].PoaCompany
                    no label
                PoaCompany.PO115PoaCompanyGroup[6].PoaCompany
                    no label
            column2
                PoaCompany.PO115PoaCompanyGroup[1].PoaReqLocation
                    no label
                    show up to 8 characters
                PoaCompany.PO115PoaCompanyGroup[2].PoaReqLocation
                    no label
                    show up to 8 characters
                PoaCompany.PO115PoaCompanyGroup[3].PoaReqLocation
                    no label
                    show up to 8 characters
                PoaCompany.PO115PoaCompanyGroup[4].PoaReqLocation
                    no label
                    show up to 8 characters
                PoaCompany.PO115PoaCompanyGroup[5].PoaReqLocation
                    no label
                    show up to 8 characters
                PoaCompany.PO115PoaCompanyGroup[6].PoaReqLocation
                    no label
                    show up to 8 characters
            column3
                BuyerArray.PO115BuyerGroup[1].PoaRequester
                    no label
                BuyerArray.PO115BuyerGroup[2].PoaRequester
                    no label
                BuyerArray.PO115BuyerGroup[3].PoaRequester
                    no label
                BuyerArray.PO115BuyerGroup[4].PoaRequester
                    no label
                BuyerArray.PO115BuyerGroup[5].PoaRequester
                    no label
                BuyerArray.PO115BuyerGroup[6].PoaRequester
                    no label
            column4
                BuyerArray.PO115BuyerGroup[1].PoaBuyerGrp
                    no label
                BuyerArray.PO115BuyerGroup[2].PoaBuyerGrp
                    no label
                BuyerArray.PO115BuyerGroup[3].PoaBuyerGrp
                    no label
                BuyerArray.PO115BuyerGroup[4].PoaBuyerGrp
                    no label
                BuyerArray.PO115BuyerGroup[5].PoaBuyerGrp
                    no label
                BuyerArray.PO115BuyerGroup[6].PoaBuyerGrp
                    no label
            column5
                BuyerArray.PO115BuyerGroup[1].PoaBuyer
                    no label
                BuyerArray.PO115BuyerGroup[2].PoaBuyer
                    no label
                BuyerArray.PO115BuyerGroup[3].PoaBuyer
                    no label
                BuyerArray.PO115BuyerGroup[4].PoaBuyer
                    no label
                BuyerArray.PO115BuyerGroup[5].PoaBuyer
                    no label
                BuyerArray.PO115BuyerGroup[6].PoaBuyer
                    no label
        paragraph
            blank line
        header2 of "ChangeTemplates"
        single column
            AllTemplatesM
                label is "All"
                when value changed
                    TemplateRef.ProcurementTemplate[1] =  blank
                    TemplateRef.ProcurementTemplate[2] =  blank
                    TemplateRef.ProcurementTemplate[3] =  blank
                    TemplateRef.ProcurementTemplate[4] =  blank
                    TemplateRef.ProcurementTemplate[5] =  blank
                    TemplateRef.ProcurementTemplate[6] =  blank
                        when (AllTemplatesM.Yes)
        visible when (AllTemplatesM.No)
            three column table
                column1
                    TemplateRef.ProcurementTemplate[1]
                        label is "Template1"
                column2
                    TemplateRef.ProcurementTemplate[2]
                        label is "Template2"
                column3
                    TemplateRef.ProcurementTemplate[3]
                        label is "Template3"
            three column table
                column1
                    TemplateRef.ProcurementTemplate[4]
                        label is "Template4"
                column2
                    TemplateRef.ProcurementTemplate[5]
                        label is "Template5"
                column3
                    TemplateRef.ProcurementTemplate[6]
                        label is "Template6"
        header2 of "ItemChange"
        ItemChange
            label is "ChangeType"
            when value changed
                DelItemA =  blank
                ReplItemA =  blank
                ReplItemB =  blank
                Item =  blank
                FromCompanyLocation.FromCompany =  blank
                AcctUnitFl =  blank
                AcctUnit =  blank
                FromCompanyLocation.FromLocation =  blank
                Quantity =  blank
                Uom =  blank
                ServiceCode =  blank
                SvcSpcDesc =  blank
                AddItemA =  blank
                AddItemState =  blank
                AddItemB =  blank
                ServiceCodeA =  blank
                SvcSpcDescA =  blank
                QuantityA =  blank
                UomA =  blank
                AcctUnitFlA =  blank
                AcctUnitA =  blank
                FromLocationA.FromLocation =  blank
                FromLocationA.FromCompany =  blank
                FilterFromCompany =  blank
                FilterFromLocation =  blank
        visible when (ItemChange.Update)
            Item
                label is "Item"
            FromCompanyLocation.FromCompany
                label is "FromCompany"
            AcctUnitFl
                label is "UseAUFromRequestingLocation"
            AcctUnit
                label is "NewDistributionAccount"
                compact format
            FromCompanyLocation.FromLocation
                label is "FromLocation"
            two column distributed
                Quantity
                Uom
                    label is "U\\O\\M"
            two column distributed
                ServiceCode
                SvcSpcDesc
                    label is "Service\\/\\SpecialDescription"
        visible when (ItemChange.Replace)
            two column distributed
                FilterFromCompany
                    label is "FromCompany"
                FilterFromLocation
                    label is "FromLocation"
            two column distributed
                ReplItemA
                    label is "ItemA"
                ReplItemB
                    label is "ItemB"
            single column
                SvcSpcDesc
                    label is "Service\\/\\SpecialDescription"
        visible when (ItemChange.Add)
            AddItemA
                label is "Item"
            AddItemState
                label is "AddItem..."
                when value changed
                    AllTemplatesI =  AllTemplatesI.Yes
                        when (AddItemState.ToAllTemplates)
                    WithItemB =  blank
                        when (AddItemState.ToAllTemplates)
                    AddItemB =  blank
                        when (AddItemState.ToAllTemplates)
                    AllTemplatesI =  blank
                        when (AddItemState.ToAllTemplatesContainingItemB)
                    WithItemB =  WithItemB.Yes
                        when (AddItemState.ToAllTemplatesContainingItemB)
                    WithItemB =  blank
                        when (AddItemState not entered)
                    AllTemplatesI =  blank
                        when (AddItemState not entered)
                    AddItemB =  blank
                        when (AddItemState not entered)
            visible when (WithItemB.Yes)
                two column distributed
                    FilterFromCompany
                        label is "FromCompany"
                    FilterFromLocation
                        label is "FromLocation"
                two column distributed
                    AddItemB
                        label is "ItemB"
            two column distributed
                ServiceCodeA
                    label is "ServiceCode"
                SvcSpcDescA
                    label is "Service\\/\\SpecialDescription"
            two column distributed
                QuantityA
                    label is "Quantity"
                UomA
                    label is "U\\O\\M"
            AcctUnitFlA
                label is "UseAUFromRequestingLocation"
            AcctUnitA
                label is "Account"
                compact format
            two column
                FromLocationA
                    label is "FromLocation"
        visible when (ItemChange.Remove)
            two column distributed
                FilterFromCompany
                    label is "FromCompany"
                FilterFromLocation
                    label is "FromLocation"
            DelItemA
                label is "ItemA"
        visible when (false)
            AllTemplatesI
            WithItemB
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                ProcurementTemplateMassChangeReport_RDG
                ProcurementTemplateErrorReport_RDG
            column2 of 25%
                ProcurementTemplateMassChangeReport_XPT
                ProcurementTemplateErrorReport_XPT
