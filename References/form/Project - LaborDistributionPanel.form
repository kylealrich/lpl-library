LaborDistributionPanel is a Form
    title is "<FinanceEnterpriseGroup.ProjectLabel>LaborDistribution"
    Layout
        visible when (InEnterpriseStructure)
            single column
                PreAwardAssignmentDays
                FinanceCodeBlock
                    compact format
                text of "DimensionsSetToBlank:"
                    align as label
                FinanceCodeBlockSetBlanks
                    no label
            two column distributed
                column1
                    FringeBurden
                column2
                    paragraph
                        BurdenCode
                            show up to 10 characters
                        BurdenCode.Description
                            no label
                            display as text
                        BurdenCode.Rate
                            display as text
            single column
                BurdenCodeBlock
                    compact format
                text of "DimensionsSetToBlank:"
                    align as label
                BurdenCodeBlockSetBlanks
                    no label
                CertificationRequired
            header2 of "SalaryCap"
            two column distributed
                column1
                    paragraph
                        SalaryCap
                            show up to 10 characters
                        SalaryCap.Description
                            no label
                            display as text
                column2
                    SalaryCapDate
            single column
                OverageCodeBlock
                    compact format
                text of "DimensionsSetToBlank:"
                    align as label
                OverageCodeBlockSetBlanks
                    no label
        visible when (not InEnterpriseStructure)
            single column
                PreAwardAssignmentDays
                    display as text
                FinanceCodeBlock
                    display as text
                    compact format
                text of "DimensionsSetToBlank:"
                    align as label
                FinanceCodeBlockSetBlanks
                    no label
                    display as text
            two column distributed
                column1
                    FringeBurden
                        display as text
                column2
                    paragraph
                        BurdenCode
                            display as text
                            show up to 10 characters
                        BurdenCode.Description
                            no label
                            display as text
                        BurdenCode.Rate
                            display as text
            single column
                BurdenCodeBlock
                    display as text
                    compact format
                text of "DimensionsSetToBlank:"
                    align as label
                BurdenCodeBlockSetBlanks
                    no label
                    display as text
                CertificationRequired
                    display as text
            header2 of "SalaryCap"
            two column distributed
                column1
                    paragraph
                        SalaryCap
                            display as text
                        SalaryCap.Description
                            no label
                            display as text
                column2
                    SalaryCapDate
                        display as text
            single column
                OverageCodeBlock
                    display as text
                    compact format
                text of "DimensionsSetToBlank:"
                    align as label
                OverageCodeBlockSetBlanks
                    no label
                    display as text
