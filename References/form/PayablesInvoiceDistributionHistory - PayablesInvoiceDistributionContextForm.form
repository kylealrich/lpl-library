PayablesInvoiceDistributionContextForm is a Form
    title is "Distribution"
    Layout
        paragraph
            PayablesInvoiceHistory.Company
                display as text
            Company.Name
                no label
                display as text
            PayablesInvoiceHistory.Vendor
                display as text
            PayablesInvoiceHistory.Invoice
                display as text
            PayablesInvoiceHistory.InvoiceAmount
                label is "InvoiceAmount"
                display as text
            PayablesInvoiceHistory.TaxAmount
                label is "InvoiceTaxAmount"
                display as text
            PayablesInvoiceHistory.TotalDistributionAmount
                label is "TotalDistributions"
                display as text
        paragraph
            PayablesInvoiceHistory.DistributionDate
                label is "PostDate"
                display as text
            PayablesInvoiceDistributionHistory
                label is "DistributionSequence"
                display as text
            OriginalDistribution
                no label
                display as text
            TaxSequenceNumber
                no label
                display as text
            Status
                display as text
