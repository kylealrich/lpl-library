SourcingGroupSetup is a CompositeForm
    is primary
    title is "SourcingGroup"
    context form is SourcingGroupContext
        background is theme color
    SourcingGroup is a Panel
        title is "Main"
        form is SourcingGroup
    EventResponseText is a Panel
        form is EventResponseText
    EventEmailAndPortalMessageConfiguration is a Panel
        title is "Email_andPortalMessages"
        form is EventEmail
    EventWizardDefinition is a Panel
        form is EventWizard
    AwardReasons is a Panel
        list is AwardReason set.AwardReasons
    EventTypes is a Panel
        list is SourcingEventType set.EventType
    NoBidReasons is a Panel
        list is NoBidReason set.NoBidReasons
    BuyerWeightingCriteria is a Panel
        list is BuyerWeighting set.BuyerWeightingCriteria
