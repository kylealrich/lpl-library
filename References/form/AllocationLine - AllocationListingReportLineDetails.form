AllocationListingReportLineDetails is a SummaryForm
    title is "_"
    Layout
        list is SelfAllocationLineRel.AllocationListinReportSourceAddressList
        visible when (HasExternalDetailedSourcesValue)
            list is AllocationLineSourceValueForReportRel.AllocationLineSourceValueForReportList
        visible when (HasExternalDetailedSources)
            list is AllocationLineSourceDetailsForReportRel.AllocationLineSourceDetailedAddressForReportList
        visible when (HasNoExternalDetailedSources)
            list is SelfAllocationLineRel.AllocationListingReportDetailedAddressList
        visible when (HasNoSourceBalances)
            NoSourceBalancesMessage
        blank line
        visible when (HasSingleOverride)
            header4 of "SingleOverride"
                center
        visible when (HasSingleFromOverride)
            list is AllocationLineSingleOverrideForReportRel.AllocationListingReportLineFromMappingList
        visible when (HasSingleToOverride)
            list is AllocationLineSingleOverrideForReportRel.AllocationListingReportLineToMappingList
        visible when (HasMultipleOverrides)
            header4 of "MultipleOverrides"
                center
        visible when (HasComputeOverrideDistinctDimension)
            form is AllocationLineTempOverridesHeaderForReportRel.AllocationLineTempOverrideListingReport
        visible when (not HasComputeOverrideDistinctDimension)
            list is AllocationLineTempOverridesForReportRel.AllocationLineMappingReportList
