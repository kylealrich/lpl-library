MPrimaryMiscForm is a Form
    title is "PrimaryMiscForm"
    Layout
        header2 of "AOC_\\Prorate"
        single column
            TransientAddOnCharge
            TransientProrateAmount
            TransientProrateAccount
                compact format
        header2 of "TaxInformation"
        two column distributed
            TaxCode
                label is "InvoiceTaxCode"
                display as text
            TaxCode.Description
                no label
                display as text
        two column distributed
            visible when (MatchProcessType.Expense)
                TaxAmount
                    display as text
            visible when (not MatchProcessType.Expense)
                DerivedMatchInvoiceTaxAmount
                    label is "InvoiceTaxAmount"
            TaxableAmount
                label is "InvoiceTaxableAmount"
                display as text
        two column distributed
            DerivedTotalTaxAmountField
                display as text
            TotalTaxableAmount
                display as text
        single column
            TransientTaxPointDate
                display as text
        visible when (InvoiceSource.ImagingSystem)
            header2 of "DocumentManagementInterface"
            two column distributed
                visible when (InvoiceSource.ImagingSystem)
                    ScanDate
                        display as text
                    ExternalReasonCode
                        display as text
            two column distributed
                visible when (InvoiceSource.ImagingSystem)
                    AlphaAttribute1
                        display as text
                    ExternalReason
                        display as text
        header2 of "Global"
        visible when (TaxEntityRel.IntrastatUsed)
            header4 of "IntrastatData"
            single column
                visible when (TaxEntityRel.IntrastatUsed)
                    visible when (IntrastatRecordExist)
                        button of "IntrastatNumber:<IntrastatNumber>"
                            visible when (IntrastatRecordExist)
                            link is IntrastatHeaderNav
                            display as link
            visible when (TaxEntityRel.IntrastatUsed)
                two column distributed
                    OriginCountry
                        label is "FromCountry/\\Jurisdiction"
                        display as text
                    OriginCountry.MemberOfEC
                        label is "ECFlag"
                        display as text
                two column distributed
                    OriginRegion
                        label is "FromRegion"
                        display as text
                    OriginRegion.ExcludeFromIntrastat
                        label is "ExcludeFlag"
                        display as text
            visible when (TaxEntityRel.IntrastatUsed)
                two column distributed
                    DestinationCountry
                        label is "ToCountry/\\Jurisdiction"
                        display as text
                    DestinationCountry.MemberOfEC
                        label is "ECFlag"
                        display as text
                two column distributed
                    DestinationRegion
                        label is "ToRegion"
                        display as text
                    DestinationRegion.ExcludeFromIntrastat
                        label is "ExcludeFlag"
                        display as text
        two column distributed
            NatureOfTransactionCode
            NatureOfTransactionCode.Description
                no label
                display as text
        two column distributed
            ShipTerm
            ShipTerm.Description
                no label
                display as text
        single column
            DestinationCountry
                display as text
            Dropship
        two column distributed
            StatisticalProcedure
            StatisticalProcedure.Description
                no label
                display as text
        single column
            ShippingMethod
        two column distributed
            UnloadingPort
            UnloadingPort.Description
                no label
                display as text
        two column distributed
            GlobalDocumentType
                label is "DocumentTypeCode"
            GlobalDocumentType.Description
                no label
                display as text
        header2 of "NordicUsage"
        single column
            ReferenceType
                display as required when (ReferenceTypeRequired)
            CustomerID
                when value changed
                    refresh ReferenceTypeRequired
            PaymentCategoryCode
                label is "PaymentCategory"
            PaymentCategoryCode.Description
                no label
                display as text
