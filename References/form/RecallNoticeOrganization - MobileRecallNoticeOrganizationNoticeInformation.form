MobileRecallNoticeOrganizationNoticeInformation is a Form
    title is "ViewNoticeInformation"
    Layout
        header3 of "<RecallNotice.MobileNoticeAndNameLabel>"
        display as text
            single column
                RecallNotice.WorkingId
                RecallNotice.Description
            header3 of "Details"
            single column
                RecallNotice.RecallReceivedDate
                    label is "ReceivedDate"
            two column table
                column1
                    RecallNotice.ReleasedTimestamp date
                        label is "ReleaseDate"
                    RecallNotice.FinalizedDate date
                        label is "FinalizedDate"
                column2
                    RecallNotice.DaysInPendingStatus
                        label is "No.OfDaysToRelease"
                    RecallNotice.DaysUntilFinalized
                        label is "No.OfDaysToFinalize"
            single column
                RecallNotice.NotApplicableTimestampDateOnly
                    label is "DateSetToNotApplicable"
                RecallNotice.ClosedTimestampDateOnly
                    label is "CloseDate"
                visible when (RecallGroup.RecallApprovalRequired)
                    RecallNotice.ApprovalTimestamp date
                        label is "ApprovalDate"
        single column
            button of "PrintAnalysisInformation"
                link is RecallNoticeAnalysisReport
