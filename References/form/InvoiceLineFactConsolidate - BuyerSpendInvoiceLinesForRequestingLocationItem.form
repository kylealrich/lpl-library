BuyerSpendInvoiceLinesForRequestingLocationItem is a CompositeForm
    title is "SpendAnalysisInvoiceLines"
    context form is ItemConsolidationContext
    NoContractInvoices is a Panel
        visible when (NoContract)
        list is NoContractInvoiceLineFactsByBuyerReqLocItemRel.InvoiceLineFacts
    OffContractInvoices is a Panel
        visible when (OffContract)
        list is OffContractInvoiceLineFactsByBuyerReqLocItemRel.OffContractInvoiceLineFacts
    OnContractInvoices is a Panel
        visible when (OnContract)
        list is OnContractInvoiceLineFactsByBuyerReqLocItemRel.InvoiceLineFacts
