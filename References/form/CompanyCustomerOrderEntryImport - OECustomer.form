OECustomer is a Form
    Layout
        paragraph
            DefaultCustomerShipTo
                label is "DefaultShipToLocation"
            PickingPriority
                label is "PickPriority"
        paragraph
            DefaultShippingLocation
                label is "DefaultLocation"
            ProofOfDeliveryRequired
                label is "P\\O\\DRequired"
        paragraph
            DefaultShippingMethod
                label is "DefaultMethod"
                select is ShippingMethods
            ProofOfDeliveryGracePeriod
                label is "P\\O\\DGraceDays"
        paragraph
            CustomeOrderAcknowledgments
                label is "AcknowledgementRequired"
            ShipCompleteFlag
                label is "ShipCompleteOrders_only"
        paragraph
            PORequiredForCustomerOrder
                label is "P\\ORequired"
            BackorderAllowed
                label is "AllowBackorders"
        paragraph
            CashInAdvanceRequired
                label is "Cash_inAdvance"
            CashInAdvanceRequiredPercent
                label is "Percent_ofCash_inAdvance"
        paragraph
            OrderDiscountCode
                label is "OrderDiscountCode"
        paragraph
            CreditLimitHoldCode
                select is HoldCodes
            AllowReturns
        paragraph
            OrderLimitHoldCode
                select is HoldCodes
            LastReturnAuthorizationDate
                label is "ReturnDate"
        paragraph
            CheckCreditOnOrderAdd
                label is "CreditCheckEntry"
            SingleOrderLimit
        paragraph
            CheckCreditBeforePickingList
                label is "CreditCheckPrint"
            SingleOrderHoldCode
                label is "SingleHoldCode"
                select is HoldCodes
        paragraph
            TotalCODOrderLimit
                label is "Cash_onDeliveryOrderLimit"
        paragraph
            CashOnDeliveryOrderHoldCode
                label is "Cash_onDeliveryHoldCode"
                select is HoldCodes
        paragraph
            AgeCredPcts.AgeCredPct[1]
                label is "AgeCreditPercent"
            AgeCredPcts.AgeCredPct[2]
                no label
            AgeCredPcts.AgeCredPct[3]
                no label
            AgeCredPcts.AgeCredPct[4]
                no label
            AgeCredPcts.AgeCredPct[5]
                no label
        paragraph
            OrderAgingPriodLimitHoldCode.AgeHoldCd[1]
                label is "AgeHoldCode"
            OrderAgingPriodLimitHoldCode.AgeHoldCd[2]
                no label
            OrderAgingPriodLimitHoldCode.AgeHoldCd[3]
                no label
            OrderAgingPriodLimitHoldCode.AgeHoldCd[4]
                no label
            OrderAgingPriodLimitHoldCode.AgeHoldCd[5]
                no label
