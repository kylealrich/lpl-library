SchoolProcessingCompositeForm is a CompositeForm
    title is "SchoolProcessing"
    restrict action Create
    restrict action Delete
    restrict action ClosePeriod
    restrict action ReopenLastClosedPeriod
    Actions
        action is Create
            hide when invalid
    ActivityTransfers is a Panel
        list is StudentActivityTransferRel.ActivityTransferAndDetails
    Vendors is a Panel
        list is StudentActivityVendorRel.Vendors
    ClubSponsors is a Panel
        list is StudentActivityClubSponsorRel.ClubSponsorsAndActivityReceipts
    Activities is a Panel
        list is SchoolActivityAccountGroupRel.SchoolActivities
    Students is a Panel
        list is StudentRecordRel.StudentRecordList
    Classes is a Panel
        list is StudentActivitySchoolClassRel.SchoolClasses
    BankTransactions is a Panel
        list is StudentActivityBankTransactionRel.BankTransactions
    BatchCheckProcessing is a Panel
        list is StudentActivityBankAccountRel.BatchCheckProcessingList
    BankStatements is a Panel
        list is StudentActivityBankStatementRel.StudentActivityBankStatementList
    ReportableIncomeRecords is a Panel
        list is StudentActivityBankTransactionRel.ReportableIncomeDisbursementsBySchoolList
