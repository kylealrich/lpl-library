ContractLineItemDetailItemmast is a Form
    Layout
        display as text
            two column distributed
                column1
                    header3 of "Details"
                    UNSPSCCode
                        label is "UNSPSC"
                        compact format
                    paragraph
                        MajorPurchasingClass
                            label is "PurchasingClass"
                            show up to 10 characters
                        MinorPurchasingClass
                            no label
                            show up to 10 characters
                    paragraph
                        MajorInventoryClass
                            label is "InventoryClass"
                            show up to 10 characters
                        MinorInventoryClass
                            no label
                            show up to 10 characters
                    paragraph
                        MajorPPEClass
                            label is "PPEClass"
                            show up to 10 characters
                        MinorPPEClass
                            no label
                            show up to 10 characters
                    PersonalProtectiveEquipment
                    CriticalItem
                    GenericName
                    Latex
                    paragraph
                        Sterile
                        Reprocessed
                    paragraph
                        Pharmaceutical
                        SingleUse
                    paragraph
                        Dressing
                        Reusable
                    Analytics
                column2
                    header3 of "ItemCodes"
                    Sku
                        label is "SKU"
                    Upc
                    UccEanUpn
                        select is ItemsbyUniversalProductNumber2
                    HazardCode
                column1
                    ConsignCode
                        label is "BillCode"
            header2 of "SpecialHandling"
            two column distributed
                column1
                    paragraph
                        ItemmastRel.SpecialHandlingCode
                        ItemmastRel.SpecialHandlingCode.Description
                            label is "Description"
                    visible when (ItemmastRel.SpecialHandlingCode.SpecialHandlingCodeExists)
                        ItemmastRel.SpecialHandlingInstructions
            single column
                ItemmastRel.SpecialHandlingCode.TimeSensitive
            visible when (ItemmastRel.SpecialHandlingCode.IsTimeSensitive)
                ItemmastRel.HandlingTimeInMinutes
                    label is "HandlingTimeInMinutes"
