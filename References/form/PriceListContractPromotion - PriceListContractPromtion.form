PriceListContractPromtion is a CompositeForm
    is primary
    title is "PriceListContractPromotion"
    context form is PriceListContractPromotionContextForm
        background is theme color
    PriceList is a Panel
        visible when (IsPriceList)
        form is PriceListForm
    ContractData1 is a Panel
        visible when (IsContract)
        form is ContractData1
    ContractData2 is a Panel
        visible when (IsContract)
        form is ContractData2
    Promotion is a Panel
        visible when (IsPromotion)
        form is PromotionForm
    SalesClass is a Panel
        list is PriceListSalesClassesRel.PriceListSalesClassesList
    Item is a Panel
        list is PriceListItemWithPriceListRel.PriceListItemsList
    PricingPreviewForPriceList is a Panel
        title is "PricingPreview"
        visible when (IsPriceList)
        list is PricingPreviewPriceListRel.PricingPreviewList
    PricingPreviewForPromotion is a Panel
        title is "PricingPreview"
        visible when (IsPromotion)
        list is PricingPreviewPromotionRel.PricingPreviewList
    PricingPreviewForContract is a Panel
        title is "PricingPreview"
        visible when (IsContract)
        list is PricingPreviewReContractRel.PricingPreviewList
