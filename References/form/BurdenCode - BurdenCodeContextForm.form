BurdenCodeContextForm is a Form
    Layout
        two column distributed
            column1
                paragraph
                    visible when (not CompanyEntered)
                        Company
                        Company.Name
                            no label
                            display as text
                    visible when (CompanyEntered)
                        Company.DerivedCompanyDisplay
                            label is "Company"
                            display as text
                paragraph
                    visible when (not InventoryLocationEntered)
                        InventoryLocation
                            label is "Location"
                        InventoryLocation.Name
                            no label
                            display as text
                    visible when (InventoryLocationEntered)
                        InventoryLocation.DerivedInventoryLocationIDName
                            label is "Location"
                            display as text
            column2
                paragraph
                    visible when (not BurdenCodeEntered)
                        BurdenCode
                    visible when (BurdenCodeEntered)
                        BurdenCode
                            display as text
                    Description
                Active
