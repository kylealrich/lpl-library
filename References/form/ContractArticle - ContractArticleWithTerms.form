ContractArticleWithTerms is a CompositeForm
    is primary
    title is "ContractArticle"
    context form is ArticleContext
        background is theme color
    Article is a Panel
        form is ContractArticleForm
    Terms is a Panel
        list is ContractTermsByDisplayRel.ContractTermsAndConditions
    ChangeDisplayOrder is a Panel
        visible when (HasTerms)
        list is ContractTermsByDisplayRel.ChangeDisplayOrderLevelOne
    History is a Panel
        visible when (ShowApprovalColumn)
        list is ContractArticleUpdate set.ContractArticleHistory
