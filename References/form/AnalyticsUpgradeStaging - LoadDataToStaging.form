LoadDataToStaging is a Form
    title is "LoadDataToStagingEnvironment"
    Layout
        single column
            text of "FSM_Analytics_Version"
                bold
        two column
            column1
                text of "Staging_Environment_Version_(STC)"
            column2
                last ProductVersionSTCRel.Value
                    label is "_"
        single column
            text of "_"
            visible when (Status.InProgress)
                button of "RefreshProductVersion"
                    action is UpdateVersion
            blank line
        single column
            blank line
            header3 of "LoadDataToStaging"
            blank line
            visible when (Status.InProgress)
                button of "LoadDataToStaging"
                    action is StartDataFlowFullLoad
            last DataFlowToAnalyticsFLRel.Status
                label is "Status"
                raise red alert when (last DataFlowToAnalyticsFLRel.IsFailed)
                raise yellow alert when (last DataFlowToAnalyticsFLRel.IsCanceled)
                raise blue alert when (last DataFlowToAnalyticsFLRel.IsActionPendingOrActionPendingInServer)
                    mouse over text is "ActionPending"
            button of "<last DataFlowToAnalyticsFLRel.PfiWorkunit>"
                valid when (AnalyticsUpgradeStagingRel.IsFullLoadWorkunitExists)
                link is DataAnalyticsFLWorkUnitNav
                label is "WorkUnit"
                display as link
            blank line
            valid when (Status.InProgress)
                display as text
                LoadDataToStagingIsCompleted
                    label is "Please_confirm_if_Status_of_workunit_is_completed_before_proceeding_with_the_next_steps."
