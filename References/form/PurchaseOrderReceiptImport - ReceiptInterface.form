ReceiptInterface is a CompositeForm
    is primary
    context form is ReceiptInterfaceContext
        background is theme color
    Actions
        navigation is PartiallyInterfacedReceiptNav
            action icon is preview
            label is "ViewPartiallyInterfacedReceipt"
            hide when invalid
        action is ResetErrorMessage
            action icon is finalize
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is Create
            label is "_"
        action is Update
            label is "_"
        action is save
            label is "_"
        action is save and new
            label is "_"
        action is Delete
            label is "_"
        action is refresh
            label is "_"
    Receipt is a Panel
        form is Receipt
    Lines is a Panel
        list is PurchaseOrderReceiptLineImportRel.Lines
    Invoice is a Panel
        form is Invoice
    HeaderAddOnCosts is a Panel
        list is HeaderAddOnCostsRel.HeaderAddOnCosts
    Global is a Panel
        form is Global
    InvalidLines is a Panel
        visible when (HasLinesWithInvalidRunGroup)
        list is LinesWithInvalidRunGroupRel.InvalidLines
    InvalidAddOnCosts is a Panel
        visible when (HasHeaderAddOnCostsWithInvalidRunGroup)
        list is HeaderAddOnCostsWithInvalidRunGroupRel.HeaderAddOnCosts
