InventoryReasonCodeForm is a Form
    is primary
    title is "InventoryReasonCode"
    Layout
        paragraph
            visible when (not CompanyEntered)
                Company
                Company.Name
                    no label
                    display as text
            visible when (CompanyEntered)
                Company.DerivedCompanyDisplay
                    label is "Company"
                    display as text
        single column
            visible when (not ReasonCodeEntered)
                InventoryReasonCode
            visible when (ReasonCodeEntered)
                InventoryReasonCode
                    display as text
        paragraph
            Description
            Description2
                no label
        single column
            InventoryReasonType
                label is "Type"
        paragraph
            DistributionAccount
                label is "Account"
                compact format
        single column
            Active
