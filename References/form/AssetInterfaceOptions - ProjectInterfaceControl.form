ProjectInterfaceControl is a CompositeForm
    is primary
    use for action Create
    title is "<FinanceEnterpriseGroup.ProjectLabel>InterfaceControl"
    context form is AssetInterfaceCreateContext
        background is theme color
    Main is a Panel
        form is ProjectInterfaceControlMain
    AssetDefaults is a Panel
        form is AssetDefaults
    ItemDetail is a Panel
        form is ItemDetails
    AdjustmentDetail is a Panel
        form is AdjustmentDetails
    RepairDetail is a Panel
        form is RepairDetails
    AssetInterfaceRecords is a Panel
        visible when (InterfaceRecordsExist)
        list is AssetInterfaceRel.ProjectInterfaceRecords
    AssetItemAdjustments is a Panel
        visible when (AssetItemAdjustmentImportExist)
        list is AssetItemAdjustmentImportRel.AssetItemUpdates
    AssetRepairs is a Panel
        visible when (AssetRepairRecordsExist)
        list is AssetRepairRecordsRel.InterfacingProjectRepairs
    UncapitalizedTransactions is a Panel
        visible when (DisplayOldPostingTransactions)
        list is UncapitalizedGLTransactionDetailRel.ProjectAssetGLTransactionDetailList
    SummaryUncapitalizedTransactions is a Panel
        title is "UncapitalizedTransactions"
        visible when (DisplayOldSummaryTransactions)
        list is SummaryUncapitalizedGLTransactionDetailRel.SummaryProjectAssetGLTransactionDetailList
    UncapitalizedAssetCapitalTransactions is a Panel
        title is "UncapitalizedTransactions"
        visible when (DisplayNewPostingTransactions)
        list is UncapitalizedAssetCapitalTransactionsRel.ProjectAssetCapitalTransactionList
    SummaryUncapitalizedAssetCapitalTransactions is a Panel
        title is "UncapitalizedTransactions"
        visible when (DisplayNewSummaryTransactions)
        list is SummaryUncapitalizedAssetCapitalTransactionsRel.SummaryProjectAssetCapitalTransactionList
    UncapitalizedErrorAssetCapitalTransactions is a Panel
        title is "DraftProjectInterfaceErrors"
        visible when (ErrorAssetTransactionExists)
        list is UncapitalizedErrorAssetCapitalTransactionsRel.DraftProjectInterfaceError
    SummaryErrorAssetCapitalTransactions is a Panel
        title is "DraftProjectInterfaceErrors"
        visible when (ErrorSummaryAssetTransactionExists)
        list is SummaryUncapitalizedErrorAssetCapitalTransactionsRel.DraftProjectInterfaceError
    DraftInterfaceRecords is a Panel
        visible when (PostingDraftRecordsExists)
        list is ProjectAssetInterfaceRel.ProjectAssetInterfaceList
    SummaryDraftInterfaceRecords is a Panel
        title is "DraftInterfaceRecords"
        visible when (SummaryDraftRecordsExists)
        list is ProjectAssetInterfaceRel.SummaryProjectAssetInterfaceList
    DraftInterfaceRecordErrors is a Panel
        title is "ProjectAssetInterfaceErrors"
        visible when (ErrorProjectAssetInterfaceExists)
        list is ErrorProjectAssetInterfaceRel.ProjectAssetInterfaceError
