InventoriedScheduledProcedureLineMain is a Form
    title is "Main"
    Layout
        display as text
            paragraph
                two column
                    column1
                        Item
                        Item.Description
                            label is "_"
                    column2
                        Item.Manufacturer
                            compact format
                        Item.ManufacturerNumber
            paragraph
                ComponentUOM
                ComponentUOM.Description
                    label is "_"
        header3 of "Quantities"
        three column table
            column headers
                "Open"
                "Hold"
                "Total"
            display as text
                column1
                    DerivedOpenQuantity
                        no label
                column2
                    DerivedHoldQuantity
                        no label
                column3
                    DerivedTotalQuantity
                        no label
