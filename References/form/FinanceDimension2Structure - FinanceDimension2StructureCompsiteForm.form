FinanceDimension2StructureCompsiteForm is a CompositeForm
    is primary
    title is "<FinanceEnterpriseGroup.FinanceDimension2Label>Structure_<FinanceDimension2Structure>_<Description>"
    Actions
        action is previous record
            label is "_"
            mouse over text is "Previous"
        action is next record
            label is "_"
            mouse over text is "Next"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is save
            action icon is save
            label is "_"
            mouse over text is "Save"
        action is refresh
            label is "_"
    StructurePanel is a Panel
        title is "Structure"
        form is FinanceDimension2StructureForm
    Dimensions is a Panel
        title is "Hierarchy"
        list is DimensionNode.DimensionChildrenRel.FinanceDimension2List
    DimensionUpload is a Panel
        title is "Upload"
        visible when (not UnusedDimensionStructure)
        list is FinanceDimension2Upload set.FinanceDimension2UploadForStructure
    Department is a Panel
        visible when (FinanceEnterpriseGroup.IsProjectInvoicing)
        list is FinanceDimension2DepartmentRel.FinanceDimension2Department
    Division is a Panel
        visible when (FinanceEnterpriseGroup.IsProjectInvoicing)
        list is FinanceDimension2DivisionRel.FinanceDimension2Division
