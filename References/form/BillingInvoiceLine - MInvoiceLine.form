MInvoiceLine is a CompositeForm
    title is "InvoiceLine"
    context form is MInvoiceLineInquiryContext
    restrict action Create
    restrict action Delete
    restrict action InterfaceCreate
    Actions
        action is refresh
            label is "_"
        action is save
            label is "_"
            hide when invalid
        action is previous record
            label is "_"
            hide when invalid
        action is next record
            label is "_"
            hide when invalid
    LineDetail is a Panel
        form is MDetail
    LineOptions is a Panel
        form is MLineOptions
    TaxInformation is a Panel
        form is MTaxInquiry
    InvoiceCharges is a Panel
        list is BillingInvoiceAddOnCharge set.MInvoiceLineAddOnCharges
    Comments is a Panel
        list is BillingInvoiceCommentsRel.MBillingInvoiceCommentsList
            helper list is BillingStandardCommentHeaderRel.BillingStandardCommentHeaderForCopyList
                action is CopyComments
    Attachments is a Panel
        list is BillingInvoiceLineAttachment set.MAttachments
