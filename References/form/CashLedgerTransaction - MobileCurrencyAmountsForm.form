MobileCurrencyAmountsForm is a Form
    title is "CurrencyAmounts"
    Layout
        visible when (false)
            IssuedBaseAmount.AssignToCurrency
        blank line
        two column distributed
            IssuedBaseAmount.FunctionalAmount.EnteredCurrencyAmount
                label is "Functional"
                display as text
            Company.OrigCompany.AccountingEntity.FunctionalCurrency
                no label
                display as text
        single column
            IssuedBaseAmount.FunctionalAmount.EnteredCurrencyRate
                no label
                display as text
        visible when (Company.OrigCompany.AccountingEntity.AlternateCurrencyExists)
            two column distributed
                IssuedBaseAmount.AlternateAmount.EnteredCurrencyAmount
                    label is "Alternate"
                    display as text
                Company.OrigCompany.AccountingEntity.AlternateCurrency
                    no label
                    display as text
            single column
                IssuedBaseAmount.AlternateAmount.EnteredCurrencyRate
                    no label
                    display as text
        visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency2Exists)
            two column distributed
                IssuedBaseAmount.AlternateAmount2.EnteredCurrencyAmount
                    label is "Alternate2"
                    display as text
                Company.OrigCompany.AccountingEntity.AlternateCurrency2
                    no label
                    display as text
            single column
                IssuedBaseAmount.AlternateAmount2.EnteredCurrencyRate
                    no label
                    display as text
        visible when (Company.OrigCompany.AccountingEntity.AlternateCurrency3Exists)
            two column distributed
                IssuedBaseAmount.AlternateAmount3.EnteredCurrencyAmount
                    label is "Alternate3"
                    display as text
                Company.OrigCompany.AccountingEntity.AlternateCurrency3
                    no label
                    display as text
            single column
                IssuedBaseAmount.AlternateAmount3.EnteredCurrencyRate
                    no label
                    display as text
        visible when (IssuedBaseAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
            two column distributed
                IssuedBaseAmount.ReportAmount1.EnteredCurrencyAmount
                    label is "<IssuedBaseAmount.FinanceEnterpriseGroup.ReportOne>"
                    display as text
                IssuedBaseAmount.FinanceEnterpriseGroup.ReportCurrencyOne
                    no label
                    display as text
            single column
                IssuedBaseAmount.ReportAmount1.EnteredCurrencyRate
                    no label
                    display as text
        visible when (IssuedBaseAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
            two column distributed
                IssuedBaseAmount.ReportAmount2.EnteredCurrencyAmount
