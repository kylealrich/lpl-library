BudgetGroupTotalLogForm is a Form
    is primary
    title is "BudgetGroupTotalLog"
    Layout
        display as text
            two column distributed
                column1
                    BudgetGroupTotal.GeneralLedgerCalendarPeriod
                        label is "Period"
                    BusinessClassName
                        label is "BusinessClass"
                column2
                    LogTimeStamp
                        label is "Timestamp"
                    EditContext
                        label is "Context"
            single column
                BudgetGroup.DisplayMessage
                    label is "BudgetGroup"
        header2 of "Amounts"
        display as text
            three column distributed
                PreviousAmount
                IncrementAmount
                Amount
