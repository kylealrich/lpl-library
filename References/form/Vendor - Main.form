Main is a Form
    Layout
        visible when (not RecordExists)
            single column
                VendorGroup
                Vendor
                    display as text
        two column distributed
            column1
                VendorName
                    label is "Name"
            column2
                VendorStatus
                    label is "Status"
        two column distributed
            paragraph
                VendorClass
                    when value changed
                        refresh EmailAddressRequired
                VendorClass.Description
                    no label
                    display as text
            effective date
        two column distributed
            column1
                LegalName
                VendorSearchName
                    label is "SearchName"
                VendorAccount
                    label is "AccountNumber"
                DunAndBradstreetNumber
                LegacyVendor
            column2
                Reference1
                Reference2
                GlobalLocationNumber
                    label is "G\\L\\N"
                    display as text
                visible when (DisplayCreatePurchasingVendorLink)
                    button of "AddPurchasingDetails"
                        form invoke is CreatePurchasingVendor
                        display as link
                visible when (PurchasingVendorExists)
                    paragraph
                        PurchasingVendor.ContactName
                            label is "PurchasingVendorContact"
                            display as text
                        button of "View_\\/UpdatePurchasingDetails"
                            link is PurchasingVendorNav
                            display as link
                visible when (DisplayCreateMobileSupplyChainVendorLink)
                    button of "AddMobileSupplyChainDetails"
                        form invoke is CreateMobileSupplyChainVendor
                        display as link
                visible when (MobileSupplyChainVendorExists)
                    paragraph
                        MobileSupplyChainVendor.Vendor.VendorName
                            label is "MobileSupplyChainVendor"
                            display as text
                        button of "View_\\/UpdateMobileSupplyChainDetails"
                            link is MobileSupplyChainVendorNav
                            display as link
                paragraph
                    URLAddress
                    button of icon.url
                        visible when (HasURLAddress)
                        link is external "<Vendor.URLAddress>"
        header2 of "TaxInformation"
        three column distributed
            column1
                paragraph
                    TaxIDType
                    TaxIDType.Description
                        no label
                        display as text
                paragraph
                    TaxID
                        display as required when (TaxIDRequired)
                    visible when (HasTaxID)
                        text of "TaxIDEntered"
                            italics
                    visible when (not HasTaxID)
                        text of "TaxIDNotEntered"
                            italics
                paragraph
                    TinVerified
                        label is "SubmittedForVerification"
                        display as text
                    TinNot
                        label is "SecondT\\I\\NNoticeReceived"
            column2
                ElectronicGovernmentID
                CheckDigitType
                    when value changed
                        refresh TaxIDRequired
                visible when (TaxCertificationExists)
                    single column
                        button of "ViewTaxCertification"
                            view first SupplierRel.TaxCertification.File
                            display as link
            column3
                visible when (VendorGroup.BusinessGroup.UseHSNSACCode)
                    TaxRegistrationType
                paragraph
                    VATRegistrationCountry
                        label is "V\\A\\TRegistrationCountry/\\Jurisdiction"
                        display as required when (VATRegistrationCountryRequired)
                    VATRegistrationNumber
                        when value changed
                            refresh VATRegistrationCountryRequired
                paragraph
                    button of "Validate"
                        visible when (PerformVATValidation)
                        action is ValidateVATRegistrationNumber
                        display as link
                    visible when (ValidateVAT)
                        VATValidation.VATNumberValid
                            display as text
                        VATValidation.ValidationDate
                            display as text
        two column distributed
            column1
                header2 of "GIIN_/FATCAInformation"
                paragraph
                    FATCAId
                        label is "G\\I\\I\\N_\\ID"
                    FinancialInstitutionType
                        label is "Type"
                    GIINCategoryCode
                        label is "Category"
                paragraph
                    W8Verified
                        label is "W\\8_\\Verified"
                    visible when (not TransientSelectISOCountryNumber)
                        ISOCountryNumber
                            label is "Country/\\Jurisdiction"
                    visible when (TransientSelectISOCountryNumber)
                        TransientCountry
                            label is "Country/\\Jurisdiction"
                            select is CountriesWithISONumberRel.Countries
                    TransientSelectISOCountryNumber
                        label is "SelectCountry/\\Jurisdiction"
                paragraph
                    FATCAPercentage
                        label is "F\\A\\T\\C\\A_\\Rate"
                        display as required when (W8Verified.No)
                    FATCAIncomeCode
                        label is "F\\A\\T\\C\\A_\\Code"
                        display as required when (W8Verified.No)
                FATCAAccount
                    label is "F\\A\\T\\C\\A_\\Account"
                    compact format
                    display as required when (W8Verified.No)
            column2
                header2 of "WithholdingInformation"
                paragraph
                    visible when (VendorGroup.GlobalWithholding)
                        EnrolledWithholding
                        LineLevelWithholding
                paragraph
                    IncomeWithholding
                        label is "IncomeWithholding"
                    paragraph
                        IncomeCode
                        IncomeCode.Description
                            no label
                            display as text
                paragraph
                    ExemptFromSecondaryWithholding
                        label is "SecondaryWithholdingExempt"
                    SecondaryWithholdingCode1
                        label is "SecondaryWithholdingCode1"
                    SecondaryWithholdingCode2
                        label is "Code2"
                    SecondaryWithholdingCode3
                        label is "Code3"
                paragraph
                    AllowWithholdingCodeOverride
                        label is "AllowOverrideOfCompanyIncomeWithholdingCode"
                    visible when (AllowWithholdingCodeOverride)
                        text of "Use_\\Vendor_\\Withholding_\\CodesPanelToDefineOverrideWithholdingCodes"
                            italics
        two column distributed
        single column
        two column distributed
            column1
                header2 of "Currency"
                paragraph
                    BalanceCurrency
                        label is "VendorBalance"
                    RevalueCurrency
                        label is "Revalue"
                paragraph
                    InvoiceCurrency
                        label is "Invoice"
                    DefaultVendorPaymentCurrency
            column2
                header2 of "CustomerInformation"
                paragraph
                    CustomerGroup
                        display as required when (Customer entered)
                    CustomerGroup.Name
                        no label
                        display as text
                    button of "NetBalance"
                        visible when (HasVendorBalanceRecord)
                        link is NetBalances
                        display as link
                paragraph
                    Customer
                        select is CustomerSelectList
                        display as required when (CustomerGroup entered)
                    Customer.Name
                        no label
                        display as text
                    button of "NetBalance"
                        visible when (not HasVendorBalanceRecord)
                        link is NetBalancesFromCustomer
                        display as link
        two column
            column1
                header2 of "CurrentAddress"
                visible when (not RecordExists)
                    VendorAddress
                        form is PostalAddressShortRequired
                visible when (RecordExists)
                    single column
                        display as text
                            CurrentAddressRel.PostalAddress.DisplayAddressMultiline
                                label is "PostalAddress"
                        paragraph
                            visible when (FutureAddressExists)
                                FutureAddressMessage
                                    no label
                                    display as tag
                                        color of red when (FutureAddressExists)
                            visible when (FutureAddressExists)
                                button of "ViewFutureAddress"
                                    link is VendorAddressAudit
                                    display as link
                            button of "ChangeAddress"
                                form invoke is ChangeCurrentAddress
                                display as link
        header2 of "PrimaryContact"
        visible when (ShowEnterableContactFields)
            two column distributed
                column1
                    ContactName
                    PhoneNumber
                    MobileNumber
                    FaxNumber
                    EmailAddress
                        display as required when (EmailAddressRequired)
                column2
                    single column
                        TwitterID
                        SocialNetworkID2
                        SocialNetworkID3
                        SocialNetworkID4
                        SocialNetworkID5
        visible when (not ShowEnterableContactFields)
            display as text
                three column distributed
                    column1
                        PrimeVendorContactRel.ContactName
                        paragraph
                            PrimeVendorContactRel.FormattedPhoneNumber
                            button of icon.phone
                                visible when (PrimeVendorContactRel.PhoneNumberEntered)
                                link is "tel:<PrimeVendorContactRel.PhoneNumber.SubscriberNumber>"
                        PrimeVendorContactRel.SocialNetworkID2
                        PrimeVendorContactRel.SocialNetworkID5
                    column2
                        paragraph
                            PrimeVendorContactRel.EmailAddress
                            button of icon.mail
                                visible when (PrimeVendorContactRel.EmailAddressEntered)
                                link is "mailto:<PrimeVendorContactRel.EmailAddress>"
                        paragraph
                            PrimeVendorContactRel.FormattedMobilePhoneNumber
                            button of icon.mobile
                                visible when (PrimeVendorContactRel.MobilePhoneNumberEntered)
                                link is "tel:<PrimeVendorContactRel.MobileNumber.MobilePhoneNumber.SubscriberNumber>"
                        PrimeVendorContactRel.SocialNetworkID3
                    column3
                        PrimeVendorContactRel.TwitterID
                        PrimeVendorContactRel.FormattedFaxNumber
                        PrimeVendorContactRel.SocialNetworkID4
        visible when (EmployeeEntered)
            header2 of "EmployeeInformation"
            two column
                column1
                    paragraph
                        VendorGroup.BusinessGroup.FinanceEnterpriseGroup.HROrganization
                            display as text
                        VendorGroup.BusinessGroup.FinanceEnterpriseGroup.HROrganization.Description
                            no label
                            display as text
                    paragraph
                        Employee
                            display as text
                        Employee.Name.PreferredFirstAndLastName
                            no label
                            display as text
