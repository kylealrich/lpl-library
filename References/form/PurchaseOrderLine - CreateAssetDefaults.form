CreateAssetDefaults is an ActionForm
    action is CreateAssetDefaults
    Layout
        single column
            display as text
                Company
                PurchaseOrder
                PurchaseOrderLine
            blank line
            AssetDescription
            TagNumber
                label is "Tag"
            AssetGroup
            AssetAccountingUnitGroup
            Project
            Combine
            HoldAsset
            blank line
            text of "Item"
                bold
            ItemNumber
            ItemDescription
            ModelNumber
            PurchaseDate
            InServiceDate
                label is "InServiceDate"
            blank line
            text of "Location"
                bold
            AssetLocation
                label is "Location"
            AssetDivision
                label is "Division"
            LocationDetail
                label is "LocationDetail"
                select is LocationDetail
