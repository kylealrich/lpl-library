MobilePurchaseRequestSearch is a SearchForm
    title is "SearchPurchaseRequests"
    Layout
        two column grid
            PurchaseOrderInterface.SourceDocumentOrigin
                label is "System"
            PurchaseOrderInterface.SourceDocumentNumeric
                label is "Document"
            Item
                select is MobileItemFilterSelection
            Item.Description
                label is "ItemDescription"
            ItemType
            Buyer
            Vendor
            Actor
                label is "SelectedBy"
