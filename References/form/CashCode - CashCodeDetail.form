CashCodeDetail is a Form
    title is "CashCodeDetail"
    Layout
        visible when (not RecordExists)
            paragraph
                CashManagementGroup
                CashManagementGroup.Description
                    no label
                    display as text
            three column distributed
                column1
                    CashCode
                    visible when (CashManagementGroup.RequiresApprovalCodesForTransactions)
                        paragraph
                            ApprovalCode
                            ApprovalCode.Description
                                no label
                                display as text
                column2
                    Description
                column3
                    paragraph
                        DefaultPostCompany
                        DefaultPostCompany.Name
                            no label
                            display as text
        visible when (RecordExists)
            three column distributed
                column1
                    Description
                column2
                    paragraph
                        DefaultPostCompany
                        DefaultPostCompany.Name
                            no label
                            display as text
                column3
                    visible when (CashManagementGroup.RequiresApprovalCodesForTransactions)
                        paragraph
                            ApprovalCode
                            ApprovalCode.Description
                                no label
                                display as text
        visible when (CashManagementGroup.FinanceEnterpriseGroup.FundAccounting)
            header2 of "FundAccounting"
            two column distributed
                RestrictedFunds
                EditAvailableFunds
        header2 of "BankAccount"
        three column distributed
            BankAccountNumber
                show up to 25 characters
            visible when (CurrencyEntered)
                Currency.CurrencyDescriptionID
                    label is "Currency"
                    display as text
            visible when (not CurrencyEntered)
                Currency
                    label is "Currency"
                    display as text
            Active
        display as text
            three column distributed
                FinancialInstitution
                FinancialInstitutionBranch
                SwiftID
            three column distributed
                SendingFinancialInstitution
                SendingFinancialInstitutionBranch
                    label is "SendingBranch"
                Sepa
        three column distributed
            ReturnOnInvestmentRate
            BankAccountValidationType
                label is "AccountValidationType"
                display as text
            visible when (CashManagementGroup.CashLedgerPaymentApprovalRequired)
                CancelRejectedPayments
        three column distributed
            column1
                SummarizeLedgerBalances
                AutoTransactionNumbering
                    label is "CashLedgerAutoTransactionNumbering"
                UseBankCurrencyForPaymentPosting
            column2
                NewCashLedgerPaymentForVoid
                EnforceDateEditOnVoidStopPaid
                AllowReconciliationForVoidStopPaid
            column3
                NewEFTTransactionForVoid
                StopPayBankConfirmRequired
        header2 of "BankFundTransfer"
        visible when (RecordExists)
            single column
                button of "BankFundTransfer"
                    form invoke is BankFundTransferDetails
        three column distributed
            paragraph
                BankFundTransferRel.FromBankTransactionCode.BankTransactionCodeName
                    label is "FromBankTransactionCode"
                    display as text
            paragraph
                BankFundTransferRel.ToBankTransactionCode.BankTransactionCodeName
                    label is "ToBankTransactionCode"
                    display as text
            paragraph
                BankFundTransferRel.ExpenseBankTransactionCode.BankTransactionCodeName
                    label is "ExpenseBankTransactionCode"
                    display as text
        header2 of "ReceiverBankDetails"
        single column
            ReceiverBank
            ReceiverEmailAddress
