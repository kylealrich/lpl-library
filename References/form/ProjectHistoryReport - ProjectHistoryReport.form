ProjectHistoryReport is an ActionForm
    action is ProjectHistoryReport
    Layout
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        single column
            text of "Or"
        paragraph
            CompanyGroup
                label is "CompanyGroup"
            CompanyGroup.Description
                no label
                display as text
        two column distributed
            column1
                FrPostDate
                    label is "FromPostDate"
            column2
                ToPostDate
                    label is "ToPostDate"
        two column distributed
            column1
                FrProject
                    label is "FromProject"
            column2
                ToProject
                    label is "ToProject"
        two column distributed
            column1
                paragraph
                    FrProcessLevel
                        label is "FromProcessLevel"
                        select is ProcessLevels
                    FrProcessLevel.Name
                        no label
                        display as text
            column2
                paragraph
                    ToProcessLevel
                        label is "ToProcessLevel"
                        select is ProcessLevels
                    ToProcessLevel.Name
                        no label
                        display as text
        single column
            TransType
                label is "TransactionType"
        single column
            System
        paragraph
            Customer
                select is GroupSearchName
            Customer.Name
                no label
                display as text
        paragraph
            CurrencyCd
                label is "Currency"
            CurrencyCd.Description
                no label
                display as text
        single column
            AcoCurrDisplay
                label is "CurrencyDisplay"
        two column distributed
            column1
                PrintDetail
            column2
                SortOption
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                ProjectHistoryReportReport_RDG
            column2 of 25%
                ProjectHistoryReportReport_XPT
