SupplierMigrationForm is a Form
    is primary
    title is "SupplierMigration"
    Layout
        single column
            SupplierGroup
            SupplierName
            Vendor
            TaxIdGroup
            MailingAddress
                form is PostalAddressMinimumWithType
            visible when (not SupplierGroup.SupplierExtEntityEnabled)
                LoginName
                Password
            visible when (SupplierGroup.SupplierExtEntityEnabled)
                paragraph
                    EmailAddress
                    text of "OS_External_EntitiesServiceBeingUsedFor_SupplierPortal.EmailAddressWillBeTheLoginSoItMustBeUniqueAndNotAlreadyExistInTheSystem.UserVerificationShouldNotBeEnabled."
                        bold
                        italics
                paragraph
                    Password
                    text of "UseTheSliderSwitchBelowToSeeThePasswordRequirementsFromThe_OS_Supplier_External_EntityThatMustBeMet."
                        bold
                        italics
                TransientDisplayPasswordPolicy
                    label is "Show_Password_Requirements"
                    display as switch
                visible when (TransientDisplayPasswordPolicy)
                    SupplierGroup.DerivedPasswordPolicy
                        no label
                        display as text
                LoginName
                    label is "ExternalUserIdentity"
                    display only
            MainContact.GivenName
            MainContact.FamilyName
            visible when (not SupplierGroup.SupplierExtEntityEnabled)
                EmailAddress
            TelephoneNumber.InternationalPrefix
                label is "*_PhoneNumber"
            TelephoneNumber.SubscriberNumber
                no label
            TelephoneNumber.Extension
                label is "Ext"
            FaxNumber
            MobilePhone
