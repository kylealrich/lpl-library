EFTNotiPriResContext is a Form
    Layout
        two column
            column1
                visible when (Company not entered)
                    paragraph
                        CustGroup
                            label is "CustomerGroup"
                        CustGroup.Name
                            no label
                            display as text
                    paragraph
                        ProcessingCompany
                            label is "ProcessingCompany"
                        ProcessingCompany.Name
                            no label
                            display as text
                        ProcessingCompany.Currency
                            label is "Currency"
                visible when (Company entered)
                    paragraph
                        Company
                        Company.Name
                            no label
                            display as text
                        Company.Currency
                blank line
                RunTime
                create stamp.actor
                    label is "User"
                Status
            column2
                text of "Parameters:"
                    bold
                    align as label
                paragraph
                    FinanceEnterpriseGroup
                    FinanceEnterpriseGroup.EnterpriseGroup.Description
                        no label
                        display as text
                BankTransactionCode
                    label is "PaymentCode"
                DataFileName
                    label is "DataFileName"
                IDMPrinter
        visible when (not IsEFTNotificationOutputCreated)
            text of "NoRecordsToProcess"
                bold
            blank line
