Main is a Form
    Layout
        display as text
            four column distributed
                column1
                    visible when (IsCatchWeight)
                        TransactionUOM
                    visible when (not IsCatchWeight)
                        DerivedUOM
                            label is "StockUOM"
                column2
                    KitType
            four column distributed
                column1
                    DerivedAllocatedQuantityDisplay
                    DerivedPrintedQuantityDisplay
                column2
                    DerivedDetailQuantityDisplay
                        label is "AssignedQuantity"
                    DerivedShippedQuantityDisplay
                column3
                    DerivedDetailQuantityDisplay
                    DerivedBackorderedQuantityDisplay
                column4
                    DerivedInProcessQuantityDisplay
                    DerivedQuantityDisplay
        header1 of "_"
        four column distributed
            FutureDemand
                display only
            Allocatable
                display only
            KitComplete
                display only
            OnHold
                display only
        visible when (PreferenceCardTransaction)
            four column distributed
                display as text
                    button of "<ScheduledProcedure>"
                        link is ScheduledProcedureRel.ScheduledProcedureNav
                        label is "ScheduleID"
                        display as link
                    button of "<ScheduledProcedureLine>"
                        link is ScheduledProcedureLineRel.ScheduledProcedureLineNav
                        label is "ScheduledProcedureLine"
                        display as link
                    visible when (IsDemandType)
                        PreferenceCardTransaction
                            label is "For_\\Scheduled_\\Procedure"
                    visible when (IsReservationType)
                        ReservedQuantity
                        DerivedUnreservedQuantity
        display as text
            visible when (IsHSNSACCodeEnabled)
                paragraph
                    HSNSACCode
                    HSNSACCode.Description
