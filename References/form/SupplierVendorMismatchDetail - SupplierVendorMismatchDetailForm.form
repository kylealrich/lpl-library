SupplierVendorMismatchDetailForm is a Form
    title is "VendorCompareDetails"
    Actions
        action is UpdateVendorFromSupplierDetail
            label is "UseSupplierValue"
        action is UpdateSupplierFromVendorDetail
            label is "UseVendorValue"
    Layout
        two column
            Supplier
                display as text
            Supplier.SupplierName
                display as text
        two column
            SupplierVendorMismatch.Vendor
                display as text
            SupplierVendorMismatch.Vendor.VendorName
                label is "VendorName"
                display as text
        visible when (SupplierSourceId entered)
            two column
                SupplierSourceId
                    label is "Contact"
                    display as text
                SupplierSourceId.MainContact.FirstAndLastName
                    label is "ContactName"
                    display as text
            single column
                SupplierSourceId.VendorLocation
                    display as text
        visible when (PayablesCertificationCode entered)
            two column
                PayablesCertificationCode
                    display as text
                PayablesCertificationCode.Description
                    label is "CertificationCodeDescription"
                    display as text
            single column
                CertificationEffectiveDate
                    display as text
        visible when (PayablesDiversityCode entered)
            two column
                PayablesDiversityCode
                    label is "DiversityCode"
                    display as text
                PayablesDiversityCode.Description
                    label is "DiversityCodeDescription"
                    display as text
        single column
            Type
                label is "CompareField"
                display as text
        two column
            DerivedSupplierValue
                label is "SupplierValue"
                display as text
            DerivedVendorValue
                label is "VendorValue"
                display as text
