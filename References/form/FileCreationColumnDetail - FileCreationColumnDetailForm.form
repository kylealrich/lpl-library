                            label is "End"
                        text of "Record20"
        single column
            InputOption
            Comments
        header3 of "Formatting"
        single column
            visible when (not FieldDataType.Boolean)
                JavaFormat
                    when value changed
                        refresh FormattedValueTest
        visible when (SupportsReplacement)
            paragraph
                ReplacementRegularExpression
                    when value changed
                        refresh FormattedValueTest
                ReplacementReplaceWith
                    label is "ReplaceWith"
                    when value changed
                        refresh FormattedValueTest
        visible when (DisplayFormatTest)
            paragraph
                visible when (SupportsReplacement)
                    TestValueAlpha
                        when value changed
                            refresh FormattedValueTest
                visible when (FieldDataType.Numeric)
                    TestValueNumeric
                        when value changed
                            refresh FormattedValueTest
                visible when (FieldDataType.Decimal)
                    TestValueDecimal
                        when value changed
                            refresh FormattedValueTest
                visible when (FieldDataType.Date)
                    TestValueDate
                        when value changed
                            refresh FormattedValueTest
                visible when (FieldDataType.Time)
                    TestValueTime
                        when value changed
                            refresh FormattedValueTest
                visible when (FieldDataType.TimeStamp)
                    TestValueTimeStamp
                        when value changed
                            refresh FormattedValueTest
                FormattedValueTest
        header3 of "SourceValue"
        single column
            SourceRelation
            SourceFields
            visible when (FileCreationRowDetail.OneToMany)
                OneToMany
        visible when (FieldDataType.Alpha)
            single column
                visible when (FileCreationSetup.FileType.Separated)
                    TruncateToLength
                FileName
                ConvertToUpperCase
            paragraph
                SubstringStart
                    label is "SubstringStart"
                SubstringEnd
                    label is "End"
            single column
                DefaultAlpha
        visible when (FieldDataType.Date)
            single column
                UseCurrentSystemDate
                ShowAlphaDefaultField
                visible when (not ShowAlphaDefaultField)
                    DefaultDate
                visible when (ShowAlphaDefaultField)
                    DefaultAlpha
        visible when (FieldDataType.Decimal)
            single column
                ShowAlphaDefaultField
                visible when (not ShowAlphaDefaultField)
                    DefaultDecimal
                visible when (ShowAlphaDefaultField)
                    DefaultAlpha
        visible when (FieldDataType.Numeric)
            single column
                OneToManySequenceNumber
                RecordCount1
                RecordCount2
                RecordCount3
                ShowAlphaDefaultField
                visible when (not ShowAlphaDefaultField)
                    DefaultNumeric
                visible when (ShowAlphaDefaultField)
                    DefaultAlpha
        visible when (FieldDataType.Time)
            single column
                UseCurrentSystemTime
                ShowAlphaDefaultField
                visible when (not ShowAlphaDefaultField)
                    DefaultTime
                visible when (ShowAlphaDefaultField)
                    DefaultAlpha
        visible when (FieldDataType.TimeStamp)
            single column
                UseCurrentSystemTimeStamp
                ShowAlphaDefaultField
                visible when (not ShowAlphaDefaultField)
                    DefaultTimeStamp
                visible when (ShowAlphaDefaultField)
                    DefaultAlpha
        visible when (FieldDataType.Boolean)
            single column
                ShowAlphaDefaultField
                visible when (not ShowAlphaDefaultField)
                    DefaultBoolean
                visible when (ShowAlphaDefaultField)
                    DefaultAlpha
        visible when (FieldDataType.Text)
            single column
                DefaultText
        visible when (ColumnUsesField)
            single column
                ExcludeFieldWhenDeterminingIfRowChanged
            header3 of "AlternateFieldsToIdentifyThatThisColumnChanged"
            three column
                AlternateChangedField1
                    no label
                    align as label
                AlternateChangedField2
                    no label
                AlternateChangedField3
                    no label
