FileCreationColumnDetailForm is a Form
    title is "ColumnDetail"
    Layout
        header3 of "Field_Information"
        single column
            ColumnOrder
            FieldName
            Description
            FieldDataType
        single column
            visible when (FileCreationSetup.FileType.FixedLength)
                Length
                    display as text
            visible when (not FileCreationSetup.FileType.FixedLength)
                Length
        visible when (FileCreationSetup.FileType.FixedLength)
            paragraph
                FixedLengthFieldRange.BeginPosition
                    label is "PositionBegin"
                FixedLengthFieldRange.EndPosition
                    label is "End"
                visible when (FixedLengthAndNoAdditionalRowMappings)
                    ResetColumnPositions
                visible when (OnlyOneDetailRowNoHeaderOrTrailer)
                    button of "AddAdditionalRecordMapping"
                        action is AddMappingsForAdditionalRecord
        visible when (FileCreationSetup.FileType.FixedLength)
            visible when (OnlyOneDetailRowNoHeaderOrTrailer)
                visible when (AdditionalRowMapping1Entered)
                    paragraph
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[1].BeginPosition
                            label is "Begin"
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[1].EndPosition
                            label is "End"
                        text of "Record2"
                visible when (AdditionalRowMapping2Entered)
                    paragraph
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[2].BeginPosition
                            label is "Begin"
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[2].EndPosition
                            label is "End"
                        text of "Record3"
                visible when (AdditionalRowMapping3Entered)
                    paragraph
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[3].BeginPosition
                            label is "Begin"
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[3].EndPosition
                            label is "End"
                        text of "Record4"
                visible when (AdditionalRowMapping4Entered)
                    paragraph
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[4].BeginPosition
                            label is "Begin"
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[4].EndPosition
                            label is "End"
                        text of "Record5"
                visible when (AdditionalRowMapping5Entered)
                    paragraph
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[5].BeginPosition
                            label is "Begin"
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[5].EndPosition
                            label is "End"
                        text of "Record6"
                visible when (AdditionalRowMapping6Entered)
                    paragraph
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[6].BeginPosition
                            label is "Begin"
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[6].EndPosition
                            label is "End"
                        text of "Record7"
                visible when (AdditionalRowMapping7Entered)
                    paragraph
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[7].BeginPosition
                            label is "Begin"
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[7].EndPosition
                            label is "End"
                        text of "Record8"
                visible when (AdditionalRowMapping8Entered)
                    paragraph
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[8].BeginPosition
                            label is "Begin"
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[8].EndPosition
                            label is "End"
                        text of "Record9"
                visible when (AdditionalRowMapping9Entered)
                    paragraph
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[9].BeginPosition
                            label is "Begin"
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[9].EndPosition
                            label is "End"
                        text of "Record10"
                visible when (AdditionalRowMapping10Entered)
                    paragraph
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[10].BeginPosition
                            label is "Begin"
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[10].EndPosition
                            label is "End"
                        text of "Record11"
                visible when (AdditionalRowMapping11Entered)
                    paragraph
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[11].BeginPosition
                            label is "Begin"
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[11].EndPosition
                            label is "End"
                        text of "Record12"
                visible when (AdditionalRowMapping12Entered)
                    paragraph
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[12].BeginPosition
                            label is "Begin"
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[12].EndPosition
                            label is "End"
                        text of "Record13"
                visible when (AdditionalRowMapping13Entered)
                    paragraph
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[13].BeginPosition
                            label is "Begin"
                        FixedLengthFieldRangeArray.FixedLengthFieldRange[13].EndPosition
                            label is "End"
