ConversionCustomer is a CompositeForm
    is primary
    title is "CustomerConversion"
    context form is ConversionCustomerContext
        background is theme color
    Main is a Panel
        form is Main
    Address is a Panel
        visible when (not CustomerAdded)
        form is Address
    AddressDisplayOnly is a Panel
        title is "Address"
        visible when (CustomerAdded)
        form is AddressDisplayOnly
    CashApplication is a Panel
        visible when (not CompanyCustomerAdded)
        form is CashApplication
    CashApplicationDisplayOnly is a Panel
        title is "CashApplication"
        visible when (CompanyCustomerAdded)
        form is CashApplicationDisplayOnly
    Company2 is a Panel
        visible when (not CompanyCustomerAdded)
        form is Company2
    Company2DisplayOnly is a Panel
        title is "Company2"
        visible when (CompanyCustomerAdded)
        form is Company2DisplayOnly
    Financial is a Panel
        form is Financial
    VAT is a Panel
        visible when (not CompanyCustomerAdded)
        form is VAT
    VATDisplayOnly is a Panel
        title is "VAT"
        visible when (CompanyCustomerAdded)
        form is VATDisplayOnly
    DunningAdvanced is a Panel
        visible when (not CompanyCustomerAdded)
        form is DunningAdvanced
    DunningAdvDisplayOnly is a Panel
        title is "DunningAdvanced"
        visible when (CompanyCustomerAdded)
        form is DunningAdvancedDisplayOnly
    CustomerPeriod is a Panel
        list is CompanyCustomerPeriodImportRel.primary
    BillTo is a Panel
        list is CustomerBillToImportRel.primary
    BankAccount is a Panel
        list is CompanyCustomerBankAccountImportRel.primary
    Comments is a Panel
        list is ConversionCommentRel.Comments
    Contacts is a Panel
        list is CompanyCustomerContactImportRel.primary
