Vertex90UserTemplateConfigForm is a Form
    Layout
        header1 of "ConfigurationParametersForBasic"
        visible when (VertexCalculateTax90ConfigExist)
            single column
                button of "UpdateVertexCalculateTax90ConfigurationParameter"
                    link is NavToVertexCalcTax90Parm
                    display as link
            two column
                column1
                    VertexCalculateTax90RelUTConfig.ConfigurationParameter.ConfigurationID
                    VertexCalculateTax90RelUTConfig.Type
                column2
                    VertexCalculateTax90RelUTConfig.ConfigurationParameter.Name
                    VertexCalculateTax90RelUTConfig.Length
            single column
                VertexCalculateTax90RelUTConfig.Value
                    show up to 120 characters
        visible when (not VertexCalculateTax90ConfigExist)
            single column
                text of "Warning...VertexCalculateTax90\\Configuration\\ParameterRecordDoesNotExist"
                    bold
                    color of red
                button of "CreateCalculateTax90ConfigurationParameter"
                    form invoke is Vertex90CalcTaxUserTemplateConfigParamRecord
                    display as link
        visible when (VertexLookupTaxAreas90ConfigExist)
            single column
                button of "UpdateVertexLookupTaxAreas90ConfigurationParameter"
                    link is NavToVertexLookupTaxArea90Parm
                    display as link
            two column
                column1
                    VertexLookupTaxAreas90RelUTConfig.ConfigurationParameter.ConfigurationID
                    VertexLookupTaxAreas90RelUTConfig.Type
                column2
                    VertexLookupTaxAreas90RelUTConfig.ConfigurationParameter.Name
                    VertexLookupTaxAreas90RelUTConfig.Length
            single column
                VertexLookupTaxAreas90RelUTConfig.Value
                    show up to 120 characters
        visible when (not VertexLookupTaxAreas90ConfigExist)
            single column
                text of "Warning...VertexLookupTaxAreas90\\Configuration\\ParameterRecordDoesNotExist"
                    bold
                    color of red
                button of "Create_\\Lookup_\\Tax_\\Areas90_\\ConfigurationParameter"
                    form invoke is Vertex90LookupTaxAreaUserTemplateConfigParamRecord
                    display as link
