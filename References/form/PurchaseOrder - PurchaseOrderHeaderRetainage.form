PurchaseOrderHeaderRetainage is a Form
    Layout
        header2 of "Retainage"
        single column
            RetainagePercentCompleteTracking
                label is "TrackPercentComplete"
        visible when (not RetainagePercentCompleteTracking.Contract)
            two column distributed
                column1
                    RetainageFirstPercent
                        label is "FirstRetainagePercent"
                        display as required when (RetainagePercentCompleteTracking.PurchaseOrderLevel)
                    RetainageSecondPercent
                        label is "SecondRetainagePercent"
                    RetainagePercentComplete
                        label is "PercentComplete"
                        display as text
                    RetainageOverridePercentComplete
                        label is "OverridePercentComplete"
                column2
                    RetainageUpToPercentComplete
                        label is "Up_toPercentComplete"
                        display as required when (RetainagePercentCompleteTracking.PurchaseOrderLevel)
                    RetainageDueDate
                    TotalMatchedAmount
                        label is "TotalAmountMatched"
                        display as text
                    PurchaseOrderLineTotal
                        label is "TotalPurchaseOrderAmount"
                        display as text
        visible when (HasRetainageContract)
            text of "RetainageOnContract"
