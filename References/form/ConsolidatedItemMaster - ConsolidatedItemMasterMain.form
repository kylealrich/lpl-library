ConsolidatedItemMasterMain is a Form
    Layout
        display as text
            three column
                Description
                Description2
                Description3
            two column
                paragraph
                    CommodityCode
                        label is "CommodityCode"
                        compact format
                    CommodityCode.Description
                        no label
                paragraph
                    UNSPSCCode
                        compact format
                    UNSPSCCode.LongDescription
                        no label
            two column
                paragraph
                    MajorPPEClass
                    MajorPPEClass.Description
                        no label
                paragraph
                    MinorPPEClass
                    MinorPPEClass.Description
                        no label
            two column
                paragraph
                    MajorPurchasingClass
                    MajorPurchasingClass.Description
                        no label
                paragraph
                    MinorPurchasingClass
                    MinorPurchasingClass.Description
                        no label
            two column
                paragraph
                    MajorInventoryClass
                    MajorInventoryClass.Description
                        no label
                paragraph
                    MinorInventoryClass
                    MinorInventoryClass.Description
                        no label
            paragraph
                HCPCSCode
                    label is "HCPCS_andSequence"
                    compact format
                HCPCSCode.ShortDescription
                    no label
            four column
                PatientChargeable
                Latex
                ChargeNumber
                HSNSACCode
            two column
                CriticalItem
                PersonalProtectiveEquipment
            three column
                paragraph
                    GMDNPTCode
                    GMDNPTCode.Name
                        no label
                paragraph
                    HazardCode
                    HazardCode.Description
                        no label
                visible when (ItemExists)
                    ConsignCode
            paragraph
                Sterile
                SingleUse
                Reusable
                Reprocessed
                Implantable
                ContainsHumanTissue
                Pharmaceutical
                Dressing
                Bioabsorbable
            paragraph
                DiscontinuedFlag
                InactiveFlag
                DeletedFlag
                NotForClinicalUseFlag
                ManufacturerInfoChangedFlag
            paragraph
                InternalItemChangedFlag
                DerivedOldItemNumber
                    label is "OldInternalItem"
                ClosedFlag
                OnHoldFlag
                HasErrorsFlag
                CostUpdatedFlag
            paragraph
                NewConsolidatedItemMaster
                    link is ConsolidatedItemMasterNav
                EffectiveDate
                ExpirationDate
