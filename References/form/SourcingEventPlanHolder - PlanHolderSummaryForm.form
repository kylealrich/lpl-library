PlanHolderSummaryForm is a Form
    title is "<DerivedContactName>'sPlanHolderInformation_forEvent<SourcingEvent.EventVersionString>"
    disable field options
    called out actions only
    Layout
        display as text
            paragraph
                SourcingEvent
                    label is "Event"
                    display only
                SourcingEvent.LastVersionNumber
                    no label
                    display only
                SourcingEvent.Name
                    label is "Description"
                    display only
            five column distributed
                column1
                    text of "SupplierInformation"
                        header3
                    visible when (NotifiedSupplier.SupplierSourceId.Supplier.SupplierName entered)
                        DerivedSupplierName
                            label is "Supplier"
                    DerivedFullAddressDisplay
                        label is "MailingAddress"
                column2
                    text of "_"
                    visible when (NotifiedSupplier.SupplierSourceId.Supplier.WebAddress entered)
                        button of "<DerivedWebAddress>"
                            link is "<DerivedWebAddress>"
                            display as link
                    visible when (NotifiedSupplier.SupplierSourceId.Supplier.DoingBusinessAs entered)
                        DerivedDBA
                            label is "DoingBusinessAs"
                    visible when (RemitToAddressExists)
                        DerivedRemitToAddress
                            label is "Remit_toAddress"
                column4
                    text of "ContactInformation"
                        header3
                    visible when (NotifiedSupplier.SupplierSourceId.ContactTitle entered)
                        DerivedContactTitle
                            label is "Title"
                    visible when (NotifiedSupplier.SupplierSourceId.MainContact entered)
                        DerivedContactName
                            label is "Name"
                    visible when (SourcingEvent.NotClosedNotSealed)
                        SupplierContactHasResponded
                            label is "Responded"
                    visible when (SourcingEvent.Status.Closed)
                        AwardedStatus
                            label is "Awarded"
                    visible when (SupplierContactEmailAddressExists)
                        button of "<DerivedEmailAddress>"
                            link is "mailto:<DerivedEmailAddress>"
                            display as link
                column5
                    text of "_"
                    visible when (NotifiedSupplier.SupplierSourceId.TelephoneNumber entered)
                        DerivedPhone
                            label is "Phone"
                    visible when (NotifiedSupplier.SupplierSourceId.MobilePhone entered)
                        DerivedMobile
                            label is "Mobile"
                    visible when (NotifiedSupplier.SupplierSourceId.FaxNumber entered)
                        DerivedFax
                            label is "Fax"
