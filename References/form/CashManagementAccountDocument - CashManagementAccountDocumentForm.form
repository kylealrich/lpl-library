CashManagementAccountDocumentForm is a Form
    is primary
    title is "Document"
    Layout
        paragraph
            CashManagementAccount
                display as text
            CashManagementAccount.Title
                no label
                display as text
        single column
            blank line
        single column
            Description
        paragraph
            visible when (HasAttachment)
                Attachment.Title
            Attachment.File
        single column
            create stamp
                label is "Created"
                display as text
