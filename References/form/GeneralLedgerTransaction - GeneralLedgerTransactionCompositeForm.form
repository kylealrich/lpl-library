GeneralLedgerTransactionCompositeForm is a CompositeForm
    is primary
    propagate as a drill
    title is "Transaction_<AccountingEntity>_<GeneralLedgerJournalControl.JournalType>_<GeneralLedgerJournalControl>"
    MainPanel is a Panel
        title is "Main"
        form is GeneralLedgerTransactionForm
    CommentsPanel is a Panel
        title is "Comments"
        list is GLTransactionComment set.Comment
    UserFieldsPanel is a Panel
        title is "UserFields"
        visible when (not IsACommitmentJournal)
        form is UserFieldsForm
    RelatedTransactionDetailPanel is a Panel
        title is "TransactionDetail"
        visible when (TransactionDetailExists)
        list is GLTransactionDetailExistsRel.GLTransactionDetailList
    RecurringJournalPanel is a Panel
        title is "RecurringEntry"
        visible when (RecurringJournalForm)
        form is RecurringJournalForm
    RecurringJournalBridgePanel is a Panel
        title is "RecurringJournal"
        visible when (RecurringJournalExists)
        list is RecurringJournalControlRel.RecurringJournalForBridgeList
