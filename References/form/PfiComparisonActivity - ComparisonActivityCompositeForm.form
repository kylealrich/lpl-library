ComparisonActivityCompositeForm is a CompositeForm
    title is "ComparisonActivity"
    context form is ComparisonActivityForm
    Variable is a Panel
        visible when (PfiActivity.IsStartOrAssignNode)
        list is PfiActivity.PfiActivityVariable set.ActivityVariableList
    UserActions is a Panel
        visible when (PfiActivity.IsUserActionOrHRUserAction)
        list is PfiActivity.PfiQueueRel.QueueList
