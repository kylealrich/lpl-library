ResetPasswordForm is a Form
    use for action ResetPassword
    title is "ResetPassword"
    Layout
        paragraph
            text of "PleaseOverwriteThePasswordValuesBelow,ThenPressSave"
        single column
            text of "*PleaseEnterNewPasswordAndConfirm"
                left align
                bold
                bullet
            text of "*ClickSaveToCompletePasswordReset"
                left align
                bold
                bullet
            blank line
            NewPassword
                label is "NewPassword"
            ConfirmNewPassword
                label is "ConfirmNewPassword"
