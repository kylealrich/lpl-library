BuyerCommodityCodeForm is a Form
    is primary
    title is "SupplierCommodityCode"
    Layout
        single column
            paragraph
                SupplierGroup
                SupplierGroup.Description
                    no label
                    display as text
            paragraph
                Supplier
                Supplier.SupplierName
                    no label
                    display as text
            paragraph
                SupplierSourceId
                    label is "SupplierContact"
                    display only
                SupplierSourceId.LoginName
                    no label
                    display as text
            paragraph
                CommodityCode
                    compact format
                    select is CommodityCodeSelect
            paragraph
                Approved
