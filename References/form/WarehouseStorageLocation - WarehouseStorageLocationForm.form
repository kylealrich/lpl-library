WarehouseStorageLocationForm is a CompositeForm
    is primary
    title is "<DerivedFormTitle>"
    context form is WarehouseStorageLocationFormContext
        background is theme color
    Main is a Panel
        form is MainForm
    Bins is a Panel
        valid when (IsNotStagingLocation)
        list is StorageLocationBinsRel.WarehouseStorageBinList
            helper list is BinsWithoutWarehouseStorageLocationRel.WarehouseStorageBinList
                action is AssignStorageLocationToBin
                valid when (not IsBin)
    StorageLocationItems is a Panel
        valid when (IsNotStagingLocation)
        list is ItemStorageLocationRel.StorageLocationItemList
