AssetInterfaceCreateContext is a Form
    title is "ProjectInterfaceControl"
    Layout
        visible when (not RecordExists)
            five column distributed
                column1
                    paragraph
                        AssetCompany
                        AssetCompany.GeneralLedgerCompany.Name
                            no label
                            display as text
                column2
                    paragraph
                        Project.Project
                            select is AssetInterfaceProjectSelect
                            display as required
                        Project.Project.Description
                            no label
                            display as text
                            show up to 20 characters
                column3
                    visible when (UseOldCapitalization)
                        TransactionQualifier
                    visible when (UseUpgradedCapitalization)
                        CapitalTransactionQualifier
                column4
                    visible when (Project.Project.ProjectType.Summary)
                        AssetCurrency
                            display as required
                    visible when (Project.Project.ProjectType.Posting)
                        AssetCurrency
                            display as text
                            initial value is Project.Project.Currency
                column5
                    Active
        visible when (RecordExists)
            six column distributed
                column1
                    AssetCompany.GeneralLedgerCompany.RepresentativeText
                        display as text
                column2
                    Project.Project.DerivedRepresentativeTextWebWidget
                        label is "Project"
                        display as text
                column3
                    visible when (UseOldCapitalization)
                        TransactionQualifier
                            display as text
                    visible when (UseUpgradedCapitalization)
                        CapitalTransactionQualifier
                            display as text
                column4
                    AssetCurrency
                        display as text
                column5
                    Active
                        display as tag
                            color of green when (true)
                            color of red when (false)
                column6
                    visible when (ErrorMessage entered)
                        ErrorMessage
                            label is "Error"
                            display as text
