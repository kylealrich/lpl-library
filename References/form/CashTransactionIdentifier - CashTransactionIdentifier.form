CashTransactionIdentifier is a Form
    is primary
    title is "CashTransactionIdentifier"
    Layout
        two column distributed
            column1
                paragraph
                    CashTransactionIdentifierType
                    CashTransactionIdentifierType.Description
                        no label
                        display as text
                Description
            column2
            CashTransactionIdentifier
            Active
        header2 of "ProcessingOptions"
        paragraph
            CashTransactionCategory
                select is CashTransactionCategorySelect
            CashTransactionCategory.Description
                no label
                display as text
        blank line
        header2 of "AccountingInformation"
        paragraph
            CashAllocationCode
                select is ActiveCashAllocationCodes
            CashAllocationCode.Description
                no label
                display as text
        single column
            text of "Or"
        paragraph
            IdentifierAccount
                compact format
