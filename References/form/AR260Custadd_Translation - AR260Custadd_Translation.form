AR260Custadd_Translation is a Form
    is primary
    Layout
        paragraph
            Base_BusinessClass.UniqueID
                display as text
        single column
            IsoLocale
                compact format
                raise yellow alert when (VerifyTranslations)
                    mouse over text is untranslatable:"The base data has been changed but this translation has not been verified"
        single column
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.Name
                label is untranslatable:"Name"
            Name
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.AnlystName
                label is untranslatable:"Anlyst Name"
            AnlystName
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.SalesrepName
                label is untranslatable:"Salesrep Name"
            SalesrepName
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.MajClassName
                label is untranslatable:"Maj Class Name"
            MajClassName
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.MinClassName
                label is untranslatable:"Min Class Name"
            MinClassName
                label is untranslatable:"Translated"
