ProjectFundingCreateForm is a Form
    title is "ExternalGrant"
    Layout
        visible when (InEnterpriseStructure)
            two column distributed
                column1
                    FundingType
                        label is "Type"
            two column distributed
                column1
                    paragraph
                        DateRange.Begin
                            label is "BeginDate"
                        DateRange.End
                            label is "EndDate"
            two column distributed
                column1
                    paragraph
                        Amount
                        Currency
            visible when (FundingType.InternalFund)
                single column
                    OffsetAccount
                        compact format
            visible when (FundingType.IntercompanyBilling)
                single column
                    paragraph
                        CompanyCustomerGroup.ReceivableCompany
                            label is "PayablesCompany"
                        CompanyCustomerGroup.ReceivableCompany.Name
                            display as text
                    OffsetAccount
                        label is "PayablesExpenseFinanceStructure"
                        compact format
            two column distributed
                column1
                    RevenueCodeBlockOption
                column2
                    RevenueRecognitionOption
            two column distributed
                column1
                    RevenuePartialAccount
                        compact format
                column2
                    RevenueFullAccount
                        compact format
            visible when (FundingType.ExternalGrant)
                header2 of "Dates"
                single column
                    paragraph
                        EstimateCloseDate
                        ActualDate
                        SubmitDate
                        NoticeDate
                        ReceiveDate
                        ProposalDate
            two column distributed
                column1
                    paragraph
                        Department
                        Department.Description
                            no label
                            display as text
            two column distributed
                column1
                    paragraph
                        Division
                        Division.Description
                            no label
                            display as text
        visible when (not InEnterpriseStructure)
            two column distributed
                column1
                    FundingType
                        label is "Type"
                        display as text
            two column distributed
                column1
                    paragraph
                        Amount
                            display as text
                        Currency
                            display as text
            visible when (FundingType.InternalFund)
                single column
                    OffsetAccount
                        display as text
                        compact format
            visible when (FundingType.IntercompanyBilling)
                single column
                    paragraph
                        CompanyCustomerGroup.ReceivableCompany
                            label is "PayablesCompany"
                            display as text
                        CompanyCustomerGroup.ReceivableCompany.Name
                            display as text
                    OffsetAccount
                        label is "PayablesExpenseFinanceStructure"
                        display as text
                        compact format
            two column distributed
                column1
                    RevenueCodeBlockOption
                        display as text
                column2
                    RevenueRecognitionOption
                        display as text
            two column distributed
                column1
                    RevenuePartialAccount
                        display as text
                        compact format
                column2
                    RevenueFullAccount
                        display as text
                        compact format
            visible when (FundingType.ExternalGrant)
                header2 of "Dates"
                single column
                    paragraph
                        EstimateCloseDate
                            display as text
                        ActualDate
                            display as text
                        SubmitDate
                            display as text
                        NoticeDate
                            display as text
                        ReceiveDate
                            display as text
                        ProposalDate
                            display as text
            two column distributed
                column1
                    paragraph
                        Department
                            display as text
                        Department.Description
                            no label
                            display as text
            two column distributed
                column1
                    paragraph
                        Division
                            display as text
                        Division.Description
                            no label
                            display as text
