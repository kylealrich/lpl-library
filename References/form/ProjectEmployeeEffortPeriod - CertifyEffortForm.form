CertifyEffortForm is a CompositeForm
    title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Employee"
    context form is ProjectEmployeeEffortPeriodContext
        background is theme color
    Actions
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is save
            label is "_"
            mouse over text is "Save"
        action is refresh
            label is "_"
            mouse over text is "Refresh"
    EffortCertification is a Panel
        list is ProjectAssignmentEffortRel.CertifyProjectEffortList
    IncludedLaborPeriods is a Panel
        visible when (ProcessedProjectEmployeeLaborPeriodRelExists)
        list is ProcessedProjectEmployeeLaborPeriodRel.ProcessedProjectSchedulePeriodList
