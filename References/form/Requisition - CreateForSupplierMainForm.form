CreateForSupplierMainForm is a Form
    title is "CreateRequisition"
    disable field options
    Layout
        paragraph
            Requester
                label is "Reviewer"
                select is SupplierRequesters
            Requester.RequesterFirstLastName
                no label
                display as text
            text of "<DerivedDefaultRequester>"
        paragraph
            Company
                select is SupplierCompanyList
            Company.Name
                no label
                display as text
            text of "<DerivedDefaultCompany>"
        paragraph
            RequestingLocation
                select is SupplierRequestingLocations
            RequestingLocation.Name
                no label
                display as text
            text of "<DerivedDefaultRequestingLocation>"
        paragraph
            TransientAttachment.File
                label is "Attachment"
            TransientCommentTitle
                label is "AttachmentTitle"
        paragraph
            RequisitionDescription
