RecallNoticeSearchForm is a SearchForm
    title is "NoticeSearch"
    Layout
        four column distributed
            column1
                RecallSearchDocument
                    use text area of 1 lines
            column2
                RecallNotice
            column3
                WorkingId
            column4
                Status
        four column distributed
            column1
                Name
            column2
                Description
                    use text area of 1 lines
            column3
                RecallType
                    label is "Type"
            column4
                RecallCoordinator
                    select is RecallCoordinatorList
        four column distributed
            column1
                ManufacturerName
                    label is "ManufacturerName"
            column2
                ManufacturerCodeDivision
                    label is "ManufacturerCode"
                    compact format
            column3
                ManufacturerGln
            column4
                ProductsUpdatedAfterQueryRunDate
                    label is "RerunProductTransactionQuery"
        four column distributed
            column1
                RecallReceivedDate
                    label is "ReceivedDate"
            column2
                HasRecallQandA
                    label is "HasQAndA"
            column3
                paragraph
                    AlertUnresolvedQuantities
                        label is "HasUnresolvedQuantities"
                    Equipment
                        label is "Equipment"
            column4
                paragraph
                    HasPastDueTasks
                        label is "NoticesWithPastDueTasksOrReviews"
