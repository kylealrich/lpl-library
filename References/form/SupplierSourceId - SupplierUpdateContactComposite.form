SupplierUpdateContactComposite is a CompositeForm
    title is "<MainContact.FirstAndLastName>'sProfile_with<Supplier.SupplierName>"
    show panel control on left
    Actions
        action is refresh
            label is "_"
    ContactSummary is a Panel
        form is ContactProfileContext
    CommodityCodes is a Panel
        list is SupplierCommodityCode set.SupplierCommodityCodeUpdate
            helper list is CommodityCodes.CommodityCodeHelper
                action is AttachToSupplierContact
    Certifications is a Panel
        valid when (SupplierGroup.AllowCertificationSelect)
        list is SupplierCertificationRel.SupplierSideContactCertifications
    BankInformation is a Panel
        visible when (CanDisplayBankInformation)
        form is BankInformationForm
    ProxyNotifications is a Panel
        visible when (UsesSupplierPortal)
        list is SupplierContactProxy set.SupplierContactProxyList
