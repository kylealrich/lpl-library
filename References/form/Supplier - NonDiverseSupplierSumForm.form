NonDiverseSupplierSumForm is a SummaryForm
    title is "_"
    Layout
        visible when (SupplyManagementReportContext.SupplierDetail)
            form is NonDiverseSupplierRel.SourcingEventNotificationSumForm
        header5 of "TotalsForSupplier\\:_<Supplier.SupplierName>"
        three column
            column1
                DerivedNonDiverseTotalEventsBid
                    label is "TotalEventsBid"
                    display when blank
                DerivedNonDiverseTotalEventsAwarded
                    label is "TotalEventsAwarded"
                    display when blank
                DerivedEventsNonDiverseBidPercent
                    label is "EventBidSuccess"
                    display when blank
            column2
                DerivedNonDiverseTotalEventLinesBid
                    label is "TotalLinesBid"
                    display when blank
                DerivedNonDiverseTotalEventLinesAwarded
                    label is "TotalLinesAwarded"
                    display when blank
                DerivedNonDiverseEventLinesBidPercent
                    label is "LineBidSuccess"
                    display when blank
            column3
                DerivedNonDiverseBidTotal
                    label is "BidTotal"
                    display when blank
                DerivedNonDiverseAwardedTotal
                    label is "TotalAwardedAmount"
                    display when blank
                DerivedNonDiverseTotalBidPercent
                    label is "AmountBidSuccess"
                    display when blank
