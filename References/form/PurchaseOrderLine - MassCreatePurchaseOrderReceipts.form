    action is MassCreatePurchaseOrderReceipts
    Layout
        single column
            RunGroup
            CompleteReceiptProcessing
            visible when (CompleteReceiptProcessing)
                ReleaseReceipts
                visible when (ReleaseReceipts)
                    ReleaseOption
            PrmCompany
                label is "Company"
        two column
            column1
                Vendor1
                    label is "Vendor1"
            column2
                Vendor2
                    label is "Vendor2"
        two column
            column1 of 27%
                PurchaseFrom1
                    label is "PurchaseFromLocation1"
                    show up to 10 characters
            column2 of 73%
                PurchaseFrom2
                    label is "PurchaseFromLocation2"
                    show up to 10 characters
        single column
            PrmVendorClass
                label is "VendorClass"
        two column
            column1 of 27%
                POCode1
                    label is "POCode"
            column2 of 73%
                POCode2
                    no label
        paragraph
            BillCode1
                label is "BillCode"
                show up to 10 characters
            text of "_"
            BillCode2
                no label
                show up to 10 characters
            text of "_"
            BillCode3
                no label
                show up to 10 characters
            text of "_"
            BillCode4
                no label
                show up to 10 characters
        single column
            PrmShipTo
                label is "ShipTo"
        two column
            column1 of 27%
                FromDeliveryDate
            column2 of 73%
                ToDeliveryDate
        two column
            column1 of 27%
                FromPODate
            column2 of 73%
                ToPODate
