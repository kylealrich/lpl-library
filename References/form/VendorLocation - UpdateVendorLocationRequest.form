UpdateVendorLocationRequest is an ActionForm
    action is RequestUpdateVendorLocation
    title is "RequestVendorLocationUpdate"
    Layout
        section of Main
            visible when (actor.initiator)
                header3 of "This_request_will_be_routed_for_approval;_after_approval_the_Vendor_Location_will_be_updated."
            visible when (not actor.initiator)
                display as text
                    two column
                        column1 of 50%
                            RequestedBy
                            button of "<RequesterEmail>"
                                link is "mailto:<RequesterEmail>"
                                label is "Email"
                                display as link
                            NeededBy
                                raise red alert when (NeededByRedBoolean)
                                    mouse over text is "Overdue"
                                raise yellow alert when (NeededByYellowBoolean)
                                    mouse over text is "Needed_\\Today"
                                raise blue alert when (NeededByGreenBoolean)
                                    mouse over text is "Due_\\Within_<NoOfDays>_\\Days"
                        column2 of 50%
                            RequesterActor
                                label is "ID"
                            button of "<RequesterPhone.SubscriberNumber>"
                                link is "tel:<RequesterPhone.SubscriberNumber>"
                                label is "Phone"
                                display as link
                            RequestPriority
                                label is "Priority"
                                display as tag
                                    color of red when (RequestPriority.High)
                                    color of yellow when (RequestPriority.Medium)
                                    color of green when (RequestPriority.Low)
            visible when (actor.initiator)
                paragraph distributed
                    NeededBy
                    RequestPriority
                        label is "Priority"
            single column
                Notes
            two column distributed
                column1
                    Vendor
                        label is "Vendor"
                column2
                    VendorLocation
                        label is "Location"
            two column distributed
                column1
                    VendorName
                        label is "Name"
                    LocationType
                    paragraph
                        RemitToCode
                        RemitToCode.VendorName
                            no label
                            display as text
                    VendorAccount
                    paragraph
                        VATRegistrationCountry
                            label is "V\\A\\TRegistrationCountry/\\Jurisdiction"
                            display as required when (VATRegistrationNumber entered)
                        VATRegistrationNumber
                            when value changed
                                refresh VATRegistrationCountry
                    paragraph
                        InvoiceCurrency
                            label is "InvoiceCurrency"
                        RevalueCurrency
                            label is "Revalue"
                column2
                    Status
                    paragraph
                        effective date
                        visible when (DisplayCreatePurchaseFromLocationLink)
                            button of "AddPurchasingDetailsToLocation"
                                form invoke is CreatePurchaseFromLocation
                                display as link
                        visible when (PurchaseFromLocationExists)
                            button of "View/\\UpdatePurchasingDetails"
                                link is PurchaseFromLocationNav
                                display as link
                    NormalRemitToLocation
                    NormalPurchaseFromLocation
                    GlobalLocationNumber
                        display as text
                    visible when (VendorGroup.BusinessGroup.UseHSNSACCode)
                        TaxRegistrationType
                    TransientAttachment.File
                        label is "Attachment"
                    TransientAttachmentDescription
                        label is "AttachmentDescription"
            header2 of "CurrentAddress"
            RequestAddressChange
                label is "RequestAddressChange"
            visible when (RequestAddressChange)
                VendorAddress
                    form is PostalAddressShortRequired
            header2 of "PrimaryContact"
            two column
                column1
                    ContactName
                    PhoneNumber
                    MobileNumber
                    FaxNumber
                    EmailAddress
                column2
                    TwitterID
                    SocialNetworkID2
                    SocialNetworkID3
                    SocialNetworkID4
                    SocialNetworkID5
        section of Banking
            header2 of "VendorBank"
            two column distributed
                column1 of 60%
                    paragraph
                        BankEntity
                            label is "BankEntity"
                        BankEntity.BankName
                            no label
                            display as text
                    paragraph
                        BankAccountNumber
                            label is "AccountNumber"
                        DerivedAccountNumber
                            label is "Entered"
                            display as text
                            mouse over text is "AccessToView_\\Account_\\NumberMayBeRestricted"
                        visible when (BankAccountNumberEntered)
                            ClearBankAccountNumber
                                label is "Clear"
                                mouse over text is "CheckToClearThe_\\Account_\\NumberField"
                    paragraph
                        ACHPrenotification
                            label is "PrenotificationStatus"
                        paragraph
                            BankCurrency
                            BankCurrency.Description
                                no label
                                display as text
                column2 of 40%
                    BankAccountValidationType
                        label is "ValidationType"
                    RIBKey
                        label is "R\\I\\BKey"
                    paragraph
                        BankAccountType
                            label is "AccountType"
                        GlobalBankAccountType
            header2 of "VendorCrossBorderPaymentOptions"
            two column distributed
                column1 of 70%
                    paragraph
                        CrossBorderIdentifier
                            label is "CrossBorder,BICNumber"
                        paragraph
                            SwiftID
                                label is "B\\I\\C"
                            DerivedSwiftID
                                label is "Entered"
                                display as text
                                mouse over text is "AccessToView_\\B\\I\\CMayBeRestricted"
                            visible when (SwiftIDEntered)
                                ClearSwiftID
                                    label is "Clear"
                                    mouse over text is "CheckToClearThe_\\B\\I\\CField"
                    paragraph
                        NormalOrExpressCode
                            label is "PriorityCode"
                        ChargeCode
                column2 of 30%
                    PaymentCategoryCode
                        label is "PaymentCategory"
                    paragraph
                        PaymentForm
                        DebitingSign
            two column distributed
                column1 of 60%
                    header3 of "Sweden_andDenmarkPaymentData"
                    paragraph
                        BankingCode
                            label is "BankCode"
                        BankID
                column2 of 40%
                    header3 of "FinlandPaymentData"
                    AssignmentNumber
            header2 of "IntermediaryBank"
            two column distributed
                column1 of 60%
                    paragraph
                        IntermediaryBankEntity
                            label is "BankEntity"
                        IntermediaryBankEntity.BankName
                            no label
                            display as text
                    paragraph
                        IntermediaryBankAccount
                            label is "AccountNumber"
                        DerivedIntermediaryBankAccount
                            label is "Entered"
                            display as text
                            mouse over text is "AccessToView_\\Account_\\NumberMayBeRestricted"
                        visible when (IntermediaryBankAccountEntered)
                            ClearIntermediaryBankAccount
                                label is "Clear"
                                mouse over text is "CheckToClearThe_\\Account_\\NumberField"
                    IntermediaryPrenotification
                        label is "PrenotificationStatus"
                column2 of 40%
                    IntermediaryBankAccountValidationType
                        label is "ValidationType"
                    IntermediaryRIBKey
                        label is "R\\I\\BKey"
                    paragraph
                        IntermediaryBankCurrency
                            label is "BankCurrency"
                        IntermediaryBankCurrency.Description
                            no label
                            display as text
            header2 of "IntermediaryCrossBorderPaymentOptions"
            two column distributed
                column1 of 70%
                    paragraph
                        IntermediaryCrossBorderIdentifier
                            label is "CrossBorder,BICNumber"
                        paragraph
                            IntermediarySwiftID
                                label is "B\\I\\C"
                            DerivedIntermediarySwiftID
                                label is "Entered"
                                display as text
                                mouse over text is "AccessToView_\\B\\I\\CMayBeRestricted"
                            visible when (IntermediarySwiftIDEntered)
                                ClearSwiftID
                                    label is "Clear"
                                    mouse over text is "CheckToClearThe_\\B\\I\\CField"
                    IntermediaryNormalOrExpressCode
                        label is "PriorityCode"
                    IntermediaryChargeCode
                        label is "ChargeCode"
                column2 of 30%
                    IntermediaryPaymentCategory
                        label is "PaymentCategory"
                    IntermediaryPaymentForm
                        label is "PaymentForm"
                    IntermediaryDebitingSign
                        label is "DebitingSign"
            two column distributed
                column1 of 60%
                    header3 of "SwedenAndDenmarkPaymentData"
                    paragraph
                        IntermediaryBankingCode
                            label is "BankCode"
                        IntermediaryBankID
                            label is "BankID"
                column2 of 40%
                    header3 of "FinlandPaymentData"
                    IntermediaryAssignmentNumber
                        label is "AssignmentNumber"
            three column distributed
                column1 of 60%
                    header2 of "OtherAccounts"
                    paragraph
                        GiroNumber
                        DerivedGiroNumber
                            label is "Entered"
                            display as text
                            mouse over text is "AccessToView_\\Giro_\\NumberMayBeRestricted"
                        visible when (GiroNumberEntered)
                            ClearGiroNumber
                                label is "Clear"
                                mouse over text is "CheckToClearThe_\\Giro_\\NumberField"
                    paragraph
                        CreditCardNumber
                            label is "CreditCardNumber"
                        DerivedCreditCardNumber
                            label is "Entered"
                            display as text
                            mouse over text is "AccessToView_\\CreditCardNumberMayBeRestricted"
                        visible when (CreditCardNumberEntered)
                            ClearCreditCardNumber
                                label is "Clear"
                                mouse over text is "CheckToClearThe_\\CreditCardNumberField"
                    BACSName
                        label is "B\\A\\C\\SName"
                    BACSReference
                        label is "B\\A\\C\\SReference"
                column2 of 20%
                    header2 of "BankInstructions"
                    BankInstruction1
                        label is "1"
                    BankInstruction2
                        label is "2"
                    BankInstruction3
                        label is "3"
                    BankInstruction4
                        label is "4"
                column3 of 20%
                    header2 of "BankCodes"
                    visible when (not DisplayBankCodes)
                        text of "BankCodeLabelDefinitionsRequiredOn_\\Vendor_\\Group"
                            bold
                    visible when (DisplayBankCodes)
                        visible when (VendorBankCode1Entered)
                            first VendorBankRel.VendorBankCode1
                        visible when (VendorBankCode2Entered)
                            first VendorBankRel.VendorBankCode2
                        visible when (VendorBankCode3Entered)
                            first VendorBankRel.VendorBankCode3
                        visible when (VendorBankCode4Entered)
                            first VendorBankRel.VendorBankCode4
                        visible when (VendorBankCode5Entered)
                            first VendorBankRel.VendorBankCode5
                        visible when (VendorBankCode6Entered)
                            first VendorBankRel.VendorBankCode6
                        visible when (VendorBankCode7Entered)
                            first VendorBankRel.VendorBankCode7
                        visible when (VendorBankCode8Entered)
                            first VendorBankRel.VendorBankCode8
                        visible when (VendorBankCode9Entered)
                            first VendorBankRel.VendorBankCode9
                        visible when (VendorBankCode10Entered)
                            first VendorBankRel.VendorBankCode10
                        visible when (VendorBankCode11Entered)
                            first VendorBankRel.VendorBankCode11
                        visible when (VendorBankCode12Entered)
                            first VendorBankRel.VendorBankCode12
                        visible when (VendorBankCode13Entered)
                            first VendorBankRel.VendorBankCode13
                        visible when (VendorBankCode14Entered)
                            first VendorBankRel.VendorBankCode14
                        visible when (VendorBankCode15Entered)
                            first VendorBankRel.VendorBankCode15
                        visible when (VendorBankCode16Entered)
                            first VendorBankRel.VendorBankCode16
                        visible when (VendorBankCode17Entered)
                            first VendorBankRel.VendorBankCode17
                        visible when (VendorBankCode18Entered)
                            first VendorBankRel.VendorBankCode18
                        visible when (VendorBankCode19Entered)
                            first VendorBankRel.VendorBankCode19
                        visible when (VendorBankCode20Entered)
                            first VendorBankRel.VendorBankCode20
                        visible when (CreateVendorBankRecord)
                            button of "BankCodes"
                                form invoke is CreateVendorBankCodes
                                is primary
                        visible when (MaintainVendorBankRecord)
                            button of "BankCodes"
                                link is VendorBankCodes
                                is primary
            header2 of "BankAddress"
            display as text
                single column
                    BankEntity.PostalAddress
                        form is PostalAddressMinimum
        section of Options
            header2 of "Invoice"
            paragraph
                ValidateCertificationDate
                    label is "ValidateCertificationDate"
                InvoiceRoutingCategory
                InvoiceRoutingCategory.Description
                    no label
                    display as text
            header2 of "Payment"
            two column distributed
                paragraph
                    TermsCode
                        label is "PaymentTerms"
                    TermsCode.Desc1
                        no label
                        display as text
                paragraph
                    InvoiceGroup
            two column distributed
                paragraph
                    CashCode
                    CashCode.Description
                        no label
                        display as text
                paragraph
                    DiscountCode
                    DiscountCode.Description
                        no label
                        display as text
            two column distributed
                paragraph
                    BankTransactionCode
                        label is "PaymentCode"
                    BankTransactionCode.Description
                        no label
                        display as text
                paragraph
                    PayImmediately
                        label is "PayImmediately"
                    SeparatePayment
                        label is "SeparatePayment"
            header2 of "Codes"
            two column distributed
                column1
                    paragraph
                        TaxCode
                            select is TaxCodeFilteredList
                        TaxCode.Description
                            no label
                            display as text
                        TaxUsageCode
                column2
                    paragraph
                        visible when (not Vendor.ValidateDiversityDates)
                            DiversityCode
                                label is "DefaultDiversityCode"
                            DerivedDiveristyDescription
                                no label
                                display as text
                        visible when (Vendor.ValidateDiversityDates)
                            DiversityCode
                                label is "DefaultDiversityCode"
                                display as text
                            DerivedDiveristyDescription
                                no label
                                display as text
                        visible when (Vendor.ValidateDiversityDates)
                            visible when (DiversityCode entered)
                                button of "ChangeDiversityCode"
                                    link is DefaultDiversityCodes
                                    display as link
                            visible when (DiversityCode not entered)
                                button of "SetDefaultDiversityCode"
                                    link is DefaultDiversityCodes
                                    display as link
            header2 of "Returns"
            four column distributed
                ReplaceGoods
                    label is "ReplaceGoods"
                ShipOrHold
                    label is "ShipOrHoldGoods"
                WriteOffAmount
                    label is "MinimumClaimAmount"
                paragraph
                    VendorClaimType
                    visible when (VendorClaimType.Chargeback)
                        VendorClaimHoldCode
                            label is "VendorClaimsChargebackHold"
            two column distributed
                column1
                    header2 of "ProcurementCard"
                    paragraph
                        UseProcurementCard
                            label is "UseProcurementCard"
                        visible when (UseProcurementCard)
                            MaskedProcurementCard
                                display as text
                            button of "Add_\\/UpdateProcurementCard"
                                action is UpdateProcurementCard
                                display as link
                column2
                    header2 of "E\\D\\I"
                    paragraph
                        EDINumber
                        ERSCapable
                            label is "ERSCapable"
                        EDIAutoRelease
                            label is "E\\D\\IInvoiceAutomaticRelease"
                        visible when (ERSCapable)
                            paragraph
                                InvoiceReference
                                    label is "E\\R\\SInvoiceReference"
            header2 of "Global"
            two column distributed
                column1
                    paragraph
                        ExternalAccountingEntity
                column2
                    paragraph
                        ExternalLocation
        section of Comments
            three column
                column1
                    PrintCode
                    Title
                column2
                    DateRange.Begin
                        label is "BeginDate"
                    DateRange.End
                        label is "EndDate"
                column3
                    paragraph
                        TransPayablesCompany
                            label is "Company"
                        TransPayablesCompany.Company.Name
                            no label
                            display as text
                    paragraph
                        PayGroup
                        PayGroup.Name
                            no label
                            display as text
            single column
                CommentText
                    label is "Comment"
                    use text area of 12 lines
