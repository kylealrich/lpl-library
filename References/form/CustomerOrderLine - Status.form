Status is a Form
    restrict action CreateCustomerOrderReturnLine
    Layout
        two column
            column1
                paragraph
                    MajorSalesClass
                        show up to 10 characters
                paragraph
                    BookedQuantity
                paragraph
                    ShippedQuantity
                paragraph
                    InvoiceQuantity
                paragraph
                    CancelQuantity
                paragraph
                    CancelledDate
                        label is "CancelDate"
                blank line
                paragraph
                    DisplayAllocatedQuantity
                        label is "AllocatedQuantity"
                        display as text
                paragraph
                    DisplayPrintedQuantity
                        label is "PrintedQuantity"
                        display as text
                paragraph
                    DisplayInProcessQuantity
                        label is "In\\-\\ProcessQuantity"
                        display as text
                paragraph
                    DisplayBackorderedQuantity
                        label is "BackkQuantity"
                        display as text
            column2
                paragraph
                    MinorSalesClass
                        show up to 10 characters
                paragraph
                    BookedDate
                paragraph
                    ShipComplete
                        label is "ShipComplete"
                paragraph
                    ReturnedQuantity
                paragraph
                    CancelledOperator
                        label is "Canceled_by"
                paragraph
                    CancelledReason
                        label is "CancelReason"
                blank line
