Main is a Form
    Layout
        two column distributed
            visible when (not RecordExists)
                column1
                    paragraph
                        CashCode
                            when value changed
                                refresh AutoNumberingAllowed
                        CashCode.Description
                            no label
                            display as text
                        CashCode.Currency
                            no label
                            display as text
                column2
                    paragraph
                        Status
                            display as text
                    visible when (AutoNumberingAllowed)
                        CashLedgerFundTransferHeader
                            label is "TransferNumber"
                            display as text
                            select is UnreleasedTransfers
                    visible when (not AutoNumberingAllowed)
                        CashLedgerFundTransferHeader
                            label is "TransferNumber"
                            select is UnreleasedTransfers
        two column distributed
            column1
                paragraph
                    Company.OrigCompany
                        label is "Company"
                        select is Company
                        when value changed
                            refresh AutoNumberingAllowed
                    Company.OrigCompany.Name
                        no label
                        display as text
                    Company.OrigCompany.Currency
                        no label
                        display as text
                paragraph
                    CurrencyTable
                    CurrencyTable.Description
                        no label
                        display as text
                visible when (not TransferType.CashCodeFundTransfer)
                    paragraph
                        TransferBankAmountUnsigned
                            label is "Amount"
                        CashCode.Currency
                            no label
                            display as text
                visible when (ShowHeaderAccountingUnit)
                    paragraph
                        TransientAccountingUnit
                        TransientAccountingUnit.Description
                            no label
                            display as text
                visible when (ShowHeaderFund)
                    paragraph
                        HeaderFund
                        HeaderFund.Description
                            no label
                            display as text
                Description
                TransferDate
                PostDate
