SourcingEventLineListingSumForm is a SummaryForm
    title is "_"
    Layout
        paragraph
            SourcingEventLine
                label is "EventLine"
                display when blank
            Name
                label is "LineName"
                display when blank
        Status
            label is "Status"
            display when blank
        list is CurrentSourcingEventLineRel.SourcingEventLineDetailListing
        four column
            column1
                ManufacturerInformation.Manufacturer
                    label is "ManufacturerCode"
                ItemNumber
                    label is "Item"
                CommodityCode
                GlobalTradeItemNumber
                    label is "GTIN"
            column2
                ManufacturerInformation.Manufacturer.ManufacturerDivision
                    label is "Division"
            column3
                ManufacturerInformation.ManufacturerNumber
                    label is "Number"
                ItemDescription
                    label is "Description_1"
            column4
                blank line
                ItemDescription2
                    label is "Description_2"
        form is SourcingEventLineResponsesRel.EventLineResponseListSumForm
