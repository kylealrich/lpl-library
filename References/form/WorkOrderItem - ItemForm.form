ItemForm is a Form
    Layout
        paragraph
            Item
                show up to 15 characters
            Item.Description
                no label
                display as text
        paragraph
            Location
                show up to 15 characters
            Location.Name
                no label
                display as text
        paragraph
            ComponentType
                label is "Type"
        paragraph
            PlanQuantity
                label is "PlannedQuantity"
            DerivedUOM
                no label
                display as text
        visible when (WorkOrder.ProductionFeedbackStatus.InProcess)
            paragraph
                ActualQuantity
            paragraph
                ActualCatchWeightQuantity
            paragraph
                WasteQuantity
            paragraph
                WasteCatchWeightQuantity
        paragraph
            visible when (Item.IsCatchWeightItem)
                CatchWeightQuantity
                Item.StockUOM
                    no label
                    display as text
        paragraph
            DerivedQuantity
                label is "AllocatedQuantity"
                display as text
        paragraph
            WasteAccount
                label is "Waste"
                compact format
        paragraph
            MaterialExpenseAccount
                label is "MaterialExpense"
                compact format
