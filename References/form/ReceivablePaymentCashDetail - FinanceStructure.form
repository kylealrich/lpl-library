FinanceStructure is a Form
    title is "FinanceStructure"
    Layout
        single column
            CashFinanceCodeBlock
                label is "CashFinanceStructure"
                compact format
            visible when (ReceivablePayment.DetailRNADistributions)
                visible when (Status.Unreleased)
                    display as text
                        RNAFinanceCodeBlock
                            label is "RNAFinanceStructure"
                            compact format
                        AdditionalDimensionsCodeBlock
                            label is "MatchingFinanceStructureForApplications"
                            compact format
                visible when (Status.Released)
                    RNAFinanceCodeBlock
                        label is "RNAFinanceStructure"
                        compact format
                    AdditionalDimensionsCodeBlock
                        label is "MatchingFinanceStructureForApplications"
                        compact format
