UpdateFilters is an ActionForm
    action is UpdateFilters
    title is "UpdateFilters"
    Layout
        paragraph
            HistoryFlag
            visible when (HistoryFlag.IncludeHistory)
                text of "Includes_Open,_Unreleased,And_Historical."
            visible when (HistoryFlag.OnlyShowHistory)
                text of "Includes_Only_Historical.Excludes_OpenAnd_Unreleased."
            visible when (HistoryFlag.NoHistory)
                text of "Includes_Open_TransactionsAnd_OpenAnd_Unreleased_Payments.Excludes_Historical."
            visible when (HistoryFlag not entered)
                text of "DefaultViewIncludes_Open_TransactionsAnd_OpenAnd_Unreleased_Payments.Excludes_Historical."
        paragraph
            PrmReceivableProcessLevel
                label is "ProcessLevel"
            visible when (PrmReceivableProcessLevel entered)
                PrmReceivableProcessLevel.Name
                    no label
            Currency
        two column table
            column headers
                "From"
                "To"
            row headers
                "Project"
                "DueDate"
                "TransactionDate"
                "OpenAmount"
                "TransactionAmount"
                "TransUser1"
                "PurchaseOrder"
                "CrossReference"
            column1
                PrmProject.FromProject
                    no label
                DueDateRange.Begin
                    no label
                TransactionDateRange.Begin
                    no label
                OpenAmountRange.FromAmount
                    no label
                TransactionAmountRange.FromAmount
                    no label
                TransUser1Range.FromTransUser1
                    no label
                CustomerPurchaseOrderRange.FromCustomerPurchaseOrder
                    no label
                XrefNbrRange.FromInvoice
                    no label
            column2
                PrmProject.ToProject
                    no label
                DueDateRange.End
                    no label
                TransactionDateRange.End
                    no label
                OpenAmountRange.ToAmount
                    no label
                TransactionAmountRange.ToAmount
                    no label
                TransUser1Range.ToTransUser1
                    no label
                CustomerPurchaseOrderRange.ToCustomerPurchaseOrder
                    no label
                XrefNbrRange.ToInvoice
                    no label
