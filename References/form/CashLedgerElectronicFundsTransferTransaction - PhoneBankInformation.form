PhoneBankInformation is a Form
    title is "BankInformation"
    Layout
        two column table
            column1
                BankEntity
                    label is "BankIdentification"
                    select is Banks
                    show up to 10 characters
                BankName
                    show up to 10 characters
                CrossBorderIdentifier
                    show up to 10 characters
            column2
                BankAccountNumber
                    label is "AccountNumber"
                    show up to 10 characters
                paragraph
                    BankAccountType
                        label is "AccountType"
                        show up to 10 characters
                    GlobalBankAccountType
                BankAccountValidationType
                    label is "AccountValidation"
                    show up to 10 characters
        paragraph
            SwiftID
        header3 of "BankAddress"
        paragraph
            visible when (ElectronicPaymentCode entered)
                ElectronicPaymentCode
        two column table
            column1
                FirstPartyReference
            column2
                SecondPartyReference
        paragraph
            BankPostalAddress
                form is PostalAddressMinimumWithType
