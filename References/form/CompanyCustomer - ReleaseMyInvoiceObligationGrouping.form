ReleaseMyInvoiceObligationGrouping is an ActionForm
    action is ReleaseMyInvoiceObligationGrouping
    Layout
        paragraph
            NewCompany
            NewCustomer
                select is CustomerContextCompanyCustomerSelectList
        single column
            NewInvoice
            NewReceivableProcessLevel
            NewReceivableReason
                select is ReceivableReasonCodeGroupingList
            NewGeneralLedgerDate
                label is "NewGlobalLedgerDate"
            NewTermsCode
            NewDueDate
            NewBankTransactionCode
            AdditionalAmount
            AdditionalAmountAccount
                compact format
            visible when (Company.CashOperatorRequired)
                NewOperator
            NewRevalueCurrencyTransactions
                label is "NewRevalueCurrencyTransactions"
                    left of field
            NewBillTo
