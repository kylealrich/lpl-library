MeetingView is a Form
    title is "<Meeting>Details_forEvent<SourcingEvent.EventVersionString>"
    disable field options
    called out actions only
    Layout
        display as text
            single column
                Description
                    use text area
            paragraph
                visible when (MeetingDateEntered)
                    MeetingDate
                        label is "Date"
                visible when (LocationNameEntered)
                    LocationName
                        label is "Location"
                visible when (MeetingAddressEntered)
                    PostalAddress.DisplayAddressMultiline
                        label is "Address"
                visible when (CallInNumberEntered)
                    CallInNumber
                        label is "CallInNumber"
                visible when (MeetingIDEntered)
                    MeetingID
                        label is "MeetingID"
                visible when (MeetingURLEntered)
                    button of "MeetingURL"
                        link is external "<MeetingURL>"
                        label is "MeetingLink"
                        display as link
                visible when (MeetingPasswordEntered)
                    MeetingPassword
                        label is "MeetingPassword"
