RemittanceSelectionSearch is a SearchForm
    Layout
        four column distributed
            CompanyRange.FromCompany
                label is "FromCompany"
            CompanyRange.ToCompany
                label is "ToCompany"
            CustomerMajorClass
                label is "MajorClass"
            CustomerMinorClass
                label is "MinorClass"
        four column distributed
            DraftNumberRange.FromDraftNumber
                label is "FromDraftNumber"
            DraftNumberRange.ToDraftNumber
                label is "ToDraftNumber"
            PostalCodeRange.FromPostalCode
                label is "FromPostalCode"
            PostalCodeRange.ToPostalCode
                label is "ToPostalCode"
        four column distributed
            MaturityDateRange.Begin
                label is "FromMaturityDate"
            MaturityDateRange.End
                label is "ToMaturityDate"
            DraftDateRange.Begin
                label is "FromDraftDate"
            DraftDateRange.End
                label is "ToDraftDate"
        four column distributed
            TransactionAmountRange.FromAmount
                label is "FromTransactionAmount"
            TransactionAmountRange.ToAmount
                label is "ToTransactionAmount"
