CreateFromInvoice is an ActionForm
    action is CreateFromInvoice
        on completion navigate to PayablesInvoicePrimary
    Layout
        paragraph
            PrmCompany
                label is "Company"
        paragraph
            PrmPayablesInvoice
                label is "InvoiceToCopy"
                select is InvoiceCopySelectList
        paragraph
            PrmPayablesInvoice.Invoice
                display as text
        paragraph
            PrmInvoiceNumber
                label is "NewInvoiceNumber"
        two column
            column1
                PrmPayablesInvoice.InvoiceDate
                    label is "OriginalInvoiceDate"
                    display as text
                PrmPayablesInvoice.DueDate
                    label is "OriginalDueDate"
                    display as text
                PrmPayablesInvoice.DistributionDate
                    label is "OriginalDistributionDate"
                    display as text
                PrmPayablesInvoice.ReceiptOfInvoiceDate
                    label is "OriginalReceiptOfInvoiceDate"
                    display as text
            column2
                PrmInvoiceDate
                    label is "NewInvoiceDate"
                PrmDueDate
                    label is "NewDueDate"
                PrmDistributionDate
                    label is "NewDistributionDate"
                PrmInvoiceReceiptDate
                    label is "NewReceiptOfInvoiceDate"
