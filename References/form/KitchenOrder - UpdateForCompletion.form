UpdateForCompletion is a CompositeForm
    use for action Preparing.Update
    title is "KitchenOrder<KitchenOrder>"
    called out actions only
    show panel navigation
        last panel navigation is ViewKitchenOrder
            next button title is "Done"
    Actions
        action is Preparing.CopyPlannedQuantities
            action icon is copy
            label is "CopyPlannedQuantities"
            hide when invalid
        action is save
            label is "_"
            hide when invalid
        action is refresh
            label is "_"
    KitchenOrder is a Panel
        form is ActualsForm
    KitchenOrderLines is a Panel
        list is KitchenOrderLine set.ActualAndWastesList
    Details is a Panel
        visible when (MultipleDetailsAllowed)
        list is KitchenOrderDetail set.RecipeItemDetailList
    Summary is a Panel
        form is PreparationSummary
