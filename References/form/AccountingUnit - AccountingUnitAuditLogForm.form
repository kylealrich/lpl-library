AccountingUnitAuditLogForm is a Form
    title is "AccountingUnitAuditDetail"
    Layout
        single column
            action
            applied stamp
            actor
        blank line
        blank line
        visible when (ActionCreate)
            single column
                text of "NewValues"
                    header2
                FinanceEnterpriseGroup
                AccountingEntity
                    label is "<AccountingEntity.AccountingEntityLabel>"
                AccountingUnitType
                AccountingUnit
                DisplayAccountingUnit
                Description
                effective date
                Active
                BudgetIdentifier
                AddressCode
                Level
                IsTopNode
                ParentAccountingUnit
                DimensionStructure
        visible when (ActionUpdate)
            two column
                column1
                    text of "NewValues"
                        header2
                    visible when (FinanceEnterpriseGroupUpdate)
                        FinanceEnterpriseGroup
                    visible when (AccountingEntityUpdate)
                        AccountingEntity
                            label is "<AccountingEntity.AccountingEntityLabel>"
                    visible when (AccountingUnitTypeUpdate)
                        AccountingUnitType
                    visible when (AccountingUnitUpdate)
                        AccountingUnit
                    visible when (DisplayAccountingUnitUpdate)
                        DisplayAccountingUnit
                    visible when (DescriptionUpdate)
                        Description
                    visible when (ActiveUpdate)
                        Active
                    visible when (BudgetIdentifierUpdate)
                        BudgetIdentifier
                    visible when (AddressCodeUpdate)
                        AddressCode
                    visible when (SequenceNumberUpdate)
                        SequenceNumber
                    visible when (LevelUpdate)
                        Level
                    visible when (IsTopNodeUpdate)
                        IsTopNode
                    visible when (ParentAccountingUnitUpdate)
                        ParentAccountingUnit
                    visible when (DimensionStructureUpdate)
                        DimensionStructure
                column2
                    text of "OldValues"
                        header2
                    visible when (FinanceEnterpriseGroupUpdate)
                        old FinanceEnterpriseGroup
                    visible when (AccountingEntityUpdate)
                        old AccountingEntity
                            label is "<AccountingEntity.AccountingEntityLabel>"
                    visible when (AccountingUnitTypeUpdate)
                        old AccountingUnitType
                    visible when (AccountingUnitUpdate)
                        old AccountingUnit
                    visible when (DisplayAccountingUnitUpdate)
                        old DisplayAccountingUnit
                    visible when (DescriptionUpdate)
                        old Description
                    visible when (ActiveUpdate)
                        old Active
                    visible when (BudgetIdentifierUpdate)
                        old BudgetIdentifier
                    visible when (AddressCodeUpdate)
                        old AddressCode
                    visible when (SequenceNumberUpdate)
                        old SequenceNumber
                    visible when (LevelUpdate)
                        old Level
                    visible when (IsTopNodeUpdate)
                        old IsTopNode
                    visible when (ParentAccountingUnitUpdate)
                        old ParentAccountingUnit
                    visible when (DimensionStructureUpdate)
                        old DimensionStructure
        visible when (ActionDelete)
            single column
                text of "DeletedValues"
                    header2
                FinanceEnterpriseGroup
                AccountingEntity
                    label is "<AccountingEntity.AccountingEntityLabel>"
                AccountingUnitType
                AccountingUnit
                DisplayAccountingUnit
                Description
                Active
                BudgetIdentifier
                AddressCode
                Level
                IsTopNode
                ParentAccountingUnit
                DimensionStructure
