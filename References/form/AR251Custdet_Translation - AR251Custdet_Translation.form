                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.SalesRepresentativeName
                label is untranslatable:"Sales Representative Name"
            SalesRepresentativeName
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.ProcessLevelName
                label is untranslatable:"Process Level Name"
            ProcessLevelName
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
