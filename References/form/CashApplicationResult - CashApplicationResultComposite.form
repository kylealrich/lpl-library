CashApplicationResultComposite is a CompositeForm
    is primary
    propagate as a drill
    title is "<DerivedFormTitle>"
    context form is CashApplicationResultContext
        background is theme color
    restrict action DeleteAllResults
    restrict action Run
    PaymentApplications is a Panel
        list is PaymentApplicationRel.CashApplicationList
    CreditApplications is a Panel
        list is CreditApplicationRel.CashApplicationList
    Remits is a Panel
        list is ReceivablePaymentRemittanceRel.CashApplicationList
    Totals is a Panel
        list is ReceivableApplicationRel.CashApplicationTotals
    Exceptions is a Panel
        visible when (ErrorRecordsExists)
        list is CashApplicationErrorResultRel.CashApplicationErrorResultList
