ProjectFundingSourceChangeRequestForm is a Form
    is primary
    Layout
        two column distributed
            column1
                paragraph
                    FinanceDimension2.DisplayDimension
                        label is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
                        display as text
                    FinanceDimension2.Description
                        label is "Description"
                        display as text
        two column distributed
            column1
                paragraph
                    ChangeRequestDate
                    Description
        two column distributed
            column1
                paragraph
                    ProjectFundingSource.Priority
                        display as text
                    NewPriority
            column2
                paragraph
                    ProjectFundingSource.FundingGroup
                        display as text
                    NewFundingGroup
        two column distributed
            column1
                paragraph
                    ProjectFundingSource.Percent
                        display as text
                    NewPercent
            column2
                paragraph
                    ProjectFundingSource.FundedAmount
                        display as text
                    NewFundedAmount
        two column distributed
            column1
                paragraph
                    ProjectContractChangeRequest
                        select is ProjectContractChangeRequestSelectList
                        show up to 5 characters
                    ProjectContractChangeRequest.Description
                        no label
                        display as text
                        show up to 30 characters
        two column distributed
            column1
                paragraph
                    Status
                        display as text
                    RequestedBy
                        display as text
                    ApprovedBy
                        display as text
        two column distributed
            column1
                Comment
            column2
                DocumentURL
        single column
            paragraph
                Attachment
        single column
            paragraph
                ErrorMessage
                    display as text
