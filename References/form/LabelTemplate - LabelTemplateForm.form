LabelTemplateForm is a Form
    is primary
    title is "LabelTemplate"
    Actions
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is save and new
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
            hide when invalid
        action is refresh
            label is "_"
    Layout
        single column
            valid when (not RecordExists)
                LabelType
                    when value changed
                        refresh IsWarehouseShipmentDistributionValid
                visible when (IsWarehouseShipmentDistributionValid)
                    text of "<DistributionManagementNotEnabledWarningMessage>" with icon.info-alert
                        color of red
            LabelTemplate
            valid when (not PreDefined)
                Description
            paragraph
                Default
                visible when (Default)
                    OverrideDefault
            PreDefined
                display only
            valid when (not PreDefined)
                TransientTemplate
                    use text area of 30 lines
