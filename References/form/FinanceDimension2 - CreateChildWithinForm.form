CreateChildWithinForm is an ActionForm
    action is CreateChildWithin
    title is "CreateChild<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>For<DisplayDimension>"
    Layout
        two column
            column1
                NewDimensionType
                    label is "DimensionType"
                NewDimension
                    label is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
            column2
                blank line
                NewDescription
                    label is "Description"
        single column
            visible when (NewDimensionType.Posting)
                NewActive
                    label is "Active"
            visible when (NewDimensionType.Summary)
                NewBudgetIdentifier
                    label is "BudgetIdentifier"
            NewAddressCode
                label is "AddressCode"
            effective date
