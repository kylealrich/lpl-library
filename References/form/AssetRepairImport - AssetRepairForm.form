AssetRepairForm is a Form
    Layout
        paragraph
            Company
            Company.GeneralLedgerCompany.Name
                no label
                display as text
        paragraph
            InvoiceAndPurchaseOrderInformation.Vendor
                select is CompanyVendorsRel.VendorSelect
            button of "CompanyInvoices"
                visible when (IsInvoice)
                link is CompanyInvoices
        paragraph
            InvoiceAndPurchaseOrderInformation.PayablesInvoice
                select is PayablesInvoicesRel.ReleasedVendorInvoices
