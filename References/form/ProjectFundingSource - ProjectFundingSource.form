ProjectFundingSource is a CompositeForm
    is primary
    title is "<FinanceEnterpriseGroup.FinanceDimension2Label>_<FinanceDimension2.DisplayDimension>_<FinanceDimension2.Description>"
    context form is ProjectFundingSourceContext
        background is theme color
    Main is a Panel
        form is ProjectFundingSourceMainForm
    Totals is a Panel
        form is ProjectFundingSourceTotalsForm
    BillingMilestones is a Panel
        visible when (IsMilestoneBilling)
        list is ProjectContractMilestoneBillingRel.ProjectContractMilestoneList
    BillingInstallments is a Panel
        visible when (ProjectContract.BillingMethod.Installment)
        list is ProjectContractMilestoneBillingRel.ProjectContractInstallmentList
    OverrideCombinedBillingMilestones is a Panel
        title is "BillingMilestones"
        visible when (OverrideCombinedBillingMilestonesExist)
        list is ProjectContractMilestoneBillingRel.ProjectContractOverrideMilestoneList
    OverrideSeparateBillingMilestones is a Panel
        title is "BillingMilestones"
        visible when (OverrideSeparateBillingMilestonesExist)
        list is ProjectContractMilestoneBillingRel.ProjectContractMilestoneList
    RevenueMilestones is a Panel
        visible when (ProjectContract.IsMilestoneRevenue)
        list is ProjectContractMilestoneRevenueRel.ProjectContractMilestoneList
    RevenueInstallments is a Panel
        visible when (ProjectContract.RevenueMethod.Installment)
        list is ProjectContractMilestoneRevenueRel.ProjectContractInstallmentList
    OverrideRevenueMilestones is a Panel
        title is "RevenueMilestones"
        visible when (OverrideRevenueMilestonesExist)
        list is ProjectContractMilestoneRevenueRel.ProjectContractOverrideMilestoneList
    Retainers is a Panel
        visible when (CanHaveRetainers)
        list is ProjectContractRetainer set.ProjectContractRetainerList
    RetainerApplication is a Panel
        visible when (ProcessedRetainersExist)
        list is ProcessedRetainersRel.ProcessedRetainerList
    AdvanceBillingList is a Panel
        visible when (ProjectContractAdvanceRelExists)
        list is ProjectContractAdvanceRel.UnappliedProjectContractAdvanceList
    LTDBalances is a Panel
        visible when (ProjectContractInvoiceBalanceSetExists)
        list is ProjectContractInvoiceBalance set.ProjectFundingSourceInvoiceBalances
    BalanceAdjustments is a Panel
        list is ProjectContractBalanceAdjustment set.ProjectFundingSourceBalanceAdjustments
    Comments is a Panel
        list is ProjectFundingSourceComment set.ProjectCommentForAFundingSourceList
    SelectedProjectList is a Panel
        visible when (SelectedProjectsOnly)
        list is FundingSourceSelectedProject set.FundingSourceSelectedProjectList
            helper list is ProjectContract.PostingProjectsRel.ContractEligiblePostingProjectSelect
                action is IncludeSelectedProject
                    invoked.PrmProjectContract = ProjectContract
                    invoked.PrmFinanceDimension2 = FinanceDimension2
