MobilePaymentDistributionForm is a Form
    title is "Distribution"
    Layout
        header2 of "Ledger"
        paragraph
            GLTransactionAmount
                label is "Amount"
                display as text
        paragraph
            GLFinanceCodeBlock
                label is "FinanceStructure"
                display as text
                compact format only
        paragraph
            Reference
                display as text
        paragraph
            Description
                display as text
        header2 of "Tax"
        two column distributed
        display as text
            TaxCode
            TaxAccountingEntity
                label is "TaxEntity"
        paragraph
            TaxableBankAmount
                label is "TaxableAmount"
