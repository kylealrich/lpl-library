AssetTypeAllocationDetail is a Form
    is primary
    title is "AssetAllocationDetail"
    Layout
        two column distributed
            paragraph
                Company
                    select is Companies
                Company.GeneralLedgerCompany.Name
                    no label
                    display as text
            paragraph
                AssetType
                    label is "AssetType"
                    compact format
                AssetType.Description
                    no label
                    display as text
        paragraph
            AssetTypeAllocation.StartDate
                label is "Date"
        paragraph
            AccountingUnit
                compact format
        paragraph
            AllocationPercent
