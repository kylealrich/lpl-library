SupplierPortalContact is a Form
    is primary
    title is "Contact"
    Actions
        action is save
            label is "_"
        action is save and new
            label is "_"
        action is save and close
    Layout
        paragraph
            visible when (PortalContactExists)
                Contact
                    display as text
                ContactName
                    no label
                    display as text
            visible when (not PortalContactExists)
                Contact
                ContactName
                    no label
                    display as text
        two column
            paragraph
                Type
                Type.Description
                    no label
                    display as text
            DoNotDisplayOnPortal
        paragraph
            text of "MessageDialogDefaults:"
                bold
            DefaultMessageDialogContact
                label is "Overall"
            PODefaultDialogContact
                label is "PurchaseOrderRelated"
            APDefaultDialogContact
                label is "PayablesRelated"
        visible when (Contact entered)
            header3 of "ContactDetails"
            display as text
                single column
                    ContactEmail
                        label is "Email"
                    ContactPhone
                        label is "Phone"
