IntercompanyBillingForm is a Form
    Layout
        header2 of "DefaultsForBillingDocuments"
        three column
            column1
                paragraph
                    ICBillingDefaultProcessLevel
                        label is "ProcessLevel"
                        display as required when (ICBillingDefaultProcessLevelRequired)
                        when value changed
                            refresh ICBillingDefaultProcessLevelRequired
                            refresh ICBillingDefaultVendorRequired
                            refresh ICBillingDefaultCashCodeRequired
                            refresh ICBillingDefaultBankTransactionCodeRequired
                            refresh ICBillingDefaultBankFeeCodeRequired
                            refresh ICBillingReverseBankFeeCodeRequired
                    ICBillingDefaultProcessLevel.Name
                        no label
                        display as text
                paragraph
                    ICBillingDefaultVendor
                        label is "Vendor"
                        display as required when (ICBillingDefaultVendorRequired)
                        when value changed
                            refresh ICBillingDefaultProcessLevelRequired
                            refresh ICBillingDefaultVendorRequired
                            refresh ICBillingDefaultCashCodeRequired
                            refresh ICBillingDefaultBankTransactionCodeRequired
                            refresh ICBillingDefaultBankFeeCodeRequired
                            refresh ICBillingReverseBankFeeCodeRequired
                    ICBillingDefaultVendor.VendorName
                        no label
                        display as text
            column2
                paragraph
                    DerivedICBVendorCashCode
                        label is "CashCode"
                        display as text
                paragraph
                    DerivedICBVendorBankTransactionCode
                        label is "BankTransactionCode"
                        display as text
            column3
                InterfaceUnreleasedInvoices
                paragraph
                    ApprovalCode
                        select is ApprovalCodesWithLevels
                        show up to 10 characters
                    ApprovalCode.Description
                        no label
                        display as text
        blank line
        header2 of "IDMEmailOptions"
        paragraph
            IntercompanyBillingInvoiceBillToEmailAddress
        blank line
        header2 of "DefaultsForSettlements"
        paragraph
            ICBillingDefaultCashCode
                label is "CashCode"
                display as required when (ICBillingDefaultCashCodeRequired)
                when value changed
                    refresh ICBillingDefaultProcessLevelRequired
                    refresh ICBillingDefaultVendorRequired
                    refresh ICBillingDefaultCashCodeRequired
                    refresh ICBillingDefaultBankTransactionCodeRequired
                    refresh ICBillingDefaultBankFeeCodeRequired
                    refresh ICBillingReverseBankFeeCodeRequired
            ICBillingDefaultCashCode.Description
                no label
                display as text
        paragraph
            ICBillingDefaultBankTransactionCode
                label is "BankTransactionCode"
                display as required when (ICBillingDefaultBankTransactionCodeRequired)
                when value changed
                    refresh ICBillingDefaultProcessLevelRequired
                    refresh ICBillingDefaultVendorRequired
                    refresh ICBillingDefaultCashCodeRequired
                    refresh ICBillingDefaultBankTransactionCodeRequired
                    refresh ICBillingDefaultBankFeeCodeRequired
                    refresh ICBillingReverseBankFeeCodeRequired
            ICBillingDefaultBankTransactionCode.Description
                no label
                display as text
        paragraph
            ICBillingDefaultBankFeeCode
                label is "BankFeeCode"
                display as required when (ICBillingDefaultBankFeeCodeRequired)
                when value changed
                    refresh ICBillingDefaultProcessLevelRequired
                    refresh ICBillingDefaultVendorRequired
                    refresh ICBillingDefaultCashCodeRequired
                    refresh ICBillingDefaultBankTransactionCodeRequired
                    refresh ICBillingDefaultBankFeeCodeRequired
                    refresh ICBillingReverseBankFeeCodeRequired
            ICBillingDefaultBankFeeCode.Description
                no label
                display as text
        paragraph
            ICBillingReverseBankFeeCode
                label is "ReverseBankFeeCode"
                display as required when (ICBillingReverseBankFeeCodeRequired)
                when value changed
                    refresh ICBillingDefaultProcessLevelRequired
                    refresh ICBillingDefaultVendorRequired
                    refresh ICBillingDefaultCashCodeRequired
                    refresh ICBillingDefaultBankTransactionCodeRequired
                    refresh ICBillingDefaultBankFeeCodeRequired
                    refresh ICBillingReverseBankFeeCodeRequired
            ICBillingReverseBankFeeCode.Description
                no label
                display as text
