StudentActivityBankTransactionForm is a CompositeForm
    is primary
    title is "<BankTransactionType>"
    show panel control on left
    Actions
        action is Entered.CreateAdjustment
            action icon is create
        action is Entered.CreateDeposit
            action icon is create
        action is Entered.CreateDisbursement
            action icon is create
        action is Entered.CreateNSFRecord
            action icon is create
        action is Entered.CreateTransfer
            action icon is create
        action is Entered.Release
            action icon is release
            hide when invalid
        action is save
        action is save and new
        action is UpdateReportableIncomeFields
            action icon is edit
            hide when invalid
        action is Released.VoidTransaction
            hide when invalid
        action is Entered.Delete
            action icon is delete
            hide when invalid
        action is next record
        action is previous record
        action is refresh
    BankTransaction is a Panel
        form is StudentActivityBankTransactionContextForm
    TransactionDetail is a Panel
        visible when (BankTransactionDetailsAllowed)
        list is StudentActivityBankTransDetailRel.StudentActivityBankTransDetailList
    AssignedUnreleasedDepositReceipts is a Panel
        title is "AssignedReceipts"
        visible when (UnreleasedDeposit)
        list is StudentActivityReceipt set.StudentActivityReceiptSelectList
            helper list is UnassignedReceiptsRel.ActivityReceipts
                action is Released.AssignReceipt
                    invoked.PrmBankAccount = StudentActivityBankAccount
                    invoked.PrmDepositNumber = StudentActivityBankTransaction
    AssignedReceiptBatches is a Panel
        visible when (UnreleasedDeposit)
        list is StudentActivityReceiptBatch set.BatchSelectedForDepositList
            helper list is UnassignedBatchReceiptsRel.BatchSelectList
                action is Released.AssignBatchToDeposit
                    invoked.PrmBankAccount = StudentActivityBankAccount
                    invoked.PrmDepositNumber = StudentActivityBankTransaction
    AssignedReleasedDepositReceipts is a Panel
        title is "AssignedReceipts"
        visible when (ReleasedDeposit)
        list is StudentActivityReceipt set.StudentActivityReceiptInquireList
    Vendor is a Panel
        visible when (BankTransactionType.Disbursement)
        form is Vendor
    VendorInvoices is a Panel
        visible when (BankTransactionType.Disbursement)
        list is StudentActivityVendorInvoice set.StudentActivityVendorInvoiceList
    TransferToBankAccount is a Panel
        visible when (TransferHeader)
        list is TransferToBankTransactionRel.StudentActivityTransferToBankList
    TransferFromBankAccount is a Panel
        visible when (TransferDetail)
        list is TransferFromBankTransactionRel.StudentActivityTransferToBankList
