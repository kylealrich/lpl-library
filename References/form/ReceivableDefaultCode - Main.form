Main is a Form
    Layout
        header2 of "ProcessingCodes"
        two column distributed
            column1
                paragraph
                    CreditAnalyst
                        select is CreditAnalyst
                    CreditAnalyst.Name
                        no label
                        display as text
                paragraph
                    TermsCode
                        select is TermsCodes
                    TermsCode.Description.Desc[1]
                        no label
                        display as text
                paragraph
                    CustomerMajorClass
                        label is "MajorClass"
                        select is MajorClass
                    CustomerMajorClass.Description
                        no label
                        display as text
                paragraph
                    AgingCode
                        select is AgingCodes
                    AgingCode.Description
                        no label
                        display as text
                paragraph
                    ReceivableLockBox
                        label is "RemitLockbox"
                        select is ReceivableLockBoxRel.ReceivableLockBoxList
                    ReceivableLockBox.Name
                        no label
                        display as text
                paragraph
                    LanguageCode
                        label is "Locale"
                        compact format
            column2
                paragraph
                    SalesRepresentative
                        select is SalesReps
                    SalesRepresentative.Name
                        no label
                        display as text
                paragraph
                    DebitCreditMemoTermsCode
                        label is "MemoPaymentTerms"
                        select is TermsCodes
                    DebitCreditMemoTermsCode.Description.Desc[1]
                        no label
                        display as text
                paragraph
                    CustomerMinorClass
                        label is "MinorClass"
                        select is MinorClass
                    CustomerMinorClass.Description
                        no label
                        display as text
                paragraph
                    BankTransactionCode
                        label is "PaymentCode"
                    BankTransactionCode.Description
                        no label
                        display as text
                paragraph
                    ReceivableGeneralLedgerCode
                        label is "AccountsReceivableCode"
                        select is ARCodes
                    ReceivableGeneralLedgerCode.Description
                        no label
                        display as text
                paragraph
                    TaxCode
                        select is TaxCodes
                    TaxCode.Description
                        no label
                        display as text
                    TaxExempt
                        label is "PreventTaxCodeDefaulting"
        header2 of "ProcessingRules"
        two column
            column1
                paragraph
                    AutomaticDunning
                        label is "AutomaticDunningCycle"
                    DunningCycle
                        no label
                        select is DunningCycle
                paragraph
                    AgeDisputesRule
                        label is "AgeDisputes"
                paragraph
                    BillOfExchangeProcessing
                        label is "BOEProcessingType"
            column2
                paragraph
                    PercentOfBalanceToCreditLimit
                        label is "CreditPercentReview"
                paragraph
                    DisputedTransFinanceCharged
                        label is "DisputesFinanced"
                paragraph
                    RevalueCurrencyTransactions
                        label is "CurrencyRevaluation"
