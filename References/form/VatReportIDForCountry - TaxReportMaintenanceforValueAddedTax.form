TaxReportMaintenanceforValueAddedTax is a CompositeForm
    is primary
    title is "TaxReportMaintenanceForValueAddedTax"
    context form is TaxReportMaintenanceforValueAddedTaxContext
        background is theme color
    ReportMaintenance is a Panel
        list is VatReportingLineRel.TaxReportingLineList
    SalesList is a Panel
        list is VatEuropeanSalesCode set.VatCodeDashboardList
    TaxCalendar is a Panel
        list is TaxCalendarsRel.TaxCalendarListWPeriods
    TaxTransactions is a Panel
        list is TaxTransactionTaxCodesRel.VATTaxTransactionSearchList
    JournalFileTransactions is a Panel
        title is "VatPurchaseSaleTransactions"
        visible when (VatPurchaseAndSalesTransactionRelExist)
        list is VatPurchaseSaleTransactionsRel.VatPurchaseSaleTransactionList2
    VATReturnReport is a Panel
        visible when (VatReturnPreviewHasValues)
        form is VATReturnForm
    JournalDetailPurgeReport is a Panel
        visible when (JournalDetailPurgeReportExist)
        list is JournalDetailPurgeReportRel.JournalDetailPurgeReportList
