SupplierResponsesHistorySumForm is a SummaryForm
    title is "-"
    Layout
        three column
            column1
                header4 of "Response_\\DetailFor_\\Supplier_\\Source\\I\\D:_<SupplierSourceId>"
            column2
            column3
                visible when (SupplyManagementReportContext.IncludeDetails)
                    text of "Name:_<MainContact>"
                    text of "Telephone:_<TelephoneNumber>"
                    text of "Email:_<EmailAddress>"
        list is SourcingEventLineResponsesForResponseHistoryRel.SupplierResponseHistoryDetailList
        header4 of "TotalsFor_\\Supplier_\\Source\\I\\D:_<SupplierSourceId>"
        three column
            column1
                text of "Total_\\Events_\\Bid\\:_<TotalSourceIdEventsBid>"
                text of "Total_\\Events_\\Award\\:_<TotalSourceIdEventAwards>"
                text of "Event_\\Bid_\\Success:_<TotalSourceIdEventBidSuccess>"
            column2
                text of "Total_\\Lines_\\Bid\\:_<TotalSourceIdLinesBid>"
                text of "Total_\\Awarded_\\Lines\\:_<TotalSourceIdAwardedLines>"
                text of "Line_\\Bid_\\Success:_<TotalSourceIdLineBidSuccess>"
            column3
                text of "Bid_\\Total_\\:_<TotalSourceIdBid>"
                text of "Total_\\Awarded_\\Amount\\:_<TotalSourceIdAwarded>"
                text of "Amount_\\Bid_\\Success:_<TotalSourceIdAmountBidSuccess>"
