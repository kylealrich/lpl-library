ContactUpdate is a Form
    title is "SupplierContactInformation"
    Layout
        header2 of "GeneralInformation"
        single column
            ContactTitle
                label is "Title"
            MainContact.GivenName
                label is "*_FirstName"
            MainContact.FamilyName
                label is "*_LastName"
        paragraph
            TelephoneNumber.InternationalPrefix
                label is "*_PhoneNumber"
            TelephoneNumber.SubscriberNumber
                no label
            TelephoneNumber.Extension
                label is "Ext"
            text of "(InternationalPrefix,PhoneNumber,Extension)"
                italics
        paragraph
            MobilePhone
            text of "(InternationalPrefix,PhoneNumber)"
                italics
        paragraph
            FaxNumber
            text of "(InternationalPrefix,FaxNumber,Extension)"
                italics
        paragraph
            EmailAddress
                label is "*_EmailAddress"
            ReceiveEmailNotification
        header2 of "MailingAddress"
        single column
            visible when (SupplierGroup.DisplayTypeForRegistration)
                LocationAddress
                    form is PostalAddressMinimumWithType
            visible when (not SupplierGroup.DisplayTypeForRegistration)
                LocationAddress
                    form is PostalAddressMinimum
        single column
            blank line
            blank line
            text of "*__=_Required"
