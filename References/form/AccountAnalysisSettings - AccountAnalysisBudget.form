AccountAnalysisBudget is a CompositeForm
    title is "<AccountAnalysisSettings>_<Description>"
    context form is BudgetAccountAnalysisSettingsContext
    SingleAccount is a Panel
        title is "<ReportingBasis.ReportingChart>_ChartIn<DisplayReportingCurrency>"
        visible when (AccountEntered)
        list is GeneralLedgerChartAccountRel.AccountAnalysisBudgetList
    ChartAccounts is a Panel
        title is "<ReportingBasis.ReportingChart>_ChartIn<DisplayReportingCurrency>"
        visible when (not AccountEntered)
        list is ReportingBasis.ChartAccountTopNode.ChartAccountChildrenRel.AccountAnalysisBudgetList
    Structures is a Panel
        visible when (not IsOldBasis)
        form is Structures
