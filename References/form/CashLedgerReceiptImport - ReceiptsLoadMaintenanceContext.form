ReceiptsLoadMaintenanceContext is a Form
    title is "ReceiptsInterfaceMaintenance"
    Layout
        visible when (RecordExists)
            three column distributed
                column1
                    display as text
                        CashManagementGroup
                        CashLedgerReceiptImport.BankTransactionCode.BankTransactionCodeName
                            label is "TransactionCode"
                column2
                    display as text
                        CashLedgerReceiptImport.RunGroup
                            label is "RunGroup"
                        paragraph
                            CashLedgerReceiptImport.TransactionNumber
                column3
                    display as text
                        paragraph
                            CashLedgerReceiptImport.CashCode.CashCodeAndDescription
                                label is "CashCode"
                            CashLedgerReceiptImport.CashCode.Currency
                        CashLedgerReceiptImport.SequenceNumber
            single column
                display as text
                    ErrorMessage
            button of "InterfacedReceiptRecord"
                visible when (CashReceiptDetailInError)
                link is CashLedgerReceiptRecordNavigation
                display as link
            button of "InterfacedTransferRecord"
                visible when (HasCashLedgerEFTInError)
                link is CashLedgerTransferRecordNavigation
                display as link
