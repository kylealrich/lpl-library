WorkunitRoutingCompositeForm is a CompositeForm
    title is "WorkUnit<PfiWorkunit>Routing"
    context form is WorkunitRoutingForm
    ApprovalTracking is a Panel
        visible when (HasProcessIndicator)
        form is ApprovalTracking
    Actions is a Panel
        title is "Actions<QueueCount>"
        list is PfiQueueRel.InbasketHistoryList
    Errors is a Panel
        title is "Errors<ActivityFailedCount>"
        visible when (ActivityFailedFlag)
        list is PfiFailedActivityRel.ActivityList
