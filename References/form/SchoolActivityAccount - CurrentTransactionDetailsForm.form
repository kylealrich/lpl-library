CurrentTransactionDetailsForm is a CompositeForm
    title is "CurrentTransactionDetails"
    context form is CurrentTransactionDetailsContextForm
    Receipts is a Panel
        visible when (HasReceiptDetails)
        list is StudentActivityReceiptDetailRel.ReceiptDetailList
    BankReceipts is a Panel
        visible when (HasBankReceiptDetails)
        list is StudentActivityBankCreditsRel.BankReceiptDetailList
    Disbursements is a Panel
        visible when (HasDisbursementDetails)
        list is StudentActivityDisbursementRel.DisbursementDetailList
    Transfers is a Panel
        visible when (HasTransferDetails)
        list is StudentActivityTransferDetailRel.TransferDetailList
    BankTransfers is a Panel
        visible when (HasBankTransferDetails)
        list is StudentActivityBankTransferRel.BankTransferDetailList
