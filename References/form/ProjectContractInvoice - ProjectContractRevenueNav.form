ProjectContractRevenueNav is a Form
    title is "<FinanceEnterpriseGroup.ProjectLabel>ContractRevenue"
    Layout
        paragraph
            ProjectContract.DisplayContract
                label is "<FinanceEnterpriseGroup.ProjectLabel>Contract"
                display as text
            ProjectContract.Project.Description
                no label
                display as text
        paragraph
            RunGroup
                label is "RunGroup"
                display as text
            FinanceDimension2.DisplayDimension
                label is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
                display as text
            FinanceDimension2.CompanyCustomerGroup.ReceivableCompany
                label is "Company"
                display as text
            FinanceDimension2.CompanyCustomerGroup.Customer
                label is "Customer"
                display as text
        paragraph
            InvoiceDate
                label is "RevenueDate"
                display as text
            Status
                display as text
        paragraph
            InvoiceTotalAmount
                label is "TotalAmount"
                display as text
            InvoiceTotalFees
                label is "Fees"
                display as text
            InvoiceTotalAllowance
                label is "TotalAllowance"
                display as text
