PurchaseOrderLineMainComposite is a CompositeForm
    is primary
    title is "<DerivedPOFormTitle>"
    context form is PurchaseOrderLineCompositeContext
        background is theme color
    restrict action FixReceivedQuantity
    restrict action AssignProcessor
    Actions
        action is CancelLine
            hide when invalid
        action is Released.CancelLineRemaining
            hide when invalid
        action is Unreleased.ReleasePurchaseOrder
            action icon is release
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is Unreleased.Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is save
            label is "_"
        action is save and new
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
    Detail is a Panel
        form is PurchaseOrderLineMainDetail
    Options is a Panel
        form is PurchaseOrderLineOptions
    QuantityDetail is a Panel
        form is POLineQuantityDetailForm
    CostDetail is a Panel
        form is POLineCostForm
    ProcedureInformation is a Panel
        visible when (DisplayOfProcedureInfo)
        form is ProcedureInformationForm
    UserFields is a Panel
        form is UserFieldsForm
    ItemAddOnCost is a Panel
        title is "AddOnCharges"
        list is PurchaseOrderAndLineAddOnCharge set.AddOnCostsForAPurchaseOrderLine
    Sources is a Panel
        list is PurchaseOrderLineSource set.primary
    Comments is a Panel
        title is "Comments<CommentCount>"
        list is PurchaseOrderLineComment set.primary
    Distributions is a Panel
        title is "<DerivedDistributionsPanelLabel>"
        visible when (not IsInventoryType)
        list is PurchaseOrderLineDistributionsRel.primary
    AcknowledgementMessages is a Panel
        visible when (HasAcknowledgementMessages)
        list is PurchaseOrderAcknowledgementMessageRel.LineAcknowledgementMessages
    Errors is a Panel
        visible when (LineErrorsExist)
        list is PurchaseOrderLineError set.PurchaseOrderLineErrorList
