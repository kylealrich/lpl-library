TaxLogSearch is a SearchForm
    title is "TaxLogSearch"
    Layout
        four column
            column1
                CreateDateRange.Begin
                    label is "FromCreateDate"
                CreateDateRange.End
                    label is "ToCreateDate"
                CreatedBy
                TaxLogHeader
            column2
                LogTaxEntity
                    label is "TaxEntity"
                SystemCode
                LogInvoice
                    label is "Invoice"
                LogCreatedTimestamp
                    label is "Timestamp"
            column3
                LogVendor
                    label is "Vendor"
                LogCustomer
                    label is "Customer"
                LogCompany
                    label is "Company"
                LogThirdParty
                    label is "ThirdParty"
                visible when (LogThirdParty.Sabrix)
                    LogOneSourceVersionType
                        label is "VersionType"
            column4
                WebServiceError
                LogCallingRoutine
                    label is "CallingModule"
                LogToTaxCode
                    label is "TaxCode"
                VertexRequestExecuted
