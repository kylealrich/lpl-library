ProjectCreateForm is an ActionForm
    action is Create
    title is "CreateNew<FinanceEnterpriseGroup.ProjectLabel>"
    Layout
        section of Main
            three column
                column1
                    visible when (InEnterpriseStructure)
                        CreateProjectType
                            label is "<FinanceEnterpriseGroup.ProjectLabel>Type"
                            when value changed
                                Contract =  false
                                    when (CreateProjectType.Posting)
                column2
                    visible when (InEnterpriseStructure)
                        visible when (CreateProjectType.Summary)
                            Contract
                        visible when (CreateProjectType.Posting)
                            SystemProject
                                label is "System<FinanceEnterpriseGroup.ProjectLabel>"
            three column distributed
                column1
                    Project
                        label is "<FinanceEnterpriseGroup.ProjectLabel>"
                column2
                    Description
                column3
                    visible when (InEnterpriseStructure)
                        Active
            visible when (InEnterpriseStructure)
                single column
                    visible when (CreateProjectType.Posting)
                        LikeProjectField
                            label is "AddToStructureWhereThis<FinanceEnterpriseGroup.ProjectLabel>Exists"
                            select is LikeProjectSelectList
                three column
                    column1
                        ProjectDateRange.BeginDate
                            label is "BeginDate"
                            display as required
                    column2
                        ProjectDateRange.EndDate
                            label is "EndDate"
                            display as required
                three column
                    column1
                        visible when (CreateProjectType.Summary)
                            paragraph
                                BudgetIdentifier
                                BudgetIdentifier.Description
                                    no label
                                    display as text
                three column
                    visible when (CreateProjectType.Summary)
                        column1
                            paragraph
                                PersonResponsible
                                    select is ActiveEmployeesOpenRestricted
                                    show up to 8 characters
                                PersonResponsible.Name.PreferredFirstAndLastName
                                    no label
                                    display as text
                        column2
                            paragraph
                                ProjectTeam
                                    show up to 12 characters
                                ProjectTeam.Description
                                    no label
                                    display as text
                four column distributed
                    visible when (Contract)
                        column1
                            ProgramIncome
                        column2
                            ProgramIncomeType
                three column
                    visible when (CreateProjectType.Summary)
                        column1
                            paragraph
                                AddressCode
                                AddressCode.Description
                                    no label
                                    display as text
                        visible when (AddressCode entered)
                            column2
                                AddressCode.PostalAddress.DisplayAddressMultiline
                                    label is "Address"
                                    display as text
                three column
                    column1
                        visible when (CreateProjectType.Posting)
                            paragraph
                                ProjectStatus
                                    label is "<FinanceEnterpriseGroup.ProjectLabel>Status"
                                    show up to 10 characters
                                    display as required
                                ProjectStatus.Description
                                    no label
                                    display as text
                    column2
                        visible when (CreateProjectType.Posting)
                            paragraph
                                Currency
                                    show up to 5 characters
                                    display as required
                                Currency.Description
                                    no label
                                    display as text
                three column
                    column1
                        paragraph
                            Watch
                            WatchedBy
                            WatchedBy.Name.PreferredFirstAndLastName
                                no label
                                display as text
            visible when (not InEnterpriseStructure)
                single column
                    paragraph
                        BudgetIdentifier
                        BudgetIdentifier.Description
                            no label
                            display as text
            visible when (InEnterpriseStructure)
                visible when (CreateProjectType.Posting)
                    header2 of "AdditionalInformation"
                    two column
                        column1
                            FinanceDimension2
                                label is "RelatedFundingSource"
                        column2
                            FinanceDimension2.Description
                                no label
                                display as text
                    four column distributed
                        column1
                            Billable
                        column2
                            Capital
                        column3
                            IncludeInBudgetEdit
                    four column distributed
                        column1
                            visible when (FinanceEnterpriseGroup.FrontEndSplits)
                                ExcludeFromFES
                                    label is "ExcludeFromFrontEndSplit"
                        column2
                            visible when (FinanceEnterpriseGroup.ExpenseManagementIntegrated)
                                ExpenseManagement
                    single column
                        DefaultCodeBlock
                            compact format
                    three column
                        column1
                            paragraph
                                WorkType
                                ServiceType
                                UsageType
                    four column distributed
                        column1
                            PreAwardID
                        column2
                            ProgramIncome
                        column3
                            ProgramIncomeType
                visible when (CreateProjectType.Posting)
                    header2 of "Address"
                    four column distributed
                        column1
                            AddressCode
                        column2
                            AddressCode.Description
                                no label
                                display as text
                        visible when (AddressCode entered)
                            column3
                                AddressCode.PostalAddress.DisplayAddressMultiline
                                    label is "Address"
                                    display as text
        section of ContractInformation visible when (Contract)
            label is "Contract"
            three column distributed
                column1
                    ContractAmount
                        display as required
                column2
                    ContractCurrency
                        label is "Currency"
                        display as required
                column3
                    ContractCurrency.Description
                        no label
                        display as text
            three column distributed
                column1
                    ContractID
                        show up to 25 characters
                column2
                    ContractType
                        show up to 10 characters
                column3
                    ContractMaximumFees
                        label is "MaximumFees"
            three column distributed
                column1
                    ContractAllocateExpenditures
                        label is "AllocateExpenditures"
                column2
                    ContractExcludeAP
                        label is "ExcludeUnpaidAP"
                column3
                    visible when (FinanceEnterpriseGroup.FrontEndSplits)
                        FrontEndSplits
            header2 of "Billing"
            three column distributed
                column1
                    ContractBillingMethod
                        label is "Method"
                        show up to 10 characters
                        display as required
                column2
                    visible when (ContractBillingMethod.CostPlus)
                        ContractMarkupPercent
                            label is "MarkupPercent"
                    visible when (ContractBillingMethod.TimeAndMaterials)
                        ContractMarkupPercent
                            label is "MarkupPercent"
                    visible when (ContractBillingMethod.RateTable)
                        ContractMarkupPercent
                            label is "MarkupPercent"
                column3
                    visible when (ContractBillingMethod.RateTable)
                        ContractBillingRateTable
                            label is "RateTable"
                            show up to 15 characters
            header2 of "Revenue"
            three column distributed
                column1
                    ContractRevenueRecognition
                        label is "RevenueRecognition"
                        display as required
                column2
                    visible when (ContractRevenueRecognition.Separate)
                        ContractRevenueMethod
                            label is "Method"
                column3
                    RemainderOption
            three column distributed
                column1
                    SingleProjectInvoice
                column2
                    visible when (ContractRevenueRecognition.Separate)
                        SingleProjectRevenue
        section of BillingOverrides visible when (CreateProjectType.Posting)
            four column distributed
                column1
                    BillingMethod
                        show up to 10 characters
                column2
                    visible when (BillingMethod.CostPlus)
                        MarkupPercent
                    visible when (BillingMethod.TimeAndMaterials)
                        MarkupPercent
                    visible when (BillingMethod.RateTable)
                        MarkupPercent
                column3
                    visible when (BillingMethod.RateTable)
                        BillingRateTable
                            show up to 15 characters
                column4
                    BillingMaximum
            single column
                TaxExempt
            paragraph
                ViewTaxCodeOrTaxLocation
                    label is "TaxCode_orTaxLocation"
                    display as horizontal radio buttons
                    when value changed
                        BuyerTaxCode =  blank
                            when (ViewTaxCodeOrTaxLocation.TaxLocation)
                        BuyerTaxLocation =  blank
                            when (ViewTaxCodeOrTaxLocation.TaxCode)
                visible when (ViewTaxCodeOrTaxLocation.TaxCode)
                    BuyerTaxCode
                    BuyerTaxCode.Description
                        no label
                        display as text
                visible when (ViewTaxCodeOrTaxLocation.TaxLocation)
                    BuyerTaxLocation
                    BuyerTaxLocation.Description
                        no label
                        display as text
                    BuyerTaxLocation.AddressCode
                        display as text
            single column
                FeesExempt
                RevenueCodeBlockOption
                RevenuePartialAccount
                    compact format
                RevenueFullAccount
                    compact format
        section of LaborDistribution visible when (CreateProjectType.Posting)
            header2 of "LaborDistribution"
            single column
                PreAwardAssignmentDays
                FinanceCodeBlock
                    compact format
                text of "DimensionsSetToBlank:"
                    align as label
                FinanceCodeBlockSetBlanks
                    no label
            four column
                column1
                    FringeBurden
                        show up to 10 characters
                column2
                    BurdenCode
                        show up to 13 characters
                column3
                    BurdenCode.Description
                        no label
                        display as text
                column4
                    BurdenCode.Rate
                        display as text
            single column
                BurdenCodeBlock
                    compact format
                text of "DimensionsSetToBlank:"
                    align as label
                BurdenCodeBlockSetBlanks
                    no label
                CertificationRequired
            header2 of "SalaryCap"
            single column
                paragraph
                    SalaryCap
                        show up to 10 characters
                    SalaryCap.Description
                        no label
                        display as text
                    SalaryCapDate
                        show up to 15 characters
            single column
                OverageCodeBlock
                    compact format
                text of "DimensionsSetToBlank:"
                    align as label
                OverageCodeBlockSetBlanks
                    no label
        section of BlankSection
            label is "_"
