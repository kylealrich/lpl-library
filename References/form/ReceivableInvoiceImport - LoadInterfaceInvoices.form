LoadInterfaceInvoices is an ActionForm
    action is InterfaceInvoices
    title is "InterfaceInvoices"
    Layout
        single column
            PrmRunGroup
                label is "RunGroup"
        paragraph
            PrmCompany
                label is "Company"
            PrmCompany.Name
                no label
                display as text
        two column distributed
            column1
                paragraph
                    CustomerRange.FromCustomer
                        label is "FromCustomer"
                    CustomerRange.FromCustomer.Name
                        no label
                        display as text
            column2
                paragraph
                    CustomerRange.ToCustomer
                        label is "ToCustomer"
                    CustomerRange.ToCustomer.Name
                        no label
                        display as text
        two column distributed
            column1
                InterfacedCustomerRange.FromCustomer
                    label is "FromExternalCustomer"
            column2
                InterfacedCustomerRange.ToCustomer
                    label is "ToExternalCustomer"
