EventResponseComposite is a CompositeForm
    use for action Draft.Create
    use for action Draft.Update
    use for action Submitted.Update
    use for action Withdrawn.Update
    use for action Submitted.WithdrawResponse
    use for action Withdrawn.ReinstateResponse
    use for action Submitted.ModifyResponse
    title is "Event#<SourcingEvent.RepresentativeText>"
    context form is SourcingEventSummaryForComposite
    Actions
        action is Draft.Submit
            action icon is submit
            hide when invalid
        navigation is PrintResponse
            action icon is print
        action is save
            action icon is save
            label is "_"
            hide when invalid
        action is save and close
            action icon is save
            hide when invalid
        action is refresh
            label is "_"
    LineResponses is a Panel
        visible when (DisplayBidAmountNoDateInWizard)
        mouse over text is "<SourcingEvent.Company.LineResponseText>"
        panel header is "<SourcingEvent.Company.LineResponseText>"
        template is SourcingEvent.ExecutiveEventLinesRel
            business class is SourcingEventLineResponse
                list is SupplierLineResponses
    EventLineResponses is a Panel
        visible when (DisplayBidAmountWithDateInWizard)
        mouse over text is "<SourcingEvent.Company.LineResponseText>"
        panel header is "<SourcingEvent.Company.LineResponseText>"
        template is SourcingEvent.ExecutiveEventLinesRel
            business class is SourcingEventLineResponse
                list is SupplierLineResponsesWithDate
    BestAndFinalLineResponses is a Panel
        visible when (CanEnterBestAndFinalAmounts)
        mouse over text is "<SourcingEvent.Company.LineResponseText>"
        panel header is "<SourcingEvent.Company.LineResponseText>"
        template is SourcingEvent.SourcingEventLine set
            business class is SourcingEventLineResponse
                list is SupplierLineResponses
    BidBondInformation is a Panel
        visible when (SourcingEventRequireBidBond)
        mouse over text is "<SourcingEvent.Company.BidBondText>"
        panel header is "<SourcingEvent.Company.BidBondText>"
        form is EnterBidBondNumber
    HeaderAOC is a Panel
        visible when (HeaderAOCsAndLineResponsesExist)
        panel header is "EnterAddOnChargePercentOrAmount"
        list is HeaderAddOnChargeResponseRel.SourcingEventResponseAOCsSupplier
    ResponseAttachments is a Panel
        visible when (DisplayBidSupplierPortal)
        mouse over text is "<SourcingEvent.Company.AttachmentResponseText>"
        panel header is "<SourcingEvent.Company.AttachmentResponseText>"
        list is SourcingEventResponseAttachment set.Attachments
    EventResponse is a Panel
        mouse over text is "<SourcingEvent.Company.HeaderResponseText>"
        panel header is "<SourcingEvent.Company.HeaderResponseText>"
        form is Response
    EventQuestions is a Panel
        visible when (SEQuestionsOldExist)
        template is SourcingEvent.SourcingEventQuestion set
            business class is SourcingEventQuestionResponse
                form is Question
