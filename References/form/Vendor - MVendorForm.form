MVendorForm is a CompositeForm
    title is "<DerivedFormTitle>"
    context form is MVendorContext
    restrict action ReleaseToARInterface
    restrict action LinkWithARCustomer
    restrict action Create
    restrict action CreateOneTimeVendor
    restrict action RequestNewVendor
    restrict action Delete
    restrict action CreateAPClerkSmartReconVendor
    Actions
        action is previous record
            label is "_"
            hide when invalid
        action is next record
            label is "_"
            hide when invalid
        action is save
            label is "_"
            hide when invalid
        action is refresh
            label is "_"
    Main is a Panel
        form is MobileMainForm
    VendorBank is a Panel
        form is MVendorBank
    Options is a Panel
        form is MobileVendorOptions
    Balances is a Panel
        list is AllVendorBalanceMasterRel.MVendorLocationBalanceList
    Contacts is a Panel
        list is VendorContactRel.MVendorContactsList
    PayVendors is a Panel
        list is PayToVendor set.MPayToVendorCardList
    Certifications is a MultiListPanel
        list is VendorCertificationsRel.MobileCertificationCodeAssignmentsVendorPanel
        list is VendorDiversityCodesRel.MobileDiversityCodeCertificationsVendorPanel
    CompanyDefaults is a Panel
        list is VendorCompanyDefaultRel.MVendorCompanyDefaultCodes
    CrossReferences is a Panel
        list is CrossReferenceVendor set.MCrossReferenceVendorDetailList
    InterfacedVendors is a Panel
        visible when (InterfacedVendorsExist)
        list is VendorMappingTableInterfacesRel.VendorMappingTableInterfaceList
    RelatedMerchants is a Panel
        list is MerchantCrossReferenceRel.MVendorMerchantCrossReferences
    Comments is a Panel
        title is "Comments<DerivedVendorCommentsCount>"
        list is VendorCommentsRel.MobileVendorComments
    Attachments is a Panel
        title is "Attachments<DerivedVendorAttachmentsCount>"
        list is VendorAttachmentsRel.MobileAttachments
    Locations is a Panel
        title is "Locations<DerivedLocationCount>"
        list is VendorLocation set.MVendorLocationsPanelList
    MultipleTaxAreas is a Panel
        visible when (VertexResultsExist)
        list is VertexTaxAreaIdResultsRel.TaxAreaResultList
    VertexError is a Panel
        visible when (VertexErrorExist)
        list is VertexWebserviceErrorRel.MobileVertexTaxAreaIdErrorList
    VendorWithholdingCodes is a Panel
        visible when (AllowWithholdingCodeOverride)
        list is VendorWithholdingCodesRel.MobileVendorWithholdingCodeFromVendorList
    DistributionRestrictions is a Panel
        list is VendorAccountRestriction set.MobileVendorAccountRestrictionList
