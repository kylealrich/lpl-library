                ThruDate
                    center
                    display when blank
            paragraph
                IncludeDetails
                    label is "IncludeDetails"
                    center
                    display when blank
            paragraph
                ExcludePOCode
                    label is "ExcludePOCode"
                    center
                    display when blank
            paragraph
                PrintParameters
                    label is "PrintParameters"
                    center
                    display when blank
        form is NonDiversePOSumForm
