NonDiverseAwardHistorySumForm is a SummaryForm
    title is "_"
    Layout
        visible when (SupplierDetail)
            header4 of "DiversityCode:\\Non-Diverse"
            form is NonDiverseSupplierResponseRel.NonDiverseSupplierSumForm
        header4 of "TotalsForDiversityCode\\:_Non-Diverse"
        three column
            column1
                DerivedNonDiverseTotalEventsBid
                    label is "TotalEventsBid"
                    display when blank
                DerivedNonDiverseTotalEventsAwarded
                    label is "TotalEventsAwarded"
                    display when blank
                DerivedEventsBidPercent
                    label is "EventBidSuccess"
                    display when blank
            column2
                DerivedNonDiverseTotalEventLinesBid
                    label is "TotalLinesBid"
                    display when blank
                DerivedNonDiverseTotalEventLinesAwarded
                    label is "TotalLinesAwarded"
                    display when blank
                DerivedEventLinesBidPercent
                    label is "LineBidSuccess"
                    display when blank
            column3
                DerivedNonDiverseBidTotal
                    label is "BidTotal"
                    display when blank
                DerivedNonDiverseAwardedTotal
                    label is "TotalAwardedAmount"
                    display when blank
                DerivedTotalBidPercent
                    label is "AmountBidSuccess"
                    display when blank
