SecurityConsoleForm is a CompositeForm
    title is "SecurityClasses"
    restrict action DisableDataTranslation
    restrict action EnableDataTranslation
    restrict action AddSubject
    restrict action RemoveSubject
    Actions
        action is refresh
    Edit is a Panel
        form is SecurityDetailForm
    Description is a Panel
        form is UpdateDescriptionForm
    LPLHistory is a Panel
        list is ConfigEntityHistory set.ConsoleLPLChangesList
    StatusHistory is a Panel
        list is ConfigEntityHistory set.ConsoleStatusChangesList
    BaseClass is a Panel
        visible when (IsExtendedSecurityClass)
        form is BaseSecurityClassForm
    CopiedFrom_vs_ConfigurationComparison is a Panel
        title is "CopiedFromComparison"
        visible when (IsCopiedSecurityClass)
        form is SecurityDifferenceForm
    Tags is a Panel
        visible when (HasTags)
        list is TaggedItemsRel.TagOnlyList
    Security is a Panel
        visible when (HasSecurityClass)
        form is SecurityClassForm
