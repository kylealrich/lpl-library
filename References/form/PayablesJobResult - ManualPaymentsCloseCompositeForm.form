ManualPaymentsCloseCompositeForm is a CompositeForm
    title is "<ManualPaymentsCloseTitle>"
    context form is ManualPaymentsContext
    restrict action PerformVendorBalanceYearEnd
    restrict action Create
    restrict action CreateVendorPaymentReport
    restrict action Delete
    Actions
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
            hide when invalid
        action is next record
            label is "_"
            mouse over text is "Next_Record"
            hide when invalid
        action is refresh
            label is "_"
    CompletedPayablesPayments is a Panel
        title is "SelectedInvoicePayments_(<NumInvoiceManualPaymentsClosedRel>)"
        visible when (ShowAPManualPaymentsClosed)
        list is InvoiceManualPaymentsClosedRel.InvoiceManualPayments
    Error is a Panel
        visible when (ErrorsExist)
        list is PayablesJobErrorResultRel.PaymentsClosingErrors
    ScheduledPayments is a Panel
        title is "ScheduledPayments_(<NumCashLedgerPayablesPaymentRel>)"
        visible when (ShowPayments)
        list is CashLedgerPayablesPaymentPayablesJobResultRel.CashRequirementsList
    ScheduledPaymentsSummary is a Panel
        visible when (ShowPayments)
        list is CashLedgerPayablesPaymentPayablesJobResultRel.SumCashRequirementsList
