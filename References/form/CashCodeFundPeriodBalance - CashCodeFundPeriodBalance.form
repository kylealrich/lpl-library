    is primary
    title is "CashCodeFundPeriodBalance"
    Layout
        single column
            CashCode
            GeneralLedgerClosePeriod
            AccountingEntity
                label is "<FinanceEnterpriseGroup.AccountingEntityLabel>"
            visible when (FinanceEnterpriseGroup.FundDimension.AccountingUnit)
                AccountingUnit
                    label is "<FinanceEnterpriseGroup.AccountingUnitLabel>"
            visible when (FinanceEnterpriseGroup.FundDimension.Dimension1)
                FinanceDimension1
                    label is "<FinanceEnterpriseGroup.FinanceDimension1Label>"
            paragraph
                ClaimOnCash
                PostedAmount
                CommittedAmount
            paragraph
                MinimumFundAmount
                LineOfCreditAmount
                AvailableAmount
            paragraph
                GlobalLedgerPeriodActivity
                GlobalLedgerPeriodBalance
                NextPeriodPostedAmount
