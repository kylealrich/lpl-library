ColumnCompositeFormNew is a CompositeForm
    use for action CreateColumn
    title is "Column:<FileCreationSetup.Description>-<FileCreationRowDetail.Description>-<FieldName>"
    Detail is a Panel
        form is ColumnDetailForm
    Formatting is a Panel
        form is NewColumnFormattingForm
    OptionsForIdentifyingRowChanges is a Panel
        visible when (FileCreationSetup.UseAuditInformation)
        form is ColumnChangeOptionsForm
    DataMapping is a Panel
        visible when (UsesDataMappings)
        list is FileCreationDataMapping set.FileCreationDataMappingPrimaryList
