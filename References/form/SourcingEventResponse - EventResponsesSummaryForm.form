EventResponsesSummaryForm is a SummaryForm
    title is "EventResponses"
    Layout
        list is SourcingEventResponsesRel.EventResponsesDetailList
        six column
            column1
                PriceScore
                    label is "Price"
                    display when blank
            column2
                DeliveryScore
                    label is "Delivery"
                    display when blank
            column3
                DiversityScore
                    label is "Diversity"
                    display when blank
            column4
                TotalQuestionScore
                    label is "Question"
                    display when blank
            visible when (SupplyManagementReportContext.IncludeSupplierContact)
                column5
                    NotifiedSupplier.SupplierSourceId.PrimaryContact
                        label is "Contact"
                        display when blank
                    NotifiedSupplier.SupplierSourceId.TelephoneNumber
                        label is "Telephone"
                        display when blank
                    NotifiedSupplier.SupplierSourceId.EmailAddress
                        label is "E\\-Mail"
                        display when blank
                column6
                    NotifiedSupplier.SupplierSourceId.LocationAddress
                        label is "Address"
                        display when blank
