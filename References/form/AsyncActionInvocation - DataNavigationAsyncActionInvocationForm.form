DataNavigationAsyncActionInvocationForm is a Form
    is primary
    title is "DataNavigationJobInvocationForm"
    Layout
        single column
            AsyncActionInvocation
                label is "JobInvocationUniqueId"
            Archived
            AsyncActionInvocationStatus
            Name
            Group
            Actor
            DataArea
        two column
            InvokedFromName
                display only
            visible when (InvokingParentExists)
                InvokedFrom
                    label is "InvokedFromJobInvocationUniqueId"
            visible when (InvokedFromButInvokingParentDoesNotExist)
                text of "InvokedFromJobInvocationRecordNoLongerExistsFor<InvokedFromName>"
        two column
            Type
            visible when (AsyncAction.IsParameterized)
                ActionParameters
            visible when (NonParameterizedParameterFormToDisplay)
                BatchTaskId
                    label is "Job"
            visible when (NoNonParameterizedParameterFormToDisplay)
                text of "NoParameterFormToDisplay"
        single column
            ImplementingClass
            AsyncAction
            AsyncActionRequest
                label is "AsyncActionRequestUniqueId"
            AsyncActionTrigger
            StatusChanged
            TimeToExec
                label is "RequestTimeToExecute"
            Started
            Finished
            LastModTime
            AdditionalInfo
            FailureCode
            ErrorMessage
            Result
        visible when (HasRunOnErrorAction)
            header2 of "ErrorProcessing"
            two column
                RunOnErrorAction.ImplementingClass
                    display only
                RunOnErrorAction.AsyncAction
                    display only
            two column
                RunOnErrorFailureCode
                    display only
                RunOnErrorErrorMessage
                    display only
                blank line
                RunOnErrorResult
                    display only
