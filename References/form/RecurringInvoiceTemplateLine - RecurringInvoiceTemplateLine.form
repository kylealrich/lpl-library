RecurringInvoiceTemplateLine is a CompositeForm
    is primary
    title is "<RecurringInvoiceTemplate.ReceivableInvoiceType>_<RecurringInvoiceTemplate>_Line_<FormTitleLineNumber>"
    context form is RecurringInvoiceTemplateLineContextForm
        background is theme color
    Main is a Panel
        form is Main
    AddOnCharges is a Panel
        title is "AddOnCharges<DerivedAddOnChargesIndicator>"
        list is RecurringInvoiceTemplateAddOnCharge set.RecurringInvoiceAddOnCharges
    Distributions is a Panel
        list is RecurringInvoiceTemplateDistributionRel.RecurringInvoiceTemplateDistributionList
