PFTermNegotiation is a CompositeForm
    title is "ContractArticle"
    context form is ArticleContext
        background is theme color
    Article is a Panel
        form is ContractArticleForm
    Terms is a Panel
        list is ContractTermsByDisplayRel.PFContractTermsAndConditions
    History is a Panel
        visible when (ShowApprovalColumn)
        list is ContractArticleUpdate set.ContractArticleHistory
