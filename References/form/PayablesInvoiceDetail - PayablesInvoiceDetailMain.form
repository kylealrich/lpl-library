PayablesInvoiceDetailMain is a Form
    Layout
        display as text
            two column distributed
                column1
                    paragraph
                        visible when (PurchaseOrderLine entered)
                            PurchaseOrder.DisplayDocumentNumber
                                label is "PurchaseOrder"
                        visible when (PurchaseOrderLine entered)
                            PurchaseOrderLine
                                label is "LineNumber"
                    paragraph
                        visible when (Contract entered)
                            Contract
                        visible when (ContractLine entered)
                            ContractLine
                column2
                    paragraph
                        visible when (PurchaseOrderReceipt entered)
                            PurchaseOrderReceipt
                                label is "ReceiptNumber"
                        visible when (PurchaseOrderReceiptLine entered)
                            PurchaseOrderReceiptLine
                                label is "ReceiptLine"
            two column distributed
                column1
                    paragraph
                        Item
                        ItemType
                        ItemGTIN
                            label is "GTIN"
                    ItemDescription
                column2
                    VendorItem
                    VendorItem.VendorItemDescription
                        label is "VendorItemDescription"
        paragraph
            EnteredQuantity
            EnteredUOM
            EnteredUnitCost
            EnteredUOMMultiplier
                display as text
            TotalDistributionAmount
                display as text
        visible when (IsCatchWeight)
            paragraph
                BuyUOMMatchedQuantity
                VendorBuyUOM
                    display as text
        visible when (IsHSNSACCodeEnabled)
            paragraph
                HSNSACCode
                HSNSACCode.Description
                    no label
                    display as text
        two column distributed
            column1
                header2 of "TaxInformation"
                paragraph
                    TaxCode
                        select is TaxCodesFromTaxMaster
                    display as text
                        visible when (not OneSourceGeneric)
                            TaxCode.Description
                                no label
                            EntityTaxCodeRel.AccruedOrInvoiced
                                label is "TaxType"
                paragraph
                    ProductTaxCategory
                paragraph
                    display as text
                        TaxAmount
                        visible when (OneSourceGeneric)
                            AccruedTaxAmount
                paragraph
                    TaxableUnitCost
                    visible when (OneSourceGeneric)
                        PayablesInvoiceDetailTaxRel.CalculatedTaxAmount
                            display as text
                        visible when (TaxToleranceApplied)
                            SumOfTaxDistributions
                                label is "AfterTaxToleranceApplied"
                                display as text
                paragraph
                    TotalExemptAmount
                    TotalExemptAmountTaxCode
                        label is "TaxCode"
            column2
                header2 of "AdditionalDetails"
                visible when (DisplayOnlyCommodityCode)
                    CommodityCode
                        label is "Commodity"
                        display as text
                        compact format
                visible when (not DisplayOnlyCommodityCode)
                    CommodityCode
                        label is "Commodity"
                        compact format
                paragraph
                    RetainagePercent
                        label is "OverrideRetainagePercentage"
                    DerivedRetainagePercentComplete
                        display as text
        visible when (MiscellaneousDetail)
            header2 of "Miscellaneous"
            three column
                RetailUnitCost
                SupplementaryQuantity
                StockWeight
