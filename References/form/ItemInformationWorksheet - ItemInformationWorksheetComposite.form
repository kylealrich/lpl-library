ItemInformationWorksheetComposite is a CompositeForm
    is primary
    title is "CreateItemAndVendorItemWorksheet"
    context form is ItemInformationWorksheetContext
        background is theme color
    restrict action CreateItemWorksheetFromCSV
    Item is a Panel
        form is ItemInformationWorksheetForm
    VendorItems is a Panel
        visible when (WorksheetExists)
        form is VendorItemInformation
    UnitsOfMeasure is a Panel
        visible when (WorksheetExistsAndNewItem)
        form is UnitOfMeasureInformation
    ItemComments is a Panel
        visible when (WorksheetExistsAndNewItem)
        form is ItemCommentInformation
    ItemLocations is a Panel
        visible when (WorksheetExists)
        list is ItemLocationInterface set.ItemLocationInterfaceList
    ItemErrors is a Panel
        visible when (ItemErrorsExist)
        list is ItemInformationError set.ItemInformationErrorList
    AssociatedContractLines is a Panel
        visible when (ContractLineByManufacturerExists)
        list is ContractLineFromManufacturerRel.AddedFromWorksheet
    GTINNumbers is a Panel
        title is "GTI\\Ns"
        visible when (WorksheetExists)
        form is ItemGTINInformation
    SKUCodes is a Panel
        visible when (WorksheetExists)
        form is ItemSKUInformation
    UPCCodes is a Panel
        title is "UP\\Cs"
        visible when (WorksheetExists)
        form is ItemUPCInformation
    NationalDrugCodes is a Panel
        visible when (WorksheetExistsAndShowHealthcareInformation)
        form is NationalDrugCodeInformation
    UCCEANItemUPNCodes is a Panel
        title is "UCC_/EAN_UP\\Ns"
        visible when (WorksheetExistsAndShowHealthcareInformation)
        form is UCCEANItemUPNInformation
    HIBCCItemUPNCodes is a Panel
        title is "HIBCC-D\\Is"
        visible when (WorksheetExistsAndShowHealthcareInformation)
        form is HIBCCItemUPNInformation
