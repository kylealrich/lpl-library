ReceivableInvoiceBatchForm is a CompositeForm
    is primary
    title is "InvoiceBatch<InvoiceBatchTitle>"
    context form is ReceivableInvoiceBatchFormContext
        background is theme color
    Actions
        action is Unreleased.Release
            action icon is release
            hide when invalid
        action is Create
            action icon is create
            label is "_"
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is refresh
            label is "_"
    Main is a Panel
        form is Main
    CurrencyRates is a Panel
        form is CurrencyRates
    Transactions is a Panel
        visible when (IsReleased)
        list is ReceivableInvoiceRel.ListReceivableInvoiceReleased
    Transaction is a Panel
        title is "Transactions"
        visible when (IsUnreleased)
        list is ReceivableInvoiceRel.ListReceivableInvoice
    SpeedEntry is a Panel
        visible when (IsUnreleased)
        list is ReceivableInvoiceRel.SpeedEntryTransactionsList
    Attachments is a Panel
        list is ReceivableInvoiceBatchAttachment set.primary
