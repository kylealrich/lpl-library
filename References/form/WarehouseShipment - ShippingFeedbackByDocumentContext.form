ShippingFeedbackByDocumentContext is a Form
    Layout
        visible when (RecordExists)
            display as text
                four column distributed
                    column1
                        Company.RepresentativeText
                            label is "Company"
                        InventoryLocation.RepresentativeText
                            label is "Location"
                        visible when (HasLinesWithSpecialHandling)
                            paragraph
                                SpecialHandlingMessageForMultipleLines
                                    no label
                                    display as tag
                                        color of yellow
                                button of "<SpecialHandlingMessageForListMultipleLines>"
                                    link is SpecialHandlingNav
                                    display as link
                    column2
                        WarehouseShipment
                            label is "ShipmentNumber"
                        Status
                    column3
                        WarehouseDemand.DemandSystemCode
                            label is "SystemCode"
                        WarehouseDemand.DemandDocument
                            label is "Document"
                    column4
                        visible when (InventoryLocation.UseAsWarehouseLocation)
                            WarehouseWave
                                label is "Wave"
                            WarehouseZone
                                label is "Zone"
