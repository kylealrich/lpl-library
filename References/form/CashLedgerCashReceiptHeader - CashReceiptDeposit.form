CashReceiptDeposit is a CompositeForm
    is primary
    title is "CashReceipt<CashLedgerCashReceiptHeader.TransactionNumber>"
    context form is CashReceiptDepositContext
        background is theme color
    Actions
        action is Release
            label is "Release"
            hide when invalid
        action is SubmitForApproval
            label is "SubmitForApproval"
            hide when invalid
        action is ApplyAdditionalCashAllocations
            label is "ApplyAdditionalCashAllocations"
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is save
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    Main is a Panel
        form is MainForm
    Comment is a Panel
        title is "Comments"
        form is Comment
    CurrencyAmounts is a Panel
        title is "CurrencyAmounts"
        form is CurrencyAmountsForm
    TransactionData is a Panel
        form is TransactionData
    UserFieldsPanel is a Panel
        title is "UserFields"
        form is UserFieldForm
    CashReceiptDetail is a Panel
        title is "ReceiptDetail"
        list is CashLedgerCashReceiptDetail set.ListCashLedgerCashReceiptDetail
    DetailsNotImported is a Panel
        visible when (ImportInProcess)
        list is CashLedgerReceiptImportInErrorRel.primary
    ApprovalComments is a Panel
        visible when (HasApprovalComments)
        list is CashLedgerReceiptReasonCodeUsage set.CashLedgerReceiptReasonCodeUsageList
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
    DistributionList is a Panel
        title is "Distributions"
        visible when (HasDistributions)
        list is CashLedgerGLDistributionsRel.ReceiptGLDistributionList
    Errors is a Panel
        title is "Errors<DerivedCashReceiptErrorCount>"
        visible when (CashReceiptErrorsExist)
        list is CashLedgerTransactionError set.CashLedgerCashReceiptHeaderErrorList
