MobileCashCodeForm is a CompositeForm
    title is "CashCode"
    context form is MobileCashCodeContext
    show panel control on left
    Detail is a Panel
        form is MobileCashCodeDetail
    FundAmounts is a Panel
        visible when (FundAccountingNoEdit)
        list is CashCodeFund set.FundAmountsList
    FundAmountsWithEdit is a Panel
        title is "FundAmounts"
        visible when (EditAvailableFunds)
        list is CashCodeFund set.FundAmountsGroupEditList
    TransactionCodes is a Panel
        list is CashCodeTransactionCode set.MobileTransactionCodeList
    ManualCheckReservation is a Panel
        visible when (CashLedgerPayablesPaymentsManualFormsReservedRelExists)
        list is CashLedgerPayablesPaymentsManualFormsReservedRel.ManualCheckReservationList
