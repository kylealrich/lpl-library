FranchiseSalesInterface is an ActionForm
    action is FranchiseSalesInterface
    Layout
        paragraph
            Company
            Company.BillingCompany.Name
                no label
                display as text
            text of "Or"
                bold
            paragraph
                CompanyGroup
                    label is "CompanyGroup"
                CompanyGroup.Description
                    no label
                    display as text
        paragraph
            UpdateOption
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                FranchiseSalesInterfaceReport_RDG
                FranchiseSalesInterfaceAuditListing_RDG
            column2 of 25%
                FranchiseSalesInterfaceReport_XPT
                FranchiseSalesInterfaceAuditListing_XPT
