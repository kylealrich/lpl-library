BillOfExchangeGeneration is an ActionForm
    action is BillOfExchangeGeneration
    Layout
        two column distributed
            column1
                VendorGroup
                CashCode
                DueDate
                    label is "DueDateCutoff"
            column2
                paragraph
                    PayGroup
                        select is PayGroups
                    PayGroup.Name
                        no label
                        display as text
                DraftDate
                    label is "Bills_ofExchangeDate"
                DueDateBreak
                    label is "Break_onDueDate"
        header2 of "InvoiceSelectionOptions"
        two column distributed
            column1
                paragraph
                    InvCompany
                        label is "InvoiceCompany"
                        select is Companies
                    InvCompany.Name
                        no label
                        display as text
                VenClass
                    label is "VendorClass"
                    select is VendorClass
                BankInstCode
                    label is "PaymentCode"
                    select is BOEPayments
                PayImmFlag
                    label is "PayImmediatelyOnly"
            column2
                ProcLevel
                    label is "ProcessLevel"
                    select is ProcessLevels
                InvoiceGroup
                SchdCashCode
                    label is "ScheduledCashCode"
                UpdateOption
                    label is "Update"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                BillOfExchangeGenerationReport_RDG
            column2 of 25%
                BillOfExchangeGenerationReport_XPT
