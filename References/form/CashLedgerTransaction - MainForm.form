MainForm is a Form
    Layout
        two column distributed
            visible when (not RecordExists)
                column1
                    paragraph
                        CashCode
                            when value changed
                                refresh AutoNumberingAllowed
                        CashCode.Description
                            no label
                            display as text
                        CashCode.Currency
                            no label
                            display as text
                    paragraph
                        BankTransactionCode
                            label is "TransactionCode"
                            when value changed
                                refresh AutoNumberingAllowed
                        BankTransactionCode.Description
                            no label
                            display as text
                        CashLedgerTransactionIdentifier
                            display as text
                    paragraph
                        CurrencyTable
                        CurrencyTable.Description
                            no label
                            display as text
                column2
                    paragraph
                        TransactionNumber
                            display as required when (not AutoNumberingAllowed)
                        Status
                            display as text
                        visible when (IsUnreleasedImportedTran)
                            ImportStatus
                        visible when (IsUnreleasedImportedTranWithMessage)
                            ImportErrorMessage
                                label is "ImportError"
                        visible when (Status.PendingApproval)
                            ApprovalRequestType
                                label is "ApprovalType"
                        visible when (IsUnreleasedCashLedgerReconciliationAdjustmentExist)
                            text of "UnreleasedAdjustmentExists"
                        visible when (LastElectronicPaymentSerialNumber entered)
                            LastElectronicPaymentSerialNumber
                                label is "ElectronicPaymentSerialNumber"
                    paragraph
                        Company.OrigCompany
                            select is Company
                            display as required when (not RecordExists)
                            when value changed
                                refresh AutoNumberingAllowed
                        Company.OrigCompany.Name
                            no label
                            display as text
                        Company.OrigCompany.Currency
                            no label
                            display as text
                    paragraph
                        visible when (IsSourceCodeAP)
                            IssuedBankAmount
                                label is "Amount"
                        visible when (not IsSourceCodeAP)
                            IssuedBankAmountUnsigned
                                label is "Amount"
                        ReconciliationData.CurrencyAmount
                            label is "ReconciledAmount"
                            display as text
                        visible when (VarianceExists)
                            DerivedBankDifferenceAmount
                                label is "VarianceAmount"
                                display as text
                        visible when (CanSeeDistributionTotals)
                            AppliedDistributionBankAmount
                                label is "AppliedAmount"
                                display as text
                            BalanceAmount
                                label is "Balance"
                                display as text
                        visible when (IsBatchApproved)
                            CashLedgerBatchApproval
        two column distributed
            visible when (UnreleasedRecordExists)
                column1
                    TransactionNumber
                    paragraph
                        CurrencyTable
                        CurrencyTable.Description
                            no label
                            display as text
                column2
                    paragraph
                        visible when (IsSourceCodeAP)
                            IssuedBankAmount
                                label is "Amount"
                        visible when (not IsSourceCodeAP)
                            IssuedBankAmountUnsigned
                                label is "Amount"
                        ReconciliationData.CurrencyAmount
                            label is "ReconciledAmount"
                            display as text
                        visible when (VarianceExists)
                            DerivedBankDifferenceAmount
                                label is "VarianceAmount"
                                display as text
                        visible when (CanSeeDistributionTotals)
                            AppliedDistributionBankAmount
                                label is "AppliedAmount"
                                display as text
                            BalanceAmount
                                label is "Balance"
                                display as text
                    paragraph
                        Company.OrigCompany
                            select is Company
                        Company.OrigCompany.Name
                            no label
                            display as text
                        Company.OrigCompany.Currency
                            no label
                            display as text
                        visible when (IsBatchApproved)
                            CashLedgerBatchApproval
        two column distributed
            column1
                Description
                paragraph
                    IssueDate
                        label is "TransactionDate"
                    visible when (ReconcileDateExists)
                        ReconciliationData.ExchangeDate
                            label is "<DerivedReconcileDateMessage>"
                            display as text
                Source
                    label is "SourceType"
                CashLedgerProcessLevel
                paragraph
                    visible when (HasTransactionAttachment)
                        TransactionAttachment.Title
                            label is "TransactionAttachment"
                    TransactionAttachment.File
            column2
                Reference
                GeneralLedgerPostDate
                    label is "PostDate"
                GeneralLedgerEvent
                    label is "GlobalLedgerEvent"
                paragraph
                    visible when (HasVoidStopPayAttachment)
                        VoidStopPayAttachment.Title
                            label is "VoidStopPayAttachment"
                        VoidStopPayAttachment.File
                paragraph
                    visible when (not CanApplyAdditionalAllocations)
                        UseCashAllocationCode
                            label is "ApplyCashAllocationCode"
                visible when (UseCashAllocationCode)
                    paragraph
                        TransientCashAllocationCode
                            label is "CashAllocationCode"
                    paragraph
                        TransientCashAllocationAmount
                            label is "CashAllocationAmount"
        blank line
        header3 of "PaymentInformation"
        two column distributed
            column1
                PayeeName
                    label is "PayeeNameOrLast"
                PayeeFirstName
                PayeeMiddleName
                MailingName
                visible when (not IsSourceCodeAP)
                    PayeePostalAddress
                        form is PostalAddressMinimumWithType
                visible when (IsSourceCodeAP)
                    display as text
                        visible when (DerivedCountryEntered)
                            DerivedPayeeAddressCountry
                                label is "Country/\\Jurisdiction"
                        visible when (DerivedAddressLine1Entered)
                            DerivedPayeeAddressLine1
                                label is "StreetAddress"
                        visible when (DerivedAddressLine2Entered)
                            DerivedPayeeAddressLine2
                                no label
                        visible when (DerivedAddressLine3Entered)
                            DerivedPayeeAddressLine3
                                no label
                        visible when (DerivedAddressLine4Entered)
                            DerivedPayeeAddressLine4
                                no label
                        visible when (DerivedMunicipalityEntered)
                            DerivedPayeeAddressMunicipality
                                label is "City"
                        visible when (DerivedStateEntered)
                            DerivedPayeeAddressState
                                label is "State/Province"
                        visible when (DerivedPostalEntered)
                            DerivedPayeeAddressPostal
                                label is "PostCode"
            column2
                PayCurrency
                PaymentAmount
                ConfirmationNumber
                paragraph
                    BankAccountType
                    GlobalBankAccountType
