InvoiceInterface is a CompositeForm
    is primary
    title is "<DocumentInvoiceInterfaceFormTitle>"
    context form is InvoiceInterfaceContext
    Actions
        action is UpdateDocument
            action icon is transfer
        action is UpdateURL
            action icon is copy
        action is InterfaceThisInvoice
            action icon is export
        navigation is PayablesInvoiceNavigation
            action icon is preview
            label is "ViewInterfacedInvoice"
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is Create
            action icon is create
            label is "_"
        action is open
            label is "_"
        action is save
            label is "_"
        action is Delete
            action icon is delete
            label is "_"
        action is search
            label is "_"
        action is refresh
            label is "_"
    Main is a Panel
        form is Main
    OneTimeVendor is a Panel
        form is OneTimeVendor
    AdditionalPurchaseOrders is a Panel
        list is DocumentInterfacePurchaseOrder set.PurchaseOrders
    AdditionalServiceContracts is a Panel
        visible when (ContractEntered)
        list is DocumentInterfaceServiceContract set.Contracts
    Details is a Panel
        list is DocumentInterfaceDetail set.InvoiceDetails
    Distributions is a Panel
        list is DocumentInterfaceDistribution set.InvoiceDistributions
    AddOnCharges is a Panel
        list is DocumentInterfaceAddOnCharge set.AddOnCharges
    Comments is a Panel
        list is DocumentInterfaceComment set.InvoiceComments
    Errors is a Panel
        visible when (RecordInError)
        list is DocumentInterfaceErrorRel.Errors
