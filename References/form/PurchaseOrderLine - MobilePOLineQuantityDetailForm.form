MobilePOLineQuantityDetailForm is a Form
    title is "POLineQuantityDetail"
    Layout
        header3 of "Quantities_\\in<EnteredBuyUOM>"
        display as text
            two column grid
                column1 of 50%
                    Quantity
                        label is "Ordered"
                    MatchedQuantity
                        label is "Matched"
                    OpenToMatchQuantity
                        label is "OpenToMatch"
                    CancelQuantity
                        label is "Canceled"
                    SubstituteQuantity
                        label is "Substitute"
                column2 of 50%
                    ReceivedQuantity
                        label is "Received"
                    MatchedNotReceivedQuantity
                        label is "MatchedNotReceived"
                    ArchivedQuantity
                        label is "Archived"
                    RejectedQuantity
                        label is "Rejected"
                    DerivedVendorReturnLineShipped
                        label is "Returned"
