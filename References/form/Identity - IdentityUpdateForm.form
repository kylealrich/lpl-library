IdentityUpdateForm is a Form
    title is "IdentityDetails"
    Layout
        header3 of "IdentityInformation"
        single column
            Service
                select is PrimaryServiceList
            Identity
            ServiceType
                display only
        visible when (IsDatabaseIdentity)
            single column
                DatabaseIdentityProperties.Password
                    label is "Password"
        visible when (IsFormBasedIdentity)
            single column
                FormBasedIdentityProperties.Password
                    label is "Password"
        visible when (IsMISocketIdentity)
            single column
                MISocketIdentityProperties.Password
                    label is "Password"
        visible when (IsUnixIdentity)
            single column
                UnixIdentityProperties.Password
                    label is "Password"
        visible when (IsWindowsIdentity)
            single column
                WindowsIdentityProperties.DomainUser
                    label is "DomainUser"
                    display only
                WindowsIdentityProperties.Password
                    label is "Password"
        visible when (IsAS400Identity)
            single column
                AS400IdentityProperties.Password
                    label is "Password"
        header3 of "PasswordInformation"
        single column
            PasswordLastSet
                display only
            IsPasswordResetRequired
        blank line
