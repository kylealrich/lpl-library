TransactionObligationGroupingSearchForm is a SearchForm
    Layout
        paragraph
            DueDateRange.Begin
                label is "DueDate"
            text of "-"
            DueDateRange.End
                no label
            TransactionDateRange.Begin
                label is "TransactionDate"
            text of "-"
            TransactionDateRange.End
                no label
        paragraph
            OpenAmountRange.FromAmount
                label is "OpenAmount"
            text of "-"
            OpenAmountRange.ToAmount
                no label
            CurrencyRange.FromCurrency
                label is "Currency"
            text of "-"
            CurrencyRange.ToCurrency
                no label
        paragraph
            BankTransactionCode
                label is "PaymentCode"
            CustomerBillTo
            SelectedForGroupingFilter
