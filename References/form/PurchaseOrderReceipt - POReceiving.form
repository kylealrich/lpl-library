POReceiving is a CompositeForm
    is primary
    title is "PurchaseOrderReceipt<PurchaseOrderReceipt>"
    context form is POReceivingContext
        background is theme color
    restrict action Unreleased.MobileAddReceiveAll
    restrict action Unreleased.MobileCreate
    restrict action Unreleased.MobileCreateReceiptWizard
    restrict action MobileReleaseFromWizard
    Actions
        action is Unreleased.Release
        action is FinalRelease
            hide when invalid
        action is Unreleased.ReleaseAndPrint
            hide when invalid
        action is Unreleased.ReleaseAndPrintNoBackorders
            hide when invalid
        action is Unreleased.ReceiveAll
            hide when invalid
        navigation is UnreleasedBuyerMessages
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is refresh
            label is "_"
    PurchaseOrderLines1 is a Panel
        title is "PurchaseOrderLines"
        visible when (AllExtraColumns)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is PurchaseOrderLineList1
    PurchaseOrderLines2 is a Panel
        title is "PurchaseOrderLines"
        visible when (BinColumn)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is PurchaseOrderLineList2
    PurchaseOrderLines3 is a Panel
        title is "PurchaseOrderLines"
        visible when (RemainingColumn)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is PurchaseOrderLineList3
    PurchaseOrderLines4 is a Panel
        title is "PurchaseOrderLines"
        visible when (CatchWeightColumn)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is PurchaseOrderLineList4
    PurchaseOrderLines5 is a Panel
        title is "PurchaseOrderLines"
        visible when (CatchWeightAndBinTracked)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is PurchaseOrderLineList5
    PurchaseOrderLines6 is a Panel
        title is "PurchaseOrderLines"
        visible when (RemainingAndBinTracked)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is PurchaseOrderLineList6
    PurchaseOrderLines7 is a Panel
        title is "PurchaseOrderLines"
        visible when (RemainingAndCatchWeight)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is PurchaseOrderLineList7
    PurchaseOrderLines8 is a Panel
        title is "PurchaseOrderLines"
        visible when (NoExtraColumns)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is PurchaseOrderLineList8
    Lines1 is a Panel
        title is "ReceiptLines<ReceiptLinesIndicator>"
        visible when (not PurchaseOrderHasCatchWeight)
        list is PurchaseOrderReceiptLine set.POReceiptLineTemplateList1
    Lines2 is a Panel
        title is "ReceiptLines<ReceiptLinesIndicator>"
        visible when (PurchaseOrderHasCatchWeight)
        list is PurchaseOrderReceiptLine set.POReceiptLineTemplateList2
    Receiver is a Panel
        form is Receiver
    Invoice is a Panel
        visible when (DisplayInvoiceAOCHdr)
        form is Invoice
    InvoiceAddOnCost is a Panel
        visible when (DisplayInvoiceAOCHdr)
        list is HeaderReceiptAddOnChargesRel.PurchaseOrderReceiptLineAOCList
    Comments is a Panel
        title is "Comments<CommentCount>"
        list is PurchaseOrderReceiptComment set.primary
