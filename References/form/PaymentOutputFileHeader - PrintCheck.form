PrintCheck is an ActionForm
    action is PrintCheck
    title is "PrintChecks"
    Layout
        paragraph
            PrmPrintAll
        visible when (not PrmPrintAll)
            paragraph
                PrmPaymentOutputFileDetailRange.FromPaymentOutputFileDetail
                text of "-"
                PrmPaymentOutputFileDetailRange.ToPaymentOutputFileDetail
        header3 of "PrinterOption"
        paragraph
            PrmPaymentPrinter
                display only
            PrmPaymentPrinter.Description
                no label
                display as text
            visible when (PrmPaymentPrinter not entered)
                text of "PaymentPrinterNotDefinedInCashPaymentFormat"
                    color of red
        visible when (not CashPaymentFormatRel.RemittanceOption.AlwaysAttached)
            paragraph
                PrmIncludeRemittanceAdvice
        visible when (PrmIncludeRemittanceAdvice)
            paragraph
                PrmRemittanceAdvicePrinter
                    display only
                PrmRemittanceAdvicePrinter.Description
                    no label
                    display as text
                visible when (PrmRemittanceAdvicePrinter not entered)
                    text of "RemittanceAdvicePrinterNotDefinedInCashPaymentFormat"
                        color of red
