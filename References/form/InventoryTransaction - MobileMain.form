MobileMain is a Form
    title is "Main"
    Layout
        paragraph
            visible when (InventoryTransactionType.InventoryIssue)
                FromToCompanyLocation.FromToCompany
                    label is "ToCompany"
                FromToCompanyLocation.FromToCompany.Name
                    no label
                    display as text
        paragraph
            visible when (InventoryTransactionType.InventoryIssue)
                FromToCompanyLocation.RequestingLocation
                    label is "RequestingLocation"
                FromToCompanyLocation.RequestingLocation.Name
                    no label
                    display as text
        paragraph
            visible when (InventoryTransactionType.InventoryTransfer)
                FromToCompanyLocation.FromToCompany
                    label is "ToCompany"
                FromToCompanyLocation.FromToCompany.Name
                    no label
                    display as text
        paragraph
            visible when (InventoryTransactionType.InventoryTransfer)
                FromToCompanyLocation.FromToLocation
                    label is "ToLocation"
                FromToCompanyLocation.FromToLocation.Name
                    no label
                    display as text
        visible when (InventoryTransactionType.InventoryIssue)
            single column
                Company.GeneralLedgerCompany.Currency
                    label is "Currency"
            single column
                TransactionDate
                    label is "IssueDateAndTime"
            single column
                DefaultDistributionAccount
                    compact format
        visible when (InventoryTransactionType.Adjustment)
            single column
                Company.GeneralLedgerCompany.Currency
                    label is "Currency"
            single column
                TransactionDate
                    label is "AdjustmentDateAndTime"
        visible when (InventoryTransactionType.InventoryTransfer)
            single column
                Company.GeneralLedgerCompany.Currency
                    label is "Currency"
            single column
                TransactionDate
                    label is "TransferDateAndTime"
        visible when (InventoryTransactionType.BinTransfer)
            single column
                Company.GeneralLedgerCompany.Currency
                    label is "Currency"
            single column
                TransactionDate
                    label is "TransferDateAndTime"
        single column
            GeneralLedgerDate
                label is "PostDate"
            Reference1
            Reference2
            visible when (InventoryTransactionType.InventoryTransfer)
                IntransitTransfer
                EstimatedDeliveryDate
                NumberOfCartons
                    label is "Cartons"
