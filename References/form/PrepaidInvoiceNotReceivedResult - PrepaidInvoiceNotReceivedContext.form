PrepaidInvoiceNotReceivedContext is a Form
    Layout
        display as text
            two column
                PrmCompany
                    label is "Company"
                PrmVendor
                    label is "Vendor"
            two column
                PrmCompanyGroup
                    label is "CompanyGroup"
                PrmVendorLocation
                    label is "Location"
            single column
                paragraph
                    PrmDateRange.Begin
                        label is "DateRange"
                    text of "-"
                    PrmDateRange.End
                        no label
            two column
                RunDate
                    label is "RunDateAndTime"
