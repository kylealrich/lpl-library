PurchaseTypeHeader is a Form
    title is "PurchaseTypeInformation"
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        two column distributed
            column1
                paragraph
                    CommodityCode
                        compact format
                        select is CommodityCodeSelect
                    CommodityCode.Description
                        no label
                        display as text
                StandardCommentCode
                    label is "CommentCode"
                paragraph
                    PaymentDueDays
                    text of "NumberOfDaysToAddToRebateReportingDueDateForPaymentExpectedDate"
                        italics
            column2
                visible when (not ServicePurchaseType)
                    paragraph
                        FreightTerm
                            label is "FreightTerms"
                        FreightTerm.Description
                            no label
                            display as text
                    paragraph
                        ShipTerm
                            label is "ShipTerms"
                        ShipTerm.Description
                            no label
                            display as text
        visible when (ManufacturerIndirect)
            paragraph
                Vendor.RepresentativeText
                    display as text
                button of "Supplier_andVendorChanges"
                    visible when (VendorChangeExists)
                    link is VendorChangeLink
                button of "ManageSupplier_andVendorChanges"
                    visible when (CanChangeSupplier)
                    link is ManageSupplierAndVendorChanges
                    display as link
            visible when (VendorEntered)
                paragraph
                    ViewVendorOrPurchaseFromLocation
                        label is "Vendor_orPurchaseFromLocation"
                        display as horizontal radio buttons
                    visible when (ViewVendorOrPurchaseFromLocation.PurchaseFrom)
                        ApPoPurchaseFrom.PurchaseFromLocation
                            select is ActivePurchaseFromLocations
                        visible when (HasPoPurchFrom)
                            ApPoPurchFromName
                                no label
                                display as text
                    visible when (ViewVendorOrPurchaseFromLocation.Vendor)
                        ApPoPurchaseFrom.VendorLocation
                            select is NonRemitToActivePurchaseFromLocations
                        visible when (HasApPurchFrom)
                            ApPoPurchFromName
                                no label
                                display as text
        visible when (not HasAPurchaseType)
            visible when (not ManufacturerIndirect)
                paragraph
                    Vendor.RepresentativeText
                        display as text
        visible when (HasAPurchaseType)
            two column distributed
                column1
                    paragraph
                        Vendor.RepresentativeText
                            display as text
                        button of "Supplier_andVendorChanges"
                            visible when (VendorChangeExists)
                            link is VendorChangeLink
                        button of "ManageSupplier_andVendorChanges"
                            visible when (CanChangeSupplier)
                            link is ManageSupplierAndVendorChanges
                            display as link
                    visible when (VendorEntered)
                        paragraph
                            ViewVendorOrPurchaseFromLocation
                                label is "Vendor_orPurchaseFromLocation"
                                display as horizontal radio buttons
                            visible when (ViewVendorOrPurchaseFromLocation.PurchaseFrom)
                                ApPoPurchaseFrom.PurchaseFromLocation
                                    select is ActivePurchaseFromLocations
                                visible when (HasPoPurchFrom)
                                    ApPoPurchFromName
                                        no label
                                        display as text
                            visible when (ViewVendorOrPurchaseFromLocation.Vendor)
                                ApPoPurchaseFrom.VendorLocation
                                    select is ActivePurchaseFromLocations
                                visible when (HasApPurchFrom)
                                    ApPoPurchFromName
                                        no label
                                        display as text
                    visible when (not StandingPurchaseType)
                        paragraph
                            PerOrderMinimumAmount
                            PerOrderMaximumAmount
                    visible when (ContractOrBlanketOrService)
                        NotificationPercent
                    visible when (HasBeenActivated)
                        visible when (MaximumAmountEntered)
                            display as text
                                paragraph
                                    PercentOrdered
                                        label is "PercentOrdered_orInvoiced"
                                    visible when (MaxEditPoAndRq)
                                        PercentRequested
                                        TotalPercent
                                    PercentRemaining
                    paragraph
                        visible when (not ServicePurchaseType)
                            LifetimeEdit
                                label is "LifetimeMaximumEdit"
                    paragraph
                        visible when (not ServicePurchaseType)
                            IncludeAOCInMaxEdits
                                label is "AddOnCharges"
                        IncludeTaxInMaxEdits
                            label is "Tax"
                        text of "For_Maximum_Edit"
                            italics
                    visible when (HasBeenActivated)
                        header2 of "Amounts_toEditAgainstMaximumAmount"
                        paragraph
                            display as text
                                visible when (ServicePurchaseType)
                                    AmountOrdered
                                        label is "Invoiced"
                                visible when (not ServicePurchaseType)
                                    AmountOrdered
                                        label is "Ordered"
                                visible when (MaxEditPoAndRq)
                                    AmountRequested
                                        label is "Requested"
                                visible when (IncludeAOCInMaxEdits)
                                    AddOnChargeAmountOrdered
                                        label is "AddOnCharges"
                                visible when (IncludeTaxInMaxEdits)
                                    TaxAmountOrdered
                                        label is "Tax"
                                MaximumAmountInUse
                                    label is "Total"
                                        bold
                    paragraph
                        visible when (CanUpdateMaximumAmount)
                            MaximumAmount
                        visible when (not CanUpdateMaximumAmount)
                            MaximumAmount
                                display as text
                        visible when (MaximumAmountEntered)
                            AmountRemaining
                                display as text
                column2
                    header2 of "Defaults"
                    paragraph
                        visible when (NonServicePurchaseType)
                            ManufacturerCodeDivision
                                compact format
                            PreferredContract
                    visible when (not UseHeaderCommitments)
                        paragraph
                            DefaultAccount
                                label is "Account"
                                compact format
                        visible when (not ServicePurchaseType)
                            PayablesDistributionCode
                                select is DistributionCodesByPercentage
                        visible when (ServicePurchaseType)
                            PayablesDistributionCode
                                select is DistributionCodesForServiceContracts
                    single column
                        visible when (BlanketOrStanding)
                            BlanketStandingRequestingLocation
                        Priority
                    paragraph
                        TaxCode
                        visible when (not ServicePurchaseType)
                            DefaultTaxable
                                label is "Taxable"
                    single column
                        ConsignCode
                            label is "BillCode"
            two column distributed
                column1
                    header2 of "Miscellaneous"
                    visible when (HasBeenActivated)
                        display as text
                            visible when (MaxEditPoOnly)
                                AmountRequested
                                    label is "RequestedAmount"
                            visible when (not IncludeAOCInMaxEdits)
                                AddOnChargeAmountOrdered
                                    label is "AddOnChargesIncurred"
                            visible when (not IncludeTaxInMaxEdits)
                                TaxAmountOrdered
                                    label is "TaxCharged"
                    paragraph
                        TermsCode
                            label is "PaymentTerms"
                        display as text
                            TermsCode.Description.Desc[1]
                                no label
                            TermsCode.Description.Desc[2]
                                no label
                        visible when (PaymentTermDifferentFromVendor)
                            text of "Warning:PaymentTermsDifferentFromVendorSetup"
                                bold
                                color of orange
                        visible when (PaymentTermDifferentFromPurchaseFrom)
                            text of "Warning:PaymentTermsDifferentFromPurchaseFromLocationSetup"
                                bold
                                color of orange
                        visible when (PaymentTermDifferentFromVendorLocation)
                            text of "Warning:PaymentTermsDifferentFromVendorLocationSetup"
                                bold
                                color of orange
                    visible when (not ServicePurchaseType)
                        paragraph
                            UOMSMustMatch
                            UOMSMustMatchForSpecials
                    paragraph
                        visible when (not ServicePurchaseType)
                            UseContractUOM
                        visible when (ContractAndNotManOrDist)
                            AllowReferenceOfItemsNotOnContract
                    visible when (IsNotManDistOrStanding)
                        LineLevelBreaks
                    paragraph
                        PoUserField1
                            label is "UserField1"
                        PoUserField3
                            label is "UserField2"
                        PoUserField5
                            label is "UserField3"
                    VendorAgreementReference
                    paragraph
                        Stockless
                column2
                    visible when (StandingPurchaseType)
                        header2 of "Standing"
                        two column
                            ScheduleFrequency
                            MaximumReleaseNumber
                        single column
                            DoNotIssue
                        visible when (HasBeenActivated)
                            display as text
                                paragraph
                                    LastReleaseDate
                                    LastPORelease
                    visible when (BlanketPurchaseType)
                        header2 of "Blanket"
                    visible when (ServicePurchaseType)
                        header2 of "Service"
                    visible when (BlanketOrStanding)
                        paragraph
                            visible when (BlanketPurchaseType)
                                LastPORelease
                                    display as text
                            POCode
                    visible when (BlanketOrStandingOrServices)
                        paragraph
                            Buyer
                            Buyer.Employee.PresentationNameSnapshot
                                no label
                                display as text
                        paragraph
                            ProcessLevelGroup.Company
                                label is "ProcessLevelCompany"
                            ProcessLevelGroup.ProcessLevel
                            ProcessLevelGroup.ProcessLevel.Name
                                no label
                                display as text
        display as text
            visible when (ProposedContractAnyStatus)
                header2 of "FromContractProposal"
                two column
                    ProposedContract
                        label is "ProposalStatus"
                    visible when (HasProposalTemplateNumber)
                        paragraph
                            ProposalTemplateNumber
                                label is "FromProposalTemplate"
                            button of "ViewTemplate"
                                link is ProposalTemplateNav
                                display as link
                    visible when (HasProposalTemplate)
                        first TemplateRelatedContractRel.RelatedContractID
                            label is "FromProposalTemplate"
                visible when (ProposedByRequester)
                    paragraph
                        ProposalRequester.Name
                            label is "Requester"
                        button of icon.mail
                            link is "mailto:<ProposalRequester.EmailAddress>"
                visible when (ProposedSupplierNameEntered)
                    text of "ProposedSupplierInformation"
                        bold
                    two column
                        ProposedSupplierName
                        paragraph
                            ProposedTaxID.TaxIdType
                            ProposedTaxID.TaxId
                    paragraph
                        ProposedMailingAddress.Country
                        ProposedMailingAddress.DeliveryAddress.AddressLine1
                            label is "Address"
                        ProposedMailingAddress.Municipality
                        ProposedMailingAddress.StateProvince
                        ProposedMailingAddress.PostalCode
        visible when (ProposedContractAnyStatus)
            paragraph
                visible when (ProposedContract.ProposedSubmitted)
                    ProposalApprover
                        select is ProposalApproverSelect
                visible when (not ProposedContract.ProposedSubmitted)
                    ProposalApprover
                        display as text
                ProposalApprover.Employee.FirstLastName
                    no label
                    display as text
