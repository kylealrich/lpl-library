OAuthSetupForm is a Form
    Layout
        two column independent
            column1
                header2 of "CalculateWebserviceConfigurationParameter"
                visible when (not Vertex90CalcTaxConfigParamForOAuthExist)
                    single column
                        text of "Warning...ConfigurationParameterRecordDoesNotExist"
                            bold
                            color of red
                        button of "CreateCalculateTaxConfigurationParameter"
                            form invoke is Vertex90CalcTaxConfigParamForOAuth
                            display as link
                visible when (Vertex90CalcTaxConfigParamForOAuthExist)
                    single column
                        button of "UpdateCalculateTaxConfigurationParameter"
                            link is NavToCalcTaxWsiConfigForOAuth
                            display as link
                        VertexCalcTax90ConfigParmOAuthRel.ConfigurationParameter.ConfigurationID
                            display as text
                        VertexCalcTax90ConfigParmOAuthRel.ConfigurationParameter.Name
                            display as text
                        VertexCalcTax90ConfigParmOAuthRel.Value
                            display as text
            column2
                header2 of "LookupWebserviceConfigurationParameter"
                visible when (not Vertex90LookupConfigParamForOAuthExist)
                    single column
                        text of "Warning...LookupTaxAreasConfigurationParameterRecordDoesNotExist"
                            bold
                            color of red
                        button of "CreateLookupTaxAreasConfigurationParameter"
                            form invoke is Vertex90LookupConfigParamForOAuth
                            display as link
                visible when (Vertex90LookupConfigParamForOAuthExist)
                    single column
                        button of "UpdateLookupTaxAreasConfigurationParameter"
                            link is NavToLookupWsiConfigForOAuth
                            display as link
                        VertexLookupTaxAreas90RelConfigOAuthRel.ConfigurationParameter.ConfigurationID
                            display as text
                        VertexLookupTaxAreas90RelConfigOAuthRel.ConfigurationParameter.Name
                            display as text
                        VertexLookupTaxAreas90RelConfigOAuthRel.Value
                            display as text
        two column independent
            column1
                header2 of "VertexCalculateTax90OAuthRecord"
                visible when (VertexCalcTaxOAuthRelExist)
                    single column
                        button of "UpdateCalculateTaxO\\AuthRecord"
                            link is NavToCalcTaxOAuth
                            display as link
                        VertexCalcTax90OAuthRel.OAuth
                            display as text
                        VertexCalcTax90OAuthRel.Type
                            display as text
                        VertexCalcTax90OAuthRel.ApplicationKey
                            display as text
                            show up to 120 characters
                        OAuthApplicationSecretCalc
                            display as text
                            show up to 120 characters
                        VertexCalcTax90OAuthRel.ResourceUser
                            display as text
                            show up to 120 characters
                        CalcTaxPasswordHiddenIfEntered
                            label is "OAuthResourcePassword"
                            display as text
                            show up to 120 characters
                        VertexCalcTax90OAuthRel.AccessTokenURL
                            display as text
                            show up to 120 characters
                visible when (not VertexCalcTaxOAuthRelExist)
                    single column
                        button of "CreateVertexCalculateTax90OAuthRecord"
                            form invoke is VertexCalculateTax90OAuthRecord
                            display as link
                        text of "Warning...O_\\AuthRecordDoesNotExist"
                            bold
                            color of red
            column2
                header2 of "VertexLookupTaxAreas90OAuthRecord"
                visible when (VertexLooiupTaxAreasOAuthRelExist)
                    single column
                        button of "UpdateO\\AuthLookupTaxAreasRecord"
                            link is NavToLookupOAuth
                            display as link
                        VertexLookup90OAuthRel.OAuth
                            display as text
                        VertexLookup90OAuthRel.Type
                            display as text
                        VertexLookup90OAuthRel.ApplicationKey
                            display as text
                            show up to 120 characters
                        OAuthApplicationSecretLookup
                            display as text
                            show up to 120 characters
                        VertexLookup90OAuthRel.ResourceUser
                            display as text
                            show up to 120 characters
                        CalcTaxPasswordHiddenIfEntered
                            label is "OAuthResourcePassword"
                            display as text
                            show up to 120 characters
                        VertexLookup90OAuthRel.AccessTokenURL
                            display as text
                            show up to 120 characters
                visible when (not VertexLooiupTaxAreasOAuthRelExist)
                    single column
                        button of "CreateVertexLookupTaxAreas90OAuthRecord"
                            form invoke is VertexLookupTaxAreas90OAuthRecord
                            display as link
                        text of "Warning...O_\\AuthRecordDoesNotExist"
                            bold
                            color of red
