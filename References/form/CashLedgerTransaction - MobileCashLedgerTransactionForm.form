MobileCashLedgerTransactionForm is a CompositeForm
    title is "CashLedgerTransaction"
    context form is MobileCashLedgerTransactionContext
    show panel control on left
    Main is a Panel
        form is MainForm
    Comment is a Panel
        title is "Comments"
        form is Comment
    TransactionData is a Panel
        form is TransactionData
    CurrencyAmounts is a Panel
        title is "CurrencyAmounts"
        form is MobileCurrencyAmountsForm
    UserFieldsPanel is a Panel
        title is "UserFields"
        form is UserFieldForm
    BankInformation is a Panel
        form is BankInformation
    CashLedgerGLDistribution is a Panel
        title is "Distributions"
        visible when (DistributionsAreApplicable)
        list is CashLedgerGLDistributionsRel.TransactionGLDistributionList
    PaymentRecords is a Panel
        visible when (CanViewPaymentRecords)
        list is CashLedgerPayablesPaymentsPayGroupRel.PaymentRejectionList
    EFTExtractRecords is a Panel
        visible when (IsElectroniFundsExtractType)
        list is CashLedgerElectronicFundsTransferTransactionRel.EFTPaymentRejectionList
    VarianceAdjustments is a Panel
        title is "VarianceAdjustments"
        visible when (IsCashLedgerReconciliationAdjustmentExist)
        list is CashLedgerReconciliationAdjustmentRel.primary
    ApprovalComments is a Panel
        visible when (HasApprovalComments)
        list is CashLedgerTransactionReasonCodeUsage set.CashLedgerTransactionReasonCodeUsageList
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
    BudgetErrors is a Panel
        title is "BudgetErrors<DerivedHeaderBudgetErrorCount>"
        visible when (HeaderBudgetEditErrorExists)
        list is HeaderBudgetEditErrorRel.primary
    WarningBudgetErrors is a Panel
        title is "WarningBudgetErrors<DerivedWarningBudgetErrorCount>"
        visible when (DistributionBudgetEditErrorExists)
        list is DistributionBudgetEditErrorRel.primary
    Errors is a Panel
        title is "Errors<DerivedCashLedgerTransactionErrorCount>"
        visible when (CashLedgerTransactionErrorsExist)
        list is CashLedgerTransactionError set.CashLedgerTransactionErrorList
