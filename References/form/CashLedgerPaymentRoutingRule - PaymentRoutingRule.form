PaymentRoutingRule is a Form
    is primary
    title is "PaymentRoutingRule"
    Layout
        paragraph
            CashManagementGroup
            CashManagementGroup.Description
                no label
                display as text
        single column
            Name
            Description
        paragraph
            ApproverAssignmentRule
            ApproverAssignmentRule.Description
                no label
                display as text
        single column
            ApproverAssignmentRule.Condition
                no label
                display as text
            blank line
        paragraph
            ApprovalCode
                select is CashLedgerPaymentApprovalCodes
            ApprovalCode.Description
                no label
                display as text
        single column
            Active
