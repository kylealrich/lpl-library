CartCompositeForm is a CompositeForm
    title is "ItemsInCart_(<CurrentRequisitionRel.NumberOfLines>)"
    called out actions only
    context form is CartForm
    implements CartView
        cart is actor.agent(Employee).RequesterRel.RequisitionRel.Requisition
    Actions
        navigation is RSSLPLRequisitionNav
            action icon is edit
            label is "_"
            mouse over text is "Update_Requisition"
            hide when invalid
            valid when (actor.agent(Employee).Employee.Requester.CurrentRequisitionForRQC entered)
        navigation is RSSLPLRequisitionComments
            action icon is attach
            label is "_"
            mouse over text is "Header_Notes"
            hide when invalid
        action is IssueDraft
            action icon is print
            label is "_"
            mouse over text is "Print_Draft"
            hide when invalid
            valid when (actor.agent(Employee).Employee.Requester.CurrentRequisitionForRQC entered)
        action is Unreleased.RSSLPLDelete
            action icon is delete
            label is "_"
            hide when invalid
            valid when (actor.agent(Employee).Employee.Requester.CurrentRequisitionForRQC entered)
        action is Unreleased.RSSLPLCreate
            action icon is create
            label is "_"
            mouse over text is "StartA_New_Requisition"
            hide when invalid
            valid when (actor.agent(Employee).Employee.Requester.CurrentRequisitionForRQC entered)
        action is refresh
            label is "_"
    CartItems is a Panel
        visible when (actor.agent(Employee).Employee.Requester.CurrentRequisitionForRQC entered)
        list is RequisitionLinesRel.RSSLPLReqLinesCardList
    EmptyCart is a Panel
        visible when (actor.agent(Employee).Employee.Requester.CurrentRequisitionForRQC not entered)
        form is EmptyCartForm
