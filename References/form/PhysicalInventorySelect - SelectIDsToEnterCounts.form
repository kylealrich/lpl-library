SelectIDsToEnterCounts is a CompositeForm
    title is "EnterCounts"
    context form is SelectIDsFormContext
        background is theme color
    CountsByPage is a Panel
        title is "EnterCountsByPage"
        visible when (IsSelectIdInUseOrCountInProgressWithoutCatchWeight)
        list is PhysicalInventoryFreezesRel.LinesByPage
    CountsByPageWithCatchWeight is a Panel
        title is "EnterCountsByPage"
        visible when (IsSelectIdInUseOrCountInProgressWithCatchWeight)
        list is PhysicalInventoryFreezesRel.LinesByPageWithCatchWeight
    CountsByItem is a Panel
        title is "EnterCountsByItem"
        visible when (IsSelectIdInUseOrCountInProgressWithoutCatchWeight)
        list is PhysicalInventoryFreezesRel.LinesByItem
    CountsByItemWithCatchWeight is a Panel
        title is "EnterCountsByItem"
        visible when (IsSelectIdInUseOrCountInProgressWithCatchWeight)
        list is PhysicalInventoryFreezesRel.LinesByItemWithCatchWeight
    CountsByPhysicalInventoryBin is a Panel
        title is "EnterCountsByPhysicalInventoryBin"
        visible when (PhysicalInventoryBinInUseOrCountInProgress)
        list is PhysicalInventoryFreezeBinRel.LinesByPhysicalInventoryBin
