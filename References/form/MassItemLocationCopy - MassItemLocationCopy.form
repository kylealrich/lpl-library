MassItemLocationCopy is an ActionForm
    action is MassItemLocationCopy
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        two column
            CopyUsrfldsFl
                label is "CopyUserFields"
            ReportOnlyFl
                label is "ReportOnly"
        header2 of "CopyAllItemsFromLocationA_toLocationB"
        blank line
        paragraph
            LocationA
            LocationA.Name
                no label
                display as text
        paragraph
            NewCompany
                label is "CopyToCompany"
            NewCompany.Name
                no label
                display as text
        paragraph
            LocationB
            LocationB.Name
                no label
                display as text
        paragraph
            GlCategoryA
                label is "GLCategory_forCopiedLocation"
            GlCategoryA.Description
                no label
                display as text
        single column
            text of "\\or"
                bold
        header2 of "CopyItemA_toEverywhereItemBExists"
        blank line
        paragraph
            LocationA2
                label is "LocationA"
            LocationA2.Name
                no label
                display as text
        paragraph
            ItemA
            display as text
                ItemA.Description
                    no label
                ItemA.Description2
                    no label
        paragraph
            ItemB
            display as text
                ItemB.Description
                    no label
                ItemB.Description2
                    no label
        paragraph
            GlCategoryA2
                label is "GLCategory_forCopiedItems"
            GlCategoryA2.Description
                no label
                display as text
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                MassItemLocationCopyReport_RDG
            column2 of 25%
                MassItemLocationCopyReport_XPT
