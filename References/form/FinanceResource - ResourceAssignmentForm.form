ResourceAssignmentForm is a CompositeForm
    title is "<EmployeeRel.Name.PreferredFirstAndLastName>'sAssignedActivity"
    context form is FinanceResourceAdminContextForm
        background is theme color
    InvoicesForProcessor is a Panel
        title is "Invoices_forProcessor"
        visible when (AssignedAsAPClerkToInvoice)
        list is APClerkPayablesInvoiceRel.XiFinanceResourcePayablesInvoiceList
    InvoicesForApprover is a Panel
        title is "Invoices_forApprover"
        visible when (AssignedAsResponsibleToInvoice)
        list is ResponsiblePayablesInvoiceRel.XiFinanceResourcePayablesInvoiceList
    InvoiceAssignmentRules is a Panel
        visible when (AssignedToInvoiceAssignmentRules)
        list is InvoiceAssignmentRulesRel.AssignmentRulesForResource
    InvoiceApprovalCodes is a Panel
        visible when (AssignedToInvoiceRoutingCode)
        list is InvoiceRoutingCodeResourcesRel.InvoiceApproversWithRoutingCode
    ApprovalCodes is a Panel
        title is "OtherApprovalCodes"
        visible when (AssignedToApprovalCodes)
        list is ApprovalCodeResourceRel.ApproversForApprovalCode
    CloseProcess is a Panel
        visible when (ShowCloseTask)
        list is TheCloseProcessRel.ResourceCloseTaskList
    ScheduledTasks is a Panel
        visible when (ShowClosePeriodTask)
        list is OpenScheduledTaskRel.ResourceClosePeriodTaskList
    ReconciliationAccounts is a Panel
        visible when (ShowMyReconciliationAccounts)
        list is MyReconciliationAccountsRel.CurrentTransactionReconList
    PeriodReconciliations is a Panel
        visible when (ShowMyPeriodEndReconciliations)
        list is MyPeriodEndReconciliationsRel.PeriodEndReconciliationList
    Teams is a Panel
        list is TeamMembershipRel.MyTeams
