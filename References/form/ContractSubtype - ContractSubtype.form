ContractSubtype is a Form
    is primary
    title is "ContractSubtype"
    Layout
        paragraph
            ContractGroup
            ContractGroup.ProcurementGroup.Description
                no label
                display as text
        single column
            ContractType
                select is BuyerContractTypesSelect
            ContractType.Description
                no label
                display as text
            ContractSubtype
            Description
            Active
            PurchaseType
                label is "VendorPurchaseType"
            visible when (PurchaseType.Contract)
                CanUseForCostDefaultingOptions
                visible when (CanUseForCostDefaultingOptions.UseAllItems)
                    SpecialItemOptions
                visible when (SpecialItemOptions.CreateItemOnActivation)
                    ItemCreationMethod
                    visible when (ItemCreationMethod.Assign)
                        AssignItemNumberUsing
            visible when (PurchaseType.Blanket)
                CanUseForCostDefaultingOptions
                visible when (CanUseForCostDefaultingOptions.UseAllItems)
                    SpecialItemOptions
                visible when (SpecialItemOptions.CreateItemOnActivation)
                    ItemCreationMethod
                    visible when (ItemCreationMethod.Assign)
                        AssignItemNumberUsing
            visible when (PurchaseType.CatalogQuote)
                CanUseForCostDefaultingOptions
                visible when (CanUseForCostDefaultingOptions.UseAllItems)
                    SpecialItemOptions
                visible when (SpecialItemOptions.CreateItemOnActivation)
                    ItemCreationMethod
                    visible when (ItemCreationMethod.Assign)
                        AssignItemNumberUsing
            visible when (PurchaseType.Standing)
                CanUseForCostDefaultingOptions
                visible when (CanUseForCostDefaultingOptions.UseAllItems)
                    SpecialItemOptions
                visible when (SpecialItemOptions.CreateItemOnActivation)
                    ItemCreationMethod
                    visible when (ItemCreationMethod.Assign)
                        AssignItemNumberUsing
            visible when (PurchaseType.Service)
                AllowNoMaxServiceItems
