CompareSetup is an ActionForm
    action is MainCompareSetup
    title is "CompareSetup"
    Layout
        single column
            PDFOrientation
                display as horizontal radio buttons
            Description
                label is "BaseSetup"
                display as text
            CompareOptions
                display as horizontal radio buttons
            visible when (CompareOptions.SetupExport)
                SetupTextFile
                    no label
                Separator
                    display as required
            visible when (CompareOptions.OtherSetup)
                paragraph
                    OtherSetup
                        no label
                        select is FileCreationSetupByExportFileRel.FileCreationSetupSelect
                    OtherSetup.Description
                        no label
                        display as text
