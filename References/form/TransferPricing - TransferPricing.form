TransferPricing is a CompositeForm
    is primary
    title is "<TransferPricingTitle>"
    context form is TransferPricingContext
    Actions
        action is CalculateRecharges
            hide when invalid
        action is CopyRecharge
            hide when invalid
        action is GenerateWeights
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "Previous"
        action is next record
            label is "_"
            mouse over text is "Next"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is save
            action icon is save
            label is "_"
            mouse over text is "Save"
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
            mouse over text is "Refresh"
    Main is a Panel
        form is Main
    Mapping is a Panel
        title is "BillingDocument"
        form is Mapping
    AllEntities is a Panel
        title is "BillToEntities"
        visible when (AllEntities)
        template is TransferPricingGlobalEntitiesRel
            business class is TransferPricingEntity
                list is TemplateTransferPricingEntityList
    BillToEntities is a Panel
        visible when (not AllEntities)
        list is TransferPricingEntity set.TransferPricingEntityList
            helper list is TransferPricingGlobalEntitiesRel.TransferPricingGlobalEntityHelperList
                action is AddBillToEntity
    Sources is a Panel
        list is TransferPricingSource set.primary
    Weights is a Panel
        visible when (RechargeSplitUsingHeaderExists)
        list is TransferPricingWeightRel.primary
    Structures is a Panel
        form is Structures
    Calculations is a Panel
        list is TransferPricingRunRel.primary
