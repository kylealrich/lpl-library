AssetOptions is a Form
    Layout
        single column
            AssetCategory
        paragraph
            AssetAccountGroup
            AssetAccountGroup.Description
                no label
                display as text
        paragraph
            AssetAccountingUnitGroup
            AssetAccountingUnitGroup.Description
                no label
                display as text
        visible when (AseetGuidelineClassesExists)
            paragraph
                AssetGuidelineClass
                    label is "GuidelineClass"
                AssetGuidelineClass.Description
                    no label
                    display as text
        paragraph
            AssetLocation
                label is "Location"
            AssetLocation.Description
                no label
                display as text
        paragraph
            AssetDivision
                label is "Division"
                select is Divisions
            AssetDivision.Description
                no label
                display as text
        blank line
        paragraph
            AccountingUnit
                compact format
        paragraph
            TransactionDimensionDefault
        paragraph
            Project
            Project.Description
                no label
                display as text
        paragraph
            DepreciationExpenseProject.Project
                label is "Depreciation_Expense_Project"
            DepreciationExpenseProject.Project.Description
                no label
                display as text
        blank line
        two column
            WorkInProcess
            TaxExempt
            Simulated
            Used
