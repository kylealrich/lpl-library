PurchaseOrderDocumentForm is a Form
    is primary
    title is "PurchaseOrderDocument"
    Layout
        single column
            display as text
                visible when (DocumentByCompany)
                    Company
                visible when (DocumentByVendor)
                    VendorGroup
                    paragraph
                        Vendor
                        PurchaseFromLocation
            PurchaseOrderDocument
                display only
            PurchaseOrderDocumentTemplate
                compact format
            Active
