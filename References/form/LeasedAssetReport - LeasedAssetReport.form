LeasedAssetReport is an ActionForm
    action is LeasedAssetReport
    Layout
        header2 of "SelectionOptions"
        paragraph
            Book
                label is "Book"
            Book.Description
                no label
                display as text
            PrelimClose
                label is "PreliminaryClose"
        paragraph
            FromDate
                label is "FromInServiceDate"
            ToDate
                label is "ToInServiceDate"
        paragraph
            AstCompany
                label is "Company"
                select is Companies
            AstCompany.CompanyName
                no label
                display as text
            text of "Or"
            CompanyGroup
                label is "CompanyGroup"
            CompanyGroup.Description
                no label
                display as text
        single column
            Classification
        paragraph
            LocationName
                label is "Location"
                select is Locations
            LocationName.Description
                no label
                display as text
        paragraph
            Division
                select is Divisions
            Division.Description
                no label
                display as text
        paragraph
            AssetType
                label is "Type"
                compact format
                select is AssetTypesWithinCompany
            AssetType.Description
                no label
                display as text
        paragraph
            Property
            Category
        single column
            Disposals
        header2 of "AdditionalOptions"
        paragraph
            FullyDepr
                label is "FullyDepreciated"
            Simulated
                label is "TaxExempt"
        paragraph
            TaxExempt
                label is "Simulated"
            Used
        paragraph
            Asset
                select is AllAssets
            Asset.Description
                no label
                display as text
        blank line
        paragraph
            AssetLease.LeaseCompany
                label is "OrCompany"
            AssetLease.LeaseCompany.Name
                no label
                display as text
            AssetLease.Lease
                label is "Lease"
        single column
            TagNumber
                label is "Tag"
        paragraph
            AssetGroup
                select is AssetGroups
            AssetGroup.Description
                no label
                display as text
        paragraph
            TransactionDimension
                label is "TransactionDimensions_orAssetDimensionGroup"
                compact format
            AstAuGroup
                no label
            AstAuGroup.Description
                no label
                display as text
        blank line
        header2 of "OutputOptions"
        single column
            PageBreak
        single column
            header3 of "PrintTotals"
        single column
            header4 of "Sequence"
            blank line
            paragraph
                LeaseOption
                    label is "Lease"
            paragraph
                LocOption
                    label is "Location"
            paragraph
                DivOption
                    label is "Division"
            paragraph
                TypeOption
                    label is "Type\\,Sub"
            paragraph
                AstgrpOption
                    label is "AssetGroup"
            paragraph
                AssetOption
                    label is "Asset"
            header4 of "Miscellaneous"
            blank line
            paragraph
                CatOption
                    label is "Category"
            paragraph
                PropOption
                    label is "Property"
            paragraph
                InservOption
                    label is "In\\-\\Service"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                LeasedAssetReportReport_RDG
            column2 of 25%
                LeasedAssetReportReport_XPT
