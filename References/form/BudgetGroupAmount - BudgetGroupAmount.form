BudgetGroupAmount is a Form
    is primary
    title is "BudgetGroupAmount"
    Layout
        paragraph
            GeneralLedgerCalendarPeriod
                label is "Period"
            CurrencyAmount
                label is "BudgetAmount"
            visible when (BudgetTemplate.IsYearToDate)
                YTD
                    label is YearToDate
            visible when (BudgetTemplate.IsLifeToDate)
                LTD
                    label is LifeToDate
