BillingInvoiceImportCommentsForm is a Form
    is primary
    title is "BillingInvoiceImportComments"
    Layout
        paragraph
            Company
                display as text
        visible when (not ContextBillingInvoiceImport.IsManualInvoiceNumbering)
            paragraph
                InvcPrefix
                    label is "InvoicePrefix"
                    display as text
                InvoiceNumber
                    display as text
        visible when (ContextBillingInvoiceImport.IsManualInvoiceNumbering)
            paragraph
                ContextBillingInvoiceImport.GlobalInvoiceNumber
                    label is "Invoice"
                    display as text
        paragraph
            visible when (LineNumber entered)
                LineNumber
                    display as text
        paragraph
            Sequence3
                label is "Sequence"
                display as text
        paragraph
            PrintCode
        paragraph
            Comment
