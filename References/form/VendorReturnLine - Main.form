Main is a Form
    Layout
        two column distributed
            column1
                ItemType
            column2
                visible when (ItemType.NonStock)
                    paragraph
                        Item
                        Item.Description
                            no label
                            display as text
                visible when (ItemType.Inventoried)
                    paragraph
                        Item
                        Item.Description
                            no label
                            display as text
                visible when (ItemType.Special)
                    paragraph
                        Item
                            label is "SpecialItem"
                        Description
                            label is "Description"
        two column distributed
            paragraph
                ReturnQuantity
                    label is "ReturnQuantity"
                EnteredUOM
                    label is "UOM"
                    select is ValidBuyUnitsOfMeasure
                EnteredUOM.Description
                    no label
                    display as text
            paragraph
                visible when (not Item.CatchWeightCode.NotCatchWeight)
                    CatchWeightQuantity
                        label is "CatchweightStockQuantity"
                    Item.StockUOM
                        no label
                        display as text
            paragraph
                CancelQuantity
                    display as text
                EnteredUOM
                    no label
                    display as text
            paragraph
                visible when (not Item.CatchWeightCode.NotCatchWeight)
                    CatchWeightCancelQuantity
                        label is "CancelCatchweightQuantity"
                        display as text
                    Item.StockUOM
                        no label
                        display as text
        two column distributed
            paragraph
                visible when (not VendorReturn.ShippedStatus)
                    UnitCost
                visible when (VendorReturn.ShippedStatus)
                    DerivedDisplayUnitCost
                        display only
            ReturnStockToInventoryOnVendorReturnCancel
        visible when (InventoryItem)
            paragraph
                ItemLocationRel.GeneralLedgerCategory.InventoryAccount
                    label is "DistributionAccount"
                    compact format
        header2 of "PurchaseOrder"
        two column distributed
            paragraph
                VendorItem
                    select is VendorItemsForVendor
                VendorItem.VendorItemDescription
                    no label
                    display as text
            ItemGTIN
                label is "G\\T\\I\\N"
                select is ActiveGTINNumbersForAnItem
        display as text
            two column distributed
                TaxCode
                paragraph
                    BuyUOMQuantity
                    BuyUOM
                        no label
            two column distributed
                paragraph
                    OriginalPurchaseOrder
                    OriginalPurchaseOrderLine.LineNumber
                        label is "Line"
                paragraph
                    Contract
                    ContractLine
                        label is "Line"
        visible when (IsHSNSACCodeEnabled)
            paragraph
                HSNSACCode
                HSNSACCode.Description
                    no label
                    display as text
        header2 of "Intrastat"
        three column distributed
            ICNCode
                select is IntrastatICNCodes
                show up to 10 characters
            paragraph
                SupplementaryQuantity
                ICNCode.SupplementaryUOM
                    no label
                    display as text
            StockWeight
