ClosedTransactionDetailsForm is a CompositeForm
    title is "TransactionDetails"
    context form is ClosedTransactionDetailsContextForm
    Receipts is a Panel
        visible when (HasClosedReceiptDetails)
        list is ClosedReceiptDetailRel.ReceiptDetailBySchoolList
    BankReceipts is a Panel
        visible when (HasClosedBankReceiptDetails)
        list is ClosedBankReceiptDetailRel.BankReceiptDetailBySchoolList
    Disbursements is a Panel
        visible when (HasClosedDisbursementDetails)
        list is ClosedDisbursementDetailRel.DisbursementDetailBySchoolList
    Transfers is a Panel
        visible when (HasClosedTransferDetails)
        list is ClosedTransferDetailRel.TransferDetailBySchoolList
    BankTransfers is a Panel
        visible when (HasClosedBankTransferDetails)
        list is ClosedBankTransferDetailRel.BankTransferDetailBySchoolList
