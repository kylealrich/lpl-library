Options is a Form
    Layout
        three column distributed
            InvoiceType
            HoldCode
                select is HoldOnInvoiceList
            ReasonCode
        three column distributed
            InvoiceReference
            APClerk
                label is "Processor"
            paragraph
                CashCode
                    show up to 12 characters
                DiscountLost
        three column distributed
            ApprovalCostCenter
            ApprovalCategory
            DiversityCode
        three column distributed
            AccrualCode
                label is "InvoiceAccrualCode"
            NumberOfRecurringInvoices
                label is "RecurrenceCount"
            RecurringFrequency
                label is "RecurrenceSchedule"
        three column distributed
            PurchaseFromLocation
                label is "PurchaseFrom"
                select is PurchaseFromLocationSelectRel.PurchaseFromLocations
            IncomeCode
            ReportableIncomeAmount
        three column distributed
            PrintChargeback
            ForeignEconomicRegulatoryCode
            IncomeWithholdingCode
                label is "VendorWithholdingCode"
        three column distributed
            ExternalPurchaseOrder
            CertificationNumber
            CertificationDate
        three column distributed
            GarnFileNumber
            GarnSocialSecurityNumber
            GarnCaseNumber
        three column distributed
            GarnFIPSCode
            GarnTerminationIndicator
            GarnMedicalIndicator
        three column distributed
            GarnParentName
