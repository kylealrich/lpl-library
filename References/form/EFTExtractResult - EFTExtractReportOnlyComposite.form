EFTExtractReportOnlyComposite is a CompositeForm
    title is "EFTExtractReportOnly"
    context form is EFTExtractReportOnlyContext
        background is theme color
    Exceptions is a Panel
        visible when (ExceptionsRecordExists)
        list is EFTExtractErrorResultRel.EFTExtractSearchErrorResultList
    CompanyManualAdjustments is a Panel
        title is "ManualAdjustments"
        visible when (ReportOnlyCompanyAdjustments)
        list is EligibleAdjustmentTotalDetailsRel.CompanyManualAdjustmentList
    GroupManualAdjustments is a Panel
        title is "ManualAdjustments"
        visible when (ReportOnlyGroupAdjsutments)
        list is EligibleAdjustmentTotalDetailsRel.GroupManualAdjustmentList
    CompanyEligibleTransactions is a Panel
        title is "Eligible"
        visible when (Company entered)
        list is EFTExtractInvoiceDetailsRel.CompanyEligibleExtractRecordsList
    GroupEligibleTransactions is a Panel
        title is "Eligible"
        visible when (Company not entered)
        list is EFTExtractInvoiceDetailsRel.GroupEligibleExtractRecordsList
    Totals is a Panel
        form is ReportOnlyTotals
