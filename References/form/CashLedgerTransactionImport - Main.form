Main is a Form
    Layout
        two column distributed
            visible when (not RecordExists)
                column1
                    CashManagementGroup
                    paragraph
                        CashLedgerTransactionImport.CashCode
                        CashLedgerTransactionImport.CashCode.Description
                            no label
                            display as text
                        CashLedgerTransactionImport.CashCode.Currency
                            no label
                            display as text
                    paragraph
                        CashLedgerTransactionImport.TransactionNumber
                column2
                    CashLedgerTransactionImport.RunGroup
                        label is "RunGroup"
                    CashLedgerTransactionImport.BankTransactionCode
                    CashLedgerTransactionImport.SequenceNumber
        single column
            Company
                label is "PostingCompany"
                select is Company
        two column distributed
            column1
                IssueDate
                AmountTransactionCurrency
                    label is "IssueTransactionAmount"
                GeneralLedgerSourceCode
                    label is "SystemCode"
                    select is CBSelect
                GeneralLedgerEvent
                Description
                paragraph
                    InterfaceAsPosted
                        label is "InterfaceAsPosted"
                            left of field
                        when value changed
                            refresh BypassCashFundUpdate
                    visible when (InterfaceAsPosted)
                        BypassCashFundUpdate
                PayCurrency
                PayeeName
                    label is "PayeeNameOrLast"
                PayeeFirstName
                PayeeMiddleName
                MailingName
            column2
                PostDate
                IssuedBaseAmount
                    label is "FunctionalAmount"
                Source
                    label is "SourceType"
                Status
                Reference
                blank line
                PaymentAmount
                ConfirmationNumber
                paragraph
                    BankAccountType
                    GlobalBankAccountType
        blank line
        header3 of "PayeeAddress"
        PayeePostalAddress
            form is PostalAddressMinimumWithType
