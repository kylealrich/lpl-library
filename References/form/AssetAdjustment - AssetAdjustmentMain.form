AssetAdjustmentMain is a Form
    title is "Detail"
    Layout
        two column
            column1 of 10%
                text of "Current_Asset_Detail"
                    bold
                Company
                    display as text
                visible when (IsDescriptionAdjusted)
                    Description
                        display as text
                visible when (IsTagAdjusted)
                    TagNumber
                        display as text
                visible when (IsCurrencyAdjusted)
                    Currency
                        display as text
                visible when (IsAssetCategoryAdjusted)
                    AssetCategory
                        display as text
                visible when (IsPhysicalInventoryAdjusted)
                    IncludeInPhysicalInventory
                        display as text
                visible when (IsSimulatedAdjusted)
                    Simulated
                        display as text
                visible when (IsWorkInProcessAdjusted)
                    WorkInProcess
                        display as text
                visible when (IsTaxExemptAdjusted)
                    TaxExempt
                        display as text
                visible when (IsUsedAdjusted)
                    Used
                        display as text
                visible when (IsAssetGroupAdjusted)
                    AssetGroup
                        display as text
                visible when (IsAssetLocationAdjusted)
                    AssetLocation
                        label is "Location"
                        display as text
                visible when (IsAssetDivisionAdjusted)
                    AssetDivision
                        label is "Division"
                        display as text
                visible when (IsAssetTypeAdjusted)
                    AssetType
                        display as text
                        compact format
                visible when (IsAssetGuidelineClassAdjusted)
                    AssetGuidelineClass
                        display as text
                visible when (IsAssetAccountGroupAdjusted)
                    AssetAccountGroup
                        display as text
                visible when (IsAssetAccountingUnitGroupAdjusted)
                    AssetAccountingUnitGroup
                        display as text
                visible when (IsAssetAccountingUnitAdjusted)
                    AssetAccountingUnit
                        label is "DefaultTransactionDimensions"
                        display as text
                        compact format
                visible when (IsAssetSummaryGroupAdjusted)
                    AssetSummaryGroup
                        display as text
                visible when (IsFundOverrideAdjusted)
                    FundOverride
                        display as text
                visible when (IsDepreciationExpenseProjectAdjusted)
                    paragraph
                        text of "Depreciation_Expense"
                            color of gray
                            align as label
                        DepreciationExpenseProject.Project
                            no label
                            display as text
                visible when (IsAssetOwnerAdjusted)
                    paragraph
                        text of "AssetOwner"
                            color of gray
                            align as label
                        AssetOwner
                            no label
                            display as text
                        AssetOwner.Name.FirstAndLastName
                            no label
                            display as text
                visible when (IsReference1Adjusted)
                    Reference1
                        display as text
                visible when (IsReference2Adjusted)
                    Reference2
                        display as text
                visible when (IsGeographicalLatitudeAdjusted)
                    GeographicalLatitude
                        display as text
                visible when (IsGeographicalLongitudeAdjusted)
                    GeographicalLongitude
                        display as text
                visible when (IsGeographicalAltitudeAdjusted)
                    GeographicalAltitude
                        display as text
            column2 of 20%
                text of "New_Asset_Detail"
                    bold
                AssetAdjustmentTo.Company
                    display as text
                visible when (IsDescriptionAdjusted)
                    AssetAdjustmentTo.Description
                        display as text
                visible when (IsTagAdjusted)
                    AssetAdjustmentTo.TagNumber
                        display as text
                visible when (IsCurrencyAdjusted)
                    AssetAdjustmentTo.Currency
                        display as text
                visible when (IsAssetCategoryAdjusted)
                    AssetAdjustmentTo.AssetCategory
                        display as text
                visible when (IsPhysicalInventoryAdjusted)
                    ToIncludeInPhysicalInventory
                        label is "IncludeInPhysicalInventory"
                        display as text
                visible when (IsSimulatedAdjusted)
                    AssetAdjustmentTo.Simulated
                        display as text
                visible when (IsWorkInProcessAdjusted)
                    AssetAdjustmentTo.WorkInProcess
                        display as text
                visible when (IsTaxExemptAdjusted)
                    AssetAdjustmentTo.TaxExempt
                        display as text
                visible when (IsUsedAdjusted)
                    AssetAdjustmentTo.Used
                        display as text
                visible when (IsAssetGroupAdjusted)
                    AssetAdjustmentTo.AssetGroup
                        display as text
                visible when (IsAssetLocationAdjusted)
                    AssetAdjustmentTo.Location
                        display as text
                visible when (IsAssetDivisionAdjusted)
                    AssetAdjustmentTo.Division
                        display as text
                visible when (IsAssetTypeAdjusted)
                    AssetAdjustmentTo.AssetType
                        display as text
                        compact format
                visible when (IsAssetGuidelineClassAdjusted)
                    ToAssetGuidelineClass
                        display as text
                visible when (IsAssetAccountGroupAdjusted)
                    AssetAdjustmentTo.AssetAccountGroup
                        display as text
                visible when (IsAssetAccountingUnitGroupAdjusted)
                    AssetAdjustmentTo.AssetAccountingUnitGroup
                        display as text
                visible when (IsAssetAccountingUnitAdjusted)
                    AssetAdjustmentTo.AssetAccountingUnit
                        display as text
                        compact format
                visible when (IsAssetSummaryGroupAdjusted)
                    AssetAdjustmentTo.AssetSummaryGroup
                        display as text
                visible when (IsFundOverrideAdjusted)
                    AssetAdjustmentTo.FundOverride
                        display as text
                visible when (IsDepreciationExpenseProjectAdjusted)
                    paragraph
                        text of "Depreciation_Expense"
                            color of gray
                            align as label
                        ToDepreciationExpenseProject.Project
                            no label
                            display as text
                visible when (IsAssetOwnerAdjusted)
                    paragraph
                        text of "AssetOwner"
                            color of gray
                            align as label
                        ToAssetOwner
                            no label
                            display as text
                        ToAssetOwner.Name.FirstAndLastName
                            no label
                            display as text
                visible when (IsReference1Adjusted)
                    ToReference1
                        label is "Reference1"
                        display as text
                visible when (IsReference2Adjusted)
                    ToReference2
                        label is "Reference2"
                        display as text
                visible when (IsGeographicalLatitudeAdjusted)
                    ToGeographicalLatitude
                        label is "GeographicalLatitude"
                        display as text
                visible when (IsGeographicalLongitudeAdjusted)
                    ToGeographicalLongitude
                        label is "GeographicalLongitude"
                        display as text
                visible when (IsGeographicalAltitudeAdjusted)
                    ToGeographicalAltitude
                        label is "GeographicalAltitude"
                        display as text
