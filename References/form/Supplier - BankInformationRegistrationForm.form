BankInformationRegistrationForm is a Form
    title is "BankInformation"
    Layout
        visible when (not SupplierExists)
            text of "BankInformationCanBeEnteredAfterSupplierRegistrationRequiredFieldsAreEnteredAndSupplierIsCreated"
                bold
                italics
        visible when (SupplierExists)
            header3 of "BankInformationForCompany"
            single column
                BankInformationGroup
                    form is BankInformationGroupRegistrationForm
            header3 of "BankInformationForLocation"
            TransientBankInformationGroup
                form is BankInformationGroupRegistrationForm
