ManufacturerContractSetup is a CompositeForm
    is primary
    title is "ManufacturerContract"
    context form is ManufacturerContractContext
        background is theme color
    Actions
        action is AddAllLinesToDistributor
            hide when invalid
        action is AddMissingLinesToDistributor
            hide when invalid
        action is AddAllMembersToDistributor
            hide when invalid
        navigation is MaintainMissingLines
            hide when invalid
        action is ReleaseMembers
            hide when invalid
        action is refresh
            label is "_"
        navigation is ViewContract
            label is "ViewDistributorContract"
    Lines is a Panel
        visible when (CanMaintainManufacturerContractNoGPOContract)
        list is MfgContractLineRel.MfgContractLines
            helper list is MfgContractLinePivotRel.AllManufacturerContractLines
                action is AddLinesToDistributor
    LinesNotInProgress is a Panel
        title is "Lines"
        visible when (not CanMaintainManufacturerContractNoGPOContract)
        list is MfgContractLineRel.MfgContractLinesNoUpdate
    Members is a Panel
        visible when (CanMaintainManufacturerContract)
        list is MfgPricingRel.DistributorPricingMembers
            helper list is MfgContractMemberPivotRel.DistributorParticipantList
                action is AddMembersToDistributor
    ParticipantsNotInProgress is a Panel
        title is "Members"
        visible when (not CanMaintainManufacturerContract)
        list is MfgPricingRel.DistributorPricingMembersNoUpdate
