ProjectContractChangeRequestNavForm is a Form
    title is "<FinanceEnterpriseGroup.ProjectLabel>ContractChangeRequestFor<Description>"
    Layout
        two column distributed
            column1
                paragraph
                    ProjectContract.DisplayContract
                        label is "<FinanceEnterpriseGroup.ProjectLabel>Contract"
                        display as text
                    ProjectContract.Project.Description
                        label is "Description"
                        display as text
        two column distributed
            column1
                paragraph
                    ChangeRequestDate
                        label is "ChangeRequest"
                        display as text
                    Description
                        display as text
        two column distributed
            column1
                paragraph
                    ProjectContract.ContractAmount
                        label is "CurrentContractAmount"
                        display as text
                    NewContractAmount
                        label is "NewContractAmount"
                        display as text
            column2
                paragraph
                    ProjectContract.MaximumFees
                        label is "CurrentMaximumFees"
                        display as text
                    NewMaximumFees
                        label is "NewMaximumFees"
                        display as text
        two column distributed
            column1
                paragraph
                    Scenario
                        label is "BudgetScenario"
                        display as text
                        show up to 5 characters
                    Scenario.Description
                        display as text
                        show up to 30 characters
            column2
                paragraph
                    BudgetChangeOrder
                        label is "BudgetChangeRequest"
                        display as text
                        show up to 5 characters
                    BudgetChangeOrder.Description
                        display as text
                        show up to 30 characters
        two column distributed
            column1
                paragraph
                    RequestedBy
                        label is "RequestedBy"
                        display as text
        two column distributed
            column1
                Comment
                    label is "Comment"
                    display as text
            column2
                DocumentURL
                    display as text
        single column
            paragraph
                Attachment
                    label is "Attachment"
                    display as text
