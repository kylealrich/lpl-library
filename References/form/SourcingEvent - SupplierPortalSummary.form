SupplierPortalSummary is a CompositeForm
    title is "EventSummary"
    disable field options
    called out actions only
    context form is SummaryContext
        background is theme color
    show panel control on left
    Actions
        action is Open.NoBid
            hide when invalid
            valid when (DisplayNoBid)
        action is Open.NoBidReason
            hide when invalid
            valid when (DisplayNoBidReason)
        navigation is EventPrint
            label is "Print"
        action is AddMeToPlanHoldersList
            label is "AddMeToPlanHoldersList"
            hide when invalid
        action is RemoveMeFromPlanHoldersList
            label is "RemoveMeFromPlanHoldersList"
            hide when invalid
    Summary is a Panel
        form is SESummary
    Lines is a Panel
        list is SourcingEventLine set.SupplierPortalEventLines
    Meetings is a Panel
        visible when (MeetingsExist)
        list is SourcingEventMeeting set.SupplierMeetingsPrint
    Contacts is a Panel
        visible when (ContactsExist)
        list is SourcingEventContact set.ListContactView
    Attachments is a Panel
        visible when (SupplierAttachmentsExist)
        list is SupplierSourcingEventAttachmentsRel.ListAttachmentView
    Comments is a Panel
        visible when (SupplierCommentsExist)
        list is SupplierSourcingEventCommentsRel.SupplierPortalSourcingEventCommentList
    Questions is a Panel
        visible when (QuestionsExist)
        list is SourcingEventQuestion set.SupplierQuestionsList
    TermsAndConditions is a Panel
        visible when (TermsExist)
        list is SourcingEventArticlesByDisplayRel.SETermsList
    AddOnCharges is a Panel
        visible when (HeaderAOCExists)
        list is SourcingEventHeaderAOCRel.SupplierSourcingEventAOC
    QAndAForum is a Panel
        visible when (AllowSupplierQandA)
        list is SourcingEventQandARel.SupplierQAndACardList
    Amendments is a Panel
        visible when (HasAmendments)
        list is SourcingEventVersion set.ListAmendmentDate
    PlanHoldersList is a Panel
        visible when (UsingPlanHoldersList)
        list is SourcingEventPlanHolder set.PlanHoldersListSupplier
    AwardSummary is a Panel
        visible when (ShowAwardSummary)
        list is SourcingEventLine set.SEEventLinesAwarded
