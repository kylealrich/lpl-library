TaxCalendarCompositeForm is a CompositeForm
    is primary
    title is "TaxCalendar"
    context form is TaxCalendarMaintenance
        background is theme color
    TaxPeriods is a Panel
        title is "\\V\\A\\TPeriods"
        list is TaxCalendarPeriodsRel.TaxPeriodsList
    IntrastatPeriods is a Panel
        title is "IntrastatPeriods"
        visible when (TaxCalendar.TaxEntity.IntrastatUsed)
        list is IntrastatCalendarPeriodsRel.IntrastatCalendarPeriodMaintenances
