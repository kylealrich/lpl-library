StudentActivityReceiptBatchForm is a CompositeForm
    is primary
    title is "<mode>ReceiptBatch"
    context form is StudentActivityReceiptBatchContextForm
    show panel control on left
    Actions
        action is Entered.Create
            action icon is create
        action is Released.AssignBatchToDeposit
            hide when invalid
        action is Entered.Release
            action icon is release
            hide when invalid
        action is save
        action is save and new
        action is Entered.Delete
            action icon is delete
            hide when invalid
        action is next record
        action is previous record
        action is refresh
    Activities is a Panel
        visible when (Status.Entered)
        list is StudentAssignedActivityByNameRel.ActivitySummaryByNameList
    BatchReceipts is a Panel
        list is StudentActivityReceipt set.BatchEnteredReceiptList
