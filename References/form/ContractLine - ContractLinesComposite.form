ContractLinesComposite is a CompositeForm
    is primary
    title is "Contract<Contract.ContractNbr>_<DisplayContractLine>"
    context form is ContractLineContext
        background is theme color
    restrict action CheckForContractLineErrors
    restrict action CompareForecasts
    restrict action CreateFromContractLine
    restrict action IncludeInProcurementTemplateLine
    restrict action Draft.SelectDraftItemForRebate
    restrict action SelectSpecialItemForItemCreation
    Actions
        action is Draft.ReleaseContract
            action icon is release
            hide when invalid
        action is Draft.ActivateContract
            hide when invalid
        action is Amendment.ReleaseContract
            action icon is release
            hide when invalid
        action is Amendment.ActivateContract
            hide when invalid
        action is KeepEnteredItem
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is Create
            action icon is create
            label is "_"
            mouse over text is "Create"
            hide when invalid
        action is save
            label is "_"
        action is save and new
            label is "_"
        action is refresh
            label is "_"
    Line is a Panel
        form is ContractLineMain
    ComponentGroups is a Panel
        visible when (IsACapitatedProcedure)
        list is ContractCapitatedComponentGroup set.ContractCapitatedComponentGroups
    ComponentGroupItems is a Panel
        visible when (IsComponentGroupItem)
        list is ContractCapitatedComponentGroupItemRel.ComponentGroupsForAComponent
    LineChangeOrderDetails is a Panel
        visible when (ChangeOrdersExistForCurrentVersionForService)
        list is ChangeOrderLineDetailRel.ChangeOrdersForALineCreate
    LineChangeOrderDetailsForNonService is a Panel
        title is "LineChangeOrderDetails"
        visible when (ChangeOrdersExistForCurrentVersionForNonService)
        list is ChangeOrderLineDetailRel.ChangeOrdersForALineCreateForNonService
    Picture is a Panel
        visible when (SpecialItemAndLineExists)
        form is Picture
    Tiers is a Panel
        title is "Members*"
        visible when (IsManufacturerContractWithLineMembers)
        list is ContractLineMember set.ContractLineMembers
    DistributorPricing is a Panel
        title is "Members*"
        visible when (IsDistributorContractWithLineMembers)
        list is ContractLineMember set.ContractLineDistPricingMembers
    TierCosts is a Panel
        title is "TierCosts<TierCostCount>"
        visible when (CanDisplayTierCostPanel)
        list is ContractLineTierCostRel.ContractLineTierCostList
    ItemDetail is a Panel
        visible when (NewItemNumber)
        form is ContractLineItemDetail
    HasItemItemDetail is a Panel
        title is "ItemDetail"
        visible when (HasItemmast)
        form is ContractLineItemDetailItemmast
    UnitsOfMeasure is a Panel
        title is "Units_ofMeasure"
        visible when (HasItemmast)
        list is ItemUOMsRel.ItemUOMsForContractLink
    NoTemplateLineDetail is a Panel
        title is "PurchaseTypeDetail"
        visible when (Contract.CostDefaultingButNotTemplate)
        form is ContractLinePricing
    TemplateLineDetail is a Panel
        title is "PurchaseTypeDetail"
        visible when (Contract.CostDefaultingAndTemplate)
        form is ContractLinePricingTemplate
    Comments is a Panel
        title is "Comments<LineCommentCount>"
        visible when (Contract.UseCommentsOnContract)
        list is ContractLineComment set.ContractLineComments
    Attachments is a Panel
        title is "Attachments<LineAttachmentCount>"
        list is ContractLineAttachment set.ContractLineAttachmentList
    Distributions is a Panel
        title is "Distributions<DistributionCount>"
        list is ContractLineDistribution set.ContractLineDistributions
    ChangeOrdersAndAllowances is a MultiListPanel
        visible when (Contract.ShowChangeOrdersForService)
        list is ChangeOrderLineDetailRel.ChangeOrdersForALine
            visible when (ChangeOrdersExist)
        list is ContractAllowance set.AllowancesForALine
    ChangeOrders is a MultiListPanel
        visible when (Contract.ShowChangeOrdersForNonService)
        list is ChangeOrderLineDetailRel.ChangeOrdersForALine
            visible when (ChangeOrdersExist)
    Locations is a Panel
        title is "Locations<LocationIndicator>"
        visible when (CanUseTaxOrClinicalLocations)
        list is ContractLineLocationRel.LocationsForALineList
    Retainage is a Panel
        title is "Retainage<RetainageIndicator>"
        visible when (not Contract.RetainagePercentCompleteTracking.NoRetainage)
        form is Retainage
    AOC is a Panel
        title is "AddOnCharges<AOCCount>"
        visible when (not Contract.ServicePurchaseType)
        list is ContractLineAOC set.ContractLineAOC
            helper list is AddOnChargeRel.ActiveAddOnChargesForContract
                action is AttachToContract
    HealthcareInformation is a Panel
        title is "Healthcare"
        visible when (CanDisplayHealthcareInformation)
        form is HealthcareInformation
    PatientCharges is a Panel
        visible when (CanDisplayHealthcareInformationNotClosed)
        list is PatientChargeItemsRel.PatientChargesOnAContractLine
    PatientChargesClosed is a Panel
        title is "PatientCharges"
        visible when (CanDisplayHealthcareInformationClosed)
        list is PatientChargeItemsRel.PatientChargesOnAContractLineNoUpdate
    MaximumSpreads is a Panel
        visible when (AvailableToSpread)
        list is ContractLineSpread set.ContractLineSpreads
    UnresolvedSuggestedItems is a Panel
        title is "UnresolvedSuggestedLineChanges"
        visible when (OpenSuggestedItemsExist)
        list is SuggestedItemsOpenRel.SuggestedItems
    ResolvedSuggestedItems is a Panel
        title is "ResolvedSuggestedLineChanges"
        visible when (ResolvedSuggestedItemsExist)
        list is SuggestedItemsResolvedRel.ResolvedSuggestedItems
    Errors is a Panel
        visible when (OtherErrorsExist)
        list is ContractLineOtherErrorRel.ContractLineErrorList
    InactiveItems is a Panel
        visible when (InactiveErrorsExist)
        list is ContractLineInactiveErrorRel.ContractLineErrorList
    ToleranceErrors is a Panel
        visible when (HasToleranceErrors)
        list is ContractLinePriceUpdateRel.ToleranceErrors
    ManualToleranceErrors is a Panel
        title is "ToleranceErrors"
        visible when (HasManualToleranceErrors)
        list is ContractLineManualPriceUpdateRel.ManualToleranceErrors
