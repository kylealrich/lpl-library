BankPeriodClose is an ActionForm
    action is BankPeriodClose
    Layout
        single column
            CashManagementGroup
        three column
            CashCode
            CashCode.Description
                no label
                display as text
            CashCode.Currency
                no label
                display as text
        two column
            CcgCashCodeGrp
                label is "CashCodeGroup"
                select is CashCodeGroup
            CcgCashCodeGrp.Description
                no label
                display as text
        blank line
        single column
            PeriodDate
                label is "PeriodCloseDate"
        blank line
        single column
            SortOption
                label is "ReportSequence"
        blank line
        single column
            UpdateOption
                label is "Update"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                BankPeriodCloseReport_RDG
            column2 of 25%
                BankPeriodCloseReport_XPT
