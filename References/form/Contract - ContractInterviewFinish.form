ContractInterviewFinish is a Form
    title is "ViewContract"
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    Layout
        display as text
            single column
                ContractGroup
                Contract
                Name
                WorkingContractID
        single column
            text of "ClickTheLinkBelowToViewTheContract."
            button of "Contract"
                link is ViewContract
                display as link
