CashCodeFundForm is a CompositeForm
    is primary
    title is "FundAmounts"
    context form is CashCodeFundContextForm
    EditGroupFunds is a Panel
        visible when (EditLevel.Group)
        list is CashCodeFund descendants.FundsForGroupList
            helper list is CashCodeFundWithNoParentRel.FundAmountsList
                action is AddToEditGroup
                    invoked.PrmEditGroup = CashCodeFund
    FundedCashCodeFunds is a Panel
        visible when (IsSourcCashCodeFund)
        list is FundedCashCodeFundRel.FundAmountsList
