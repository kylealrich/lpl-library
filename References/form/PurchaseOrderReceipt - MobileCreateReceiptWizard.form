MobileCreateReceiptWizard is a CompositeForm
    use for action Unreleased.MobileCreateReceiptWizard
    title is "CreateReceipt"
    suppress standard toolbar
    show panel navigation
        last panel navigation is MobileReceiverHomePage
            next button title is "Release"
    restrict action Unreleased.Create
    restrict action Unreleased.MobileCreate
    restrict action Unreleased.MobileCreateReceiptWizard
    restrict action Unreleased.MobileAddReceiveAll
    restrict action Unreleased.AddReceiveAll
    restrict action Unreleased.ReleaseAndPrintLPL
    restrict action Unreleased.ReleaseAndPrintNoBackordersLPL
    restrict action Unreleased.ReleaseAndPrint
    restrict action Unreleased.ReleaseAndPrintNoBackorders
    restrict action Unreleased.Delete
    restrict action Unreleased.Release
    restrict action Unreleased.MobileCreateReceiptWizard
    restrict action MobileReleaseFromWizard
    Actions
        action is refresh
            label is "_"
        action is save
            hide when invalid
            valid when (false)
        action is save and new
            hide when invalid
            valid when (false)
    Receipt is a Panel
        form is MobileCreateReceiptForm
        save on next
    PurchaseOrderLines1 is a Panel
        title is "PurchaseOrderLines"
        visible when (AllExtraColumns)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is MobilePOLineList1
    PurchaseOrderLines2 is a Panel
        title is "PurchaseOrderLines"
        visible when (BinColumn)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is MobilePOLineList2
    PurchaseOrderLines3 is a Panel
        title is "PurchaseOrderLines"
        visible when (RemainingColumn)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is MobilePOLineList3
    PurchaseOrderLines4 is a Panel
        title is "PurchaseOrderLines"
        visible when (CatchWeightColumn)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is MobilePOLineList4
    PurchaseOrderLines5 is a Panel
        title is "PurchaseOrderLines"
        visible when (CatchWeightAndBinTracked)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is MobilePOLineList5
    PurchaseOrderLines6 is a Panel
        title is "PurchaseOrderLines"
        visible when (RemainingAndBinTracked)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is MobilePOLineList6
    PurchaseOrderLines7 is a Panel
        title is "PurchaseOrderLines"
        visible when (RemainingAndCatchWeight)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is MobilePOLineList7
    PurchaseOrderLines8 is a Panel
        title is "PurchaseOrderLines"
        visible when (NoExtraColumns)
        template is PurchaseOrder.NonServicePurchaseOrderLinesRel
            business class is POReceiptLineTemplate
                list is MobilePOLineList8
    ReceiptLines is a Panel
        list is PurchaseOrderReceiptLine set.MobilePOReceiptLineTemplateCardList
    Summary is a Panel
        visible when (PurchaseOrderReceipt entered)
        action is MobileReleaseFromWizard
