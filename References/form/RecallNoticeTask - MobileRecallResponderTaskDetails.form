MobileRecallResponderTaskDetails is a Form
    title is "_"
    restrict action MobileAskAQuestion
    Layout
        header3 of "Details"
        single column
            RecallNotice.create date
                label is "NoticeCreationDate"
            RecallNotice.ReleasedTimestamp
                label is "AssignmentDate"
        two column table
            column1
                visible when (RecallGroup.RequireTaskAcceptance)
                    AcceptedTimestamp
                        label is "AcceptedDate"
                CompletedTimestamp
                    label is "CompletedDate"
            column2
                visible when (RecallGroup.RequireTaskAcceptance)
                    DaysToAccept
                DaysToComplete
        single column
            button of "PrintTask"
                link is ResponderTaskAnalysisReport
