Receipt is a Form
    Layout
        three column distributed
            column1
                paragraph
                    Buyer
                    Buyer.Employee.PresentationNameSnapshot
                        no label
                        display as text
                paragraph
                    PurchaseFromLocation
                        label is "PurchaseFrom"
                    VendorLocationRel.VendorName
                        no label
                        display as text
                paragraph
                    Location
                    Location.Name
                        no label
                        display as text
                paragraph
                    ReceivedDate
                        label is "DateAndTimeReceived"
                paragraph
                    text of "_"
            column2
                paragraph
                    BillOfLading
                        label is "Bill_ofLading"
                paragraph distributed
                    MerchandiseCarrier
                        label is "Carrier"
                    ReceiptImportLoggedFl
                        label is "LoggedFlag"
                paragraph
                    Currency
                        label is "CurrencyCode"
                    Currency.Description
                        no label
                        display as text
                paragraph
                    TermCode
                        label is "TermsCode"
            column3
                paragraph
                    OperatorID
                paragraph
                    MatchReferenceNumber
                paragraph
                    ReceiptComments
