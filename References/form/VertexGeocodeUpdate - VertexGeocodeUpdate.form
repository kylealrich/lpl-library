VertexGeocodeUpdate is an ActionForm
    action is VertexGeocodeUpdate
    Layout
        paragraph
            TaxEntity
            TaxEntity.Name
                no label
                display as text
        paragraph
            Company
            Company.Name
                no label
                display as text
        blank line
        single column
            Update
        two column
            CustomerFrom
                label is "FromCustomer"
            CustomerTo
                label is "ToCustomer"
        two column
            ShipToFrom
                label is "FromShipTo"
            ShipToTo
                label is "ToShipTo"
        two column
            ServicelocFrom
                label is "FromBilling_andRevenueServiceLocation"
            ServicelocTo
                label is "ToBilling_andRevenueServiceLocation"
        blank line
        two column
            IclocationFrom
                label is "FromInventoryControlLocation"
            IclocationTo
                label is "ToInventoryControlLocation"
        single column
            IclocationFlag
                label is "TaxCodeType"
        blank line
        two column
            FromVendor
                label is "FromAccountsPayableVendor"
            ToVendor
                label is "ToAccountsPayableVendor"
        two column
            FromVenloc
                label is "FromAccountsPayableVendorLocation"
            ToVenloc
                label is "ToAccountsPayableVendorLocation"
        blank line
        two column
            OeproclevFrom
                label is "FromBillingProcessLevel"
            OeproclevTo
                label is "ToBillingProcessLevel"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                VertexGeocodeUpdateReport_RDG
                Tx140err_RDG
            column2 of 25%
                VertexGeocodeUpdateReport_XPT
                Tx140err_XPT
