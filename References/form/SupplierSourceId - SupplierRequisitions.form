SupplierRequisitions is a Form
    Layout
        single column
            text of "LineCreationOptions"
        paragraph
            AllowTemplate
            AllowContractSearch
            AllowAdHoc
        single column
            ContractItemsOnly
            AutoApprove
            text of "Defaults:"
                bold
            paragraph
                DefaultRequester
                    label is "Requester"
                    select is SupplierRequesters
                DefaultRequester.RequesterFirstLastName
                    no label
                    display as text
            paragraph
                DefaultRequestingLocation
                    label is "RequestingLocation"
                    select is RequestingLocationsWithFromLocation
                DefaultRequestingLocation.Name
                    no label
                    display as text
            paragraph
                DefaultCompany
                    label is "Company"
                DefaultCompany.Name
                    no label
                    display as text
        header2 of "ProcedureFieldsToDisplay"
        two column
            ShowProcedureDetails
            button of "SupplierGroupProcedureAccess"
                link is SupplierGroupProcedures
                display as link
        two column
            visible when (ProcedureDetailsFromContact)
                SupplierViewVisitNumber
                SupplierViewMedicalRecordNumber
                SupplierViewCaseID
                SupplierViewProcedure
                SupplierViewPractitioner
                SupplierViewNurseCoordinator
                SupplierViewPatientName
                SupplierViewPatientBirthdate
                SupplierViewPatientGender
                SupplierViewSupplierOrderNumber
                SupplierViewLotNumber
                SupplierViewSerialNumber
                SupplierViewUniqueDeviceId
                SupplierViewGTIN
                SupplierViewManufacturingDate
                SupplierViewExpirationDate
