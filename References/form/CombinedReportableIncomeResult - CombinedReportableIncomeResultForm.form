CombinedReportableIncomeResultForm is a CompositeForm
    is primary
    title is "ReportableIncomeResults"
    context form is CombinedReportableIncomeResultParmForm
    Actions
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            action icon is save
            label is "Save_andReload"
            mouse over text is "SaveParametersAndReloadData"
            hide when invalid
        action is export to pdf
            action icon is icon.export-to-pdf
            label is "Export"
            mouse over text is "ExportToPDF"
        action is Delete
            action icon is delete
            label is "_"
        action is refresh
            action icon is icon.refresh
            label is "_"
    Payables is a Panel
        title is "Payables<DisplayPayablesCount>"
        list is PayablesInvoicePaymentRel.CombinedPayablesReportableIncomeList
    NonPayables is a Panel
        title is "NonPayables<DisplayNonPayablesCount>"
        list is NonPayablesReportableIncomeRel.NonPayablesReportableIncomeAdjustmentsList
