MobileFullPaymentDataForm is a CompositeForm
    title is "<DerivedFormTitle>"
    context form is MobilePaymentEntry
    Main is a Panel
        form is Main
    PayeeInformation is a Panel
        form is Payee
    Defaults is a Panel
        form is Defaults
    BankInformation is a Panel
        form is Bank
    Comments is a Panel
        form is Comments
    TransactionData is a Panel
        form is TransactionData
    CurrencyAmounts is a Panel
        form is MobileCurrencyAmounts
    CashLedgerGLDistribution is a Panel
        title is "Distributions"
        visible when (HasDistributions)
        list is CashLedgerGLDistributionsRel.TransactionGLDistributionList
    ApprovalComments is a Panel
        visible when (HasApprovalComments)
        list is CashLedgerPayablesPaymentReasonCodeUsage set.CashLedgerPayablesPaymentReasonCodeUsageList
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
    RelatedInvoices is a Panel
        visible when (HasApPayments)
        list is AppaymentRel.PayablesInvoicePaymentInqueryList
    RelatedInvoiceHistory is a Panel
        visible when (HasApAppHist)
        list is ApapphistRel.PayablesInvoicePaymentHistoryList
