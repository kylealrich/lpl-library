ContractRebateActivitySumForm is a SummaryForm
    title is "ContractRebateActivity"
    First Page Header
        Layout
            PrintDate
                right align
            header3 of "ContractRebateActivity"
                center
    Layout
        visible when (PrintParameters)
            single column
                SupplyManagementReportContractGroupField.ContractGroup
                visible when (SupplyManagementReportContractGroupField.Contract entered)
                    SupplyManagementReportContractGroupField.Contract
                visible when (SupplyManagementReportContractGroupField.Contract not entered)
                    AllTextForParameters
                        label is "Contract"
                visible when (FromDate entered)
                    FromDate
                        label is "FromRebateDueDate"
                    ThruDate
                        label is "ToRebateDueDate"
                visible when (FromDate not entered)
                    AllTextForParameters
                        label is "FromRebateDueDate"
                    AllTextForParameters
                        label is "ToRebateDueDate"
                PrintParameters
                    label is "PrintParameters"
        single column
            paragraph
                SupplyManagementReportContractGroupField.ContractGroup
        form is ContractsForContractComplianceReportRel.ContractRebateActivitySumForm
