MobileFinancialInstitution is a CompositeForm
    title is "FinancialInstitution"
    context form is MobileFinancialInstitutionContext
        background is theme color
    Branches is a Panel
        list is FinancialInstitutionBranch set.MobileFinancialInstitutionBranches
    FeeServiceCodes is a Panel
        list is BankServiceCode set.MobileBankServiceCodeList
    InstitutionRatings is a Panel
        list is FinancialInstitutionRating set.MobileFinancialInstitutionRatingList
    Documents is a Panel
        list is FinancialInstitutionDocument set.MobileFinancialInstitutionDocumentList
