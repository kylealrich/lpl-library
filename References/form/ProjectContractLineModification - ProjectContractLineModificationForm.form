ProjectContractLineModificationForm is a Form
    is primary
    title is "ContractModificationLine"
    Layout
        two column distributed
            column1
                paragraph
                    FinanceDimension2.DisplayDimension
                        label is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
                        display as text
                    FinanceDimension2.Description
                        label is "Description"
                        display as text
        two column distributed
            column1
                paragraph
                    ReleaseDate
        two column distributed
            column1
                paragraph
                    OriginalPriority
                    Priority
                        label is "NewFundedPriority"
        two column distributed
            column1
                paragraph
                    OriginalFundingGroup
                    FundingGroup
                        label is "NewFundingGroup"
        two column distributed
            column1
                paragraph
                    OriginalBillingGroup
                    BillingGroup
                        label is "NewBillingGroup"
        two column distributed
            column1
                paragraph
                    OriginalPercent
                    Percent
                        label is "NewPercent"
        two column distributed
            column1
                paragraph
                    OriginalFundedAmount
                    FundedAmount
                        label is "NewFundedAmount"
        two column distributed
            column1
                paragraph
                    Status
                        display as text
        single column
            paragraph
                ErrorMessage
                    display as text
