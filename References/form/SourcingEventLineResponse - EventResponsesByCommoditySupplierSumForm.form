EventResponsesByCommoditySupplierSumForm is a SummaryForm
    title is "_"
    Layout
        header5 of "Supplier\\:_<NotifiedSupplier.Supplier.SupplierName>"
        list is EventResponsesGroupByCommoditySupplierDetailsRel.SupplierResponseByCommodityCodeList
        four column
            column1
                SourcingEventResponse.NotifiedSupplier.Supplier
                    label is "TotalsForSupplier"
                    display when blank
                SourcingEventResponse.NotifiedSupplier.SupplierSourceId
                    label is "Contact"
                    display when blank
            column2
                SupplierResponsebyCommodityCode
                    label is "TotalEventsBid"
                    display when blank
                EventAwardedResponseByCommodity
                    label is "TotalEventsAward"
                    display when blank
                ResponseByCommodityPercent
                    label is "EventsBidSuccess"
                    display when blank
            column3
                SupplierResponseLineBidByCommodity
                    label is "TotalLinesBid"
                    display when blank
                EventAwardedResponseLineByCommodity
                    label is "TotalAwardedLines"
                    display when blank
                ResponseLineByCommodityPercent
                    label is "LineBidSuccess"
                    display when blank
            column4
                BidTotalByCommodity
                    label is "BidTotal"
                    display when blank
                AwardTotalByCommodity
                    label is "AwardedTotal"
                    display when blank
                AmountBidSuccessByCommodity
                    label is "AmountBidSuccess"
                    display when blank
        four column
            column1
                header2 of "DiversityBreakdown"
            column2
                ResponseByCommodityWithDiversity
                    label is "DiversityEventResponses"
                    display when blank
                AwardedResponseByCommodityWithDiversity
                    label is "DiversityAwards"
                    display when blank
                TotalAwardedResponseByCommodityWithDiversity
                    label is "TotalEventAwards"
                    display when blank
                PercentAwardedResponseByCommodityWithDiversity
                    label is "DiversityEventAwardRate"
                    display when blank
            column3
                DiversityResponseLineCommodity
                    label is "DiversityLinesResponded"
                    display when blank
                DiversityAwardedReponseLineByCommodity
                    label is "DiversityLinesAwarded"
                    display when blank
                TotalDiversityAwardResponseLineByCommodity
                    label is "TotalAwardedLines"
                    display when blank
                DiversityPercentAwardedByCommodity
                    label is "DiversityPercentAwarded"
                    display when blank
            column4
                DiversityAmountBidByCommodity
                    label is "DiversityAmountBid"
                    display when blank
                DiversityAwardAmountByCommodity
                    label is "DiversityAwardAmount"
                    display when blank
                AwardedTotalByDiversity
                    label is "AwardedTotal"
                    display when blank
                AmountAwardRatePercentByCommodity
                    label is "AmountAwardRate"
                    display when blank
