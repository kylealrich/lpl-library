BillingScheduleContext is a Form
    Layout
        display as text
            two column distributed
                column1
                    paragraph
                        Company
                        Company.Name
                            no label
                    paragraph
                        Sublease
                        Sublease.Currency
                            no label
                    paragraph
                        CompanyAndCustomer.Company
                            label is "ReceivableCompany"
                        CompanyAndCustomer.Company.Name
                            no label
                column2
                    paragraph
                        Sublease.Lease.BeginDate
                            label is "HeadLeaseBeginDate"
                        Sublease.Lease.EndDate
                            label is "HeadLeaseEndDate"
                    paragraph
                        Sublease.BeginDate
                            label is "SubleaseBeginDate"
                        Sublease.EndDate
                            label is "SubleaseEndDate"
                    paragraph
                        CompanyAndCustomer.Customer
                            label is "Sublessee"
                            select is CustomerContextCompanyCustomerSelectList
                        CompanyAndCustomer.Customer.Name
                            no label
