ParLocationUtilizationHeaderSummaryForm is a SummaryForm
    title is "ParLocationUtilization"
    Layout
        visible when (IsByItemNumber)
            list is ParLocationUtilizationLine set.ParLocationUtilizationLineSummaryList
        visible when (IsByAverageUtilizationAscending)
            list is LinesByAverageUtilizationAscendingRel.ParLocationUtilizationLineSummaryList
        visible when (IsByAverageUtilizationDescending)
            list is LinesByAverageUtilizationDescendingRel.ParLocationUtilizationLineSummaryList
        visible when (IsByItemDescription)
            list is LinesByItemDescriptionRel.ParLocationUtilizationLineSummaryList
        visible when (IsByShelfLocation)
            list is LinesByShelfLocationRel.ParLocationUtilizationLineSummaryList
        form is ParLocationUtilizationLineTotal
