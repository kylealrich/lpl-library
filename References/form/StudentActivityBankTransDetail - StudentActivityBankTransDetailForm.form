StudentActivityBankTransDetailForm is a Form
    is primary
    Actions
        action is Entered.Create
        action is save
        action is save and new
        action is Entered.Delete
        action is previous record
        action is next record
        action is refresh
    Layout
        two column
            column1
                paragraph
                    StudentActivitySchool
                        label is "School"
                        display as text
                paragraph
                    StudentActivityBankAccount
                        label is "BankAccount"
                        display as text
                paragraph
                    StudentActivityBankTransaction.BankTransactionType
                        label is "TransactionType"
                        display as text
                paragraph
                    StudentActivityBankTransaction
                        label is "TransactionNumber"
                        display as text
                paragraph
                    SchoolActivityAccount
                        label is "Activity"
                        compact format
                        select is SchoolActivityAccountSelectList
                paragraph
                    StudentActivityBankTransaction.Description
                        display as text
                paragraph
                    CorrectedPosting
                        display only
            column2
                paragraph
                    StudentActivitySchool.SchoolName
                        no label
                        display as text
                paragraph
                    StudentActivityBankAccount.Description
                        no label
                        display as text
                paragraph
                    StudentActivityBankTransaction.TransactionDate
                        display as text
                paragraph
                    Status
                        display as text
                paragraph
                    SchoolActivityAccount.Description
                        no label
                        display as text
                paragraph
                    BankTransDetailAmount
                        label is "Amount"
        visible when (GLAccountRequired)
            visible when (GLAccountNotEntered)
                header3 of "GLAccountRequiredForRevenueTypeActivities"
                paragraph
                    GLAccount
                        compact format
