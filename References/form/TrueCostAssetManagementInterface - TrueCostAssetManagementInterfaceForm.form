TrueCostAssetManagementInterfaceForm is a Form
    is primary
    propagate as a drill
    title is "AssetManagementInterface"
    Layout
        single column
            visible when (TrueCostAssetManagementInterface.RunGroup entered)
                TrueCostAssetManagementInterface.RunGroup
                    display as text
            visible when (TrueCostAssetManagementInterface.RunGroup not entered)
                TrueCostAssetManagementInterface.RunGroup
            TrueCostConfiguration
                label is "True\\CostConfiguration"
            AccountingEntity
            AssetAccountingUnitGroup
                label is "AssetDimensionGroup"
            Asset
            CurrentPeriodDepreciation
            YearEndDate
            Period
            visible when (HasError)
                ErrorDescription
                    display as text
