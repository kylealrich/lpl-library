Details is a Form
    title is "VendorClass"
    Layout
        three column
            column1
                header2 of "VendorClassDetails"
                paragraph
                    Currency
                        label is "InvoiceCurrency"
                    Currency.Description
                        no label
                        display as text
                paragraph
                    PostingOption
                paragraph
                    IncomeCode
                    IncomeCode.Description
                        no label
                        display as text
                paragraph
                    TaxCode
                        select is TaxCodeFilteredList
                    TaxCode.Description
                        no label
                        display as text
                paragraph
                    TaxUsageCode
                    TaxUsageCode.Description
                        no label
                        display as text
                paragraph
                    TaxOverride
                paragraph
                    DefaultTaxIndicator
                        label is "TaxIndicator"
                paragraph
                    DistributionCode
                    DistributionCode.Description
                        no label
                        display as text
                paragraph
                    Enclosure
                paragraph
                    InvoiceRoutingCategory
                    InvoiceRoutingCategory.Description
                        no label
                        display as text
            column2
                header2 of "Options"
                paragraph
                    CashCode
                    CashCode.Description
                        no label
                        display as text
                paragraph
                    BankTransactionCode
                        label is "PaymentCode"
                    BankTransactionCode.Description
                        no label
                        display as text
                paragraph
                    PaymentPriority
                paragraph
                    ZeroPaymentAllowed
                paragraph
                    ZeroPaymentCode
                    ZeroPaymentCode.Description
                        no label
                        display as text
                paragraph
                    MaximumPaymentAmount
                paragraph
                    MaximumPaymentHoldCode
                        label is "MaximumHoldCode"
                    MaximumPaymentHoldCode.Description
                        no label
                        display as text
                paragraph
                    MinimumPaymentAmount
                paragraph
                    MinimumPaymentHoldCode
                        label is "MinimumHoldCode"
                    MinimumPaymentHoldCode.Description
                        no label
                        display as text
                paragraph
                    DerivedExcludedInvoiceTemplates
                        label is "ExcludedInvoiceTemplates"
                        display as text
                    button of "Edit"
                        action is ExcludeInvoiceTemplate
                paragraph
                    RestrictToOneInvoice
                paragraph
                    AllowDuplicateInvoiceByDate
            column3
                header2 of "MatchOptions"
                paragraph
                    RequirePurchaseOrder
                paragraph
                    MatchPrepayment
                paragraph
                    MatchPrepaymentMethod
                paragraph
                    MSCMSync
                        label is "M\\S\\C\\MSync"
                paragraph
                    InterfaceVendor
                paragraph
                    MatchTable
                        select is MatchTablesForAVendor
                    MatchTable.Description
                        no label
                        display as text
                paragraph
                    RuleGroup
                    RuleGroup.Description
                        no label
                        display as text
                paragraph
                    CostComponentMatch
                        mouse over text is "PerformToleranceCheckOnAddOnChargesDuringMatch"
                paragraph
                    BypassInvoiceOnlyAOC
                        label is "BypassInvoiceOnlyAddOnCharges"
                        mouse over text is "DoNotPerformToleranceCheckForInvoiceOnlyAddOnCharges"
