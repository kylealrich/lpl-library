MobileProjectContractInvoiceReversalNav is a Form
    title is "InvoiceReversal"
    Layout
        header2 of "<FinanceEnterpriseGroup.ProjectLabel>ContractInvoiceReversal"
        two column table
            column1
                ProjectContract.DisplayContract
                    label is "<FinanceEnterpriseGroup.ProjectLabel>Contract"
                    display as text
                RunGroup
                    display as text
                FinanceDimension2.CompanyCustomerGroup.ReceivableCompany
                    label is "Company"
                    display as text
                PhoneDerivedCust
                    label is "Customer"
                    display as text
            column2
                ProjectContract.Project.Description
                    label is "Description"
                    display as text
                ProjectContractInvoice
                    label is "Invoice"
                    display as text
                InvoiceDate
                    display as text
                FinanceDimension2.DisplayDimension
                    label is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
                    display as text
        header4 of "Amounts"
        three column table
            column1
                InvoiceTotalAmount
                    label is "Total"
                    display as text
            column2
                InvoiceTotalTaxAmount
                    label is "Tax"
                    display as text
            column3
                InvoiceTotalAmountaAndTaxAmount
                    label is "TotalInvoiced"
                    display as text
        two column table
            column1
                InvoiceBilledAmount
                    label is "TotalBilledAmount"
                    display as text
                InvoiceTotalAllowance
                    label is "Allowance"
                    display as text
            column2
                InvoiceTotalAdjustment
                    label is "Adjustments"
                    display as text
                InvoiceTotalFees
                    label is "Fees"
                    display as text
        paragraph distributed
            button of "ViewInvoice"
                visible when (GeneratedInvoiceExists)
                view GeneratedInvoice.File
            button of "ViewInvoice"
                visible when (GeneratedIDMInvoiceExists)
                link is external "<DerivedIDMInvoiceLink>"
                display as link
            button of "ViewInvoiceURL"
                visible when (ThirdPartyInvoicePrinted)
                link is external "<VendorInvoiceLink>"
                display as link
