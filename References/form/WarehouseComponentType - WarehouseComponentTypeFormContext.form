WarehouseComponentTypeFormContext is a Form
    Layout
        visible when (not RecordExists)
            paragraph
                Company
                    label is "Company"
                Company.Name
                    no label
                    display as text
            paragraph
                InventoryLocation
                    label is "InventoryLocation"
                InventoryLocation.Name
                    no label
                    display as text
        visible when (RecordExists)
            paragraph
                Company.DerivedCompanyDisplay
                    label is "Company"
                    display as text
            paragraph
                InventoryLocation.DerivedInventoryLocationIDName
                    label is "InventoryLocation"
                    display as text
