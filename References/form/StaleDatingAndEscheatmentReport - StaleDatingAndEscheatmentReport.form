StaleDatingAndEscheatmentReport is an ActionForm
    action is StaleDatingAndEscheatmentReport
    Layout
        single column
            CashManagementGroup
        three column
            CashCode
            CashCode.Description
                no label
                display as text
            CashCode.Currency
                no label
                display as text
        two column
            CcgCashCodeGrp
                label is "CashCodeGroup"
                select is CashCodeGroup
            CcgCashCodeGrp.Description
                no label
                display as text
        blank line
        two column
            BankInstCode
                label is "TransactionCode"
                select is CashPayments
            BankInstCode.Description
                no label
                display as text
        single column
            ProcLevel
                label is "ProcessLevel"
        blank line
        single column
            RunOption
        single column
            SortOption
        blank line
        single column
            PrintAddress
        blank line
        single column
            FromDate
                label is "FromDate"
            ToDate
                label is "ToDate"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                StaleDatingAndEscheatmentReportReport_RDG
            column2 of 25%
                StaleDatingAndEscheatmentReportReport_XPT
