InventorySearch is a SearchForm
    title is "SearchFields"
    Layout
        four column distributed
            column1
                AssetItemInventory.RunGroup
                    label is "RunGroup"
                AssetItemInventory.BarCode
                    label is "BarCode"
                Company
                TransientAssetOwner
                    label is "Owner"
            column2
                InventoryLocation
                InventoryLocationDetail
                    select is ActiveAssetLocationDetails
                TransientAssetDivision
                    label is "Division"
                ItemCondition
                    label is "Condition"
            column3
                InventoryLocationDetail.Level1
                InventoryLocationDetail.Level2
                InventoryLocationDetail.Level3
            column4
                text of "InventoryDateRange"
                    header4
                InventoryDateRange.Begin
                    label is "Begin"
                InventoryDateRange.End
                    label is "End"
