POLineQuantityDetailForm is a Form
    title is "POLineQuantityDetail"
    Layout
        display as text
            visible when (not IsCatchWeightItem)
                header2 of "Quantities_\\in<EnteredBuyUOM>"
                three column distributed
                    column1
                        DerivedOrderedQuantity
                        CancelQuantity
                        ReceivedQuantity
                        MatchedQuantity
                        OpenToMatchQuantity
                    column2
                        MatchedNotReceivedQuantity
                        ChargebackQuantity
                        SubstituteQuantity
                        DerivedTotalRejectedQuantityInPOUOM
                        DerivedReturnedQuantityInPOUOM
                    column3
                        ArchivedQuantity
                        MemoQuantity
                        WriteOffQuantity
                        paragraph
                            DerivedOrderedPriceQuantity
                            visible when (not IsVendorBuyUOMMatched)
                                VendorPriceUOM
                                    no label
            visible when (IsCatchWeightItem)
                two column table
                    column headers
                        "Stock"
                        "Secondary"
                    row headers
                        "UnitOfMeasure"
                        "Ordered"
                        "Canceled"
                        "Received"
                        "Matched"
                        "OpenToMatch"
                        "MatchedNotReceived"
                        "Chargeback"
                        "Substitute"
                        "Rejected"
                        "Returned"
                        "Archived"
                        "Memo"
                        "WriteOff"
                        "OrderedPrice"
                    column1
                        Item.StockUOM
