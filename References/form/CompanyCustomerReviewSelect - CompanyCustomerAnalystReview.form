CompanyCustomerAnalystReview is a CompositeForm
    is primary
    title is "CreditAnalystReviewDetail"
    context form is Main
        background is theme color
    Main is a Panel
        form is MainPanelForm
    Finance is a Panel
        form is ContactFinance
    Aging is a Panel
        visible when (IsCompanyCurrency)
        list is AgingPeriodForCompanyRel.CompanyCustomerAgingList
    CompanyCustomerCurrencyAging is a Panel
        title is "Aging"
        visible when (IsCompanyCustomerCurrency)
        list is AgingPeriodForCompanyRel.CompanyCustomerCurrencyAgingList
    NationalAccountAging is a Panel
        title is "Aging"
        visible when (IsNationalAccountCompanyCurrency)
        list is AgingPeriodRel.NationalAccountAgingList
    CompanyCustomerNationalAccountAging is a Panel
        title is "Aging"
        visible when (IsNationalAccountCompanyCustomerCurrency)
        list is AgingPeriodRel.NationalAccountCompanyCustomerCurrencyAgingList
    Contacts is a Panel
        list is CompanyCustomerRel.CompanyCustomerContactRel.CustomerContacts
    Comments is a Panel
        list is CompanyCustomerCommentRel.CustomerCommentsList
    Address is a Panel
        form is AddressForm
    NetBalance is a Panel
        visible when (HasVendor)
        list is CustomerRel.VendorGroupAndVendor.Vendor.TotalVendorBalancesRel.CustomerBalanceInterfaceList
