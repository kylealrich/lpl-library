PurchaseOrderEntry is a CompositeForm
    is primary
    title is "<DerivedPurchaseOrderTitle>"
    context form is PurchaseOrderEntryContext
        background is theme color
    restrict action CreateIDMXML
    restrict action DeleteIDMXML
    restrict action CreateInvoicePurchaseOrder
    Actions
        action is Unreleased.Release
            action icon is release
            hide when invalid
        action is IssueDraft
            hide when invalid
        action is Released.IssueFinal
            action icon is print
            hide when invalid
        action is Released.RegenerateDocument
            action icon is pdf
            hide when invalid
        action is Canceled.IssueFinal
            action icon is print
            hide when invalid
        action is Canceled.RegenerateDocument
            action icon is pdf
            hide when invalid
        action is Released.CancelPurchaseOrder
            action icon is reject
            hide when invalid
        action is Released.CancelRemainingPurchaseOrder
            action icon is reject
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is refresh
            label is "_"
    Main is a Panel
        form is PurchaseOrderEntryMain
    Lines is a Panel
        list is PurchaseOrderLinesRel.HeaderListOfLines
    HeaderDetails is a Panel
        form is PurchaseOrderHeader
    Addresses is a Panel
        form is PurchaseOrderAddresses
    Totals is a Panel
        form is PurchaseOrderAmountTotals
    LineDefaults is a Panel
        form is PurchaseOrderLineDefaults
    ProcedureInfomationDefaults is a Panel
        title is "ProcedureDefaults"
        visible when (DisplayOfProcedureInfo)
        form is ProcedureInfoDefaults
    Retainage is a Panel
        form is PurchaseOrderHeaderRetainage
    AddOnCharges is a MultiListPanel
        list is PurchaseOrderAddOnChargesRel.MiscellaneousAddOnCostsForAPurchaseOrder
        list is PurchaseOrderSpreadAddOnCharge set.PurchaseOrderSpreadAddOnChargeList
    Comments is a Panel
        title is "Comments<CommentCount>"
        list is PurchaseOrderComment set.primary
    VendorInformation is a DashBoardPanel
        show 4 columns
        Vendor is a Panel
            double width
            overview navigation is VendorDashboard
            form is VendorInformationForm
        PurchaseFromLocation is a Panel
            visible when (PurchaseFromLocationContactDetailsExists)
            double width
            overview navigation is PurchaseFromLocationNav
            form is PurchaseFromLocationInfo
        APVendorPurchaseFromLocation is a Panel
            title is "PurchaseFromLocation"
            visible when (APVendorLocationContactDetailsExists)
            double width
            overview navigation is APVendorPurchaseFromLocationNav
            form is APVendorPurchaseFromLocationInfo
        PurchasingVendor is a Panel
            visible when (PurchasingVendorContactDetailsExists)
            double width
            overview navigation is PurchasingVendorNav
            form is PurchasingVendorInfo
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
    MatrixAggregation is a Panel
        visible when (HasDistributionAggregation)
        list is PODistributionAggregationRel.PODistributionAggregationSummary
    Diversity is a Panel
        visible when (VendorHasValidDiversity)
        list is PurchaseOrderDiversityListRel.primary
    IssuesAndAcknowledgements is a Panel
        visible when (HasRevision)
        list is PurchaseOrderRevisionsRel.RevisionsforaPurchaseOrder
    FaxStatus is a Panel
        visible when (DisplayFaxStatus)
        list is FaxDetail set.primary
    BudgetErrors is a Panel
        visible when (HeaderBudgetErrorsExist)
        list is HeaderBudgetEditErrorRel.primary
    WarningBudgetErrors is a Panel
        visible when (DistributionBudgetErrorsExist)
        list is DistributionBudgetEditErrorRel.primary
    LineCopyErrors is a Panel
        visible when (LineErrorsFromHeaderExists)
        list is PurchaseOrderLineErrorsRel.PurchaseOrderLineErrorFromHeader
    UserFields is a Panel
        form is UserFields
