TransactionDetailsContextForm is a Form
    title is "TransactionDetails"
    Layout
        paragraph
            StudentActivitySchool
                label is "School"
            StudentActivitySchool.SchoolName
                no label
            button of "PrintReport"
                link is PrintableTransactionDetails
        paragraph
            StudentActivityClosedPeriod
                label is "Period"
            SchoolActivityAccount
                label is "Activity"
                compact format
            SchoolActivityAccount.Description
                no label
                display only
        paragraph
            PeriodReceipts
                label is "Receipts"
                display only
            PeriodDisbursements
                label is "Disbursements"
                display only
            PeriodTransfers
                label is "Transfers"
                display only
