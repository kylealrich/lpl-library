ReviewGroupedApplication is a Form
    Layout
        display as text
            three column distributed
                paragraph
                    Company.RepresentativeText
                        label is "Company"
                    Company.Currency
                        no label
                paragraph
                    ReceivableInvoiceDetailRel.Customer.RepresentativeText
                Status
            blank line
            three column distributed
                paragraph
                    ReceivableApplication.TransType
                        label is "Type"
                    ReceivableApplication.Invoice
                ReceivableInvoiceDetailRel.DueDate
                ReceivableInvoiceDetailRel.InvoiceAmount.CurrencyAmount
            blank line
            three column distributed
                ReceivableOperator
                    label is "Operator"
                GeneralLedgerDate
                    label is "GlobalLedgerDate"
                BaseDebitCreditAdjustAmount
                    label is "AdjustAmount"
            blank line
            three column distributed
                paragraph
                    FirstAdjustmentReason
                        label is "Reason"
                    FirstAdjustmentDescription
                        label is "Description"
                ReceivableInvoiceDetailRel.BankTransactionCode
            blank line
            header2 of "NewMemo:"
            paragraph
                CreditTransaction.CreditType
                    label is "Type"
                CreditTransaction.CreditNumber
                    label is "Number"
                CreditTransaction.CreditCompany
                CreditTransaction.CreditCustomer
                    label is "Customer"
