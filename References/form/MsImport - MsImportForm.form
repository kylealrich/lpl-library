MsImportForm is a Form
    is primary
    title is "ImportFromSecurityClassesToMenuSecurity"
    Layout
        paragraph
            text of "CreateUI_Access_Control_NavigationPoliciesBasedOn_Security_Class_Web_AppAccessibilityPolicies"
        WorkDirectory
        header3 of "ImportOptions"
        single column
            WebApp
                raise blue alert when (true)
                    mouse over text is "LeaveBlankToRunAgainstAll_Web_Apps"
            RoleTemplate
                label is "AllowedRoleTemplate"
            Status
                label is "Status"
