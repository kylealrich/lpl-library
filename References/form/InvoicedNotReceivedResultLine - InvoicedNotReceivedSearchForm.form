InvoicedNotReceivedSearchForm is a SearchForm
    title is "<InvoicedNotReceivedResult.DerivedFormTitle>"
    Layout
        four column distributed
            column1
                Company
                Vendor
                PayablesInvoiceDetail.Location
                    label is "ShipTo"
                    show up to 8 characters
            column2
                Buyer
                    show up to 8 characters
                ItemType
                InvoiceType
            column3
                Item
                    label is "Item"
                    show up to 8 characters
                VendorItem
                    show up to 8 characters
                DueDateRange.Begin
                    label is "FromDueDate"
            column4
                PurchaseOrder
                Invoice
                DueDateRange.End
                    label is "ToDueDate"
