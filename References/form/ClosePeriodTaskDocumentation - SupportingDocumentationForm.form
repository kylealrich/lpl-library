SupportingDocumentationForm is a Form
    is primary
    title is "DocumentDetails"
    Layout
        single column
            paragraph
                update stamp
                    label is "Updated"
                    display as text
                Author.FirstAndLastName
                    label is "By"
                    display as text
            blank line
            Description
            Notes
                use text area of 10 lines
            Attachment
            Link
                label is "URL"
