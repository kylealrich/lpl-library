    title is "InventoriedAllocationsDetailForm"
    Layout
        two column distributed
            WarehouseDemand.DemandDocument
                label is "Document"
                display as text
            WarehouseDemandLine.LineNumber
                label is "LineNumber"
                display as text
            ItemLocation.StockOnHandQuantity
                label is "SOH"
                display as text
            ItemLocation.IntransitQuantity
                label is "Intransit"
                display as text
            ItemLocation.InspectionHoldQuantity
                label is "OnHold"
                display as text
            ItemLocation.AllocatableQuantity
                label is "Allocatable"
                display as text
            Quantity
                label is "OpenDemand"
                display as text
            WarehouseDemand.DemandSystemCode
                label is "Source"
                display as text
            AllocationDate
                label is "Date"
                display as text
            ItemLocation.DefaultTransactionUOM
                label is "UOM"
                display as text
            ItemLocation.AllocatedQuantity
                label is "Allocated"
                display as text
            ItemLocation.InProcessQuantity
                label is "InProcess"
                display as text
            ItemLocation.BackorderedQuantity
                label is "Backordered"
                display as text
            ItemLocation.OnOrderQuantity
                label is "OnOrderQuantity"
                display as text
