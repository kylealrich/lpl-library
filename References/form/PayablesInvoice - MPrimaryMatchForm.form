MPrimaryMatchForm is a Form
    title is "PrimaryMatchForm"
    Layout
        visible when (Vendor.RequireMatchReference)
            single column
                MatchReferenceNumber
        two column distributed
            column1
                ReasonCode
            column2
                ReasonCode.Description
                    no label
                    display as text
        single column
            PrintChargeback
                label is "PrintCreditMemo"
        two column distributed
            column1
                PurchaseFromLocation
                    label is "PurchaseFrom"
                    select is APVendorPurchaseLocationsSelect
            column2
                PurchaseFromLocation.VendorName
                    no label
                    display as text
        single column
            RetailAmount
        visible when (InvoiceType.CreditMemo)
            two column distributed
                column1
                VendorReturn
                    label is "ReturnNumber"
                    select is VendorReturnsByCompanyVendorRel.VendorReturnList
                column2
                    VendorReturn.ReferenceNumber
                        no label
                        display as text
        visible when (ShowOriginalInvoice)
            two column distributed
                column1
                    OriginalInvoice
                column2
                    OriginalInvoice.Invoice
                        no label
                        display as text
        visible when (HasMatchInvoiceReferenceRel)
            two column distributed
                column1
                    MatchInvoiceReferenceRel.OriginalInvoice
                        label is "OriginalInvoice"
                        display as text
                column2
                    MatchInvoiceReferenceRel.OriginalInvoice.Invoice
                        display as text
        visible when (not MatchProcessType.Expense)
            header2 of "MatchSettings"
            two column distributed
                column1
                    visible when (not MatchProcessType.Expense)
                        visible when (IsNotMatched)
                            RuleGroup
                        visible when (not IsNotMatched)
                            RuleGroup
                                display as text
                column2
                    RuleGroup.Description
                        no label
                        display as text
            visible when (not MatchProcessType.Expense)
                two column distributed
                    MatchProcessType
                        display as text
                    MatchStatus
                        display as text
                    MatchLevel
                        display as text
                    MatchDate
                        display as text
