ContractWizard is a WizardForm
    use for action Draft.CreateFromWizard
    title is "CreateContractFromWizard"
    summary form is ContractWizardFinish
    show steps
    Contract is a Panel
        form is ContractWizardMain
    Commitments is a Panel
        valid when (CommitmentForContractWizard)
        form is PurchaseCommitments
    Retainage is a Panel
        valid when (ContractGroup.WizardRetainage)
        form is Retainage
    Terms is a Panel
        valid when (ContractGroup.WizardTerms)
        list is ContractArticle set.ContractWizardArticles
            helper list is ArticleDisplayRel.ContractArticles
                action is AttachToContractWithTerms
                action is AttachToContractWithoutTerms
    Questions is a Panel
        valid when (ContractGroup.WizardQuestions)
        list is ContractQuestion set.ContractQuestions
            helper list is AllQuestionsRel.AttachToContractQuestions
                action is AttachQuestionToContract
    Attachments is a Panel
        valid when (ContractGroup.WizardAttachments)
        list is ContractAttachmentRel.ContractAttachmentList
    Milestones is a Panel
        valid when (ContractGroup.WizardMilestones)
        list is ContractMilestone set.ContractWizardMilestones
    Deliverables is a Panel
        valid when (ContractGroup.WizardDeliverables)
        list is ContractDeliverable set.ContractWizardDeliverables
    Rebates is a Panel
        valid when (ContractGroup.WizardRebates)
        list is ContractRebate set.ContractWizardRebates
    Comments is a Panel
        valid when (ContractGroup.WizardComments)
        list is ContractCommentRel.ContractComments
    Pricing is a Panel
        valid when (PricingForContractWizard)
        list is ContractPricing set.ContractPricings
    Tiers is a Panel
        valid when (IsManufacturerContract)
        list is ContractTier set.ContractWizardTiers
    Lines is a Panel
        mouse over text is "UseTheSaveActionAfterLineInformationHasBeenChanged"
        list is ContractLine set.ContractWizardLines
    Members is a Panel
        valid when (IsManufacturerContract)
        list is ContractTierMember set.ContractWizardTierMembers
    PricingIdentifiers is a Panel
        valid when (IsDistributorContract)
        list is ContractDistributorPricing set.ContractWizardDistributorPricing
    ManufacturerContracts is a Panel
        valid when (IsDistributorContract)
        list is ManufacturerContractForDistContractRel.AttachedManufacturerContracts
            helper list is MfgContractsRel.AllManufacturerContracts
                action is AttachMfgToContract
    Subcontractors is a Panel
        valid when (SubcontractorContract)
        list is ContractSubcontractor set.ContractSubcontractorList
            helper list is SubcontractorRels.SubcontractorActiveList
                action is AttachToContract
                valid when (not ContractInactiveClosed)
