                        ReceivableCompanyDataGroup.ReceivableDistributionCategory.Description
                            no label
                    paragraph
                        ReceivableCompanyDataGroup.ReceivableProcessLevel
                            label is "ProcessLevel"
                        ReceivableCompanyDataGroup.ReceivableProcessLevel.Name
                            no label
                    CurrencyDecimalsNumber
                    paragraph
                        ReceivableCompanyGroup.OriginCompany
                        ReceivableCompanyGroup.OriginCompany.Name
                            no label
                    header3 of "Transaction"
                    DocumentNumber
                    paragraph
                        ReceivableCompanyDataGroup.Customer
                        ReceivableCompanyDataGroup.Customer.Name
                            no label
                    BatchNumber
                    TransType
                    Invoice
                column2
                    header3 of "Amounts"
                    TransactionDate
                    DistributionAmount.FunctionalAmount.EnteredCurrencyAmount
