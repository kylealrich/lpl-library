MobileRecallNoticeAnalysisDetail is a Form
    title is "_"
    Layout
        header3 of "<RecallNotice.Name>"
        two column table
            column1
                RecallNotice
                Status
                RecallDate
                create date
            column2
                WorkingId
                RecallType
                RecallReceivedDate
        header2 of "Details"
        paragraph
            Description
        two column table
            column1
                RecallDate
                    label is "AnnouncementDate"
                create date
                    label is "CreationDate"
                ClosedTimestamp
                    label is "CloseDate"
                FinalizedDate
            column2
                RecallReceivedDate
                ReleasedTimestamp
                    label is "ReleaseDate"
                visible when (RecallGroup.RecallApprovalRequired)
                    ApprovalTimestamp
                        label is "ApprovalDate"
                blank line
        visible when (NotApplicableTimestamp entered)
            single column
                NotApplicableTimestamp
                    label is "DateSetToNotApplicable"
        single column
            blank line
            DaysInPendingStatus
                label is "DaysToReleaseAfterCreation"
            DaysInActiveStatus
                label is "DaysToCloseAfterRelease"
            visible when (RecallGroup.RecallApprovalRequired)
                DaysInAwaitingApprovalStatus
                    label is "DaysToApproveAfterClose"
            DaysUntilFinalized
                label is "DaysToFinalizeAfterCreation"
        blank line
        visible when (ManufactureDateRange entered)
            paragraph
                ManufactureDateRange.Begin
                    label is "ManufactureDateRange"
                text of "-"
                ManufactureDateRange.End
                    no label
        visible when (DistributionDateRange entered)
            paragraph
                DistributionDateRange.Begin
                    label is "DistributionDateRange"
                text of "-"
                DistributionDateRange.End
                    no label
        single column
            ManufacturerName
                label is "Manufacturer"
        two column table
            column1
                RecallCoordinator.RecallUserName
                    label is "Coordinator"
            column2
                button of "<RecallCoordinator.EmployeeWorkEmailAddress>"
                    link is "mailto:<RecallCoordinator.EmployeeWorkEmailAddress>"
                    display as link
        single column
            RecallCoordinator.Employee.EmployeeWorkTelephone
            button of "PrintAnalysisInformation"
                link is NoticeAnalysisReport
