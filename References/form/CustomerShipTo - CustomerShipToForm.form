CustomerShipToForm is a CompositeForm
    is primary
    title is "CustomerShipTo"
    context form is CustomerShipToContext
        background is theme color
    Address is a Panel
        form is Address
    Options is a Panel
        form is Options
    MultipleTaxCodes is a Panel
        visible when (VertexResultsExist)
        list is VertexTaxAreaIdResultsRel.TaxAreaResultList
    VertexError is a Panel
        visible when (VertexErrorExist)
        list is VertexWebserviceErrorRel.VertexTaxAreaIdErrorList
    Attachments is a Panel
        list is CustomerShipToAttachment set.CustomerShipToAttachmentFormList
