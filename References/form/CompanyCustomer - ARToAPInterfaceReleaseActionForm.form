ARToAPInterfaceReleaseActionForm is an ActionForm
    action is ReleaseToAPInterface
    title is "InterfaceUnreleasedPayables"
    Layout
        two column
            column1
                paragraph
                    Company
                    Company.Name
                        no label
                        display as text
                paragraph
                    PrmDetailPayablesInterface
                        label is "DetailPayablesInterface"
                            left of field
                paragraph
                    PrmReceivableReason
                        label is "ReasonCode"
                        select is ReceivableReasonCodeOSList
                    PrmReceivableReason.Description
                        no label
                        display as text
                paragraph
                    PrmAuthorityCode
                        label is "AuthorityCode"
                    PrmAuthorityCode.Description
                        no label
                        display as text
                paragraph
                    PrmPayVendor
                        label is "PayVendor"
                    PrmPayVendor.VendorName
                        no label
                        display as text
                paragraph
                    PrmRemitToCode
                        label is "RemitToLocation"
                        select is APVendorRemitLocationsSelect
                paragraph
                    PrmDescription
                        label is "Description"
                paragraph
                    PrmPayImmediately
                        label is "PayImmediately"
                paragraph
                    PrmARPostDate
                        label is "PostDate"
                paragraph
                    visible when (not PrmDetailPayablesInterface)
                        PrmAPInvoice
                            label is "APInvoice"
                paragraph
                    visible when (not PrmDetailPayablesInterface)
                        PrmAPInvoiceDate
                            label is "InvoiceDate"
            column2
                paragraph
                    visible when (Company.CashOperatorRequired)
                        PrmOperator
                            label is "Operator"
                paragraph
                    PrmProcessLevel
                        label is "*APProcessLevel"
                    PrmProcessLevel.Name
                        no label
                        display as text
                paragraph
                    PrmAccrualCode
                        label is "APAccrualCode"
                    PrmAccrualCode.Description
                        no label
                        display as text
                paragraph
                    PrmCashCode
                        label is "CashCode"
                    PrmCashCode.Description
                        no label
                        display as text
                paragraph
                    PrmBankTransactionCode
                        label is "PaymentCode"
                        select is CashPaymentsOrBillOfExchangePayments
                    PrmBankTransactionCode.Description
                        no label
                        display as text
                paragraph
                    PrmInvoiceGroup
                        label is "InvoiceGroup"
                paragraph
                    visible when (not PrmDetailPayablesInterface)
                        PrmTermsCode
                            label is "PaymentTerms"
                paragraph
                    visible when (not PrmDetailPayablesInterface)
                        PrmDueDate
                            label is "DueDate"
                paragraph
                    visible when (Company.GeneralLedgerCompany.RequireHSNSACCode)
                        PrmHSNSACCode
                            label is "HSNSACCode"
