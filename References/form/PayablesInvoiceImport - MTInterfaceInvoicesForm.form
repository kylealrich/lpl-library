MTInterfaceInvoicesForm is an ActionForm
    action is MultiThreadInterfaceInvoices
    title is "MultiThreadInterfacePayablesInvoices"
    Layout
        single column
            PrmRunGroup
                label is "RunGroup"
            PrmCompany
                label is "Company"
            PrmAuthorityCode
                label is "AuthorityCode"
            PrmStatus
                label is "Status"
            UpdateTaxSystem
                label is "UpdateTaxSystem"
                    left of field
            UpdateVendorBalance
                label is "UpdateVendorBalance"
                    left of field
            LastYearEndingDate
            IncludeInvoicesInError
                label is "IncludeInvoicesInError"
                    left of field
            PrmMoveErrorsToNewRunGroup
                label is "MoveErrorsToNewRunGroup"
                    left of field
            visible when (PrmMoveErrorsToNewRunGroup)
                PrmErrorRunGroupPrefix
                    label is "ErrorRunGroupPrefix"
            PerformBalanceEdits
                label is "PerformBalanceEdits"
                    left of field
            PrmJournalizeDistributions
                label is "JournalizeReleasedDistributions"
                    left of field
            PrmThreads
            KeepOriginalRunGroup
