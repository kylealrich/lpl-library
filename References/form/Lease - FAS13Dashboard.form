FAS13Dashboard is a Form
    Layout
        visible when (ShowASC842Classification)
            single column
                LeaseClassification
                    label is "Classification"
                    display as tag
                        color of blue
            paragraph distributed
                Test7a
                    label is "OwnershipTest"
                Test7b
                    label is "BargainPurchaseOption"
            single column
                RemainingLifeToTotalLife
                    label is "RemainingLifeToTotalLife"
                LeaseTermToRemainingLife
                    label is "EconomicLifeToLeaseTerm"
                NetPresentValueToMarketValue
                    label is "PresentValueToMarketValue"
        visible when (ShowGASBPresentValue)
            visible when (not IsSameCurrency)
                two column table
                    column headers
                        "LeaseCurrency"
                        "BaseCurrency"
                    column1
                        blank line
                        paragraph
                            LeaseObligation.TransactionAmount
                                label is "LeaseObligation"
                            Currency
                                no label
                        paragraph
                            LeaseROUAssetAmount.TransactionAmount
                                label is "RightOfUse"
                            Currency
                                no label
                    column2
                        blank line
                        paragraph
                            LeaseObligation.BaseAmount.EnteredCurrencyAmount
                                label is "LeaseObligation"
                            LeaseObligation.BaseAmount.ToCurrency
                                no label
                        paragraph
                            LeaseROUAssetAmount.BaseAmount.EnteredCurrencyAmount
                                label is "RightOfUse"
                            LeaseROUAssetAmount.BaseAmount.ToCurrency
                                no label
                single column
                    LeaseTermToRemainingLife
                        label is "EconomicLifeToLeaseTerm"
                    NetPresentValueToMarketValue
                        label is "PresentValueToMarketValue"
            visible when (IsSameCurrency)
                two column table
                    column headers
                        "LeaseCurrency"
                        "_"
                    column1
                        blank line
                        LeaseObligation.TransactionAmount
                            label is "LeaseObligation"
                        LeaseROUAssetAmount.TransactionAmount
                            label is "RightOfUse"
                    column2
                        blank line
                        Currency
                            no label
                        Currency
                            no label
                single column
                    LeaseTermToRemainingLife
                        label is "EconomicLifeToLeaseTerm"
                    NetPresentValueToMarketValue
                        label is "PresentValueToMarketValue"
