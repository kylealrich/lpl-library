ReconciliationInterfaceForm is a Form
    title is "TransactionInterface"
    Layout
        paragraph
            ReconciliationAccountStructure
            ReconciliationAccountStructure.Description
                no label
                display as text
        visible when (ReconciliationAccount.LevelType.Group)
            paragraph
                ReconciliationAccount.ReconciliationGroup
                    display only
        visible when (ReconciliationAccount.IsAccountWithinGroup)
            paragraph
                ReconciliationAccount.ParentReconciliationAccount.ReconciliationGroup
                    display only
        visible when (DisplayReconciliationAccountSelect)
            paragraph
                ReconciliationAccount
                ReconciliationAccount.ReconciliationCodeBlock
                    display only
                ReconciliationAccount.ReconciliationCodeBlock.GeneralLedgerChartAccount.AccountDescription
                    no label
                    display as text
        visible when (DisplayReconciliationNonAccountSelect)
            paragraph
                ReconciliationAccount
                    select is NonAccountReconciliationSelectList
                ReconciliationAccount.NonAccountReconciliation
                    display only
        visible when (ReconciliationAccountStructure.ReconciliationType.Account)
            paragraph
                visible when (ReconciliationInterfaceExists)
                    ReconciliationAccount.ReconciliationCodeBlock
                        display only
                    ReconciliationAccount.ReconciliationCodeBlock.GeneralLedgerChartAccount.AccountDescription
                        no label
                        display as text
        visible when (ReconciliationAccountStructure.ReconciliationType.NonAccount)
            paragraph
                visible when (ReconciliationInterfaceExists)
                    ReconciliationAccount.NonAccountReconciliation
                        display only
                    ReconciliationAccount.Description
                        no label
                        display as text
        visible when (ReconciliationInterfaceExists)
            paragraph
                ReconciliationAccount
                    display as text
        header2 of "_"
        two column distributed
            TransactionDate
            PostingDate
        two column distributed
            SystemCode
            SourceCode
        two column distributed
            DetailLineNumber
            TransactionNumber
        two column distributed
            column1
                SignedAmount.CurrencyAmount
                    label is "SignedAmount"
            column2
                paragraph
                    Currency
                    AmountType
                        display as text
        visible when (ReconciliationAccountStructure.ReconciliationType.NonAccount)
            paragraph
                NonAccountFunctionalCurrency
        two column distributed
            UnitsAmount
            Reference
        two column distributed
            Description
            Operator
                label is "CreatedBy"
        visible when (ReconciliationInterfaceExists)
            blank line
            header2 of "CurrencyAmounts"
            paragraph
                visible when (false)
                    ReconciliationFromEntity
                    SignedAmount.AssignToCurrency
                SignedAmount.FunctionalAmount.EnteredCurrencyAmount
                    label is "Functional"
                    display only
                visible when (ReconciliationAccountStructure.ReconciliationType.Account)
                    SignedAmount.AccountingEntity.FunctionalCurrency
                        no label
                        display only
                visible when (ReconciliationAccountStructure.ReconciliationType.NonAccount)
                    NonAccountFunctionalCurrency
                        no label
                        display only
                SignedAmount.FunctionalAmount.EnteredCurrencyRate
                    no label
                    display only
            visible when (SignedAmount.AccountingEntity.AlternateCurrencyExists)
                paragraph
                    SignedAmount.AlternateAmount.EnteredCurrencyAmount
                        label is "Alternate"
                        display only
                    SignedAmount.AccountingEntity.AlternateCurrency
                        no label
                        display only
                    SignedAmount.AlternateAmount.EnteredCurrencyRate
                        no label
                        display only
            visible when (SignedAmount.AccountingEntity.AlternateCurrency2Exists)
                paragraph
                    SignedAmount.AlternateAmount2.EnteredCurrencyAmount
                        label is "Alternate2"
                        display only
                    SignedAmount.AccountingEntity.AlternateCurrency2
                        no label
                        display only
                    SignedAmount.AlternateAmount2.EnteredCurrencyRate
                        no label
                        display only
            visible when (SignedAmount.AccountingEntity.AlternateCurrency3Exists)
                paragraph
                    SignedAmount.AlternateAmount3.EnteredCurrencyAmount
                        label is "Alternate3"
                        display only
                    SignedAmount.AccountingEntity.AlternateCurrency3
                        no label
                        display only
                    SignedAmount.AlternateAmount3.EnteredCurrencyRate
                        no label
                        display only
            visible when (SignedAmount.InterAccountingEntityAlternate)
                paragraph
                    SignedAmount.ToAlternateAmount.EnteredCurrencyAmount
                        label is "PostToAlternate"
                        display only
                    SignedAmount.TransactionCodeBlock.ToAccountingEntity.AlternateCurrency
                        no label
                        display only
                    SignedAmount.ToAlternateAmount.EnteredCurrencyRate
                        no label
                        display only
            visible when (SignedAmount.InterAccountingEntityAlternate2)
                paragraph
                    SignedAmount.ToAlternateAmount2.EnteredCurrencyAmount
                        label is "PostToAlternate2"
                        display only
                    SignedAmount.TransactionCodeBlock.ToAccountingEntity.AlternateCurrency2
                        no label
                        display only
                    SignedAmount.ToAlternateAmount2.EnteredCurrencyRate
                        no label
                        display only
            visible when (SignedAmount.InterAccountingEntityAlternate3)
                paragraph
                    SignedAmount.ToAlternateAmount3.EnteredCurrencyAmount
                        label is "PostToAlternate3"
                        display only
                    SignedAmount.TransactionCodeBlock.ToAccountingEntity.AlternateCurrency3
                        no label
                        display only
                    SignedAmount.ToAlternateAmount3.EnteredCurrencyRate
                        no label
                        display only
            visible when (SignedAmount.ProjectTransaction)
                paragraph
                    SignedAmount.ProjectAmount.EnteredCurrencyAmount
                        label is "<SignedAmount.FinanceEnterpriseGroup.ProjectLabel>"
                        display only
                    SignedAmount.TransactionCodeBlock.Project.Currency
                        no label
                        display only
                    SignedAmount.ProjectAmount.EnteredCurrencyRate
                        no label
                        display only
            visible when (SignedAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                paragraph
                    SignedAmount.ReportAmount1.EnteredCurrencyAmount
                        label is "<SignedAmount.FinanceEnterpriseGroup.ReportOne>"
                        display only
                    SignedAmount.FinanceEnterpriseGroup.ReportCurrencyOne
                        no label
                        display only
                    SignedAmount.ReportAmount1.EnteredCurrencyRate
                        no label
                        display only
            visible when (SignedAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                paragraph
                    SignedAmount.ReportAmount2.EnteredCurrencyAmount
                        label is "<SignedAmount.FinanceEnterpriseGroup.ReportTwo>"
                        display only
                    SignedAmount.FinanceEnterpriseGroup.ReportCurrencyTwo
                        no label
                        display only
                    SignedAmount.ReportAmount2.EnteredCurrencyRate
                        no label
                        display only
            visible when (SignedAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                paragraph
                    SignedAmount.ReportAmount3.EnteredCurrencyAmount
                        label is "<SignedAmount.FinanceEnterpriseGroup.ReportThree>"
                        display only
                    SignedAmount.FinanceEnterpriseGroup.ReportCurrencyThree
                        no label
                        display only
                    SignedAmount.ReportAmount3.EnteredCurrencyRate
                        no label
                        display only
            visible when (SignedAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                paragraph
                    SignedAmount.ReportAmount4.EnteredCurrencyAmount
                        label is "<SignedAmount.FinanceEnterpriseGroup.ReportFour>"
                        display only
                    SignedAmount.FinanceEnterpriseGroup.ReportCurrencyFour
                        no label
                        display only
                    SignedAmount.ReportAmount4.EnteredCurrencyRate
                        no label
                        display only
            visible when (SignedAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                paragraph
                    SignedAmount.ReportAmount5.EnteredCurrencyAmount
                        label is "<SignedAmount.FinanceEnterpriseGroup.ReportFive>"
                        display only
                    SignedAmount.FinanceEnterpriseGroup.ReportCurrencyFive
                        no label
                        display only
                    SignedAmount.ReportAmount5.EnteredCurrencyRate
                        no label
                        display only
