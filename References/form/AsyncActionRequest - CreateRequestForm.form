CreateRequestForm is a Form
    title is "Schedule"
    Layout
        single column
            Name
                label is "ScheduledActionName"
        header2 of "NotifyRecipient"
        single column
            visible when (false)
                EmailAddressMultiple
                    label is "EmailAddress_(Optional)"
                    use text area of 1 lines
                ActionGroupNotification.EmailAddressMultiple
                    use text area of 1 lines
                ActionGroupNotification.ThresholdMinutes
            TranEmailAddressMultiple
                label is "EmailAddress_(Optional)"
                use text area of 1 lines
            NotificationType
            text of "NotificationTypeForEmailAndRequestsSupportingUserNotifications"
        header2 of "ExecutionDurationNotification"
        single column
            TranAGEmailAddress.EmailAddressMultiple
                use text area of 1 lines
            TranAGEmailAddress.ThresholdMinutes
        header2 of "SchedulingDetails"
        paragraph
            ScheduleType
                when value changed
                    refresh DisplayExclusions
            TimeToExec
                label is "FirstTimeToRun"
            LastTimeToExec
                label is "LatestTimeToRun"
            text of untranslatable:"<ContextTimeZoneDisplayName>"
        visible when (DisplayExclusions)
            paragraph
                ScheduleExclusion.ScheduleExclusion
                    select is ScheduleExclusionsList
                ScheduleExclusion.ScheduleExclusion.Description
                    no label
                text of "or"
                ScheduleExclusion.ScheduleExclusionSet
                ScheduleExclusion.ScheduleExclusionSet.Description
                    no label
        paragraph
            valid when (not DisableConcurrency)
                ScheduleConcurrency
                    label is "RunActionConcurrently"
        header2 of "Additional"
        paragraph
            EnterMisFireData
                label is "OverrideActionFailureConfiguration"
