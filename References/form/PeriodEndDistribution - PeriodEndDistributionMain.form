                GLFinanceCurrencyAmount.FinanceEnterpriseGroup.ReportCurrencyFive
                    no label
                    display only
                GLFinanceCurrencyAmount.ReportAmount5.EnteredCurrencyRate
                    no label
                    display only
        visible when (DistributionExists)
            header2 of "Timeline"
            four column distributed
                create stamp
                    label is "Created"
                    display as text
                visible when (PendingApprovalTimestamp entered)
                    PendingApprovalTimestamp
                        label is "PendingApproval"
                        display as text
                ReleasedTimestamp
                    label is "Released"
                    display as text
                PostedTimestamp
                    label is "Posted"
                    display as text
