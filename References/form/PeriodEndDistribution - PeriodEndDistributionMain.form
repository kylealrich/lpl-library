PeriodEndDistributionMain is a Form
    Layout
        blank line
        single column
            GLFinanceCodeBlock
                label is "FinanceCodeBlock"
                compact format
            visible when (GLFinanceCodeBlock entered)
                GLFinanceCodeBlock.CompactDescription
                    no label
                    display as text
            GLTransactionAmount
                label is "TransactionAmount"
            Description
            paragraph
                Comment
                    use text area of 2 lines
        blank line
        single column
        header2 of "CurrencyAmounts"
        paragraph
            visible when (false)
                GLFinanceCurrencyAmount.AssignToCurrency
            GLFinanceCurrencyAmount.FunctionalAmount.EnteredCurrencyAmount
                label is "Functional"
                display only
            GLFinanceCurrencyAmount.AccountingEntity.FunctionalCurrency
                no label
                display only
            GLFinanceCurrencyAmount.FunctionalAmount.EnteredCurrencyRate
                no label
                display only
        visible when (GLFinanceCurrencyAmount.AccountingEntity.AlternateCurrencyExists)
            paragraph
                GLFinanceCurrencyAmount.AlternateAmount.EnteredCurrencyAmount
                    label is "Alternate"
                    display only
                GLFinanceCurrencyAmount.AccountingEntity.AlternateCurrency
                    no label
                    display only
                GLFinanceCurrencyAmount.AlternateAmount.EnteredCurrencyRate
                    no label
                    display only
        visible when (GLFinanceCurrencyAmount.AccountingEntity.AlternateCurrency2Exists)
            paragraph
                GLFinanceCurrencyAmount.AlternateAmount2.EnteredCurrencyAmount
                    label is "Alternate2"
                    display only
                GLFinanceCurrencyAmount.AccountingEntity.AlternateCurrency2
                    no label
                    display only
                GLFinanceCurrencyAmount.AlternateAmount2.EnteredCurrencyRate
                    no label
                    display only
        visible when (GLFinanceCurrencyAmount.AccountingEntity.AlternateCurrency3Exists)
            paragraph
                GLFinanceCurrencyAmount.AlternateAmount3.EnteredCurrencyAmount
                    label is "Alternate3"
                    display only
                GLFinanceCurrencyAmount.AccountingEntity.AlternateCurrency3
                    no label
                    display only
                GLFinanceCurrencyAmount.AlternateAmount3.EnteredCurrencyRate
                    no label
                    display only
        visible when (GLFinanceCurrencyAmount.InterAccountingEntityAlternate)
            paragraph
                GLFinanceCurrencyAmount.ToAlternateAmount.EnteredCurrencyAmount
                    label is "PostToAlternate"
                    display only
                GLFinanceCurrencyAmount.TransactionCodeBlock.ToAccountingEntity.AlternateCurrency
                    no label
                    display only
                GLFinanceCurrencyAmount.ToAlternateAmount.EnteredCurrencyRate
                    no label
                    display only
        visible when (GLFinanceCurrencyAmount.InterAccountingEntityAlternate2)
            paragraph
                GLFinanceCurrencyAmount.ToAlternateAmount2.EnteredCurrencyAmount
                    label is "PostToAlternate2"
                    display only
                GLFinanceCurrencyAmount.TransactionCodeBlock.ToAccountingEntity.AlternateCurrency2
                    no label
                    display only
                GLFinanceCurrencyAmount.ToAlternateAmount2.EnteredCurrencyRate
                    no label
                    display only
        visible when (GLFinanceCurrencyAmount.InterAccountingEntityAlternate3)
            paragraph
                GLFinanceCurrencyAmount.ToAlternateAmount3.EnteredCurrencyAmount
                    label is "PostToAlternate3"
                    display only
                GLFinanceCurrencyAmount.TransactionCodeBlock.ToAccountingEntity.AlternateCurrency3
                    no label
                    display only
                GLFinanceCurrencyAmount.ToAlternateAmount3.EnteredCurrencyRate
                    no label
                    display only
        visible when (GLFinanceCurrencyAmount.ProjectTransaction)
            paragraph
                GLFinanceCurrencyAmount.ProjectAmount.EnteredCurrencyAmount
                    label is "<GLFinanceCurrencyAmount.FinanceEnterpriseGroup.ProjectLabel>"
                    display only
                GLFinanceCurrencyAmount.TransactionCodeBlock.Project.Currency
                    no label
                    display only
                GLFinanceCurrencyAmount.ProjectAmount.EnteredCurrencyRate
                    no label
                    display only
        visible when (GLFinanceCurrencyAmount.FinanceEnterpriseGroup.ReportCurrencyOneEntered)
            paragraph
                GLFinanceCurrencyAmount.ReportAmount1.EnteredCurrencyAmount
                    label is "<GLFinanceCurrencyAmount.FinanceEnterpriseGroup.ReportOne>"
                    display only
                GLFinanceCurrencyAmount.FinanceEnterpriseGroup.ReportCurrencyOne
                    no label
                    display only
                GLFinanceCurrencyAmount.ReportAmount1.EnteredCurrencyRate
                    no label
                    display only
        visible when (GLFinanceCurrencyAmount.FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
            paragraph
                GLFinanceCurrencyAmount.ReportAmount2.EnteredCurrencyAmount
                    label is "<GLFinanceCurrencyAmount.FinanceEnterpriseGroup.ReportTwo>"
                    display only
                GLFinanceCurrencyAmount.FinanceEnterpriseGroup.ReportCurrencyTwo
                    no label
                    display only
                GLFinanceCurrencyAmount.ReportAmount2.EnteredCurrencyRate
                    no label
                    display only
        visible when (GLFinanceCurrencyAmount.FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
            paragraph
                GLFinanceCurrencyAmount.ReportAmount3.EnteredCurrencyAmount
                    label is "<GLFinanceCurrencyAmount.FinanceEnterpriseGroup.ReportThree>"
                    display only
                GLFinanceCurrencyAmount.FinanceEnterpriseGroup.ReportCurrencyThree
                    no label
                    display only
                GLFinanceCurrencyAmount.ReportAmount3.EnteredCurrencyRate
                    no label
                    display only
        visible when (GLFinanceCurrencyAmount.FinanceEnterpriseGroup.ReportCurrencyFourEntered)
            paragraph
                GLFinanceCurrencyAmount.ReportAmount4.EnteredCurrencyAmount
                    label is "<GLFinanceCurrencyAmount.FinanceEnterpriseGroup.ReportFour>"
                    display only
                GLFinanceCurrencyAmount.FinanceEnterpriseGroup.ReportCurrencyFour
                    no label
                    display only
                GLFinanceCurrencyAmount.ReportAmount4.EnteredCurrencyRate
                    no label
                    display only
        visible when (GLFinanceCurrencyAmount.FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
            paragraph
                GLFinanceCurrencyAmount.ReportAmount5.EnteredCurrencyAmount
