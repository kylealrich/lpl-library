UX1BatchReviewContextForm is a Form
    title is "BatchReview"
    Layout
        display as text
            five column distributed
                column1
                    paragraph
                        Company
                        Company.Name
                            label is "_"
                    ReceivablePaymentHeader
                        label is "Batch"
                column2
                    Company.Currency
                        label is "CompanyCurrency"
                    Currency
                        label is "BatchCurrency"
                column3
                    DepositDate
                    GeneralLedgerDate
                        label is "GlobalLedgerDate"
                column4
                    CashCode
                    ReceivableProcessLevel.RepresentativeText
                column5
                    Status
                        display as tag
                            color of gray when (Status.OpenBatchUnreleased)
                            color of green
                    paragraph
                        DetailCount
                        DetailTotal
