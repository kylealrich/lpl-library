CreateChildWithinForm is an ActionForm
    action is CreateChildWithin
    title is "CreateChild<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>For<DisplayAccountingUnit>"
    Layout
        single column
            NewAccountingUnitType
                label is "Type"
            NewAccountingUnit
                label is "<FinanceEnterpriseGroup.AccountingUnitLabel>"
            NewDescription
                label is "Description"
            visible when (NewAccountingUnitType.Posting)
                NewActive
                    label is "Active"
            visible when (NewAccountingUnitType.Summary)
                NewBudgetIdentifier
                    label is "BudgetIdentifier"
        single column
            NewAddressCode
                label is "AddressCode"
            effective date
                label is "Effective_Date"
