PhoneApprovalBudgetChangeOrderForm is a CompositeForm
    title is "BudgetChangeApproval"
    context form is PhoneControlForm
        background is theme color
    LinesPanel is a Panel
        title is "Lines"
        list is BudgetChangeOrderLine set.PhoneLines
    CommentsPanel is a Panel
        title is "Comments"
        list is BudgetChangeOrderComment set.PhoneCommentsForChangeOrder
