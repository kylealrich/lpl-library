SupplierRegistration is a CompositeForm
    is primary
    title is "<DisplaySupplierNumber>"
    context form is SupplierContext
        background is theme color
    PrimaryContactInformation is a Panel
        visible when (not SupplierExists)
        form is BuyerRegistration
    ViewPrimaryContactInformation is a Panel
        visible when (SupplierExists)
        form is BuyerRegistrationUpdate
    Contacts is a Panel
        title is "Contacts<SupplierContactCount>"
        list is SupplierSourceId set.SupplierContacts
    ContactBankInformation is a Panel
        visible when (CanDisplayBankInformation)
        list is SupplierSourceId set.BuyerContactBankInformation
    Certifications is a Panel
        title is "Certifications<SupplierCertificationCount>"
        list is AllSupplierCertificationsRel.SupplierCertifications
    CompanyInformation is a Panel
        valid when (SupplierExists)
        form is Supplier
    CompanyBankInformation is a Panel
        visible when (CanDisplayBankInformation)
        form is BankInformationInputForm
    Diversity is a Panel
        title is "Diversity<DiversityCount>"
        list is SupplierDiversityResponse set.primary
    Questions is a Panel
        visible when (SupplierGroup.QuestionsExist)
        template is SupplierGroup.ActiveSupplierGroupQuestions
            business class is SupplierQuestionResponse
                form is SupplierQResponse
    QuestionsConditional is a Panel
        title is "Questions"
        visible when (QuestionsExist)
        template is ActiveSupplierQuestions
            business class is SupplierQuestionsSetResponse
                form is SupplierQResponse
    Comments is a Panel
        title is "Comments<SupplierCommentCount>"
        list is SupplierComment set.SupplierComment
    PerformanceEvaluations is a Panel
        title is "PerformanceEvaluations<SupplierPerformanceCount>"
        visible when (SupplierExists)
        list is SupplierPerformanceEvaluation set.SupplierEvaluationsPrimaryList
    PortalMessages is a Panel
        visible when (SupplierExists)
        list is SupplierMessage set.SupplierMessages
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
    PurchaseFrom is a Panel
        title is "PurchaseFroms<PurchaseFromIndicator>"
        visible when (IsVendor)
        list is SupplierPurchaseFrom set.SupplierPurchaseFromList
