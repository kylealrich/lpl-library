CustomerOrderProcess is a Form
    Layout
        header2 of "Process"
        two column distributed
            column1
                paragraph
                    HoldCode
                    HoldCode.Description
                        no label
                        display as text
                paragraph
                    OrderEntryOrderSource
                        label is "OrderSource"
                    OrderEntryOrderSource.SrcDesc
                        no label
                        display as text
                CustomerPurchaseOrder
                    label is "CustomerP\\O"
                ShipComplete
                    label is "ShipComplete"
                PickingPriority
                    label is "Priority"
            column2
                SalesQuote
                    label is "SalesQuote"
                FutureOrder
                    label is "FutureOrder"
                Acknowledgement
                    label is "Acknowledgement"
                ProofOfDeliveryRequired
                    label is "ProofOfDeliveryRequired"
                BackOrder
                    label is "AllowBackorders"
        header2 of "Freight"
        two column distributed
            column1
                paragraph
                    BillingFreightCode
                        label is "FreightCode"
                    BillingFreightCode.BillingFreightDescription
                        no label
                        display as text
                paragraph
                    FreightCharge
                        label is "EnteredCharges"
                paragraph
                    TransactionId
                        label is "TransportationI\\D"
                paragraph
                    ShipTerm
            column2
                paragraph
                    Carrier
                    Carrier.VendorName
                        no label
                        display as text
                paragraph
                    ShippingMethod
                    ShippingMethod.Description
                        no label
                        display as text
                paragraph
                    ActualFreightCharge
                        label is "ActualFreightCharges"
                        display as text
