AttributeUpdateCompositeConsolidatedItemMaster is a CompositeForm
    title is "UpdateAttributes"
    context form is AttributeUpdateContextConsolidatedItemMaster
        background is theme color
    Actions
        action is UpdateAttributes
        action is RecheckErrors
            action icon is validate
            hide when invalid
        action is UpdateAttributesInstance
            hide when invalid
        action is SetToNotProcessed
            hide when invalid
        action is ChangeItemNumberForGTIN
            hide when invalid
        action is save
            label is "_"
    Attributes is a Panel
        form is AttributesFormConsolidatedItemMaster
    ContractLinesFound is a Panel
        visible when (ContractLinesForConsolidatedFound)
        list is OpenContractLinesRel.ContractLinesForAttributeUpdate
    Errors is a Panel
        visible when (ErrorsExist)
        list is ContractLineAttributeError set.ContractLineAttributeErrorList
