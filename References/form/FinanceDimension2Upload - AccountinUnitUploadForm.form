    propagate as a drill
    title is "FinanceDimension2Upload_<FinanceDimension2Structure>_<FinanceDimension2Upload.RunGroup>_<FinanceDimension2Upload.Sequence>"
    context form is FinanceDimension2UploadContext
    Main is a Panel
        form is FinanceDimension2UploadMain
    ProjectFunding is a Panel
        title is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
        visible when (FinanceEnterpriseGroup.ProjectInvoicing)
        form is ProjectFundingForm
    ReceivableCustomer is a Panel
        title is "ReceivableCustomer"
        visible when (FundingType.ExternalGrant)
        form is ReceivableCustomerForm
    ReceivableCustomer2 is a Panel
        title is "ReceivableCustomer"
        visible when (FundingType.ExternalCustomer)
        form is ReceivableCustomerForm2
    Grantor is a Panel
        title is "GrantReporting"
        visible when (FundingType.ExternalGrant)
        form is GrantorForm
    UserFields is a Panel
        form is UserFieldsForm
