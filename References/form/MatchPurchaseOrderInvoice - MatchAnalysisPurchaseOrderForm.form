MatchAnalysisPurchaseOrderForm is a Form
    title is "PurchaseOrder"
    Layout
        four column distributed
            column1
                PurchaseOrder.DerivedPONumberAndCode
                    label is "PurchaseOrder"
                    header4
                PurchaseOrder.Buyer.DerivedBuyerNameID
                    label is "Buyer"
                paragraph
                    button of icon.mail
                        visible when (PurchaseOrder.Buyer.Employee.EmailAddressExists)
                        link is "mailto:<PurchaseOrder.Buyer.EmployeeWorkEmailAddress>"
                    button of icon.phone
                        visible when (PurchaseOrder.Buyer.Employee.EmployeeWorkPhoneExists)
                        link is "tel:<PurchaseOrder.Buyer.EmployeeWorkTelephone>"
                visible when (PurchaseOrder.Reference1 entered)
                    PurchaseOrder.Reference1
                        label is "Reference1"
            column2
                PurchaseOrder.PurchaseOrderDate
                    label is "Date"
                    header4
                PurchaseOrder.Company.DerivedCompanyNameID
                    label is "Company"
                PurchaseOrder.ShipToLocation.DerivedNameID
                    label is "ShipTo"
                visible when (PurchaseOrder.Reference2 entered)
                    PurchaseOrder.Reference2
                        label is "Reference2"
            column3
                paragraph
                    PurchaseOrder.CalculateTotalAmount
                        label is "Total"
                        header4
                    PurchaseOrder.Currency
                        no label
                PurchaseOrder.Vendor.DerivedVendorNameID
                    label is "Vendor"
                PurchaseOrder.ProcessLevel
                    label is "ProcessLevel"
            column4
                PurchaseOrder.DerivedPurchaseOrderStatus
                    label is "Status"
                    header4
                visible when (PurchaseOrder.PurchaseFromLocation entered)
                    PurchaseOrder.PurchaseFromLocation.DerivedLocationNameID
                        label is "PurchaseFromLocation"
                PurchaseOrder.NumberOfLines
                    label is "Lines"
                visible when (PurchaseOrder.DefaultDeliveryDate entered)
                    PurchaseOrder.DefaultDeliveryDate
                        label is "DeliveryDate"
