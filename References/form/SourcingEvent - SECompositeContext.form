SECompositeContext is a Form
    title is "EventSummary!"
    Layout
        visible when (not InformalQuote)
            header2 of "Dates"
            two column
                PreviewDate
                    label is "Preview"
                    display when blank
                OpenDate
                    label is "Open"
                    display when blank
                CloseDate
                    label is "Close"
                    display when blank
                QandAOpenDate
                    label is "QAndAOpen"
                    display when blank
                QandACloseDate
                    label is "QAndAClose"
                    display when blank
                DisputeCloseDate
                    label is "DisputeClose"
                    display when blank
                visible when (StepTwoStarted)
                    StepTwoCloseDate
                visible when (BestAndFinalStarted)
                    BestAndFinalCloseDate
        header2 of "Description"
        Description
            use text area of 5 lines
        header2 of "Rules"
        two column
            SourcingEventType
                label is "Type"
            Sealed
                label is "SealedBid"
            CurrencyCode
                label is "Currency"
            RequireAllLines
                label is "AllLinesRequired"
        two column
            RequireBidBond
                label is "BidBondRequired"
            visible when (RequireBidBond)
                BidBondPercent
