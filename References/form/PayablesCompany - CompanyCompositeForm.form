CompanyCompositeForm is a CompositeForm
    is primary
    title is "<DerivedFormTitle>"
    context form is CompanyContext
        background is theme color
    Main is a Panel
        form is Main
    Options is a Panel
        form is Options
    DistributionAccounts is a Panel
        list is PayablesDistributionAccountStructure set.DistributionReportingSetUps
    UserFields is a Panel
        form is UserFields
    ProcessLevels is a Panel
        list is PayablesProcessLevel set.PayablesProcessLevelsForCompany
    IntercompanyBilling is a Panel
        form is IntercompanyBillingForm
    DocumentTemplates is a Panel
        form is DocumentTemplatesForm
