ParItemLocationForm is a CompositeForm
    title is "CartParLocation"
    context form is ParItemContext
        background is theme color
    Main is a Panel
        form is ParItemForm
    ReplenishFrom is a Panel
        visible when (not ReplenishmentSourceExists)
        form is ReplenishFrom
    Replenishment is a Panel
        visible when (ReplenishmentSourceExists)
        list is ItemReplenishmentSourcesRel.ReplenishmentPriority
