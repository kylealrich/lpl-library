MobileSupplyChainResourceForm is a CompositeForm
    is primary
    title is "Resource"
    context form is MobileSupplyChainResourceContextForm
        background is theme color
    Main is a Panel
        form is Main
    Locations is a Panel
        valid when (not HasGlobalLocationAccess)
        list is MobileSupplyChainResourceAccess set.MobileSupplyChainResourceAccessList
    MyParForms is a Panel
        valid when (HasLocationAccess)
        list is MobileSupplyChainResourceParLocation set.MobileSupplyChainResourceParLocationList
    DefaultPrinters is a Panel
        list is MobileSupplyChainResourceAccessPrinter set.MobileSupplyChainResourceAccessPrinterList
    DefaultLocation is a Panel
        valid when (HasLocationAccess)
        form is DefaultLocationForm
