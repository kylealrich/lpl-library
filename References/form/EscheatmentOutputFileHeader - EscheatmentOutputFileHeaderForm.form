EscheatmentOutputFileHeaderForm is a CompositeForm
    is primary
    title is "EscheatmentOutputFileHeader"
    context form is EscheatmentOutputFileHeaderContext
        background is theme color
    Actions
        action is PerformEscheatment
            hide when invalid
        action is DeleteEscheatmentOutputFiles
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is refresh
            label is "_"
    Parameters is a Panel
        form is ParametersForm
    OutputFileDetailsSelected is a Panel
        title is "Selected"
        visible when (SelectedWithoutProcessLevelProcessing)
        list is EscheatmentOutputFileDetailsByCashCodeSelectedRel.EscheatmentOutputFileDetailByCashCodeList
    OutputFileDetailsByProcessLevelSelected is a Panel
        title is "Selected"
        visible when (SelectedWithProcessLevelProcessing)
        list is EscheatmentOutputFileDetailsByProcessLevelSelectedRel.EscheatmentOutputFileDetailByProcessLevelList
    OutputFileDetailsProcessed is a Panel
        title is "Processed"
        visible when (ProcessedWithoutProcessLevelProcessing)
        list is EscheatmentOutputFileDetailsByCashCodeProcessedRel.EscheatmentOutputFileDetailByCashCodeList
    OutputFileDetailsByProcessLevelProcessed is a Panel
        title is "Processed"
        visible when (ProcessedWithProcessLevelProcessing)
        list is EscheatmentOutputFileDetailsByProcessLevelProcessedRel.EscheatmentOutputFileDetailByProcessLevelList
    TransactionsInError is a Panel
        title is "Errors"
        visible when (ErrorsExist)
        list is CashLedgerTransactionErrorForEscheatmentRel.EscheatmentErrorList
