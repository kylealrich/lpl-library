AssetBookTransactionForm is a Form
    is primary
    propagate as a drill
    title is "AssetBookTransaction"
    restrict action WizardCreate
    Layout
        single column
            FinanceEnterpriseGroup
            Asset
            Book
            Company
        blank line
        paragraph
            DistributionAccount
                label is "TransactionDimensions"
            TransactionAmount
                label is "TransactionAmount"
            TransientBookCurrency
                label is "TransactionCurrency"
            AssetTransactionAmount.BaseAmount.EnteredCurrencyAmount
                label is "BookAmount"
            Book.Currency
                label is "BookCurrency"
        three column
            TransactionDate
            ProcessDate
            TransactionStatus
                label is "Status"
        three column
            Reference
            AssetAccountType
                label is "AccountType"
            AssetProcess
