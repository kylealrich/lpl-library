StatementParsingRuleGroupDetail is a Form
    is primary
    title is "ParsingRule"
    Layout
        paragraph
            Priority
                display as text
            NewPriority
        paragraph
            StatementParsingRuleGroup
            StatementParsingRuleGroup.Description
                no label
                display as text
        paragraph
            StatementParsingRule
            StatementParsingRule.Description
                no label
                display as text
        blank line
        visible when (StatementParsingRule entered)
            display as text
                single column
                    StatementParsingRule.ParsingRule
                    StatementParsingRule.ParsingRule.Condition
                        no label
                        display as text
                header2 of "ParsingOptions"
                single column
                    paragraph
                        StatementParsingRule.LiteralValueField
                        StatementParsingRule.LiteralValue
                            no label
                    paragraph
                        StatementParsingRule.RemoveCharacterString
                        StatementParsingRule.RemoveString
                            no label
                    paragraph
                        StatementParsingRule.RemoveCharactersBefore
                        StatementParsingRule.RemoveBefore
                            no label
                    paragraph
                        StatementParsingRule.RemoveCharactersAfter
                        StatementParsingRule.RemoveAfter
                            no label
                    paragraph
                        StatementParsingRule.CharacterStringValueRange
                        StatementParsingRule.CharacterStringRange.CharacterFromValue
                            no label
                        visible when (StatementParsingRule.CharacterStringRange.CharacterFromValue entered)
                            text of "-"
                        StatementParsingRule.CharacterStringRange.CharacterToValue
                            no label
