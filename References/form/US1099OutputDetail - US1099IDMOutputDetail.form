US1099IDMOutputDetail is a Form
    restrict action GenerateForm1099
    restrict action PrintForm1099
    restrict action EmailForm1099
    Actions
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is refresh
            label is "_"
        action is save
            hide when invalid
    Layout
        display as text
            single column
                paragraph
                    ReportType
                    US1099OutputHeader.TapeSelection
            two column distributed
                column1
                    header3 of "PayerDetails"
                    FormattedPayorTaxID
                        label is "TIN"
                    PayorName
                        label is "Name"
                    PayorAddress.DeliveryAddress.AddressLine1
                        label is "AddressLine1"
                    PayorAddress.DeliveryAddress.AddressLine2
                        label is "AddressLine2"
                    PayorAddress.DeliveryAddress.AddressLine3
                        label is "AddressLine3"
                    PayorAddress.DeliveryAddress.AddressLine4
                        label is "AddressLine4"
                    PayorAddress.Municipality
                        label is "Municipality"
                    PayorAddress.StateProvince
                        label is "State"
                    PayorAddress.PostalCode
                        label is "PostalCode"
                    DerivedPayorPhoneNumber
                        label is "PhoneNumber"
                    PayorAccountNumber
                        label is "AccountNumber"
                column2
                    header3 of "RecipientDetails"
                    MaskedVendorTaxID
                        label is "TIN"
                    PayorAccountNumber
                        label is "AccountNumber"
                    VendorName
                        label is "Name"
                    VendorAddress.DeliveryAddress.AddressLine1
                        label is "AddressLine1"
                    VendorAddress.DeliveryAddress.AddressLine2
                        label is "AddressLine2"
                    VendorAddress.DeliveryAddress.AddressLine3
                        label is "AddressLine3"
                    VendorAddress.DeliveryAddress.AddressLine4
                        label is "AddressLine4"
                    VendorAddress.Municipality
                        label is "Municipality"
                    VendorAddress.StateProvince
                        label is "State"
                    VendorAddress.PostalCode
                        label is "PostalCode"
                    DerivedForeignCountry
                        label is "ForeignCountry"
            header3 of "IncomeFields"
            two column distributed
                column1
                    IncomeAmount1
                    IncomeAmount2
                    IncomeAmount3
                    IncomeAmount4
                    IncomeAmount5
                    IncomeAmount6
                    IncomeAmount7
                    IncomeAmount8
                    IncomeAmount9
                column2
                    IncomeAmount10
                    IncomeAmount11
                    IncomeAmount12
                    IncomeAmount13
                    IncomeAmount14
                    IncomeAmount15
                    IncomeAmount16
                    IncomeAmount17
                    IncomeAmount18
