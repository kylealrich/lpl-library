ContractParticipantMain is a CompositeForm
    is primary
    title is "ContractParticipant"
    context form is ContractParticipantContext
        background is theme color
    Participant is a Panel
        form is ContractParticipantLocation
    Department is a Panel
        visible when (Contract.IsNotManDistOrStanding)
        form is ContractParticipantDepartment
