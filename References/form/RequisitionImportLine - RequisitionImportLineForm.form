RequisitionImportLineForm is a Form
    title is "RequisitionImportLine"
    Layout
        two column distributed
            FromCompanyLocationBin
            paragraph
                DistributionBy
                DistributionCode
        two column distributed
            paragraph
                AllocationPriority
                    show up to 15 characters
                CreatePurchaseOrder
            OverrideCost
        two column distributed
            paragraph
                LateDeliveryDate
                RequestedDeliveryDate
            TransactionCurrencyCode
        header3 of "PurchasingOptions"
        single column
            NewRequisition
        two column distributed
            Vendor
            PurchaseFromLocation
        two column distributed
            POCode
            Buyer
        two column distributed
            paragraph
                PurchaseTaxable
                PurchaseTaxCode
            paragraph
                CertificationRequired
                InspectionRequired
        two column distributed
            paragraph
                RequisitionInterfaceInformation
            paragraph
                Contract
                ContractLine
        header3 of "ClassesAndCodes"
        two column distributed
            MajorPurchasingClass
                show up to 10 characters
            MinorPurchasingClass
                show up to 10 characters
        two column distributed
            ItemGTIN
            SourcingEventRequired
        two column distributed
            paragraph
                Manufacturer
            ManufacturerNumber
        two column distributed
            CommodityCode
            ConsignCode
        two column distributed
            visible when (IsHSNSACCodeEnabled)
                paragraph
                    HSNSACCode
                    HSNSACCode.Description
                        no label
                        display as text
        header3 of "Miscellaneous"
        two column distributed
            LocationRule
            DeliverTo
        two column distributed
            FillOrKill
            PatientProcedure
        two column distributed
            GlobalLineType
