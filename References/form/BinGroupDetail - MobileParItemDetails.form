MobileParItemDetails is a Form
    title is "ParItemDetails"
    Layout
        display as text
            single column
                Company.RepresentativeText
                    label is "Company"
                InventoryLocation.DerivedInventoryLocationIDName
                    label is "Location"
        single column
            TransientBinSequence
                label is "BinSequence"
        visible when (not BinGroupDetailExists)
            two column grid
                column1 of 50%
                    TransientItem
                        select is MobileItemFilterSelection
                        show up to 5 characters
                column2 of 50%
                    TransientItem.Description
                        no label
                        display as text
        two column grid
            column1 of 50%
                TransientBin
                    select is MobileBinSelect
                TransientParLevel
            column2 of 50%
                TransientLeadtimeDays
                TransientSuggestedReorderPoint
        single column
            TransientUOM.UnitOfMeasure
                select is MobileUnitOfMeasureList
            paragraph
                TransientStandardCost
                    show up to 10 characters
                Company.GeneralLedgerCompany.Currency
                    no label
                    display as text
            TransientInventoryLocationAccountGroup
                select is MobileAccountGroupsSelect
            paragraph distributed
                TransientChargeable
                TransientNoCharge
                TransientActive
            TransientMinimumOrderQuantity
            paragraph
                TransientManufacturer
                    compact format
                    select is MobileManufacturerList
                TransientManufacturer.Description
                    no label
                    display as text
            TransientManufacturerNumber
            first ItemLocationRel.NumberOfBins
                display as text
