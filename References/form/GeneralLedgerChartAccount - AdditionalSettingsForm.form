AdditionalSettingsForm is a Form
    title is "Additional<FinanceEnterpriseGroup.AccountLabel>Setting"
    Layout
        two column distributed
            column1
                paragraph
                    visible when (GeneralLedgerChartAccount not entered)
                        NewAccount
                            label is "Summary<FinanceEnterpriseGroup.AccountLabel>"
                    AccountDescription
                        label is "Description"
            column2
                visible when (ReportingChart entered)
                    NewDisplayOrder
        two column distributed
            column1
                effective date
            column2
                visible when (FinanceEnterpriseGroup.CurrencyTranslationAndRevalue)
                    visible when (ReportingChart entered)
                        paragraph
                            NewTranslationCode
                                label is "TranslationCode"
                            NewTranslationCode.Description
                                no label
                                display as text
                            NewRevalue
                                label is "Revalue"
                    visible when (ReportingChart not entered)
                        paragraph
                            TranslationCode
                                label is "TranslationCode"
                            TranslationCode.Description
                                no label
                                display as text
                            Revalue
        two column distributed
            column1
                visible when (ReportingChart entered)
                    visible when (BalanceSheetAccount)
                        paragraph
                            NewCloseToAccount
                                label is "CloseTo<FinanceEnterpriseGroup.AccountLabel>"
                                select is PostingBalanceSheetAccountList
                            NewCloseToAccount.AccountDescription
                                no label
                                display as text
                    visible when (IncomeStatementAccount)
                        paragraph
                            NewCloseToAccount
                                label is "CloseTo<FinanceEnterpriseGroup.AccountLabel>"
                                select is PostingRetainedEarningAccountList
                            NewCloseToAccount.AccountDescription
                                no label
                                display as text
            column2
                visible when (ChartType.Summary)
                    paragraph
                        SummaryChartAccountMaintenance
                    visible when (IsAchild)
                        paragraph
                            BudgetIdentifier
                            BudgetIdentifier.Description
                                no label
                                display as text
                    visible when (not IsAchild)
                        paragraph
                            BudgetIdentifier
                                display only
                            BudgetIdentifier.Description
                                no label
                                display as text
                visible when (ChartType.Posting)
                    IncludeInBudgetEdit
        two column distributed
            column1
                visible when (ReportingChart entered)
                    NewXbrlTag
                        label is "XbrlTag"
                visible when (ReportingChart not entered)
                    XbrlTag
            column2
                visible when (ReportingChart entered)
                    paragraph
                        RequireProject
                            label is "Require_<FinanceEnterpriseGroup.ProjectLabel>"
                            display only
                        visible when (ChartType.Posting)
                            NewIsGainLossAccount
                                label is "IsGainLossAccount"
                visible when (ReportingChart not entered)
                    paragraph
                        RequireProject
                            label is "Require_<FinanceEnterpriseGroup.ProjectLabel>"
                            display only
                        visible when (ChartType.Posting)
                            IsReconcilable
        two column distributed
            column1
                paragraph
                    visible when (GeneralLedgerChartAccount not entered)
                        DisplayChartSection
                            label is "ChartSection"
                    visible when (not ChartType.Posting)
                        AccountType
                            label is "<FinanceEnterpriseGroup.AccountLabel>Type"
            column2
                visible when (ChartType.Summary)
                    DisplayAccount
                        label is "Display<FinanceEnterpriseGroup.AccountLabel>"
        paragraph
            visible when (false)
                ReportingChart
