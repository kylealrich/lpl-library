ContractImportGPOSupplierForm is a Form
    is primary
    title is "GPOSupplierMapping"
    Layout
        single column
            SupplierGroup
        display as text
            single column
                ContractImportGPOSupplier
                    label is "GPOSupplier"
                Name
                visible when (ContractGroupRel.GPOIsVizient)
                    Address1
                    Address2
                    Municipality
                    StateProvince
                    PostalCode
                    Active
        two column
            DefaultDistributorContract
                select is OpenGPODistributorContracts
            DoNotCreateDistributorContract
        single column
            ManufacturerCodeDivision
                compact format
            blank line
        two column
            ContractSupplier
            ContractSupplier.SupplierName
                display as text
        display as text
            single column
                ContractSupplier.Vendor
            single column
                ContractSupplier.MailingAddress
                    form is PostalAddressMinimum
        visible when (ContractGroupRel.GPOIsHealthTrust)
            display as text
                single column
                    TaxID
                    first ContractGPOSupplierRel.VendorTypeID
                    first ContractGPOSupplierRel.LegalName
                    first ContractGPOSupplierRel.MinorityIndicator
                    first ContractGPOSupplierRel.ParentVendorNumber
                    first ContractGPOSupplierRel.WebAddress
                    first ContractGPOSupplierRel.DoingBusinessAsName
        visible when (ContractGroupRel.GPOIsHealthTrust2021)
            display as text
                single column
                    TaxID
                    visible when (GPOSupplier2021Exists)
                        first ContractGPOSupplierRel.LegalName
                    visible when (GPOHeaderSupplier2021Exists)
                        first ContractGPOHeaderRel.ContractVendorLegalName
