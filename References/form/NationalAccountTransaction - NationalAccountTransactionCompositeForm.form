NationalAccountTransactionCompositeForm is a CompositeForm
    is primary
    title is "NationalAccountTransaction_forReceivableInvoice<ReceivableInvoiceDetail.Invoice>"
    context form is NationalAccountTransactionContextForm
        background is theme color
    NationalAccountTransactionContextPanel is a Panel
        title is "Main"
        form is NationalAccountTransactionContextFormPanel
    PriorSummarizeCreditApplications is a Panel
        title is "PriorSummarizeCreditApplications"
        visible when (ReceivableInvoiceDetail.SummarizeDetailInvoiceHasCreditApplicationInvoice)
        list is ReceivableInvoiceDetail.ReceivableCreditApplicationInvoiceRel.ReviewCreditMemoApplications
    PriorCreditApplications is a Panel
        title is "PriorCreditApplications"
        visible when (CreditMemoHasApplications)
        list is CreditMemoReceivableApplicationRel.ReviewCreditMemoApplications
    PriorSummarizedApplications is a Panel
        visible when (ReceivableInvoiceDetail.SummarizedDetailInvoiceHasPaymentApplication)
        list is ReceivableInvoiceDetail.ReceivableApplicationInvoiceRel.TransactionApplicationsList
    PriorApplications is a Panel
        visible when (HasReceivableApplications)
        list is ReceivableApplicationRel.TransactionApplicationsList
    PendingSummarizeApplications is a Panel
        title is "SelectedSummarizeApplications"
        visible when (ReceivableInvoiceDetail.SummarizeDetailInvoiceHasApplicationSelect)
        list is ReceivableInvoiceRel.ReceivableApplicationSelectForThisInvoiceRel.ReviewReceivableApplicationSelectList
    PendingApplications is a Panel
        title is "SelectedApplications"
        visible when (HasReceivableApplicationSelect)
        list is ReceivableApplicationSelectRel.ReviewReceivableApplicationSelectList
