InventoryTransactionInterfaceResultContext is a Form
    title is "InventoryTransactionInterfaceResult"
    Layout
        display as text
            three column distributed
                column1
                    InventoryTransactionInterfaceResult
                        label is "ID"
                    RunGroup
                    RunTime
                    Status
                    paragraph
                        InterfacedInventoryTransactionCount
                            label is "InterfacedTransactionCount"
                    ErrorRunGroup
                column2
                    text of "Parameters:"
                        bold
                    ReleaseInventoryTransactions
                    IncludeInventoryTransactionsInError
                    OriginalRunGroup
                    ErrorRunGroupPrefix
                    MoveErrorsToNewRunGroup
            blank line
            visible when (NoRecordsToProcess)
                text of "<NoRecordsToProcessMsg>"
                    bold
                blank line
            visible when (NoRecordsForRunGroup)
                visible when (not NoRecordsToProcess)
                    text of "NoRecordsFoundForRunGroup"
