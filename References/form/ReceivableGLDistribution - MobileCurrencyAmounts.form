MobileCurrencyAmounts is a Form
    title is "CurrencyAmounts"
    Layout
        visible when (false)
            DistributionAmount.AssignToCurrency
        display as text
            two column grid
                column1
                    DistributionAmount.CurrencyAmount
                        label is "DistributionAmount"
                column2
                    DistributionAmount.ExchangeDate
                        label is "ExchangeDate"
            single column
                OriginalCurrency.Description
                    no label
            two column grid
                column1
                    DistributionAmount.FunctionalAmount.EnteredCurrencyAmount
                        label is "FunctionalAmount"
                column2
                    DistributionAmount.FunctionalAmount.EnteredCurrencyRate
                        label is "FunctionalRate"
            single column
                ReceivableCompanyDataGroup.TransactionCompany.AccountingEntity.FunctionalCurrency.Description
                    no label
            visible when (DistributionAmount.AccountingEntity.AlternateCurrencyExists)
                two column grid
                    column1
                        DistributionAmount.AlternateAmount.EnteredCurrencyAmount
                            label is "AlternateAmount"
                    column2
                        DistributionAmount.AlternateAmount.EnteredCurrencyRate
