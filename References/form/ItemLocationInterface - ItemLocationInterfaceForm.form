ItemLocationInterfaceForm is a Form
    is primary
    title is "ItemLocationInterface"
    Layout
        visible when (HasErrors)
            single column
                text of "**ErrorsExist**"
                    bold
                    italics
                    color of red
        display as text
            single column
                RunGroup
        paragraph
            InventoryCompany
            InventoryCompany.Name
                no label
                display as text
        paragraph
            InventoryLocation
                label is "Location"
            InventoryLocation.Name
                no label
                display as text
        paragraph
            Item
                display as text
            Item.Description
                no label
                display as text
        two column
            paragraph
                GeneralLedgerCategory
                    label is "InventoryLocationAccountGroup"
                GeneralLedgerCategory.Description
                    no label
                    display as text
        single column
            Active
        three column
            DefaultTransactionUOM.UnitOfMeasure
                label is "DefaultTransactionUOM"
            DefaultSourceUOM.UnitOfMeasure
                label is "DefaultSourceUOM"
            SupplementalUOM
        two column
            paragraph
                Buyer
                Buyer.Employee.PresentationNameSnapshot
                    no label
                    display as text
            paragraph
                Manufacturer
                    compact format
                ManufacturerNumber
        single column
            MaximumRequisitionQuantity
        two column
            InventoryTracked
            AllowReceipt
            AllowIssue
            InspectionRequired
            NoCharge
            PatientChargeable
            Consignment
            SelectForSpecialCycleCount
        blank line
        two column
            MovementClass
            ForecastNumber
                compact format
        header2 of "DetailTrackingInformation"
        two column
            SerialTracked
            LotTracked
        paragraph
            BinTracked
            PreferredBin
            StorageCode
            PreferredHoldBin
        header2 of "TaxInformation"
        two column
            PurchaseTaxable
            TaxableWhenSold
        two column
            PurchaseTaxCode
            SellTaxCode
        header2 of "MaterialSafetyDataSheet"
        paragraph
            MSDSDate
                label is "Date"
            MSDSDocument
                label is "Document"
            MSDSVersion
                label is "Version"
        header2 of "Healthcare"
        single column
            ChargeNumber
            UDITracked
                label is "UDI/DSCSATracked"
        header2 of "Reorder"
        two column
            ReorderPolicy
            ReorderDocument
        two column
            ReorderPointCode
            ReorderPoint
        two column
            AutomaticPurchaseOrder
            ReceivingDays
        blank line
        single column
            ReorderQuantityCode
            ReorderQuantity
            OrderMultiple
        two column
            MaximumOrderQuantity
            MinimumOrderQuantity
            SafetyStock
            EconomicOrderQuantity
        two column
            column1
                header2 of "Replenishment"
                paragraph
                    ReplenishmentPriority
                        label is "Priority"
                paragraph
                    Vendor
                    Vendor.VendorName
                        no label
                        display as text
                paragraph
                    PurchaseFromLocation
                    PurchaseFromLocation.VendorLocation.VendorName
                        no label
                        display as text
            column2
                header3 of "Or"
                paragraph
                    ReplenishFrom.ReplenishFromCompany
                        label is "ReplenishFromCompany"
                    ReplenishFrom.ReplenishFromCompany.Name
                        no label
                        display as text
                paragraph
                    ReplenishFrom.ReplenishFromLocation
                        label is "ReplenishFromLocation"
                    ReplenishFrom.ReplenishFromLocation.Name
                        no label
                        display as text
                paragraph
                    ReplenishFrom.ReplenishFromBin
                        label is "ReplenishFromBin"
        blank line
        header2 of "Leadtime"
        two column
            LeadtimeControl
                label is "Control"
            LeadtimeDays
                label is "AverageDays"
        header2 of "Sales"
        single column
            CreatePurchaseOrderForCustomerOrderLine
            DropshipPurchaseOrderForCustomerOrderLine
        blank line
        paragraph
            MinimumSaleQuantity
                label is "MinimumQuantity"
            MaximumSaleQuantity
                label is "MaximumQuantity"
        blank line
        single column
            AllowCustomerOrderReturns
