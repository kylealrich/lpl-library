ResourceAccessPrinterForm is a Form
    title is "MapToLocationPrinter"
    Layout
        single column
            paragraph
                MobileSupplyChainResource
                    label is "ResourceID"
                    display as text
                MobileSupplyChainResource.Employee.FirstLastName
                    display as text
            paragraph
                Company
                    select is MobileSupplyChainResourceContextAccessibleCompanies
                Company.InventoryCompany.Name
                    no label
                    display as text
            paragraph
                MobileSupplyChainLocation
                    select is MobileSupplyChainResourceContextAccessibleLocations
                MobileSupplyChainLocation.DerivedLocationName
                    no label
                    display as text
            paragraph
                DefaultReportPrinter
                    label is "ReportPrinter"
                    select is MSCMReportCompanyPrinterSelect
            paragraph
                DefaultLabelPrinter
                    label is "LabelPrinter"
                    select is MSCMLabelCompanyPrinterSelect
