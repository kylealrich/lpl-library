TotalSpendInvoiceLinesForLocationBuyerItem is a CompositeForm
    title is "SpendAnalysisInvoiceLines"
    context form is ItemConsolidationContext
    NoContractInvoices is a Panel
        visible when (NoContract)
        list is NoContractInvoiceLineFactsByLocationBuyerItemRel.InvoiceLineFacts
    OffContractInvoices is a Panel
        visible when (OffContract)
        list is OffContractInvoiceLineFactsByLocationBuyerItemRel.OffContractInvoiceLineFacts
    OnContractInvoices is a Panel
        visible when (OnContract)
        list is OnContractInvoiceLineFactsByLocationBuyerItemRel.InvoiceLineFacts
