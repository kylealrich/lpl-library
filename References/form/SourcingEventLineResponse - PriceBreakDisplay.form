PriceBreakDisplay is a Form
    title is "PriceBreaks"
    Layout
        header2 of "Line<SourcingEventLine>ResponseDetails"
        single column
            text of "ResponseUnitPrice:_1<UOM.UnitOfMeasure>@<ResponseAmount.UnitPrice>"
        header2 of "EnterPriceBreakInformation"
        single column
            MarkupDiscount
                label is "MarkupOrDiscount"
                display as text
            QuantityAmountBreakType
                label is "ByQuantityOrAmount"
                display as text
            BasePercent
                display as text
        blank line
        three column
            AdvancedPricing.BreakpointOccurs.QuantityPrice[1].Quantity
                label is "Quantity/Amount"
                display as text
            AdvancedPricing.BreakpointOccurs.QuantityPrice[1].UnitPercent
                label is "NewPercent"
                display as text
            AdvancedPricing.BreakpointOccurs.QuantityPrice[1].UnitPrice
                label is "NewPrice"
                display as text
        three column
            AdvancedPricing.BreakpointOccurs.QuantityPrice[2].Quantity
                no label
                display as text
            AdvancedPricing.BreakpointOccurs.QuantityPrice[2].UnitPercent
                no label
                display as text
            AdvancedPricing.BreakpointOccurs.QuantityPrice[2].UnitPrice
                no label
                display as text
        three column
            AdvancedPricing.BreakpointOccurs.QuantityPrice[3].Quantity
                no label
                display as text
            AdvancedPricing.BreakpointOccurs.QuantityPrice[3].UnitPercent
                no label
                display as text
            AdvancedPricing.BreakpointOccurs.QuantityPrice[3].UnitPrice
                no label
                display as text
        three column
            AdvancedPricing.BreakpointOccurs.QuantityPrice[4].Quantity
                no label
                display as text
            AdvancedPricing.BreakpointOccurs.QuantityPrice[4].UnitPercent
                no label
                display as text
            AdvancedPricing.BreakpointOccurs.QuantityPrice[4].UnitPrice
                no label
                display as text
        three column
            AdvancedPricing.BreakpointOccurs.QuantityPrice[5].Quantity
                no label
                display as text
            AdvancedPricing.BreakpointOccurs.QuantityPrice[5].UnitPercent
                no label
                display as text
            AdvancedPricing.BreakpointOccurs.QuantityPrice[5].UnitPrice
                no label
                display as text
