ContractData1 is a Form
    Layout
        paragraph
            BeginDate
                label is "EffectiveFrom"
            EndDate
                label is "EffectiveTo"
        paragraph
            InventoryLocation
            InventoryLocation.Name
                no label
                display as text
        paragraph
            BaseName
                label is "Base"
                select is PriceBaseList
            BaseName.Description
                no label
                display as text
        paragraph
            Percent
                label is "DefaultPercent"
        paragraph
            OrderDiscount
                label is "Discountable"
        paragraph
            PriceDiscountCode
                label is "DiscountCode"
                select is DiscountCodes
            PriceDiscountCode.PriceDiscountCodeAccountArray.PriceDiscountCodeAccountGroup.InvoiceDescription
                no label
                display as text
        paragraph
            Currency
            Currency.Description
                no label
                display as text
        paragraph
            CurrencyRate
                label is "ContractRate"
        paragraph
            Miscellaneous
                label is "BrokenCaseCharge"
                compact format
            Miscellaneous.Description
                no label
                display as text
