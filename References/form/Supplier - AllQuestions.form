AllQuestions is a CompositeForm
    title is "Questions_for<SupplierName>"
    called out actions only
    context form is QuestionsContext
    Actions
        action is save
            action icon is save
            hide when invalid
        action is save and close
            action icon is save
            hide when invalid
        action is refresh
            label is "_"
    Questions is a Panel
        visible when (NoConditionalQuestionsAndQuestionsExists)
        mouse over text is "<SupplierGroup.QuestionsText>"
        panel header is "<SupplierGroup.QuestionsText>"
        template is SupplierGroup.SupplierGroupQuestionsByDisplayOrder
            business class is SupplierQuestionResponse
                form is SupplierQuestionResponseAnswerForm
    QuestionsConditional is a Panel
        visible when (ConditionalQuestionsAndQuestionsExist)
        mouse over text is "<SupplierGroup.QuestionsText>"
        panel header is "<SupplierGroup.QuestionsText>"
        template is ActiveSupplierQuestions
            business class is SupplierQuestionsSetResponse
                form is SupplierQuestionsSetResponseAnswerForm
