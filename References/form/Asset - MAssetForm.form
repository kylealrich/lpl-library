MAssetForm is a CompositeForm
    title is "Asset"
    context form is MAssetContextForm
    show panel control on left
    restrict action IncludeInMassSelection
    restrict action Unreleased.Create
    restrict action Unreleased.PortableCreateFromTemplate
    restrict action Unreleased.Delete
    Main is a Panel
        form is MAssetMain
    Funds1 is a Panel
        title is "Funds"
        visible when (FundOverrideSelected)
        list is AssetFund set.AssetFunds
    Funds2 is a Panel
        title is "Funds"
        visible when (AssetLease.Lease.CreateAssetFundRecords)
        list is AssetFund set.AssetFundsNoUpdate
    Items is a Panel
        visible when (not IsWorkInProcess)
        list is AssetItem set.MAssetItems
    AssetItems is a Panel
        title is "Items"
        visible when (IsFundUnreleased)
        list is AssetItem set.MAssetItemCreate
    Books1 is a Panel
        title is "Books"
        list is AssetBook set.MAssetBooks
    Transactions is a Panel
        visible when (NotDisposedAndDisplayJournalEntries)
        list is AssetTransactionsRel.MAssetTransactions
    AdditionTransactions is a Panel
        title is "Transactions"
        visible when (AssetCreatePostingTransactionList)
        list is AssetTransactionsRel.MNewAssetCreateTransactions
    BookTransactions is a Panel
        valid when (NotPostingBookDisplayJournalEntries)
        list is UnreleasedBookTransactionsRel.AssetBookTransactions
    Disposals is a Panel
        visible when (DisposalExists)
        list is AssetDisposalsRel.InContextOfAssetDisposals
    Transfers is a Panel
        visible when (AssetTransferExists)
        list is AssetTransfer set.MAssetTransfers
    Adjustments is a Panel
        visible when (AssetAdjustmentExists)
        list is AssetAdjustment set.MAssetAdjustments
    Repairs is a Panel
        list is AssetRepairsRel.AssetRepairs
    InterfacingRepairs is a Panel
        valid when (HasInterfacingRepairs)
        list is AssetRepairImport set.InterfacingAssetRepairs
    InterfacingAdjustments is a Panel
        valid when (HasInterfacingAdjustments)
        list is AssetItemAdjustmentImportsRel.AssetItemAdjustmentImportList
    Comments is a Panel
        list is AssetComment set.primary
