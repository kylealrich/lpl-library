POLineQuantityDetailView is a Form
    title is "PurchaseOrderLineQuantityDetail"
    Layout
        display as text
            five column distributed table
                column1
                    Company
                column2
                    paragraph
                        PurchaseOrder
                        PurchaseOrder.PurchaseOrderRevision
                            label is "-"
                        PurchaseOrder.POCode
                            label is "Code"
                column3
                    LineNumber
                        label is "Line"
                column4
                    PurchaseOrder.Released
                        label is "POReleased"
                column5
                    paragraph
                        ShipToLocation
                            label is "ShipTo"
                        ShipToLocation.NameForPurchaseOrder
                            no label
            three column distributed
                column1
                    paragraph
                        Item
                        visible when (InventoriedOrNonstock)
                            Item.Description
                                no label
                        visible when (not ItemExists)
                            Description
                                no label
                column2
                    ItemType
                column3
                    paragraph
                        EarlyDeliveryDate
                            label is "DeliveryDateBegin"
                        LateDeliveryDate
                            label is "DeliveryDateEnd"
            two column table
                column headers
                    "Quantity"
                    "UOM"
                row headers
                    "Ordered"
                    "Canceled"
                    "Received"
                    "Matched"
                    "OpenToMatch"
                    "MatchedNotReceived"
                    "Chargeback"
                    "Substitute"
                    "Rejected"
                    "Returned"
                    "Archived"
                    "Memo"
                    "WriteOff"
                    "OrderedPrice"
                column1
                    Quantity
                        no label
                    CancelQuantity
                        no label
                    ReceivedQuantity
                        no label
                    MatchedQuantity
                        no label
                    OpenToMatchQuantity
                        no label
                    MatchedNotReceivedQuantity
                        no label
