SupplierGroupSetup is a CompositeForm
    is primary
    title is "SupplierGroup"
    context form is SupplierGroupContext
        background is theme color
    General is a Panel
        form is SupplierGroup
    Subcontractor is a Panel
        form is Subcontractor
    Questions is a Panel
        list is SupplierGroupQuestion set.Questions
            helper list is SourcingQuestions.SupplierGroupHelperQuestions
                action is AttachToSupplierGroup
    RegistrationText is a Panel
        visible when (UseSupplierPortal)
        form is SupplierGroupRegistrationText
    MessageConfiguration is a Panel
        form is SupplierGroupEmailConfiguration
    PortalContacts is a Panel
        visible when (UseSupplierPortal)
        list is SupplierGroupPortalContacts set.primary
    PortalLinks is a Panel
        visible when (UseSupplierPortal)
        list is SupplierPortalLinksAndInstructions set.Links
    PortalInstructions is a Panel
        visible when (UseSupplierPortal)
        list is SupplierPortalLinksAndInstructions set.Instructions
    PortalAttachments is a Panel
        visible when (UseSupplierPortal)
        list is SupplierPortalLinksAndInstructions set.Attachments
    PortalHomePage is a Panel
        visible when (UseSupplierPortal)
        form is PortalHomePage
    SupplierCreatedInvoices is a Panel
        visible when (UseSupplierPortal)
        form is SupplierCreatedInvoicesForm
    SupplierPortalAccess is a Panel
        visible when (UseSupplierPortal)
        form is SupplierPortalAccess
    AnonymousPortalAccess is a Panel
        visible when (UseSupplierPortal)
        form is AnonymousPortalAccess
