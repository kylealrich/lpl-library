GlobalLocationNumberDetail is a Form
    is primary
    Layout
        paragraph
            ItemGroup.ItemGroupDescription
                label is "ItemGroup"
                display as text
        paragraph
            GlobalLocationNumber
                display as text
            GlobalLocationNumber.Description
                display as text
        paragraph
            GlobalLocationNumber.ParentGlobalLocationNumber
                display as text
        paragraph
            visible when (GlobalLocationNumberTypeEntered)
                GlobalLocationNumberDetail.GlobalLocationNumberType
                    display as text
            visible when (not GlobalLocationNumberTypeEntered)
                GlobalLocationNumberDetail.GlobalLocationNumberType
            visible when (GlobalLocationNumberDetail.GlobalLocationNumberType.GeneralLedgerCompany)
                Company
            visible when (GlobalLocationNumberDetail.GlobalLocationNumberType.Bin)
                CompanyLocationBin
            visible when (GlobalLocationNumberDetail.GlobalLocationNumberType.InventoryCompany)
                CompanyLocationBin.Company
            visible when (GlobalLocationNumberDetail.GlobalLocationNumberType.InventoryLocation)
                CompanyLocationBin.Company
                CompanyLocationBin.InventoryLocation
            visible when (GlobalLocationNumberDetail.GlobalLocationNumberType.Vendor)
                VendorAndVendorLocation.Vendor
            visible when (GlobalLocationNumberDetail.GlobalLocationNumberType.VendorLocation)
                VendorAndVendorLocation
            visible when (GlobalLocationNumberDetail.GlobalLocationNumberType.Buyer)
                Buyer
            visible when (GlobalLocationNumberDetail.GlobalLocationNumberType.ManufacturerCode)
                Manufacturer.ManufacturerCode
            visible when (GlobalLocationNumberDetail.GlobalLocationNumberType.ManufacturerDivision)
                Manufacturer
            visible when (GlobalLocationNumberDetail.GlobalLocationNumberType.RequestingLocation)
                RequestingCompanyAndLocation.RequestingCompany
                RequestingCompanyAndLocation.RequestingLocation
            visible when (GlobalLocationNumberDetail.GlobalLocationNumberType.PurchasingVendor)
                PurchasingVendorAndPurchaseFromLocation.PurchasingVendor
                    label is "PurchasingVendor"
            visible when (GlobalLocationNumberDetail.GlobalLocationNumberType.PurchaseFromLocation)
                PurchasingVendorAndPurchaseFromLocation.PurchasingVendor
                    label is "PurchasingVendor"
                PurchasingVendorAndPurchaseFromLocation.PurchaseFromLocation
