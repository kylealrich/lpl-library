ScheduledProcedureForm is a Form
    Layout
        visible when (not RecordExists)
            two column distributed
                column1
                    ItemGroup
                    paragraph
                        Company
                        Company.Name
                            no label
                            display as text
                column2
                    paragraph
                        PreferenceCard
                            left align
                            show up to 20 characters
                        PreferenceCard.PreferenceCardName
                            no label
                            display as text
                    paragraph
                        RequestingLocation
                        RequestingLocation.Name
                            no label
                            display as text
        two column distributed
            column1
                header2 of "Procedure"
                visible when (not RescheduledProcedure)
                    paragraph
                        ProcedureDate
                    paragraph
                        ProcedureTime
                visible when (RescheduledProcedure)
                    paragraph
                        OriginalProcedureDate
                            label is "ProcedureDate"
                            display only
                        ProcedureDate
                            label is "NewProcedureDate"
                    paragraph
                        OriginalProcedureTime
                            label is "ProcedureTime"
                            display only
                        ProcedureTime
                            label is "NewProcedureTime"
                paragraph
                    Case
                paragraph
                    TransientProvider
                        label is "Provider"
                        show up to 10 characters
                    DerivedProviderFullName
                        no label
                        display as text
                paragraph
                    Room
                        show up to 10 characters
            column2
                header2 of "Patient"
                paragraph
                    TransientPatientID
                        label is "MedicalRecordNumber"
                        show up to 15 characters
                paragraph
                    TransientPatientLastName
                        label is "LastName"
                        show up to 15 characters
                    TransientPatientFirstName
                        label is "FirstName"
                        show up to 15 characters
                    TransientPatientMiddleName
                        label is "MiddleInitial"
                        show up to 5 characters
        two column distributed
            column1
                header2 of "ForInventoriedItems"
                paragraph
                    FromCompanyLocation.FromCompany
                        label is "FromCompany"
                    FromCompanyLocation.FromCompany.Name
                        no label
                        display as text
                paragraph
                    FromCompanyLocation.FromLocation
                    FromCompanyLocation.FromLocation.Name
                        no label
                        display as text
            column2
                header2 of "ForNonStockItems"
                paragraph
                    FromCompanyParLocationBin.FromCompany
                        label is "FromCompany"
                    FromCompanyParLocationBin.FromCompany.Name
                        no label
                        display as text
                paragraph
                    FromCompanyParLocationBin.ParLocationGroup
                text of "Or"
                paragraph
                    FromCompanyParLocationBin.ParLocation
                        label is "FromParLocation"
                        select is ParAndExchangeCartLocationsSelect
                    FromCompanyParLocationBin.ParLocation.Name
                        no label
                        display as text
        visible when (false)
            TransientUpdatePatientProcedure
