SupplierRequisitionCompositeForm is a CompositeForm
    title is "Requisition"
    called out actions only
    context form is SupplierRequisitionContext
        background is theme color
    Actions
        action is SubmitForSupplier
            label is "Submit"
            hide when invalid
        action is SupplierDelete
            hide when invalid
        action is CreateSupplierMessageAndDialog
            label is "StartADialog"
            hide when invalid
        navigation is PurchaseOrderForCapitatedReq
            hide when invalid
        navigation is ComponentLinesWithNoSerialOrLotNav
            hide when invalid
            valid when (HasLinesWithNoLotOrSerialSupplier)
        action is save
            label is "_"
    Header is a Panel
        form is SupplierRequisitionHeader
    ProcedureInformation is a Panel
        visible when (CanDisplayAnyProcedureInformation)
        form is ProcedureInfoSupplier
    CapitatedLines is a MultiListPanel
        title is "ProceduresAndConstructComponents"
        visible when (CapitatedHelperLists)
        list is RequisitionLinesRel.CapitatedRequisitionLineForSupplierList
            helper list is ContractLinesForVendorRel.CapitatedProceduresForASupplierRequisition
                action is AddProcedureToRequisition
                title is "Procedures"
        list is RequisitionLinesRel.CapitatedComponentsRequisitionLineForSupplierList
            helper list is CapitatedComponentItemsRel.ContractCapitatedComponentGroupItemsSelect
                action is AddToRequisitionConstructComponent
                title is "NoChargeComponentsOnConstruct"
        list is RequisitionLinesRel.CapitatedChargedComponentsRequisitionLineForSupplierList
    ChargedCapitatedLines is a Panel
        title is "ChargedLinesNotOnConstruct"
        visible when (CapitatedHelperLists)
        list is RequisitionLinesRel.NonCapitatedComponentsRequisitionLineForSupplierList
            helper list is ContractLinesForVendorRel.ContractLinesForASupplierRequisitionSelect
                action is AddToRequisition
                title is "OtherContractItems"
            helper list is ContractLinesForVendorRel.ContractLinesForASupplierRequisitionSelect
                action is AddWasteItemToRequisition
                title is "WasteItems"
    CapitatedLinesSubmitted is a MultiListPanel
        title is "ProceduresAndConstructLines"
        visible when (CapitatedNoHelperLists)
        list is RequisitionLinesRel.CapitatedRequisitionLineForSupplierList
        list is RequisitionLinesRel.CapitatedComponentsRequisitionLineForSupplierList
        list is RequisitionLinesRel.CapitatedChargedComponentsRequisitionLineForSupplierList
    ChargedCapitatedLinesSubmitted is a Panel
        title is "ChargedLinesNotOnConstruct"
        visible when (CapitatedNoHelperLists)
        list is RequisitionLinesRel.NonCapitatedComponentsRequisitionLineForSupplierList
    Lines is a Panel
        title is "Lines"
        visible when (NoHelperLists)
        list is RequisitionLinesRel.RequisitionLineForSupplierListNoHelper
    Lines2 is a Panel
        title is "Lines"
        visible when (ContractSearchOnly)
        list is RequisitionLinesRel.RequisitionLineForSupplierList
            helper list is ContractLinesForVendorRel.ContractLinesForASupplierRequisition
                action is AddToRequisition
                    invoked.PrmCompany = Company
                    invoked.PrmRequisition = Requisition
                title is "ContractItems"
    Lines3 is a Panel
        title is "Lines"
        visible when (SingleTemplateSearchOnly)
        list is RequisitionLinesRel.RequisitionLineForSupplierList
            helper list is ProcurementTemplate.ValidProcurementTemplateLinesRel.SupplierProcurementTemplateLines
                action is IncludeInRequisitionLine
                    invoked.PrmCompany = Company
                    invoked.PrmRequisition = Requisition
                title is "TemplateLines"
    Lines4 is a Panel
        title is "Lines"
        visible when (MultipleTemplateSearchOnly)
        list is RequisitionLinesRel.RequisitionLineForSupplierList
            helper list is ProcurementTemplateLinesForAVendorRel.SupplierMultipleProcurementTemplateLines
                action is IncludeInRequisitionLine
                    invoked.PrmCompany = Company
                    invoked.PrmRequisition = Requisition
                title is "TemplateLines"
    Lines5 is a Panel
        title is "Lines"
        visible when (ContractAndSingleTemplateSearch)
        list is RequisitionLinesRel.RequisitionLineForSupplierList
            helper list is ContractLinesForVendorRel.ContractLinesForASupplierRequisition
                action is AddToRequisition
                    invoked.PrmCompany = Company
                    invoked.PrmRequisition = Requisition
                title is "ContractItems"
            helper list is ProcurementTemplate.ValidProcurementTemplateLinesRel.SupplierProcurementTemplateLines
                action is IncludeInRequisitionLine
                    invoked.PrmCompany = Company
                    invoked.PrmRequisition = Requisition
                title is "TemplateLines"
    Lines6 is a Panel
        title is "Lines"
        visible when (ContractAndMultipleTemplateSearch)
        list is RequisitionLinesRel.RequisitionLineForSupplierList
            helper list is ContractLinesForVendorRel.ContractLinesForASupplierRequisition
                action is AddToRequisition
                    invoked.PrmCompany = Company
                    invoked.PrmRequisition = Requisition
                title is "ContractItems"
            helper list is ProcurementTemplateLinesForAVendorRel.SupplierMultipleProcurementTemplateLines
                action is IncludeInRequisitionLine
                    invoked.PrmCompany = Company
                    invoked.PrmRequisition = Requisition
                title is "TemplateLines"
    Attachments is a Panel
        list is RequisitionComment set.SupplierAttachmentsCardList
    PurchaseOrderLines is a Panel
        visible when (HasPOLines)
        list is PurchaseOrderLineSourcesRel.PurchaseOrderLineForSupplierSourceList
