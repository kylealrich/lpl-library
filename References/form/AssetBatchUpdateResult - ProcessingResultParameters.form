ProcessingResultParameters is a Form
    title is "Parameters"
    Layout
        visible when (Source.AssetPeriodClose)
            three column distributed
                column1
                    FinanceEnterpriseGroup
                        display as text
                    paragraph
                        Release
                            label is "Update"
                            display as text
                        IncludeTransactionAmounts
                            label is "DisplayTransactionCurrency"
                            display as text
                    PostingDate
                        display as text
                column2
                    paragraph
                        Book
                            display as text
                        Book.Description
                            no label
                            display as text
                    paragraph
                        Company
                            display as text
                        Company.GeneralLedgerCompany.Name
                            no label
                            display as text
                    Queue
                        display as text
                column3
                    paragraph
                        BookGroup
                            display as text
                        BookGroup.Description
                            no label
                            display as text
                    paragraph
                        CompanyGroup
                            display as text
                        CompanyGroup.Description
                            no label
                            display as text
        visible when (Source.ReleaseAssets)
            three column distributed
                column1
                    FinanceEnterpriseGroup
                        display as text
                    paragraph
                        DerivedRelease
                            label is "Update"
                            display as text
                        IncludeTransactionAmounts
                            label is "DisplayTransactionCurrency"
                    Process
                        label is "Transaction"
                        display as text
                    paragraph
                        AssetAccountingUnitGroup
                            label is "AssetDimensionGroup"
                            display as text
                        AssetAccountingUnitGroup.Description
                            no label
                            display as text
                column2
                    paragraph
                        Company
                            display as text
                        Company.GeneralLedgerCompany.Name
                            no label
                            display as text
                    paragraph
                        LocationName
                            label is "Location"
                            display as text
                            select is Locations
                        LocationName.Description
                            no label
                            display as text
                    AssetType
                        label is "AssetType"
                        display as text
                        compact format
                    CreatorId
                        label is "CreatorI\\D"
                        display as text
                column3
                    paragraph
                        CompanyGroup
                            display as text
                        CompanyGroup.Description
                            no label
                            display as text
                    paragraph
                        Division
                            label is "Division"
                            display as text
                        Division.Description
                            no label
                            display as text
                    AssetGroup
                        label is "AssetGroup"
                        display as text
            paragraph
                DefaultTransactionDimension
                    compact format
        visible when (Source.DepreciationCalculation)
            display as text
                three column distributed
                    FinanceEnterpriseGroup
                    Queue
                three column distributed
                    Classification
                    Compute
                        label is "ComputeOption"
                    Simulated
