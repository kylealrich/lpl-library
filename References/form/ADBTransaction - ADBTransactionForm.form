ADBTransactionForm is a Form
    is primary
    title is "DailyTransaction"
    Layout
        two column
            FinanceEnterpriseGroup
            FinanceEnterpriseGroup.FinanceEnterpriseGroupDescription
                no label
        two column
            AccountingEntity
                label is "<FinanceEnterpriseGroup.AccountingEntityLabel>"
            AccountingEntity.Name
                no label
        visible when (FinanceEnterpriseGroup.ADBSelection.AccountAndAccountingUnit)
            two column
                AccountingUnit
                    label is "<FinanceEnterpriseGroup.AccountingUnitLabel>"
                AccountingUnit.Description
                    no label
        visible when (FinanceEnterpriseGroup.ADBSelection.AccountAndDimension1)
            two column
                FinanceDimension1
                    label is "<FinanceEnterpriseGroup.FinanceDimension1Label>"
                FinanceDimension1.Description
                    no label
        two column
            ADBTransaction.GeneralLedgerChartAccount
                label is "<FinanceEnterpriseGroup.AccountLabel>"
            ADBTransaction.GeneralLedgerChartAccount.AccountDescription
                no label
        blank line
        single column
            ADBTransaction.PostingDate
                label is "PostDate"
            ADBTransaction.TransactionDate
                label is "TransactionDate"
            ADBTransaction.RunDate
                label is "RunDate"
            Status
            FunctionalAmount
