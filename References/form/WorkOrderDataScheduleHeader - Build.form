Build is a Form
    Layout
        visible when (not RecordExists)
            two column distributed
                column1
                    paragraph
                        Company
                            select is CompaniesWorkOrderInUse
                        Company.Name
                            no label
                            display as text
                column2
                    paragraph
                        InventoryLocation
                            label is "Location"
                            select is ProductionLocationForWO
                        InventoryLocation.Name
                            no label
                            display as text
            paragraph
                Item
                    label is "FinishedGood"
                    select is FinishedGoodsForWO
                Item.Description
                    no label
                    display as text
                Item.StockUOM
                    label is "UOM"
                    display as text
        two column distributed
            column1
                Calendar
                DateUsed
                StartDate
                EndDate
                FirmAllocation
            column2
                DocumentDemandView
                visible when (ShowAlert)
                    text of "<MessageForContiguousProduction>" with icon.empty-error-loading-new
                        header4
                        color of orange
                Capacity
                Status
        five column distributed table
            column headers
                "Previous_Date"
                "Period1_End_Date"
                "Period2_End_Date"
                "Period3_End_Date"
                "Future_Date"
            row headers
                "Date"
                "Quantity"
            column1
                PreviousDate
                    no label
                    when value changed
                        refresh DerivedPreviousQuantity
                DerivedPreviousQuantity
                    no label
                    display as text
            column2
                Period1EndDate
                    no label
                    when value changed
                        refresh DerivedPeriod1Quantity
                DerivedPeriod1Quantity
                    no label
                    display as text
            column3
                Period2EndDate
                    no label
                    when value changed
                        refresh DerivedPeriod2Quantity
                DerivedPeriod2Quantity
                    no label
                    display as text
            column4
                Period3EndDate
                    no label
                    when value changed
                        refresh DerivedPeriod3Quantity
                DerivedPeriod3Quantity
                    no label
                    display as text
            column5
                FutureDate
                    no label
                    when value changed
                        refresh DerivedFutureQuantity
                DerivedFutureQuantity
                    no label
                    display as text
