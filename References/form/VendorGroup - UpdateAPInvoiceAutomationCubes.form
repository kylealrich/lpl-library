UpdateAPInvoiceAutomationCubes is an ActionForm
    action is UpdateAPInvoiceAutomationCubes
    title is "UpdateAnalyticCube"
    Layout
        single column
            text of "SelectTheAnalyticCubesYouWouldLikeToUpdate:"
                header2
        blank line
        four column table
            column headers
                "_"
                "_"
                "LastRefreshDate"
                "LastReloadDate"
            column1
                UpdatePayablesInvoice
                    label is "PayablesInvoice"
                UpdatePayablesInvoiceDistribution
                    label is "PayablesInvoiceDistribution"
                UpdatePayablesInvoicePayment
                    label is "PayablesInvoicePayment"
            column2
                paragraph
                    visible when (UpdatePayablesInvoice)
                        PayablesInvoiceOption
                            no label
                            display as horizontal radio buttons
                    visible when (not UpdatePayablesInvoice)
                        PayablesInvoiceOption
                            no label
                            display only
                            display as horizontal radio buttons
                paragraph
                    visible when (UpdatePayablesInvoiceDistribution)
                        PayablesInvoiceDistributionOption
                            no label
                            display as horizontal radio buttons
                    visible when (not UpdatePayablesInvoiceDistribution)
                        PayablesInvoiceDistributionOption
                            no label
                            display only
                            display as horizontal radio buttons
                paragraph
                    visible when (UpdatePayablesInvoicePayment)
                        PayablesInvoicePaymentOption
                            no label
                            display as horizontal radio buttons
                    visible when (not UpdatePayablesInvoicePayment)
                        PayablesInvoicePaymentOption
                            no label
                            display only
                            display as horizontal radio buttons
            column3
                display as text
                    PayablesInvoiceAnalyticCubeRel.LastRefreshDate
                        no label
                    PayablesInvoiceDistributionAnalyticCubeRel.LastRefreshDate
                        no label
                    PayablesInvoicePaymentAnalyticCubeRel.LastRefreshDate
                        no label
            column4
                display as text
                    PayablesInvoiceAnalyticCubeRel.LastReloadDate
                        no label
                    PayablesInvoiceDistributionAnalyticCubeRel.LastReloadDate
                        no label
                    PayablesInvoicePaymentAnalyticCubeRel.LastReloadDate
                        no label
        header2 of "_"
        single column
            text of "SelectRefreshToUpdateExistingCubeData.SelectReloadToDeleteExistingCubeDataAndRebuildData."
                header3
                italics
        blank line
