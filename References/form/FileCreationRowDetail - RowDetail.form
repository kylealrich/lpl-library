RowDetail is a Form
    title is "Detail"
    Layout
        single column
            Description
            RowOrder
            Disable
                display as switch
        visible when (not Linked)
            visible when (not RowExtendsOneToManyRow)
                visible when (not Subtotal)
                    visible when (FileCreationSetup.SupportsOneToMany)
                        single column
                            RowType
                                display as horizontal radio buttons
                                when value changed
                                    OneToMany =  OneToManyCalculation
                                    Header =  HeaderCalculation
                                    Trailer =  TrailerCalculation
                    visible when (not FileCreationSetup.SupportsOneToMany)
                        single column
                            RowTypeNoOTM
                                display as horizontal radio buttons
                                when value changed
                                    Header =  HeaderCalculationNoOTM
                                    Trailer =  TrailerCalculationNoOTM
                visible when (false)
                    paragraph
                        Header
                            display as switch
                        Trailer
                            display as switch
                single column
                    visible when (false)
                        OneToMany
                            display as switch
                    visible when (not Linked)
                        visible when (not RowExtendsOneToManyRow)
                            visible when (OneToMany)
                                visible when (FileCreationSetup.ExportFile.PayablesInvoice)
                                    OneToManyBusinessClassPayablesInvoice
                                        label is "OneToManyBusinessClass"
                                visible when (FileCreationSetup.ExportFile.GeneralLedgerChartAccount)
                                    OneToManyBusinessClassGeneralLedgerChartAccount
                                        label is "OneToManyBusinessClass"
                                visible when (FileCreationSetup.ExportFile.Asset)
                                    OneToManyBusinessClassAsset
                                        label is "OneToManyBusinessClass"
                                visible when (FileCreationSetup.ExportFile.BinGroup)
                                    OneToManyBusinessClassBinGroup
                                        label is "OneToManyBusinessClass"
                                visible when (FileCreationSetup.ExportFile.Contract)
                                    OneToManyBusinessClassContract
                                        label is "OneToManyBusinessClass"
                                visible when (FileCreationSetup.ExportFile.PurchaseOrder)
                                    OneToManyBusinessClassPurchaseOrder
                                        label is "OneToManyBusinessClass"
                                visible when (FileCreationSetup.ExportFile.BillingInvoice)
                                    OneToManyBusinessClassBillingInvoice
                                        label is "OneToManyBusinessClass"
                                visible when (FileCreationSetup.ExportFile.CustomerOrder)
                                    OneToManyBusinessClassCustomerOrder
                                        label is "OneToManyBusinessClass"
                                visible when (FileCreationSetup.ExportFile.Vendor)
                                    OneToManyBusinessClassVendor
                                        label is "OneToManyBusinessClass"
                                visible when (FileCreationSetup.ExportFile.IntercompanyBillingSettlementHeader)
                                    OneToManyBusinessClassIntercompanyBillingSettlementHeader
                                        label is "OneToManyBusinessClass"
                                visible when (FileCreationSetup.ExportFile.IntercompanyBillingDocumentHeader)
                                    OneToManyBusinessClassIntercompanyBillingDocumentHeader
                                        label is "OneToManyBusinessClass"
                                visible when (FileCreationSetup.ExportFile.AccountingEntity)
                                    OneToManyBusinessClassAccountingEntity
                                        label is "OneToManyBusinessClass"
                                visible when (FileCreationSetup.ExportFile.ReceivablePayment)
                                    OneToManyBusinessClassReceivablePayment
                                        label is "OneToManyBusinessClass"
                                visible when (FileCreationSetup.ExportFile.ReceivableInvoice)
                                    OneToManyBusinessClassReceivableInvoice
                                        label is "OneToManyBusinessClass"
        visible when (Subtotal)
            header2 of "Subtotal"
            single column
                SubtotalPlacement
                SubtotalField
                    display as text
        header2 of "LinkToARowInAnotherSetup"
        visible when (LinkedRowValid)
            paragraph
                visible when (FileTypeIsSeparated)
                    LinkedSetup
                        select is LinkedSetupRel.FileCreationLinkedSetupListSeparated
                visible when (FileTypeIsFixedLength)
                    LinkedSetup
                        select is LinkedSetupRel.FileCreationLinkedSetupListFixedLength
                LinkedSetup.Description
                    no label
                    display as text
        visible when (LinkedRowValid)
            paragraph
                LinkedRow
                    select is LinkedSetup.LinkedRowSelectRel.FileCreationRowDetailSelectList
                LinkedRow.Description
                    no label
                    display as text
        visible when (Linked)
            single column
                LinkedRow.Header
                    display as switch
                LinkedRow.Trailer
                    display as switch
                LinkedRow.OneToMany
                    display as switch
                visible when (Linked)
                    visible when (FileCreationSetup.ExportFile.PayablesInvoice)
                        LinkedRow.OneToManyBusinessClassPayablesInvoice
                            label is "BusinessClass"
                    visible when (FileCreationSetup.ExportFile.GeneralLedgerChartAccount)
                        LinkedRow.OneToManyBusinessClassGeneralLedgerChartAccount
                            label is "BusinessClass"
                    visible when (FileCreationSetup.ExportFile.Asset)
                        LinkedRow.OneToManyBusinessClassAsset
                            label is "BusinessClass"
                    visible when (FileCreationSetup.ExportFile.BinGroup)
                        LinkedRow.OneToManyBusinessClassBinGroup
                            label is "BusinessClass"
                    visible when (FileCreationSetup.ExportFile.Contract)
                        LinkedRow.OneToManyBusinessClassContract
                            label is "BusinessClass"
                    visible when (FileCreationSetup.ExportFile.PurchaseOrder)
                        LinkedRow.OneToManyBusinessClassPurchaseOrder
                            label is "BusinessClass"
                    visible when (FileCreationSetup.ExportFile.BillingInvoice)
                        LinkedRow.OneToManyBusinessClassBillingInvoice
                            label is "BusinessClass"
                    visible when (FileCreationSetup.ExportFile.CustomerOrder)
                        LinkedRow.OneToManyBusinessClassCustomerOrder
                            label is "BusinessClass"
                    visible when (FileCreationSetup.ExportFile.Vendor)
                        LinkedRow.OneToManyBusinessClassVendor
                            label is "BusinessClass"
                    visible when (FileCreationSetup.ExportFile.IntercompanyBillingSettlementHeader)
                        LinkedRow.OneToManyBusinessClassIntercompanyBillingSettlementHeader
                            label is "BusinessClass"
                    visible when (FileCreationSetup.ExportFile.IntercompanyBillingDocumentHeader)
                        LinkedRow.OneToManyBusinessClassIntercompanyBillingDocumentHeader
                            label is "BusinessClass"
                    visible when (FileCreationSetup.ExportFile.AccountingEntity)
                        LinkedRow.OneToManyBusinessClassAccountingEntity
                            label is "BusinessClass"
                    visible when (FileCreationSetup.ExportFile.ReceivablePayment)
                        LinkedRow.OneToManyBusinessClassReceivablePayment
                            label is "BusinessClass"
                    visible when (FileCreationSetup.ExportFile.ReceivableInvoice)
                        LinkedRow.OneToManyBusinessClassReceivableInvoice
                            label is "BusinessClass"
        single column
            visible when (FileCreationSetup.UseAuditInformation)
                OnlyWriteRowIfMappedFieldChanged
        header3 of "IncludeInCounts"
        paragraph
            IncludeInRecordCount1
                label is "1"
                display as switch
            IncludeInRecordCount2
                label is "2"
                display as switch
            IncludeInRecordCount3
                label is "3"
                display as switch
        paragraph
            IncludeInRecordCount4
                label is "4"
                display as switch
            IncludeInRecordCount5
                label is "5"
                display as switch
            IncludeInRecordCount6
                label is "6"
                display as switch
