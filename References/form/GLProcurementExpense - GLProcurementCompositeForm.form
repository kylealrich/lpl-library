GLProcurementCompositeForm is a CompositeForm
    is primary
    propagate as a drill
    title is "ProcurementExpenseDetail_<AccountingEntity>_<JournalizeGroup>"
    context form is GLProcurementDetailForm
    MainPanel is a Panel
        title is "Main"
        form is GLProcurementDetailMainForm
    AmountsPanel is a Panel
        title is "CurrencyAmounts"
        form is GLProcurementDetailAmountsForm
    APPanel is a Panel
        title is "PayablesDetails"
        visible when (SystemAPPO)
        form is APProcurementForm
    POPanel is a Panel
        title is "PurchaseOrder"
        visible when (SystemAPPORQ)
        form is POProcurementForm
    ICPanel is a Panel
        title is "Inventory"
        visible when (SystemIC)
        form is ICProcurementForm
    RQPanel is a Panel
        title is "Requisition"
        visible when (SystemAPPORQ)
        form is RQProcurementForm
