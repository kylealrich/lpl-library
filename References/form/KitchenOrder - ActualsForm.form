ActualsForm is a Form
    Layout
        display as text
            three column distributed
                Company.DerivedCompanyDisplay
                    label is "Company"
                KitchenOrder
                InventoryLocation.DerivedInventoryLocationIDName
                    label is "InventoryLocation"
                Item.CardViewItemWithDescription
                    label is "Item"
                DateNeeded
                BatchYieldQuantity
                    label is "YieldPerBatch_(<BatchYieldUOM>)"
        header3 of "_"
        two column
            column1 of 20%
                BatchQuantity
                    label is "PlannedBatchQuantity"
                    display only
            column2 of 40%
                ActualBatchQuantity
        header3 of "_"
        six column
            column1
                text of "_"
                text of "StockQuantity_(<Item.StockUOM>)"
                visible when (Item.SecondaryUOM entered)
                    text of "SecondaryQuantity_(<Item.SecondaryUOM>)"
            column2
