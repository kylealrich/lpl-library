AssetProjectionReport is an ActionForm
    action is AssetProjectionReport
    Layout
        header2 of "SelectionOptions"
        paragraph
            Book
            Book.Description
                no label
                display as text
        paragraph
            FromDate
                label is "FromProjectionDate"
            ToDate
                label is "ToProjectionDate"
        paragraph
            InsrvFromDate
                label is "FromInServiceDate"
            InsrvToDate
                label is "ToInServiceDate"
        paragraph
            Company
                label is "Company"
                select is Companies
            Company.CompanyName
                no label
                display as text
            text of "Or"
            CompanyGroup
                label is "CompanyGroup"
            CompanyGroup.Description
                no label
                display as text
        paragraph
            Classification
            LocationName
                label is "Location"
                select is Locations
            LocationName.Description
                no label
                display as text
        paragraph
            Division
                select is Divisions
            Division.Description
                no label
                display as text
        paragraph
            AssetType
                label is "Type"
                compact format
                select is AssetTypesWithinCompany
            AssetType.Description
                no label
                display as text
        paragraph
            Property
            Disposals
        header2 of "AdditionalOptions"
        paragraph
            Simulated
            TaxExempt
        single column
            Used
        paragraph
            Asset
                select is AllAssets
            Asset.Description
                no label
                display as text
        paragraph
            AssetLease.LeaseCompany
                label is "OrLease"
            AssetLease.LeaseCompany.Name
                no label
                display as text
            AssetLease.Lease
                no label
        single column
            TagNumber
                label is "Tag"
        blank line
        paragraph
            AssetGroup
                select is AssetGroups
            AssetGroup.Description
                no label
                display as text
        paragraph
            TransactionDimension
                label is "TransactionDimensions_or_AssetDimensionGroup"
                compact format
            AuGroup
                no label
            AuGroup.Description
                no label
                display as text
        header2 of "OutputOptions"
        paragraph
            RptOption
                label is "ReportOption"
            PageBreak
        single column
            header3 of "PrintTotals"
        single column
            header4 of "Sequence"
            blank line
            paragraph
                LeaseOption
                    label is "Lease"
            paragraph
                ExpenseSortOption
                    label is "DimensionSort"
            paragraph
                LocOption
                    label is "Location"
            paragraph
                DivOption
                    label is "Division"
            paragraph
                StructureRow
                    label is "Dimension"
                    select is ReconciliationStructureRowDimensionSelectList
            paragraph
                TypeOption
                    label is "Type\\,Sub"
            paragraph
                AstgrpOption
                    label is "AssetGroup"
            paragraph
                AssetOption
                    label is "Asset"
            header4 of "Miscellaneous"
            blank line
            paragraph
                CatOption
                    label is "Category"
            paragraph
                PropOption
                    label is "Property"
            paragraph
                InservOption
                    label is "In\\-\\Service"
            paragraph
                Wip
                    label is "IncludeW\\I\\P"
        blank line
        single column
            CreateCsv
                label is "C\\S\\V"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                AssetProjectionReportReport_RDG
                AssetProjectionDetailReport_RDG
            column2 of 25%
                AssetProjectionReportReport_XPT
                AssetProjectionDetailReport_XPT
