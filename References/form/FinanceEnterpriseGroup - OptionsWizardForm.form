OptionsWizardForm is a Form
    title is "Options"
    Layout
        two column distributed
            RequireUniqueJournalID
            UnitControl
        two column distributed
            InterEntityDimension
            HROrganization
                label is "HROrganization"
        two column distributed
            column1
                paragraph
                    BODTrigger
                        label is "BODTrigger"
                    visible when (BODTrigger)
                        BODCostCenter
            column2
                CreateAMonthlyCalendar
        three column distributed
            ValidateFinanceStructure
                label is "StructureExceptions"
            visible when (not OtherFEGUsingDEPM)
                UseDEPMBudgeting
                    label is "Use_EPMBudgeting"
            visible when (not EnableIDMForAttachments)
                EnableIDMForAttachments
        single column
            ADBSelection
            DimensionForDepartmentJournal
            blank line
            visible when (ProjectLabel entered)
                ProjectDateEdit
                    label is "<ProjectLabel>DateEdit"
                ProjectBurdenDate
                    label is "<ProjectLabel>BurdenDate"
            visible when (ProjectLabel entered)
                paragraph
                    ProjectInvoicing
                        label is "<ProjectLabel>Invoicing"
                    visible when (ProjectInvoicing)
                        BillingOverrideLevel
                        BillingOverrideLevelLabel
                            no label
                            display as text
                        ThirdPartyInvoicePrint
                paragraph
                    StructureValidationDimension
                        label is "ProjectStructureValidationDimension"
                    StructureValidationDimensionLabel
                        no label
                        display as text
                    visible when (HasStructureValidationDimension)
                        ValidationDimensionRequiresProject
                ProjectStructureValidation
                    label is "ProjectStructureValidationEnabled"
            blank line
            visible when (FinanceDimension1Label entered)
                Dimension1ZoneBalancing
                    label is "<FinanceDimension1Label>ZoneBalancing"
        visible when (ProjectLabel entered)
            single column
                TransientCurrencyTable
                    label is "CurrencyExchangeTable"
            four column table
                column headers
                    "CurrencyName"
                    "Code"
                    "Description"
                    "AutoBalanceThreshold"
                column1
                    ReportOne
                        no label
                column2
                    ReportCurrencyOne
                        no label
                column3
                    ReportCurrencyOne.Description
                        no label
                        display as text
                column4
                    ReportOneAutoBalanceThreshold
                        no label
        visible when (Dimension1ZoneBalancing)
            blank line
            four column table
                column headers
                    "Account"
                    "Description"
                    "ChartSection"
                    "AccountType"
                row headers
                    "ZonePayables"
                    "ZoneReceivables"
                column1
                    ZonePayables
                        no label
                    ZoneReceivables
                        no label
                column2
                    ZonePayablesDescription
                        no label
                    ZoneReceivablesDescription
                        no label
                column3
                    ZonePayablesChartSection
                        no label
                    ZoneReceivablesChartSection
                        no label
                column4
                    ZonePayablesAccountType
                        no label
                    ZoneReceivablesAccountType
                        no label
