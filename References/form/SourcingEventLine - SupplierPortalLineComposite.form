SupplierPortalLineComposite is a CompositeForm
    title is "EventLineDetails"
    called out actions only
    context form is SummaryLineContext
        background is theme color
    Actions
        action is refresh
            label is "_"
    LineSummary is a Panel
        form is SELines
    AddOnCharges is a Panel
        visible when (AddOnChargesExist)
        list is SourcingEventLineAOC set.SupplierSourcingEventAOC
    Attachments is a Panel
        visible when (SupplierLineAttachmentsExist)
        list is SupplierSourcingEventLineAttachmentsRel.ListAttachmentView
    LineQuestions is a Panel
        visible when (LineQuestionsExist)
        list is SourcingEventLineQuestion set.SupplierLineQuestions
    LineComments is a Panel
        visible when (SupplierLineCommentsExist)
        list is SupplierSourcingEventLineCommentsRel.SupplierPortalSourcingEventLineCommentList
    Awarded is a Panel
        visible when (SupplierCanViewAward)
        list is QuantityOrContractAwarded.SEAwardedSupplier
