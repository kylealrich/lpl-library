MigrationResultContextForm is a Form
    title is "MigrationResult"
    Layout
        paragraph
            MigrationResult
                display as text
            MigrationJobType
                display as text
            Status
                display as text
            RunTime
                display as text
        paragraph
            visible when (IsMigratePurchaseOrderCommitments)
                POCompany
                    display as text
            visible when (IsMigrateRequisitionCommitments)
                RQCompany
                    display as text
            visible when (IsMigrateContractCommitments)
                ContractGroup
                    display as text
                Contract
                    display as text
            visible when (IsMigrateContractSpreadCommitments)
                ContractGroup
                    display as text
                Contract
                    display as text
            visible when (IsMigrateCBCommitments)
                CashManagementGroup
                    display as text
            visible when (IsMigratePayablesCommitments)
                PayablesCompany
                    display as text
