ReceivablesInterfaceResultComposite is a CompositeForm
    is primary
    propagate as a drill
    title is "ReceivablesInterfaceResult"
    context form is ReceivablesInterfaceResultContext
        background is theme color
    implements IndependentFormPanels
    restrict action DeleteAllInterfaceResults
    restrict action DeleteAllConversionResults
    ProcessingCounts is a Panel
        form is ProcessingCountsForm
    PaymentInterfaceTotals is a Panel
        visible when (HasCustomerPaymentsCreated)
        list is ReceivablePaymentsRel.InterfaceTotals
    NonARPaymentInterfaceTotals is a Panel
        visible when (HasNonARPaymentsCreated)
        list is ReceivablePaymentsRel.NonARInterfaceTotals
    TransactionInterfaceTotals is a Panel
        visible when (HasTransactionsCreated)
        list is ReceivableInvoicesRel.InterfaceTotals
    ApplicationConversionTotals is a Panel
        visible when (HasConvertedApplications)
        list is ReceivableApplicationsRel.InterfaceTotals
    DistribuionConversionTotals is a Panel
        visible when (HasConvertedDistributions)
        list is ReceivableDistributionsRel.InterfaceTotals
    DisputeConversionTotals is a Panel
        visible when (HasConvertedDisputes)
        list is ReceivableInvoiceDisputeRel.InterfaceTotals
    PaymentsCreated is a Panel
        visible when (HasCustomerPaymentsCreated)
        list is ReceivablePaymentsRel.InterfacedPayments
    NonARPaymentsCreated is a Panel
        visible when (HasNonARPaymentsCreated)
        list is ReceivablePaymentsRel.InterfacedNonARPayments
    UninterfacedPayments is a Panel
        visible when (IncompleteHasPendingInterfacePayments)
        list is ReceivablePaymentImportRel.InterfacePaymentsList
    UnconvertedPayments is a Panel
        title is "UnconvertedPayments"
        visible when (IncompleteHasPendingConversionPayments)
        list is ReceivablePaymentConversionRel.primary
    UninterfacedPayments2 is a Panel
        visible when (IncompleteHasPendingPayments2)
        list is ReceivablePaymentImport2Rel.primary
    UninterfacedPaymentRemittances is a Panel
        visible when (IncompleteHasPendingPayment2Remittances)
        list is ReceivablePaymentRemitImportRel.primary
    UninterfacedLockbox is a Panel
        visible when (IncompleteHasPendingLockBox)
        list is ReceivableLockBoxImportRel.primary
    UninterfacedNonARPayments is a Panel
        visible when (IncompleteHasPendingNonARPayments)
        list is ReceivableNonARPaymentImportRel.UninterfacedPaymentsList
    UninterfacedDocumentARPayments is a Panel
        title is "UninterfacedDocumentPayments"
        visible when (IncompleteHasPendingDocumentARPaymentImport)
        list is DocumentARPaymentImportRel.primary
    TransactionsCreated is a Panel
        visible when (HasTransactionsCreated)
        list is ReceivableInvoicesRel.InterfacedInvoices
    UninterfacedTransactions is a Panel
        visible when (IncompleteHasPendingInterfaceInvoices)
        list is ReceivableInvoiceImportRel.InterfaceInvoices
    UnconvertedTransactions is a Panel
        visible when (IncompleteHasPendingConversionInvoices)
        list is ReceivableInvoiceImportRel.ConversionInvoices
    ApplicationsCreated is a Panel
        visible when (HasConvertedApplications)
        list is ReceivableApplicationsRel.ConvertedApplications
    UnconvertedApplications is a Panel
        visible when (IncompleteHasPendingConversionApplications)
        list is ReceivableApplicationImportRel.ApplicationConversions
    DistributionsCreated is a Panel
        visible when (HasConvertedDistributions)
        list is ReceivableDistributionsRel.ConvertedDistributions
    UnconvertedDistributions is a Panel
        visible when (IncompleteHasPendingConversionDistributions)
        list is ReceivableInvoiceDistributionImportRel.ConversionDistributions
    DisputesCreated is a Panel
        visible when (HasConvertedDisputes)
        list is ReceivableInvoiceDisputeRel.ConvertedDisputes
    UnconvertedDisputes is a Panel
        visible when (IncompleteHasPendingConversionDisputes)
        list is ReceivableInvoiceDisputeImportRel.ConversionDisputes
    CommentsCreated is a Panel
        visible when (HasConvertedComments)
        list is CompanyCustomerCommentRel.ConvertedComments
    UnconvertedComments is a Panel
        visible when (IncompleteHasPendingConversionComments)
        list is ReceivableInvoiceCommentImportRel.ConversionComments
