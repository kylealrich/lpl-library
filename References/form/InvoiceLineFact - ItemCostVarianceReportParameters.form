ItemCostVarianceReportParameters is a SearchForm
    title is "ItemCostVarianceParameters"
    Layout
        two column
            Vendor
            Item
        single column
            CommodityCode
        two column
            PurchMajcl
                label is "MajorPurchasingClass"
                show up to 10 characters
            PurchMincl
                label is "MinorPurchasingClass"
                show up to 10 characters
        two column
            ManufacturerCode
            ManufacturerDivision
        four column
            UNSPSC.UNSPSCSegment
            UNSPSC.UNSPSCFamily
                label is "Family"
            UNSPSC.UNSPSCClass
                label is "Class"
            UNSPSC.UNSPSCCommodity
                label is "Commodity"
        three column
            PercentVarianceThreshhold
                label is "AbovePercentVariance"
            AmountVarianceThreshhold
                label is "AboveAmountVariance"
            SpendWithContractOnly
        header3 of "InvoiceDateRange"
        single column
            InvoiceDateRange
