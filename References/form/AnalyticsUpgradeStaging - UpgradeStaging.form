UpgradeStaging is a Form
    title is "UpgradeStaging"
    Layout
        single column
            text of "FSM_Analytics_Version"
                bold
        two column
            column1
                text of "Latest_Metadata_Version_(SMD)"
                text of "Staging_Environment_Version_(STC)"
            column2
                last ProductVersionSMDRel.Value
                    label is "_"
                last ProductVersionSTCRel.Value
                    label is "_"
        single column
            text of "_"
            visible when (Status.InProgress)
                button of "RefreshProductVersion"
                    action is UpdateVersion
            blank line
        single column
            blank line
            header3 of "Upgrade_Staging_Environment"
            blank line
            visible when (Status.InProgress)
                button of "StartStagingEnvironmentUpgrade"
                    action is StartSMDToSTCUpgrade
            last LatestSMDToSTCSwapRel.Status
                label is "Status"
                raise red alert when (last LatestSMDToSTCSwapRel.IsFailed)
                raise yellow alert when (last LatestSMDToSTCSwapRel.IsCanceled)
                raise blue alert when (last LatestSMDToSTCSwapRel.IsActionPendingOrActionPendingInServer)
                    mouse over text is "ActionPending"
            button of "<SMDToSTCWorkUnit>"
                valid when (IsSMDToSTCWorkunitExists)
                link is SMDtoSTCUpgradeWorkUnitNav
                label is "WorkUnit"
                display as link
            blank line
            valid when (Status.InProgress)
                display as text
                UpgradeStagingIsCompleted
                    label is "Please_confirm_if_Status_of_workunit_is_completed_before_proceeding_with_the_next_steps."
