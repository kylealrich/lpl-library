CreateForVendorItemsForm is a CompositeForm
    context form is TierQualifierItemContext
    Actions
        action is CreateVendorItemQualifierItems
            hide when invalid
        navigation is QualifierInformationNav
            label is "ReturnToQualifierInformation"
        action is save
            label is "_"
        action is refresh
            label is "_"
    OtherLinesForManufacturerInformation is a Panel
        visible when (HasOtherQualifierItemsWithVendorItem)
        list is QualifierItemsForManufacturerWithVendorItemRel.TierQualifierItemsForManufacturerWithVendorItem
