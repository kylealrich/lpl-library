ContextMain is a Form
    title is "BudgetGroup"
    Layout
        three column distributed
            column1
                Scenario.ScenarioDescription
                    label is "Scenario"
                    display as text
                paragraph
                    text of "Needs_Recalculation:"
                    NeedsRecalculation
                        no label
                        display as tag
                            color of green when (not NeedsRecalculation)
                            color of yellow when (NeedsRecalculation)
            column2
                paragraph
                    BudgetTemplate.BudgetTemplateDescription
                        label is "Template"
                        display as text
                paragraph
                    visible when (BudgetTemplate.LazyActivation)
                        text of "SelectiveInitialized"
                        LazyInitialized
                            no label
                            display as text
            column3
                button of "CubeData"
                    link is GroupCubeData
                    display as link
