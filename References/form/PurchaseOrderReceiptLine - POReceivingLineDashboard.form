POReceivingLineDashboard is a CompositeForm
    title is "PurchaseOrder<PurchaseOrder>Receipt<PurchaseOrderReceipt>_<LineDisplay>"
    context form is POReceivingLineDashboardContext
        background is theme color
    implements DashBoardView
        show context form on top
    restrict action PopulateMissingUOM
    restrict action MobileUpdate
    restrict action MobileUpdateReceiptLine
    Cost is a Panel
        visible when (PurchaseOrderReceipt.DisplayInvoiceAOCHdr)
        form is PO304InvoiceCost
    VendorItem is a Panel
        visible when (HasVendorItem)
        form is PO304VendorItem
    Source is a Panel
        visible when (IsFromRequisition)
        list is PurchaseOrderLineSourcesRel.PurchaseOrderLineSourcesCardList
    Safety is a Panel
        visible when (HasSafetyInfo)
        form is PO304Safety
    CatchWeight is a Panel
        visible when (IsCatchWeightItemAndHasItemMaster)
        form is PO304CatchWeight
    InvoiceAddOnCosts is a Panel
        visible when (HasAOCs)
        list is ReceiptLineAOCsRel.PurchaseOrderReceiptLineAOCCardList
    ItemDetails is a Panel
        visible when (IsInventoryBinLotSerialTracked)
        list is PurchaseOrderTransactionDetailsRel.AdditionalReceivingEntrys
    ReceiptAdjustments1 is a Panel
        title is "ReceiptAdjustments"
        visible when (IsNotCatchWeightItemAndAdjustmentsExists)
        triple width
        list is POReceiptLineAdjustmentsRel.ReceiptAdjustmentLines1
    ReceiptAdjustments2 is a Panel
        title is "ReceiptAdjustments"
        visible when (IsCatchWeightItemAndHasItemMasterAndAdjustmentsExists)
        triple width
        list is POReceiptLineAdjustmentsRel.ReceiptAdjustmentLines2
    ReceiptInspections is a Panel
        visible when (InspectionsExist)
        triple width
        list is POReceiptLineInspectionsRel.ReceiptInspectionLines
    Track is a Panel
        visible when (POTrackNotExistsAtLineLevel)
        triple width
        form is POTrackForm
    TrackDetails is a Panel
        title is "Track"
        visible when (POTrackExistsAtLineLevel)
        list is PurchaseOrderReceiptTrackingRel.RequesterPurchaseOrderReceiptTracking
    AssetDetail is a Panel
        visible when (AssetDetailExist)
        triple width
        list is PurchaseOrderLineAssetRel.PurchaseOrderLineAssetDetailsRels.PurchaseOrderLineAssetDetailList
