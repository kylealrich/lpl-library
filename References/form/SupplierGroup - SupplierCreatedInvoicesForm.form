SupplierCreatedInvoicesForm is a Form
    title is "SupplierCreatedInvoiceProcessing"
    Layout
        header2 of "InvoiceOptions"
        paragraph
            DisplayTaxAmount
                label is "AllowSupplierToEnterTaxAmount"
        paragraph
            SupplierSubmittedInvoiceCompany
                label is "InvoiceCompany"
        header2 of "InvoiceCreationText"
        single column
            InvoiceHeaderText
                label is "Header"
                use text area of 3 lines
            InvoiceLineText
                label is "Line"
                use text area of 3 lines
            InvoiceAddOnChargeText
                label is "AddOnCharge"
                use text area of 3 lines
            InvoiceAttachmentText
                label is "Attachment"
                use text area of 3 lines
            InvoiceSummaryText
                label is "Summary"
                use text area of 3 lines
