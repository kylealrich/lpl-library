IdentifierDistributionSearchForm is a SearchForm
    title is "DistributionSearch"
    Layout
        two column
            DebtInstrument
            DebtInstrumentIdentifier
            paragraph
                DistributionDateRange.Begin
                    label is "DistributionDate"
                text of "_-"
                DistributionDateRange.End
                    no label
        two column
            DistributionAccount.Ledger
                label is "Ledger"
            DistributionAccount.ToAccountingEntity
                label is "AccountingEntity"
        two column
            DistributionAccount.AccountingUnit
                label is "AccountingUnit"
            DistributionAccount.GeneralLedgerChartAccount
                label is "Account"
        two column
            DistributionAccount.Project
                label is "Project"
            GLTransactionAmount
                label is "TransactionAmount"
        two column
            Status
            FinanceJournalInterface
