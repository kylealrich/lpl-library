PhoneContractComposite is a CompositeForm
    title is "<DisplayContract>"
    called out actions only
    context form is PhoneContractContext
        background is theme color
    Overview is a Panel
        form is MobileContractOverview
    Lines is a Panel
        title is "Lines<LineIndicator>"
        visible when (NonServicePurchaseType)
        list is ContractLine set.MobileNonServiceContractLines
    ServiceLines is a Panel
        title is "Lines<LineIndicator>"
        visible when (ServicePurchaseType)
        list is ContractLine set.MobileServiceContractLines
    NoContractLines is a Panel
        title is "Lines<LineIndicator>"
        visible when (not HasAPurchaseType)
        list is ContractLine set.MobileNonServiceContractLines
    DocumentLinks is a MultiListPanel
        list is ContractCommentRel.MobileContractCommentList
            visible when (DisplayComments)
            show 5 lines
        list is RelatedContract set.MobileRelatedContractList
            visible when (DisplayRelatedReferences)
            show 5 lines
        list is ContractRedlineDocumentDescRel.MobilRedlineDocumentList
            visible when (CanDisplayNonIDMRedlinePanel)
            show 1 lines
