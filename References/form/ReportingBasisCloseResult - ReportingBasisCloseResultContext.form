ReportingBasisCloseResultContext is a Form
    propagate as a drill
    title is "BasisYearCloseResults"
    Layout
        three column distributed
            ReportingBasis
                display as text
            ReportingBasisCloseResult
                label is "Sequence"
                display as text
            RunTime
                display as text
        blank line
        header2 of "Parameters"
        three column distributed
            AccountingEntity
                display as text
            AccountingEntityGroup
                display as text
            CloseOption
                display as text
            CloseLedger
                display as text
            NewYear
                label is "Period"
                display as text
            NewYearDate
                label is "PeriodDate"
                display as text
            ChangedTotalsOnly
                display as text
            paragraph
                DateRange.Begin
                    label is "CloseDateRange"
                    display as text
                DateRange.End
                    label is "-"
                    display as text
