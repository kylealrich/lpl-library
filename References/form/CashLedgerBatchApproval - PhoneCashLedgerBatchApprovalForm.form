PhoneCashLedgerBatchApprovalForm is a CompositeForm
    title is "BatchApproval<CashLedgerBatchApproval>"
    context form is PhoneCashLedgerBatchApprovalContextForm
        background is theme color
    Actions
        action is Open.ClearAllRecords
            hide when invalid
        action is Open.SelectRecords
            hide when invalid
        action is Open.ReleaseRecords
            hide when invalid
        action is Open.SubmitForApproval
            hide when invalid
        action is previous record
            label is "_"
            mouse over text is "Previous_Record"
        action is next record
            label is "_"
            mouse over text is "Next_Record"
        action is Open.Create
            action icon is create
            label is "_"
            mouse over text is "Create"
        action is save
            label is "_"
            mouse over text is "Save"
        action is Open.Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is refresh
            label is "_"
    Payments is a Panel
        visible when (BatchApprovalType.CashLedgerPayment)
        list is CashLedgerPayment set.PhoneCashLedgerBatchApprovalPayments
    CashReceipts is a Panel
        visible when (BatchApprovalType.CashLedgerReceipt)
        list is CashLedgerCashReceiptHeader set.PhoneCashLedgerBatchApprovalReceipts
    ElectronicFundsTransactions is a Panel
        visible when (BatchApprovalType.ElectronicFundsTransaction)
        list is CashLedgerElectronicFundsTransferTransaction set.PhoneCashLedgerBatchApprovalEFTTransactions
    Parameters is a Panel
        form is PhoneBatchParameters
    ApprovalComments is a Panel
        visible when (HasApprovalComments)
        list is CashLedgerBatchApprovalReasonCodeUsage set.PhoneReasonCodeUsageList
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
