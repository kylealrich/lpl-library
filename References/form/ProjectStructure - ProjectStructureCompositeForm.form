ProjectStructureCompositeForm is a CompositeForm
    is primary
    title is "<FinanceEnterpriseGroup.ProjectLabel>_Structure_<ProjectStructure>_<Description>"
    StructurePanel is a Panel
        title is "Structure"
        form is ProjectStructureForm
    Projects is a Panel
        title is "Hierarchy"
        list is ProjectNode.ProjectChildrenRel.ProjectList
    ProjectUpload is a Panel
        title is "Upload"
        visible when (not UnusedDimensionStructure)
        list is ProjectUpload set.ProjectUploadForStructure
    ProjectEmployees is a Panel
        title is "Employees"
        visible when (IsEnterpriseStructure)
        list is ProjectEmployeeRel.ProjectEmployeeList
            helper list is ActiveEmployeeRel.ActiveEmployeesForProjects
                action is CreateProjectEmployee
    ProjectTeams is a Panel
        title is "<FinanceEnterpriseGroup.ProjectLabel>Teams"
        visible when (IsEnterpriseStructure)
        list is FinanceEnterpriseGroup.ProjectTeam set.ProjectTeamList
