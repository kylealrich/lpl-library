MMatchMessageContext is a Form
    title is "MatchMessageContext"
    Layout
        display as text
            two column distributed
                paragraph
                    Company
                    Company.PayablesCompany.Name
                        no label
                button of "<PayablesInvoice.Invoice>"
                    link is MPayablesInvoiceDetails
                    label is "Invoice"
                    display as link
                AuthorityCode
                    label is "AuthorityCode"
                visible when (not PayablesInvoice.MatchProcessType.ServiceContract)
                    button of "<PurchaseOrder>"
                        link is PurchaseOrderDetails
                        label is "P\\ONumber"
                        display as link
                visible when (PayablesInvoice.MatchProcessType.ServiceContract)
                    button of "<Contract>"
                        link is ContractNav
                        label is "Contract"
                        display as link
                Operator
                Status
                MessageType
                    label is "Type"
                paragraph
                    Vendor
                        select is ActiveVendorsByName
                    Vendor.VendorName
                        no label
                paragraph
                    create stamp
                        label is "DateAndTime"
                BuyerOwner
                Buyer
