ConfiguredVsDeliveredDifferenceForm is a Form
    title is "Delivered_vs_Current"
    Layout
        two column distributed
            column1
                visible when (Copied)
                    paragraph
                        text of "DeliveredBasedOn_'<BasedOnDeliveredPolicy.MenuSecurity.Role>'"
            column2
                paragraph
                    visible when (Configured)
                        button of "ResolveDifferences"
                            action is EditWithComparison
                            right align
                            is primary
                        button of "RevertToDelivered"
                            action is RevertToDelivered
                            right align
                    visible when (not Configured)
                        button of "Configure"
                            action is Configure
                            right align
                            is primary
                    visible when (BasedOnDelivered)
                        button of "MarkReviewed"
                            action is MarkReviewed
                            right align
        visible when (BasedOnDelivered)
            six column distributed
                column2
                    text of "Delivered"
                        header3
                        bold
                column5
                    paragraph
                        text of "Current"
                            header3
                            bold
            single column
                ConfigurationCompare
                    no label
                    display only
                    use text area of 30 lines
                    compare with DeliveredConfigurationCompare
