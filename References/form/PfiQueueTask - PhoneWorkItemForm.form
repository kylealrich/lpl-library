PhoneWorkItemForm is a CompositeForm
    use for smartphone
    title is "WorkItem"
    WorkItem is a Panel
        form is PhoneWorkItemContextForm
    DisplayExec2 is a Panel
        title is "Details"
        visible when (PhoneDisplayTypeIsURL)
        url is "<PhoneDisplayExec>"
    History is a Panel
        title is "History"
        list is PfiWorkunit.PfiQueueRel.PhoneInbasketHistoryList
    Messages is a Panel
        title is "Messages"
        list is PfiWorkunit.PfiMessage set.InbasketMessageList
    RelatedLinks is a Panel
        title is "RelatedLinks"
        list is PfiWorkunit.PfiWorkunitFolder set.PhoneInbasketWorkunitFolderList
