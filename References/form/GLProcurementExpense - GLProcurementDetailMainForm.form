GLProcurementDetailMainForm is a Form
    title is "Main"
    Layout
        two column distributed
            AccountingEntity
                label is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
            paragraph
                PostingDate
                EntityYearPeriod
        two column distributed
            FinanceCodeBlock
                label is "FinanceStructure"
                compact format
            FinanceCodeBlock.GeneralLedgerChartAccount.AccountDescription
        two column distributed
            paragraph
                JournalCode
                TransactionDate
            paragraph
                System
                GeneralLedgerEvent
                    label is "Event"
        two column distributed
            Description
            Reference
        two column distributed
            DocumentNumber
            RelatedJournalTransaction.BusinessClassName
                label is "LinkedTo"
            RelatedJournalTransaction.SymbolicKeyText
                label is "KeyFields"
            OriginatingTransaction.BusinessClassName
                label is "SourceTransaction"
