FutureApplicationReportHeaderComposite is a CompositeForm
    is primary
    title is "FutureApplicationReport"
    context form is FutureApplicationReportHeaderContext
        background is theme color
    restrict action DeleteAllResults
    Actions
        action is Delete
            action icon is delete
            label is "_"
            mouse over text is "Delete"
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is refresh
            label is "_"
        action is save
            label is "_"
            hide when invalid
    FutureApplicationReportDetail is a Panel
        visible when (FutureApplicationDetailRecordExists)
        list is FutureApplicationReportDetailRel.FutureApplicationReportDetailList
