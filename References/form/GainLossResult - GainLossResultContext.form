GainLossResultContext is a Form
    title is "GainLossResult"
    Layout
        three column distributed
            display as text
                ReportingBasis
                GainLossResult
                    label is "RunSequence"
                RunTime
        blank line
        header2 of "Parameters"
        single column
            RunOption
                display as text
        blank line
        three column distributed
            display as text
                Ledger
                AccountingEntity
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>"
                AccountingEntityGroup
                    label is "<FinanceEnterpriseGroup.AccountingEntityLabel>Group"
        three column distributed
            display as text
                Account
                    label is "<FinanceEnterpriseGroup.AccountLabel>"
                AccountGroup
                    label is "<FinanceEnterpriseGroup.AccountLabel>Group"
                ChartSection
                    label is "ChartSection"
        three column distributed
            display as text
                PeriodOption
                DateRange
                    compact format
                ChangesOnly
        three column distributed
            display as text
                RateOption
                PostOption
                OtherDimensions
                visible when (RunOption.Revalue)
                    AutoReverse
                BySystemCode
                Released
                SaveCalculation
        blank line
        header2 of "CurrencyRevalued"
        single column
            display as text
                visible when (RunOption.Revalue)
                    Functional
                        label is "FunctionalCurrency"
                Alternate
                    label is "AlternateCurrency"
                Alternate2
                    label is "Alternate2Currency"
                Alternate3
                    label is "Alternate3Currency"
                visible when (FinanceEnterpriseGroup.ProjectLabelExists)
                    Project
                        label is "<FinanceEnterpriseGroup.ProjectLabel>Currency"
                visible when (FinanceEnterpriseGroup.ReportCurrencyOneEntered)
                    Report1
                        label is "<FinanceEnterpriseGroup.ReportOne>"
                visible when (FinanceEnterpriseGroup.ReportCurrencyTwoEntered)
                    Report2
                        label is "<FinanceEnterpriseGroup.ReportTwo>"
                visible when (FinanceEnterpriseGroup.ReportCurrencyThreeEntered)
                    Report3
                        label is "<FinanceEnterpriseGroup.ReportThree>"
                visible when (FinanceEnterpriseGroup.ReportCurrencyFourEntered)
                    Report4
                        label is "<FinanceEnterpriseGroup.ReportFour>"
                visible when (FinanceEnterpriseGroup.ReportCurrencyFiveEntered)
                    Report5
                        label is "<FinanceEnterpriseGroup.ReportFive>"
