SendDBSessionDebug is an ActionForm
    action is Share
    title is "TimeStatSummary"
    Layout
        single column
            SendVia
                display as radio buttons
        visible when (SendVia.FTP)
            single column
                ServerName
                ServerPort
                Username
                Password
                SendViaSFTP
        visible when (SendVia.Email)
            single column
                To1
                    label is "To"
                To2
                    label is "__"
                To3
                    label is "__"
                To4
                    label is "__"
                Cc1
                    label is "Cc"
                Cc2
                    label is "__"
                Cc3
                    label is "__"
                Cc4
                    label is "__"
                Subject
                IncidentNumber
                Contents
        visible when (SendVia.SaveToLocalDrive)
            single column
                SessionDebugZipDefault
                    label is "FileName"
