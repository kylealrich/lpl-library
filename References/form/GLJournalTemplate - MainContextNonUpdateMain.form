MainContextNonUpdateMain is a Form
    propagate as a drill
    title is "JournalTemplate"
    Layout
        display as text
            two column distributed
                column1
                    AllowMultiplePerDay
                    Ledger
                        label is "DefaultLedger"
                    GeneralLedgerEvent
                        label is "Event"
                    Currency
                    JournalType
                column2
                    LastUsedDate
                    AccountingEntity
                        label is "<FinanceEnterpriseGroup.AccountingEntityLabel>"
                    Active
                    UseCurrentDateForPostingDate
                    AutoReverse
