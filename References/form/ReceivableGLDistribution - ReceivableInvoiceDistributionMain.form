ReceivableInvoiceDistributionMain is a Form
    Layout
        four column distributed
            column1
                paragraph
                    LineType
                visible when (TaxEntityUsesOneSource)
                    paragraph
                        ReceivableCompanyDataGroup.UNSPSCCode
                            compact format
                        ReceivableCompanyDataGroup.UNSPSCCode.ShortDescription
                            label is "_"
                            display as text
            column2
                paragraph
                    TaxCode
                        display as text
            column3
                paragraph
                    TransactionTaxableAmount
                        display as text
            column4
                paragraph
                    Units
        header1 of "Intrastat"
        four column distributed
            column1
                paragraph
                    ICNCode
                        show up to 10 characters
                    ICNCode.Description
                        no label
                        display as text
            column2
                paragraph
                    SupplementaryQuantity
            column3
                paragraph
                    ICNCode.SupplementaryUOM
            column4
                paragraph
                    StockWeight
                        label is "Weight"
        two column distributed
            column1
                header3 of "GlobalInput"
                paragraph
                    CompanyCustomerRel.VATRegistrationCountry
                        label is "VendorVatRegistration"
                        display as text
                    CompanyCustomerRel.VATRegistrationNumber
                        no label
                        display as text
                paragraph
                    TaxEntityRel.VATRegistrationCountry
                        label is "CompanyVatRegistration"
                        display as text
                    TaxEntityRel.VATRegistrationNumber
                        no label
                        display as text
                paragraph
                    ProductTaxCategory
                        display as text
                    ProductTaxCategory.Description
                        no label
                        display as text
                paragraph
                    ReceivableInvoiceRel.ShipTerm
                        label is "DeliveryTerms"
                    ReceivableInvoiceRel.ShipTerm.Description
                        no label
                        display as text
                paragraph
                    TaxUsageCode
                        display as text
                    TaxUsageCode.Description
                        no label
                        display as text
                single column
                    RecoverableImpositionType
                    RecoverableTaxAreaId
                        show up to 20 characters
                    RecoverableJurisLevel
                        show up to 20 characters
                    VATInputTaxAmount
                        label is "ChargedTax/InputVAT"
                    VertexVATType
                        label is "VatType"
                        show up to 20 characters
                    visible when (IsHSNSACCodeEnabled)
                        paragraph
                            HSNSACCode
                            HSNSACCode.Description
                                no label
                                display as text
            column2
                header3 of "GlobalOutput"
                single column
                    Imposition
                        display as text
                        show up to 20 characters
                    RateClassificationText
                        label is "RateClassification"
                        display as text
                        show up to 20 characters
                single column
                    RecoverablePercent
                        display as text
                    RecoverableAmount
                        display as text
                    UnrecoverableAmount
                        display as text
                paragraph
                    InvoiceTextCode.InvoiceTextCode
                        display as text
                    InvoiceTextCode.InvoiceTextCodeDesc
                        no label
                        display as text
                        show up to 20 characters
                single column
                    ForeignOrDomestic
                        display as text
                    IsService
                        display as text
