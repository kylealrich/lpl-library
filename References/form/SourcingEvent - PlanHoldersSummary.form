PlanHoldersSummary is a CompositeForm
    title is "PlanHoldersListForEvent#<SourcingEvent>:_<SourcingEvent.Name>"
    Actions
        action is AddMeToPlanHoldersList
            hide when invalid
        action is RemoveMeFromPlanHoldersList
            hide when invalid
    PlanHolders is a Panel
        list is SourcingEventPlanHolder set.PlanHoldersListSupplier
