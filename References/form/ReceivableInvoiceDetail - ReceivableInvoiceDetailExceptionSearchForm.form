ReceivableInvoiceDetailExceptionSearchForm is a SearchForm
    Layout
        four column distributed
            Company
            Customer
        four column distributed
            ReceivableProcessLevel
                label is "ProcessLevel"
            ReceivableGeneralLedgerCode
                label is "AccountsReceivableCode"
            ReceivableInvoiceRel.CurrencyTable
            OriginalCurrency
        four column distributed
            IsUnreleased
                label is "Unreleased"
            HasReceivableInvoiceObligationGroupSearch
                label is "ObligationGroupPending"
            visible when (actor.context.FinanceEnterpriseGroup.FundAccounting)
                HasDistributionSetNeedsReviewApplications
                    label is "DistributionSetNeedsReview"
        four column distributed
            HasPendingReversal
                label is "ReversalPending"
            HasARToAPApplicationSelectSearch
                label is "ARToAPPending"
            HasApplicationSelect
                label is "ApplicationsPending"
            HasReceivableApplicationAdjustmentSelectSearch
                label is "AdjustmentsPending"
        four column distributed
            TransactionDateRange.Begin
                label is "FromTransactionDate"
            TransactionDateRange.End
                label is "ToTransactionDate"
            DueDateRange.Begin
                label is "FromDueDate"
            DueDateRange.End
                label is "ToDueDate"
