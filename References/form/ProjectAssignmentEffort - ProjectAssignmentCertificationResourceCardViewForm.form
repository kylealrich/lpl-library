ProjectAssignmentCertificationResourceCardViewForm is a Form
    title is "<FinanceEnterpriseGroup.ProjectLabel>"
    Layout
        single column
            text of "<Employee.Name.PreferredFirstAndLastName>-<Employee>"
            text of "<Project>-<Project.Description>"
            text of "<DateRange.Begin>-<DateRange.End>"
            visible when (Certified)
                text of "By<CertifyingActor>_<DateCertified>"
            CertifiedCommittedEffort
                display as progress bar
                    green alert range is from 0.001 to 1
            AveragePlannedPercent
                display as progress bar
                    green alert range is from 0.001 to 1
            AverageActualPercent
                display as progress bar
                    green alert range is from 0.001 to 1
            EffortPercent
