QuickWriteoff is an ActionForm
    action is QuickWriteoff
    Layout
        single column
            Reason
                select is ReceivableReasonCodeOSOrTWList
            Amount
            PrmGeneralLedgerDate
                label is "GlobalLedgerDate"
        visible when (Reason.ReceivableAdjustmentReasonType.TaxWithholding)
            single column
                PrmWriteoffARIncomeWithholdingCode
                    label is "IncomeWithholdingCode"
                PrmWriteoffARIncomeWithholdingDocumentNumber
                    label is "IncomeWithholdingDocumentNumber"
        single column
            PrmAttachment.File
                label is "Attachment"
