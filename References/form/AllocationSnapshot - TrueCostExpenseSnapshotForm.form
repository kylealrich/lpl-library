TrueCostExpenseSnapshotForm is a CompositeForm
    title is "ExpenseSnapshot<AllocationSnapshot>"
    context form is Header
        background is theme color
    Actions
        action is search
            action icon is filter
            label is "_"
        action is previous record
            label is "_"
        action is next record
            label is "_"
    Details is a Panel
        list is AllocationSnapshotDetail set.ExpenseSnapshotDetailList
    RelatedRuns is a Panel
        list is TrueCostAllocationRunRel.ExpenseSnapshotRunList
