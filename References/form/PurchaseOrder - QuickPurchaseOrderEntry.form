QuickPurchaseOrderEntry is a Form
    Layout
        single column
            PurchaseOrder
                display as text
        paragraph
            Company
            Company.Name
                no label
                display as text
        paragraph
            Buyer
            Buyer.Employee.PresentationNameSnapshot
                no label
                display as text
        paragraph
            POCode
            POCode.Description
                no label
                display as text
        paragraph
            Vendor
                select is ActiveVendorsByName
            Vendor.VendorName
                no label
                display as text
        paragraph
            PurchaseFromLocation
                label is "PurchaseFrom"
                select is NonRemitToActivePurchaseFromLocations
            PurchaseFromLocation.VendorName
                no label
                display as text
        paragraph
            ShipToLocation
                label is "ShipTo"
            ShipToLocation.NameForPurchaseOrder
                no label
                display as text
        single column
            PurchaseOrderDate
            DefaultDeliveryDate
                label is "DeliveryDate"
