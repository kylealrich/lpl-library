InterfaceReceipts is an ActionForm
    action is InterfaceReceipts
    Layout
        single column
            PrmRunGroup
                label is "RunGroup"
            PrmCompany
                label is "Company"
                when value changed
                    PrmLocation =  blank
            valid when (PrmCompany entered)
                PrmLocation
                    label is "Location"
            ReleaseReceipts
                label is "ReleaseReceipts"
                    left of field
            visible when (ReleaseReceipts)
                ReleaseOption
                    label is "ReleaseOption"
                        left of field
            AllowDropshipReceivers
                label is "AllowDropshipReceivers"
                    left of field
            AllowMatchItemOrVendorItem
                label is "AllowMatchOnEitherItemOrVendorItem"
                    left of field
            EnableGTINProcessing
                label is "EnableGTINProcessing"
                    left of field
            EnableGLNProcessing
                label is "EnableGLNProcessing"
                    left of field
            MoveErrorsToNewRunGroup
                label is "MoveErrorsToNewRunGroup"
                    left of field
            PrmErrorRunGroupPrefix
                label is "ErrorRunGroupPrefix"
            RemoveExistingReceiptsInReceiptMaintenance
                label is "RemoveExistingReceiptsInReceiptMaintenance"
                    left of field
