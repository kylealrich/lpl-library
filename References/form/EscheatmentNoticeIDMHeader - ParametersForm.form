            single column
                ParameterCashManagementGroup
                    label is "CashManagementGroup"
            single column
                ParameterCashCode
                    label is "CashCode"
            single column
                ParameterCashCodeGroup
                    label is "CashCodeGroup"
            single column
                ParameterBankTransactionCode
                    label is "BankTransactionCode"
            single column
                ParameterProcessLevel
                    label is "ProcessLevel"
            single column
                ParameterStatus
                    label is "Status"
            two column
                ParameterDateRange.Begin
                    label is "FromIssueDate"
                ParameterDateRange.End
                    label is "ToIssueDate"
            single column
                ParameterIDMPrinter
                    label is "IDMPrinter"
