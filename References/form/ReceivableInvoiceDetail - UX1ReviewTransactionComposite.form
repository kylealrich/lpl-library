UX1ReviewTransactionComposite is a CompositeForm
    title is "ReviewApplications_-_<ReceivableInvoiceDetail.ReceivableInvoiceDetailType>_<ReceivableInvoiceDetail.Invoice>"
    context form is UX1ReviewTransactionContextForm
        background is theme color
    Actions
        action is DeleteAllApplications
            action icon is delete
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
    CreditsApplied is a Panel
        visible when (CreditMemoHasApplications)
        list is CreditMemoReceivableApplicationRel.CreditsAppliedGenericTitleList
    Applications is a Panel
        list is ReceivableApplicationRel.TransactionApplicationsGenericTitleList
