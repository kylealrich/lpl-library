ProjectAssignmentSplit is a Form
    is primary
    title is "AssignmentSplit"
    Layout
        two column distributed
            column1
                paragraph
                    Project
                        label is "<FinanceEnterpriseGroup.ProjectLabel>"
                        display as text
                        show up to 10 characters
                    Project.Description
                        no label
                        display as text
            column2
                paragraph
                    Project.ProjectDateRange.BeginDate
                        label is "<FinanceEnterpriseGroup.ProjectLabel>BeginDate"
                        display as text
                    Project.ProjectDateRange.EndDate
                        label is "<FinanceEnterpriseGroup.ProjectLabel>EndDate"
                        display as text
        two column distributed
            column1
                paragraph
                    Employee
                    Employee.Employee.Name.PreferredFirstAndLastName
                        no label
                        display as text
            column2
                paragraph
                    Employee.ProjectLaborSchedule
                        display as text
                    Employee.ProjectLaborSchedule.Description
                        no label
                        display as text
        single column
            CommittedEffort
            FinanceCodeBlock
                compact format
            text of "DimensionsSetToBlank:"
                align as label
            FinanceCodeBlockSetBlanks
                no label
            visible when (not Project.FringeBurden.No)
                BurdenCodeBlock
                    compact format
                text of "DimensionsSetToBlank:"
                    align as label
                BurdenCodeBlockSetBlanks
                    no label
