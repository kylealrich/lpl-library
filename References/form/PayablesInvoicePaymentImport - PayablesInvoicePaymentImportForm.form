PayablesInvoicePaymentImportForm is a Form
    is primary
    title is "InvoicePaymentImport"
    Layout
        header2 of "InvoiceInformation"
        display as text
            three column distributed
                paragraph
                    Company
                paragraph
                    Vendor
                    OldVendor
                        label is "InterfacedVendor"
                paragraph
                    Invoice
                    Suffix
                        no label
                    EDINumber
        header2 of "PaymentInformation"
        three column distributed
            column1
                paragraph
                    DueDate
                paragraph
                    PaymentAmount
                paragraph
                    CashCode
                        show up to 12 characters
                    CashCode.Description
                        no label
                        display as text
                paragraph
                    ImportBankTransactionCode
                        label is "PaymentCode"
            column2
                paragraph
                    CheckDate
                paragraph
                    InvoiceGroup
                paragraph
                    IncomeCode
                    IncomeCode.Description
                        no label
                        display as text
                paragraph
                    ReportableIncomeAmount
            column3
                paragraph
                    SeparatePayment
                paragraph
                    PayImmediately
                paragraph
                    Enclosure
                paragraph
                    TransactionNumber
        header2 of "Discount"
        three column distributed
            DiscountDate
            DiscountPercent
            DiscountAmount
