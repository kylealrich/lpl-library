ProcessLevelForm is a CompositeForm
    is primary
    title is "BillingProcessLevel"
    context form is ProcessLevelFormContext
        background is theme color
    RemitTo is a Panel
        form is RemitTo
    GlobalLedgerAccounts is a Panel
        form is GLAccounts
    Invoicing is a Panel
        form is Invoicing
    OrderEntryDefaults is a Panel
        form is OEDefaults
    OrderAcceptance is a Panel
        form is OrderAcceptance
    MultipleTaxAreas is a Panel
        visible when (VertexResultsExist)
        list is VertexTaxAreaIdResultsRel.TaxAreaResultList
    VertexError is a Panel
        visible when (VertexErrorExist)
        list is VertexWebserviceErrorRel.VertexTaxAreaIdErrorList
