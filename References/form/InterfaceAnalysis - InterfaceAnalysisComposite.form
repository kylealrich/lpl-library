InterfaceAnalysisComposite is a CompositeForm
    is primary
    title is "InterfaceAnalysis"
    context form is InterfaceAnalysisContext
    ByCurrencyAndMonth is a Panel
        visible when (IsCompleted)
        list is InterfaceAnalysisDetail set.CurrencyMonthYearSummaryList
    Details is a Panel
        visible when (IsCompleted)
        list is InterfaceAnalysisDetail set.InterfaceAnalysisDetailList
