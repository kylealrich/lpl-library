AssetMain is a Form
    restrict action FullDispose
    Layout
        two column distributed
            column1
                paragraph
                    FinanceEnterpriseGroup
                        display as text
                    Asset
                        display as text
                    Description
                        show up to 20 characters
                TagNumber
                    show up to 20 characters
                paragraph
                    visible when (not RestrictLeaseUpdate)
                        AssetLease.LeaseCompany
                            label is "LeaseCompany"
            column2
                paragraph
                    Company
                        label is "AssetCompany"
                    Company.GeneralLedgerCompany.Name
                        no label
                        display as text
                        show up to 20 characters
                AssetCategory
                    label is "Category"
                paragraph
                    visible when (not RestrictLeaseUpdate)
                        paragraph
                            AssetLease.Lease
                                show up to 20 characters
                            AssetLease.Lease.Description
                                no label
                                display as text
                                show up to 20 characters
        paragraph
            visible when (FundAccounting)
                visible when (not AssetLease.Lease.CreateAssetFundRecords)
                    FundOverride
                        label is "FundOverride"
                            left of field
                visible when (AssetLease.Lease.CreateAssetFundRecords)
                    FundOverride
                        label is "FundOverride"
                            left of field
                        display as text
        three column
            column1 of 50%
                Reference1
            column2 of 30%
                Reference2
            column3 of 20%
                ProcessGroup
        two column distributed
            paragraph
                visible when (DisplayAdjustmentDate)
                    AdjustmentDate
                        label is "AdjustmentDate"
                visible when (not PostingDateValidation)
                    PostingDate
                        label is "PostingDate"
            visible when (AseetGuidelineClassesExists)
                paragraph
                    AssetGuidelineClass
                        label is "GuidelineClass"
                        select is ActiveGuidelineClassesRel.AssetGuidelineClasses
                    AssetGuidelineClass.Description
                        no label
                        display as text
        header2 of "AssetAccounts"
        two column distributed
            column1
                AssetType
                    label is "Type_/Subtype"
                    compact format
                    select is AssetTypeList
                AssetGroup
                    label is "AssetGroup"
            column2
                paragraph
                    AssetAccountGroup
                        label is "AccountGroup"
                    AssetAccountGroup.Description
                        no label
                        display as text
                paragraph
                    AssetAccountingUnitGroup
                    AssetAccountingUnitGroup.Description
                        no label
                        display as text
        two column distributed
            column1
                paragraph
                    AssetSummaryGroup
                        label is "SummaryGroup"
                    AssetSummaryGroup.Description
                        no label
                        display as text
                    visible when (HasSummaryGroup)
                        button of "View"
                            link is SummaryGroup
        two column distributed
            AssetAccountingUnit
                label is "DefaultTransactionDimensions"
                compact format
            paragraph
                visible when (ProjectsInUse)
                    DepreciationExpenseProject.Project
                        label is "Depreciation_Expense_Project"
        header2 of "Location_andProcessingOptions"
        two column distributed
            column1
                paragraph
                    AssetLocation
                        label is "Location"
                    AssetLocation.Description
                        no label
                        display as text
                        show up to 20 characters
                paragraph
                    AssetDivision
                        label is "Division"
                    AssetDivision.Description
                        no label
                        display as text
                        show up to 20 characters
                paragraph
                    CurrencyTable
                    Currency
                paragraph distributed
                    Simulated
                    IncludeInPhysicalInventory
                        label is "Inventory"
                    WorkInProcess
            column2
                paragraph
                    AssetOwner
                        label is "Owner"
                        show up to 15 characters
                    AssetOwner.Name.FirstAndLastName
                        no label
                        display as text
                        show up to 20 characters
                paragraph
                    visible when (ProjectsInUse)
                        AssetProject
                            left align
                    visible when (not ProjectsInUse)
                        text of "_"
                paragraph
                    GeographicalLatitude
                        label is "Latitude"
                    GeographicalLongitude
                        label is "Longitude"
                    GeographicalAltitude
                        label is "Altitude"
                paragraph distributed
                    TaxExempt
                    Used
                    Classification
                        label is "Classification"
                        display as text
