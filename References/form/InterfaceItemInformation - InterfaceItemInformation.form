    action is InterfaceItemInformation
    title is "InterfaceItemInformation"
    Layout
        single column
            PrmRunGroup
                label is "RunGroup"
            ProcessingOption
        paragraph
            PrmItemGroup
                label is "ItemGroup"
            PrmItemGroup.Description
                no label
                display as text
        visible when (ProcessingOption.CSVFile)
            single column
                Item
                ItemComment
                VendorItem
                ItemLocation
                ItemGTIN
                ItemSKU
                ItemUPC
                NationalDrugCode
                UCCEANItemUPN
                HIBCCItemUPN
