TabletWorkItemForm is a CompositeForm
    use for tablet
    title is "WorkItem"
    context form is TabletWorkItemContextForm
    DisplayExec1 is a Panel
        title is "Details"
        visible when (TabletDisplayTypeIsContentOrFile)
        form is TabletDisplayExec1Form
    DisplayExec2 is a Panel
        title is "Details"
        visible when (TabletDisplayTypeIsURL)
        url is "<TabletDisplayExec>"
    History is a Panel
        title is "History"
        list is PfiWorkunit.PfiQueueRel.InbasketHistoryList
    Messages is a Panel
        title is "Messages"
        list is PfiWorkunit.PfiMessage set.InbasketMessageList
    RelatedLinks is a Panel
        title is "RelatedLinks"
        list is PfiWorkunit.PfiWorkunitFolder set.InbasketWorkunitFolderList
