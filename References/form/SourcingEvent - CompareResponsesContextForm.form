CompareResponsesContextForm is a Form
    title is "ResponseComparison"
    Layout
        four column
            display as text
                SourcingEvent
                    label is "Event#"
                Name
                    label is "EventName"
                NumberOfSourcingEventLines
                    label is "NumberOfLines"
                CurrencyCode
                    label is "AllAmountsIn"
        three column
            DerivedStatus
                label is "Status"
                display as text
            visible when (UsingEstimatedTotal)
                EstimatedTotal
                    display as text
            paragraph
                button of "FinalAward"
                    visible when (FinalAwardAvailable)
                    action is Open.Notified.FinalAward
                button of "CreateOutput"
                    visible when (CreateOutputAvailable)
                    action is Open.Notified.CreateOutput
