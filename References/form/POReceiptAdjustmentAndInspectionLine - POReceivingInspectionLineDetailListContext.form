POReceivingInspectionLineDetailListContext is a Form
    Layout
        display as text
            four column distributed
                column1
                    Company.DerivedCompanyIDName
                        label is "Company"
                    paragraph
                        DerivedDisplayReceiver
                        ReceiptLinesForTemplateRel.PurchaseOrderLine.LineNumber
                            label is "Line"
                        visible when (AssetDetailExist)
                            button of "Asset_\\Detail"
                                link is POLineAsset
                                display as link
                column2
                    paragraph
                        DisplayItem
                            label is "Item"
                        DerivedDisplayItemType
                    DerivedDisplayVendorItem
                column3
                    visible when (LineExists)
                        PurchaseOrderReceiptLine.ReceivedDate
                    DisplayReceivedLocation
                        label is "ReceivedLocation"
                column4
                    Inspected
                        label is "InspectionStatus"
                        display as text
