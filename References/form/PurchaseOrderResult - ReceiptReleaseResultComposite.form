ReceiptReleaseResultComposite is a CompositeForm
    title is "ReceiptBatchReleaseResult"
    context form is ReceiptReleaseResultContext
        background is theme color
    Overview is a Panel
        form is ReceiptReleaseResultMainForm
    Released is a Panel
        list is ReleasedReceiversRel.ReleasedReceiverList
    Errors is a Panel
        visible when (HasErrors)
        list is ReceiversWithErrorRel.ReceiverWithErrorsList
