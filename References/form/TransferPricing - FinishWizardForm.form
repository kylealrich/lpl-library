FinishWizardForm is a Form
    title is "ViewTransferPricingRecharge"
    Layout
        display as text
            two column distributed
                column1
                    TransferPricing
                        label is "RechargeName"
                    Description
                    TransferPricingType
                    paragraph
                        visible when (not CompanyExists)
                            Company
                                label is "ReceivablesCompany"
                                select is ReceivableCompanySelectForIntercompanyBilling
                        visible when (CompanyExists)
                            ReceivablesCompanyDescription
                                label is "ReceivablesCompany"
                            Company.Currency
                                no label
                    Company.GeneralLedgerCompany.AccountingEntity
                        label is "BillFromEntity"
                    Ledger
                column2
                Calendar
                PeriodType
                YearToDate
                Currency
                CurrencyTable
        blank line
        single column
            text of "ClickTheLinkBelowToViewTransferPricingRecharge."
                italics
        blank line
        single column
            button of "TransferPricingRecharge"
                link is ViewTransferPricingRecharge
                display as link
