Miscellaneous is a Form
    Layout
        two column
            column1
                Company
                Lease
                Vendor
                StraightLine
                StraightLineAmountOverride
                    label is "OverrideAmount"
            column2
                paragraph
                    DoNotCreateAPInvoice
                paragraph
                    visible when (LessorPaymentSchedule)
                        visible when (not DoNotAllowUpdateLeasePaymentDueDay)
                            LeasePaymentEOM
                                label is "EndOfMonthPayments"
                                display as horizontal radio buttons
                            visible when (LeasePaymentEOM.No)
                                LeasePaymentDueDay
                                    label is "LeasePaymentDueDay"
                                    display as required when (LessorPaymentSchedule)
                        visible when (DoNotAllowUpdateLeasePaymentDueDay)
                            visible when (IsEOMPayment)
                                LeasePaymentEOM
                                    label is "EndOfMonthPayments"
                                    display as text
                            visible when (not IsEOMPayment)
                                LeasePaymentDueDay
                                    label is "LeasePaymentDueDay"
                                    display as text
                paragraph
                    ProcessLevel
                    ProcessLevel.Name
                        no label
                        display as text
                paragraph
                    visible when (LessorPaymentSchedule)
                        IncomeCode
                paragraph
                    RemitToCode
                        label is "PayablesVendorLocation"
                paragraph
                    AccrualCode
                        label is "PayablesAccrualCode"
        paragraph
            LeaseInvoiceAccrualAccount
                compact format
            LeaseInvoiceAccrualAccount.GeneralLedgerChartAccount.AccountDescription
                label is "_"
                display as text
