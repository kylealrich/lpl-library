MainExtractForm is a CompositeForm
    is primary
    title is "ConsolidatedItemLevelMasterData"
    context form is ConsolidatedItemMasterContext
    Actions
        action is refresh
            label is "_"
        action is CreateUOMsAndDeviceIdentifiers
            label is "CreateUnitsOfMeasureAndDeviceIdentifiers"
            hide when invalid
    Main is a Panel
        form is ConsolidatedItemMasterMain
    Locations is a Panel
        visible when (HasLocations)
        list is ConsolidatedLocationRel.primary
    ContractLines is a Panel
        visible when (HasContractLinesByItem)
        list is ContractLinesByItemRel.ContractLinesForConsolidatedItemMasterList
    ContractLinesForManufacturerInformation is a Panel
        title is "ContractLines"
        visible when (HasContractLinesByManufacturer)
        list is ContractLinesByManufacturerRel.ContractLinesForConsolidatedItemMasterList
    UOMAndDIInformation is a Panel
        form is UnitOfMeasureInformationForm
    DeletedAndInactiveDeviceIdentifiers is a Panel
        visible when (HasDeletedOrInactiveUnitsOfMeasure)
        form is DeletedInactiveUOMForm
    UnitsOfMeasureAndDeviceIdentifiersToMaintain is a Panel
        visible when (HasUnitsOfMeasureToMaintain)
        form is UOMToMaintainForm
    UnitOfMeasureAndDeviceIdentifierMaintenance is a MultiListPanel
        visible when (UOMMaintenanceNeeded)
        list is AllItemUOMRel.ItemUOMsForContractErrors
        list is AllGTINRel.GlobalTradeItemNumbers
    VendorItems is a Panel
        visible when (VendorItemEntered)
        form is VendorItemForm
    PreferenceCards is a Panel
        visible when (HasPreferenceCards)
        list is ComponentItemRel.PreferenceCardsForConsolidatedItemMaster
