ProjectFundingSourceChangeRequestNavForm is a Form
    title is "<FinanceEnterpriseGroup.FinanceDimension2Label>ChangeRequestFor<Description>"
    Layout
        two column distributed
            column1
                paragraph
                    FinanceDimension2.DisplayDimension
                        label is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
                        display as text
                    FinanceDimension2.Description
                        label is "Description"
                        display as text
        two column distributed
            column1
                paragraph
                    ChangeRequestDate
                        label is "ChangeRequestDate"
                        display as text
                    Description
                        label is "Description"
                        display as text
        two column distributed
            column1
                paragraph
                    ProjectFundingSource.Priority
                        label is "CurrentPriorty"
                        display as text
                    NewPriority
                        label is "NewPriorty"
                        display as text
            column2
                paragraph
                    ProjectFundingSource.FundingGroup
                        label is "CurrentFundingGroup"
                        display as text
                    NewFundingGroup
                        label is "NewFundingGroup"
                        display as text
        two column distributed
            column1
                paragraph
                    ProjectFundingSource.Percent
                        label is "CurrentPercent"
                        display as text
                    NewPercent
                        label is "NewPercent"
                        display as text
            column2
                paragraph
                    ProjectFundingSource.FundedAmount
                        label is "CurrentFundedAmount"
                        display as text
                    NewFundedAmount
                        label is "NewFundedAmount"
                        display as text
        two column distributed
            column1
                paragraph
                    ProjectContractChangeRequest
                        label is "ProjectContractChangeRequest"
                        display as text
                        show up to 5 characters
                    ProjectContractChangeRequest.Description
                        label is "Description"
                        display as text
                        show up to 30 characters
        two column distributed
            column1
                paragraph
                    RequestedBy
                        label is "RequestedBy"
                        display as text
        two column distributed
            column1
                Comment
                    label is "Comment"
                    display as text
            column2
                DocumentURL
                    display as text
        single column
            paragraph
                Attachment
                    label is "Attachment"
                    display as text
