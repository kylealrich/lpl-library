ContractLineImportsComposite is a CompositeForm
    is primary
    title is "ContractLineInterface"
    context form is ContractLineImportContext
        background is theme color
    Actions
        action is Create
        action is CreateWithTierCosts
            hide when invalid
        action is save
            label is "_"
    Line is a Panel
        form is ContractLineImportMain
    TierCosts is a Panel
        visible when (IsManufacturerWithFixedTier)
        list is ContractLineImportTierCostRel.ContractLineImportTierCostList
    DistributorLines is a Panel
        visible when (ContractImport.IsManufacturerContractForGPONotMultiTierDistributor)
        list is ContractLineImportDistributor set.primary
    ItemDetail is a Panel
        form is ContractLineItemDetail
    LineDetail is a Panel
        form is ContractLinePricing
    Retainage is a Panel
        visible when (not ContractImport.RetainagePercentCompleteTracking.NoRetainage)
        form is Retainage
    HealthTrust2021Information is a Panel
        title is "HealthTrustInformation"
        visible when (ContractImport.HasGPOSupplierAndIsHealthTrust)
        form is HealthTrust2021InformationForm
    GHXInformation is a Panel
        visible when (ContractImport.HasGPOSupplierAndIsGHX)
        form is GHXInformationForm
    Errors is a Panel
        visible when (ErrorsExist)
        list is ContractLineImportError set.ContractLineImportErrorList
