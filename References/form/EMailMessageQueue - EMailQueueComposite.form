EMailQueueComposite is a CompositeForm
    is primary
    title is "EMailQueue"
    context form is EMailQueueContext
    Errors is a Panel
        visible when (HasErrorMessage)
        form is ErrorForm
    Message is a Panel
        form is EMailQueueContent
    AlternateContents is a Panel
        list is EMailMessageContent set.ContentList
    Attachments is a Panel
        list is EMailMessageAttachment set.AttachmentList
    Appointments is a Panel
        list is EMailMessageAppointment set.AppointmentList
    EmailDomainVerification is a Panel
        visible when (HasNonVerifiedEmailAddress)
        form is EmailDomainVerification
