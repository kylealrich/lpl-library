CreateWithdrawalHeaderContext is a Form
    title is "<DerivedTransferTypeLabel>"
    Layout
        visible when (RecordExists)
            two column distributed
                column1
                    display as text
                        paragraph
                            CashCode.CashCodeAndDescription
                                label is "ToCashCode"
                            CashCode.Currency
                                no label
                        paragraph
                            Company.OrigCompany.RepresentativeText
                                label is "Company"
                            Company.OrigCompany.Currency
                                no label
                        paragraph
                            TransferBankAmountUnsigned
                                label is "Amount"
                column2
                    display as text
                        paragraph
                            CashLedgerFundTransferHeader
                                label is "TransferNumber"
                            Status
                        paragraph
                            CurrencyTable
                            CurrencyTable.Description
                                no label
                        paragraph
                            visible when (IsUnreleased)
                                AppliedBankAmount
                                    label is "AppliedBankAmount"
                                BalanceAmount
                                    label is "Balance"
                                AppliedBaseAmount
                                    label is "AppliedBaseAmount"
                        paragraph
                            visible when (BudgetEditInProgress)
                                text of "BudgetEditsInProgress"
                                    bold
                                    color of red
                            visible when (BudgetEditFailure)
                                text of "BudgetsExceeded;CouldNotRelease;SeeBudgetErrorsTab"
                                    bold
                                    color of red
