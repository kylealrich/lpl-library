VendorDetailForm is a Form
    is primary
    Layout
        paragraph
            VendorReturn
                display as text
        paragraph
            VendorReturnLine
                label is "Line"
                display as text
        paragraph
            VendorReturnLineDetail
                label is "Sequence"
                display as text
        paragraph
            DocumentType
                label is "DocumentType"
                display as text
        paragraph
            Item
                display as text
        paragraph
            visible when (VendorReturnLine.ItemLocationRel.LotTracked.LotRequiredAtReceipt)
                Lot
                Sublot
        paragraph
            visible when (VendorReturnLine.ItemLocationRel.LotTracked.LotRequiredAtReceipt)
                LotExpirationDate
                LotOnHold
        paragraph
            visible when (VendorReturnLine.ItemLocationRel.SerialTracked.SerialRequiredAtReceipt)
                Serial
        paragraph
            visible when (IsBinTrackedAndNotAWarehouseLocation)
                Bin
                    select is BinsWithStockOnHandRel.BinList
            visible when (IsBinTrackedAndAWarehouseLocation)
                Bin
                    select is BinsWithStockOnHandAndItemStorageLocationRel.DistributionManagementBinList
        visible when (not VendorReturnLine.IsCatchWeightItem)
            paragraph
                visible when (not IsSerialRequiredAtReceipt)
                    Quantity
                visible when (IsSerialRequiredAtReceipt)
                    Quantity
                        display as text
                UnitOfMeasure
                    no label
                    display as text
        visible when (VendorReturnLine.IsCatchWeightItem)
            paragraph
                visible when (not IsSerialRequiredAtReceipt)
                    SecondaryQuantity
                visible when (IsSerialRequiredAtReceipt)
                    SecondaryQuantity
                        display as text
                UnitOfMeasure
                    no label
                    display as text
            paragraph
                Quantity
                    label is "CatchWeightStockQuantity"
                Item.StockUOM
                    no label
                    display as text
