MobileContractSearch is a SearchForm
    title is "ContractSearch"
    Layout
        single column
            ContractSearchDocument
                label is "Search_byContract,Supplier,_orContact"
            ContractStatus
                label is "Status"
            ContractType
                label is "Type"
                select is MobileBuyerContractTypesSelect
            ContractClassification
                label is "Class"
                select is MobileContractClassifications
