RequestNewCustodialRecord is an ActionForm
    action is RequestNewCustodialRecord
    title is "New_<PrmFinanceEnterpriseGroup.FinanceDimension10Label>"
    Layout
        visible when (not actor.initiator)
            header2 of "ReviewNew_<PrmFinanceEnterpriseGroup.FinanceDimension10Label>"
        visible when (actor.initiator)
            header4 of "This_request_will_be_routed_for_approval;_after_approval_the_<PrmFinanceEnterpriseGroup.FinanceDimension10Label>_will_be_created."
        header3 of "RequestDetails"
        two column distributed
            column1
                visible when (not actor.initiator)
                    display as text
                        RequestedBy
                        button of "<RequesterPhone.SubscriberNumber>"
                            link is "tel:<RequesterPhone.SubscriberNumber>"
                            label is "Phone"
                        blank line
                        RequestPriority
                            label is "Priority"
                visible when (actor.initiator)
                    RequestPriority
                        label is "Priority"
            column2
                visible when (not actor.initiator)
                    display as text
                        RequesterActor
                            label is "ID"
                        button of "<RequesterEmail>"
                            link is "mailto:<RequesterEmail>"
                            label is "Email"
                        blank line
                        NeededBy
                visible when (actor.initiator)
                    NeededBy
        header3 of "<PrmFinanceEnterpriseGroup.FinanceDimension10Label>_Details"
        two column distributed
            column1
                PrmFinanceEnterpriseGroup
                    display as text
                CustodialAccountDescription
                paragraph
                    AccountingEntity
                    AccountingEntity.Name
                        no label
                        display as text
                paragraph
                    visible when (PrmFinanceEnterpriseGroup.FundDimension.AccountingUnit)
                        AccUnitSummaryFund
                            label is "Summary_<PrmFinanceEnterpriseGroup.AccountingUnitLabel>"
                            select is SummaryAccountingUnits
                            when value changed
                                AccUnitPostingFund =  blank
                        AccUnitSummaryFund.Description
                            no label
                            display as text
                    visible when (PrmFinanceEnterpriseGroup.FundDimension.Dimension1)
                        DimOneSummaryFund
                            label is "Summary_<PrmFinanceEnterpriseGroup.FinanceDimension1Label>"
                            select is SummaryFinanceDimension1Select
                            when value changed
                                DimOnePostingFund =  blank
                        DimOneSummaryFund.Description
                            no label
                            display as text
                InterestBearing
                CamCategory
                    label is "Category"
                CustodialAccountType
                DefaultFinanceCodeBlock
                    compact format
            column2
                FinanceDimension10Structure
                    display as text
                NewCustodialSummary
                paragraph
                    visible when (actor.initiator)
                        DateOfRequest
                    visible when (not actor.initiator)
                        DateOfRequest
                            display as text
                paragraph
                    visible when (PrmFinanceEnterpriseGroup.FundDimension.AccountingUnit)
                        AccUnitPostingFund
                            label is "Posting_<PrmFinanceEnterpriseGroup.AccountingUnitLabel>"
                            select is AccUnitSummaryFund.DescendantsRel.PostingAccountingUnits
                        AccUnitPostingFund.Description
                            no label
                            display as text
                    visible when (PrmFinanceEnterpriseGroup.FundDimension.Dimension1)
                        DimOnePostingFund
                            label is "Posting_<PrmFinanceEnterpriseGroup.FinanceDimension1Label>"
                            select is DimOneSummaryFund.DescendantsRel.FinanceDimension1Select
                        DimOnePostingFund.Description
                            no label
                            display as text
                InterestBearingThreshold
                paragraph
                    visible when (CamCategory.SSNRequired.Yes)
                        CamCategory.TaxIDType
                        CamCategory.TaxIDType.Description
                            no label
                            display as text
                        TaxID
                paragraph
                    visible when (CustodialAccountType.Other)
                        Title
                    visible when (CustodialAccountType.Individual)
                        FirstName
                        MiddleName
                        LastName
        header3 of "ActionComment"
        single column
            action comment
                no label
                use text area
        header3 of "AccountAttachment"
        single column
            column1
                DocumentId
        single column table
            column1
                TransientAttachmentDescription
        single column table
            column1
                TransientAttachment.File
                    label is "Attachment"
