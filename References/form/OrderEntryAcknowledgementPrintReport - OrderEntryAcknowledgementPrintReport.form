OrderEntryAcknowledgementPrintReport is an ActionForm
    action is OrderEntryAcknowledgementPrintReport
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        blank line
        paragraph
            ProcessLevel
            ProcessLevel.Name
                no label
                display as text
        single column
            SelectOption
                label is "SalesQuotes"
        paragraph
            OscCommentCode
                label is "StandardCommentCode"
            OscCommentCode.Description
                no label
                display as text
        single column
            OrderNbr
                label is "ReprintOrderNumber"
            NbrAlignments
                label is "NumberOfAlignmentForms"
            PrintEdiAck
                label is "PrintE\\D\\IAcknowledgement"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                OrderEntryAcknowledgementPrintReportReport_RDG
                OrderEntryAcknowledgementPrintForm_RDG
            column2 of 25%
                OrderEntryAcknowledgementPrintReportReport_XPT
                OrderEntryAcknowledgementPrintForm_XPT
