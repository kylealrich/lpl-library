        paragraph
            text of "ScheduleBy:"
            button of "Frequency"
                valid when (AllowUpdate)
                action is MyScheduleFrequency
            button of "DateInMonth"
                valid when (AllowUpdate)
                action is MyScheduleDateTime
            button of "Day"
                valid when (AllowUpdate)
                action is MyScheduleWeekDay
            button of "WeekNumber"
                valid when (AllowUpdate)
                action is MyScheduleWeekNumber
            button of "AdvancedSchedule"
                valid when (AllowUpdate)
                action is MyScheduleAdvanced
            button of "CopyScheduleFromTemplate"
                valid when (AllowUpdate)
                action is ScheduleCopyFromTemplate
        header2 of "ActionFailedToRunOnTime"
        paragraph
            valid when (not DisableMisfire)
                MisfireStrategy
                    label is "Strategy"
            MisfireReportMode
                label is "ReportOptions"
            MisfireThreshold
                label is "ThresholdMinutes"
            ContinueOnMisfire
