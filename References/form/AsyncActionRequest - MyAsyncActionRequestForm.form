MyAsyncActionRequestForm is a Form
    title is "<Name>Details"
    restrict action DisableAllDBSessionDebug
    restrict action PurgeOrphanedRequests
    restrict action BuildScheduleForecast
    Layout
        visible when (not InvokedFromExists)
            two column
                Name
                    label is "ScheduledActionName"
        visible when (InvokedFromExists)
            two column
                Name
                    label is "ScheduledActionName"
                button of "InvokedFromRequest<InvokedFrom.Name>"
                    link is ViewInvokedFrom
                    display as link
        visible when (DisplayBackgroundName)
            two column
                DerBackgroundGroupName
                    label is "BackgroundGroup"
        visible when (WaitForExists)
            two column
                StatusMessage
                    label is "Status"
                visible when (WaitForSingletonActionGroupExists)
                    button of "DependentOnActionGroup<WaitForName>"
                        link is ViewDependsOnActionGroup
                        display as link
                visible when (WaitForSingletonRequestExists)
                    button of "DependentOnRequest<WaitForName>"
                        link is ViewDependsOnRequest
                        display as link
                visible when (WaitForSingletonBackgroundGroupExists)
                    button of "DependentOnBackgroundGroup<WaitForName>"
                        link is ViewDependsOnBackgroundGroup
                        display as link
                visible when (WaitForMultipleActionGroupsExists)
                    button of "DependentOnActionGroup<WaitForName>"
                        link is ViewDependsOnActionGroupList
                        display as link
                visible when (WaitForMultipleBackgroundGroupsExists)
                    button of "DependentOnBackgroundGroup<WaitForName>"
                        link is ViewDependsOnBackgroundGroupList
                        display as link
        visible when (ActionGroupExists)
            two column
                ActionGroupStatus
                button of "ViewActionGroupMembers"
                    link is ViewActionGroupMembers
                    display as link
        two column
            Actor
                label is "Actor"
                display only
            IsProxy
                display only
        two column
            AuthenticatedActor
                display only
            DBSessionDebugIsEnabled
                label is "LogDBSessionDebug"
                display only
        two column
            column1
                EmailAddressMultiple
                    use text area of 1 lines
                visible when (EmailAddress entered)
                    EmailAddress
                        raise yellow alert when (true)
                            mouse over text is "ThisFieldHasBeenDeprecated.PleaseUseTheOtherEmailAddressField"
            column2
                NotificationType
        two column
            ImplementingClass
                display only
            AsyncAction
                display only
        paragraph
            visible when (ActionType.ActionDataType.DataView)
                valid when (InCorrectDataArea)
                    ActionParameters
                        label is "Parameters"
            visible when (ActionType.ActionDataType.PlainText)
                ActionData
                    label is "Data"
                    use text area
        visible when (HasRunOnErrorAction)
            header2 of "ErrorProcessing"
            two column
                RunOnErrorAction.ImplementingClass
                    display only
                RunOnErrorAction.AsyncAction
                    display only
            paragraph
                visible when (RunOnErrorAction.ActionType.ActionDataType.DataView)
                    valid when (InCorrectDataArea)
                        RunOnErrorAction.ActionParameters
                            label is "Parameters"
                visible when (RunOnErrorAction.ActionType.ActionDataType.PlainText)
                    RunOnErrorAction.ActionData
                        label is "Data"
                        use text area
        header2 of "ExecutionDurationNotification"
        two column
            column1
                visible when (ActionGroupNotification.EmailAddress entered)
                    ActionGroupNotification.EmailAddress
                        raise yellow alert when (true)
                            mouse over text is "ThisFieldHasBeenDeprecated.PleaseUseTheOtherEmailAddressField"
                ActionGroupNotification.EmailAddressMultiple
                    use text area of 1 lines
            column2
                ActionGroupNotification.ThresholdMinutes
        header2 of "SchedulingDetails"
        paragraph
            CreatedStamp
                label is "Created"
                display only
            PendingScheduling
            IsScheduled
                display only
            valid when (not DisableConcurrency)
                ScheduleConcurrency
                    label is "RunActionConcurrently"
        paragraph
            TimeToExec
                label is "NextTimeToRun"
            LastTimeToExec
                label is "LatestTimeToRun"
            visible when (IsScheduled)
                NextScheduledExecution1
                    no label
                NextScheduledExecution2
                    no label
                NextScheduledExecution3
                    no label
            text of untranslatable:"<ContextTimeZoneDisplayName>"
        visible when (DisplayExclusionsUpdate)
            paragraph
                ScheduleExclusion.ScheduleExclusion
                    select is ScheduleExclusionsList
                ScheduleExclusion.ScheduleExclusion.Description
                    no label
                text of "or"
                ScheduleExclusion.ScheduleExclusionSet
                ScheduleExclusion.ScheduleExclusionSet.Description
                    no label
        visible when (IsScheduled)
            visible when (not IsScheduledByFrequency)
                paragraph
                    visible when (IsScheduleYearSpecified)
                        visible when (DisplayScheduleYearType)
                            ScheduleYearType
                                label is "Years"
                                display only
                        visible when (DisplayScheduleYear)
                            visible when (not ScheduleYearType.Range)
                                AsyncScheduleYear.ScheduleYear[1]
                                    no label
                                    display only
                            visible when (ScheduleYearType.Range)
                                AsyncScheduleYear.ScheduleYear[1]
                                    label is "Range"
                                    display only
                                text of "-"
                            visible when (DisplayYearElement2)
                                visible when (AsyncScheduleYear.ScheduleYear[2] entered)
                                    AsyncScheduleYear.ScheduleYear[2]
                                        no label
                                        display only
                            visible when (ScheduleYearType.Multiple)
                                visible when (AsyncScheduleYear.ScheduleYear[3] entered)
                                    AsyncScheduleYear.ScheduleYear[3]
                                        no label
                                        display only
                                visible when (AsyncScheduleYear.ScheduleYear[4] entered)
                                    AsyncScheduleYear.ScheduleYear[4]
                                        no label
                                        display only
                                visible when (AsyncScheduleYear.ScheduleYear[5] entered)
                                    AsyncScheduleYear.ScheduleYear[5]
                                        no label
                                        display only
                paragraph
                    visible when (not ScheduleMonth.EveryMonth)
                        ScheduleMonth
                            label is "Month"
                            display only
                        visible when (DisplayScheduleMonth)
