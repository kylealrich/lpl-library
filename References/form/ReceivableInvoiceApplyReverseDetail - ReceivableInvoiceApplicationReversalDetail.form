ReceivableInvoiceApplicationReversalDetail is a Form
    is primary
    title is "ReceivableInvoiceApplicationReversalDetail"
    Layout
        three column
            column1
                CreditCompany
                    label is "Company"
                    display as text
                ReceivableInvoiceApplyReverseHeaderRel.ReceivableInvoiceDetailRel.Customer
                    label is "Customer"
                    select is GroupSearchName
                CreditInvoice
                    label is "Invoice"
                    display as text
                CreditTransactionNumber
                    label is "TransactionNumber"
                    display as text
                CreditBatchNumber
                    label is "Batch"
                    display as text
                ReceivableInvoiceApplyReverseHeaderRel.GeneralLedgerDate
                    label is "ReversalPostDate"
                ReceivableInvoiceApplyReverseHeaderRel.ReceivableOperator
                    label is "Operator"
                ReceivableInvoiceDetailRel.InvoiceAmount.CurrencyAmount
                    label is "Amount"
                    display as text
            column2
                CreditCompany.Name
                    no label
                    display as text
                ReceivableInvoiceApplyReverseHeaderRel.ReceivableInvoiceDetailRel.Customer.Name
                    no label
                    display as text
                CreditTransactionType
                    label is "Type"
                    display as text
                ReceivableInvoiceDetailRel.ReceivableProcessLevel
                    label is "ProcessLevel"
                    display as text
                ReceivableInvoiceDetailRel.TransactionDate
                    no label
                    display as text
                ReceivableInvoiceDetailRel.AdjustmentAmount.FunctionalAmount.EnteredCurrencyAmount
                    label is "UnappliedAmount"
                    display as text
                blank line
            column3
                CreditCompany.Currency
                    no label
                    display as text
                blank line
                ReceivableInvoiceDetailRel.OriginalCurrency
                    label is "OriginalCurrency"
                ReceivableInvoiceDetailRel.ReceivableInvoiceDetail
                    label is "PaymentSequence"
                ReceivableInvoiceDetailRel.DueDate
                    label is "DueDate"
                    display as text
                blank line
