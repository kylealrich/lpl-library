SummaryChartAccountComposite is a CompositeForm
    is primary
    title is "<SummaryChartAccountFormTitle>"
    context form is SummaryChartAccountForm
        background is theme color
    AdditionalAccountSettings is a Panel
        title is "Main"
        form is AdditionalSettingsForm
    CurrencyAccountSetting is a Panel
        title is "Currency<FinanceEnterpriseGroup.AccountLabel>"
        visible when (CurrencyDefaultSetup)
        form is CurrencyAccountForm
    UserFieldsPanel is a Panel
        title is "UserFields"
        form is UserFieldsForm
