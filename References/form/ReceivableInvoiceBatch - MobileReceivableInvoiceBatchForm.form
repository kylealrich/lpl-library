MobileReceivableInvoiceBatchForm is a CompositeForm
    title is "InvoiceBatch"
    context form is MobileReceivableInvoiceBatchFormContext
    show panel control on left
    Main is a Panel
        form is MobileMain
    CurrencyRates is a Panel
        form is CurrencyRates
    Transactions is a Panel
        visible when (IsReleased)
        list is ReceivableInvoiceRel.MobileListReceivableInvoiceReleased
    Transaction is a Panel
        title is "Transactions"
        visible when (IsUnreleased)
        list is ReceivableInvoiceRel.MobileListReceivableInvoice
    SpeedEntry is a Panel
        visible when (IsUnreleased)
        list is ReceivableInvoiceRel.MobileSpeedEntryTransactionsList
    Attachments is a Panel
        list is ReceivableInvoiceBatchAttachment set.MobileAttachments
