MMatchInvoiceDashboardForm is a CompositeForm
    title is "MatchInvoiceDetails"
    context form is MMatchInvoiceDashboardContext
    implements DashBoardView
    restrict action Released.ApplyACreditToThisInvoice
    restrict action Released.ApplyAnInvoiceToThisCredit
    restrict action CreateMatchReferenceInvoice
    restrict action Unreleased.CreateInvoice
    restrict action Unreleased.CreateAndRelease
    restrict action RequestNewInvoicePayment
    restrict action Unreleased.Delete
    restrict action CreateCompanyCustomer
    restrict action PeriodClosePreprocessorToUpdatePostDate
    Actions
        action is save
            label is "_"
            hide when invalid
        action is refresh
            label is "_"
    PurchaseOrders is a Panel
        list is MatchPurchaseOrderInvoicesRel.MatchAnalysisPurchaseOrderList
    InvoiceDetails is a Panel
        list is PayablesInvoiceDetailRel.MatchAnalysisList
    MatchedReceiptLines is a Panel
        title is "ReceiptLines"
        visible when (UseReceiptLinesForAnalysis)
        list is PurchaseOrderReceiptLineRel.MatchAnalysisList
    MatchedReceiptInvoiceLinkLines is a Panel
        title is "ReceiptLines"
        visible when (UseReceiptLinkForAnalysis)
        list is PurchaseOrderReceiptInvoiceLinkRel.MatchAnalysisList
    ReceiptLines is a Panel
        title is "ReceiptLines"
        visible when (not MatchObjectIdEntered)
        list is PurchaseOrderReceiptLinesForInvoiceRel.MatchAnalysisList
    InvoiceAddOnCharges is a Panel
        list is PayablesInvoiceAddOnChargeRel.MMatchAnalysisList
    MatchedReceiptLineAddOnCharges is a Panel
        title is "ReceiptLineAddOnCharges"
        visible when (MatchObjectIdEntered)
        list is PurchaseOrderReceiptAddOnChargesByMatchIDRel.MatchAnalysisList
    ReceiptLineAddOnCharges is a Panel
        title is "ReceiptLineAddOnCharges"
        visible when (not MatchObjectIdEntered)
        list is PurchaseOrderReceiptAddOnChargesForInvoiceRel.MatchAnalysisList
    Commitments is a Panel
        valid when (GLCommitRelExists)
        list is GLCommitRel.DrillGLCommitList
    Chargebacks is a Panel
        list is PayablesInvoiceChargebackRel.MChargebackMatchAnalysisList
