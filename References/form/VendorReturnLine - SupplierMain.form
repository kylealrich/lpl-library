SupplierMain is a Form
    disable field options
    Layout
        display as text
            two column distributed
                column1
                    ReturnQuantity
                        label is "ReturnQuantity"
                    EnteredUOM
                        label is "UOM"
                    paragraph
                        visible when (not Item.CatchWeightCode.NotCatchWeight)
                            CatchWeightQuantity
                                label is "CatchweightStockQuantity"
                        ItemStockUOM
                            no label
                    visible when (not VendorReturn.ShippedStatus)
                        UnitCost
                    visible when (VendorReturn.ShippedStatus)
                        DerivedDisplayUnitCost
                    paragraph
                        VendorItem
                            label is "SupplierItemNumber"
                        VendorItemDescription
                            no label
                column2
                    ItemGTIN
                        label is "G\\T\\I\\N"
                    paragraph
                        BuyUOMQuantity
                        BuyUOM
                            no label
                    paragraph
                        Contract
                        ContractLine
                            label is "Line"
