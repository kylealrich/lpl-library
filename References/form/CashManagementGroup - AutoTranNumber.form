AutoTranNumber is a Form
    Layout
        header2 of "AutoTransactionNumberBy_(SelectOnlyOne)"
        single column
            AutoTranNumberByCompany
                label is "CompanyLevel"
            AutoTranNumberByCashCode
                label is "CashCodeLevel"
            AutoTranNumberByBankTransactionCode
                label is "BankTransactionCodeLevel"
            AutoTranNumberByCashCodeBankTranCode
                label is "CashCodeAndBankTransactionCodeLevel"
        header2 of "FormsToApplyAutoNumberingTo_(SelectOneOrMore)"
        single column
            AutoTranNumberForTransactionForm
                label is "CashLedgerTransaction"
            AutoTranNumberForReceiptForm
                label is "CashReceipts"
            AutoTranNumberForReturnedPaymentForm
                label is "CashLedgerReturnedPayment"
            AutoTranNumberForTransferForm
                label is "CashLedgerBankFundTransfer"
