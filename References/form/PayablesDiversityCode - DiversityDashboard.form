DiversityDashboard is a CompositeForm
    title is "DiversityCodeReporting"
    context form is PayablesDiversityCodeForm
    implements DashBoardView
    Vendors is a Panel
        list is VendorDiversitysRel.VendorsForADiversityCode
    Suppliers is a Panel
        visible when (SupplierDiversityExists)
        list is SupplierDiversitysRel.SuppliersForADiversityCode
    Subcontractors is a Panel
        visible when (SubcontractorDiversitiesExist)
        list is SubcontractorDiversitysRel.SubcontractorsForADiversityCode
    Invoices is a Panel
        list is PayablesInvoiceRel.InvoicesForADiversityCode
    Contracts is a Panel
        list is ContractDiversityRel.ContractsForADiversityCode
    ContractSubcontractors is a Panel
        visible when (ContractSubcontractorDiversitiesExist)
        list is ContractSubcontractorDiversitiesRel.ContractSubcontractorDiversityForADiversityCode
    PurchaseOrders is a Panel
        list is PurchaseOrderDiversityRel.PurchaseOrdersForADiversityCode
