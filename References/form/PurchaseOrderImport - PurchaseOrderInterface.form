PurchaseOrderInterface is a CompositeForm
    is primary
    title is "PurchaseOrderInterface<PurchaseOrderImport>"
    context form is PurchaseOrderInterfaceContext
        background is theme color
    Header is a Panel
        visible when (AddOrUpdateType)
        form is PurchaseOrderHeader
    Addresses is a Panel
        visible when (AddOrUpdateType)
        form is HeaderAddresses
    LineDefaults is a Panel
        visible when (AddOrUpdateType)
        form is LineDefaults
    Retainage is a Panel
        visible when (AddOrUpdateType)
        form is HeaderRetainage
    Comments is a Panel
        visible when (AddOrUpdateType)
        list is PurchaseOrderImportCommentsRel.HeaderComments
    Lines is a Panel
        visible when (not CancelType)
        list is PurchaseOrderImportLinesRel.PurchaseOrderImportLineList
    AddOnCharges is a Panel
        visible when (not CancelType)
        list is PurchaseOrderImportAOCsRel.PurchaseOrderImportAddOnChargeList
