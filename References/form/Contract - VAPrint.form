VAPrint is a SummaryForm
    title is "AdditionalContractInformation"
    Layout
        paragraph
            Vendor
            Vendor.VendorName
                no label
        visible when (not HasBeenActivated)
            single column
                paragraph
                    ApPoPurchaseFrom.PurchaseFromLocation
                        label is "POVendorLocation"
                    visible when (HasPoPurchFrom)
                        ApPoPurchFromName
                            no label
                paragraph
                    ApPoPurchaseFrom.VendorLocation
                        label is "APVendorLocation"
                    visible when (HasApPurchFrom)
                        ApPoPurchFromName
                            no label
        visible when (HasBeenActivated)
            single column
                paragraph
                    ApPoPurchaseFrom.PurchaseFromLocation
                        label is "POVendorLocation"
                    visible when (HasPoPurchFrom)
                        ApPoPurchFromName
                            no label
                paragraph
                    ApPoPurchaseFrom.VendorLocation
                        label is "APVendorLocation"
                    visible when (HasApPurchFrom)
                        ApPoPurchFromName
                            no label
        visible when (OnHold)
            paragraph
                ProcurementReasonCode
                ProcurementReasonCode.Description
                    no label
        visible when (not StandingPurchaseType)
            single column
                PerOrderMinimumAmount
                PerOrderMaximumAmount
        visible when (ContractOrBlanketOrService)
            single column
                NotificationPercent
        visible when (LifetimeEdit.PurchaseOrdersAndReqs)
            single column
                AmountRequested
                visible when (MaximumAmountEntered)
                    PercentRequested
        visible when (MaximumAmountEntered)
            single column
                PercentOrdered
                PercentRemaining
        single column
            visible when (ServicePurchaseType)
                AmountOrdered
                    label is "AmountInvoiced"
            visible when (not ServicePurchaseType)
                AmountOrdered
        single column
            visible when (MaximumAmountEntered)
                AmountRemaining
        single column
            UOMSMustMatch
            UOMSMustMatchForSpecials
        single column
            UseContractUOM
        single column
            LifetimeEdit
                label is "LifetimeMaximumEdit"
            AllowReferenceOfItemsNotOnContract
                label is "AllowItemsNotOnContract"
            visible when (IsNotManDistOrStanding)
                LineLevelBreaks
        paragraph
            TermsCode
                label is "PaymentTerms"
            TermsCode.Description
                no label
        paragraph
            CommodityCode
                compact format
            CommodityCode.Description
                no label
        single column
            visible when (NonServicePurchaseType)
                ManufacturerCodeDivision
                    compact format
        visible when (not UseCommitments)
            paragraph
                DefaultAccount
                    compact format
            two column
                PayablesDistributionCode
                    label is "DistributionCode"
        single column
            visible when (BlanketOrStanding)
                BlanketStandingRequestingLocation
        single column
            Priority
        paragraph
            DefaultTaxable
            TaxCode
        paragraph
            GroupPurchasingOrganization.Vendor
                label is "GPOVendor"
            GroupPurchasingOrganization.Vendor.VendorName
                no label
        paragraph
            Stockless
        paragraph
            Buyer
            Buyer.Employee.PresentationNameSnapshot
                no label
        paragraph
            LastPORelease
            POCode
        visible when (StandingPurchaseType)
            two column
                ScheduleFrequency
                MaximumReleaseNumber
            single column
                DoNotIssue
                LastReleaseDate
                    label is "LastPODeliveryDate"
        visible when (not ServicePurchaseType)
            paragraph
                FreightTerm
                    label is "FreightTerms"
                FreightTerm.Description
                    no label
            two column
                ShipTerm
                    label is "ShipTerms"
                ShipTerm.Description
                    no label
        visible when (BlanketOrStandingOrServices)
            paragraph
                ProcessLevelGroup
        paragraph
            CreateCommitments
        single column
            CommitmentCompany
        paragraph
            CommitmentAccount
                compact format
        paragraph
            CommitmentsClosed
        paragraph
            PoUserField1
                label is "UserField1"
            PoUserField3
                label is "UserField2"
        single column
            PoUserField5
                label is "UserField3"
