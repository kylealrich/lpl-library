AwardLineNoOutput is an ActionForm
    action is Submitted.AwardLineNoOutput
    title is "Award"
    Layout
        display as text
            four column distributed
                Company
                SourcingEvent.RepresentativeText
                NotifiedSupplier.Supplier.RepresentativeText
                NotifiedSupplier.SupplierSourceId.RepresentativeText
            four column distributed
                SourcingEventLine.OutputType
                visible when (not SourcingEventLine.OutputType.Service)
                    SourcingEventLine.ItemNumber
                        label is "Item"
                SourcingEventLine.ItemDescription
                    label is "Description"
                visible when (SourcingEventLine.HasSecondDescription)
                    SourcingEventLine.ItemDescription2
                        no label
            two column distributed
                SourcingEventLine
                    label is "Line#"
                visible when (SourcingEventLine.HasThirdDescription)
                    SourcingEventLine.ItemDescription3
                        label is "LongItemDescription"
            three column distributed
                visible when (EventLineAwarded)
                    LineQuantityToAward
                        label is "RemainingQuantityToAward"
                visible when (not EventLineAwarded)
                    LineQuantityToAward
                        label is "QuantityToAward"
                visible when (SourcingEventLine.GTINEntered)
                    paragraph
                        SourcingEventLine.GlobalTradeItemNumber
                            label is "GTIN"
                        SourcingEventLine.GlobalTradeItemNumber.FullDescription
                            no label
        header3 of "EnterAwardInformation"
        three column distributed
            AwardReason
                select is BuyerAwardReasonsSelect
            visible when (DisplayNegotiatedTerms)
                TermsHaveBeenNegotiated
            button of "ViewDescriptionAndComments"
                link is ViewComments
                display as link
        eight column
            visible when (AlternatesExist)
                column1
                    visible when (NonNoBidResponseExists)
                        text of "_"
                        text of "MainResponse"
                            bold
                    visible when (not NonNoBidResponseExists)
                        text of "OnlyAlternateResponsesExist"
                            bold
                    text of "AlternateResponses"
                        bold
            column2
                text of "VendorItem"
                    bold
                visible when (ResponseExists)
                    VendorItem
                        no label
                visible when (not ResponseExists)
                    blank line
                visible when (Alt1Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].VendorItem
                        no label
                        display as text
                visible when (Alt2Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].VendorItem
                        no label
                        display as text
                visible when (Alt3Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[3].VendorItem
                        no label
                        display as text
                visible when (Alt4Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[4].VendorItem
                        no label
                        display as text
                visible when (Alt5Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[5].VendorItem
                        no label
                        display as text
            column3
                text of "ResponseQuantity"
                    bold
                visible when (ResponseExists)
                    Quantity
                        no label
                        display as text
                visible when (not ResponseExists)
                    blank line
                visible when (Alt1Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].Quantity
                        no label
                        display as text
                visible when (Alt2Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].Quantity
                        no label
                        display as text
                visible when (Alt3Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[3].Quantity
                        no label
                        display as text
                visible when (Alt4Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[4].Quantity
                        no label
                        display as text
                visible when (Alt5Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[5].Quantity
                        no label
                        display as text
            column4
                text of "AwardQuantity"
                    bold
                visible when (ResponseExists)
                    AwardQuantity
                        no label
                visible when (not ResponseExists)
                    blank line
                visible when (Alt1Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].AwardQuantity
                        no label
                visible when (Alt2Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].AwardQuantity
                        no label
                visible when (Alt3Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[3].AwardQuantity
                        no label
                visible when (Alt4Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[4].AwardQuantity
                        no label
                visible when (Alt5Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[5].AwardQuantity
                        no label
            column5
                text of "UOM"
                    bold
                visible when (ResponseExists)
                    SourcingEventLine.UOM.UnitOfMeasure
                        no label
                        display as text
                visible when (not ResponseExists)
                    blank line
                visible when (Alt1Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].UOM.UnitOfMeasure
                        no label
                        display as text
                visible when (Alt2Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].UOM.UnitOfMeasure
                        no label
                        display as text
                visible when (Alt3Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[3].UOM.UnitOfMeasure
                        no label
                        display as text
                visible when (Alt4Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[4].UOM.UnitOfMeasure
                        no label
                        display as text
                visible when (Alt5Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[5].UOM.UnitOfMeasure
                        no label
                        display as text
            column6
                text of "UnitAmount"
                    bold
                visible when (ResponseExists)
                    ResponseAmountDisplay
                        no label
                        display as text
                visible when (not ResponseExists)
                    blank line
                visible when (Alt1Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].ResponseAmountDisplay
                        no label
                        display as text
                visible when (Alt2Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].ResponseAmountDisplay
                        no label
                        display as text
                visible when (Alt3Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[3].ResponseAmountDisplay
                        no label
                        display as text
                visible when (Alt4Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[4].ResponseAmountDisplay
                        no label
                        display as text
                visible when (Alt5Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[5].ResponseAmountDisplay
                        no label
                        display as text
            column7
                text of "DeliveryDate"
                    bold
                visible when (ResponseExists)
                    DeliveryDate
                        no label
                visible when (not ResponseExists)
                    blank line
                visible when (Alt1Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[1].DeliveryDate
                        no label
                        display as text
                visible when (Alt2Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[2].DeliveryDate
                        no label
                        display as text
                visible when (Alt3Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[3].DeliveryDate
                        no label
                        display as text
                visible when (Alt4Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[4].DeliveryDate
                        no label
                        display as text
                visible when (Alt5Exists)
                    AlternateResponseArray.AlternateResponseOccurs.AlternateResponseDetails[5].DeliveryDate
                        no label
                        display as text
        two column distributed
            visible when (BuyerDisplayGTIN)
                paragraph
                    GlobalTradeItemNumber
                        label is "GTIN"
                    GlobalTradeItemNumber.FullDescription
                        no label
                        display as text
            visible when (AllowEntryOfManufacturerInfoOnAward)
                paragraph
                    ManufacturerInformation.Manufacturer
                        label is "ManufacturerCode"
                        compact format
                    ManufacturerInformation.ManufacturerNumber
                        label is "ManufacturerNumber"
            visible when (not AllowEntryOfManufacturerInfoOnAward)
                paragraph
                    DerivedManufacturerCode
                        label is "ManufacturerCode"
                        display as text
                    DerivedManufacturerDivision
                        label is "Division"
                        display as text
                    DerivedManufacturerNumber
                        label is "ManufacturerNumber"
                        display as text
