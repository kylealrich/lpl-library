Main is a Form
    Layout
        visible when (not RecordExists)
            two column distributed
                column1
                    paragraph
                        Company
                        Company.Name
                            no label
                            display as text
                    InventoryTransactionType
                        label is "DocumentType"
                column2
                    paragraph
                        InventoryLocation
                            label is "Location"
                        InventoryLocation.Name
                            no label
                            display as text
                    paragraph
                        Company.GeneralLedgerCompany.Currency
                            label is "Currency"
                            display as text
        visible when (RecordExists)
            single column
                paragraph
                    visible when (not DisplayDocumentType)
                        InventoryTransactionType
                            label is "DocumentType"
        two column distributed
            column1
                visible when (not IsUnreleased)
                    GeneralLedgerDate
                        label is "PostDate"
                        display only
                visible when (IsUnreleased)
                    GeneralLedgerDate
                        label is "PostDate"
            column2
                visible when (InventoryTransactionType.InventoryReceipt)
                    TransactionDate
                        label is "ReceivedDate"
                visible when (InventoryTransactionType.BinTransfer)
                    TransactionDate
                        label is "TransferDate"
                visible when (InventoryTransactionType.InventoryTransfer)
                    TransactionDate
                        label is "TransferDate"
                visible when (InventoryTransactionType.InventoryIssue)
                    TransactionDate
                        label is "IssueDateAndTime"
                visible when (InventoryTransactionType.Adjustment)
                    TransactionDate
                        label is "AdjustmentDate"
                visible when (IsInventoryReturns)
                    TransactionDate
                        label is "ReturnDateAndTime"
        two column distributed
            column1
                visible when (InventoryTransactionType.InventoryTransfer)
                    paragraph
                        FromToCompanyLocation.FromToCompany
                            label is "ToCompany"
                            display as required
                        FromToCompanyLocation.FromToCompany.Name
                            no label
                            display as text
                visible when (InventoryDocumentType.DirectTransfer)
                    paragraph
                        FromToCompanyLocation.FromToCompany
                            label is "ToCompany"
                        FromToCompanyLocation.FromToCompany.Name
                            no label
                            display as text
                visible when (InventoryTransactionType.InventoryIssue)
                    paragraph
                        FromToCompanyLocation.FromToCompany
                            label is "ToCompany"
                            display as required
                        FromToCompanyLocation.FromToCompany.Name
                            no label
                            display as text
                visible when (IsIntransitReceivingOrReceivingTransfer)
                    paragraph
                        FromToCompanyLocation.FromToCompany
                            label is "FromCompany"
                        FromToCompanyLocation.FromToCompany.Name
                            no label
                            display as text
                visible when (IsInventoryReturns)
                    paragraph
                        FromToCompanyLocation.FromToCompany
                            label is "FromCompany"
                        FromToCompanyLocation.FromToCompany.Name
                            no label
                            display as text
            column2
                visible when (InventoryTransactionType.InventoryTransfer)
                    paragraph
                        FromToCompanyLocation.FromToLocation
                            label is "ToLocation"
                            display as required
                        FromToCompanyLocation.FromToLocation.Name
                            no label
                            display as text
                visible when (InventoryDocumentType.DirectTransfer)
                    paragraph
                        FromToCompanyLocation.FromToLocation
                            label is "ToLocation"
                        FromToCompanyLocation.FromToLocation.Name
                            no label
                            display as text
                visible when (InventoryTransactionType.InventoryIssue)
                    paragraph
                        FromToCompanyLocation.RequestingLocation
                            label is "RequestingLocation"
                            display as required
                        FromToCompanyLocation.RequestingLocation.Name
                            no label
                            display as text
                visible when (IsIntransitReceivingOrReceivingTransfer)
                    paragraph
                        FromToCompanyLocation.FromToLocation
                            label is "FromLocation"
                        FromToCompanyLocation.FromToLocation.Name
                            no label
                            display as text
                visible when (IsInventoryReturns)
                    paragraph
                        FromToCompanyLocation.RequestingLocation
                            label is "RequestingLocation"
                        FromToCompanyLocation.RequestingLocation.Name
                            no label
                            display as text
        two column distributed
            visible when (IsInventoryIssuesOrReturns)
                DefaultDistributionAccount
                    compact format
            visible when (IsInventoryIntransitReceiving)
                WarehouseShipment
                    label is "IntransitShipment"
        two column distributed
            Reference1
            Reference2
        visible when (InventoryTransactionType.InventoryTransfer)
            two column distributed
                column1
                    EstimatedDeliveryDate
                    NumberOfCartons
                        label is "Cartons"
                column2
                    IntransitTransfer
        visible when (IsInventoryIntransitReceiving)
            two column distributed
                column1
                    paragraph
                        IntransitReceivingStatus
                            label is "ReceivingStatus"
                            display as text
                        button of "Receiving"
                            visible when (IsUnreleasedOrReleaseInProgress)
                            action is IntransitReceiving
                    NumberOfCartons
                        label is "Cartons"
                column2
                    Reprint
        visible when (IsInventoryReceivingTransfer)
            single column
                NumberOfCartons
                    label is "Cartons"
        visible when (false)
            TransientOnline
