                                no label
        visible when (ContractOutput)
            text of "Contract_Created"
                header3
            two column
                column1
                    display as text
                        DisplayQuantity
                            label is "Requested"
                        visible when (SourcingEventExists)
                            QuantityOnEvent
                        DisplayInterfaceCancelQuantity
                            label is "Canceled"
                        text of "Contract<first RelatedContractRel.Contract>Created"
                column2
                    display as text
                        DisplayEnteredUOM
                            no label
