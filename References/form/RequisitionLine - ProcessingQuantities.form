                            no label
                            right align
                        DeliveredQuantity
                            no label
                            right align
                column3
                    display as text
                        text of "UOM"
                        DisplayEnteredUOM
                            no label
                        DisplayFromPurchaseOrderInterfaceEnteredUOM
