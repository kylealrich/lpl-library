        visible when (WarehouseOnly)
            text of "Warehouse_Order_Quantities"
                header3
            two column table
                column1
                    text of "_"
                    text of "Requested"
                    text of "Demand_Order"
                    text of "Allocated"
                    text of "Backordered"
                    text of "Killed"
                    text of "In_Process"
                    text of "Shipped"
                    text of "Voided"
                    text of "Return"
                column2
