RequestNewItemLocation is an ActionForm
    action is RequestNewItemLocation
    title is "RequestNewItemLocation"
    Layout
        visible when (actor.initiator)
            text of "This_request_will_be_routed_for_approval;_after_it_is_approved_this_record_will_be_created."
                header4
                italics
                color of blue
            text of "_"
        visible when (not actor.initiator)
            three column
                RequestedBy
                    display only
                RequesterActor
                    label is "ID"
                    display only
                RequesterPhone
                    label is "PhoneNumber"
                    display only
                    compact format
            paragraph
                RequesterEmail
                    label is "Email"
                    display only
                button of "SendEmail"
                    link is "mailto:<RequesterEmail>"
            blank line
        two column
            RequestPriority
            NeededBy
        header2 of "ActionComment"
        single column
            action comment
                no label
                use text area
        header2 of "Main"
        paragraph
            visible when (false)
                Company
            InventoryCompany
            InventoryCompany.Name
                no label
                display as text
            InventoryCompany.GeneralLedgerCompany.Currency
                label is "Currency"
                display as text
        paragraph
            InventoryLocation
                label is "Location"
                show up to 15 characters
            InventoryLocation.Name
                no label
                display as text
        paragraph
            Item
                show up to 15 characters
            Item.Description
                no label
                display as text
            Item.Description2
                no label
                display as text
        visible when (not actor.initiator)
            two column
                Active
                Item.StockUOM
                    display as text
            two column
                paragraph
                    GeneralLedgerCategory
                        label is "InventoryLocationAccountGroup"
                    GeneralLedgerCategory.Description
                        no label
                        display as text
                AddedDate
                    label is "DateAdded"
            two column
                DefaultTransactionUOM.UnitOfMeasure
                    label is "DefaultTransactionUOM"
                    select is ValidTransactionUnitsOfMeasure
                DefaultSourceUOM.UnitOfMeasure
                    label is "DefaultSourceUOM"
                    select is ValidBuyUnitsOfMeasure
            paragraph
                Buyer
                Buyer.Employee.PresentationNameSnapshot
                    no label
                    display as text
            paragraph
                Manufacturer
                    compact format
                ManufacturerNumber
            two column
                InventoryTracked
                AllowReceipt
                AllowIssue
                InspectionRequired
                NoCharge
                PatientChargeable
                Consignment
                SelectForSpecialCycleCount
            blank line
            single column
                FrozenForPhysicalInventory
                    display only
            three column
                MovementClass
                CountAccuracy
                ForecastNumber
            header3 of "DetailTrackingInformation"
            two column
                SerialTracked
                LotTracked
            paragraph
                BinTracked
                PreferredBin
                StorageCode
            header3 of "TaxInformation"
            two column
                PurchaseTaxable
                PurchaseTaxCode
                TaxableWhenSold
                SellTaxCode
            header3 of "MaterialSafetyDataSheet"
            paragraph
                MSDSDate
                    label is "Date"
                MSDSDocument
                    label is "Document"
                MSDSVersion
                    label is "Version"
            header3 of "Reorder"
            two column
                ReorderPolicy
                ReorderDocument
            two column
                ReorderPointCode
                ReorderPoint
            two column
                AutomaticPurchaseOrder
                ReceivingDays
            blank line
            single column
                ReorderQuantityCode
                ReorderQuantity
                OrderMultiple
            two column
                MaximumOrderQuantity
                MinimumOrderQuantity
                SafetyStock
                EconomicOrderQuantity
            blank line
            header3 of "Leadtime"
            two column
                LeadtimeControl
                    label is "Control"
                LeadtimeDays
                    label is "AverageDays"
                LastLeadtime
                    display as text
                PreviousLeadtime
                    display as text
            header3 of "Cost"
            single column
                StandardCost
                FutureStandardCost
                blank line
                MarketPrice
                OverheadUnitCost
                    label is "OverheadCost"
                blank line
                AverageCost
                    display as text
                AverageAddOnCost
                    display as text
                blank line
            paragraph
                LastReceiptCost
                    display as text
                LastReceiptDate
                    label is "Date"
                    display as text
            paragraph
                LastIssueCost
                    display as text
                LastIssueDate
                    label is "Date"
                    display as text
            header3 of "Sales"
            single column
                CreatePurchaseOrderForCustomerOrderLine
                DropshipPurchaseOrderForCustomerOrderLine
                blank line
            paragraph
                MinimumSaleQuantity
                    label is "MinimumQuantity"
                MaximumSaleQuantity
                    label is "MaximumQuantity"
            blank line
            two column
                AllowCustomerOrderReturns
                CustomerOrderReturnDate
                    label is "ReturnDate"
