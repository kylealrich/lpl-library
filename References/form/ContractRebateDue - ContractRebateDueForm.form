ContractRebateDueForm is a SummaryForm
    Layout
        three column
            column1
                ContractRebateDue
                    label is "Due"
                Status
                    label is "ContractRebateDueStatus"
            column2
                blank line
                blank line
                blank line
            column3
                ExpectedAmount
                    label is "TotalRebatesExpected"
                    display when blank
                TotalRebatesReceived
                    label is "LessReceived"
                    display when blank
                ComputeRemainingAmt
                    label is "Remaining"
                    display when blank
        single column
            Contract
                label is "ManufacturerContract"
                display when blank
            paragraph
                Contract.ManufacturerCodeDivision.ManufacturerCode
                    label is "Manufacturer"
                    display when blank
                Contract.ManufacturerCodeDivision.ManufacturerDivision
                    no label
                    display when blank
        list is PayablesInvoiceDetailRel.InvoiceDetailForContractRebateActivity
        two column
            column1
                blank line
                blank line
                blank line
            column2
                DerivedManufacturerExtendedAmount
                    label is "TotalForManufacturerContract"
