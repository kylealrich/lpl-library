CashForecastPublishDateContextForm is a Form
    is primary
    title is "CashForecastPublishDate"
    Layout
        display as text
            paragraph
                CashForecast
                CashForecast.Description
                    no label
                    display as text
            single column
                Description
                PublishType
            paragraph
                PublishDateRange.Begin
                    label is "PublishedDates"
                text of "-"
                PublishDateRange.End
                    no label
