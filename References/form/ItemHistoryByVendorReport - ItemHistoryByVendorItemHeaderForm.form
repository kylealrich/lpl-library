ItemHistoryByVendorItemHeaderForm is a SummaryForm
    title is "_"
    Layout
        three column distributed
            column1
                text of "Item:<ItemHistoryByVendorReport.Item>"
                    header4
                    center
                    bold
            column2
                text of "<DerivedItemDescription>"
                    header4
                    center
                    bold
            column2
                blank line
        form is RNISubreportHeaderForm
        form is ItemHistoryReportVendorRel.RNISubreportSumForm
        visible when (SupplyManagementReport.PrintGraph)
            list is ItemHistoryReportVendorRel.ItemHistoryByVendorChart
        form is ItemHistoryReportVendorRel.ItemHistoryByVendorList
        form is ItemHistoryByVendorTotalsForm
