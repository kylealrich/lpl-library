PaidLateInvoicesSearch is a SearchForm
    Layout
        four column distributed
            column1
                Company
            column2
                ReceivableInvoiceDetailRel.Customer
                    label is "Customer"
            column3
                CustomerGroupRel.CustomerGroup
                    label is "CustomerGroup"
            column4
                Company.GeneralLedgerCompany.Currency
                    label is "BaseCurrency"
        four column distributed
            column1
                DaysPaidLateRange.From
                    label is "FromDaysPaidLate"
            column2
                DaysPaidLateRange.To
                    label is "ToDaysPaidLate"
            column3
                TransactionDateRange.Begin
                    label is "FromTransactionDate"
            column4
                TransactionDateRange.End
                    label is "ToTransactionDate"
        four column distributed
            column1
                DueDateRange.Begin
                    label is "FromDueDate"
            column2
                DueDateRange.End
                    label is "ToDueDate"
            column3
                DepositDateRange.Begin
                    label is "FromPaymentDate"
            column4
                DepositDateRange.End
                    label is "ToPaymentDate"
