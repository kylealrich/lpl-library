PayablesInvoiceErrorForm is a Form
    is primary
    title is "InvoiceError"
    Layout
        two column
            Company.GeneralLedgerCompany.FinanceEnterpriseGroup
                display as text
            Company.FinanceEnterpriseGroup.EnterpriseGroup.Description
                no label
                display as text
        two column
            PayablesInvoice.Invoice
                display as text
        single column
            blank line
            ErrorLevel
                display as text
            ErrorClass
                label is "ErrorType"
                display as text
            ErrorMessage
                display as text
            visible when (ErrorType.InvalidCompany)
                PayablesInvoice.PayablesInvoiceExtension.InterfaceData.Company
                    display as text
            visible when (ErrorType.InvalidVendor)
                PayablesInvoice.PayablesInvoiceExtension.InterfaceData.Vendor
                    display as text
                PayablesInvoice.PayablesInvoiceExtension.InterfaceData.VendorName
                    display as text
            visible when (ErrorType.InvalidPurchaseOrder)
                PayablesInvoice.PayablesInvoiceExtension.InterfaceData.PoCode
                    display as text
                PayablesInvoice.PayablesInvoiceExtension.InterfaceData.PoNumber
                    display as text
                PayablesInvoice.PayablesInvoiceExtension.InterfaceData.PoRelease
                    display as text
