BudgetTemplateManual is a Form
    is primary
    title is "ManualElement"
    Layout
        visible when (IsCreated)
            paragraph
                Pending
                PendingRemove
        blank line
        visible when (BudgetTemplate.ManualAccountingUnit)
            paragraph
                AccountingEntity
                    label is FinanceEnterpriseGroup.AccountingEntityLabel
                AccountingUnit
                    label is FinanceEnterpriseGroup.AccountingUnitLabel
        visible when (BudgetTemplate.ManualProject)
            paragraph
                Project
                    label is FinanceEnterpriseGroup.ProjectLabel
        visible when (BudgetTemplate.ManualAccount)
            visible when (BudgetTemplate.AccountChartSection.IncomeStatement)
                paragraph
                    GeneralLedgerChartAccount
                        label is FinanceEnterpriseGroup.AccountLabel
                        select is PostingIncomeStatementAccountList
            visible when (BudgetTemplate.AccountChartSection.BalanceSheet)
                paragraph
                    GeneralLedgerChartAccount
                        label is FinanceEnterpriseGroup.AccountLabel
                        select is PostingBalanceSheetAccountList
            visible when (BudgetTemplate.AccountChartSection.Both)
                paragraph
                    GeneralLedgerChartAccount
                        label is FinanceEnterpriseGroup.AccountLabel
        visible when (BudgetTemplate.ManualDimension1)
            paragraph
                FinanceDimension1
                    label is FinanceEnterpriseGroup.FinanceDimension1Label
        visible when (BudgetTemplate.ManualDimension2)
            paragraph
                FinanceDimension2
                    label is FinanceEnterpriseGroup.FinanceDimension2Label
        visible when (BudgetTemplate.ManualDimension3)
            paragraph
                FinanceDimension3
                    label is FinanceEnterpriseGroup.FinanceDimension3Label
        visible when (BudgetTemplate.ManualDimension4)
            paragraph
                FinanceDimension4
                    label is FinanceEnterpriseGroup.FinanceDimension4Label
        visible when (BudgetTemplate.ManualDimension5)
            paragraph
                FinanceDimension5
                    label is FinanceEnterpriseGroup.FinanceDimension5Label
        visible when (BudgetTemplate.ManualDimension6)
            paragraph
                FinanceDimension6
                    label is FinanceEnterpriseGroup.FinanceDimension6Label
        visible when (BudgetTemplate.ManualDimension7)
            paragraph
                FinanceDimension7
                    label is FinanceEnterpriseGroup.FinanceDimension7Label
        visible when (BudgetTemplate.ManualDimension8)
            paragraph
                FinanceDimension8
                    label is FinanceEnterpriseGroup.FinanceDimension8Label
        visible when (BudgetTemplate.ManualDimension9)
            paragraph
                FinanceDimension9
                    label is FinanceEnterpriseGroup.FinanceDimension9Label
        visible when (BudgetTemplate.ManualDimension10)
            paragraph
                FinanceDimension10
                    label is FinanceEnterpriseGroup.FinanceDimension10Label
