RSSLPLRequisitionLineMiscellaneous is a Form
    title is "Miscellaneous"
    Layout
        two column distributed
            visible when (RSSLPLDisplayFromCompany)
                FromCompanyLocationBin.FromCompany
                    label is "DeliverFromCompany"
                    display as required
            visible when (RSSLPLDisplayFromLocation)
                FromCompanyLocationBin.FromLocation
                    label is "DeliverFromLocation"
                    select is RQInventoryLocations
                    display as required
                    when value changed
                        refresh PurchaseTaxCodeRequired
        two column distributed
            LocationRule
            visible when (DisplayFillOrKillFlag)
                paragraph
                    visible when (not IsCreatePurchaseOrder)
                        FillOrKill
                            label is "Fill_orKillFlag"
                        KilledQuantity
                            label is "KillQuantity"
                            display as text
        two column distributed
            column1
                header2 of "Currency"
                display as text
                    paragraph
                        UnitCost
                            label is "BaseUnitCost"
                        ConversionRate
            column2
                header2 of "Global"
                GlobalLineType
                    label is "LineType"
        visible when (false)
            PurchaseTaxCode
                display as required when (PurchaseTaxCodeRequired)
