MobileCustomerContractComposite is a CompositeForm
    title is "FranchiseContract"
    context form is MobileCustomerContractContext
    restrict action Create
    restrict action Delete
    Actions
        action is Delete
            hide when invalid
        action is save
            label is "_"
    ContractDetail is a Panel
        form is FranchiseContractMain
    SalesBudget is a Panel
        list is FranchiseContractBudget set.MobileFranchiseContractBudgetList
    Licensee is a Panel
        form is LicenseeDetail
    FranchiseLocation is a Panel
        form is FranchiseLocationDetail
    ContractCharges is a Panel
        list is FranchiseContractChargesRel.MobileContractChargesWithoutContextKeys
    CommentsAndAttachments is a Panel
        list is FranchiseContractComment set.MobileFranchiseContractCommentList
