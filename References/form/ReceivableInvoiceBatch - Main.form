Main is a Form
    title is "Main"
    Layout
        visible when (not IsReleased)
            two column distributed
                column1
                    paragraph
                        ReceivableGeneralLedgerCode
                            label is "AccountsReceivableCode"
                            select is ARCodes
                        ReceivableGeneralLedgerCode.Description
                            no label
                            display as text
                    paragraph
                        BatchDate
                    paragraph
                        GeneralLedgerDate
                            label is "PostDate"
                column2
                    paragraph
                        TransientReceivableOperator
                            label is "Operator"
                    paragraph
                        CurrencyTable
                    paragraph
                        OriginalCurrency
                            label is "Currency"
                        OriginalCurrency.Description
                            no label
                            display as text
            blank line
            header2 of "_"
            four column distributed table
                column headers
                    "Control"
                    "Entry"
                    "Difference"
                row headers
                    "Count"
                    "Invoice"
                    "DebitMemo"
                    "CreditMemo"
                    "BatchTotals"
                column1
                    CountTotal
                        no label
                    InvoiceTotal
                        no label
                    DebitTotal
                        no label
                    CreditTotal
                        no label
                    BatchControlDifference
                        no label
                        display as text
                column2
                    DerivedCountEntry
                        no label
                        display as text
                    DerivedInvoiceEntry
                        no label
                        display as text
                    DerivedDebitEntry
                        no label
                        display as text
                    DerivedCreditEntry
                        no label
                        display as text
                    BatchEntryDifference
                        no label
                        display as text
                column3
                    CountDifference
                        no label
                        display as text
                    InvoiceDifference
                        no label
                        display as text
                    DebitDifference
                        no label
                        display as text
                    CreditDifference
                        no label
                        display as text
                    BatchDifference
                        no label
                        display as text
        visible when (IsReleased)
            display as text
                two column distributed
                    column1
                        paragraph
                            ReceivableGeneralLedgerCode
                                label is "AccountsReceivableCode"
                                select is ARCodes
                            ReceivableGeneralLedgerCode.Description
                                no label
                        paragraph
                            BatchDate
                        paragraph
                            GeneralLedgerDate
                                label is "PostDate"
                    column2
                        paragraph
                            TransientReceivableOperator
                                label is "Operator"
                        paragraph
                            CurrencyTable
                        paragraph
                            OriginalCurrency
                                label is "Currency"
                            OriginalCurrency.Description
                                no label
                blank line
                header2 of "_"
                four column distributed table
                    column headers
                        "Control"
