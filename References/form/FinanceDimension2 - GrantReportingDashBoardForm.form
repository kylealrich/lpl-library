GrantReportingDashBoardForm is a Form
    title is "GrantReporting"
    Layout
        visible when (InEnterpriseStructure)
            visible when (IsExternalOrIntercompanyBilling)
                three column distributed
                    column1
                        GrantID
                            label is "GrantI\\D"
                            display as text
                    column2
                        PrimeFlag
                            display as text
                    column3
                        PreAwardID
                            display as text
                two column distributed
                    column1
                        SEFAFlag
                            label is "IncludeInS\\E\\F\\A"
                            display as text
                    column2
                        PrimeSponsorAwardNumber
                            display as text
                            show up to 15 characters
                single column
                    MajorCategoryDescriptionMessage
                        label is "MajorCategory"
                        display as text
                    CFDANumber
                        display as text
                        show up to 15 characters
                single column
                    ProgramTitle
                        display as text
                        show up to 15 characters
