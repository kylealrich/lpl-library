                        label is FinanceEnterpriseGroup.FinanceDimension5Label
                visible when (AllocationControl.Dimension6Enabled)
                    FinanceDimension6Structure
                        label is FinanceEnterpriseGroup.FinanceDimension6Label
                visible when (AllocationControl.Dimension7Enabled)
                    FinanceDimension7Structure
                        label is FinanceEnterpriseGroup.FinanceDimension7Label
            column4
                visible when (AllocationControl.Dimension8Enabled)
                    FinanceDimension8Structure
                        label is FinanceEnterpriseGroup.FinanceDimension8Label
                visible when (AllocationControl.Dimension9Enabled)
