EFTNotificationExtract is an ActionForm
    action is EFTNotificationExtract
    Layout
        paragraph
            FinanceEnterpriseGroup
            FinanceEnterpriseGroup.EnterpriseGroup.Description
                no label
                display as text
        paragraph
            CustGroup
                label is "CustomerGroup"
                select is CustomerGroups
            CustGroup.Name
                no label
                display as text
        single column
            text of "Or"
        paragraph
            Company
                select is Companies
            Company.Name
                no label
                display as text
        single column
            ProcessDate
                label is "ProcessingDate"
        paragraph
            OverrideDateFr
                label is "FromOverrideDate"
            OverrideDateTo
                label is "ToOverrideDate"
        paragraph
            BankTransactionCode
                label is "PaymentCode"
                select is CashPaymentAndOriginReceivablesList
            BankTransactionCode.Description
                no label
                display as text
        paragraph
            Update
            ExemptPrenoteForImmediates
                label is "ExemptPrenotificationForImmediatelyDueTransactions"
                    left of field
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                EFTNotificationExtractReport_RDG
                EFTNotificationExtractPrenote_RDG
                EFTNotificationExtractFinal_RDG
                EFTNotificationExtractErrorReport_RDG
            column2 of 25%
                EFTNotificationExtractReport_XPT
                EFTNotificationExtractPrenote_XPT
                EFTNotificationExtractFinal_XPT
                EFTNotificationExtractErrorReport_XPT
