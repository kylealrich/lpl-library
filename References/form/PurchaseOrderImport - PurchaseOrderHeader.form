PurchaseOrderHeader is a Form
    Layout
        single column
            Closed
        header2 of "Shipping"
        two column distributed
            column1
                FreightTerm
                    label is "FreightTerms"
                ShipTerm
                    label is "ShipTerms"
            column2
                PrintReceipt
                    label is "ReceiverPrint"
                paragraph
                    ShippingMethod
                        label is "ShipVia"
                    UnloadingPort
                        label is "Port"
        header2 of "Currency"
        three column distributed
            paragraph
                Currency
                Currency.first ToCurrencyRelationshipRel.MultDiv
                    no label
                    display as text
            paragraph
                EnteredCurrencyConversionRate
                Revalue
            CurrencyExchangeSetPoint
        header2 of "Issue"
        three column distributed
            column1
                IssueMethod
                    label is "PurchaseOrderIssueMethod"
                paragraph
                    FaxNumber.InternationalPrefix
                        label is "FaxCountry/\\JurisdictionCode"
                        show up to 2 characters
                    FaxNumber.SubscriberNumber
                        label is "PhoneNumber"
                    FaxNumber.Extension
                        label is "Extension"
                        show up to 5 characters
            column2
                RevisionsInclude
        header2 of "Terms"
        three column distributed
            column1
                ProcessLevel
                TermCode.FirstNetDays
                    label is "DueDays"
                    display as text
                ShipToArriveDate
                UseProcurementCard
            column2
                paragraph
                    TermCode
                    TermCode.Description.Desc[1]
                        no label
                        display as text
                    TermCode.Description.Desc[2]
                        no label
                        display as text
                TermCode.TermsDiscountArray.TermsDiscountGroup[1].DaysToCalcDiscount
                    label is "DiscountDays"
                    display as text
                LetterOfCredit
                ProcurementCardNumber
            column3
                InvoiceMethod
                TermCode.TermsDiscountArray.TermsDiscountGroup[1].DiscountPercent
                    label is "DiscountRatePercent"
                    display as text
        header2 of "Global"
        four column distributed
            NatureOfTransactionCode
            StatisticalProcedure
            GlobalDocumentType
            LetterOfGuarantee
        single column
            header2 of "GrantAccounting"
            SubcontractorPurchaseOrder
        header2 of "UserFields"
        two column
            UserDate1
            UserDate2
            PurchaseOrderUserField1
            PurchaseOrderUserField3
            PurchaseOrderUserField5
            PurchaseOrderUserField7
