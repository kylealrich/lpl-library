MobileMain is a Form
    title is "Main"
    Layout
        header3 of "_"
        paragraph
            Item
                select is MobileItemFilterSelection
                show up to 5 characters
            ItemType
        paragraph
            display as text
                visible when (ItemType.Inventoried)
                    Item.Description
                visible when (ItemType.NonStock)
                    Item.Description
        paragraph
            visible when (ItemType.Special)
                Description
        paragraph
            ReturnQuantity
                label is "ReturnQuantity"
            EnteredUOM
                label is "UOM"
                select is MobileValidBuyUnitsOfMeasure
        paragraph
            CatchWeightQuantity
                label is "FromStock"
            Item.StockUOM
                no label
                display as text
        display as text
            two column
                column1 of 50%
                    CancelQuantity
                column2 of 50%
                    visible when (VendorReturn.ShippedStatus)
                        DerivedDisplayUnitCost
        paragraph
            visible when (not VendorReturn.ShippedStatus)
                UnitCost
        paragraph
            ReturnStockToInventoryOnVendorReturnCancel
        visible when (InventoryItem)
            paragraph
                ItemLocationRel.GeneralLedgerCategory.InventoryAccount
                    label is "DistributionAccount"
                    compact format
        header3 of "PurchaseOrder"
        display as text
            two column
                column1 of 50%
                    OriginalPurchaseOrder
                    visible when (Contract entered)
                        Contract
                    TaxCode
                column2 of 50%
                    OriginalPurchaseOrderLine.LineNumber
                        label is "Line"
                    visible when (ContractLine entered)
                        ContractLine
                    MobileBuyUOMQuantity
                        label is "BuyUOMQuantity"
        paragraph
            VendorItem
                select is MobileVendorItemSelect
        paragraph
            ItemGTIN
                label is "G\\T\\I\\N"
                select is MobileItemGTINSelect
        header3 of "Intrastat"
        two column
            column1 of 50%
                ICNCode
                    select is MobileIntrastatICNCodes
                SupplementaryQuantity
            column2 of 50%
                StockWeight
                    label is "Weight"
                ICNCode.SupplementaryUOM
                    no label
                    display as text
