MobileOrderReturnLines is a CompositeForm
    title is "OrderReturnLines"
    context form is MobileReturnLineContext
    show panel control on left
    restrict action Entered.Delete
    restrict action Entered.Create
    Actions
        action is save
            label is "_"
        action is Entered.Delete
            hide when invalid
    Main is a Panel
        form is Processing
    Details is a Panel
        visible when (HasItemDetail)
        list is CustomerOrderReturnLineDetailsRel.MobileDetails
    AddOnCharges is a Panel
        list is CustomerOrderReturnLineAddOnChargesRel.MobileReturnLineAddOnCharges
    Attachments is a Panel
        list is CustomerOrderReturnLineAttachmentRel.MobileCustomerOrderReturnLineAttachmentList
