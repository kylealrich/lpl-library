    is primary
    propagate as a drill
    title is "NorthAmericanIndustrialClassificationGroup"
    context form is NorthAmericanIndustrialClassificationGroup
    NAICSCodes is a Panel
        list is NorthAmericanIndustrialClassification set.primary
    AssociatedCountries is a Panel
        title is "AssociatedCountries/\\Jurisdictions"
        list is Country set.Countries
            helper list is CountryRel.Countries
                action is AssociateNAICSGroup
                    invoked.PrmNAICSGroup = NorthAmericanIndustrialClassificationGroup
