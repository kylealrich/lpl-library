CommitmentEncumbranceVarianceResult is a Form
    is primary
    title is "CommitmentsAndEncumbrancesVarianceReport"
    Layout
        paragraph
            FinanceEnterpriseGroup
                display only
            FinanceEnterpriseGroup.EnterpriseGroup.Description
                no label
                display as text
        single column
            Scenario
            AccountingEntity
                label is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
            System
            GeneralLedgerChartAccount
                label is "<actor.context.FinanceEnterpriseGroup.AccountLabel>"
            visible when (AccountingUnitUsed)
                AccountingUnit
                    label is "<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>"
            visible when (ProjectUsed)
                Project
                    label is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
            visible when (Dimension1Used)
                FinanceDimension1
                    label is "<actor.context.FinanceEnterpriseGroup.FinanceDimension1Label>"
            CurrencyCode
            EntityYearPeriod.Date
                label is "PeriodDate"
            GLTTransactionAmount
            GLCommitTransactionAmount
            GLCommitTransactionVariance
                label is "Variance"
