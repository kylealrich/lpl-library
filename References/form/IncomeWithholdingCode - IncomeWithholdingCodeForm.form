IncomeWithholdingCodeForm is a CompositeForm
    is primary
    title is "<DerivedFormTitle>"
    context form is IncomeWithholdingCodeContextForm
        background is theme color
    GlobalWithholding is a Panel
        visible when (VendorGroup.GlobalWithholding)
        form is GlobalWithholdingForm
    WithholdingCodeDetails is a Panel
        list is IncomeWithholdingCodeDetail set.IncomeWithholdingCodeDetailList
    WithholdingRate is a Panel
        visible when (VendorGroup.GlobalWithholding)
        list is IncomeWithholdingRate set.primary
