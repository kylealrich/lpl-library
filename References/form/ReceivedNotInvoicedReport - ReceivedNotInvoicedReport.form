        single column
            text of "Or"
        paragraph
            CompanyGroup
            CompanyGroup.Description
                no label
                display as text
        blank line
        single column
            UpdateOption
            ThruGlPeriod
                label is "G\\LPostingPeriod"
            ThruGlYear
                label is "G\\LPostingYear"
        paragraph
            FromPurchaseOrderDate
            ToPurchaseOrderDate
        blank line
        header2 of "SkipProcessing"
        single column
            SkipMatchInvoiceMessage
                label is "SkipProcessingMatchInvoiceMessage"
        blank line
        single column
            DisplayOriginalLineNumber
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                ReceivedNotInvoicedReportReport_RDG
                ReceivedNotInvoicedPostingReport_RDG
                ReceivedNotInvoicedSummaryPostingReport_RDG
            column2 of 25%
