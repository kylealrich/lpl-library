Book is a CompositeForm
    is primary
    propagate as a drill
    title is "Book"
    context form is BookContext
    restrict action ProjectMidQuarterAmounts
    restrict action Reset
    BookCalendars is a Panel
        visible when (ThisRecordExists)
        list is BookCalendarsRel.BookCalendarDetailList
    Assets is a Panel
        visible when (ThisRecordExists)
        list is AssetBook set.AssetsForBook
    ClassDepreciation is a Panel
        visible when (IsClassLevelDepreciation)
        list is AssetBookType set.AssetBookTypeList
