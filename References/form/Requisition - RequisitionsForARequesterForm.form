RequisitionsForARequesterForm is a Form
    propagate as a drill
    title is "RequisitionsForARequester"
    restrict action Unreleased.UpdateRequisitionLineFillOrKill
    Layout
        paragraph
            Requisition
                label is "RequisitionNumber"
                display as text
        paragraph
            Requester
                display as text
        paragraph
            DeliverTo
                display as text
        paragraph
            Approved
                display as text
        paragraph
            AttainedLevel
                label is "LevelToAttain"
                display as text
        paragraph
            ApprovalLevel
                label is "ApprovalLevelAttained"
                display as text
        paragraph
            ProcessFlowApprovalLevel
                label is "P/FHeaderApprovalLevel"
                display as text
        paragraph
            RequestingLocation
                display as text
        paragraph
            RequestingLocation.InventoryTransactionType
                label is "TransactionType"
                display as text
        paragraph
            Status
                display as text
            Closed
                display as text
        paragraph
            CreationDate
                display as text
        paragraph
            ReleasedDate
                display as text
        paragraph
            ApprovedRejectedDate
                label is "Processed/RejectedDate"
                display as text
        paragraph
            RequisitionApprovalType
                label is "ApprovalType"
                display as text
