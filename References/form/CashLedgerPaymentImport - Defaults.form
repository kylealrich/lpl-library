Defaults is a Form
    Layout
        single column
            PayablesProcessGroup
                label is "ProcessGroup"
            GeneralLedgerSourceCode
                label is "EventCode"
            Status
            paragraph
                InterfaceAsPosted
                    label is "InterfaceAsPosted"
                        left of field
                    when value changed
                        refresh BypassCashFundUpdate
                visible when (InterfaceAsPosted)
                    BypassCashFundUpdate
        paragraph
            PayablesData.PayablesIncomeCode
                label is "IncomeCode"
                select is IncomeCodes
            PayablesData.PayablesIncomeCode.Description
                no label
                display as text
        paragraph
            TaxID
            TaxIDType
        single column
            ReportableIncomeAmount
                label is "ReportableAmount"
