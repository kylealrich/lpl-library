InterEntityNetting is an ActionForm
    action is InterEntityNetting
    Layout
        single column
            visible when (not PrmFinanceEnterpriseGroup.IncludeBeginningBalanceInNetting)
                PrmAccountingEntityOptions
                    label is "<PrmFinanceEnterpriseGroup.AccountingEntityLabel>Option"
            visible when (PrmAccountingEntityOptions.Single)
                PrmAccountingEntity
                    label is "<PrmFinanceEnterpriseGroup.AccountingEntityLabel>"
            visible when (PrmAccountingEntityOptions.Group)
                PrmAccountingEntityGroup
                    label is "<PrmFinanceEnterpriseGroup.AccountingEntityLabel>Group"
            visible when (PrmFinanceEnterpriseGroup.IncludeBeginningBalanceInNetting)
                blank line
                PrmReportingBasis
                    label is "ReportingBasis"
                PrmAccountingEntityOptionsNetting
                    label is "<PrmFinanceEnterpriseGroup.AccountingEntityLabel>Option"
                visible when (PrmAccountingEntityOptionsNetting.Single)
                    PrmAccountingEntity
                        label is "<PrmFinanceEnterpriseGroup.AccountingEntityLabel>"
                PrmEndingPeriod
                    label is "YTDPeriod"
                    select is ClosePeriods
            visible when (not PrmFinanceEnterpriseGroup.IncludeBeginningBalanceInNetting)
                blank line
                single column
                    PrmPeriodOption
                        label is "PeriodOption"
                two column
                    visible when (PrmPeriodOption.SpecifyPeriod)
                        PrmPeriod
                            label is "Period"
                            select is ClosePeriods
                two column
                    visible when (PrmPeriodOption.PeriodRange)
                        PrmStartingPeriod
                            label is "StartingPeriod"
                            select is ClosePeriods
                        PrmEndingPeriod
                            label is "EndingPeriod"
                            select is ClosePeriods
        single column
            visible when (not PrmFinanceEnterpriseGroup.NettingJournalApproval)
                PrmCreateInPendingApproval
                    label is "ApprovalRequired"
        two column
            visible when (PrmCreateInPendingApproval)
                PrmApprovalCode
                    label is "ApprovalCode"
