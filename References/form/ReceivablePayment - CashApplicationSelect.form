CashApplicationSelect is a CompositeForm
    is primary
    title is "CashApplicationSelection<PaymentNumberApplicationInProcessTitle>"
    context form is CashApplicationSelectContextForm
        background is theme color
    restrict action SelectForAPInterface
    restrict action RefreshData
    restrict action Unreleased.CreateDistribution
    restrict action Unreleased.Delete
    restrict action Unreleased.ReleaseBatch
    Actions
        action is Unreleased.ProcessSelections
            mouse over text is "Process_The_Selected_Application_Records"
            hide when invalid
        action is Open.ProcessSelections
            mouse over text is "Process_The_Selected_Application_Records"
            hide when invalid
        navigation is CashApplicationSelectContextFormShowContextFormOnLeftNav
            action icon is preview
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
            hide when invalid
        action is refresh
            label is "_"
            hide when invalid
    MatchCashDetails is a Panel
        title is "MatchCashDetails"
        visible when (ShowMatchCashDetails)
        list is ReceivablePaymentCashDetail set.CashDetailsForApplication
    InvoiceSelectionNA is a Panel
        title is "InvoiceSelectionNA"
        visible when (not CompanyCustomerRel.NationalAccountIndicator.No)
        list is NationalAccountTransactionsRel.CashApplicationSelectEntryList2
    InvoiceSelection is a Panel
        visible when (not ReceivableCompanyRel.IsApplyCashAcrossReceivableCompanies)
        list is TransactionsForCustomerRel.CashApplicationSelectEntryList2
    InvoiceSelectionAcrossCompanies is a Panel
        title is "InvoiceSelection"
        visible when (ReceivableCompanyRel.IsApplyCashAcrossReceivableCompanies)
        list is TransactionsForCustomerAcrossCompanyRel.CashApplicationSelectEntryList2
    SpeedEntryPreview is a Panel
        title is "ManualEntry"
        list is ReceivableApplicationSelectRel.SelectPaymentApplicationsSpeedEntry
    WriteOffsAndMemos is a MultiListPanel
        title is "<WriteOffsAndMemosPanelWithCount>"
        visible when (not IsZeroPayment)
        list is PendingAdjustNonMemoRel.PaymentSelectWriteoffs
            show 5 lines
        list is PendingAdjustMemoRel.Memos
            visible when (Company.PaymentOverApplication)
            show 5 lines
        list is PendingAdjustMemoRel.MemosNotValid
            visible when (not Company.PaymentOverApplication)
            show 5 lines
    ReceivableProjectTransactions is a Panel
        visible when (OpenReceivableProjectTransactionRelExists)
        list is OpenReceivableProjectTransactionRel.ReceivableProjectTransactionCashApplicationList
    LineItemSelectionNA is a Panel
        title is "LineItemSelectionNA"
        visible when (not CompanyCustomerRel.NationalAccountIndicator.No)
        list is NationalAccountTransactionsRel.LineItemCashApplicationSelectEntryList2
    LineItemSelection is a Panel
        visible when (not ReceivableCompanyRel.IsApplyCashAcrossReceivableCompanies)
        list is TransactionsForCustomerRel.LineItemCashApplicationSelectEntryList2
    LineItemSelectionAcrossCompanies is a Panel
        title is "LineItemSelection"
        visible when (ReceivableCompanyRel.IsApplyCashAcrossReceivableCompanies)
        list is TransactionsForCustomerAcrossCompanyRel.LineItemCashApplicationSelectEntryList2
    PendingSelections is a Panel
        title is "Selected_(<NumberOfReceivableApplicationSelect>)"
        visible when (HasSelectApplications)
        list is ReceivableApplicationSelectRel.ReviewReceivableApplicationSelectList
    Applications is a Panel
        visible when (HasApplications)
        list is ReceivableApplicationRel.PaymentApplications
    EditPaymentInfo is a Panel
        visible when (UnreleasedOrUnassigned)
        form is EditPaymentInfo
