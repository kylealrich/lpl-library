ReceivableInvoiceLine is a CompositeForm
    is primary
    title is "<ReceivableInvoiceType>_<ReceivableInvoice>_Line_<FormTitleLineNumber>"
    context form is ReceivableInvoiceLineContextForm
        background is theme color
    Main is a Panel
        form is Main
    AddOnCharges is a Panel
        title is "AddOnCharges<DerivedAddOnChargesIndicator>"
        list is ReceivableInvoiceAddOnCharge set.AddOnCharges
    Distributions is a Panel
        list is ReceivableGLDistributionDetailTypeRel.ReceivableInvoiceDistributionList
