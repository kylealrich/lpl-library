MainProjectForm is a Form
    title is "<FinanceEnterpriseGroup.ProjectLabel>"
    Layout
        visible when (HasBudgetChanges)
            text of "Budget_Template_Changes_Pending"
                bold
                italics
                color of red
        visible when (InEnterpriseStructure)
            three column distributed
                column1
                    Description
                column2
                    paragraph
                        ProjectDateRange.BeginDate
                            label is "BeginDate"
                            display as required
                        ProjectDateRange.EndDate
                            label is "EndDate"
                            display as required
                column3
                    paragraph
                        Active
            visible when (ProjectType.Summary)
                three column distributed
                    column1
                        paragraph
                            BudgetIdentifier
                            BudgetIdentifier.Description
                                no label
                                display as text
                    column2
                        paragraph
                            Watch
                            WatchedBy
                            WatchedBy.Name.PreferredFirstAndLastName
                                no label
                                display as text
            visible when (ProjectType.Posting)
                three column distributed
                    column1
                        paragraph
                            ProjectStatus
                                label is "<FinanceEnterpriseGroup.ProjectLabel>Status"
                                show up to 10 characters
                                display as required
                            ProjectStatus.Description
                                no label
                                display as text
                    column2
                        paragraph
                            Currency
                                show up to 3 characters
                                display as required
                            Currency.Description
                                no label
                                display as text
                    column3
                        paragraph
                            Watch
                            WatchedBy
                            WatchedBy.Name.PreferredFirstAndLastName
                                no label
                                display as text
            visible when (ProjectType.Posting)
                header2 of "AdditionalInformation"
                four column distributed
                    column1
                        Billable
                    column2
                        Capital
                    column3
                        IncludeInBudgetEdit
                    column4
                        SystemProject
                            label is "System<FinanceEnterpriseGroup.ProjectLabel>"
                four column distributed
                    column1
                        visible when (FinanceEnterpriseGroup.FrontEndSplits)
                            ExcludeFromFES
                                label is "ExcludeFromFrontEndSplit"
                    column2
                        visible when (FinanceEnterpriseGroup.ExpenseManagementIntegrated)
                            ExpenseManagement
                single column
                    DefaultCodeBlock
                        compact format
                four column distributed
                    column1
                        WorkType
                    column2
                        ServiceType
                    column3
                        UsageType
                four column distributed
                    column1
                        PreAwardID
                    column2
                        ProgramIncome
                    column3
                        ProgramIncomeType
            visible when (ProjectType.Posting)
                two column
                    column1
                        FinanceDimension2
                            label is "RelatedFundingSource"
                    column2
                        FinanceDimension2.Description
                            no label
                            display as text
            header2 of "Address"
            four column distributed
                column1
                    paragraph
                        AddressCode
                        AddressCode.Description
                            no label
                            display as text
                visible when (AddressCode entered)
                    column2
                        AddressCode.PostalAddress.DisplayAddressMultiline
                            label is "Address"
                            display as text
        visible when (not InEnterpriseStructure)
            visible when (ProjectType.Posting)
                display as text
                    three column distributed
                        column1
                            Description
                        column2
                            paragraph
                                ProjectDateRange.BeginDate
                                    label is "BeginDate"
                                ProjectDateRange.EndDate
                                    label is "EndDate"
                        column3
                            paragraph
                                Watch
                                WatchedBy
                                WatchedBy.Name.PreferredFirstAndLastName
                                    no label
                                    display as text
            visible when (ProjectType.Summary)
                three column distributed
                    column1
                        Description
                    column2
                        paragraph
                            ProjectDateRange.BeginDate
                                label is "BeginDate"
                                display as text
                            ProjectDateRange.EndDate
                                label is "EndDate"
                                display as text
                    column3
                        paragraph
                            Watch
                            WatchedBy
                            WatchedBy.Name.PreferredFirstAndLastName
                                no label
                                display as text
            visible when (ProjectType.Summary)
                three column distributed
                    column1
                        paragraph
                            BudgetIdentifier
                            BudgetIdentifier.Description
                                no label
                                display as text
                    column2
            visible when (ProjectType.Posting)
                display as text
                    three column distributed
                        column1
                            paragraph
                                ProjectStatus
                                    label is "<FinanceEnterpriseGroup.ProjectLabel>Status"
                                ProjectStatus.Description
                                    no label
                        column2
                            paragraph
                                Currency
                                Currency.Description
                                    no label
                header2 of "AdditionalInformation"
                four column distributed
                    column1
                        Billable
                            display as text
                    column2
                        Capital
                            display as text
                    column3
                        IncludeInBudgetEdit
                    column4
                        SystemProject
                            label is "System<FinanceEnterpriseGroup.ProjectLabel>"
                            display as text
                four column distributed
                    column1
                        visible when (FinanceEnterpriseGroup.ExpenseManagementIntegrated)
                            ExpenseManagement
                                display as text
                    column2
                        visible when (FinanceEnterpriseGroup.FrontEndSplits)
                            ExcludeFromFES
                                label is "ExcludeFromFrontEndSplit"
                                display as text
                single column
                    DefaultCodeBlock
                        display as text
                        compact format
                four column distributed
                    column1
                        WorkType
                            display as text
                    column2
                        ServiceType
                            display as text
                    column3
                        UsageType
                            display as text
                four column distributed
                    column1
                        PreAwardID
                            display as text
                    column2
                        ProgramIncome
                            display as text
                    column3
                        ProgramIncomeType
                            display as text
        visible when (SummaryInEnterpriseStructure)
            single column
                visible when (ProjectContractExists)
                    button of "ContractInformation"
                        link is ProjectContract
                        display as link
                visible when (not ProjectContractExists)
                    button of "CreateContract"
                        action is CreateContract
                        display as link
            three column distributed
                column1
                    paragraph
                        PersonResponsible
                            select is ActiveEmployeesOpenRestricted
                        PersonResponsible.Name.PreferredFirstAndLastName
                            no label
                            display as text
                column2
                    paragraph
                        ProjectTeam
                        ProjectTeam.Description
                            no label
                            display as text
        visible when (not InEnterpriseStructure)
            two column
                column1
                    paragraph
                        AddressCode
                            display as text
                        AddressCode.Description
                            no label
                            display as text
                visible when (AddressCode entered)
                    column2
                        AddressCode.PostalAddress.DisplayAddressMultiline
                            label is "Address"
                            display as text
        visible when (ProjectType.Summary)
            single column
                button of "<FinanceEnterpriseGroup.ProjectLabel>HierarchyChart"
                    link is ProjectChart
                    display as link
