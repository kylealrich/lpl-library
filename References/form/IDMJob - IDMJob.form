IDMJob is a Form
    is primary
    Layout
        display as text
            header2 of "JobDetails"
            three column distributed
                column1
                    Status
                        display as tag
                            color of green when (Status.Finished)
                            color of red when (Status.Failed)
                            color of orange when (Status.TimedOut)
                            color of blue when (Status.Submitted)
                    JobID
                column2
                    CreatedDate
                    visible when (JobID entered)
                        button of "indata.txt"
                            link is external "<DerivedInputDataURL>"
                            label is "InputData"
                            display as link
                column3
                    DerivedJobDescription
            three column distributed
                column1
                    header2 of "DocumentDetails"
                    GenerationStatus
                        display as tag
                            color of green when (GenerationStatus.Finished)
                            color of red when (GenerationStatus.Failed)
                            color of orange when (GenerationStatus.TimedOut)
                            color of gray when (GenerationStatus.NotApplicable)
                            color of blue when (GenerationStatus.Submitted)
                    visible when (DisplayFile)
                        button of "<FileName>"
                            link is external "<DerivedIDMDocumentURL>"
                            label is "File"
                            display as link
                    DocumentType
                    visible when (MDSID entered)
                        MDSID
                    visible when (AdditionalDetails entered)
                        AdditionalDetails
                            use text area of 5 lines
                column2
                    header2 of "EmailDetails"
                    EmailStatus
                        label is "Status"
                        display as tag
                            color of green when (EmailStatus.Finished)
                            color of red when (EmailStatus.Failed)
                            color of orange when (EmailStatus.TimedOut)
                            color of gray when (EmailStatus.NotApplicable)
                            color of blue when (EmailStatus.Submitted)
                    visible when (EmailStatus entered)
                        EmailSubject
                        EmailFrom
                        DerivedEmailTo
                        DerivedEmailCc
                column3
                    header2 of "PrintDetails"
                    PrintStatus
                        label is "Status"
                        display as tag
                            color of green when (PrintStatus.Finished)
                            color of red when (PrintStatus.Failed)
                            color of orange when (PrintStatus.TimedOut)
                            color of gray when (PrintStatus.NotApplicable)
                            color of blue when (PrintStatus.Submitted)
                    visible when (PrintStatus entered)
                        visible when (PrinterType.IEP)
                            Printer
                            PrinterID
                            PrinterLocation
                            PrinterName
                            visible when (PrinterSettingsExist)
                                button of "View"
                                    link is SettingsLink
                                    label is "PrinterSettings"
                                    display as link
                        visible when (PrinterType.Email)
                            Printer
                            PrinterEmailAddress
                            PrinterName
            visible when (DisplayErrorMessage)
                single column
                    header2 of "ErrorMessage"
                    ErrorMessage
                        no label
                        color of red
                    button of "ExportToP\\D\\F" with icon.download
                        link is IDMJobFormNavigation
                        is primary
