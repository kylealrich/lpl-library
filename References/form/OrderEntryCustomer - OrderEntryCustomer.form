OrderEntryCustomer is a Form
    is primary
    Layout
        two column distributed
            paragraph
                Company
                Company.Name
                    no label
                    display as text
            paragraph
                Customer
                Customer.Name
                    no label
                    display as text
        two column distributed
            column1
                BillingFreightCode
                PickingPriority
                Carrier
                CashInAdvanceRequired
                CashInAdvanceRequiredPercent
                ProformaCashInAdvanceRequired
                ProformaCashInAdvanceRequiredPercent
                OrderDiscountCode
                Territory
                Salesman2
                CommissionSplitPercentage
                PriceList
                    compact format
                CustomeOrderAcknowledgments
                BackorderAllowed
                ShipCompleteFlag
                PORequiredForCustomerOrder
                SubstitutesAllowed
                DefaultCustomerShipTo
                    label is "DefaultShipToLocation"
                DefaultShippingLocation
                DefaultShippingMethod
                ProofOfDeliveryRequired
            column2
                ProofOfDeliveryGracePeriod
                SingleOrderLimit
                SingleOrderHoldCode
                TotalCODOrderLimit
                CashOnDeliveryOrderHoldCode
                CheckCreditOnOrderAdd
                CheckCreditBeforePickingList
                BrokenCaseCharge
                CreditLimitHoldCode
                OrderLimitHoldCode
                BillingLineItemRequired
                Vendor
                PurchFrLoc
                    label is "PurchaseFromLocation"
                ShipTerm
                PortCode
                TaxCalulationAndPrinting
                AllowReturns
                LastReturnAuthorizationDate
                CarrierFlag
                BillingCustomerCode
        single column
            AgeCredPcts
                label is "AgeCreditPercent"
            OrderAgingPriodLimitHoldCode
                label is "AgeHoldCode"
