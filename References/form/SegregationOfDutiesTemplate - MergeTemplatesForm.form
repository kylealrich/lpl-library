MergeTemplatesForm is a WizardForm
    use for action MergeTemplates
    title is "MergeTemplates"
    show panel navigation
        last panel navigation is MergeTemplatesNav
            next button title is "Finish"
    restrict action CopyTemplate1LeftConditions
        when (false)
    restrict action CopyTemplate1RightConditions
        when (false)
    restrict action CopyTemplate2LeftConditions
        when (false)
    restrict action CopyTemplate2RightConditions
        when (false)
    SegregationOfDutiesTemplate is a Panel
        form is SegregationOfDutiesTemplatePrimary
    SelectTemplates is a Panel
        form is SelectTemplates
    Template1 is a MultiListPanel
        title is "Merge:<Template1>_<TransTemplate1.TemplateName>"
        list is Template1LeftCondObjectRel.WizardCondSODObjectTypeList
            panel text is "LeftSideConditions"
            Special Actions
                action is CopyTemplate1LeftConditions
                    label is "CopyConditions"
        list is Template1RightCondObjectRel.WizardCondSODObjectTypeList
            panel text is "RightSideConditions"
            Special Actions
                action is CopyTemplate1RightConditions
                    label is "CopyConditions"
    Template2 is a MultiListPanel
        title is "Merge:<Template2>_<TransTemplate2.TemplateName>"
        list is Template2LeftCondObjectRel.WizardCondSODObjectTypeList
            panel text is "LeftSideConditions"
            Special Actions
                action is CopyTemplate2LeftConditions
                    label is "CopyConditions"
        list is Template2RightCondObjectRel.WizardCondSODObjectTypeList
            panel text is "RightSideConditions"
            Special Actions
                action is CopyTemplate2RightConditions
                    label is "CopyConditions"
