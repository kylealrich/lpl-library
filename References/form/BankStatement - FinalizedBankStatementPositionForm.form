FinalizedBankStatementPositionForm is a CompositeForm
    title is "CashPositionWorksheet"
    context form is BankStatementPositionContextForm
        background is theme color
    restrict action DeleteOpenReconciliationTransactions
    restrict action Unreconciled.DeleteStatementReconciliationDetails
    restrict action Unreconciled.ReclassifyAllStatementLines
    restrict action SwitchToCombinedListView
    restrict action SwitchToListOverListView
    restrict action MassLoadAndReconcile
    restrict action UpdateCashCodeCashPosition
    restrict action Purge
    Actions
        action is EmailCashPositionSummary
            label is "Email"
        action is export to pdf
            action icon is print
            label is "_"
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is refresh
            label is "_"
    Details is a Panel
        list is EnteredPositionAmountRel.FinalizedBankStatementPositionList
