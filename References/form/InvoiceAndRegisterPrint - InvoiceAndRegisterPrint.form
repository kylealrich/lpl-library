InvoiceAndRegisterPrint is an ActionForm
    action is InvoiceAndRegisterPrint
    Layout
        single column
            paragraph
                Company
                    select is Companies
                Company.Name
                    no label
                    display as text
            paragraph
                ProcessLevel
                    select is ProcessLevels
                ProcessLevel.Name
                    no label
                    display as text
            text of "Or"
            paragraph
                CompanyGroup
                    label is "CompanyGroup"
                CompanyGroup.Description
                    no label
                    display as text
            paragraph
                InvoiceDate
                    label is "InvoiceCutoffDate"
                text of "Or"
                AddSubtract
                    label is "Number_ofDays"
                CutoffDays
                    no label
            SelectOption
                label is "PrintOption"
        three column distributed
            InvFormOption
                label is "InvoiceFormRequired"
            OscCommentCode
                label is "StandardCommentCode"
                select is StandardCommentCodes
            visible when (not Company.UseIDM)
                Alignments
                    label is "Number_ofAlignments"
        three column distributed
            visible when (not Company.ManualInvoiceNumbering)
                InvcPrefixGrp
                    label is "ReprintInvoiceRange"
                FrInvcNumber
                    label is "FromInvoice"
                ToInvcNumber
                    label is "ToInvoice"
        three column distributed
            column1
                EdiInvcOption
                    label is "PrintE\\D\\IInvoice"
            column2
                CertNbrOption
                    label is "PrintCertificationNumber"
            column3
                text of "_"
        single column
            visible when (not Company.UseIDM)
                InvcFormType
                    label is "InvoiceFormType"
            FileName
        visible when (Company.UseIDM)
            header2 of "IDMOption"
            paragraph
                SummarizeInvoicesByCustomer
                PrintInvoicesPerCustomer
            paragraph
                IDMPrinter
                    label is "Printer"
                IDMPrinter.Description
                    no label
                    display as text
            visible when (IDMPrinter.Type.IEP)
                paragraph
                    PrinterSettings
        visible when (CompanyGroup.CompanyMemberIsUsingIDM)
            header2 of "IDMOption"
            paragraph
                SummarizeInvoicesByCustomer
                PrintInvoicesPerCustomer
            paragraph
                IDMPrinter
                    label is "Printer"
                IDMPrinter.Description
                    no label
                    display as text
            visible when (IDMPrinter.Type.IEP)
                paragraph
                    PrinterSettings
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                InvoiceAndRegisterPrintReport_RDG
                BillingInvoiceForm_RDG
                BillingInvoiceRegister_RDG
                BillingInvoiceFormData_RDG
            column2 of 25%
                InvoiceAndRegisterPrintReport_XPT
                BillingInvoiceForm_XPT
                BillingInvoiceRegister_XPT
                BillingInvoiceFormData_XPT
