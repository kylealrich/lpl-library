TierQualifierContractSetup is a CompositeForm
    is primary
    title is "TierQualifierContract"
    context form is TierQualifierContractContext
        background is theme color
    QualifierItems is a Panel
        visible when (ContractHasLinesToSelect)
        list is ContractTierQualifierItemsRel.TierQualifierItemsForAQualifierContract
            helper list is ContractQualifiedLinesRel.AllContractItemsForTierQualifiers
                action is SelectItemForTierQualifier
                    invoked.PrmFromContract = Contract
    QualifierItemsAllSelected is a Panel
        title is "QualifierItems"
        visible when (not ContractHasLinesToSelect)
        list is ContractTierQualifierItemsRel.TierQualifierItemsForAQualifierContractNoHelper
