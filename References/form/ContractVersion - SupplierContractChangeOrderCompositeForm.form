SupplierContractChangeOrderCompositeForm is a CompositeForm
    title is "ChangeOrder"
    disable field options
    called out actions only
    context form is SupplierChangeOrderContext
        background is theme color
    Actions
        action is SubmitChangeOrder
            hide when invalid
        action is DeleteChangeOrder
            hide when invalid
        action is save
            label is "_"
        action is save and close
        action is refresh
            label is "_"
    ChangeOrder is a Panel
        form is ChangeOrderMain
    LinesForChangeOrder is a Panel
        visible when (CanUpdate)
        list is ContractLineRel.SupplierChangeOrderContractLinesExisting
    ChangeOrderDetail is a Panel
        visible when (Contract.ContractLinesExist)
        list is ContractChangeOrder set.SupplierChangeOrderList
