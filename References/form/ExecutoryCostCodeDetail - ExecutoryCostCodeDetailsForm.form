ExecutoryCostCodeDetailsForm is a Form
    title is "NonLeaseComponentFormDetails"
    Layout
        two column
            column1
                FinanceEnterpriseGroup
                    label is "FinanceEnterpriseGroup"
                    display as text
                ExecutoryCostCode.ExecutoryCostCodeRepresentativeText
                    display as text
                ExecutoryCostCode.Active
                    display as text
                Company
            column2
                DistributionAccount
                    compact format
                Taxable
                paragraph
                    TaxCode
                    TaxCode.Description
                        no label
                        display as text
                paragraph
                    TaxUsageCode
                    TaxUsageCode.Description
                        no label
                        display as text
