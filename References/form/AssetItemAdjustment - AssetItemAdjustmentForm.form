AssetItemAdjustmentForm is a Form
    is primary
    title is "AssetItemAdjustment"
    Layout
        display as text
            three column distributed
                paragraph
                    Asset
                    Asset.Description
                        no label
                        display as text
                Asset.DerivedStatus
                    label is "Status"
                    display as text
                visible when (IsTagEntered)
                    Asset.TagNumber
                        display as text
        visible when (Asset.LeasedAsset)
            paragraph
                Asset.AssetLease.LeaseCompany
                    label is "LeaseCompany"
                    display as text
                Asset.AssetLease.LeaseCompany.Name
                    no label
                    display as text
            paragraph
                Asset.AssetLease.Lease
                    display as text
                Asset.AssetLease.Lease.Description
                    no label
                    display as text
        three column distributed
            AssetAdjustment.AdjustmentDate
                display as text
            AssetAdjustment.AdjustmentStatus
                display as text
            paragraph
                AssetAdjustment.CreationDate
                    display as text
                AssetAdjustment.CreatedBy
                    display as text
        visible when (AssetItemFundsExists)
            button of "FundItemAudit"
                link is AssetItemFundAudit
                display as link
        header2 of "_"
        two column
            column1 of 10%
                text of "Current_Asset_Detail"
                    bold
                visible when (IsItemNumberAdjusted)
                    ItemNumber
                        display as text
                visible when (IsDiscriptionAdjusted)
                    Description
                        display as text
                visible when (IsPurchaseDateAdjusted)
                    PurchaseDate
                        display as text
                visible when (IsQuantityAdjusted)
                    ItemQuantity
                        display as text
                visible when (IsCostAdjusted)
                    TransactionItemCost
                        label is "Cost"
                        display as text
                visible when (IsTaxAdjusted)
                    TransactionItemTax
                        label is "Tax"
                        display as text
                visible when (IsPOAdjusted)
                    paragraph
                        PONumber
                            label is "PurchaseOrder"
                            display as text
                        PORelease
                            no label
                            display as text
                        POCode
                            no label
                            display as text
                visible when (IsVendorAdjusted)
                    Vendor
                        display as text
                visible when (IsVendoeNameAdjusted)
                    VendorName
                        display as text
                visible when (IsInvoiceAdjusted)
                    PayablesInvoice
                        display as text
                visible when (IsModelAdjusted)
                    ModelNumber
                        display as text
                visible when (IsSerialAdjusted)
                    SerialNumber
                        display as text
                visible when (IsLocAdjusted)
                    LocationDetail
                        display as text
                visible when (IsBarcodeAdjusted)
                    BarCode
                        display as text
                visible when (IsInvDateAdjusted)
                    paragraph
                        InventoryDate
                            label is "InvetoryDateTime"
                            display as text
                visible when (IsItemConditionAdjusted)
                    ItemCondition
                        display as text
                visible when (IsGeographicalLatitudeAdjusted)
                    GeographicalLatitude
                        display as text
                visible when (IsGeographicalLongitudeAdjusted)
                    GeographicalLongitude
                        display as text
                visible when (IsGeographicalAltitudeAdjusted)
                    GeographicalAltitude
                        display as text
                visible when (IsReferenceCompanyAdjusted)
                    AssetItemReferenceFields.ReferenceCompany
                        label is "ReferenceCompany"
                        display as text
                visible when (IsReferenceVendorAdjusted)
                    AssetItemReferenceFields.ReferenceVendor
                        label is "ReferenceVendor"
                        display as text
                visible when (IsReferenceInvoiceAdjusted)
                    AssetItemReferenceFields.ReferenceInvoice
                        label is "ReferenceInvoice"
                        display as text
                visible when (IsReferencePurchaseOrderAdjusted)
                    AssetItemReferenceFields.ReferencePurchaseOrder
                        label is "ReferencePurchaseOrder"
                        display as text
                visible when (IsReferenceSequenceNumberAdjusted)
                    AssetItemReferenceFields.ReferenceSequenceNumber
                        label is "ReferenceSequenceNumber"
                        display as text
                visible when (IsReferenceProjectAdjusted)
                    AssetItemReferenceFields.ReferenceProject
                        label is "ReferenceProject"
                        display as text
                visible when (IsInvoiceCompanyAdjusted)
                    InvoiceCompany
                        display as text
                visible when (IsPurchaseOrderLineAdjusted)
                    PurchaseOrderLine
                        display as text
            column2 of 20%
                text of "New_Asset_Detail"
                    bold
                visible when (IsItemNumberAdjusted)
                    ToItemNumber
                        label is "ItemNumber"
                        display as text
                visible when (IsDiscriptionAdjusted)
                    ToDescription
                        label is "Description"
                        display as text
                visible when (IsPurchaseDateAdjusted)
                    ToPurchaseDate
                        label is "PurchaseDate"
                        display as text
                visible when (IsQuantityAdjusted)
                    ToItemQuantity
                        label is "ItemQuantity"
                        display as text
                visible when (IsCostAdjusted)
                    ToTransactionItemCost
                        label is "Cost"
                        display as text
                visible when (IsTaxAdjusted)
                    ToTransactionItemTax
                        label is "Tax"
                        display as text
                visible when (IsPOAdjusted)
                    paragraph
                        ToPONumber
                            label is "PurchaseOrder"
                            display as text
                        ToPORelease
                            no label
                            display as text
                        ToPOCode
                            no label
                            display as text
                visible when (IsVendorAdjusted)
                    ToVendor
                        label is "Vendor"
                        display as text
                visible when (IsVendoeNameAdjusted)
                    ToVendorName
                        label is "VendorName"
                        display as text
                visible when (IsInvoiceAdjusted)
                    ToPayablesInvoice
                        label is "PayablesInvoice"
                        display as text
                visible when (IsModelAdjusted)
                    ToModelNumber
                        label is "ModelNumber"
                        display as text
                visible when (IsSerialAdjusted)
                    ToSerialNumber
                        label is "SerialNumber"
                        display as text
                visible when (IsLocAdjusted)
                    ToLocationDetail
                        label is "LocationDetail"
                        display as text
                visible when (IsBarcodeAdjusted)
                    ToBarCode
                        label is "BarCode"
                        display as text
                visible when (IsInvDateAdjusted)
                    paragraph
                        ToInventoryDate
                            label is "InvetoryDateTime"
                            display as text
                visible when (IsItemConditionAdjusted)
                    ToItemCondition
                        label is "ItemCondition"
                        display as text
                visible when (IsGeographicalLatitudeAdjusted)
                    ToGeographicalLatitude
                        label is "GeographicalLatitude"
                        display as text
                visible when (IsGeographicalLongitudeAdjusted)
                    ToGeographicalLongitude
                        label is "GeographicalLongitude"
                        display as text
                visible when (IsGeographicalAltitudeAdjusted)
                    ToGeographicalAltitude
                        label is "GeographicalAltitude"
                        display as text
                visible when (IsReferenceCompanyAdjusted)
                    ToAssetItemReferenceFields.ReferenceCompany
                        label is "ReferenceCompany"
                        display as text
                visible when (IsReferenceVendorAdjusted)
                    ToAssetItemReferenceFields.ReferenceVendor
                        label is "ReferenceVendor"
                        display as text
                visible when (IsReferenceInvoiceAdjusted)
                    ToAssetItemReferenceFields.ReferenceInvoice
                        label is "ReferenceInvoice"
                        display as text
                visible when (IsReferencePurchaseOrderAdjusted)
                    ToAssetItemReferenceFields.ReferencePurchaseOrder
                        label is "ReferencePurchaseOrder"
                        display as text
                visible when (IsReferenceSequenceNumberAdjusted)
                    ToAssetItemReferenceFields.ReferenceSequenceNumber
                        label is "ReferenceSequenceNumber"
                        display as text
                visible when (IsReferenceProjectAdjusted)
                    ToAssetItemReferenceFields.ReferenceProject
                        label is "ReferenceProject"
                        display as text
                visible when (IsInvoiceCompanyAdjusted)
                    ToInvoiceCompany
                        label is "InvoiceCompany"
                        display as text
                visible when (IsPurchaseOrderLineAdjusted)
                    ToPurchaseOrderLine
                        label is "PurchaseOrderLine"
                        display as text
