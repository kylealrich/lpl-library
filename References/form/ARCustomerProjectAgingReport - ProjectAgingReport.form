ProjectAgingReport is an ActionForm
    action is ARCustomerProjectAgingReport
    Layout
        paragraph
            FinanceEnterpriseGroup
            FinanceEnterpriseGroup.EnterpriseGroup.Description
                no label
                display as text
        two column
            AsOfDate
                label is "As_ofDate"
            FutureApps
                label is "QualifyFutureApplications"
        header2 of "Project"
        paragraph
            visible when (ReportSeq.Project)
                AllProjects
                    label is "AllProjects"
        three column
            visible when (ReportSeq.Project)
                Project.Project[1]
                    label is "Projects"
                    display as required when (AllProjects.No)
            visible when (not ReportSeq.Project)
                Project.Project[1]
                    label is "Projects"
            Project.Project[2]
                no label
            Project.Project[3]
                no label
        three column
            Project.Project[4]
                no label
            Project.Project[5]
                no label
            Project.Project[6]
                no label
        header2 of "ProjectContract"
        three column
            visible when (ReportSeq.ProjectContract)
                ProjectContract.ProjectContract[1]
                    label is "ProjectContracts"
                    display as required when (ReportSeq.ProjectContract)
            visible when (not ReportSeq.ProjectContract)
                ProjectContract.ProjectContract[1]
                    label is "ProjectContracts"
            ProjectContract.ProjectContract[2]
                no label
            ProjectContract.ProjectContract[3]
                no label
        three column
            ProjectContract.ProjectContract[4]
                no label
            ProjectContract.ProjectContract[5]
                no label
            ProjectContract.ProjectContract[6]
                no label
        header2 of "AgingRules"
        two column distributed
            AgingMethod
                label is "AgingDateMethod"
            AgeCredits
        two column distributed
            AgePayments
            AgeDisputes
        single column
            DisplayDate
        single column
            ExpandedAgingPeriods
                label is "ExpandedAgingPeriods"
        two column
            paragraph
                AgeCurrent
                    label is "CurrentDays"
            paragraph
                AgePeriods1
                    label is "*Periods1\\-\\4"
                AgePeriods2
                    no label
                AgePeriods3
                    no label
                AgePeriods4
                    no label
        visible when (ExpandedAgingPeriods.Yes)
            two column
                paragraph
                    FutureDays
                        label is "FutureDays"
                paragraph
                    AgePeriods5
                        label is "Periods5\\-\\7"
                    AgePeriods6
                        no label
                    AgePeriods7
                        no label
        header2 of "Format"
        single column
            DetailOption
                label is "DetailTransaction"
        single column
            ReportSeq
                label is "ReportSequence"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                ARCustomerProjectAgingReportReport_RDG
            column2 of 25%
                ARCustomerProjectAgingReportReport_XPT
