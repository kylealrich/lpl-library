ProjectFundingSourceMainForm is a Form
    title is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
    Layout
        three column distributed
            column1
                paragraph
                    FinanceDimension2
                        label is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
                        select is FinanceDimension2FundingSourceSelect
                    FinanceDimension2.Description
                        no label
                        display as text
            column2
                paragraph
                    FinanceDimension2.DateRange.Begin
                        label is "BeginDate"
                        display as text
                    FinanceDimension2.DateRange.End
                        label is "EndDate"
                        display as text
                    visible when (DisplayFD2Link)
                        button of "<FinanceEnterpriseGroup.FinanceDimension2Label>"
                            link is ViewFinanceDimension2
                            display as link
            column3
                Active
        three column distributed
            column1
                Priority
            column2
                Percent
            column3
                paragraph
                    FundedAmount
                    ProjectContract.Currency
                        no label
                        display as text
        three column distributed
            column1
                FundingGroup
            column2
                paragraph
                    BillingGroup
                    BillingGroup.Description
                        no label
                        display as text
            column3
                paragraph
                    visible when (FinanceDimension2.IsExternalGrantOrCustomer)
                        CustomerType
                        CompanyCustomerGroup.InvoiceTypeCode
                        CompanyCustomerGroup.InvoiceTypeCode.Description
                            no label
                            display as text
        visible when (FinanceDimension2.IsExternalGrantOrCustomer)
            three column distributed
                column1
                    paragraph
                        CompanyCustomerGroup.CustomerBillTo
                            label is "BillTo"
                        CompanyCustomerGroup.CustomerBillTo.Name
                            no label
                            display as text
                column2
                    BillToEmailAddress
                column3
                    InvoiceIssueMethod
            three column
                visible when (ProjectContract.UseIDM)
                    column1
                        paragraph
                            ProjectInvoiceIDMEmailTemplate
                                select is ProjectContractInvoiceIDMEmailTemplate
                            button of "ViewEmailFormat"
                                link is external "<ProjectInvoiceIDMEmailTemplate.RetrieveUrl>"
                        paragraph
                            ProjectInvoiceReversalIDMEmailTemplate
                                select is ProjectContractInvoiceReversalIDMEmailTemplate
                            button of "ViewEmailFormat"
                                link is external "<ProjectInvoiceReversalIDMEmailTemplate.RetrieveUrl>"
                    column2
                        paragraph
                            ProjectInvoiceIDMEmailSubject
                            visible when (ProjectInvoiceIDMEmailSubject entered)
                                text of "Sample:<DerivedProjectInvoiceIDMEmailSubject>"
                        paragraph
                            ProjectInvoiceReversalIDMEmailSubject
                            visible when (ProjectInvoiceReversalIDMEmailSubject entered)
                                text of "Sample:<DerivedProjectInvoiceReversalIDMEmailSubject>"
            three column distributed
                visible when (FinanceDimension2.IsInforTax)
                    column1
                        paragraph
                            BuyerTaxCode
                            BuyerTaxCode.Description
                                no label
                                display as text
                    column2
                        TaxCalculationOption
            three column distributed
                visible when (FinanceDimension2.IsVertex)
                    paragraph
                        BuyerTaxLocation
                        BuyerTaxLocation.Description
                            no label
                            display as text
                        BuyerTaxLocation.TaxCode
                            display as text
                        BuyerTaxLocation.AddressCode
                            display as text
                        TaxCalculationOption
            four column
                visible when (FinanceDimension2.IsOneSource)
                    column1
                        BuyerTaxLocation
                    column2
                        BuyerTaxLocation.Description
                            no label
                            display as text
                    column3
                        BuyerTaxLocation.TaxCode
                            display as text
                    column4
                        BuyerTaxLocation.AddressCode
                            display as text
        visible when (FinanceDimension2.IsExternalGrantOrCustomer)
            two column distributed
                column1
                    paragraph
                        TermsCode
                        TermsCode.Description.Desc[1]
                            no label
                            display as text
        two column distributed
            visible when (FinanceDimension2.CompanyCustomerGroup.ReceivableCompany.GeneralLedgerCompany.RequireHSNSACCode)
                column1
                    paragraph
                        HSNSACCode
                        HSNSACCode.Description
                            no label
                            display as text
        two column distributed
            column1
                PostingProject
                    label is "DefaultMilestone<FinanceEnterpriseGroup.ProjectLabel>"
                    select is ProjectContract.PostingProjectsRel.ContractPostingProjectSelect
            column2
                InvoiceMinimum
        two column distributed
            visible when (FinanceDimension2.IsExternalGrantOrCustomer)
                column1
                    paragraph
                        ProjectJurisdictionCode
                            show up to 5 characters
                        ProjectJurisdictionCode.Description
                            no label
                            display as text
                column2
                    CustomerPoNumber
        header2 of "RevenueAccounts"
        single column
            RevenueCodeBlockOption
        two column distributed
            RevenuePartialAccount
                compact format
            RevenueFullAccount
                compact format
        visible when (not RevenueCodeBlockOption.None)
            single column
                RevenueAdjCodeBlockOption
            two column distributed
                RevenueAdjPartialAccount
                    compact format
                RevenueAdjFullAccount
                    compact format
        visible when (FinanceDimension2.IsExternalGrantOrCustomer)
            visible when (RevenueRecognitionSeparate)
                header2 of "ExternalFunding"
                single column
                    ClearingProject
                        label is "Clearing<FinanceEnterpriseGroup.ProjectLabel>"
                        select is ProjectContract.PostingProjectsRel.ContractPostingProjectSelect
                two column distributed
                    BilledUnearnedAccount
                        compact format
                    EarnedUnbilledAccount
                        compact format
            two column distributed
                PassThroughAccount
                    compact format
                AdvanceBillFullAccount
                    compact format
            visible when (CompanyCustomerGroup.ReceivableCompany.DetailedInvoiceAccrualDistributions)
                single column
                    InvoiceFinanceCodeBlock
                        compact format
        visible when (FinanceDimension2.FundingType.InternalFund)
            visible when (RevenueRecognitionSeparate)
                visible when (not RevenueCodeBlockOption.None)
                    header2 of "InternalFunding"
                    single column
                        ClearingProject
                            label is "Clearing<FinanceEnterpriseGroup.ProjectLabel>"
                            select is ProjectContract.PostingProjectsRel.ContractPostingProjectSelect
                    two column distributed
                        RevenueClearingAccount
                            compact format
                        OffsetAccount
                            compact format
            visible when (not RevenueRecognitionSeparate)
                visible when (not RevenueCodeBlockOption.None)
                    header2 of "InternalFunding"
                    single column
                        OffsetAccount
                            compact format
        visible when (FinanceDimension2.FundingType.IntercompanyBilling)
            header2 of "IntercompanyBilling"
            three column distributed
                column1
                    FromCompany
                column2
                    FromCompany.Name
                        display as text
                column3
                    IntercompanyBillingRechargeItem
                        label is "RechargeItem"
            single column
                OffsetAccount
                    label is "PayablesExpenseFinanceStructure"
                    compact format
        header2 of "EligibilityCriteria"
        single column
            SelectedProjectsOnly
                label is "IncludeSelectedProjectsOnly"
        visible when (FrontEndSplitsEnabled)
            two column distributed
                FESEligibilityGroup
                FESEligibilityGroupOption
                    label is "EligibilityGroupOption"
        two column distributed
            ExpenseGroup
            ExpenseGroupOption
        visible when (FinanceDimension2.IsExternalGrantOrCustomer)
            header2 of "Allowance"
            three column distributed
                AllowancePercent
                AllowanceProject
                    label is "Allowance<FinanceEnterpriseGroup.ProjectLabel>"
                    select is ProjectContract.PostingProjectsRel.ContractPostingProjectSelect
                AllowanceTaxable
            single column
                AllowanceFullAccount
                    compact format
        visible when (FinanceDimension2.IsExternalGrantOrCustomer)
            header2 of "Retainage"
            three column distributed
                column1
                    RetainagePercent
                column2
                    RetainageMaximum
                column3
                    RetainageProject
                        label is "Retainage<FinanceEnterpriseGroup.ProjectLabel>"
                        select is ProjectContract.PostingProjectsRel.ContractPostingProjectSelect
                        show up to 15 characters
            three column distributed
                visible when (not ProjectContract.IsMilestoneType)
                    column1
                        RetainageExpenseGroup
                    column2
                        RetainageGroupOption
            single column
                RetainageCodeBlock
                    compact format
