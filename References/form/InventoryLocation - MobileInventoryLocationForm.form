MobileInventoryLocationForm is a CompositeForm
    use for action CreateInventoryLocation
    title is "InventoryLocation"
    context form is InventoryLocationContext
    show panel control on left
    Actions
        action is CreateInventoryLocation
            action icon is create
            hide when invalid
        action is CreateByCopy
            action icon is copy
            hide when invalid
        action is UpdateTaxCodeUsingVertex
            action icon is process
            hide when invalid
        action is previous record
            label is "_"
        action is next record
            label is "_"
        action is save
            label is "_"
        action is refresh
            label is "_"
    Address is a Panel
        form is Address
    Options is a Panel
        form is Options
    WorkOrder is a Panel
        visible when (Company.WorkOrderInUse)
        form is WorkOrder
    SubstituteLocations is a Panel
        visible when (WorkOrderLocationType.ProductionLocation)
        list is SubstituteLocationsRel.SubstituteLocationList
    AccountGroups is a Panel
        list is GeneralLedgerCategory set.GeneralLedgerCategoriesForALocation
    StorageCodes is a Panel
        list is StorageCode set.StorageCodeList
    Bins is a Panel
        list is Bin set.BinList
    BinGroups is a Panel
        list is BinGroup set.BinGroupList
    BinReorderPolicies is a Panel
        list is BinReorderPolicy set.BinReorderPoliciesForALocation
    ParItems is a Panel
        visible when (ParOrExchangeCart)
        list is BinGroupDetail set.CartParLocations
    KitItems is a Panel
        list is KitItemRel.KitItemList
    BurdenCodes is a Panel
        list is BurdenCode set.Burdens
    MultipleTaxAreas is a Panel
        visible when (VertexResultsExist)
        list is VertexTaxAreaIdResultsRel.TaxAreaResultList
    VertexError is a Panel
        visible when (VertexErrorExist)
        list is VertexWebserviceErrorRel.VertexTaxAreaIdErrorList
