POInterface is an ActionForm
    action is POInterface
    title is "ProcessPurchaseRequests"
    Layout
        single column
            header2 of "Main"
            paragraph
                Company
                Company.Name
                    no label
                    display as text
            paragraph
                DefaultBuyer
                    label is "DefaultBuyer"
                DefaultBuyer.Employee.PresentationNameSnapshot
                    no label
                    display as text
            DefaultDeliveryDays
            IncludeAllCostOptions
            MaximumPOLines
                label is "MaximumNumber_ofLines_onP\\O"
            CombineOnPOLine
            OneShipToPerPO
                label is "OneShipTo_perP\\O"
            ReleasePurchaseOrder
        blank line
        header2 of "Filters"
        single column
            SystemCode
            VendorOption
            paragraph
                Vendor.VendorAndPurchaseFrom[1].Vendor
                    label is "Vendor\\,PurchaseFrom"
                Vendor.VendorAndPurchaseFrom[1].PurchaseFromLocation
                    no label
            paragraph
                Vendor.VendorAndPurchaseFrom[2].Vendor
                    no label
                Vendor.VendorAndPurchaseFrom[2].PurchaseFromLocation
                    no label
            paragraph
                Vendor.VendorAndPurchaseFrom[3].Vendor
                    no label
                Vendor.VendorAndPurchaseFrom[3].PurchaseFromLocation
                    no label
            paragraph
                Vendor.VendorAndPurchaseFrom[4].Vendor
                    no label
                Vendor.VendorAndPurchaseFrom[4].PurchaseFromLocation
                    no label
            blank line
            paragraph
                FilterBuyer
                    label is "Buyer"
                FilterBuyer.Employee.PresentationNameSnapshot
                    no label
                    display as text
            paragraph
                BuyerOnPurchasingClasses
                    label is "BuyerPurchaseClasses"
                BuyerOnVendor
                    label is "Vendors"
                BuyerOnLocation
                    label is "Locations"
                BuyerOnRequestingLocation
                    label is "ReqLocations"
            paragraph
                FilterLocation
                    label is "ShipToLocation"
                FilterLocation.NameForPurchaseOrder
                    no label
                    display as text
                text of "Or"
                FilterReportGroup
                    label is "ReportGroup"
                FilterReportGroup.Name
                    no label
                    display as text
            paragraph
                FilterRequestingLocation
                    label is "RequestingLocation"
                FilterRequestingLocation.Name
                    no label
                    display as text
            POCodeOption
            paragraph
                FilterPOCode.POCode[1]
                    label is "P\\OCode"
                FilterPOCode.POCode[2]
                    no label
                FilterPOCode.POCode[3]
                    no label
                FilterPOCode.POCode[4]
                    no label
            BillCodeOption
                label is "BillCodeOption"
            paragraph
                FilterConsignCode.ConsignCode[1]
                    label is "BillCode"
                FilterConsignCode.ConsignCode[2]
                    no label
            paragraph
                FilterConsignCode.ConsignCode[3]
                    no label
                FilterConsignCode.ConsignCode[4]
                    no label
            FilterDeliveryDays
                label is "DeliveryDays"
            paragraph
                FilterFromPriority
                    label is "FromPriority"
                FilterToPriority
                    label is "ToPriority"
            paragraph
                FilterMajorPurchasingClass
                    label is "FromPurchasingClass"
                    show up to 10 characters
                FilterMinorPurchasingClass
                    label is "ToPurchasingClass"
                    show up to 10 characters
            FilterItem
                label is "Item"
            FilterItemType
                label is "ItemType"
            FilterManufacturerCode
                label is "Manufacturer"
                compact format
            FilterCostCode
                label is "UnitCostOrigin"
            paragraph
                FilterRequistion
                    label is "RequisitionNumber"
                text of "Or"
                FilterProcurementTemplate
                    label is "ProcurementTemplateNumber"
            blank line
        header2 of "AccountFilters"
        paragraph
            FilterAccountingUnit
                label is "AccountingUnit"
            FilterAccountingUnit.Description
                no label
                display as text
            FilterActivity
                label is "Project"
            FilterActivity.Description
                no label
                display as text
