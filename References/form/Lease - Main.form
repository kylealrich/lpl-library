                                no label
                                display as text
                        paragraph
                            DefaultDistributionAccount
                                label is "InvoiceDistribution<Company.FinanceEnterpriseGroup.ProjectLabel>"
                column3
                    paragraph
                        visible when (IsUnreleased)
