Main is a Form
    Layout
        visible when (not LeaseExists)
            five column distributed
                column1
                    Company
                    Lease
                    Description
                        label is "LeaseDescription"
                    visible when (Company.AccountingEntity.LeaseAccountingStandard.GASB)
                        TransientGASBLease
                            label is "AccountingStatement"
                            display as horizontal radio buttons
                            display as required
                    visible when (Company.AccountingEntity.LeaseAccountingStandard.IFRS)
                        TransientIFRSLease
                            label is "AccountingStatement"
                            display as text
                    visible when (Company.AccountingEntity.LeaseAccountingStandard.ASC)
                        TransientASCLease
                            label is "AccountingStatement"
                            display as text
                visible when (not Company.AccountingEntity.LeaseAccountingMethod.GASB87)
                    column2
                        visible when (LeaseExistsAndUnreleased)
                            LeaseClassification
                                label is "Classification"
                        visible when (not IsUnreleased)
                            LeaseClassification
                                label is "Classification"
                                display as text
                        visible when (LeaseExistsAndUnreleased)
                            Description
                        paragraph
                            visible when (IsUnreleased)
                                LeaseCategory
                            visible when (not IsUnreleased)
                                LeaseCategory
                                    display as text
                        paragraph
                            LeaseType
                                label is "Type"
                            LeaseType.Description
                                no label
                                display as text
                        DefaultDistributionAccount
                            label is "InvoiceDistribution<Company.FinanceEnterpriseGroup.ProjectLabel>"
                visible when (Company.AccountingEntity.LeaseAccountingMethod.GASB87)
                    column2
                        visible when (LeaseExistsAndUnreleased)
                            Description
                        paragraph
                            visible when (IsUnreleased)
                                LeaseCategory
                            visible when (not IsUnreleased)
                                LeaseCategory
                                    display as text
                        paragraph
                            LeaseType
                                label is "Type"
