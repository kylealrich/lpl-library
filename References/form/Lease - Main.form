                    paragraph
                        visible when (IsUnreleased)
                            RemainingLife
                                label is "RemainingLifeInMonthsAsOfBeginDate"
                        visible when (not IsUnreleased)
                            RemainingLife
                                label is "RemainingLifeInMonthsAsOfBeginDate"
                                display as text
                    paragraph
                        visible when (IsUnreleased)
                            TotalLife
                                label is "TotalDepreciableLifeOfAssetInMonths"
                        visible when (not IsUnreleased)
                            TotalLife
                                label is "TotalDepreciableLifeOfAssetInMonths"
                                display as text
                column5
                    paragraph
