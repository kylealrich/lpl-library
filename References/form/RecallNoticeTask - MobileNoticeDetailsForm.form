MobileNoticeDetailsForm is a Form
    title is "_"
    restrict action MobileAskAQuestion
    Layout
        two column table
            column1
                MobileTransactionDateRange
                    label is "TransactionDateRange"
                    display as text
            column2
                button of "ViewNotice"
                    link is RecallNoticeNav
                    display as link
        single column
            RecallNotice.Description
                label is "Description"
                display as text
                use text area of 3 lines
