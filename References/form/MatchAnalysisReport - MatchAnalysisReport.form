MatchAnalysisReport is an ActionForm
    action is MatchAnalysisReport
    Layout
        single column
            paragraph
                VendorGroup
                    select is VendorGroups
                text of "Or"
                Company
                    select is Companies
        single column
            paragraph
                CpyProcLevel
                    label is "ProcessLevel"
                    select is ProcessLevels
                PayGroup
                    select is PayGroups
        two column
            column1 of 65%
                paragraph
                    FrVendor
                        label is "FromVendor"
                    ToVendor
                        label is "ToVendor"
            column2 of 35%
                VenClass
                    label is "VendorClass"
        two column
            column1 of 65%
                paragraph
                    FrDate
                        label is "FromInvoiceDate"
                    ToDate
                        label is "ToInvoiceDate"
            column2 of 35%
                CurrencyCode
        two column
            column1 of 65%
                VendorSequence
                ReportSequence
                IncludeArchive
            column2 of 35%
                blank line
                ReportOption
                CurrencyOption
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                MatchAnalysisReportReport_RDG
            column2 of 25%
                MatchAnalysisReportReport_XPT
