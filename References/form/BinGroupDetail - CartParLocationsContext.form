CartParLocationsContext is a Form
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
            visible when (false)
                TransientOnline
                TransientVendor
                TransientPurchaseFromLocation
                TransientReplenishFrom
        two column
            paragraph
                InventoryLocation
                    label is "Location"
                    select is ParAndExchangeCartLocationsSelect
                InventoryLocation.Name
                    no label
                    display as text
            InventoryLocation.LocationType
                display as text
        two column
            InventoryLocation.Status
                display as text
            InventoryLocation.OneSourceDocumentToOnePO
                label is "SingleDocumentToAPurchaseOrder"
                display as text
        header3 of "ParItem"
        two column
            column1
                TransientBinSequence
                    label is "BinSequence"
                visible when (not BinGroupDetailExists)
                    paragraph
                        TransientItem
                        TransientItem.Description
                            no label
                            display as text
                visible when (BinGroupDetailExists)
                    paragraph
                        TransientItem
                            display only
                        TransientItem.Description
                            no label
                            display as text
                TransientBin
                TransientParLevel
                TransientUOM
                    raise yellow alert when (InvalidOnLocationItemUOM)
                        mouse over text is "<DerivedInvalidUOMText>"
                TransientInventoryLocationAccountGroup
                TransientChargeable
                TransientNoCharge
                TransientActive
            column2
                blank line
                TransientMinimumOrderQuantity
                TransientLeadtimeDays
                TransientSuggestedReorderPoint
                paragraph
                    TransientStandardCost
                    Company.GeneralLedgerCompany.Currency
                        no label
                        display as text
                paragraph
                    TransientManufacturer
                        compact format
                    TransientManufacturer.Description
                        no label
                        display as text
                TransientManufacturerNumber
                first ItemLocationRel.NumberOfBins
                    display as text
