ProjectReportingSettingsForm is a Form
    is primary
    title is "<FinanceEnterpriseGroup.ProjectLabel>ReportingSettings"
    Layout
        visible when (CubeOffline)
            text of "Cube_Is_Offline"
                bold
                italics
                color of red
        visible when (Scenario.Basis.BasisIsIncomplete)
            text of "Reporting_BasisDefinitionIsIncomplete"
                bold
                italics
                color of red
        header2 of "CubeStatus"
        display as text
            three column table
                column headers
                    "LastRefreshDate"
                    "LastReloadDate"
                row headers
                    "FinanceEnterpriseGroup"
                    "GlobalLedgerTotal"
                column1
                    FinanceEnterpriseGroupAnalyticCubeRel.LastRefreshDate
                        no label
                    GeneralLedgerTotalAnalyticCubeRel.LastRefreshDate
                        no label
                column2
                    FinanceEnterpriseGroupAnalyticCubeRel.LastReloadDate
                        no label
                    GeneralLedgerTotalAnalyticCubeRel.LastReloadDate
                        no label
        single column
            Actor
        two column distributed
            ProjectReportingSettings
                label is "<FinanceEnterpriseGroup.ProjectLabel>ReportingSettings"
            Description
        two column distributed
            paragraph
                ProjectStructure
                ProjectStructure.Description
                    no label
                    display as text
            paragraph
                Scenario
                    label is "BudgetScenario"
                    select is ProjectBudgets
                Scenario.Description
                    no label
                    display as text
        two column distributed
            paragraph
                Scenario.Basis
                    label is "ReportingBasis"
                    display as text
                Scenario.Basis.Description
                    no label
                    display as text
                Scenario.Basis.ReportingChart
                    label is "ReportingChart"
                    display as text
                Scenario.Basis.ReportingChart.Description
                    no label
                    display as text
            paragraph
                ReportingAccount
                    label is "Account"
                    select is ProjectAccountList
                ReportingAccount.AccountDescription
                    no label
                    display as text
                ExcludeUndistributedRetainedEarnings
        two column distributed
            paragraph
                ReportingPeriod
            paragraph
                ReportingCurrency
                    label is "ReportCurrencyOption"
                DisplayReportingCurrency
                    label is CurrencyDisplayLabel
                    display as text
                SuppressZeroRows
        visible when (ReportingPeriod.YearToDate)
            two column distributed
                paragraph
                    ReportingDate1
                        label is "PeriodEndDate"
                        select is Scenario.Basis.GeneralLedgerCalendar.YTDTopNodeRel.ChildrenRel.CalendarTreeView
                    ReportingDate.DisplayPeriod
                        label is "DisplayPeriodEndDate"
                        display as text
                paragraph
                    ComparisonPeriodEndDate
                        label is "ComparisonPeriodEndDate"
                        select is Scenario.Basis.GeneralLedgerCalendar.YTDTopNodeRel.ChildrenRel.CalendarTreeView
                    ComparisonPeriodEndDate.DisplayPeriod
                        label is "DisplayComparisonPeriodEndDate"
                        display as text
        visible when (ReportingPeriod.LifeToDate)
            two column distributed
                paragraph
                    ReportingDate2
                        label is "PeriodEndDate"
                        select is Scenario.Basis.GeneralLedgerCalendar.LTDTopNodeRel.ChildrenRel.CalendarTreeView
                    ReportingDate.DisplayPeriod
                        label is "DisplayPeriodEndDate"
                        display as text
        visible when (ReportingPeriod.PeriodActivity)
            two column distributed
                paragraph
                    ReportingDate3
                        label is "PeriodEndDate"
                        select is Scenario.Basis.GeneralLedgerCalendar.TopNodeRel.ChildrenRel.CalendarTreeView
                    ReportingDate.DisplayPeriod
                        label is "DisplayPeriodEndDate"
                        display as text
                paragraph
                    ComparisonPeriodEndDate
                        label is "ComparisonPeriodEndDate"
                        select is Scenario.Basis.GeneralLedgerCalendar.TopNodeRel.ChildrenRel.CalendarTreeView
                    ComparisonPeriodEndDate.DisplayPeriod
                        label is "DisplayComparisonPeriodEndDate"
                        display as text
        header2 of "BudgetVariancePercentSettings"
        three column table
            column headers
                "Minimum"
                "Maximum"
                "Mouse_\\Over_\\Text"
            row headers
                "Red_\\Alert"
                "Yellow_\\Alert"
                "Green_\\Alert"
            column1
                PercentOfAvailableLimitRanges.RedAlertRangeMin
                    no label
                PercentOfAvailableLimitRanges.YellowAlertRangeMin
                    no label
                PercentOfAvailableLimitRanges.GreenAlertRangeMin
                    no label
            column2
                PercentOfAvailableLimitRanges.RedAlertRangeMax
                    no label
                PercentOfAvailableLimitRanges.YellowAlertRangeMax
                    no label
                PercentOfAvailableLimitRanges.GreenAlertRangeMax
                    no label
            column3
                PercentOfAvailableLimitRanges.RedMouseOverText
                    no label
                PercentOfAvailableLimitRanges.YellowMouseOverText
                    no label
                PercentOfAvailableLimitRanges.GreenMouseOverText
                    no label
