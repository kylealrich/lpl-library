MobileBankEntityContact is a Form
    title is "BankEntityContact"
    Layout
        header2 of "Contact"
        single column
            display as text
                BankEntity.Contact
                    label is "ContactName"
                BankEntity.PhoneNumber.SubscriberNumber
                    label is "ContactNum"
                    link is "tel:<MobileNumber.MobilePhoneNumber.SubscriberNumber>"
        paragraph
            button of icon.mail
                link is "mailto:<EmailAddress>"
            button of icon.phone
                link is "tel:<PhoneNumber.SubscriberNumber>"
            button of icon.mobile
                link is "tel:<MobileNumber.MobilePhoneNumber.SubscriberNumber>"
