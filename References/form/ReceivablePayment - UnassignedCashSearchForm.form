UnassignedCashSearchForm is a SearchForm
    Layout
        four column distributed
            Company
            ReceivableProcessLevel
                label is "ProcessLevel"
            ReceivablePaymentHeader
                label is "Batch"
            PaymentNumber
        four column distributed
            ReceivablePaymentHeader.ReceivableLockBox
                label is "Lockbox"
            OriginalCurrency
                label is "Currency"
            ReferenceNumber
            ReceivableOperator
                label is "Operator"
        four column distributed
            BankEntity
            BankAccountNumber
                label is "AccountNumber"
            TransactionAmountRange.FromAmount
                label is "FromTransactionAmount"
            TransactionAmountRange.ToAmount
                label is "ToTransactionAmount"
        four column distributed
            CashCode
            BaseAmountRange.FromAmount
                label is "FromBaseAmount"
            BaseAmountRange.ToAmount
                label is "ToBaseAmount"
        four column distributed
            DepositDateRange.Begin
                label is "FromDepositDate"
            DepositDateRange.End
                label is "ToDepositDate"
            PostingDateRange.Begin
                label is "FromPostingDate"
            PostingDateRange.End
                label is "ToPostingDate"
