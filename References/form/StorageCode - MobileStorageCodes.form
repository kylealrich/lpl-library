MobileStorageCodes is a Form
    title is "StorageCode"
    Layout
        two column distributed
            paragraph
                Company
                Company.Name
                    label is "CompanyName"
                    display as text
            paragraph
                InventoryLocation
                    label is "Location"
                InventoryLocation.Name
                    label is "LocationName"
                    display as text
        paragraph
            StorageCode
            Description
            Active
        blank line
        single column
            UnitOfMeasure
                select is MobileUnitOfMeasureList
            MaximumVolume
                label is "MaximumCubic"
            MaximumWeight
            MaximumQuantity
