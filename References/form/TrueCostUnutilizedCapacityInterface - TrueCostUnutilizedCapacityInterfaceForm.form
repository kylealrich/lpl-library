TrueCostUnutilizedCapacityInterfaceForm is a Form
    is primary
    title is "UnutilizedCapacity"
    Layout
        single column
            TrueCostConfiguration
                label is "Configuration"
                display as text
            visible when (TrueCostUnutilizedCapacityInterface.RunGroup entered)
                TrueCostUnutilizedCapacityInterface.RunGroup
                    display as text
            visible when (TrueCostUnutilizedCapacityInterface.RunGroup not entered)
                TrueCostUnutilizedCapacityInterface.RunGroup
            TrueCostCapacity
            TrueCostCapacityUnutilized
            UnutilizedCapacityDimension
            visible when (HasError)
                ErrorDescription
                    display as text
