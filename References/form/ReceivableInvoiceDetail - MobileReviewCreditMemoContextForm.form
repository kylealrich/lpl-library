MobileReviewCreditMemoContextForm is a Form
    title is "ReviewCreditMemo"
    Layout
        display as text
            two column distributed
                Company
                Company.Name
                    label is "CompanyName"
            two column distributed
                Customer
                Customer.Name
                    label is "CustomerName"
            two column distributed
                ReceivableProcessLevel
                    label is "ProcessLevel"
                TransientSignedTransactionAmount
            two column distributed
                BatchNumber
                    label is "Batch"
                ReceivableInvoiceDetail.Invoice
            two column distributed
                ReceivableInvoiceDetail.PaymentSeq
                    label is "Sequence"
                GeneralLedgerDate
            two column distributed
                Status
                OpenAmount
