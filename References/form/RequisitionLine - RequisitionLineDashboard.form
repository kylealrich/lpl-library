RequisitionLineDashboard is a CompositeForm
    title is "Requisition<Requisition>Line<RequisitionLine>Details"
    context form is RequisitionLineDashboardContext
        background is theme color
    restrict action Processed.UpdateLineReturn
    restrict action MobileCopyRequisitionLine
    restrict action ModalCopyMultipleRequisitionLines
    restrict action NonModalCopyRequisitionLine
    restrict action Unreleased.RSSLPLUpdateRequisitionLine
    restrict action RSSLPLAddNewSpecialServiceToNewRequisition
    restrict action RSSLPLAddNewSpecialServiceToRequisition
    restrict action ModalCopyMultipleRequisitionLinesForSpecialAndService
    restrict action NonModalCopyRequisitionLineForSpecialAndService
    restrict action ModalCopyRequisitionLineForSpecialAndService
    restrict action MobileCopyRequisitionLineForSpecialAndService
    restrict action Unreleased.RSSLPLUpdateRequisitionLineQTY
    restrict action RSSLPLModalCopyLineOptionPromptForRecentlyRequestedItems
    restrict action RSSLPLModalCopyLineOptionPromptForSpecialAndService
    Actions
        navigation is LinePrimary
            action icon is open
            label is "_"
    Details is a MultiListPanel
        list is WarehouseDemandLinesDrillRel.DashboardDemandLinesForDocument
            visible when (ShowDemandLinesPanel)
        list is WarehouseShipmentLinesRel.DashboardShipmentLines
            visible when (ShowShipmentLinesPanel)
        list is PurchaseOrderLineSourcesRel.DashboardPurchaseOrderLineList
            visible when (SourceRecordExists)
        list is RelatedContractRel.ReqDashboardContracts
            visible when (ContractOutput)
        list is SourcingEventLineSourceRel.RequisitionDashboardSourcingEventLines
            visible when (SourcingEventExists)
        list is RequisitionLineDistributionsRel.DashboardRequisitionLineDistributions
            visible when (DistributionExists)
        list is RequisitionLineAddOnCharge set.DashboardMiscAddOnCosts
            visible when (LineAddOnChargeExists)
        list is RequisitionLineCommentRel.DashboardRequisitionLineComments
            visible when (HasComments)
        list is RequisitionLineError set.DashboardLineErrors
            visible when (LineErrorsExist)
    ApprovalHistory is a Panel
        visible when (HasWorkunit)
        form is ApprovalHistory
    PurchaseOrderQuantities is a Panel
        visible when (PurchaseOrderAndWarehouse)
        form is PurchaseOrderQuantitiesDashboardForm
    POQuantities is a Panel
        title is "PurchaseOrderQuantities"
        visible when (IsCreatePurchaseOrderNotContract)
        form is PurchaseOrderQuantitiesDashboardForm
    WarehouseQuantities is a Panel
        visible when (PurchaseOrderAndWarehouse)
        form is WarehouseQuantitiesDashboardForm
    WHQuantities is a Panel
        title is "WarehouseQuantities"
        visible when (not IsCreatePurchaseOrder)
        form is WarehouseQuantitiesDashboardForm
    ContractQuantities is a Panel
        visible when (ContractOutput)
        form is ContractQuantitiesDashboardForm
