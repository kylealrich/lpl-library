ContractGroupSetup is a CompositeForm
    is primary
    title is "ContractGroupAndRelatedSetup"
    context form is ContractGroupContext
        background is theme color
    Actions
        action is save
            label is "_"
        action is RecalculateTotalLineMaximumAmountToIncludeClosedLines
            hide when invalid
    Setup is a Panel
        form is ContractGroup
    EmailAndPortalMessageConfiguration is a Panel
        form is ContractGroupEmailConfiguration
    FeaturesInUse is a Panel
        form is ContractFeaturesUsed
    ContractTypes is a Panel
        list is ContractType set.ContractTypes
    ContractClassifications is a Panel
        list is ContractClassification set.ContractClassifications
    ContractClassificationGroups is a Panel
        list is ContractClassificationGroup set.ClassificationGroups
    ContractRebateTypes is a Panel
        list is ContractRebateType set.primary
    PricingGroups is a Panel
        list is PricingGroup set.PricingGroupsInline
    ContractDocumentTemplates is a Panel
        list is ContractDocumentTemplate set.ContractDocumentTemplates
    AddendumDocumentTemplates is a Panel
        list is AddendumDocumentTemplate set.AddendumDocumentTemplates
    ContractGroupApprovers is a Panel
        list is ContractGroupApprover set.ContractGroupApproverList
    ContractApproverCategories is a Panel
        list is ContractApproverCategory set.ContractApproverCategoryList
