VendorDetailSubreport is a SummaryForm
    title is "ItemHistoryByVendor"
    Layout
        two column
            column1
                text of "DetailForVendor:_<VendorName>"
                    header4
                    align as label
                VendorClass
                    display when blank
                URLAddress
                    label is "Vendor_\\U\\R\\L"
                    display when blank
                ContactName
                    label is "Contact"
                    display when blank
                EmailAddress
                    display when blank
                PhoneNumber.SubscriberNumber
                    label is "PhoneNumber"
                    display when blank
                MobileNumber.MobilePhoneNumber.SubscriberNumber
                    label is "MobileNumber"
                    display when blank
                FaxNumber.SubscriberNumber
                    label is "FaxNumber"
                    display when blank
            column2
                blank line
                VendorAddressLine1
                    label is "Address"
                    display when blank
                VendorAddressLine2
                    no label
                    display when blank
                VendorAddressLine3
                    no label
                    display when blank
                VendorAddressLine4
                    no label
                    display when blank
                VendorCity
                    label is "City,_\\State,_\\Zip"
                    display when blank
                VendorCounty
                    label is "County"
                    display when blank
                VendorCountry
                    label is "Country/\\Jurisdiction"
                    display when blank
        visible when (VendorDiversityRelExists)
            form is Vendor.VendorDetailDiversityCertificationSumForm
