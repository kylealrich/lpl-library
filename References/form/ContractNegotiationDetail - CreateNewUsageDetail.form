CreateNewUsageDetail is an ActionForm
    action is NewUsage
    title is "CalculateNewUsageDetail"
    Layout
        header3 of "EnterDateRangeForItemUsage"
        single column
            PrmDateRange
        header3 of "EnterCompanyAndLocationsForItemUsage(Optional)"
        single column
            PrmCompany
                label is "Company"
        paragraph
            PrmLocation
                label is "InventoryLocation"
            PrmRequestingLocation
                label is "OrRequestingLocation"
        header3 of "EnterReceiptOrInvoiceOption"
        single column
            PrmReceiptOrInvoiceUsage
                label is "ItemUsageCalculationOption"
        header3 of "EnterPercentToAdjustUsageBy"
        single column
            PrmAdjustmentPercent
                label is "AdjustmentPercent"
        header3 of "Annualization"
        single column
            PrmAnnualize
                label is "AnnualizeUsage"
