MobileReceivingPutawayContext is a Form
    Layout
        display as text
            MobileCompanyDisplay
                label is "Company"
            button of "<PurchaseOrder>"
                link is MobilePODetails
                label is "PurchaseOrder"
                display as link
            button of "<PurchaseOrderReceipt>"
                link is MobileReceiptDetails
                label is "PurchaseOrderReceipt"
                display as link
            PutawayStatus
                label is "Putaway_Status"
                display as tag
                    color of green when (PutawayStatus.ConfirmedPutawayFeedback)
                    color of blue when (PutawayStatus.ForReceivingPutaway)
                    color of gray when (PutawayStatus.NotForPutaway)
