SubSystemUpdate is an ActionForm
    action is SubSystemUpdate
    Layout
        single column
            paragraph
                Company
                    select is Companies
                Company.Name
                    no label
                    display as text
            paragraph
                ProcessLevel
                    select is ProcessLevels
                ProcessLevel.Name
                    no label
                    display as text
            text of "Or"
            paragraph
                CompanyGroup
                    label is "CompanyGroup"
                CompanyGroup.Description
                    no label
                    display as text
            CertNbrOption
                label is "PrintCertificationNumber"
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                SubSystemUpdateReport_RDG
                ARLLSubSystemUpdateTransactionListing_RDG
            column2 of 25%
                SubSystemUpdateReport_XPT
                ARLLSubSystemUpdateTransactionListing_XPT
