MobilePaymentEntry is a Form
    title is "PaymentEntry"
    Layout
        two column distributed
            paragraph
                CashCode
                    display as text
                CashCode.Description
                    no label
                    display as text
        two column distributed
            CashLedgerPayablesPayment.BankTransactionCode
                display as text
                select is CashPayments
            CashLedgerPayablesPayment.BankTransactionCode.Description
                no label
                display as text
        two column distributed
            PaymentAmount
            CashCode.Currency
                label is "Currency"
                display as text
        single column
            TransactionNumber
        two column distributed
            visible when (LastElectronicPaymentSerialNumber entered)
                LastElectronicPaymentSerialNumber
                    label is "ElectronicPaymentSerialNumber"
                    display as text
            CashLedgerTransactionIdentifier
                display as text
        two column distributed
            paragraph
                PaymentStatus
                    display as text
                Status
                    display as text
                visible when (IsRejected)
                    CancelRequestType
                        label is "CancelType"
                        display as text
        single column
            PaymentDate
                display as text
