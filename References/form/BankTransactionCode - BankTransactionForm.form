BankTransactionForm is a CompositeForm
    is primary
    title is "BankTransactionCode<BankTransactionCode>"
    context form is BankTransactionContext
        background is theme color
    CashPayment is a Panel
        valid when (IsCashPayment)
        form is UpdateCashPaymentForm
    PayablesElectronicTransferID is a Panel
        visible when (PayablesElectronicTransferIDsAreApplicable)
        list is PayablesElectronicTransferID set.ElectronicTransferIdentifications
    BillOfExchangePayment is a Panel
        valid when (IsBillOfExchangePayment)
        form is UpdateBillOfExchangePaymentForm
    BankServiceTransaction is a Panel
        valid when (IsBankServiceTransaction)
        form is UpdateBankServiceForm
