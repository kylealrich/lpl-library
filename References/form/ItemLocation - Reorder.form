Reorder is a Form
    Layout
        visible when (false)
            TransientOnlineReorder
        three column distributed
            ReorderPolicy
            ReorderDocument
            ReorderPointCode
        three column distributed
            visible when (not HasItemLocationStockUOM)
                ReorderPoint
            visible when (HasItemLocationStockUOM)
                TransientReorderPointInLocationStockUOM
                    label is "ReorderPoint"
            AutomaticPurchaseOrder
            ReceivingDays
        three column distributed
            ReorderQuantityCode
            visible when (not HasItemLocationStockUOM)
                ReorderQuantity
            visible when (HasItemLocationStockUOM)
                TransientReorderQuantityInLocationStockUOM
                    label is "ReorderQuantity"
            OrderMultiple
        three column distributed
            visible when (not HasItemLocationStockUOM)
                paragraph
                    MaximumOrderQuantity
                    MinimumOrderQuantity
                SafetyStock
                EconomicOrderQuantity
            visible when (HasItemLocationStockUOM)
                paragraph
                    TransientMaximumOrderInLocationStockUOM
                        label is "MaximumOrderQuantity"
                    TransientMinimumOrderInLocationStockUOM
                        label is "MinimumOrderQuantity"
                TransientSafetyStockInLocationStockUOM
                    label is "SafetyStock"
                TransientEconomicOrderInLocationStockUOM
                    label is "EconomicOrderQuantity"
        header2 of "Leadtime"
        three column distributed
            LeadtimeControl
                label is "Control"
            LeadtimeDays
                label is "AverageDays"
            paragraph
                LastLeadtime
                    display as text
                PreviousLeadtime
                    display as text
