FundingSourceCreateComposite is a CompositeForm
    use for action CreateFundingSource
    title is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
    context form is FinanceDimension2CreateContext2
        background is theme color
    MainPanel is a Panel
        title is "Main"
        form is FundingSourceCreateMainForm
    ProjectFunding is a Panel
        title is "<FinanceEnterpriseGroup.FinanceDimension2Label>"
        visible when (CreateDimensionType.Posting)
        form is ProjectFundingCreateForm
    ReceivableCustomer is a Panel
        title is "ReceivableCustomer"
        visible when (FundingType.ExternalGrant)
        form is ReceivableCustomerForm
    ReceivableCustomer2 is a Panel
        title is "ReceivableCustomer"
        visible when (FundingType.ExternalCustomer)
        form is ReceivableCustomerForm2
    FundingSourceProject is a Panel
        visible when (IsProjectInvoicing)
        list is FundingSourceProject set.FundingSourceProjectList
            helper list is FundingSourcePostingProjectsRel.FundingSourceProjectSelect
                action is IncludeFundingSourceProject
                    invoked.PrmFinanceDimension2 = FinanceDimension2
    Deliverables is a Panel
        title is "Deliverables"
        visible when (IsProjectInvoicing)
        list is ProjectFundingSourceDeliverable set.DeliverableList
    GrantReporting is a Panel
        title is "GrantReporting"
        visible when (IsExternalOrIntercompanyBilling)
        form is GrantReportingForm
    FederalFinancialReports is a Panel
        title is "FederalFinancialReports"
        visible when (IsExternalOrIntercompanyBilling)
        list is GrantReportingSettings set.FederalFinancialReportsListList
    UserFieldsPanel is a Panel
        title is "UserFields"
        form is UserFieldsForm
