ReturnedPaymentDistributionContextForm is a Form
    title is "ReturnedPaymentDistribution"
    Layout
        display as text
            two column distributed
                column1
                    paragraph
                        CashCode.CashCodeAndDescription
                            label is "CashCode"
                        CashCode.Currency
                            no label
                    CashLedgerReturnedPaymentRel.TransactionNumber
                    paragraph
                        CashLedgerReturnedPaymentRel.BankAccountReturnAmount
                            label is "ReturnAmount"
                        visible when (CashLedgerReturnedPaymentRel.IsUnreleased)
                            CashLedgerReturnedPaymentRel.AppliedDistributionBankAmount
                                label is "AppliedAmount"
                            CashLedgerReturnedPaymentRel.BalanceAmount
                                label is "Balance"
                column2
                    CashLedgerGLDistribution.BankTransactionCode.BankTransactionCodeName
                        label is "TransactionCode"
                    paragraph
                        CashLedgerReturnedPaymentRel.Company.OrigCompany.RepresentativeText
                            label is "Company"
                        CashLedgerReturnedPaymentRel.Company.OrigCompany.Currency
                            no label
