IONOutQueueForm is a Form
    title is "IONOutQueue"
    disable all actions
    Layout
        single column
            button of "ViewAllStatus"
                link is IONOutQueueNav
                display as link
            button of "View_\\New_\\Outbox_\\Request"
                link is IONOutQueueNewNav
                label is "New"
                display as link
            button of "View_\\Failed_\\Outbox_\\Request"
                link is IONOutQueueFailedNav
                label is "Failed"
                display as link
            button of "View_\\PendingRetry_\\Outbox_\\Request"
                link is IONOutQueuePendingRetryNav
                label is "PendingRetry"
                display as link
