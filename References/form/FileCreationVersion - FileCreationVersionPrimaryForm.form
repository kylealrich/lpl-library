FileCreationVersionPrimaryForm is a Form
    is primary
    title is "Version"
    Layout
        single column
            FileCreationVersion
                label is "Version"
                display as text
        three column
            column1
                visible when (SetupExportFile entered)
                    paragraph
                        button of "<SetupExportFile.Title>"
                            view SetupExportFile.File
                            label is "SetupExport"
                            display as link
                    paragraph
                        SetupExportTimeStamp
                            label is "AsOf"
                            display as text
            column2
                visible when (MappingsDocument entered)
                    paragraph
                        button of "<MappingsDocument.Title>"
                            link is MappingsDocumentLink
                            label is "Mappings"
                            display as link
                    paragraph
                        MappingsDocumentTimeStamp
                            label is "AsOf"
                            display as text
            column3
                visible when (CompareReport entered)
                    paragraph
                        button of "<CompareReport.Title>"
                            link is CompareReportLink
                            label is "CompareReport"
                            display as link
                    paragraph
                        CompareReportTimeStamp
                            label is "AsOf"
                            display as text
        single column
            Comments
