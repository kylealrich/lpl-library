DeliveryAndPutawayResultForm is a CompositeForm
    context form is DeliveryAndPutawayResultContext
    DeliveryHeadings is a Panel
        visible when (ReportType.DeliveryTicketAndBackorderReport)
        list is PurchaseOrderResultHeading set.DeliveryHeadingsList
    BackorderHeadings is a Panel
        visible when (ReportType.DeliveryTicketAndBackorderReport)
        list is PurchaseOrderResultHeading set.BackorderHeadingsList
    PutawayHeadings is a Panel
        visible when (ReportType.PurchaseOrderPutawayReport)
        list is PurchaseOrderResultHeading set.PutawayHeadingsList
