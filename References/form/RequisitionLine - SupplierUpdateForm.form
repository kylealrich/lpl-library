SupplierUpdateForm is an ActionForm
    action is SupplierUpdate
    title is "UpdateRequisitionLine"
    disable field options
    Layout
        visible when (not Requisition.ForCapitatedProcedure)
            two column distributed
                column1
                    VendorItem
                        select is VendorItemsForASupplier
                    Description
                    EnteredUOM
                        label is "UOM"
                        select is SupplierUnitsOfMeasure
                column2
                    ManufacturerNumber
                        label is "ManufacturerPart"
                    Quantity
                    TransactionUnitCost
                        label is "UnitCost"
                    paragraph
                        ConsignCode
                            label is "BillCode"
                        text of "DefaultBillCodeIs<Requisition.DefaultBillCode>"
        visible when (Requisition.ForCapitatedProcedure)
            two column distributed
                column1
                    VendorItem
                        display as text
                    Description
                        display as text
                    EnteredUOM
                        label is "UOM"
                        display as text
                    visible when (IsACapitatedProcedure)
                        visible when (PrimaryOrRevisionProcedure)
                            visible when (ProcedureHasComponents)
                                CapitatedProcedureLevel
                                    display as text
                            visible when (not ProcedureHasComponents)
                                CapitatedProcedureLevel
                        visible when (PrimaryProcedure)
                            single column
                                text of "ProcedureIsPrimary"
                                    bold
                        visible when (RevisionProcedure)
                            single column
                                text of "ProcedureIsRevision"
                                    bold
                column2
                    ManufacturerNumber
                        display as text
                    Quantity
                    TransactionUnitCost
                        label is "UnitCost"
                        display as text
                    paragraph
                        ConsignCode
                            label is "BillCode"
                        text of "DefaultBillCodeIs<Requisition.DefaultBillCode>"
        visible when (not ItemType.Service)
            visible when (ItemType.Service)
                TransientVendor
        two column distributed
            column1
                visible when (CanDisplaySerialNumber)
                    TransientSerialNumber
                        label is "Serial"
                visible when (CanDisplayGTIN)
                    TransientGTIN
                        label is "GTIN"
                visible when (ItemExists)
                    Item
                        label is "ItemMasterItem"
            column2
                visible when (CanDisplayManufacturingDate)
                    TransientManufacturingDate
                        label is "ManufacturingDate"
                paragraph
                    visible when (TransientGTINEntered)
                        visible when (CanDisplayUDI)
                            TransientUniqueDeviceId
                                label is "UDI"
                                display as text
                    visible when (CanDisplayExpirationDate)
                        TransientExpirationDate
                            label is "ExpirationDate"
                visible when (CanDisplayLot)
                    TransientLot
                        label is "Lot"
