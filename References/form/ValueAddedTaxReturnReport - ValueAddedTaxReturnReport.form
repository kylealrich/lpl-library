ValueAddedTaxReturnReport is an ActionForm
    action is ValueAddedTaxReturnReport
    Layout
        paragraph
            TaxCalendar.TaxEntity
                label is "TaxEntity"
            TaxCalendar.TaxEntity.Name
                no label
                display as text
        paragraph
            TaxCalendar.TaxYear
                label is "TaxYear"
        paragraph
            FromPeriod
                label is "FromTaxPeriod"
        paragraph
            ToPeriod
                label is "ToTaxPeriod"
        paragraph
            VatRptId
                label is "ReportIdentification"
                compact format
                select is TaxCalendar.TaxEntity.VatReportIDForCountry set.VATReportIDs
            VatRptId.Description
                no label
                display as text
        paragraph
            UpdateOption
        paragraph
            Reprint
        paragraph
            VATReverseIndicator
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                ValueAddedTaxReturnReportReport_RDG
            column2 of 25%
                ValueAddedTaxReturnReportReport_XPT
