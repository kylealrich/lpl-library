AM300AiaOut_Translation is a Form
    is primary
    Layout
        paragraph
            Base_BusinessClass.UniqueID
                display as text
        single column
            IsoLocale
                compact format
                raise yellow alert when (VerifyTranslations)
                    mouse over text is untranslatable:"The base data has been changed but this translation has not been verified"
        single column
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.AiaDescription
                label is untranslatable:"Aia Description"
            AiaDescription
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.AiaToDescription
                label is untranslatable:"Aia To Description"
            AiaToDescription
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.AiaVendorName
                label is untranslatable:"Aia Vendor Name"
            AiaVendorName
                label is untranslatable:"Translated"
            blank line
            header4 of untranslatable:"Translation for {0}"
            Base_BusinessClass.AiaToVendorName
                label is untranslatable:"Aia To Vendor Name"
            AiaToVendorName
                label is untranslatable:"Translated"
