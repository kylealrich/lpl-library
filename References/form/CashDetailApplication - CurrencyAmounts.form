CurrencyAmounts is a Form
    Layout
        header2 of "AppliedAmount"
        visible when (false)
            DistributionAmount.AssignToCurrency
        display as text
            six column distributed
                column1
                    DistributionAmount.CurrencyAmount
                        label is "TransactionAmount"
                    DistributionAmount.FunctionalAmount.EnteredCurrencyAmount
                        label is "FunctionalAmount"
                    visible when (DistributionAmount.AccountingEntity.AlternateCurrencyExists)
                        DistributionAmount.AlternateAmount.EnteredCurrencyAmount
                            label is "AlternateAmount"
                    visible when (DistributionAmount.AccountingEntity.AlternateCurrency2Exists)
                        DistributionAmount.AlternateAmount2.EnteredCurrencyAmount
                            label is "AlternateAmount2"
                    visible when (DistributionAmount.AccountingEntity.AlternateCurrency3Exists)
                        DistributionAmount.AlternateAmount3.EnteredCurrencyAmount
