ChangeOrderPrintCompositeForm is a CompositeForm
    title is "ChangeOrderForContract<Contract.RepresentativeText>"
    context form is ChangeOrderPrintContext
        background is theme color
    Actions
        action is export to pdf
            action icon is print
            label is "PrintChangeOrder"
        navigation is FullContractThisVersion
            label is "PrintWholeContract"
            hide when invalid
        navigation is ChangeOrderDocumentation
            label is "PrintChangeOrderDocument"
            hide when invalid
        navigation is AdditionalChangeOrderDocuments
            hide when invalid
        navigation is ContractLink
            label is "GoToContract"
    ChangeOrderDetail is a Panel
        visible when (IsServicePurchaseType)
        list is ContractChangeOrder set.ChangeOrderPrintList
    ChangeOrderDetailNonService is a Panel
        title is "ChangeOrderDetail"
        visible when (not IsServicePurchaseType)
        list is ContractChangeOrder set.ChangeOrderPrintListForNonService
