PendingTemplateUpdate is a Form
    is primary
    title is "PendingObligation"
    Layout
        display as text
            three column distributed
                column1
                    Scenario.ScenarioDescription
                        label is "Scenario"
                    BudgetTemplate.BudgetTemplateDescription
                        label is "BudgetTemplate"
                column2
                    paragraph
                        Amount
                        BudgetTemplateGroupTotal.BudgetGroup.DerivedCurrency
                            no label
                    BudgetTemplateGroupTotal.GeneralLedgerCalendarPeriod
                        label is "Period"
                column3
                    HasError
                        display as tag
                            color of red when (HasError)
                            color of green when (not HasError)
            header2 of "FinanceStructure"
            four column distributed
                column1
                    BudgetTemplateGroupTotal.BudgetGroup.AccountingEntity
                        label is actor.context.FinanceEnterpriseGroup.AccountingEntityLabel
                    visible when (actor.context.FinanceEnterpriseGroup.AccountingUnitSelected)
                        BudgetTemplateGroupTotal.BudgetGroup.AccountingUnit
                            label is actor.context.FinanceEnterpriseGroup.AccountingUnitLabel
                    BudgetTemplateGroupTotal.BudgetGroup.GeneralLedgerChartAccount
                        label is actor.context.FinanceEnterpriseGroup.AccountLabel
                    visible when (actor.context.FinanceEnterpriseGroup.ProjectEntry)
                        BudgetTemplateGroupTotal.BudgetGroup.Project
                            label is actor.context.FinanceEnterpriseGroup.ProjectLabel
                column2
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension1Entry)
                        BudgetTemplateGroupTotal.BudgetGroup.FinanceDimension1
                            label is actor.context.FinanceEnterpriseGroup.FinanceDimension1Label
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension2Entry)
                        BudgetTemplateGroupTotal.BudgetGroup.FinanceDimension2
                            label is actor.context.FinanceEnterpriseGroup.FinanceDimension2Label
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension3Entry)
                        BudgetTemplateGroupTotal.BudgetGroup.FinanceDimension3
                            label is actor.context.FinanceEnterpriseGroup.FinanceDimension3Label
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension4Entry)
                        BudgetTemplateGroupTotal.BudgetGroup.FinanceDimension4
                            label is actor.context.FinanceEnterpriseGroup.FinanceDimension4Label
                column3
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension5Entry)
                        BudgetTemplateGroupTotal.BudgetGroup.FinanceDimension5
                            label is actor.context.FinanceEnterpriseGroup.FinanceDimension5Label
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension6Entry)
                        BudgetTemplateGroupTotal.BudgetGroup.FinanceDimension6
                            label is actor.context.FinanceEnterpriseGroup.FinanceDimension6Label
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension7Entry)
                        BudgetTemplateGroupTotal.BudgetGroup.FinanceDimension7
                            label is actor.context.FinanceEnterpriseGroup.FinanceDimension7Label
                column4
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension8Entry)
                        BudgetTemplateGroupTotal.BudgetGroup.FinanceDimension8
                            label is actor.context.FinanceEnterpriseGroup.FinanceDimension8Label
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension9Entry)
                        BudgetTemplateGroupTotal.BudgetGroup.FinanceDimension9
                            label is actor.context.FinanceEnterpriseGroup.FinanceDimension9Label
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension10Entry)
                        BudgetTemplateGroupTotal.BudgetGroup.FinanceDimension10
                            label is actor.context.FinanceEnterpriseGroup.FinanceDimension10Label
