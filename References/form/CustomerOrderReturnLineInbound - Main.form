Main is a Form
    Layout
        two column distributed
            paragraph
                InventoryLocation
                InventoryLocation.Name
                    no label
                    display as text
            paragraph
                CustomerOrderReturnLineInbound
                    label is "ReturnInboundLine"
                Status
        two column distributed
            paragraph
                Customer
                    select is CustomerContextCompanySelectList
                Customer.Name
                    no label
                    display as text
            paragraph
                CustomerShipTo
                CustomerShipTo.Name
                    no label
                    display as text
        two column distributed
            OrderNbr
                label is "OrderNumber"
            OrigLineNbr
                label is "OriginalLineNumber"
        two column distributed
            paragraph
                Item
                Description
                    no label
                    display as text
            LineType
        two column distributed
            Quantity
            StockOnHandQuantity
        two column distributed
            InvcCwQty
                label is "InvoiceCatchWeightQuantity"
            InventoryCatchWeightQuantity
        two column distributed
            DetailQuantity
            DetailCatchWeightQuantity
                label is "DetailCatchWeight"
        two column distributed
            StockUOM
                label is "UOM"
            SellUom
                label is "SellU\\O\\M"
                select is ValidSellUnitsOfMeasure
        two column distributed
            OrigUnitCost
                label is "OrignialUnitCost"
            EntUnitCost
                label is "EnteredUnitCost"
        two column distributed
            UnitCost
            AverageCost
        two column distributed
            StandardCost
            SellToStock
                label is "Sell_toStock"
