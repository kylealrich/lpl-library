InventoryLocationReplenishmentReport is an ActionForm
    action is InventoryLocationReplenishmentReport
    Layout
        paragraph
            Company
            Company.Name
                no label
                display as text
        blank line
        paragraph
            Location
            Location.Name
                no label
                display as text
        single column
            text of "\\or"
                bold
        paragraph
            ReportGrp
                label is "ReportGroup"
            ReportGrp.Name
                no label
                display as text
        text of "_"
        header2 of "_"
        text of "_"
        blank line
        two column
            paragraph
                InvenMajcl
                    label is "MajorInventoryClass"
                    show up to 10 characters
                InvenMajcl.Description
                    no label
                    display as text
            paragraph
                InvenMincl
                    label is "MinorInventoryClass"
                    show up to 10 characters
                InvenMincl.Description
                    no label
                    display as text
        blank line
        single column
            UpdateOnly
                label is "UpdateOnly"
            GlPostingDate
                label is "GL_PostDate"
            EstDeliveryDate
                label is "EstimatedDeliveryDate"
            IncludeInactive
                label is "IncludeInactiveItems"
            PrintExceptionRpt
                label is "PrintExceptionReport"
            SortOption
        header2 of untranslatable:"Report Distribution"
        two column distributed
            column1 of 40%
                InventoryLocationReplenishmentReportReport_RDG
            column2 of 25%
                InventoryLocationReplenishmentReportReport_XPT
