AssetRepairComposite is a Form
    is primary
    title is "AssetRepairInterface"
    Layout
        two column
            column1
                paragraph
                    Company
                    Company.GeneralLedgerCompany.Name
                        no label
                        display as text
                InvoiceAndPurchaseOrderInformation.PayablesInvoice
                    select is PayablesInvoicesRel.ReleasedVendorInvoices
            column2
                InvoiceAndPurchaseOrderInformation.Vendor
                    select is CompanyVendorsRel.VendorSelect
                InvoiceAndPurchaseOrderInformation.PurchaseOrder
                InvoiceAndPurchaseOrderInformation.PurchaseOrderLine
        button of "CompanyInvoices"
            visible when (IsInvoice)
            link is CompanyInvoices
        Project
        header3 of "RepairDetail"
        paragraph
            Asset
                select is CompanyAssetsRel.AssetList
        paragraph
            Description
        two column
            RepairDate
            TransactionRepairCost
        two column
            TransactionRepairTax
            Currency
        paragraph
            Asset.TagNumber
