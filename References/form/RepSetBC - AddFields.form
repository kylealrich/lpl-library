AddFields is an ActionForm
    action is AddFields
    Layout
        two column
            column1
                AddPersistentFields
                AddPersistentUserFields
            column2
                ExplodeCompoundFields
                    mouse over text is "IfACompoundFieldAlreadyExistsUnexplodedItWillBeSkipped."
                visible when (ExplodeCompoundFields)
                    UseSQLName
                ReplacePeriodsWithUnderscores
        single column
            blank line
            AddPersistentFieldCubeDimensionValues
            AddPersistentUserFieldCubeDimensionValues
            visible when (not DataToExtract.CurrentData)
                AddAuditFields
