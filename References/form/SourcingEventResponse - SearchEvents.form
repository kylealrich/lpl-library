SearchEvents is a SearchForm
    Layout
        four column distributed
            column1
                SourcingEvent
                    label is "Event#"
                    select is EventHistory
                Status
                    label is "ResponseStatus"
            column2
                SourcingEvent.Name
                SourcingEvent.SourcingEventType
                    label is "Type"
                    select is SupplierEventTypes
            column3
                SourcingEvent.Category
                    label is "Category"
                    select is SupplierCategories
                SourcingEvent.EventReference
                    label is "Reference"
            column4
                SourcingEvent.SubCategory
                    label is "SubCategory"
                    select is SupplierSubCategories
