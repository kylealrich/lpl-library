SubcontractorComposite is a CompositeForm
    is primary
    title is "<DisplaySubcontractorNumber>"
    context form is SubcontractorContext
        background is theme color
    Actions
        action is save
        action is save and new
        action is save and close
        action is refresh
            label is "_"
    CompanyInformation is a Panel
        form is Subcontractor
    Diversity is a Panel
        list is SubcontractorDiversityRel.primary
    Contacts is a Panel
        list is SubcontractorContact set.SubcontractorContactList
    Comments is a Panel
        list is SubcontractorComment set.SubcontractorCommentList
    Certifications is a Panel
        list is SubcontractorCertification set.SubcontractorCertificationList
    CommodityCodes is a Panel
        list is SubcontractorCommodityCode set.SubcontractorCommodityCodeList
            helper list is CommodityCodesRel.CommCodesSubcontractor
                action is AttachToSubcontractor
