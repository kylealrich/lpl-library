MobileBinForm is a Form
    title is "Bin"
    Actions
        action is Create
            action icon is create
            label is "_"
        action is save
            label is "_"
            hide when invalid
    Layout
        single column
            Company.DerivedCompanyNameID
                label is "Company"
                display as text
            InventoryLocation
                display as text
            InventoryLocation.Name
                label is "LocationName"
                display as text
        paragraph
            Bin
            Active
        single column
            BinType
            StorageCode
                select is MobileStorageCodeSelect
            GlobalLocationNumber
                display as text
        blank line
        header3 of "AvailableBinTypes"
