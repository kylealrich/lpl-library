MobileBudgetTemplate is a CompositeForm
    title is "BudgetTemplate_<Scenario>_<BudgetTemplate>"
    context form is MobileMain
    Dimensions is a Panel
        title is "Main"
        form is MobileDimensions
    Structures is a Panel
        form is MobileStructures
    ManualElements is a Panel
        visible when (HasManual)
        list is BudgetTemplateManual.MobileBudgetTemplateManualForTemplate
    Groups is a Panel
        visible when (IsCreated)
        list is BudgetGroupsRel.MobileBudgetGroupForHeaderList
    PendingMembers is a Panel
        visible when (HasNewNonManualembers)
        list is BudgetPendingsRel.MobileBudgetTemplatePendingForTemplate
    PendingManual is a Panel
        visible when (HasPendingManual)
        list is BudgetManualPendingsRel.MobileBudgetTemplateManualForTemplate
    PendingRecalculate is a Panel
        visible when (HasRecalculate)
        list is BudgetRecalculatesRel.MobileBudgetTemplateRecalculateForTemplate
    PendingRemove is a Panel
        visible when (HasRemove)
        list is BudgetRemovesRel.MobileBudgetTemplateRemoveForTemplate
    CheckedGroups is a Panel
        visible when (DisplayCheckedGroups)
        list is CheckedGroupsRel.MobileCheckedGroups
    GroupData is a Panel
        visible when (IsCreated)
        form is MobileGroupData
