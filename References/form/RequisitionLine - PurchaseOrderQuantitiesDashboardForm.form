PurchaseOrderQuantitiesDashboardForm is a Form
    title is "PurchaseOrderQuantities"
    Layout
        display as text
            two column table
                column headers
                    "Quantity"
                    "UOM"
                row headers
                    "Requested"
                    "WaitingToCreatePO"
                    "UnreleasedOnPO"
                    "ReleasedOnPO"
                    "Canceled"
                    "Delivered"
                column1
                    Quantity
                        no label
                    PurchaseOrderInterfaceQuantity
                        no label
                    UnreleasedPurchaseOrderQuantity
                        no label
                    ReleasedPurchaseOrderQuantity
                        no label
                    InterfaceCancelQuantity
                        no label
                    DeliveredQuantity
                        no label
                column2
                    EnteredUOM
                        no label
                    PurchaseOrderInterfaceRel.EnteredUOM
                        no label
                    first UnreleasedPurchaseOrderLineSourcesRel.EnteredUOM
                        no label
                    first ReleasedPurchaseOrderLineSourcesRel.EnteredUOM
                        no label
                    EnteredUOM
                        no label
                    EnteredUOM
                        no label
            visible when (SourcingEventExists)
                paragraph
                    QuantityOnEvent
