MobilePO33VendorItem is a Form
    title is "VendorItem"
    Layout
        visible when (LineExists)
            single column
                PurchaseOrderReceiptLine.VendorItem.VendorItemDescription
                    label is "VendorItemDescription"
                    display as text
                PurchaseOrderReceiptLine.VendorItem.VendorBuyUOM.UnitOfMeasure
                    label is "BuyUOM"
                    display as text
            visible when (HasVendorItemBuyUOM)
                paragraph
                    text of "ThereAre<PurchaseOrderReceiptLine.VendorItem.VendorBuyUOM.UOMConversion>_\\<PurchaseOrderReceiptLine.Item.StockUOM>_\\per_\\<PurchaseOrderReceiptLine.VendorItem.VendorBuyUOM.UnitOfMeasure>"
            single column
                header3 of "ManufacturerInformation"
                PurchaseOrderReceiptLine.VendorItem.Manufacturer.ManufacturerCode
                    display as text
                PurchaseOrderReceiptLine.VendorItem.Manufacturer.ManufacturerDivision
                    display as text
                PurchaseOrderReceiptLine.VendorItem.ManufacturerNumber
                    display as text
                header3 of "Labeler"
                PurchaseOrderReceiptLine.VendorItem.Manufacturer.LabelerIdentificationCode
                    label is "LICCode"
                    display as text
                PurchaseOrderReceiptLine.ItemGTIN
                    label is "GTIN"
                    display as text
        visible when (not LineExists)
            single column
                ReceiptLinesForTemplateRel.VendorItem.VendorItemDescription
                    no label
                    display as text
                ReceiptLinesForTemplateRel.VendorItem.VendorBuyUOM.UnitOfMeasure
                    label is "BuyUOM"
                    display as text
            visible when (HasTemplateVendorItemBuyUOM)
                paragraph
                    text of "ThereAre<ReceiptLinesForTemplateRel.VendorItem.VendorBuyUOM.UOMConversion>_\\<ReceiptLinesForTemplateRel.Item.StockUOM>_\\per_\\<ReceiptLinesForTemplateRel.VendorItem.VendorBuyUOM.UnitOfMeasure>"
            single column
                header3 of "ManufacturerInformation"
                ReceiptLinesForTemplateRel.VendorItem.Manufacturer.ManufacturerCode
                    display as text
                ReceiptLinesForTemplateRel.VendorItem.Manufacturer.ManufacturerDivision
                    display as text
                ReceiptLinesForTemplateRel.VendorItem.ManufacturerNumber
                    display as text
                header3 of "Labeler"
                ReceiptLinesForTemplateRel.VendorItem.Manufacturer.LabelerIdentificationCode
                    label is "LICCode"
                    display as text
                ReceiptLinesForTemplateRel.ItemGTIN
                    label is "GTIN"
                    display as text
