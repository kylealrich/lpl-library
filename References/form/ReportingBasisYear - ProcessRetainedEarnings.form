ProcessRetainedEarnings is an ActionForm
    action is ProcessRetainedEarnings
    title is "ProcessRetainedEarnings"
    Layout
        single column
            paragraph
                PrmAccountingEntity
                PrmAccountingEntity.Name
                    no label
                    display as text
            text of "Or"
            paragraph
                PrmAccountingEntityGroup
                PrmAccountingEntityGroup.Description
                    no label
                    display as text
            blank line
            paragraph
                PrmLedger
                    select is LedgerRel.WithoutCloseandRetainedEarningsList
                PrmLedger.Description
                    no label
                    display as text
            text of "Or"
            paragraph
                PrmLedgerGroup
                PrmLedgerGroup.Description
                    no label
                    display as text
