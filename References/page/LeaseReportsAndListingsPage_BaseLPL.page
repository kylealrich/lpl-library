LeaseReportsAndListingsPage is a Page
    title is "ReportsAndListings"
    implements MenuSearch

    ReportsAndListings is a DashBoardPanel
        title is "ReportsAndListings"
        use 12 column grid
        Reports is a Pane
            span 4 columns
            title is "Reports"
            Layout
                single column
                    small menu
                        page is LeaseDisclosureReports
                            title is "ASCDisclosureSummaries"
                            menu item name is LeaseDisclosureReports
                            icon is icon.check-emerald
                        page is GASBLeaseDisclosureReports
                            title is "GASBDisclosureSummaries"
                            menu item name is GASBLeaseDisclosureReports
                            icon is icon.check-emerald
                        page is IFRSLeaseDisclosureReports
                            title is "IFRSDisclosureSummaries"
                            menu item name is IFRSLeaseDisclosureReports
                            icon is icon.check-emerald
                        page is LeaseWeightedAverageReports
                            title is "WeightedAverageReports"
                            menu item name is LeaseWeightedAverageReports
                            icon is icon.check-emerald
                        list is Lease.LeaseAmortizationReport
                            title is "AmortizationReport"
                            menu item name is LeaseAmortizationReport
                            icon is icon.check-emerald
                        action is LeaseAnalysisReport.LeaseAnalysisReport
                            title is "LeaseAnalysisReport"
                            menu item name is LeaseAnalysisReport
                            icon is icon.check-emerald
                        list is Lease.NewDebtReportList
                            title is "NewDebtReport"
                            menu item name is NewDebtReport
                            icon is icon.check-emerald
                        action is LeaseProjections.LeaseProjections
                            title is "LeaseProjections"
                            menu item name is LeaseProjections
                            icon is icon.check-emerald
                        list is LeaseReportResult.LeaseRollforwardReportList
                            title is "LeaseReports"
                            menu item name is RollforwardReports
                            icon is icon.check-emerald
        LeaseLists is a Pane
            span 4 columns
            title is "LeaseLists"
            Layout
                single column
                    small menu
                        list is Lease.AllLeaseList
                            title is "Leases"
                            menu item name is Leases
                            icon is icon.check-azure
                        list is LeaseInvoice.AllLeaseInvoiceList
                            title is "Invoices"
                            menu item name is Invoices
                            icon is icon.check-azure
                        list is LeasePayment.AllPaymentSchedules
                            title is "PaymentSchedules"
                            menu item name is PaymentSchedules
                            icon is icon.check-azure
                        list is LeasePaymentBalance.AllLeasePaymentBalanceList
                            title is "PaymentBalances"
                            menu item name is PaymentBalances
                            icon is icon.check-azure
                        list is LeasePaymentDetail.VariablePaymentList
                            title is "VariableCostPayment"
                            menu item name is VariableCostPayment
                            icon is icon.check-azure
                        list is LeaseReportResult.LeaseRollforwardReportList
                            title is "RollforwardReportList"
                            menu item name is RollforwardReportList
                            icon is icon.check-azure
                        list is LeaseReportResult.LeaseDisclosureSummaryReportList
                            title is "DisclosureSummaryReportList"
                            menu item name is DisclosureSummaryReportList
                            icon is icon.check-azure
        SubleaseLists is a Pane
            span 4 columns
            title is "SubleaseLists"
            Layout
                single column
                    small menu
                        list is Sublease.SubleaseListByCompany
                            title is "Subleases"
                            menu item name is Subleases
                            icon is icon.check-ruby
                        list is SubleaseBillingInvoice.SubleaseBillingInvoiceDashboardList
                            title is "Invoices"
                            menu item name is Invoices
                            icon is icon.check-ruby

