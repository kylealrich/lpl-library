AdminConsoleSecurityPage is a Page
    title is "Administration"

    AdminDashboard is a DashBoardPanel
        title is "AdminDashboard"
        use 12 column grid
        PermissionBOD is a Pane
            span 4 columns
            title is "PermissionBOD"
            Layout
                single column
                    small menu
                        action is PermissionBODsGenerator.UpdatePermissionBODs
                            title is "UpdatePermissionBOD\\s"
                            menu item name is UpdatePermissionBODs
                    small menu
                        list is PermissionBODsStatistic.PermissionBODsStatisticList
                            title is "PermissionBOD\\sStatistic"
                            menu item name is PermissionBODsStatisticList
                    small menu
                        list is PermissionBODsSync.PermissionBODsSyncList
                            title is "PermissionBOD\\sSyncTime"
                            menu item name is PermissionBODsSyncList
                    small menu
                        list is PermissionBODsRecordUpdates.PermissionBODsRecordUpdatesList
                            title is "PermissionBOD\\sRecordUpdates"
                            menu item name is PermissionBODsRecordUpdatesList
                    small menu
                        action is SecurityClass.SendBODForAllSecClassPermissions
                            title is "SendBODForAllSecurityClassPermissions"
                            menu item name is SendBODForAllSecClassPermissions
                    small menu
                        action is RoleSecurityClass.SendBODForAllRolePermissions
                            title is "SendBODForAllRolePermissions"
                            menu item name is SendBODForAllRolePermissions
        Maintenance is a Pane
            span 4 columns
            title is "Maintenance"
            Layout
                single column
                    small menu
                        action is MergeAgents.RunCommand
                            title is "MergeAgentsReport"
                            menu item name is MergeAgentsReport
                    small menu
                        list is SecurityReport.MenuSecurityReportList
                            title is "MenuSecurityReport"
                            menu item name is MenuSecurityReportList
                    small menu
                        list is MissingSecurityClass.MissingSecurityClassList
                            title is "MissingSecurityClasses"
                            menu item name is MissingSecurityClassList
                    small menu
                        page is RoleAndSecurityClassPermissions
                            title is "RoleAndSecurityClassPermissions"
                            menu item name is RoleAndSecurityClassPermissions
        DataMovement is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        list is RolePermissionMaster.DefaultRolePermissionList
                            title is "RolePermissionMaster"
                            menu item name is RolePermissionMaster
                    small menu
                        list is SecurityReport.UserSecurityReportList
                            title is "UserSecurity"
                            menu item name is UserSecurityReportList
                    small menu
                        list is SecurityPermissionMaster.DefaultSecurityPermissionList
                            title is "SecurityPermissionMaster"
                            menu item name is SecurityPermissionMaster

