MatchAdministrationUX1 is a Page
    title is "<actor.context.FinanceEnterpriseGroup.MatchAdministrationTitle>"
    implements MenuSearch
    show panel control on left

    Setup is a DashBoardPanel
        use 12 column grid
        Setup is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        list is MatchCompany.AllMatchCompanyList
                            title is "Companies"
                            menu item name is MatchCompanies
                            description is "Set_up_companies"
                            icon is icon.hcm-amethyst
                        list is MatchTable.MatchTableList
                            title is "MatchTables"
                            menu item name is MatchTables
                            description is "Set_up_match_tables"
                            icon is icon.hcm-amethyst
                        list is MatchRule.MatchRuleList
                            title is "MatchRules"
                            menu item name is MatchRules
                            description is "Set_up_match_rules"
                            icon is icon.hcm-amethyst
                        list is ProcurementReasonCode.ProcurementReasonCodesInline
                            title is "ReasonCodes"
                            menu item name is ReasonCodes
                            description is "Set_up_reason_codes"
                            icon is icon.hcm-amethyst
                        list is AddOnCharge.AddOnChargeList
                            title is "AddOnCharges"
                            menu item name is AddOnCharges
                            description is "Set_up_add_on_charges"
                            icon is icon.hcm-amethyst
        Resources is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        list is FinanceResource.FinanceResourceDashboard
                            title is "ResourceDashboard"
                            menu item name is ResourceDashboard
                            icon is icon.home-azure
                        list is FinanceResource.FinanceResourceList
                            title is "Resources"
                            menu item name is Resources
                            icon is icon.home-azure

