ContractInterfacePageNoGPO is a Page
    title is "ContractInterfaces"
    implements MenuSearch

    ContractInterfaceProcessing is a DashBoardPanel
        title is "ContractInterfaces"
        use 12 column grid
        Interfaces is a Pane
            span 3 columns
            title is "Interfaces"
            Layout
                single column
                    list is ContractImport.ImportedContracts
                        title is "AllInterfaceContracts"
                        menu item name is AllInterfaceContracts
                        icon is icon.clipboard-list-amethyst
                    list is ContractInterfaceCatalog.ContractInterfaceCatalogList
                        title is "ViewCatalogs"
                        menu item name is ViewCatalogs
                        icon is icon.clipboard-list-amethyst
                    action is ContractInterfaceCatalog.CreateImportRecords
                        title is "ProcessCatalogs"
                        menu item name is ProcessCatalogs
                        icon is icon.clipboard-list-amethyst

