InventoryTransactionsPage is a Page
    title is "InventoryTransactions"
    show panel control on left

    Unreleased is a Panel
        business class is InventoryTransaction
            list is UnreleasedInventoryTransactionList

    Receipts is a Panel
        business class is InventoryTransaction
            list is ReceiptTransactions

    Issues is a Panel
        business class is InventoryTransaction
            list is IssueTransactions

    Returns is a Panel
        business class is InventoryTransaction
            list is InventoryReturnsTransactions

    Adjustments is a Panel
        business class is InventoryTransaction
            list is AdjustmentTransactions

    Transfers is a Panel
        business class is InventoryTransaction
            list is TransferTransactions

    TransferReceipts is a Panel
        business class is InventoryTransaction
            list is IntransitTransactions

    BinTransfers is a Panel
        business class is InventoryTransaction
            list is BinTransferTransactions

    AllTransactions is a Panel
        business class is InventoryTransaction
            list is AllInventoryTransactions

    SearchLines is a Panel
        business class is InventoryTransactionLine
            list is InventoryTransactionLines

    AuditHistory is a Panel
        title is "AuditHistory"
        visible when (not actor.context.FinanceEnterpriseGroup.ColumnarInventoryTransactionAuditInUse)
        business class is InventoryTransaction
            list is InventoryTransactionAuditList
                title is "AuditHistory"

    AuditHistoryColumnar is a Panel
        title is "AuditHistory"
        visible when (actor.context.FinanceEnterpriseGroup.ColumnarInventoryTransactionAuditInUse)
        business class is InventoryTransaction
            list is ColumnarInventoryTransactionAuditList
                title is "AuditHistory"

    LineAuditHistory is a Panel
        visible when (not actor.context.FinanceEnterpriseGroup.ColumnarInventoryTransactionLineAuditInUse)
        business class is InventoryTransactionLine
            list is InventoryTransactionLineAuditList

    LineAuditHistoryColumnar is a Panel
        title is "LineAuditHistory"
        visible when (actor.context.FinanceEnterpriseGroup.ColumnarInventoryTransactionLineAuditInUse)
        business class is InventoryTransactionLine
            list is ColumnarInventoryTransactionLineAuditList
                title is "LineAuditHistory"

