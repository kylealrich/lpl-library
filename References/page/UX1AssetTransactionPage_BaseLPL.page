UX1AssetTransactionPage is a Page
    title is "Transactions_andBalances"
    implements MenuSearch

    AssetTransactions is a Panel
        business class is AssetTransaction
            list is AssetTransactionsFilter

    BookTransactions is a Panel
        business class is AssetBookTransaction
            list is AssetBookTransactionsFilter

    AccountBeginningBalances is a Panel
        business class is AssetTransactionBeginningBalance
            list is AssetBookAccountBeginningBalances

    AccountBalances is a Panel
        business class is AssetTransactionSummary
            list is TransactionBalances

    AccountBalanceDetails is a Panel
        business class is AssetTransactionSummary
            list is TransactionBalanceDetail

    ActivityByProcess is a Panel
        business class is AssetTransactionSummary
            list is AssetActivityByProcess

    JournalResults is a Panel
        business class is JournalizeGroupResult
            list is JournalizeGroupResults

