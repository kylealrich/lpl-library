OrderEntryAdministrationPageUX1 is a Page
    title is "Administration"
    implements MenuSearch

    Setup is a DashBoardPanel
        use 12 column grid
        Company is a Pane
            span 3 columns
            icon is icon.briefcases-emerald
            Layout
                two column distributed
                    small menu
                        list is BillingCompany.primary
                            title is "Companies"
                            menu item name is Companies
                        list is BillingProcessLevel.primary
                            title is "ProcessLevels"
                            menu item name is ProcessLevels
                        list is OrderCancelCreditReason.primary
                            title is "OrderCancelCreditReasons"
                            menu item name is OrderCancelCreditReason
                        list is BillingStandardCommentHeader.primary
                            title is "StandardCommentHeaders"
                            menu item name is StandardCommentHeaders
                        list is BillingStandardComment.primary
                            title is "StandardComments"
                            menu item name is StandardComments
        OrderEntry is a Pane
            span 3 columns
            icon is icon.outline-star-emerald
            Layout
                two column distributed
                    small menu
                        list is OrderEntryOperator.primary
                            title is "Operators"
                            menu item name is Operators
                        list is OrderEntryOrderType.primary
                            title is "OrderTypes"
                            menu item name is OrderTypes
                        list is OrderEntryStandardOrder.primary
                            title is "StandardOrders"
                            menu item name is StandardOrders
                        list is OrderEntryOrderSource.primary
                            title is "OrderSources"
                            menu item name is OrderSources
                        list is PickShipRoute.primary
                            title is "DeliveryRoutes"
                            menu item name is DeliveryRoutes
                        list is OrderEntryCustomer.OrderEntryCustomerList
                            title is "OrderEntryCustomers"
                            menu item name is OrderEntryCustomers
        Codes is a Pane
            span 3 columns
            icon is icon.outline-maintenance-emerald
            Layout
                single column
                    small menu
                        list is BillingCustomerCode.primary
                            title is "Customer"
                            menu item name is Customer
                        list is CustomerShipTo.primary
                            title is "CustomerShipTo"
                            menu item name is CustomerShipTo
                        list is OrderDiscountCode.primary
                            title is "OrderDiscount"
                            menu item name is OrderDiscount
                        list is PriceDiscountCode.primary
                            title is "PriceDiscount"
                            menu item name is PriceDiscount
                        list is BillingFreightCode.primary
                            title is "Freight"
                            menu item name is Freight
        Item is a Pane
            span 3 columns
            icon is icon.outline-diagonals-emerald
            Layout
                single column
                    small menu
                        list is BillingItemGeneralLedgerAccount.primary
                            title is "GlobalLedgerAccounts"
                            menu item name is GlobalLedgerAccounts
                        list is BillingItemAddOnCost.primary
                            title is "AddOnCosts"
                            menu item name is AddOnCosts
                        list is OrderEntryAddOnCharge.primary
                            title is "AddOnCharges"
                            menu item name is ItemAndOrderBillingAddOnCharge
                        list is CustomerItemCrossReference.primary
                            title is "CrossReferences"
                            menu item name is CustomerItemCrossReference
                        list is RecurringBillingItemOperator.primary
                            title is "RecurringBillingItemOperators"
                            menu item name is RecurringBillingItemOperators
                        list is OrderEntryItemGroupDiagram.primary
                            title is "OrderEntryItemGroupDiagrams"
                            menu item name is ItemGroupDiagram
        Invoices is a Pane
            span 3 columns
            icon is icon.document-finance-emerald
            Layout
                single column
                    small menu
                        list is BillingInvoiceType.primary
                            title is "Type"
                            menu item name is Types
                        list is BillingInvoicePrefix.primary
                            title is "Prefix"
                            menu item name is Prefix
                        list is BillingInvoiceDocument.primary
                            title is "Document"
                            menu item name is Documents
        Pricing is a Pane
            span 6 columns
            icon is icon.cash-emerald
            Layout
                two column distributed
                    small menu
                        list is PriceBase.primary
                            title is "Bases"
                            menu item name is Bases
                        list is PriceListContractPromotion.primary
                            title is "ListContractPromotions"
                            menu item name is ListContractPromotions
                        list is PriceListSalesClass.primary
                            title is "ListSalesClasses"
                            menu item name is ListSalesClasses
                            description is "Set_up_sales_classes"
                        list is PriceBook.primary
                            title is "Books"
                            menu item name is Books
                        list is PriceBookSalesClassComment.primary
                            title is "BookSalesClassComments"
                            menu item name is BookSalesClassComments
                        list is PriceBook.PriceBookDetailList
                            title is "BookDetails"
                            menu item name is BookDetails
                        list is PricingWorksheet.primary
                            title is "Worksheets"
                            menu item name is Worksheets
                        list is PricingPreview.primary
                            title is "Previews"
                            menu item name is Preview
        DocumentTemplates is a Pane
            span 3 columns
            icon is icon.outline-report-emerald
            Layout
                single column
                    small menu
                        page is BillingIDMTemplatesPage
                            title is "IDM"
                            menu item name is IDM
                        list is SampleDocumentTemplate.SampleDocumentTemplatesForBilling
                            title is "Sample_Document"
                            menu item name is SampleDocument
                        page is IDMPrinterPage
                            title is "IDM_Printers"
                            menu item name is IDMPrinters

    Utilities is a DashBoardPanel
        use 12 column grid
        OrderEntryUtilities is a Pane
            span 3 columns
            icon is icon.window-settings-ruby
            Layout
                single column
                    small menu
                        action is CustomerOrder.PostMigrateUtility
                            title is "PostMigrateUtility"
                            menu item name is PostMigrateUtility
        PurgeAndLoad is a Pane
            span 3 columns
            icon is icon.warning-emerald
            Layout
                single column
                    small menu
                        action is CustomerOrder.OrderEntryHistoryPurge
                            title is "HistoryPurge"
                            menu item name is HistoryPurge
                        action is CustomerOrderReturn.OrderEntryReturnsPurge
                            title is "ReturnsPurge"
                            menu item name is ReturnsPurge
                        action is OrderEntryLoadProgram.OrderEntryLoadProgram
                            title is "LoadProgram"
                            menu item name is LoadProgram
                        action is OrderEntryReturnsLoadProgram.OrderEntryReturnsLoadProgram
                            title is "ReturnsLoadProgram"
                            menu item name is ReturnsLoadProgram

    Actions
        action is CustomerOrder.Unreleased.Create
            label is "CreateOrder"
        action is refresh
            label is "_"
