AssetHistory is a Page
    title is "BatchProcessing"
    implements MenuSearch

    Process is a DashBoardPanel
        use 12 column grid
        General is a Pane
            span 6 columns
            title is "Process"
            Layout
                two column distributed
                    large menu
                        action is MassAssetAdjustments.MassAssetAdjustments
                            title is "Adjust"
                            menu item name is MassAssetAdjustments
                            description is "Adjust_a_group_of_assets"
                            icon is icon.cycle-azure
                            Search Terms
                                    "AssetAdjustments"
                                    "MassAssetAdjustments"
                                    "MassAdjustments"
                        action is AssetMassTransfers.AssetMassTransfers
                            title is "Transfer"
                            menu item name is AssetMassTransfers
                            description is "Transfer_a_group_of_assets"
                            icon is icon.compare-azure
                            Search Terms
                                    "AssetMassTransfers"
                                    "MassTransfers"
                                    "AssetTransfers"
                two column distributed
                    large menu
                        action is AssetMassDisposals.AssetMassDisposals
                            title is "Dispose"
                            menu item name is AssetMassDisposals
                            description is "Dispose_of_a_group_of_assets"
                            icon is icon.recall-azure
                            Search Terms
                                    "AssetMassDisposals"
                                    "AssetDisposals"
                                    "MassDisposals"
                        action is MassAssetRevalue.MassAssetRevalue
                            title is "Revalue"
                            menu item name is MassAssetRevalue
                            description is "Revalue_a_group_of_assets"
                            icon is icon.document-finance-azure
                            Search Terms
                                    "MassAssetRevalue"
                                    "MassRevalue"
                                    "AssetRevalue"
                two column distributed
                    large menu
                        action is AssetCompany.DisposedAssetsPurge
                            title is "PurgeDisposedAssets"
                            menu item name is DisposedAssetsPurge
                            description is "Purge_disposed_assets"
                            icon is icon.check-azure
                            Search Terms
                                    "AssetPurge"
                                    "DisposedAssetsPurge"
                        action is AssetCompany.PurgeAssetHistory
                            title is "PurgeAssetHistory"
                            menu item name is PurgeAssetHistory
                            description is "Purging_asset_history_permanently_deletes_data"
                            icon is icon.check-azure
                            Search Terms
                                    "PurgeAssetHistory"
                                    "PurgeHistory"
        SelectionGroups is a Pane
            span 6 columns
            title is "MassSelections"
            business class is MassSelection
                list is MassSelections
        ViewAdjustments is a Pane
            span 3 columns
            title is "RecentAdjustments"
            business class is AssetAdjustment
                list is AssetAdjustmentDashBoardList
                    overview navigation is OverviewNav
        ViewTransfers is a Pane
            span 3 columns
            title is "RecentTransfers"
            business class is AssetTransfer
                list is NewAssetTransfers
                    overview navigation is OverviewNav
        ViewDisposals is a Pane
            span 3 columns
            title is "RecentDisposals"
            business class is AssetDisposal
                list is AssetDisposals
                    overview navigation is OverviewNav
        ViewRevaluations is a Pane
            span 3 columns
            title is "Revaluations"
            business class is Asset
                list is RevaluedAssets

    Select is a Panel
        title is "MassSelections"
        business class is MassSelection
            list is CardWithDetailSections
                title is "MassSelections"

    Adjustments is a Panel
        business class is AssetAdjustment
            list is AssetAdjustmentDashBoardList

    Transfers is a Panel
        business class is AssetTransfer
            list is NewAssetTransfers

    Disposals is a Panel
        business class is AssetDisposal
            list is AssetDisposals

    Revaluations is a Panel
        business class is Asset
            list is RevaluedAssets

    Reinstatements is a Panel
        business class is AssetReinstatement
            list is AssetReinstatementList

    Disposed is a Panel
        title is "DisposedAssets"
        business class is Asset
            list is AllDisposedAssets
                title is "DisposedAssets"

