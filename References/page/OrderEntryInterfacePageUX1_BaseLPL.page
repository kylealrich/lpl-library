OrderEntryInterfacePageUX1 is a Page
    title is "Interfaces"
    implements MenuSearch

    InterfaceDashboard is a DashBoardPanel
        title is "Main"
        use 12 column grid
        hide border on menu panels
        Interfaces is a Pane
            span 6 columns
            title is "_"
            LinkBoxLayout
                dual width link
                    action is OrderEntrySalesOrderImport.OrderEntrySalesOrderImport
                        title is "SalesOrder"
                        menu item name is SalesOrder
                        icon is icon.shopping-cart-amethyst
                dual width link
                    action is OrderEntryReturnFeedbackInterface.OrderEntryReturnFeedbackInterface
                        title is "ReturnFeedback"
                        menu item name is ReturnFeedback
                        icon is icon.recall-amethyst
        Returns is a Pane
            span 6 columns
            title is "_"
            LinkBoxLayout
                dual width link
                    list is CustomerOrderReturnHeaderImport.primary
                        title is "ReturnHeaderImports"
                        menu item name is ReturnHeaderImports
                        icon is icon.outline-interface-amethyst
                dual width link
                    list is CustomerOrderReturnHeaderInbound.primary
                        title is "ReturnHeaderInbounds"
                        menu item name is ReturnHeaderInbound
                        icon is icon.inbox-amethyst

    Actions
        action is CustomerOrder.Unreleased.Create
            label is "CreateOrder"
        action is refresh
            label is "_"
