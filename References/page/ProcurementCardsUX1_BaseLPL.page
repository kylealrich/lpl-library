ProcurementCardsUX1 is a Page
    title is "ProcurementCards"
    implements MenuSearch

    AllProcurementCards is a DashBoardPanel
        use 12 column grid
        Setup is a Pane
            span 3 columns
            Layout
                single column
                    page is ProcurementCardSetupPage
                        title is "ProcurementCard"
                        menu item name is ProcurementCardSetup
                        icon is icon.document-emerald
                    page is MerchantSetupPage
                        title is "Merchant"
                        menu item name is ProcurementCardMerchants
                        icon is icon.document-emerald
                    list is ProcurementCardReasonCode.ProcurementCardReasonCodes
                        title is "ReasonCodes"
                        menu item name is ProcurementCardReasonCodes
                        icon is icon.document-emerald
        Processes is a Pane
            span 6 columns
            Layout
                two column distributed
                    page is ProcurementCardProcessingPage
                        title is "Processing"
                        menu item name is ProcurementCardProcessing
                        icon is icon.cycle-azure
                    page is ProcurementCardCharges
                        title is "Charges"
                        menu item name is ProcurementCardCharges
                        icon is icon.cycle-azure
                    page is ProcurementCardStatements
                        title is "Statements"
                        menu item name is ProcurementCardStatements
                        icon is icon.cycle-azure
                    list is ProcurementCardStatementInput.ProcurementCardStatementInputList
                        title is "StatementInput"
                        menu item name is ProcurementCardStatementInput
                        icon is icon.cycle-azure
                    action is LoadInterfacedProcurementCardStatements.LoadInterfacedProcurementCardStatements
                        title is "LoadInterfacedStatements"
                        menu item name is LoadInterfacedProcurementCardStatements
                        icon is icon.cycle-azure
                    page is ProcurementCardInterfaceMaintenancePage
                        title is "StatementInterfaceMaintenance"
                        menu item name is ProcurementCardStatementInterfaceMaintenance
                        icon is icon.cycle-azure
                    action is ProcurementCardStatementTransaction.ReleaseAndInvoiceProcurementCardTransactions
                        title is "ReleaseAndInvoiceTransactions"
                        menu item name is ReleaseAndInvoiceProcurementCardTransactions
                        icon is icon.cycle-azure

