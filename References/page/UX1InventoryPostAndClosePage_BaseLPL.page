UX1InventoryPostAndClosePage is a Page
    title is "PeriodEndClose"
    implements MenuSearch

    Process is a Panel
        Layout
            single column
                large menu
                    action is InventoryPeriodClose.InventoryPeriodClose
                        title is "PeriodClose"
                        menu item name is PeriodClose
                        description is "Run Closing to close out the current inventory period"
                        icon is icon.book-amber
                    action is InventoryCompany.CloseInventoryControlSystem
                        title is "CloseInventoryControl"
                        menu item name is CloseInventoryControl
                        description is "Close the subsystem period before running the closing procedure in the General Ledger application"
                        icon is icon.clipboard-check-amber
                    page is DepartmentalExpenseReportPage
                        title is "DepartmentalProcurementExpenses"
                        menu item name is DepartmentalProcurementExpenses
                        description is "View_Departmental_Procurement_Expense_Report"
                        icon is icon.document-finance-amber
                    action is DistributionRequirementsReport.DistributionRequirementsReport
                        title is "DistributionRequirementsReport"
                        menu item name is DistributionRequirementsReport
                        description is "Create Distribution Requirements Report"
                        icon is icon.compare-amber

    JournalizePreview is a Panel
        business class is InventoryTransactionLineDistribution
            list is PostToGLList

    Journalize is a Panel
        title is "JournalDistributions"
        business class is JournalizeGroupResult
            list is JournalizeGroupResults
                title is "JournalDistributions"

