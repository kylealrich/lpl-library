SupplierContractManagementPage is a Page
    title is "Contracts"
    implements MenuSearch

    MyActiveContracts is a Panel
        business class is Contract
            list is SupplierActiveContractDetail

    AllActiveContracts is a Panel
        business class is Contract
            list is SupplierAllContractsActiveContractDetail

    OtherContractInformation is a DashBoardPanel
        use 12 column grid
        Expiring is a Pane
            span 3 columns
            title is "Expiring"
            Layout
                single column
                    small menu
                        page is ManageContracts.Expiring
                            title is "MyExpiring"
                            menu item name is MyExpiringContracts
                            icon is icon.warning-amethyst
                        page is ManagePrimaryContracts.Expiring
                            title is "AllExpiring"
                            menu item name is AllExpiringContracts
                            icon is icon.warning-amethyst
        Negotiation is a Pane
            span 9 columns
            title is "Negotiation"
            visible when (session.key.SupplierGroup.CanViewNegotiationOrProposals)
            Layout
                two column
                    column1
                        header4 of "MyNegotiatedContracts"
                        small menu
                            page is ManageContractsWithTermsNegotiation.InNegotiationRedlineOnDocument
                                title is "InNegotiationRedlineOnDocument"
                                menu item name is MineInNegotiationRedlineOnDocument
                                icon is icon.document-certified-amber
                            page is ManageContractsWithTermsNegotiation.NegotiationCompleteRedlineOnDocument
                                title is "CompletedRedlineOnDocument"
                                menu item name is MineCompletedRedlineOnDocument
                                icon is icon.document-certified-amber
                            page is ManageContractsWithTermsNegotiation.InNegotiationRedlineOnContract
                                title is "InNegotiationRedlineOnContract"
                                menu item name is MineInNegotiationRedlineOnContract
                                icon is icon.document-certified-amber
                            page is ManageContractsWithTermsNegotiation.NegotiationCompleteRedlineOnContract
                                title is "CompletedRedlineOnContract"
                                menu item name is MineCompletedRedlineOnContract
                                icon is icon.document-certified-amber
                    column2
                        header4 of "AllNegotiatedContracts"
                        small menu
                            page is ManagePrimaryContracts.InDocumentNegotiation
                                title is "InNegotiationRedlineOnDocument"
                                menu item name is AllInNegotiationRedlineOnDocument
                                icon is icon.document-certified-amber
                            page is ManagePrimaryContracts.DocumentNegotiationComplete
                                title is "CompletedRedlineOnDocument"
                                menu item name is AllCompletedRedlineOnDocument
                                icon is icon.document-certified-amber
                            page is ManagePrimaryContracts.InTermsNegotiation
                                title is "InNegotiationRedlineOnContract"
                                menu item name is AllInNegotiationRedlineOnContract
                                icon is icon.document-certified-amber
                            page is ManagePrimaryContracts.TermsNegotiationComplete
                                title is "CompletedRedlineOnContract"
                                menu item name is AllCompletedRedlineOnContract
                                icon is icon.document-certified-amber
        Proposed is a Pane
            span 9 columns
            title is "Proposed"
            visible when (session.key.SupplierGroup.CanViewNegotiationOrProposals)
            Layout
                three column
                    column1
                        small menu
                            action is Contract.Draft.SupplierCreateProposedContract
                                title is "CreateAProposedContract"
                                menu item name is SupplierCreateProposedContract
                                icon is icon.outline-action-ruby
                    column2
                        header2 of "MyContracts"
                        small menu
                            page is SupplierProposedContractsPage.ProposedContracts
                                title is "Proposed"
                                menu item name is MyProposedContracts
                                icon is icon.compare-ruby
                            page is SupplierProposedContractsPage.AcceptedContracts
                                title is "Accepted"
                                menu item name is MyAcceptedContracts
                                icon is icon.compare-ruby
                            page is SupplierProposedContractsPage.RejectedContracts
                                title is "Rejected"
                                menu item name is MyRejectedContracts
                                icon is icon.compare-ruby
                    column3
                        header3 of "AllContracts"
                        small menu
                            page is ManagePrimaryContracts.ProposedContracts
                                title is "Proposed"
                                menu item name is AllProposedContracts
                                icon is icon.compare-ruby
                            page is ManagePrimaryContracts.AcceptedContracts
                                title is "Accepted"
                                menu item name is AllAcceptedContracts
                                icon is icon.compare-ruby
                            page is ManagePrimaryContracts.RejectedContracts
                                title is "Rejected"
                                menu item name is AllRejectedContracts
                                icon is icon.compare-ruby
        ContractsForAllSuppliers is a Pane
            span 3 columns
            title is "AllSuppliers"
            visible when (session.key.SupplierGroup.DisplayContractsToAll)
            Layout
                single column
                    small menu
                        list is Contract.ContractsForAllSuppliers
                            title is "AllSuppliers"
                            menu item name is AllSuppliers
                            icon is icon.document-certified-amber
        ChangeOrders is a Pane
            span 3 columns
            title is "ChangeOrders"
            visible when (session.key.SupplierGroup.HasChangeOrderClassifications)
            Layout
                single column
                    small menu
                        list is Contract.SupplierContractsEligibleForChangeOrders
                            title is "CreateAChangeOrder"
                            menu item name is SupplierCreateChangeOrder
                            icon is icon.outline-process-emerald
                        list is ContractVersion.SupplierChangeOrders
                            title is "MyChangeOrders"
                            menu item name is MyChangeOrders
                            icon is icon.outline-process-emerald
        Subcontractors is a Pane
            span 3 columns
            title is "Subcontractors"
            visible when (not session.key.SupplierGroup.SPDisablePortalSubcontractors)
            Layout
                single column
                    small menu
                        page is ManageSubcontractors.ContractSubcontractors
                            title is "ContractSubcontractors"
                            menu item name is ContractSubcontractors
                            icon is icon.group-azure
                        page is ManageSubcontractors.Subcontractors
                            title is "Subcontractors"
                            menu item name is Subcontractors
                            icon is icon.group-azure

