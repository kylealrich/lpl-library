ContractsAdministratorUX1 is a Page
    title is "ContractsAdministration"
    implements MenuSearch

    OtherModules is a DashBoardPanel
        use 12 column grid
        Contracts is a Pane
            span 6 columns
            icon is icon.outline-maintenance-emerald
            Layout
                two column
                    small menu
                        list is ContractType.ContractTypes
                            title is "ContractTypes"
                            menu item name is ContractTypes
                        list is ContractClassification.ContractClassifications
                            title is "ContractClassifications"
                            menu item name is ContractClassifications
                        list is ContractClassificationGroup.ClassificationGroups
                            title is "ContractClassificationGroups"
                            menu item name is ContractClassificationGroups
                        list is ContractRebateType.ContractRebateTypes
                            title is "RebateTypes"
                            menu item name is RebateTypes
                        page is PricingGroupPage
                            title is "PricingGroups"
                            menu item name is PricingGroups
                        list is ProcurementReasonCode.HoldReasonCodes
                            title is "HoldReasonCodes"
                            menu item name is HoldReasonCodes
                        list is ContractEvaluatorAndApprover.ContractAllEvaluatorAndApproverList
                            title is "ContractEvaluators,Approvers,AndCreators"
                            menu item name is ContractEvaluatorsAndApprovers
                        list is ContractApproverCategory.ContractApproverCategoryList
                            title is "ContractApproverCategories"
                            menu item name is ContractApproverCategories
                        list is ContractGroupApprover.ContractGroupApproverList
                            title is "ContractGroupApprovers"
                            menu item name is ContractGroupApprovers
                        page is ContractTemplates
                            title is "Contract_andProposalTemplates"
                            menu item name is ContractTemplates
                        list is ContractPreferredManufacturer.ContractPreferredManufacturerList
                            title is "PreferredContracts_andManufacturers"
                            menu item name is PreferredContractsAndManufacturers
                        list is ContractGroup.ContractGroups
                            title is "ContractGroups"
                            menu item name is ContractGroups
                        list is SupplierGroup.SupplierGroups
                            title is "SupplierGroups"
                            menu item name is SupplierGroups
                        list is ContractCompany.ContractCompanies
                            title is "Companies"
                            menu item name is ContractCompanies
        Procurement is a Pane
            span 6 columns
            icon is icon.outline-maintenance-ruby
            Layout
                two column
                    small menu
                        page is Questions
                            title is "Questions"
                            menu item name is Questions
                        list is PurchasingContactType.ContactTypes
                            title is "ContactTypes"
                            menu item name is ContactTypes
                        list is Category.Categories
                            title is "Categories"
                            menu item name is Categories
                        list is Article.Articles
                            title is "ArticlesAndTerms"
                            menu item name is ArticlesAndTerms
                        page is SupplierPage
                            title is "Suppliers"
                            menu item name is Suppliers
                        page is SubcontractorPage
                            title is "Subcontractors"
                            menu item name is Subcontractors
                        list is ParticipantGroup.ParticipantGroups
                            title is "Participant_andMemberGroups"
                            menu item name is ParticipantAndMemberGroups
                        list is PerformanceEvaluationTemplate.PerformanceEvaluationTemplates
                            title is "SupplierEvaluationTemplates"
                            menu item name is SupplierPerformanceEvaluationTemplates
                        list is ProcurementGroup.primary
                            title is "ProcurementGroups"
                            menu item name is ProcurementGroups
        Utilities is a Pane
            span 6 columns
            icon is icon.outline-action-azure
            Layout
                single column
                    small menu
                        action is ContractGroup.UpdateContractContacts
                            title is "UpdateContractContacts"
                            menu item name is UpdateContractContacts
                        action is ContractLine.UpdateInventoryClass
                            title is "UpdateContractLineInventoryClasses"
                            menu item name is UpdateContractLineInventoryClasses
                        action is ContractLine.UpdateCurrentNetAndDefaultBuyCost
                            title is "UpdateCurrentNet_andDefaultBuyCost"
                            menu item name is UpdateCurrentNetAndDefaultBuyCost
                        action is Contract.CloseCommitmentsForClosedContracts
                            title is "CloseCommitments_forClosedContracts"
                            menu item name is CloseCommitmentsForClosedContracts

