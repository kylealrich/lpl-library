AdminConsoleAnalytics is a Page
    title is "Analytics"
    implements MenuSearch

    Cubes is a Panel
        title is "RefreshAndReload"
        Search Terms
                "RefreshAndReload"
        business class is AnalyticCube
            list is AnalyticCubeList
                title is "RefreshAndReload"

    Setup is a DashBoardPanel
        use 12 column grid
        Cubes is a Pane
            span 4 columns
            Layout
                single column
                    list is AnalyticCube.DimensionSurrogates
                        title is "DimensionSurrogates"
                        menu item name is DimensionSurrogates
                        description is "DimensionSurrogates"
                        icon is icon.change-department
                    list is ACDimensionSurrogate.DimensionSurrogateByDimension
                        title is "SurrogatesByDimensions"
                        menu item name is SurrogatesByDimension
                        description is "SurrogatesByDimensions"
                        icon is icon.change-department-alt
                    list is AnalyticCube.CubeReportsList
                        title is "CubeReports"
                        menu item name is CubeReports
                        description is "CubeReports"
                        icon is icon.report
                    action is CubeDbExport.RunCommand
                        title is "ExportCubeDatabaseData"
                        menu item name is CubeDbExportRunCommand
                        description is "ExportCubeDatabaseData"
                        icon is icon.export-2
        Columnar is a Pane
            span 4 columns
            Layout
                single column
                    list is ColumnarQueryGroup.primary
                        title is "ColumnarQueryGroups"
                        menu item name is ColumnarQueryGroups
                        description is "ColumnarQueryGroups"
                        icon is icon.group
                    list is ColumnarQuery.primary
                        title is "ColumnarQueries"
                        menu item name is ColumnarQueries
                        description is "ColumnarQueries"
                        icon is icon.columns
                    action is CSCMaint.RunCommand
                        title is "LoadColumnarCalendars"
                        menu item name is CSCMaintRunCommand
                        description is "ColumnarStandardCalendarsMaintenanceActions"
                        icon is icon.line-bar-chart
        Birst is a Pane
            span 4 columns
            visible when (actor.context.Actor.EnableBirstModel)
            Layout
                single column
                    list is BirstModel.BirstModels
                        title is "BirstModels"
                        menu item name is BirstModel
                        description is "BirstModels"
                        icon is icon.design-mode
                    list is BirstModelResult.BirstModelResults
                        title is "BirstModelResults"
                        menu item name is BirstModelResults
                        description is "BirstModelResults"
                        icon is icon.search-results-history

