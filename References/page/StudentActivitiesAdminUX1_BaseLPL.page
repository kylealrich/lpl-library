StudentActivitiesAdminUX1 is a Page
    title is "<actor.context.FinanceEnterpriseGroup.StudentActivitiesAdministrationTitle>"
    implements MenuSearch
    show panel control on left

    District is a DashBoardPanel
        use 12 column grid
        Setup is a Pane
            span 6 columns
            Layout
                two column distributed
                    large menu
                        list is StudentActivityDistrict.StudentActivityDistrictList
                            title is "Districts"
                            menu item name is Districts
                            description is "Create_a_district."
                            icon is icon.outline-maintenance-emerald
                        list is StudentActivityAccount.StudentActivityAccountList
                            title is "ActivityAccounts"
                            menu item name is ActivityAccounts
                            description is "Create_a_repository_of_activity_accounts,_including_bank_activities."
                            icon is icon.outline-maintenance-emerald
                        list is StudentActivitySchoolType.StudentActivitySchoolTypeList
                            title is "SchoolTypes"
                            menu item name is SchoolTypes
                            description is "Create_a_school_type."
                            icon is icon.outline-maintenance-emerald
                        list is StudentActivityAccountCode.StudentActivityAccountCodeList
                            title is "AccountCodes"
                            menu item name is AccountCodes
                            description is "Create_a_repository_of_account_codes."
                            icon is icon.outline-maintenance-emerald

    School is a DashBoardPanel
        use 12 column grid
        Setup is a Pane
            span 9 columns
            Layout
                three column distributed
                    large menu
                        list is StudentActivitySchool.StudentActivitySchoolList
                            title is "Schools"
                            menu item name is Schools
                            description is "Create_a_school."
                            icon is icon.outline-utility-azure
                        list is SchoolActivityAccount.SchoolActivityAccountList
                            title is "SchoolActivityAccounts"
                            menu item name is SchoolActivityAccounts
                            description is "Assign_activity_accounts_to_a_school."
                            icon is icon.outline-utility-azure
                        list is StudentActivitySchoolClass.StudentActivitySchoolClassList
                            title is "SchoolClasses"
                            menu item name is SchoolClasses
                            description is "Create_a_school_class."
                            icon is icon.outline-utility-azure
                        list is StudentRecord.StudentRecordList
                            title is "StudentRecords"
                            menu item name is StudentRecords
                            description is "Create_student_records_and_assign_classes_to_students."
                            icon is icon.outline-utility-azure
                        list is StudentActivityBankAccount.StudentActivityBankAccountProcessingList
                            title is "BankAccounts"
                            menu item name is BankAccounts
                            description is "Create_bank_accounts_for_a_school."
                            icon is icon.outline-utility-azure
                        list is StudentActivityVendor.StudentActivityVendorList
                            title is "Vendors"
                            menu item name is Vendors
                            description is "Create_vendors."
                            icon is icon.outline-utility-azure
                        list is StudentActivityClubSponsor.StudentActivityClubSponsorList
                            title is "ClubSponsors"
                            menu item name is ClubSponsors
                            description is "Create_club_sponsors."
                            icon is icon.outline-utility-azure
                        list is SchoolAccountCode.AccountCodeList
                            title is "SchoolAccountCodes"
                            menu item name is SchoolAccountCodes
                            description is "Assign_account_codes_to_a_school."
                            icon is icon.outline-utility-azure
        Templates is a Pane
            span 4 columns
            Layout
                single column
                    large menu
                        list is CheckDocumentTemplate.CheckDocumentTemplates
                            title is "LaserCheckTemplates"
                            menu item name is LaserCheckTemplates
                            icon is icon.outline-list-amber
                        list is ReceiptDocumentTemplate.ReceiptDocumentTemplates
                            title is "ReceiptTemplates"
                            menu item name is ReceiptTemplates
                            icon is icon.outline-list-amber
                        list is SampleDocumentTemplate.SampleTemplatesForStudentActivities
                            title is "SampleStudentActivityTemplates"
                            menu item name is SampleStudentActivityTemplates
                            icon is icon.outline-list-amber

