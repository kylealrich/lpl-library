CISIntegrationArchitectPage is a Page
    title is untranslatable:"Configurable Integration Solution"
    implements MenuSearch

    Overview is a DashBoardPanel
        use 12 column grid
        Checkpoint is a Pane
            span 6 columns
            business class is CheckPoint
                list is CheckPointList
        FailedAsyncActionTriggerList is a Pane
            span 6 columns
            business class is AsyncActionTrigger
                list is FailedAsyncActionTriggerList
        ActiveIONReceiverList is a Pane
            span 6 columns
            business class is PfiReceiver
                list is ActiveIONReceiverList
        FileChannelList is a Pane
            span 6 columns
            business class is PfiChannel
                list is FileChannelList

    InterfaceGroups is a Panel
        business class is CISInboundIntegrationModule
            list is CISInboundIntegrationModuleList

    BusinessClasses is a Panel
        business class is CISInboundIntegrationBusinessClass
            list is CISInboundIntegrationBusinessClassList

    ParameterSets is a Panel
        business class is CISInboundIntegrationParameterSet
            list is CISInboundIntegrationParameterSetList

    Parameter is a Panel
        title is "Parameters"
        business class is CISInboundIntegrationParameter
            list is CISInboundIntegrationParameterList
                title is "Parameters"

    GlobalConfiguration is a Panel
        business class is CISGlobalConfiguration
            list is CISGlobalConfigurationList

