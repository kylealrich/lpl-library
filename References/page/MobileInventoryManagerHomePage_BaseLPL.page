MobileInventoryManagerHomePage is a Page
    title is "InventoryManager"

    MobileInventoryDashBoard is a Panel
        title is "InventoryDashboard"
        Layout
            single column
                large menu
                    list is Item.MobilePrimaryDashboard
                        allow scannable search
                            open when single result
                        title is "ItemMaster"
                        menu item name is ItemMaster
                        description is "ViewItemInformation"
                        icon is icon.CSFWarehouse
                    list is InventoryLocation.MobileLocationCardList
                        allow scannable search
                            open when single result
                        title is "InventoryLocations"
                        menu item name is InventoryLocationList
                        description is "ViewAllInventoryLocations"
                        icon is icon.CSFAssetAnalyst
                    list is InventoryLocation.MobileParLocationCardList
                        allow scannable search
                            open when single result
                        title is "ParLocations"
                        menu item name is ParLocationList
                        description is "ViewAllParLocations"
                        icon is icon.CSFFranchiseManagement
                    page is MobileInventoryInformationPage
                        title is "InventoryInformation"
                        menu item name is InventoryInformation
                        description is "ViewInventoryInformation"
                        icon is icon.itemSearch
                    page is MobileInventoryTransactionsPage
                        title is "InventoryTransactions"
                        menu item name is InventoryTransactions
                        description is "ViewInventoryTransactions"
                        icon is icon.CSFInventory
                    page is MobilePhysicalInventoryPage
                        title is "PhysicalInventory"
                        menu item name is PhysicalInventory
                        description is "ViewPhysicalInventory"
                        icon is icon.outline-list-ruby

