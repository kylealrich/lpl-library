CloseManagerUX3 is a Page
    title is "PastAndOneTimeTasks"
    implements MenuSearch

    PastCloses is a MultiPanePanel
        title is "PastPeriods"
        mouse over text is "ViewYourClosedProcesses"
        pane 1
            business class is ClosePeriod
                list is ClosedPeriodList
        pane 2
            business class is pane 1.ClosePeriodTask set
                list is ClosedViewList

    ManageOneTimeTasks is a Panel
        business class is ClosePeriodTask
            list is AllOneTimeTaskList

