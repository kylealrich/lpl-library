ProjectManagerIDMPageUX1 is a Page
    title is "DocumentTemplates"
    implements MenuSearch

    DocumentTemplates is a DashBoardPanel
        title is "DocumentTemplates"
        use 12 column grid
        Templates is a Pane
            span 6 columns
            Layout
                single column
                    column1
                        large menu
                            page is ProjectDocumentTemplatesPage
                                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>_Document_Templates"
                                menu item name is ProjectDocumentTemplatesPage
                                description is "View_and_maintain_<actor.context.FinanceEnterpriseGroup.ProjectLabel>_document_templates."
                                icon is icon.window-value-charts-azure
                        large menu
                            page is ProjectDocumentsIDMTemplates
                                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>_Document_IDM_Templates"
                                menu item name is ProjectDocumentsIDMTemplates
                                description is "View_and_maintain_<actor.context.FinanceEnterpriseGroup.ProjectLabel>_document_IDM_templates."
                                icon is icon.window-value-charts-azure
                        large menu
                            list is SampleDocumentTemplate.SampleTemplatesForProjectDocuments
                                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>_Delivered_Sample_Document_Templates"
                                menu item name is ProjectSampleDocumentTemplatesPage
                                description is "View_<actor.context.FinanceEnterpriseGroup.ProjectLabel>_delivered_sample_document_templates."
                                icon is icon.window-value-charts-azure
        IDM is a Pane
            span 6 columns
            Layout
                single column
                    column1
                        large menu
                            page is MyIDMJobsPage
                                title is "MyIDMJobs"
                                menu item name is MyIDMJobs
                                description is "View_IDM_jobs."
                                icon is icon.clipboard-list-emerald
                        large menu
                            page is IDMPrinterPage
                                title is "IDMPrinters"
                                menu item name is IDMPrinters
                                description is "Setup_IDM_printers."
                                icon is icon.clipboard-list-emerald

