            list is AssetDashboard
                title is "Dashboard"

    Unreleased is a Panel
        business class is Asset
            list is AllUnreleasedAssets

    InProgress is a Panel
        business class is Asset
            list is AllPostReleasedAssets

    Released is a Panel
        business class is Asset
            list is AllReleasedAssets

    WaitingForApproval is a Panel
        business class is Asset
            list is SubmittedForApprovalList

    Disposed is a Panel
        business class is Asset
            list is AllDisposedAssets

    InterfacingAssets is a Panel
