PurchaseOrderProcessingPage is a Page
    title is "ManagePurchaseOrders"
    show panel control on left

    Unreleased is a Panel
        business class is PurchaseOrder
            list is UnreleasedPurchaseOrders

    Released is a Panel
        business class is PurchaseOrder
            list is ReleasedPurchaseOrders

    Unapproved is a Panel
        business class is PurchaseOrder
            list is UnapprovedPurchaseOrders

    All is a Panel
        business class is PurchaseOrder
            list is PurchaseOrders

    InProcess is a Panel
        business class is PurchaseOrder
            list is InProcessPurchaseOrders

    OpenRushOrders is a Panel
        business class is PurchaseOrder
            list is OpenRushOrders

    Closed is a Panel
        business class is PurchaseOrder
            list is ClosedPurchaseOrders

    Alerts is a MultiPanePanel
        pane 1
            business class is PurchaseOrderLine
                list is POLinesOverdueReceipts
        pane 2
            business class is PurchaseOrderAcknowledgementMessage
                list is POAcknowledgementMessagesForABuyer

    POSearch is a Panel
        business class is PurchaseOrder
            list is POSearchList

    LineSearch is a Panel
        business class is PurchaseOrderLine
            list is LineSearchList

    LinesByOrigin is a Panel
        business class is PurchaseOrderLineSource
            list is PurchaseOrderLineSourcesList

    AOCSearch is a Panel
        business class is PurchaseOrderAndLineAddOnCharge
            list is POInquiryByAOCs

    ReceiptSearch is a Panel
        business class is PurchaseOrderReceiptLine
            list is ReceiptSearchList

    Encumbrances is a Panel
        business class is PurchaseOrderLineDistribution
            list is PurchaseOrderCommitments

    ContractCosts is a Panel
        business class is ContractLine
            list is ContractCostsForAnItem

    Diversity is a MultiPanePanel
        pane 1
            business class is PurchaseOrderDiversity
                list is PurchaseOrdersByDiversity
        pane 2
            business class is PurchaseOrderDiversity
                list is PurchaseOrdersByDiversityTotalsOnly
        pane 3
            business class is PurchaseOrder
                list is PurchaseOrdersWithDiversity
        pane 4
            business class is PurchaseOrder
                list is PurchaseOrdersWithDiversityTotalsOnly

    AuditHistory is a Panel
        visible when (not actor.context.FinanceEnterpriseGroup.ColumnarPurchaseOrderAuditInUse)
        business class is PurchaseOrder
            list is PurchaseOrderAuditList

    AuditHistoryColumnar is a Panel
        title is "AuditHistory"
        visible when (actor.context.FinanceEnterpriseGroup.ColumnarPurchaseOrderAuditInUse)
        business class is PurchaseOrder
            list is ColumnarPurchaseOrderAuditList
                title is "AuditHistory"

    LineAuditHistory is a Panel
        visible when (not actor.context.FinanceEnterpriseGroup.ColumnarPurchaseOrderLineAuditInUse)
        business class is PurchaseOrderLine
            list is PurchaseOrderLineAuditList

    LineAuditHistoryColumnar is a Panel
        title is "LineAuditHistory"
        visible when (actor.context.FinanceEnterpriseGroup.ColumnarPurchaseOrderLineAuditInUse)
        business class is PurchaseOrderLine
            list is ColumnarPurchaseOrderLineAuditList
                title is "LineAuditHistory"

