WorkOrderListAndReportsPageUX1 is a Page
    title is "ListsAndReports"
    implements MenuSearch

    ListsAndReports is a DashBoardPanel
        use 12 column grid
        Lists is a Pane
            span 3 columns
            Layout
                single column
                    large menu
                        page is WorkOrderListPageUX1.LotAndSerialNumberTraceback
                            title is "LotAndSerialNumberTraceback"
                            menu item name is LotAndSerialNumberTraceback
                            icon is icon.clipboard-text-amethyst
                        page is WorkOrderListPageUX1.WorkOrderDistribution
                            title is "WarehouseGlobalLedgerDistribution"
                            menu item name is WarehouseGlobalLedgerDistribution
                            icon is icon.book-amethyst
                        page is WorkOrderListPageUX1.OffsiteWorkOrders
                            title is "OffsiteWorkOrders"
                            menu item name is OffsiteWorkOrders
                            icon is icon.clipboard-list-amethyst
        Reports is a Pane
            span 3 columns
            Layout
                single column
                    large menu
                        action is WarehouseSchedulePlanningReport.WarehouseSchedulePlanningReport
                            title is "WarehouseSchedulePlanningReport"
                            menu item name is WarehouseSchedulePlanningReport
                            icon is icon.clipboard-check-amber
                        action is WarehouseWorkInProcessReport.WarehouseWorkInProcessReport
                            title is "WarehouseWorkInProcessReport"
                            menu item name is WarehouseWorkInProcessReport
                            icon is icon.window-text-amber

