ReceiptProcessingByStudentPage is a Page
    title is "StudentReceiptsProcessing"

    Context Form
        Layout
            single column
                StudentActivitySchool
                    label is "School"
    Student is a MultiPanePanel
        pane 1
            business class is StudentRecord
                list is StudentReceiptProcessingList
                Context
                    StudentActivityDistrict
        pane 2
            business class is pane 1.StudentPaidReceiptsRel
                list is StudentPaidReceiptsList
        pane 4
            business class is pane 1.StudentAssignedActivityRel
                list is StudentAssignedActivityList

    OpenBatchReceipts is a MultiPanePanel
        pane 1
            business class is StudentActivityReceiptBatch
                list is OpenBatchReceipts
                Context
                    StudentActivityDistrict
                    StudentActivitySchool
        pane 2
            business class is pane 1.StudentActivityReceipt set
                list is BatchEnteredReceiptList
        pane 3
            business class is pane 1.StudentAssignedActivityByNameRel
                list is ActivitySummaryByNameList

    ReleasedBatchReceipts is a MultiPanePanel
        pane 1
            business class is StudentActivityReceiptBatch
                list is ReleasedBatchReceipts
                Context
                    StudentActivityDistrict
                    StudentActivitySchool
        pane 3
            business class is pane 1.StudentActivityReceipt set
                list is BatchEnteredReceiptList

