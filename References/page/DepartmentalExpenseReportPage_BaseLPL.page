DepartmentalExpenseReportPage is a Page
    title is "DepartmentalProcurementExpenseReport"
    is report for InventoryManager,Purchasing,ApplicationAdministrator,SupplyManagementProcessor,Buyer,PurchasingManager,SupplyManagementViewer
        category is "Analysis"

    ProcurementExpenseSummary is a Panel
        title is "Summary"
        business class is GLExpenseTotal
            list is GLExpenseTotalList
                title is "Summary"

    ProcurementExpenseDetails is a Panel
        title is "Details"
        business class is GLProcurementExpense
            list is ExpenseReportMasterList
                title is "Details"

