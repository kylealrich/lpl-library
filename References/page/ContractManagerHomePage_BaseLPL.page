ContractManagerHomePage is a Page
    title is "ContractInformation"
    implements MenuSearch

    Overview is a DashBoardPanel
        use 12 column grid
        ImportantLinks is a Pane
            span 3 columns
            Layout
                large menu
                    single column
                        page is ContractProcessingPageNew
                            title is "ManageContracts"
                            menu item name is ManageContracts
                            icon is icon.outline-report-azure
                        page is SupplierMaintenancePage
                            title is "ManageSuppliers"
                            menu item name is ManageSuppliers
                            icon is icon.group-azure
                        page is SearchContracts
                            title is "SearchContracts"
                            menu item name is SearchContracts
                            icon is icon.look-up-azure
        InvoiceCostDifferences is a Pane
            span 5 columns
            business class is PayablesInvoiceDetail
                list is ContractCostDifferencesShort
                    title is "InvoiceCostDifferences"
                    overview navigation is ContractCostDifferencesNav
        OpenServiceInvoiceMessages is a Pane
            span 4 columns
            business class is MatchInvoiceMessage
                list is OpenContractInvoiceMessagesWidget
                    title is "OpenServiceInvoiceMessages"
                    overview navigation is OpenContractInvoiceMessagesNav
        OnYourRadar is a Pane
            span 5 columns
            Layout
                two column distributed
                    column1
                    page is ExpiringAndExpiredContractsPage
                        title is "ExpiringAndExpiredContracts"
                        menu item name is ExpiringAndExpiredContracts
                    page is ContractsWithErrorsPage
                        title is "ContractsWithErrors"
                        menu item name is ContractsWithErrors
                    page is InTermsNegotiationPage
                        title is "InTermsNegotiation"
                        menu item name is InTermsNegotiation
                    page is ProposedContractsPage
                        title is "ProposedContracts"
                        menu item name is ProposedContracts
                    page is SpendAnalysis1
                        title is "SpendAnalysis"
                        menu item name is SpendAnalysis
                    page is TopContractsPage
                        title is "TopContracts"
                        menu item name is TopContracts
                    column2
                    page is MilestonesPage
                        title is "Milestones"
                        menu item name is Milestones
                    page is DeliverablesPage
                        title is "Deliverables"
                        menu item name is Deliverables
                    page is RebatesPage
                        title is "Rebates"
                        menu item name is Rebates
                    page is SupplierDialogPage1
                        title is "SupplierDialog"
                        menu item name is SupplierDialog
                    page is CostComparisonsPage
                        title is "CostComparisons"
                        menu item name is CostComparisons
        ChangeOrdersRequiringAction is a Pane
            span 7 columns
            business class is ContractVersion
                list is ChangeOrderAlertsWidget
                    title is "ChangeOrdersRequiringAction"
                    overview navigation is ChangeOrderAlertsNavigation

    SearchContracts is a Panel
        business class is Contract
            list is CMManagerDashboard

    SearchContractLines is a Panel
        business class is ContractLine
            list is ItemSearch

    ArticlesAndTerms is a Panel
        business class is Article
            list is Articles

    SearchTermsAndConditions is a Panel
        business class is TermAndCondition
            list is TAndCSearch

