EventPage is a Page
    title is "Events"

    BrowseOpenEvents is a Panel
        business class is SourcingEvent
            list is XiOpenForBid

    SearchAllEvents is a Panel
        business class is SourcingEvent
            list is XiAllEvents

    EventsWithPlanHolders is a Panel
        visible when (not session.key.SupplierGroup.SPDisablePortalPlanHolder)
        business class is SourcingEvent
            list is XiEventPlanHolders

    BidTabulations is a Panel
        visible when (session.key.SupplierGroup.UseBidTabulationReport)
        business class is SourcingEvent
            list is EventsForBidTabulation

