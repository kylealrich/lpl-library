UX1BillingProcessingPage is a Page
    title is "Processing"
    implements MenuSearch

    Dashboard is a DashBoardPanel
        use 12 column grid
        MaintenanceAndClose is a Pane
            span 12 columns
            Layout
                three column distributed
                    column1
                        large menu
                            action is InvoiceBatchRelease.InvoiceBatchRelease
                                title is "InvoiceBatchRelease"
                                menu item name is InvoiceBatchRelease
                                description is "Release invoices by batch"
                                icon is icon.check-emerald
                            action is InvoiceVerificationReport.InvoiceVerificationReport
                                title is "InvoiceVerificationReport"
                                menu item name is InvoiceVerificationReport
                                description is "Preview invoices before printing"
                                icon is icon.outline-approval-emerald
                            action is InvoiceAndRegisterPrint.InvoiceAndRegisterPrint
                                title is "InvoiceAndRegisterPrint"
                                menu item name is InvoiceAndRegisterPrint
                                description is "Process edited invoices to printed state"
                                icon is icon.receipt-emerald
                    column2
                        large menu
                            action is SubSystemUpdate.SubSystemUpdate
                                title is "SubSystemUpdate"
                                menu item name is SubSystemUpdate
                                description is "Interface printed invoices to receivables and create journal entries"
                                icon is icon.document-finance-emerald
                            action is BillingCompany.BillingSystemClose
                                title is "BillingSystemPeriodClose"
                                menu item name is BillingSystemPeriodClose
                                description is "Close billing system current period"
                                icon is icon.cycle-emerald
                            action is BaseListWorksheetCreation.BaseListWorksheetCreation
                                title is "BaseListWorksheetCreation"
                                menu item name is BaseListWorksheetCreation
                                description is "Create worksheet price bases"
                                icon is icon.check-emerald
                    column3
                        large menu
                            action is BaseListWorksheetUpdate.BaseListWorksheetUpdate
                                title is "BaseListWorksheetUpdate"
                                menu item name is BaseListWorksheetUpdate
                                description is "Updates released worksheet price bases"
                                icon is icon.cycle-emerald
        Reports is a Pane
            span 6 columns
            Layout
                two column distributed
                    column1
                        large menu
                            action is InvoiceRegister.InvoiceRegister
                                title is "InvoiceRegister"
                                menu item name is InvoiceRegister
                                description is "Reprint invoices and invoice registers"
                                icon is icon.clipboard-list-amethyst
                            action is SalesJournal.SalesJournal
                                title is "SalesJournal"
                                menu item name is SalesJournal
                                description is "Generate list of invoices from Order Entry or Billing"
                                icon is icon.clipboard-list-amethyst
                    column2
                        large menu
                            action is WorksheetReprint.WorksheetReprint
                                title is "WorksheetReprint"
                                menu item name is WorksheetReprint
                                description is "Generate list of worksheets"
                                icon is icon.clipboard-list-amethyst
                            action is ExpiredContracts.ExpiredContracts
                                title is "ExpiredContracts"
                                menu item name is ExpiredContracts
                                description is "Generate list of expired pricing contracts"
                                icon is icon.clipboard-list-amethyst
        Listings is a Pane
            span 6 columns
            Layout
                two column distributed
                    column1
                        large menu
                            list is BillingInvoiceOutputHeader.primary
                                title is "InvoiceAndRegisterPrintOutput"
                                menu item name is InvoiceAndRegisterPrintOutput
                                description is "View invoice and register print job results"
                                icon is icon.clipboard-list-amber
                            list is JournalizeGroupResult.JournalizeGroupResults
                                title is "ViewJournalizedDistributions"
                                menu item name is ViewJournalizedDistributions
                                description is "View journalized distributions and journal entries"
                                icon is icon.clipboard-list-amber
                    column2
                        large menu
                            list is BillingInvoice.BilledToDateList
                                title is "BilledToDate"
                                menu item name is BilledToDate
                                description is "View printed or global ledger updated invoices"
                                icon is icon.clipboard-list-amber
                            list is BillingInvoice.InvoicesAndDistributions
                                title is "BillingGlobalLedgerDistribution"
                                menu item name is BillingGlobalLedgerDistribution
                                description is "View billing distributions"
                                icon is icon.clipboard-list-amber

