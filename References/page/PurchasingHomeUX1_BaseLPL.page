PurchasingHomeUX1 is a Page
    title is "Overview"
    implements MenuSearch

    Purchasing is a MultiPanePanel
        pane 1
            business class is PurchaseOrder
                list is UnreleasedPurchaseOrders
        pane 2
            business class is PurchaseOrderLine
                list is POLinesOverdueReceipts
        pane 3
            business class is PurchaseOrder
                list is UnapprovedPurchaseOrders
        pane 4
            business class is PurchaseOrderAcknowledgementMessage
                list is POAcknowledgementMessagesForABuyer

    MyItems is a DashBoardPanel
        use 12 column grid
        OpenPurchaseRequests is a Pane
            span 3 columns
            business class is PurchaseOrderInterface
                list is ManagePurchaseRequestCardList
                    overview navigation is ManagePurchaseRequestNav
        UnreleasedPurchaseOrders is a Pane
            span 3 columns
            business class is PurchaseOrder
                list is UnreleasedPurchaseOrdersCardList
                    overview navigation is UnreleasedPurchaseOrderNav
        UnapprovedPurchaseOrders is a Pane
            span 3 columns
            business class is PurchaseOrder
                list is UnapprovedPurchaseOrdersCardList
                    overview navigation is UnapprovedPurchaseOrdersNav
        PastDeliveryDate is a Pane
            span 3 columns
            business class is PurchaseOrderLine
                list is MyOpenPOLinesToReceiveCardList
                    overview navigation is MyOpenPOLinesToReceiveNav
        UnreleasedBuyerMessages is a Pane
            span 3 columns
            business class is PurchaseOrderBuyerMessage
                list is MyUnreleasedBuyerMessagesCardList
                    overview navigation is MyUnreleasedBuyerMessagesNav
        VendorReturns is a Pane
            span 3 columns
            business class is VendorReturn
                list is VendorReturnCardList
                    overview navigation is VendorReturnNav
        OpenInvoiceMessages is a Pane
            span 3 columns
            business class is MatchInvoiceMessage
                list is MyOpenInvoiceMessagesCardList
                    overview navigation is MyOpenInvoiceMessagesNav
        AcknowledgementsNeedingResponse is a Pane
            span 3 columns
            business class is SupplierMessageDialog
                list is DialogNeedingResponsesCardList
                    overview navigation is DialogResponsesNav
        AcknowledgementMessagesNeedingAction is a Pane
            span 6 columns
            business class is PurchaseOrderAcknowledgementMessage
                list is POAcknowledgementMessagesForABuyerCardList
                    overview navigation is POAcknowledgementMessagesForABuyerNav

    Receiving is a MultiPanePanel
        pane 1
            business class is PurchaseOrderReceiptLine
                list is ReceiptLinesPendingInspection
        pane 2
            business class is POReceiptAdjustmentAndInspection
                list is PurchaseOrderReceiptAdjustmentList
        pane 3
            business class is POReceiptAdjustmentAndInspection
                list is PurchaseOrderReceiptInspectionList
        pane 4
            business class is PurchaseOrderReceipt
                list is MatchReferenceAdjustmentList

    AutoMatchResults is a Panel
        business class is AutoMatchResult
            list is PurchasingManagerAutoMatchResults

