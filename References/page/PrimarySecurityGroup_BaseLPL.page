PrimarySecurityGroup is a Page
    title is "SecurityGroups"

    ChartAccount is a Panel
        title is "ChartAccountSecurityGroups_(FEG_Dimension_'\\<actor.context.FinanceEnterpriseGroup.AccountLabel>')"
        business class is FinanceDimensionSecurityGroup
            list is ChartAccountSecurityGroupList
                title is "ChartAccountSecurityGroups_(FEG_Dimension_'\\<actor.context.FinanceEnterpriseGroup.AccountLabel>')"
            Context
                FinanceEnterpriseGroup

    Ledger is a Panel
        title is "LedgerSecurityGroups"
        business class is FinanceDimensionSecurityGroup
            list is LedgerBasisSecurityGroupList
                title is "LedgerSecurityGroups"
            Context
                FinanceEnterpriseGroup

    AccountingEntity is a Panel
        title is "AccountingEntitySecurityGroups_(FEG_Dimension_'\\<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>')"
        business class is FinanceDimensionSecurityGroup
            list is AccountingEntitySecurityGroupList
                title is "AccountingEntitySecurityGroups_(FEG_Dimension_'\\<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>')"

    AccountingUnit is a Panel
        title is "AccountingUnitSecurityGroups_(FEG_Dimension_'\\<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>')"
        visible when (actor.context.FinanceEnterpriseGroup.AccountingUnitSelected)
        business class is AccountingUnitSecurityGroup
            list is AccountingUnitSecurityGroups
                title is "AccountingUnitSecurityGroups_(FEG_Dimension_'\\<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>')"

    Projects is a Panel
        title is "ProjectSecurityGroups_(FEG_Dimension_'\\<actor.context.FinanceEnterpriseGroup.ProjectLabel>')"
        visible when (actor.context.FinanceEnterpriseGroup.ProjectEntry)
        business class is FinanceDimensionSecurityGroup
            list is ProjectSecurityGroupList
                title is "ProjectSecurityGroups_(FEG_Dimension_'\\<actor.context.FinanceEnterpriseGroup.ProjectLabel>')"
            Context
                FinanceEnterpriseGroup

