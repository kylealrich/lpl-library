EventsUX1 is a Page
    title is "Events"
    implements MenuSearch

    AllEvents is a DashBoardPanel
        use 12 column grid
        Events is a Pane
            span 3 columns
            Layout
                single column
                    page is EventProcessingPage
                        title is "ManageEvents"
                        menu item name is ManageEvents
                        icon is icon.document-azure
        VendorsAndSuppliers is a Pane
            span 6 columns
            title is "Vendors_andSuppliers"
            Layout
                two column distributed
                    page is PayablesVendorsUX1
                        title is "Vendors"
                        menu item name is Vendors
                        icon is icon.hcm-emerald
                    page is SupplierPage
                        title is "Suppliers"
                        menu item name is Suppliers
                        icon is icon.hcm-emerald
                    list is Subcontractor.SubcontractorList
                        title is "Subcontractors"
                        menu item name is Subcontractors
                        icon is icon.hcm-emerald
                    list is Supplier.SupplierMetricsList
                        title is "SupplierEventMetrics"
                        menu item name is SupplierEventMetrics
                        icon is icon.hcm-emerald
                    page is SupplierMessagePage
                        title is "Messages_andDialog"
                        menu item name is SupplierMessagesAndDialog
                        icon is icon.hcm-emerald
                    action is Supplier.CreateAdHocSupplierMessages
                        title is "CreateAdHocMessages"
                        menu item name is CreateAdHocSupplierMessages
                        icon is icon.cycle-azure
                    list is SupplierVendorMismatch.SupplierVendorMismatchList
                        title is "Supplier_andVendorComparison"
                        menu item name is SupplierAndVendorComparison
                        icon is icon.hcm-emerald

