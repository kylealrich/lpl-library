CapitalizationErrorsPage is a Page
    title is "CapitalizationErrors"
    implements MenuSearch
    is report for AssetManager,AssetAnalyst,ApplicationAdministrator,FinancialsViewer,Assets,FinancialsProcessor
        category is "Asset"

    InterfaceControlErrors is a Panel
        title is "InterfaceControlErrors"
        business class is AssetInterfaceOptions
            list is ErrorProjectInterfaceOptionsList
                title is "InterfaceControlErrors"

    DraftProjectInterfaceErrors is a Panel
        title is "DraftProjectInterfaceErrors"
        visible when (actor.context.FinanceEnterpriseGroup.CapitalizationUpgradeCompleted)
        business class is AssetCapitalTransaction
            list is DraftProjectInterfaceErrors
                title is "DraftProjectInterfaceErrors"

    ProjectAssetInterfaceErrors is a Panel
        title is "ProjectAssetInterfaceErrors"
        business class is ProjectAssetInterface
            list is ProjectAssetInterfaceErrors
                title is "ProjectAssetInterfaceErrors"

