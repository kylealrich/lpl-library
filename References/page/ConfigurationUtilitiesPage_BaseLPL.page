ConfigurationUtilitiesPage is a Page
    title is "Utilities"
    implements MenuSearch

    Utilities is a DashBoardPanel
        use 12 column grid
        Troubleshooting is a Pane
            span 3 columns
            icon is icon.utilities
            Layout
                single column
                    small menu
                        action is CdVerify.RunCommand
                            title is "VerifyConfigurations"
                            menu item name is CdVerifyRunCommand
                            Search Terms
                                    "CdVerify"
                    small menu
                        action is ConfigEntity.ValidateConfigurations
                            title is "ValidateConfigurations"
                            menu item name is ConfigEntityValidateConfigurations
                    small menu
                        action is ConfigEntity.ScheduleValidateConfigurations
                            title is "ScheduledValidateConfigurations"
                            menu item name is ConfigEntityScheduleValidateConfigurations
                    small menu
                        list is ConfigEntityResults.ValidateConfigurationsResults
                            title is "ValidateConfigurationsResults"
                            menu item name is ConfigEntityResultsValidateConfigurationsResults
                    small menu
                        action is MenuConfigurationLogging.RunCommand
                            title is "MenuConfigurationLogging"
                            menu item name is MenuConfigurationLoggingRunCommand
                    small menu
                        page is ConfigurationCacheManager
                            title is "ConfigurationCacheManager"
                            menu item name is ConfigurationCacheManager
                    small menu
                        action is ClearConfigurations.RunCommand
                            title is "ClearConfigurations"
                            menu item name is ClearConfigurationsRunCommand
        ImportAndExport is a Pane
            span 3 columns
            icon is icon.import-spreadsheet
            Layout
                two column
                    small menu
                        action is CdImport.RunCommand
                            title is "ImportConfigurations"
                            menu item name is CdImportRunCommand
                            Search Terms
                                    "CdImport"
                    small menu
                        action is CdExport.RunCommand
                            title is "ExportConfigurations"
                            menu item name is CdExportRunCommand
                            Search Terms
                                    "CdExport"
                    small menu
                        action is CdExportPersonalization.RunCommand
                            title is "ExportPersonalizations"
                            menu item name is CdExportPersonalizationRunCommand
                            Search Terms
                                    "CdExportPersonalization"
                    small menu
                        action is CdImportList.RunCommand
                            title is "CreateSourceFileManifest"
                            menu item name is CdImportListRunCommand
                            Search Terms
                                    "SourceFileManifest"
                                    "CdImportList"
        Maintenance is a Pane
            span 3 columns
            icon is icon.tools
            Layout
                single column
                    small menu
                        action is UserCube.ApplyPendingChanges
                            title is "DeployCubes"
                            menu item name is UserCubeApplyPendingChanges
                    small menu
                        action is CdDelete.RunCommand
                            title is "DeleteConfigurations"
                            menu item name is CdDeleteRunCommand
                            Search Terms
                                    "CdDelete"
                    small menu
                        action is CdDeletePersonalization.RunCommand
                            title is "DeletePersonalization"
                            menu item name is CdDeletePersonalizationRunCommand
                            Search Terms
                                    "CdDeletePersonalizationRunCommand"
                    small menu
                        action is PurgePersonalizations.Purge
                            title is "PurgePersonalizations"
                            menu item name is PurgePersonalizationsPurge
                    small menu
                        list is ConfigEntity.ConfigEntityDuplicateList
                            title is "ManageDuplicates"
                            menu item name is ConfigEntityDuplicateList
        DataArea is a Pane
            span 3 columns
            icon is icon.area
            Layout
                single column
                    small menu
                        list is MimeType.MimeTypeList
                            title is "MIMETypes"
                            menu item name is MimeTypeMimeTypeList
                    small menu
                        page is TimeZonePage
                            title is "TimeZoneComponents"
                            menu item name is TimeZonePage
                    small menu
                        list is UserPhrase.Phrases
                            title is "Phrases"
                            menu item name is UserPhrasePhrases
        Business_LogicBasicAccess is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        list is UserProd.primary
                            title is "ProductLine"
                            menu item name is UserProdprimary
                    small menu
                        list is UserAction.primary
                            title is "Actions"
                            menu item name is UserActionprimary
                    small menu
                        list is UserCube.primary
                            title is "Cubes"
                            menu item name is UserCubeprimary
                    small menu
                        list is UserBusinessClass.primary
                            title is "BusinessClasses"
                            menu item name is UserBusinessClassprimary
                    small menu
                        list is UserField.primary
                            title is "Fields"
                            menu item name is UserFieldprimary
                    small menu
                        list is UserRelation.primary
                            title is "Relations"
                            menu item name is UserRelationprimary
        User_InterfaceBasicAccess is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        list is UserPage.ConfigConsoleList
                            title is "Pages"
                            menu item name is UserPageConfigConsoleList
                    small menu
                        list is UserForm.ConfigConsoleList
                            title is "Forms"
                            menu item name is UserFormConfigConsoleList
                    small menu
                        list is UserCardView.ConfigConsoleList
                            title is "CardViews"
                            menu item name is UserCardViewConfigConsoleList
                    small menu
                        list is UserSJRptMap.ConfigConsoleList
                            title is "StaticReports"
                            menu item name is UserSJRptMapConfigConsoleList
        NavigationBasicAccess is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        list is UserMenu.ConfigConsoleList
                            title is "Menus"
                            menu item name is UserMenuConfigConsoleList
                    small menu
                        list is UserWebApp.ConfigConsoleList
                            title is "WebApplications"
                            menu item name is UserWebAppConfigConsoleList
                    small menu
                        list is UserNavigation.ConfigConsoleList
                            title is "Navigations"
                            menu item name is UserNavigationConfigConsoleList

