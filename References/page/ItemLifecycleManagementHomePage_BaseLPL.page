ItemLifecycleManagementHomePage is a Page
    title is "Overview"
    implements MenuSearch

    Alerts is a DashBoardPanel
        show 4 columns
        PastDueTasks is a Pane
            double width
            business class is RecallNotice
                list is PastDueTasksCardList
                    overview navigation is PastDueTasksNav
        UnresolvedNoticeQuantities is a Pane
            double width
            business class is RecallNotice
                list is UnresolvedQuantitiesCardList
                    overview navigation is UnresolvedQuantitiesNav
        QAndAToAnswer is a Pane
            double width
            business class is RecallNoticeQandA
                list is MyOpenQandACardList
        MyOpenTasks is a Pane
            double width
            business class is RecallNoticeTask
                list is MyOpenRecallNoticeTasksCardList
                    overview navigation is MyOpenRecallNoticeTasksNav
        NoticeUpdates is a Pane
            double width
            business class is RecallNotice
                list is RecallNoticeUpdatesCardList
                    overview navigation is RecallNoticeUpdatesNav

    OpenTasks is a Panel
        mouse over text is "MyOpenTasks"
        business class is RecallNoticeTask
            list is MyOpenRecallNoticeTasks

    FinishedTasks is a Panel
        mouse over text is "MyFinishedTasks"
        business class is RecallNoticeTask
            list is MyFinishedRecallNoticeTasks
            Context
                RecallGroup

    OpenItemProposals is a Panel
        mouse over text is "MyOpenItemProposals"
        business class is RecallNoticeTask
            list is MyOpenItemProposals
            Context
                RecallGroup

    FinishedItemProposals is a Panel
        mouse over text is "MyFinishedItemProposals"
        business class is RecallNoticeTask
            list is MyFinishedItemProposals
            Context
                RecallGroup

    NoticeReporting is a Panel
        Layout
            two column
                small menu
                    action is RecallNotice.Pending.RecallPortalCreate
                        title is "ReportANotice"
                        menu item name is ReportNotice
                        icon is icon.CSFProject
                    list is RecallNotice.MyReportedRecallPortalNotices
                        title is "ViewMyReportedNotices"
                        menu item name is ViewNotices
                        icon is icon.CSFFinancialBusinessAnalyst

    Setup is a Panel
        Layout
            single column
                large menu
                    page is ItemLifecycleManagementSetupPage.Groups
                        title is "Groups"
                        menu item name is ItemLifecycleGroups
                        icon is icon.window-settings-amethyst
                    page is ItemLifecycleManagementSetupPage.Users
                        title is "Users"
                        menu item name is ItemLifecycleUsers
                        icon is icon.window-settings-amethyst
                    page is ItemLifecycleManagementSetupPage.Organizations
                        title is "Organizations"
                        menu item name is ItemLifecycleOrganizations
                        icon is icon.window-settings-amethyst
                    page is ItemLifecycleManagementSetupPage.TaskInstructions
                        title is "TaskInstructions"
                        menu item name is ItemLifecycleTaskInstructions
                        icon is icon.window-settings-amethyst
                    page is ItemLifecycleManagementSetupPage.Types
                        title is "Types"
                        menu item name is ItemLifecycleTypes
                        icon is icon.window-settings-amethyst
                    page is ItemLifecycleManagementSetupPage.Domains
                        title is "Domains"
                        menu item name is ItemLifecycleDomains
                        icon is icon.window-settings-amethyst
                    page is ItemLifecycleManagementSetupPage.ActionReasons
                        title is "ActionReasons"
                        menu item name is ActionReasons
                        icon is icon.window-settings-amethyst

