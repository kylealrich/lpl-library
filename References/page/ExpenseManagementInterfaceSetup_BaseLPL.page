ExpenseManagementInterfaceSetup is a Page
    title is "ExpenseManagementInterface"

    Configuration is a Panel
        business class is ExpenseManagementInterfaceConfiguration
            list is ExpenseManagementInterfaceConfigurationList

    CreditCardVendors is a Panel
        business class is ExpenseManagementInterfaceCreditCardIssuer
            list is ExpenseManagementInterfaceCreditCardIssuerList

    ExpenseTypes is a Panel
        business class is ExpenseManagementInterfaceExpenseType
            list is ExpenseManagementInterfaceExpenseTypeList

    FundAccountingRules is a Panel
        visible when (actor.context.FinanceEnterpriseGroup.FundAccounting)
        business class is ExpenseManagementFinanceStructureRule
            list is ExpenseManagementFinanceStructureRulesPrimary

