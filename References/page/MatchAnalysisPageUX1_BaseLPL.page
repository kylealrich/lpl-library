MatchAnalysisPageUX1 is a Page
    title is "Analysis"
    implements MenuSearch

    StartAging is a Panel
        visible when (not actor.context.FinanceEnterpriseGroup.MatchAgingComplete)
        business class is MatchAging
            list is MatchAgingList

    Aging is a DashBoardPanel
        use 12 column grid
        VendorAgingWithPercents is a Pane
            span 6 columns
            title is "ByVendor"
            business class is MatchAgingSummary
                list is VendorPeriod4List
                    overview navigation is MatchAgingPageNav
        YearMonthAgingWithPercents is a Pane
            span 6 columns
            title is "ByMonth"
            business class is MatchAgingSummary
                list is YearMonthPeriod4List
                    overview navigation is MatchAgingPageNav
        YearMonthAgingChart is a Pane
            span 6 columns
            title is "ByMonthBarChart"
            business class is MatchAgingSummary
                list is YearMonthAgingChart
        UnmatchedCard is a Pane
            span 6 columns
            title is "Unmatched"
            business class is MatchAgingSummary
                list is VendorUnmatched

    MatchingRate is a DashBoardPanel
        use 12 column grid
        YearMonth is a Pane
            span 12 columns
            title is "YearMonth"
            business class is MatchingRate
                list is MatchingRateDescendingList
                    overview navigation is MatchingRatePageNav

    AllTransactions is a MultiPanePanel
        pane 1
            business class is PayablesInvoice
                list is MatchAnalysisList
        pane 2
            business class is pane 1.PayablesInvoiceDetail set
                list is MatchAnalysisList
        pane 3
            business class is pane 1.MatchPurchaseOrderInvoicesRel
                list is MatchAnalysisPurchaseOrderList
        pane 4
            business class is pane 3.PurchaseOrderReceiptLineRel
                list is MatchAnalysisList

    Summarization is a MultiPanePanel
        pane 1
            business class is MatchPurchaseOrderInvoice
                list is MatchAnalysisList
        pane 2
            business class is pane 1.PayablesInvoice.MatchPurchaseOrderInvoicesRel
                list is MatchAnalysisPurchaseOrderList
        pane 3
            business class is pane 1.InvoicesForAPurchaseOrderRel
                list is MatchAnalysisInvoiceList
        pane 4
            business class is pane 1.PurchaseOrderReceiptRel
                list is MatchAnalysisList

    More is a DashBoardPanel
        title is "More"
        use 12 column grid
        Analysis is a Pane
            span 4 columns
            Layout
                single column
                    large menu
                        list is PayablesInvoice.MatchAnalysisDashboard
                            title is "InvoiceAnalysis"
                            menu item name is InvoiceAnalysis
                            description is "AnalysisStartingFromMatchInvoiceToPurchaseOrderAndReceipt"
                            icon is icon.look-up-amethyst
                        list is PurchaseOrder.MatchAnalysisDashboard
                            title is "PurchaseOrderAnalysis"
                            menu item name is PurchaseOrderAnalysis
                            description is "AnalysisStartingFromPurchaseOrderToInvoiceAndReceipt"
                            icon is icon.look-up-amethyst
                        list is PayablesInvoiceDetail.MatchLineAnalysisListWithDetails
                            title is "AnalysisDetails"
                            menu item name is AnalysisDetails
                            description is "MatchAnalysisOfAllTransactions,AnalysisDetailsAndSummarization"
                            icon is icon.look-up-amethyst

