ContractProcessingPageNew is a Page
    title is "Contracts"
    implements MenuSearch

    Contracts is a Panel
        business class is Contract
            list is ContractSearch

    Dashboard is a Panel
        business class is Contract
            list is CMManagerDashboard

    InProcess is a Panel
        business class is Contract
            list is InProcessContracts

    Lines is a Panel
        business class is ContractLine
            list is ItemSearch

    CreateAndSearch is a DashBoardPanel
        use 12 column grid
        Create is a Pane
            span 3 columns
            title is "Create"
            Layout
                single column
                    small menu
                        action is Contract.Draft.Create
                            title is "Manually"
                            menu item name is Manually
                            icon is icon.document-certified-amethyst
                            Search Terms
                                    "CreateContractManually"
                        action is Contract.Draft.CreateContractFromContract
                            title is "FromAnotherContract"
                            menu item name is FromContract
                            icon is icon.document-certified-amethyst
                        list is Contract.ContractsToCopyFromSelectedLines
                            title is "FromAnotherContractForSelectedLines"
                            menu item name is FromAnotherContractForSelectedLines
                            icon is icon.document-certified-amethyst
                        action is Contract.Draft.CreateFromInterview
                            title is "FromInterview"
                            menu item name is FromInterview
                            icon is icon.document-certified-amethyst
                        action is Contract.Draft.CreateFromWizard
                            title is "UsingWizard"
                            menu item name is FromWizard
                            icon is icon.document-certified-amethyst
                        action is Contract.Draft.CreateContractFromTemplate
                            title is "FromTemplate"
                            menu item name is FromTemplate
                            icon is icon.document-certified-amethyst
        AdditionalSearch is a Pane
            span 9 columns
            title is "SearchRelatedData"
            Layout
                three column
                    small menu
                        list is ContractLine.ContractCostsForAnItem
                            title is "ContractCosts"
                            menu item name is ContractCosts
                            icon is icon.money-bag-amber
                        list is ContractParticipant.ContractParticipantSearch
                            title is "Participants"
                            menu item name is Participants
                            icon is icon.search-amber
                        list is ContractDistributorPricingMember.DistributorMemberSearch
                            title is "DistributorMembers"
                            menu item name is DistributorMembers
                            icon is icon.search-amber
                        list is ContractTierMember.ManufacturerMemberSearch
                            title is "ManufacturerMembers"
                            menu item name is ManufacturerMembers
                            icon is icon.search-amber
                        list is ContractAttachedContact.ContractContactSearch
                            title is "Contacts"
                            menu item name is Contacts
                            icon is icon.search-amber
                        list is ContractSubcontractor.ContractSubcontractorSearch
                            title is "Subcontractors"
                            menu item name is Subcontractors
                            icon is icon.search-amber
                        list is ContractQuestionAnswer.QuestionAnswerSearchList
                            title is "QuestionsAnswers"
                            menu item name is QuestionAnswers
                            icon is icon.search-amber
                        list is ContractTermAndCondition.TermAndConditionSearch
                            title is "TermsAndConditions"
                            menu item name is TermsAndConditions
                            icon is icon.search-amber
                        list is ContractComment.CommentSearch
                            title is "Comments"
                            menu item name is Comments
                            icon is icon.search-amber
                        list is ContractAttachment.AttachmentSearch
                            title is "Attachments"
                            menu item name is Attachments
                            icon is icon.search-amber
                        list is ContractRebate.RebateSearch
                            title is "Rebates"
                            menu item name is Rebates
                            icon is icon.search-amber
                        list is ContractMilestone.MilestoneSearch
                            title is "Milestones"
                            menu item name is Milestones
                            icon is icon.search-amber
                        list is ContractDeliverable.DeliverableSearch
                            title is "Deliverables"
                            menu item name is Deliverables
                            icon is icon.search-amber
                        list is ContractPurchaseFrom.PurchaseFromSearch
                            title is "PurchaseFroms"
                            menu item name is PurchaseFroms
                            icon is icon.search-amber

    Alerts is a DashBoardPanel
        use 12 column grid
        Alerts is a Pane
            title is "Alerts"
            Layout
                two column distributed
                    small menu
                        page is ContractAdditionalAlerts.ContractAlerts
                            title is "Alerts"
                            menu item name is ContractAlerts
                            icon is icon.clipboard-list-emerald
                        list is ContractLine.ErrorsToFix
                            title is "LineErrorsToBeFixed"
                            menu item name is LineErrorsToBeFixed
                            icon is icon.clipboard-list-emerald
                        list is Contract.ToleranceErrors
                            title is "CostUpdateToleranceErrors"
                            menu item name is ToleranceErrors
                            icon is icon.clipboard-list-emerald
                        list is ContractSubcontractor.SubcontractorAlert
                            title is "ContractSubcontractorsWithPaymentsOwed"
                            menu item name is Subcontractors
                            icon is icon.clipboard-list-emerald
                        list is Contract.ContractsWithProcurementTemplates
                            title is "TemplatesToUpdate"
                            menu item name is ProcurementTemplatesToUpdate
                            icon is icon.clipboard-list-emerald
                        list is Contract.ContractsCreatedFromEvents
                            title is "UnreleasedContractsCreatedFromEvents"
                            menu item name is UnreleasedContractsCreatedFromEvents
                            icon is icon.clipboard-list-emerald
                        list is ContractLineSuggestedItem.SuggestedLineChanges
                            title is "SuggestedLineChanges"
                            menu item name is SuggestedLineChanges
                            icon is icon.clipboard-list-emerald

