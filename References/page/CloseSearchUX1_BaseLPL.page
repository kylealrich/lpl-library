    Administration is a Panel
        title is "Administration"
        Layout
            two column distributed
                column1
                    large menu
                        list is CloseManagementGroup.CloseManagementGroupList
                            title is "CloseManagementGroups"
                            menu item name is CloseManagementGroup
                            description is "Set_up_close_management_groups"
                            icon is icon.CSFSourcing
                        list is SystemCalendar.SystemCalendars
                            title is "SystemCalendars"
                            menu item name is SystemCalendar
                            description is "Set_up_system_calendars"
                            icon is icon.CSFOrderEntry
                        page is FinanceResourceAdmin
                            title is "FinanceResources"
                            menu item name is FinanceResource
                            description is "Set_up_finance_resources"
                            icon is icon.CSFChiefFinancialOfficer
                        list is FinanceTeam.BuildTeamList
                            title is "FinanceTeams"
                            menu item name is FinanceTeam
                            description is "Set_up_finance_teams"
                            icon is icon.CSFAssetAnalyst
                        page is CloseTaskTypeSetupPage
                            title is "TaskTypes"
                            menu item name is CloseTaskType
                            description is "Set_up_task_types"
                            icon is icon.CSFInventory
                column2
                    large menu
                        page is CloseManageProcessPage
                            title is "ManageProcess"
                            menu item name is ManageProcess
                            description is "Set_up_close_processes_and_schedule_periods"
                            icon is icon.CSFProject
                        list is ActionReason.ActionReasonList
                            title is "ActionReasons"
                            menu item name is ActionReason
                            description is "Set_up_action_reasons"
                            icon is icon.CSFSupplyManagement
                        list is ApprovalCode.ApprovalCodes
                            title is "ApprovalCodes"
                            menu item name is ApprovalCode
                            description is "Set_up_approval_codes"
                            icon is icon.CSFApprover
                        list is SampleDocumentTemplate.SampleSpreadsheetsForCloseTasks
                            title is "SampleCloseTaskInterface"
                            menu item name is SampleCloseTaskInterface
                            description is "Sample_spreadsheets_to_upload_close_processes"
                            icon is icon.CSFReceivables

