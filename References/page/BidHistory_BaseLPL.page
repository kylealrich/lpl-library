BidHistory is a Page
    title is "MyResponses"

    UnsubmittedInitialResponses is a Panel
        business class is SourcingEventResponse
            list is UnsubmittedResponses

    UnsubmittedBestAndFinal is a Panel
        business class is SourcingEventResponse
            list is UnsubmittedBestAndFinalResponses

    UnsubmittedPricing is a Panel
        business class is SourcingEventResponse
            list is UnsubmittedPricingResponses

    OpenSubmittedResponses is a Panel
        title is "ResponsesOpenForAward"
        business class is SourcingEventResponse
            list is OpenSubmittedResponses
                title is "ResponsesOpenForAward"

    EventHistory is a Panel
        title is "HistoricalResponses"
        business class is SourcingEventResponse
            list is EventHistory
                title is "HistoricalResponses"

    ModificationRequests is a Panel
        visible when (session.key.SupplierGroup.CanDisplayResponseModificationRequests)
        business class is SupplierContactMessage
            list is ModificationRequests

