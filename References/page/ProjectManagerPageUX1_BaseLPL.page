ProjectManagerPageUX1 is a Page
    title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
    implements MenuSearch
    is report for Projects

    MyProjects is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
        business class is FinanceEnterpriseGroup.first EnterpriseProjectStructureRel.MySummaryProjectsRel
            list is XiMySummaryProjectList
                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
            Context
                FinanceEnterpriseGroup

    MyTeamProjects is a Panel
        title is "Team<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
        business class is FinanceEnterpriseGroup.first EnterpriseProjectStructureRel.MySummaryProjectsTeamRel
            list is XiMySummaryProjectList
                title is "Team<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
            Context
                FinanceEnterpriseGroup

    ProjectLaborHistory is a Panel
        title is "LaborHistory"
        business class is FinanceEnterpriseGroup.first EnterpriseProjectStructureRel.MySummaryProjectsRel
            list is XiMySummaryLaborProjectList
                title is "LaborHistory"
            Context
                FinanceEnterpriseGroup

    ProjectBillableLabor is a Panel
        title is "BilledLabor"
        business class is Employee.MyActiveProjectContractInvoiceLinesBilledRel
            list is MyProjectBillableLaborInvoiceLinesDetail
                title is "BilledLabor"
            Context
                HROrganization
                Employee

