UX1AssetTax is a Page
    title is "Tax"
    implements MenuSearch

    BookRegulations is a Panel
        business class is BookRegulation
            list is BookRegulations

    GuidelineClasses is a Panel
        business class is AssetGuidelineClass
            list is AssetGuidelineClasses

    GuidelineClassRegulations is a Panel
        business class is AssetGuidelineClassRegulation
            list is AssetGuidelineClassRegulations

    TaxCreditTables is a Panel
        business class is AssetTaxCreditTable
            list is AssetTaxCreditTableList

    PersonalUseCodes is a Panel
        business class is PersonalUseCode
            list is PersonalUseCodeList

    Reports is a Panel
        Layout
            three column distributed
                column1
                    large menu
                        action is AssetTaxCreditReport.AssetTaxCreditReport
                            title is "TaxCredit"
                            menu item name is TaxCredit
                            icon is icon.check-azure
                            Search Terms
                                    "TaxCreditReport"
                column2
                    large menu
                        action is PropertyReport.PropertyReport
                            title is "Property"
                            menu item name is Property
                            icon is icon.check-azure
                            Search Terms
                                    "PropertyReport"
                column3
                    large menu
                        action is AssetRecaptureReport.AssetRecaptureReport
                            title is "Recapture"
                            menu item name is Recapture
                            description is "View_depreciation_and_tax_credit_recapture_by_book"
                            icon is icon.check-azure
                            Search Terms
                                    "RecaptureReport"
                                    "AssetRecaptureReport"

