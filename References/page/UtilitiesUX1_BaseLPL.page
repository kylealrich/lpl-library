UtilitiesUX1 is a Page
    title is "Utilities"
    implements MenuSearch

    AllUtilities is a DashBoardPanel
        use 12 column grid
        Actions is a Pane
            span 3 columns
            Layout
                single column
                    action is PurchaseOrder.RefreshBuyerCubes
                        title is "RefreshPurchasingCubes"
                        menu item name is RefreshPurchasingCubes
                        icon is icon.cycle-settings-amethyst
                    action is UserDefaultPrinter.OverrideDefaultIDMPrinter
                        title is "OverrideDefaultIDMPrinter"
                        menu item name is OverrideDefaultIDMPrinter
                        icon is icon.cycle-settings-amethyst
                    action is FaxDetail.CheckAllFaxStatus
                        title is "CheckAllFaxStatus"
                        menu item name is CheckAllFaxStatus
                        icon is icon.cycle-settings-amethyst
                    action is FaxDetail.ResendErroredFaxes
                        title is "ResendErroredFaxes"
                        menu item name is ResendErroredFaxes
                        icon is icon.cycle-settings-amethyst
        Lists is a Pane
            span 4 columns
            Layout
                single column
                    page is MyIDMJobsPage
                        title is "MyIDMJobs"
                        menu item name is MyIDMJobs
                        icon is icon.clipboard-list-ruby
                    list is SampleDocumentTemplate.SampleDocumentTemplatesForPurchaseOrder
                        title is "SampleDocumentTemplatesForPurchasing"
                        menu item name is SampleDocumentTemplatesForPurchaseOrder
                        icon is icon.clipboard-list-ruby
                    list is SampleDocumentTemplate.FactSheetsForPurchaseOrder
                        title is "PurchaseOrderFactSheets"
                        menu item name is PurchaseOrderFactSheets
                        icon is icon.clipboard-list-ruby
                    list is SampleDocumentTemplate.FactSheetsForStrategicSourcing
                        title is "StrategicSourcingFactSheets"
                        menu item name is StrategicSourcingFactSheets
                        icon is icon.clipboard-list-ruby
        PurgeAndArchival is a Pane
            span 3 columns
            title is "Purge_andArchival"
            Layout
                single column
                    action is POOutputPurge.POOutputPurge
                        title is "PurgePOOutput"
                        menu item name is POOutputPurge
                        icon is icon.cycle-azure
                    action is PurchaseOrder.PurgeClosedPurchaseOrder
                        title is "PurgePurchaseOrders"
                        menu item name is PurgePurchaseOrders
                        icon is icon.cycle-azure
                    action is PurchaseOrderReceipt.PurgePurchaseOrderReceipt
                        title is "PurgePurchaseOrderReceipts"
                        menu item name is PurgePurchaseOrderReceipts
                        icon is icon.cycle-azure
                    action is VendorReturn.PurgeClosedVendorReturn
                        title is "PurgeVendorReturns"
                        menu item name is PurgeVendorReturns
                        icon is icon.cycle-azure
                    action is Contract.PurgeContract
                        title is "PurgeContracts"
                        menu item name is PurgeContracts
                        icon is icon.cycle-azure

