FileCreationAllDataPage is a Page
    title is "AllSetupData"

    Records is a Panel
        title is "Records"
        business class is FileCreationRecord
            list is FileCreationRecordPrimaryList
                title is "Records"

    Files is a MultiPanePanel
        pane 1
            business class is FileCreationFile
                list is FileCreationFilePrimaryList
                    title is "GeneratedFiles"
        pane 2
            business class is pane 1.FileCreationRecordRel
                list is FileCreationRecordFileList
                    title is "IncludedRecords"

    Mappings is a MultiPanePanel
        pane 1
            business class is FileCreationRowDetail
                list is FileCreationRowDetailPageList
                    title is "Row"
        pane 2
            business class is pane 1.FileCreationColumnDetailAndLinkedColumnRel
                list is LinkedFileCreationColumnDetailList
                    title is "Column"

