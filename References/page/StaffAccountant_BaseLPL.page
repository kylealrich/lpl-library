            three column
                column1
                    header1 of "SetupInquiries"
                    large menu
                        list is FinanceEnterpriseGroup.FinanceEnterpriseGroupList
                            title is "FinanceEnterpriseGroup"
                            menu item name is FinanceEnterpriseGroup
                            description is "Set_up_and_maintain_finance_enterprise_group"
                            icon is icon.CSFChiefFinancialOfficer
                        page is ChartAccountCompare
                            title is "ReportingChart"
                            menu item name is ChartAccountCompare
                            description is "Set_up_and_maintain_reporting_chart"
                            icon is icon.CSFChiefFinancialOfficer
                        list is FinanceEnterpriseGroup.FinanceEnterpriseGroupStaffAccountantList
