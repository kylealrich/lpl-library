                            menu item name is FinanceTransactions
                            description is "View_transactions"
                            icon is icon.CSFFinancialBusinessAnalyst
                        page is TotalsPage
                            title is "Totals"
                            menu item name is Totals
                            description is "View_totals"
                            icon is icon.CSFFinancialBusinessAnalyst
