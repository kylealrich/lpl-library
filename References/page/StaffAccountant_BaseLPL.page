                            menu item name is AccountingEntity
                            description is "Set_up_and_maintain_accounting_entity"
                            icon is icon.CSFChiefFinancialOfficer
                column2
                    header1 of "Processing"
                    large menu
                        page is AllocationControlPage
                            title is "Allocations"
