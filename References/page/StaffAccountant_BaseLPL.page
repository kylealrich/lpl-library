                            menu item name is Journals
                            description is "Create_and_view_journals"
                            icon is icon.CSFStaffAccountant
                        list is EntityLedgerPeriod.EntityLedgerPeriodList
                            title is "PeriodStatus"
                            menu item name is PeriodStatus
                            description is "Maintain_period_status"
                            icon is icon.CSFStaffAccountant
                        form is FinanceResource.MyResourceForm
                            title is "ClosingTasks"
                            menu item name is ClosingTasks
                            description is "Maintain_closing_tasks"
