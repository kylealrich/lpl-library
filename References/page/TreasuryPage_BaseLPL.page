TreasuryPage is a Page
    title is "DebtManagement"

    DebtInstruments is a Panel
        title is "DebtInstruments"
        business class is DebtInstrument
            list is DebtInstrumentCardList
                title is "DebtInstruments"

    Derivatives is a Panel
        title is "Derivatives"
        business class is DebtInstrument
            list is DerivativesCardList
                title is "Derivatives"

    CreditSupport is a Panel
        title is "CreditSupport"
        business class is DebtInstrument
            list is CreditSupportCardList
                title is "CreditSupport"

    LineOfCredit is a Panel
        title is "LineOfCredit"
        business class is DebtInstrument
            list is LineOfCreditCardList
                title is "LineOfCredit"

    DebtMgmtDashBoard is a DashBoardPanel
        title is "Analysis"
        mouse over text is "DebtManagementDashboard"
        DebtSummary is a Pane
            business class is DebtInstrumentIdentifierAmortizationSchedule
                list is DebtInstrumentIdentifierAmortizationScheduleDebtCube
                    title is "DebtSummary"
                    overview navigation is DebtInstrumentIdentifierAmortizationScheduleCubeLink
                        label is "Detail"
        DebtBalance is a Pane
            business class is DebtInstrumentIdentifierAmortizationSchedule
                list is DebtInstrumentIdentifierAmortizationScheduleDebtPaymentCube
                    title is "DebtRemainingBalance"
                    display DebtBalanceChart
                    Row Dimensions
                        PaymentDatePeriod
                    overview navigation is DebtInstrumentIdentifierAmortizationScheduleDebtPaymentCubeLink
                        label is "Detail"
        DebtServiceSchedule is a Pane
            business class is DebtInstrumentIdentifierAmortizationSchedule
                list is DebtInstrumentIdentifierAmortizationScheduleDebtPaymentCube
                    title is "DebtServiceSchedule"
                    display DebtPaymentChart
                    Row Dimensions
                        PaymentDatePeriod
                    overview navigation is DebtInstrumentIdentifierAmortizationScheduleDebtPaymentCubeLink
                        label is "Detail"
        Types is a Pane
            business class is DebtInstrumentIdentifierAmortizationSchedule
                list is DebtInstrumentIdentifierAmortizationScheduleDebtCube
                    title is "DebtByType"
                    display DebtByTypePieChart
                    Row Dimensions
                        DebtInstrument.DebtType
                        DebtInstrument
                        DebtInstrumentIdentifier
                    overview navigation is DebtInstrumentIdentifierAmortizationScheduleCubeLink
                        label is "Detail"
        Accounts is a Pane
            business class is DebtInstrumentIdentifierAmortizationSchedule
                list is DebtInstrumentIdentifierAmortizationScheduleDebtCube
                    title is "DebtCurrentBalance"
                    display DebtByAccountBarChart
                    Row Dimensions
                        DebtInstrument
                        DebtInstrumentIdentifier
                    overview navigation is DebtInstrumentIdentifierAmortizationScheduleCubeLink
                        label is "Detail"
        Locations is a Pane
            business class is DebtInstrumentIdentifierAmortizationSchedule
                list is DebtInstrumentIdentifierAmortizationScheduleDebtCube
                    title is "DebtByLocation"
                    display DebtByLocationPieChart
                    Row Dimensions
                        DebtInstrument.CashManagementLocation
                        DebtInstrument
                        DebtInstrumentIdentifier
                    overview navigation is DebtInstrumentIdentifierAmortizationScheduleCubeLink
                        label is "Detail"
        ObligatedGroups is a Pane
            business class is DebtInstrumentIdentifierAmortizationSchedule
                list is DebtInstrumentIdentifierAmortizationScheduleDebtCube
                    title is "DebtByObligatedGroup"
                    display DebtByObligatedGroupPieChart
                    Row Dimensions
                        DebtInstrument.DebtReportGroup
                        DebtInstrument
                        DebtInstrumentIdentifier
                    overview navigation is DebtInstrumentIdentifierAmortizationScheduleCubeLink
                        label is "Detail"
        LegalEntities is a Pane
            business class is DebtInstrumentIdentifierAmortizationSchedule
                list is DebtInstrumentIdentifierAmortizationScheduleDebtCube
                    title is "DebtByLegalEntity"
                    display DebtByLegalEntityPieChart
                    Row Dimensions
                        DebtInstrument.LegalEntityGroup.LegalEntity
                        DebtInstrument
                        DebtInstrumentIdentifier
                    overview navigation is DebtInstrumentIdentifierAmortizationScheduleCubeLink
                        label is "Detail"

    Search is a Panel
        title is "Search"
        business class is DebtInstrument
            list is DebtInstrumentSearch
                title is "Search"

