ProjectAccountantContractsPage is a Page
    title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Contracts"
    show panel control on left
    is report for ProjectAdministrator,ProjectAccountant
        category is "InvoicingAndRevenue"

    Contracts is a Panel
        business class is ProjectContract
            list is ProjectManagerAccountantContracts

    DashBoard is a Panel
        title is "Dashboard"
        business class is ProjectContract
            list is ProjectManagerContractsDashBoardView
                title is "Dashboard"

    ProjectFundingSources is a Panel
        business class is ProjectFundingSource
            list is ProjectFundingSources2

    FrontEndSplitContracts is a Panel
        business class is ProjectFundingSource
            list is ProjectFundingSourcesFrontEndSplit

    ForecastReport is a Panel
        title is "Forecast"
        business class is ProjectFundingSource
            list is ProjectFundingSourcesForecastList
                title is "Forecast"

    InvoicedAndRevenueComparison is a Panel
        title is "ComparisonReport"
        business class is ProjectFundingSource
            list is ProjectFundingSources3
                title is "ComparisonReport"

    RevenueAndExpenseComparison is a Panel
        title is "RevenueToExpenseVariance"
        business class is Project
            list is ProjectContractRevenueAndExpenseComparison
                title is "RevenueToExpenseVariance"

    JournalizedInvoicedAndRevenueBalances is a Panel
        title is "JournalizedBalances"
        business class is ProjectFundingSource
            list is JournalizedInvoicedAndRevenueBalances
                title is "JournalizedBalances"

    Invoices is a Panel
        business class is ProjectContractInvoice
            list is ProjectContractInvoiceAccountantList

    Reversals is a Panel
        business class is ProjectContractInvoice
            list is ProjectContractInvoiceAccountantReversalList

    Lines is a Panel
        mouse over text is "InvoiceAndRevenueLines"
        business class is ProjectContractInvoiceLine
            list is ProjectContractInvoiceLinesDetail

    ValidationErrors is a Panel
        title is "ValidationResults"
        business class is ProjectStructureValidationResult
            list is ProjectStructureValidationInvoiceAndRevenueResultList
                title is "ValidationResults"

    Errors is a Panel
        mouse over text is "InvoicingAndRevenueRecognitionErrors"
        business class is ProjectContractInvoiceError
            list is ProjectContractInvoiceErrorList

    PartialBilledTransaction is a Panel
        title is "PartiallyBilled"
        mouse over text is "PartiallyBilledTransactions"
        business class is ProjectContractPartialTransaction
            list is AllProjectContractPartialBilledTransactions
                title is "PartiallyBilled"

    PartialRevenueTransaction is a Panel
        title is "PartiallyRecognized"
        mouse over text is "PartiallyRecognizedTransactions"
        business class is ProjectContractPartialTransaction
            list is AllProjectContractPartialRecognizedTransactions
                title is "PartiallyRecognized"

    Revenue is a Panel
        business class is ProjectContractInvoice
            list is ProjectContractRevenueAccountantList

    Milestones is a Panel
        mouse over text is "MilestonesAndInstallments"
        business class is ProjectContractMilestone
            list is ProjectContractMilestoneAccountantList

    PercentComplete is a Panel
        business class is ProjectContractPercentComplete
            list is ContractPercentCompleteByContractList

    Retainers is a Panel
        business class is ProjectContractRetainer
            list is ProjectContractRetainerAccountantList

    FrontEndSplitResults is a Panel
        business class is ProjectContractFESResult
            list is AllProjectContractFESResultList

    SelectedTransactionsForReallocation is a Panel
        business class is ProjectContractReallocationSelection
            list is AllProjectContractReallocationSelectionList

    ReallocationResults is a Panel
        business class is ProjectContractReallocationResult
            list is AllProjectContractReallocationResultList

    FundingSourceDeliverables is a Panel
        title is "Deliverables"
        business class is ProjectFundingSourceDeliverable
            list is AccountantFundingSourceDeliverableList
                title is "Deliverables"

    ContractModifications is a Panel
        title is "Modifications"
        business class is ProjectContractModification
            list is ProjectContractModificationPageList
                title is "Modifications"

    ProjectContractChangeRequests is a Panel
        title is "ContractChangeRequests"
        business class is ProjectContractChangeRequest
            list is ProjectContractChangeRequestList
                title is "ContractChangeRequests"

    ProjectFundingSourceChangeRequests is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>ChangeRequests"
        business class is ProjectFundingSourceChangeRequest
            list is ProjectFundingSourceChangeRequestList
                title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>ChangeRequests"

    ReceivableTransactionsWithCashApplied is a Panel
        business class is ReceivableInvoiceDetail
            list is ReviewProjectTransactions

    ReceivableTransactions is a Panel
        business class is ReceivableInvoiceDetail
            list is ReviewAllProjectTransactions

    ProjectContractFee is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>ContractFees"
        business class is ProjectContractFee
            list is ProjectContractFeeCreateNewVersionList
                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>ContractFees"

