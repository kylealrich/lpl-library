ContractProcessingPage is a Page
    title is "ManageContracts"
    show panel control on left

    Contracts is a Panel
        business class is Contract
            list is CMManagerDashboard

    SearchList is a Panel
        business class is Contract
            list is ContractSearch

    InProcess is a Panel
        business class is Contract
            list is InProcessContracts

    ContractCosts is a Panel
        business class is ContractLine
            list is ContractCostsForAnItem

    ProposedContracts is a Panel
        business class is Contract
            list is BuyerViewSupplierProposedContracts

    ContractHierarchy is a Panel
        business class is Contract
            list is ContractHierarchy

    OnHold is a MultiPanePanel
        pane 1
            business class is Contract
                list is ContractsOnHold
        pane 2
            business class is ContractLine
                list is LinesOnHold

    InvoiceCostDifferences is a Panel
        business class is PayablesInvoiceDetail
            list is ContractCostDifferences

    SubcontractorsAndEvents is a MultiPanePanel
        pane 1
            business class is ContractSubcontractor
                list is SubcontractorAlert
        pane 2
            business class is Contract
                list is ContractsCreatedFromEvents

    MaxAmounts is a MultiPanePanel
        pane 1
            business class is Contract
                list is ContractUsageNoNotification
        pane 2
            business class is Contract
                list is ContractUsageWithNotificationPercent

    DaysToProcess is a Panel
        business class is Contract
            list is DaysToProcess

