ItemProposalsAndTrialItems is a Page
    title is "ItemProposalsAndTrialItems"
    implements MenuSearch

    ProcessingAndSetup is a DashBoardPanel
        use 12 column grid
        Processing is a Pane
            span 6 columns
            title is "ProposalProcessing"
            Layout
                two column
                    small menu
                        page is ItemProposalsAndTrialItems.NewItemProposalsInProcess
                            title is "ProposalsInProcess"
                            menu item name is NewItemProposalsInProcess
                            icon is icon.clipboard-check-amber
                        page is ItemProposalsAndTrialItems.ReviewsInProcess
                            title is "ReviewsInProcess"
                            menu item name is ReviewsInProcess
                            icon is icon.clipboard-check-amber
                        page is ItemProposalsAndTrialItems.ItemsInProcess
                            title is "ItemsInProcess"
                            menu item name is ItemsInProcess
                            icon is icon.clipboard-check-amber
                        page is ItemProposalsAndTrialItems.OpenTrialRequisitions
                            title is "OpenTrialRequisitions"
                            menu item name is OpenTrialRequisitions
                            icon is icon.clipboard-check-amber
                        page is ItemProposalsAndTrialItems.AllNewItemProposals
                            title is "AllProposals"
                            menu item name is AllNewItemProposals
                            icon is icon.clipboard-check-amber
                        page is ItemProposalsAndTrialItems.AllReviews
                            title is "AllReviews"
                            menu item name is AllReviews
                            icon is icon.clipboard-check-amber
                        page is ItemProposalsAndTrialItems.AllItems
                            title is "AllItems"
                            menu item name is AllItems
                            icon is icon.clipboard-check-amber
                        page is ItemProposalsAndTrialItems.AllTrialRequisitions
                            title is "AllTrialRequisitions"
                            menu item name is AllTrialRequisitions
                            icon is icon.clipboard-check-amber
                        page is ItemProposalsAndTrialItems.MyOpenItemProposals
                            title is "MyOpenProposals"
                            menu item name is MyOpenItemProposals
                            icon is icon.clipboard-check-amber
                        page is ItemProposalsAndTrialItems.MyFinishedItemProposals
                            title is "MyFinishedProposals"
                            menu item name is MyFinishedItemProposals
                            icon is icon.clipboard-check-amber
                        page is ItemProposalsAndTrialItems.QandA
                            title is "QandA"
                            menu item name is QandA
                            icon is icon.clipboard-check-amber
        ProposalSetup is a Pane
            span 3 columns
            title is "Setup"
            Layout
                single column
                    small menu
                        form is RecallGroup.NewItemProposalForm
                            title is "ItemLifecycleManagementGroup"
                            menu item name is NewItemProposalForm
                            icon is icon.document-certified-amethyst
                        list is RecallUser.ItemReviewerList
                            title is "Reviewers"
                            menu item name is ItemProposalReviewers
                            icon is icon.document-certified-amethyst
                        list is SampleDocumentTemplate.FactSheetAndTemplateForItemProposals
                            title is "FactSheetAndItemTemplate"
                            menu item name is ItemProposalFactSheet
                            icon is icon.document-amethyst

    NewItemProposalsInProcess is a Panel
        title is "ItemProposalsInProcess"
        business class is RecallNotice
            list is ItemProposalsInProcess
                title is "ItemProposalsInProcess"
            Context
                RecallGroup

    ReviewsInProcess is a Panel
        business class is RecallNoticeTask
            list is ViewOpenItemReview

    ItemsInProcess is a Panel
        business class is RecallProduct
            list is OpenProductsForNewItemProposal

    OpenTrialRequisitions is a Panel
        business class is Requisition
            list is OpenTrialRequisitions

    AllNewItemProposals is a Panel
        business class is RecallNotice
            list is ItemProposals
            Context
                RecallGroup

    AllReviews is a Panel
        business class is RecallNoticeTask
            list is ViewItemReview

    AllItems is a Panel
        business class is RecallProduct
            list is ProductsForNewItemProposal

    AllTrialRequisitions is a Panel
        business class is Requisition
            list is TrialRequisitions

    MyOpenItemProposals is a Panel
        business class is RecallNoticeTask
            list is MyOpenItemProposals

    MyFinishedItemProposals is a Panel
        business class is RecallNoticeTask
            list is MyFinishedItemProposals

    QandA is a Panel
        Search Terms
                "ProposalQAndA"
        business class is RecallNoticeQandA
            list is ItemProposalQAndAList

