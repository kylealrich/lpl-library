RequisitionsAdministratorUX1 is a Page
    title is "RequisitionsAdministration"
    implements MenuSearch

    RequisitionsAdminHome is a DashBoardPanel
        use 12 column grid
        Processing is a Pane
            span 6 columns
            icon is icon.outline-process-azure
            Layout
                two column distributed
                    small menu
                        action is RequisitionInterface.RequisitionInterface
                            title is "InterfaceRequisitions"
                            menu item name is InterfaceRequisitions
                        page is RequisitionsPage
                            title is "ManageRequisitions"
                            menu item name is ManageRequisitions
                        action is RequisitionLineDistribution.MigrateRQCommitments
                            title is "MigrateRequisitionCommitments"
                            menu item name is MigrateRequisitionCommitment
                        action is PrintRequisitions.PrintRequisitions
                            title is "PrintRequisitions"
                            menu item name is PrintRequisitions
                        action is RequestLocationLoad.RequestLocationLoad
                            title is "RequestLocationLoad"
                            menu item name is RequestLocationLoad
                        action is RequesterSetupExtract.RequesterSetupExtract
                            title is "RequesterSetupExtract"
                            menu item name is RequesterSetupExtract
                        page is RequisitionInterfaceMaintenance
                            title is "RequisitionInterfaceMaintenance"
                            menu item name is RequisitionInterfaceMaintenance
                        list is RequisitionInterfaceResult.RequisitionInterfaceResults
                            title is "RequisitionInterfaceResults"
                            menu item name is RequisitionInterfaceResults
                        action is Requisition.PurgeRequisition
                            title is "RequisitionPurge"
                            menu item name is RequisitionPurge
                        action is InventoryCompany.PurgeRequisitionsForAllCompanies
                            title is "RequisitionPurgeForAllCompanies"
                            menu item name is RequisitionPurgeForAllCompanies
                        action is RequisitionStatusReport.RequisitionStatusReport
                            title is "RequisitionStatusReport"
                            menu item name is RequisitionStatusReport
                        page is SupplierRequisitionsPage
                            title is "SupplierRequisitions"
                            menu item name is SupplierRequisitions
        Utilities is a Pane
            span 6 columns
            icon is icon.outline-maintenance-azure
            Layout
                two column distributed
                    column1
                        action is ContractLine.RemoveKeywordShadowServiceAndStanding
                            title is "CleanRequisitionSelfServiceSearchForStandingContracts"
                            menu item name is CleanRequisitionSelfServiceSearchForStandingContracts
                        action is ProcurementGroup.CreateTemplateAndTemplateLineSearchKeywords
                            title is "CreateTemplateAndTemplateLineSearchKeywords"
                            menu item name is CreateTemplateAndTemplateLineSearchKeywords
                        action is ItemGroup.RecreateKeywords
                            title is "RecreateKeywords"
                            menu item name is RecreateKeywords
                        action is ProcurementTemplate.RecreateTemplateSearchKeywords
                            title is "RecreateTemplateSearchKeywords"
                            menu item name is RecreateTemplateSearchKeywords
                        action is RequisitionLine.RecalculateRequesterItemCount
                            title is "RecalculateRequesterItemCount"
                            menu item name is RecalculateRequesterItemCount
                        action is Requisition.UpdateRequisitionCubes
                            title is "RefreshRequisitionCubes"
                            menu item name is RefreshRequisitionCubes
                        list is Requester.RequestersForFix
                            title is "RequestersForFix"
                            menu item name is RequestersForFix
                    column2
                        action is InventoryCompany.SetAllowAlternateShipToAddressOptionForAllRequesters
                            title is "SetAllowAlternateShipToAddressOptionForAllRequesters"
                            menu item name is SetAllowAlternateShipToAddressOptionForAllRequesters
                        list is IDMTemplate.RequisitionIDMTemplate
                            title is "IDMTemplates"
                            menu item name is RequisitionsIDMTemplates
                        list is Requester.RequesterDashboard
                            title is "Requesters"
                            menu item name is Requesters
                        page is RequestingLocationPage
                            title is "RequestingLocations"
                            menu item name is RequestingLocations
                        list is RequestingReportGroup.RequestingReportGroupList
                            title is "RequestingReportGroups"
                            menu item name is ReportGroups

