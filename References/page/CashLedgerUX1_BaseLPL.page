CashLedgerUX1 is a Page
    title is "CashLedger"
    implements MenuSearch
    show panel control on left

    Processing is a DashBoardPanel
        use 12 column grid
        Process is a Pane
            span 12 columns
            Layout
                three column distributed
                    small menu
                        page is CashLedgerTransactionPage
                            title is "Transactions"
                            menu item name is CashLedgerTransactions
                            icon is icon.cycle-amber
                        page is CashLedgerTransactionApprovalPage
                            title is "TransactionApprovals"
                            menu item name is TransactionApprovals
                            icon is icon.cycle-amber
                        list is CashLedgerTransactionDetail.CashLedgerTransactionDetailPrimary
                            title is "TransactionDetails"
                            menu item name is CashLedgerTransactionDetails
                            icon is icon.cycle-amber
                        page is CashReceiptsPage
                            title is "CashReceipts"
                            menu item name is CashReceipts
                            icon is icon.cycle-amber
                        list is CashLedgerPayment.CashLedgerPaymentsList
                            title is "Payments"
                            menu item name is CashLedgerPayments
                            icon is icon.cycle-amber
                        page is CashLedgerBatchApprovalPage
                            title is "BatchApproval"
                            menu item name is BatchApproval
                            icon is icon.cycle-amber
                        page is WireTransferHome
                            title is "WireTransfers"
                            menu item name is WireTransfers
                            icon is icon.cycle-amber
                        list is CashLedgerFundTransferHeader.primary
                            title is "BankFundTransfers"
                            menu item name is BankFundTransfers
                            icon is icon.cycle-amber
                        list is CashLedgerElectronicFundsTransferTransaction.CashLedgerElectronicFundsTransferTransactionList
                            title is "EFTExtracts"
                            menu item name is EFTExtracts
                            icon is icon.cycle-amber
                        page is CashLedgerReconciliationAdjustmentPage
                            title is "ReconciliationVarianceAdjustments"
                            menu item name is ReconciliationVarianceAdjustments
                            icon is icon.cycle-amber
                        list is CashLedgerReturnedPayment.primary
                            title is "ReturnedPayments"
                            menu item name is CashLedgerReturnedPayments
                            icon is icon.cycle-amber
                        action is CashLedgerTransaction.MassUpdate
                            title is "MassUpdateTransactions"
                            menu item name is MassUpdateTransactions
                            icon is icon.cycle-amber
                        page is CashLedgerTransactionWriteOffPage
                            title is "TransactionWriteOff"
                            menu item name is TransactionWriteOff
                            icon is icon.cycle-amber
                        list is CashLedgerGLDistribution.primary
                            title is "Distributions"
                            menu item name is CashLedgerGLDistributions
                            icon is icon.cycle-amber
                        page is CBOpenCommitmentsAndEncumbrances
                            title is "ViewOpenEncumbrances"
                            menu item name is ViewOpenEncumbrances
                            icon is icon.cycle-amber
                        list is CashLedgerBankUpdate.primary
                            title is "BankUpdateMaintenance"
                            menu item name is BankUpdateMaintenance
                            icon is icon.cycle-amber
        MassRelease is a Pane
            span 8 columns
            Layout
                two column distributed
                    small menu
                        action is CashLedgerTransaction.MassRelease
                            title is "Transactions"
                            menu item name is ReleaseTransactions
                            icon is icon.clipboard-check-ruby
                        action is CashLedgerPayment.MassRelease
                            title is "Payments"
                            menu item name is ReleasePayments
                            icon is icon.clipboard-check-ruby
                        action is CashLedgerCashReceiptHeader.MassRelease
                            title is "CashReceipts"
                            menu item name is ReleaseCashReceipts
                            icon is icon.clipboard-check-ruby
                        action is CashLedgerReturnedPayment.MassRelease
                            title is "ReturnedPayments"
                            menu item name is ReleaseReturnedPayments
                            icon is icon.clipboard-check-ruby
                        action is CashLedgerElectronicFundsTransferTransaction.MassRelease
                            title is "EFTExtracts"
                            menu item name is ReleaseEFTExtracts
                            icon is icon.clipboard-check-ruby
                        action is CashLedgerFundTransferHeader.MassRelease
                            title is "BankFundTransfers"
                            menu item name is ReleaseBankFundTransfers
                            icon is icon.clipboard-check-ruby
                        page is CashLedgerMassReleaseResultsPage
                            title is "Results"
                            menu item name is MassReleaseResults
                            icon is icon.clipboard-check-ruby
                        page is CashLedgerMassReleaseErrorPage
                            title is "Errors"
                            menu item name is MassReleaseErrors
                            icon is icon.clipboard-check-ruby
        Search is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        list is CashLedgerTransaction.BankHistorySearch
                            title is "BankHistory"
                            menu item name is BankHistorySearch
                            icon is icon.search-azure
                        list is CashLedgerPayment.OpenPaymentSearchList
                            title is "OpenPaymentsList"
                            menu item name is OpenPaymentSearch
                            icon is icon.search-azure
                        list is CashLedgerJobResult.OpenCashPaymentList
                            title is "OpenPaymentReport"
                            menu item name is OpenPaymentReport
                            icon is icon.search-azure
                        list is CashLedgerPayment.CashLedgerPaymentsSearchList
                            title is "Payments"
                            menu item name is CashLedgerPaymentSearch
                            icon is icon.search-azure
                        visible when (actor.context.FinanceEnterpriseGroup.FundAccounting)
                            list is CashCodeFundEdit.CurrentCommittedCashCodeFundList
                                title is "CurrentCommittedCashCodeFunds"
                                menu item name is CurrentCommittedCashCodeFunds
                                icon is icon.search-azure
                        list is CashLedgerElectronicFundsTransferTransaction.OpenEFTTransactionsSearchList
                            title is "OpenEFTTransactions"
                            menu item name is OpenEFTTransactionSearch
                            icon is icon.search-azure
                        page is CashLedgerTransactionSearchPage
                            title is "TransactionByIdentifiers"
                            menu item name is CashTransactionSearch
                            icon is icon.search-azure
        Balances is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        list is CashCodeFund.CashFundBalancesList
                            title is "CashFund"
                            menu item name is CashFundBalances
                            icon is icon.document-amethyst
                        list is CashCode.CurrentCashCodeBalanceInquiryList
                            title is "CurrentCashCode"
                            menu item name is CurrentCashCodeBalances
                            icon is icon.document-amethyst
                        list is BankAccountBalance.BankAccountPeriodBalancesInquiry
                            title is "PeriodCashCode"
                            menu item name is PeriodCashCodeBalances
                            icon is icon.document-amethyst
                        visible when (actor.context.FinanceEnterpriseGroup.FundAccounting)
                            list is CashCodeFundPeriodBalance.CashCodeFundPeriodBalanceList
                                title is "CashCodeFundPeriodBalance"
                                menu item name is CashCodeFundPeriodBalance
                                icon is icon.document-amethyst
                        list is CashCodeCashPosition.FullCashCodePositionList
                            title is "CashCodeCashPosition"
                            menu item name is CashCodeCashPosition
                            icon is icon.document-amethyst
                        list is CashLedgerConsolidation.primary
                            title is "ConsolidatedTransaction"
                            menu item name is ConsolidatedTransactionBalanceInquiry
                            icon is icon.document-amethyst

    UpdatesAndOutputs is a DashBoardPanel
        use 12 column grid
        Processes is a Pane
            span 12 columns
            title is "Processes"
            Layout
                three column distributed
                    small menu
                        action is CashLedgerTransaction.VoidStopPayUpdate
                            title is "VoidStopPayUpdate"
                            menu item name is VoidStopPayUpdate
                            icon is icon.outline-process-ruby
                        list is CashLedgerTransaction.CashLedgerTransactionsAvailableForVoidStopPayUpdateList
                            title is "VoidStopPayUpdatePreview"
                            menu item name is VoidStopPayUpdatePreview
                            icon is icon.outline-report-ruby
                        list is CashLedgerTransaction.CashLedgerTransactionsProcessedByVoidStopPayUpdateList
                            title is "TransactionsProcessedByVoidStopPayUpdate"
                            menu item name is TransactionsProcessedByVoidStopPayUpdate
                            icon is icon.outline-report-ruby
                        action is EFTExtractUpdate.EFTExtractUpdate
                            title is "EFTExtractFileCreation"
                            menu item name is EFTExtractFileCreation
                            icon is icon.outline-process-ruby
                        action is CashLedgerElectronicFundsTransferTransaction.EFTRejectUpdate
                            title is "EFTRejectUpdate"
                            menu item name is EFTRejectUpdate
                            icon is icon.outline-process-ruby
                        list is CashLedgerElectronicFundsTransferTransaction.EFTPaymentsAvailableForEFTRejectUpdateList
                            title is "EFTRejectUpdatePreview"
                            menu item name is EFTRejectUpdatePreview
                            icon is icon.outline-report-ruby
                        list is CashLedgerElectronicFundsTransferTransaction.EFTPaymentsProcessedByEFTRejectUpdateList
                            title is "EFTPaymentsProcessedByEFTRejectUpdate"
                            menu item name is EFTPaymentsProcessedByEFTRejectUpdate
                            icon is icon.outline-report-ruby
                        action is CashPositionReconciliation.CashPositionReconciliation
                            title is "CashPositionReconciliation"
                            menu item name is CashPositionReconciliation
                            icon is icon.outline-process-ruby
                        action is PositivePayFileCreation.PositivePayFileCreation
                            title is "PositivePayCreation"
                            menu item name is PositivePayCreation
                            icon is icon.outline-process-ruby
                        action is CashLedgerBankUpdate.UpdateLedgerTransactions
                            title is "BankUpdate"
                            menu item name is CashLedgerBankUpdate
                            icon is icon.outline-process-ruby
        OutputFiles is a Pane
            span 8 columns
            Layout
                two column distributed
                    small menu
                        list is PaymentOutputFileHeader.PaymentOutputFileHeaderList
                            title is "Payment"
                            menu item name is PaymentOutputFiles
                            icon is icon.window-text-emerald
                        list is EFTExtractOutputFileHeader.EFTExtractOutputFileHeaderList
                            title is "EFTExtract"
                            menu item name is EFTExtractOutputFiles
                            icon is icon.window-text-emerald
                        page is WireTransferOutputHome
                            title is "WireTransfer"
                            menu item name is WireTransferOutputFiles
                            icon is icon.window-text-emerald
                        list is StaleDateOutputFileHeader.StaleDateOutputFileHeaderList
                            title is "StaleDate"
                            menu item name is StaleDateOutputFiles
                            icon is icon.window-text-emerald
                        list is EscheatmentOutputFileHeader.EscheatmentOutputFileHeaderList
                            title is "Escheatment"
                            menu item name is EscheatmentOutputFiles
                            icon is icon.window-text-emerald
                        list is EscheatmentNoticeIDMHeader.EscheatmentNoticeIDMResultList
                            title is "IDMEscheatmentNoticeResults"
                            menu item name is IDMEscheatmentNoticeResults
                            icon is icon.window-text-emerald
                        list is PositivePayOutputFileHeader.primary
                            title is "PositivePay"
                            menu item name is PositivePayCreationOutput
                            icon is icon.window-text-emerald

    ViewProcessingMonitor is a Panel
        business class is CashLedgerProcessingMonitor
            list is ProcessingMonitorMenuList

