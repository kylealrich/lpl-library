ReceivableInvoicesPage is a Page
    title is "ManageInvoices"

    InvoiceDetailsReplicated is a Panel
        title is "InvoiceDetails_(Replicated)"
        business class is ReceivableInvoiceDetail
            list is ColumnarList
                title is "InvoiceDetails_(Replicated)"
            visible when (actor.context.FinanceEnterpriseGroup.ShowReceivablesColumnarSearch)
    InvoicesReplicated is a Panel
        title is "Invoices_(Replicated)"
        business class is ReceivableInvoice
            list is ColumnarList
                title is "Invoices_(Replicated)"
            visible when (actor.context.FinanceEnterpriseGroup.ShowReceivablesColumnarSearch)
    InvoiceBatches is a Panel
        business class is ReceivableInvoiceBatch
            list is ReceivableInvoiceBatchList

    UnreleasedInvoices is a Panel
        business class is ReceivableInvoice
            list is UnreleasedTransactions

    Invoices is a Panel
        business class is ReceivableInvoice
            list is ReceivableInvoiceList

    Disputes is a Panel
        business class is ReceivableInvoiceDispute
            list is ReceivableInvoiceDisputeList

    InvoiceTransfer is a Panel
        title is "InvoiceTransfers"
        business class is ReceivableInvoice
            list is TransferList
                title is "InvoiceTransfers"

    TransferredInvoices is a Panel
        business class is ReceivableInvoiceTransfer
            list is ReceivableInvoiceTransferredList

    TransactionApplication is a MultiPanePanel
        title is "TransactionApplications"
        pane 1
            business class is ReceivableApplication
                list is ReceivableApplicationSearchList
        pane 2
            business class is pane 1.ReceivablePaymentRelOnetoMany
                list is ApplicationPaymentList
        pane 4
            business class is pane 1.ReceivableApplicationAdjustment set
                list is AdjustmentsForPaymentApplication

    InvoiceDetailExceptions is a Panel
        business class is ReceivableInvoiceDetail
            list is ReceivableInvoiceDetailExceptionList

    InvoiceAuditInquiry is a Panel
        business class is ReceivableInvoice
            list is ReceivableInvoiceAuditList

    InvoiceDetailAuditInquiry is a Panel
        business class is ReceivableInvoiceDetail
            list is ReceivableInvoiceDetailAuditList

