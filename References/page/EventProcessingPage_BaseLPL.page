EventProcessingPage is a Page
    title is "ManageEvents"
    show panel control on left

    CreateEventsFromRequisitions is a MultiPanePanel
        pane 1
            business class is PurchaseOrderInterface
                list is OpenRequisitions
        pane 2
            business class is SourcingEventWorksheet
                list is CreateEventFromRequisitions
                Context
                    Actor
        pane 4
            business class is SourcingEvent
                list is RequisitionDraftEvents
                Context
                    Company

    Draft is a Panel
        business class is SourcingEvent
            list is DraftEvents
            Context
                Company

    PendingOpen is a Panel
        business class is SourcingEvent
            list is PendingOpenEvents
            Context
                Company

    Open is a Panel
        business class is SourcingEvent
            list is OpenEvents
            Context
                Company

    PendingEvaluation is a Panel
        business class is SourcingEvent
            list is EventsPendingAward
            Context
                Company

    RecentlyFinalized is a Panel
        business class is SourcingEvent
            list is RecentlyFinalized
            Context
                Company

    BidTabulations is a Panel
        business class is SourcingEvent
            list is EventsForBidTabulationBuyer
            Context
                Company

    StatusAnalysis is a MultiPanePanel
        pane 1
            business class is SourcingCompany
                list is ChartCompany
                Context
                    Company
        pane 3
            business class is pane 1.SourcingStatus set
                list is SourcingStatus
        pane 4
            business class is pane 3.SourcingEventRel
                list is ChartEvents

    Search is a Panel
        business class is SourcingEvent
            list is EventSearchList

    Alerts is a MultiPanePanel
        pane 1
            business class is SourcingEvent
                list is RedAndYellowAlertEvents
                Context
                    Company
        pane 2
            business class is SourcingEvent
                list is UnsubmittedResponses
                Context
                    Company
        pane 3
            business class is SourcingEvent
                list is EventAwardsOverdue
                Context
                    Company
        pane 4
            business class is SourcingEventQandA
                list is QandAAlert
                Context
                    Company

    MoreAlerts is a Panel
        business class is SourcingEventResponse
            list is OpenResponseModificationRequests

    EventBuyerWeighting is a Panel
        business class is SourcingEventResponseBuyerWeighting
            list is ResponseBuyerWeightingForABuyer

