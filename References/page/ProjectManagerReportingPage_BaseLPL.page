ProjectManagerReportingPage is a Page
    title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Reports"
    show panel control on left

    ProjectObligations is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Obligations"
        business class is ProjectReportingSettings
            list is ProjectManagerReportingSettingsProjectObligationsList
                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Obligations"

    ProjectVariance is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Variance"
        business class is ProjectReportingSettings
            list is ProjectManagerReportingSettingsProjectVarianceList
                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Variance"

    ProjectPeriodComparison is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>PeriodComparison"
        business class is ProjectReportingSettings
            list is ProjectManagerReportingSettingsProjectPeriodComparisonList
                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>PeriodComparison"

    ContractPercentComplete is a Panel
        business class is ProjectReportingSettings
            list is ProjectManagerReportingSettingsContractPercentCompleteFlatList

    ContractGrossMargin is a Panel
        business class is ProjectReportingSettings
            list is ProjectManagerReportingSettingsContractGrossMarginFlatList

    Requisitions is a MultiPanePanel
        vertical split is 35/65
        pane 2
            business class is FinanceEnterpriseGroup.first EnterpriseProjectStructureRel.MySummaryProjectsRel
                list is MySummaryProjectList
                Context
                    FinanceEnterpriseGroup
        pane 3
            business class is pane 2.ProjectChildrenRel
                list is MyProjectList
        pane 4
            business class is pane 3.RequisitionLineDistributionsAllRel
                list is ProjectRequisitionLineDistributions

    PurchaseOrders is a MultiPanePanel
        vertical split is 35/65
        pane 2
            business class is FinanceEnterpriseGroup.first EnterpriseProjectStructureRel.MySummaryProjectsRel
                list is MySummaryProjectList
                Context
                    FinanceEnterpriseGroup
        pane 3
            business class is pane 2.ProjectChildrenRel
                list is MyProjectList
        pane 4
            business class is pane 3.PurchaseOrderLineDistributionsAllRel
                list is PurchaseOrderLinesForAProject

    ContractLineDistributions is a MultiPanePanel
        title is "ContractLine"
        pane 2
            business class is FinanceEnterpriseGroup.first EnterpriseProjectStructureRel.MySummaryProjectsRel
                list is MySummaryProjectList
                Context
                    FinanceEnterpriseGroup
        pane 3
            business class is pane 2.ProjectChildrenRel
                list is MyProjectList
        pane 4
            business class is pane 3.ContractLineDistributionAllRel
                list is ProjectContractLineDistributions

    PayablesInvoices is a MultiPanePanel
        vertical split is 35/65
        pane 2
            business class is FinanceEnterpriseGroup.first EnterpriseProjectStructureRel.MySummaryProjectsRel
                list is MySummaryProjectList
                Context
                    FinanceEnterpriseGroup
        pane 3
            business class is pane 2.ProjectChildrenRel
                list is MyProjectList
        pane 4
            business class is pane 3.PayablesInvoiceDistributionsAllRel
                list is ProjectPayablesInvoiceDistributionList

    CashLedgerDistributions is a MultiPanePanel
        title is "CashLedger"
        pane 2
            business class is FinanceEnterpriseGroup.first EnterpriseProjectStructureRel.MySummaryProjectsRel
                list is MySummaryProjectList
                Context
                    FinanceEnterpriseGroup
        pane 3
            business class is pane 2.ProjectChildrenRel
                list is MyProjectList
        pane 4
            business class is pane 3.CashLedgerGLDistributionsRel
                list is ProjectCashLedgerGLDistributionList

    CommitmentsAndEncumbrances is a Panel
        business class is GLCommit
            list is ProjectCommitments

    CommitmentsAndEncumbrancesSearch is a Panel
        business class is GLCommit
            list is ProjectCommitmentsSearch

    ScheduleOfFederalAwards is a Panel
        business class is ProjectReportingSettings
            list is ProjectManagerReportingSettingsScheduleOfFederalAwardFlatList

    UnassignedProjectReportingSettings is a Panel
        business class is ProjectReportingSettings
            list is ProjectReportingSettingsNoActorList

