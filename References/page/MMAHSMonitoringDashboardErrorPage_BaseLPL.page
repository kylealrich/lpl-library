MMAHSMonitoringDashboardErrorPage is a Page
    title is "MonitoringOverview"
    implements MenuSearch

    Overview is a DashBoardPanel
        use 12 column grid
        CriticalJobs is a Pane
            span 3 columns
            business class is MMAHSMonitoringControl
                list is MMAHSMonitoringControlCriticalList
        HighPriorityJobs is a Pane
            span 3 columns
            business class is MMAHSMonitoringControl
                list is MMAHSMonitoringControlHighList
        MediumPriorityJobs is a Pane
            span 3 columns
            business class is MMAHSMonitoringControl
                list is MMAHSMonitoringControlMediumList
        LowPriorityJobs is a Pane
            span 3 columns
            business class is MMAHSMonitoringControl
                list is MMAHSMonitoringControlLowList
        IPAFlowStatus is a Pane
            span 3 columns
            business class is PfiWorkunitCountDisplay
                list is MMAHSPfiWorkunitDisplayCountList
        AsyncStatus is a Pane
            span 3 columns
            business class is AsyncStatus
                list is MMAHSAsyncStatus
        ReplicationIssueStatus is a Pane
            span 3 columns
            business class is ReplicationSet
                list is ReplicationRedAlertList
        ChannelIssueStatus is a Pane
            span 3 columns
            business class is PfiChannel
                list is MMAHSFileChannelIssueList

    MaintainMonitors is a Panel
        business class is MMAHSMonitoringControl
            list is MMAHSMonitoringControlDashboard

    MonitorSchedules is a Panel
        business class is AsyncActionRequest
            list is MMAHSAsyncActionRequestPendingMonitors

    AsyncIssues is a Panel
        business class is AsyncActionTrigger
            list is FailedAsyncActionTriggerList

    Configuration is a Panel
        business class is MMAHSMonitoringConfiguration
            list is MMAHSMonitoringConfigurationList

