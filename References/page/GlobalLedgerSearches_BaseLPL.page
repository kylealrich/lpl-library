GlobalLedgerSearches is a Page
    title is "Analysis"
    implements MenuSearch

    GLAnalytics is a DashBoardPanel
        title is "Main"
        use 12 column grid
        GLAnalyticsPane is a Pane
            span 6 columns
            title is "Transactions_andBalances"
            Layout
                two column distributed
                    visible when (actor.context.FinanceEnterpriseGroup.ShowGlobalLedgerColumnarSearch)
                        page is TransactionAnalysis
                            title is "TransactionAnalysis"
                            menu item name is TransactionAnalysis
                            icon is icon.outline-list-amber
                    page is TotalsPage
                        title is "Totals"
                        menu item name is Totals
                        icon is icon.outline-list-amber
                    page is TransactionPage
                        title is "FinanceTransactions"
                        menu item name is FinanceTransactions
                        icon is icon.outline-list-amber
                    page is GLTransactionDetailPage
                        title is "FinanceTransactionDetails"
                        menu item name is FinanceTransactionDetails
                        icon is icon.outline-list-amber
                    visible when (actor.context.FinanceEnterpriseGroup.GLCommitDocumentExists)
                        page is GLCommitmentReport
                            title is "CommitmentsAndEncumbrances"
                            menu item name is CommitmentsAndEncumbrances
                            icon is icon.outline-list-amber
                    visible when (actor.context.FinanceEnterpriseGroup.GLCommitDocumentExists)
                        list is EncumbranceRolloverResult.primary
                            title is "RolledOverCommitmentsAndEncumbrancesResult"
                            menu item name is RolledOverCommitmentsAndEncumbrancesResult
                            icon is icon.outline-list-amber
                    visible when (actor.context.FinanceEnterpriseGroup.GLCommitDocumentExists)
                        list is CommitmentEncumbranceVarianceResult.primary
                            title is "CommitmentsAndEncumbrancesVarianceReport"
                            menu item name is CommitmentsAndEncumbrancesVarianceReport
                            icon is icon.outline-list-amber
                    visible when (actor.context.FinanceEnterpriseGroup.GLCommitDocumentExists)
                        list is CommitmentEncumbrancePOVarianceResult.primary
                            title is "PO_EncumbranceVarianceReport"
                            menu item name is CommitmentsAndEncumbrancesPOVarianceReport
                            icon is icon.outline-list-amber
                    page is DimensionOnePivotAnalysisPage
                        title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension1Label>_PivotAnalysis"
                        menu item name is FinanceDimension1PivotAnalysis
                        icon is icon.outline-list-amber
                    page is AccountAnalysisSettingsPage
                        title is "AccountAnalysis"
                        menu item name is AccountAnalysis
                        icon is icon.outline-list-amber
        GLReportingBasis is a Pane
            span 3 columns
            title is "ReportingBasis"
            Layout
                single column
                    page is ReportingBasisGainLoss
                        title is "BasisGainLoss"
                        menu item name is BasisGainLoss
                        icon is icon.outline-report-emerald
                    page is ReportingBasisCloseReport
                        title is "BasisCloseReport"
                        menu item name is BasisCloseReport
                        icon is icon.outline-report-emerald
                    page is ReportingBasisByPage
                        title is "ByLedger_and_<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
                        menu item name is ByLedgerAndAccountingEntity
                        icon is icon.outline-report-emerald
                    visible when (actor.context.FinanceEnterpriseGroup.ReportingBasisAssignJournal)
                        page is ReportingBasisAssignJournalReference
                            title is "AssignJournalReference"
                            menu item name is AssignJournalReference
                            icon is icon.outline-report-emerald
                    visible when (actor.context.FinanceEnterpriseGroup.JournalReferenceExists)
                        page is ReportingBasisWithJournalReference
                            title is "JournalReferenceReport"
                            menu item name is JournalReferenceReport
                            icon is icon.outline-report-emerald
                    visible when (actor.context.FinanceEnterpriseGroup.ADBSelectionExists)
                        page is AggregateAndDaily
                            title is "AverageDailyBalanceReport"
                            menu item name is AverageDailyBalanceReport
                            icon is icon.outline-report-emerald
        RDPane is a Pane
            span 3 columns
            title is "ReportDesigner"
            visible when (actor.context.FinanceEnterpriseGroup.ShowGLReportDesigner)
            Layout
                single column
                    page is GLReportWriter
                        title is "Dashboard"
                        menu item name is Dashboard
                        icon is icon.outline-report-azure
                    visible when (actor.context.FinanceEnterpriseGroup.ShowGLReportDesignerLog)
                        page is GLReportDesignerLog
                            title is "RunResults"
                            menu item name is RunResults
                            icon is icon.outline-report-azure
                    action is GLReport.RefreshReportDesignerData
                        title is "RefreshData"
                        menu item name is RefreshData
                        icon is icon.outline-process-azure
                two column distributed
                    paragraph
                        text of "LastRefreshDate"
                    paragraph
                        visible when (actor.context.GLReport.DerivedReplicationSetInProcess)
                            text of "Replication_is_Running"
                                bold
                                color of orange
                        visible when (not actor.context.GLReport.DerivedReplicationSetInProcess)
                            text of "<actor.context.GLReport.DerivedReplicationRefreshDate>"
                                bold
        StructureComparePane is a Pane
            span 6 columns
            title is "StructureCompare"
            Layout
                two column distributed
                    list is ReportingChart.XiReportingChartList
                        title is "Chart_of_Account"
                        menu item name is ChartOfAccount
                        icon is icon.compare-amethyst
                    visible when (actor.context.FinanceEnterpriseGroup.AccountingUnitSelected)
                        list is AccountingUnitStructure.XiAccountingUnitStructureList
                            title is "<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>"
                            menu item name is AccountingUnit
                            icon is icon.compare-amethyst
                    page is StructureComparePage
                        title is "FinancialDimensions"
                        menu item name is APagewithAllDimensions
                        icon is icon.compare-amethyst
                    visible when (actor.context.FinanceEnterpriseGroup.ProjectEntry)
                        list is ProjectStructure.XiProjectStructureAdmiList
                            title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
                            menu item name is Project
                            icon is icon.compare-amethyst
        CubeRefresh is a Pane
            span 6 columns
            title is "Cube"
            Layout
                single column
                    text of "Status"
                        header4
                    visible when (actor.context.AccountAnalysisSettings.CubeOnlineMode)
                        text of "Cube_is_online"
                            bold
                            color of green
                    visible when (actor.context.AccountAnalysisSettings.CubeOfflineMode)
                        text of "Cube_is_offline"
                            bold
                            color of red
                    text of "Finance_Enterprise_Group_Cube"
                        header4
                    paragraph
                        text of "LastRefreshDate"
                        text of "<actor.context.FinanceEnterpriseGroup.FinanceEnterpriseGroupAnalyticCubeRel.LastRefreshDate>"
                            bold
                    text of "Global_Ledger_Total_Cube"
                        header4
                    paragraph
                        text of "LastRefreshDate"
                        text of "<actor.context.FinanceEnterpriseGroup.GeneralLedgerTotalAnalyticCubeRel.LastRefreshDate>"
                            bold
                    action is FinanceEnterpriseGroup.RefreshFinancialCubes
                        title is "RefreshCube"
                        menu item name is RefreshCube
                        description is "Will_refresh_the_cube"
                        icon is icon.outline-process-ruby

    CompareAnalysis is a Panel
        business class is GeneralLedgerTotal
            list is GeneralLedgerDimensionAnalysisCompareDimension1

    PeriodOverPeriodAnalysis is a Panel
        business class is GeneralLedgerTotal
            list is GeneralLedgerDimensionAnalysisPeriodOverPeriodDimension1

    VarianceAnalysis is a Panel
        business class is GeneralLedgerTotal
            list is GeneralLedgerDimensionAnalysisVarianceDimension1

    TransactionAmountAnalysis is a Panel
        business class is GeneralLedgerTotal
            list is TransactionAmountPivotAnalysis

