RequesterSelfServiceHomePage is a Page
    title is "<actor.agent(Employee).Employee.Requester.RSSLPLHomePageTitle>"

    Dashboard is a DashBoardPanel
        use 12 column grid
        StartRequisitions is a Pane
            span 3 columns
            valid when (actor.agent(Employee).Employee.Requester.CanCreateCapitatedRequisition)
            Layout
                single column
                    small menu
                        page is RequesterShoppingPage
                            title is "ContinueShopping"
                            menu item name is ContinueShopping
                            icon is icon.CSFBuyer
                        action is Requisition.Unreleased.RSSLPLCreate
                            title is "CreateNewRequisition"
                            menu item name is CreateNewRequisition
                            icon is icon.target-amethyst
                        action is Requisition.Unreleased.RSSLPLCreateUsingCopyFromRequisition
                            title is "CopyRequisition"
                            menu item name is CopyRequisition
                            icon is icon.documents-emerald
                        visible when (not actor.agent(Employee).Employee.Requester.HideShoppingList)
                            page is RequesterShoppingPage.AddFromShoppingLists
                                title is "Add_fromShoppingLists"
                                menu item name is AddFromShoppingLists
                                icon is icon.clipboard-check-amber
                        visible when (not actor.agent(Employee).Employee.Requester.HideCatalogSearch)
                            page is RequesterShoppingPage.SearchTheCatalog
                                title is "SearchTheCatalog"
                                menu item name is SearchTheCatalog
                                icon is icon.CSFSupplyManagementViewer
                        visible when (not actor.agent(Employee).Employee.Requester.HideShopPunchoutVendors)
                            page is RequesterShoppingPage.ShopPunchoutVendors
                                title is "ShopPunchoutVendors"
                                menu item name is ShopPunchoutVendors
                                icon is icon.directory-azure
                        form is Requisition.CreateForCapitatedContract
                            title is "CreateCapitatedRequisition"
                            menu item name is CreateCapitatedRequisition
                            icon is icon.directory-amethyst
        StartRequisitions2 is a Pane
            span 3 columns
            title is "StartRequisitions"
            valid when (actor.agent(Employee).Employee.Requester.CannotCreateCapitatedRequisition)
            Layout
                single column
                    small menu
                        page is RequesterShoppingPage
                            title is "ContinueShopping"
                            menu item name is ContinueShopping
                            icon is icon.CSFBuyer
                        action is Requisition.Unreleased.RSSLPLCreate
                            title is "CreateNewRequisition"
                            menu item name is CreateNewRequisition
                            icon is icon.target-amethyst
                        action is Requisition.Unreleased.RSSLPLCreateUsingCopyFromRequisition
                            title is "CopyRequisition"
                            menu item name is CopyRequisition
                            icon is icon.documents-emerald
                        visible when (not actor.agent(Employee).Employee.Requester.HideShoppingList)
                            page is RequesterShoppingPage.AddFromShoppingLists
                                title is "Add_fromShoppingLists"
                                menu item name is AddFromShoppingLists
                                icon is icon.clipboard-check-amber
                        visible when (not actor.agent(Employee).Employee.Requester.HideCatalogSearch)
                            page is RequesterShoppingPage.SearchTheCatalog
                                title is "SearchTheCatalog"
                                menu item name is SearchTheCatalog
                                icon is icon.CSFSupplyManagementViewer
                        visible when (not actor.agent(Employee).Employee.Requester.HideShopPunchoutVendors)
                            page is RequesterShoppingPage.ShopPunchoutVendors
                                title is "ShopPunchoutVendors"
                                menu item name is ShopPunchoutVendors
                                icon is icon.directory-azure
        MyUnreleasedRequisitions is a Pane
            span 3 columns
            business class is Requisition
                list is RSSLPLUnreleasedRequisitions
                    overview navigation is RSSLPLUnreleasedRequisitionsNav
        MyRecentlyRequestedItems is a Pane
            span 6 columns
            business class is RequisitionLine
                list is RSSLPLRecentlyRequestedItems
                    overview navigation is RSSLPLRecentlyRequestedItemsNav
        MyRequisitionsNotFullyReceived is a Pane
            span 6 columns
            business class is Requisition
                list is RSSLPLInProccessRequisitions
                    overview navigation is RSSLPLInProccessRequisitionsNav
        MyRecentlyRequestedSpecial_andServiceItems is a Pane
            span 6 columns
            business class is RequisitionLine
                list is RSSLPLRecentlyRequestedSpecialItems
                    overview navigation is RSSLPLRecentlyRequestedSpecialtemsNav

