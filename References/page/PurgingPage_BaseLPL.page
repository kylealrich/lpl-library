PurgingPage is a Page
    title is "Purging"
    implements MenuSearch

    Purging is a Panel
        Layout
            two column
                column1
                    header4 of "PurgeActionCatalog"
                    large menu
                        page is PurgeActionPage
                            title is "PurgeCatalog"
                            menu item name is PurgeActionPage
                            description is "PredefinedDeliveredPurgeActionsAndResults"
                            icon is icon.inventory
                column2
                    header4 of "GenericPurging"
                    large menu
                        action is PurgeData.RunCommand
                            title is "PurgeData"
                            menu item name is PurgeData
                            description is "AGenericPurgeActionForPurgingRecordsFromTheDatabase"
                            icon is icon.delete-grid-record

