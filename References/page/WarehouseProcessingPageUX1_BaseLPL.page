WarehouseProcessingPageUX1 is a Page
    title is "Processing"
    implements MenuSearch

    Warehousing is a DashBoardPanel
        use 12 column grid
        Processing is a Pane
            span 12 columns
            Layout
                three column distributed
                    small menu
                        list is WarehouseDemandLine.InventoriedAllocationsDetail
                            title is "InventoriedAllocations"
                            menu item name is InventoriedAllocations
                            icon is icon.CSFWarehouse
                        action is WarehouseBatchAllocation.WarehouseBatchAllocation
                            title is "BatchAllocation"
                            menu item name is BatchAllocation
                            icon is icon.CSFWarehouse
                        action is WarehouseBatchFeedback.WarehouseBatchFeedback
                            title is "BatchFeedback"
                            menu item name is BatchFeedback
                            icon is icon.CSFWarehouse
                        action is WarehousePickListPrint.WarehousePickListPrint
                            title is "PrintPickLists"
                            menu item name is PrintPickList
                            icon is icon.CSFWarehouse
                        visible when (actor.context.FinanceEnterpriseGroup.FSMAdvancedWarehouseEnabled)
                            action is WarehouseDemandLine.WarehousePickListPrintPivotAction
                                title is "NEWPrintPickLists"
                                menu item name is NEWPrintPickList
                                icon is icon.CSFWarehouse
                        action is WarehouseBulkPickList.WarehouseBulkPickList
                            title is "BulkPickLists"
                            menu item name is BulkPickList
                            icon is icon.CSFWarehouse
                        action is WarehousePackingListPrint.WarehousePackingListPrint
                            title is "PrintPackingLists"
                            menu item name is PrintPackingLists
                            icon is icon.CSFWarehouse
                        action is WarehouseShipmentLoadProgram.WarehouseShipmentLoadProgram
                            title is "LoadShipmentsProgram"
                            menu item name is LoadShipmentsProgram
                            icon is icon.CSFWarehouse
                        action is WarehouseShipmentRelease.WarehouseShipmentRelease
                            title is "ReleaseShipments"
                            menu item name is ReleaseShipment
                            icon is icon.CSFWarehouse
                        action is WarehouseShipmentOut.WarehouseShipmentInterfacePurge
                            title is "PurgeShipmentInterface"
                            menu item name is PurgeShipmentInterface
                            icon is icon.CSFWarehouse
                        action is WarehouseProofOfDeliveryPrint.WarehouseProofOfDeliveryPrint
                            title is "PrintProofOfDelivery"
                            menu item name is PrintProofOfDelivery
                            icon is icon.CSFWarehouse
                        action is WarehouseBatchFeedbackInterface.WarehouseBatchFeedbackInterface
                            title is "BatchFeedbackInterface"
                            menu item name is BatchFeedbackInterface
                            icon is icon.documents-amber
                        list is WarehouseShipmentImport.primary
                            title is "MaintainShipmentInterface"
                            menu item name is MaintainShipmentInterface
                            icon is icon.recall-amber
        BillOfLading is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        list is WarehouseShipment.EnterBillOfLading
                            title is "EnterBillOfLading"
                            menu item name is EnterBillOfLading
                            icon is icon.document-amethyst
                        action is WarehouseBillOfLadingPrint.WarehouseBillOfLadingPrint
                            title is "PrintBillOfLading"
                            menu item name is PrintBillOfLading
                            icon is icon.document-amethyst
                        list is BillOfLading.PrintedBillsOfLading
                            title is "ViewBillsOfLadingDocuments"
                            menu item name is ViewBillsOfLadingDocuments
                            icon is icon.document-amethyst
        DistributionManagement is a Pane
            span 4 columns
            visible when (actor.context.FinanceEnterpriseGroup.FSMAdvancedWarehouseEnabled)
            Layout
                single column
                    small menu
                        page is ReceivingPutawayPage
                            title is "ReceivingPutaways"
                            menu item name is ReceivingPutaways
                            icon is icon.document-amethyst
                        list is WarehouseShipmentLineDetail.ShipmentsByWaveList
                            title is "ShipmentsByWave"
                            menu item name is ShipmentsByWave
                            icon is icon.document-amethyst
                        page is DistributionManagementShipmentsPage
                            title is "Shipments"
                            menu item name is Shipments
                            icon is icon.document-amethyst
                        page is ShipmentLoadConsolidationPage
                            title is "ShipmentLoadConsolidation"
                            menu item name is ShipmentLoadConsolidation
                            icon is icon.document-amethyst
                        page is VehicleTripPage
                            title is "VehicleTrips"
                            menu item name is VehicleTrips
                            icon is icon.document-amethyst
                        page is LogisticDeliveryPage
                            title is "LogisticDeliveries"
                            menu item name is LogisticDeliveries
                            icon is icon.document-amethyst
                        page is WarehouseExpirationPage
                            title is "Lot_Expiration_List"
                            menu item name is WarehouseExpirationPage
                            icon is icon.document-amethyst
                            Search Terms
                                    "ExpiringItems"
                                    "ExpiredItems"
                                    "LotManagement"

    WarehouseShipmentLineWebTracking is a Panel
        title is "ShipmentLineWebTracking"
        business class is WarehouseShipmentLineWebTracking
            list is WarehouseShipmentLineWebTrackingList
                title is "ShipmentLineWebTracking"

