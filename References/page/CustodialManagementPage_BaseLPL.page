CustodialManagementPage is a Page
    title is "CustodialManagement"
    implements MenuSearch

    CustodialMgmt is a DashBoardPanel
        title is "CustodialManagement"
        use 12 column grid
        CMSetup is a Pane
            span 6 columns
            title is "Setup"
            Layout
                two column distributed
                    list is CamCategory.CamCategoryList
                        title is "Category"
                        menu item name is Category
                        icon is icon.document-certified-azure
                    list is CamClassification.CamClassificationList
                        title is "Classification"
                        menu item name is Classification
                        icon is icon.document-certified-azure
                    list is CamCustodian.CamCustodianList
                        title is "Custodian"
                        menu item name is Custodian
                        icon is icon.document-certified-azure
                    list is CamFundingCharacteristics.CamFundingCharacteristicsList
                        title is "FundingCharacteristics"
                        menu item name is FundingCharacteristics
                        icon is icon.document-certified-azure
                    list is CamSite.CamSiteList
                        title is "Site"
                        menu item name is Site
                        icon is icon.document-certified-azure
                    list is CamSourceOfFunds.CamSourceOfFundsList
                        title is "SourceOfFunds"
                        menu item name is SourceOfFunds
                        icon is icon.document-certified-azure
                    page is CustodialBalanceUpdatePage
                        title is "UpdateBalances"
                        menu item name is UpdateBalances
                        icon is icon.document-certified-azure
        CustodialLists is a Pane
            span 3 columns
            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension10Label>_Management"
            Layout
                single column
                    list is CustodialAccountManagement.CustodialAccountManagementList
                        title is "CustodialAccountManagement"
                        menu item name is CustodialAccountManagement
                        icon is icon.document-certified-amethyst
                    list is CustodialDetailTransfer.CustodialDetailTransferList
                        title is "Transfer"
                        menu item name is Transfer
                        icon is icon.document-certified-amethyst
                    list is FinanceDimension10.CustodialAccountCompareList
                        title is "CustodialAccountCompare"
                        menu item name is CustodialAccountCompare
                        icon is icon.document-certified-amethyst

