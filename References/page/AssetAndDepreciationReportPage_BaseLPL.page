AssetAndDepreciationReportPage is a Page
    title is "AssetAndDepreciationReport"
    implements MenuSearch
    is report for AssetManager,AssetAnalyst,ApplicationAdministrator,FinancialsViewer,Assets,FinancialsProcessor
        category is "Depreciation"

    TransactionsReport is a Panel
        title is "PostingBooks"
        business class is AssetTransaction
            list is TransactionColumnarList
                title is "PostingBooks"

    BookTransactionsReport is a Panel
        title is "NonPostingBooks"
        business class is AssetBookTransaction
            list is BookTransactionColumnarList
                title is "NonPostingBooks"

