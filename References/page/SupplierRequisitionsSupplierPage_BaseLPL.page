SupplierRequisitionsSupplierPage is a Page
    title is "Requisitions"
    implements MenuSearch

    MyRequisitions is a Panel
        business class is Requisition
            list is SupplierRequisitions

    OtherProcesses is a DashBoardPanel
        use 12 column grid
        CreateAndManageRequisitions is a Pane
            span 3 columns
            title is "CreateAndManage"
            Layout
                single column
                    small menu
                        action is Requisition.SupplierCreate
                            title is "CreateRequisition"
                            menu item name is CreateSupplierRequisition
                            icon is icon.check-emerald
                        visible when (actor.agent(SupplierSourceId).AllowTemplate)
                            list is ProcurementTemplate.ProcurementTemplateListByVendorMenu
                                title is "ProcurementTemplates"
                                menu item name is ProcurementTemplates
                                icon is icon.check-emerald
                        page is SupplierCapitatedProceduresPage
                            title is "CapitatedProcedures"
                            menu item name is CapitatedProcedures
                            icon is icon.check-emerald

