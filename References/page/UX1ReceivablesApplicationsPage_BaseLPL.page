UX1ReceivablesApplicationsPage is a Page
    title is "Applications"
    implements MenuSearch

    SelectInvoicesAndApplyPayments is a Panel
        title is "Select_Invoices_and_Apply_Payments"
        Search Terms
                "SelectInvoicesAndApplyPayment"
                "SelectInvoicesAndApplyCash"
                "SelectAndApply"
                "SelectInvoices"
                "SelectInvoice"
                "Select"
                "SelectAPaymentToCreateSelectedApplications"
        business class is ReceivablePayment
            list is CashApplicationSelectListWithDetail
                title is "Select_Invoices_and_Apply_Payments"

    ApplyPayments is a Panel
        Search Terms
                "ApplyPayment"
                "ApplyCash"
                "Apply"
                "ApplyInvoices"
                "ApplyInvoice"
                "SelectAPaymentToCreateApplications"
        business class is ReceivablePayment
            list is CashApplicationListWithDetail

    ApplyCredit is a Panel
        title is "ApplyCredits"
        Search Terms
                "ApplyCredits"
                "ApplyCreditMemo"
                "ApplyCreditMemos"
                "ApplyToCredit"
                "ApplyToCredits"
                "SelectACreditMemoToCreateApplications"
        business class is ReceivableInvoiceDetail
            list is OpenCreditMemoList
                title is "ApplyCredits"

    InvoiceWriteoffs is a MultiPanePanel
        title is "InvoiceWriteoffs"
        Search Terms
                "InvoiceWriteoff"
                "Writeoff"
                "Writeoffs"
                "WriteOff"
                "WriteOffs"
                "TransactionWriteoffs"
                "TransactionWriteoff"
        pane 1
            business class is CompanyCustomer
                list is CustomerTransactionWriteoffList
                    title is "CustomerInvoiceWriteoffs"
        pane 2
            business class is ReceivableCompany
                list is CompanyTransactionWriteoffList
                    title is "CompanyInvoiceWriteoffs"

    AutomaticApplication is a DashBoardPanel
        use 12 column grid
        AutomaticApplication is a Pane
            span 6 columns
            title is "Process"
            Layout
                two column distributed
                    large menu
                        action is AutoCashApplication.AutoCashApplication
                            title is "AutomaticCashApplication"
                            menu item name is AutomaticCashApplication
                            description is "Automatically_apply_payments_to_invoices_using_the_customer's_automatic_application_method."
                            icon is icon.check-azure
                            Search Terms
                                    "AutoCashApp"
                                    "AutoCashApplication"
                                    "AutoPaymentApp"
                                    "AutoPaymentApplication"
                        action is AutomaticMemoApplication.AutomaticMemoApplication
                            title is "AutomaticMemoApplication"
                            menu item name is AutomaticMemoApplication
                            description is "Automatically_apply_credit_memos_to_cross-referenced_invoices,_or_apply_all_open_credits_to_open_invoices."
                            icon is icon.compare-azure
                            Search Terms
                                    "AutoMemoApp"
                                    "AutoMemoApplication"
                                    "AutoCreditApp"
                                    "AutomaticCreditApplication"
                                    "AutoCreditMemoApp"
                                    "AutomaticCreditMemoApplication"
                        action is AutomaticPrepaymentApply.AutomaticPrepaymentApply
                            title is "AutomaticPrepaymentApply"
                            menu item name is AutomaticPrepaymentApply
                            description is "Automatically_apply_customer_prepayments."
                            icon is icon.document-finance-azure
                            Search Terms
                                    "AutoPrepayApp"
                                    "AutoPrepaymentApplication"
                                    "PrepaymentApply"
                                    "PrepayApply"
                        action is AutomaticTransactionRemoval.AutomaticTransactionRemoval
                            title is "AutomaticTransactionRemoval"
                            menu item name is AutomaticTransactionRemoval
                            description is "Automatically_write_off_transactions_based_on_specified_criteria."
                            icon is icon.recall-azure
                            Search Terms
                                    "AutoTransactionRemoval"
                                    "AutoTranRemoval"
                                    "TransactionRemoval"
                                    "TranRemoval"
        AutomaticCashApplicationResults is a Pane
            span 6 columns
            business class is CashApplicationResult
                list is ShortCashApplicationResultsWithTag
                    overview navigation is CashApplicationResultsListNav

    ReviewApplications is a DashBoardPanel
        use 12 column grid
        ReviewApplications is a Pane
            span 12 columns
            title is "Applications"
            Layout
                three column distributed
                    large menu
                        list is ReceivablePayment.ReviewPaymentApplicationsList
                            title is "Payments"
                            menu item name is Payments
                            description is "Review_applications_for_payments."
                            icon is icon.payment-azure
                            Search Terms
                                    "ReviewPaymentApplications"
                                    "ReviewPaymentApplication"
                                    "ReviewApplicationsForPayments"
                        list is ReceivableInvoiceDetail.TransactionApplicationsInvoiceDetailList
                            title is "Invoices"
                            menu item name is Invoices
                            description is "Review_applications_for_invoices."
                            icon is icon.document-finance-azure
                            Search Terms
                                    "ReviewInvoiceApplications"
                                    "ReviewInvoiceApplication"
                                    "ReviewApplicationsForInvoices"
                        list is ReceivablePaymentHeader.ReviewPaymentBatchApplicationsList
                            title is "PaymentBatches"
                            menu item name is PaymentBatches
                            description is "Review_applications_for_payments_within_a_batch."
                            icon is icon.document-payables-azure
                            Search Terms
                                    "ReviewPaymentBatchApplications"
                                    "ReviewPaymentBatchApplication"
                                    "ReviewApplicationsForPaymentBatches"
                        list is ReceivableApplication.ReviewGroupedApplications
                            title is "GroupedApplications"
                            menu item name is GroupedApplications
                            description is "Review_transactions_that_were_combined_to_create_a_new_debit_memo_or_credit_memo."
                            icon is icon.documents-azure
                            Search Terms
                                    "ReviewGroupedApplications"
                        list is ReceivableApplication.UnpostedReceivableInvoiceApplications
                            title is "Unposted"
                            menu item name is Unposted
                            description is "Review_applications_that_have_not_been_processed_by_application_close."
                            icon is icon.clipboard-list-azure
                            Search Terms
                                    "ReviewUnposted"
                                    "ReviewUnpostedApplications"
                        list is ReceivableApplication.ReviewAllApplicationsList
                            title is "AllApplications"
                            menu item name is AllApplications
                            description is "Review_all_unposted_and_historical_applications."
                            icon is icon.book-azure
                            Search Terms
                                    "ReviewAllApplications"
                                    "ReviewApplications"
                visible when (actor.context.FinanceEnterpriseGroup.FundAccounting)
                    three column distributed
                        column1
                            large menu
                                page is DistributionSetsPage
                                    title is "DistributionSets"
                                    menu item name is DistributionSets
                                    description is "Create_and_maintain_distribution_sets_for_unposted_applications."
                                    icon is icon.document-azure
                                    Search Terms
                                            "DistributionSet"
                                            "CreateDistributionSet"
                                            "MaintainDistributionSet"
                                            "ReviewDistributionSet"
                                            "CreateDistributionSets"
                                            "MaintainDistributionSets"
                                            "ReviewDistributionSets"
        ReviewAdjustments is a Pane
            span 6 columns
            title is "Adjustments"
            Layout
                two column distributed
                    large menu
                        list is CompanyCustomer.CustomerAdjustmentMaintenanceList
                            title is "Customer"
                            menu item name is Customer
                            description is "Review_and_create_adjustments_by_customer."
                            icon is icon.person-emerald
                            Search Terms
                                    "ReviewCustomerAdjustments"
                        list is ReceivableCompany.CompanyAdjustmentMaintenanceList
                            title is "Company"
                            menu item name is Company
                            description is "Review_and_create_adjustments_by_company."
                            icon is icon.book-emerald
                            Search Terms
                                    "ReviewCompanyAdjustments"
                        list is ReceivableApplicationAdjustment.AdjustmentsExceededReviewList
                            title is "ExceededLimit"
                            menu item name is ExceededLimit
                            description is "Review_exceeded_adjustments."
                            icon is icon.clipboard-check-emerald
                            Search Terms
                                    "ReviewAdjustmentsExceededLimit"
                                    "AdjustmentsExceededLimit"
                        list is ReceivableApplicationAdjustment.AdjustmentsRequiringAttachment
                            title is "AdjustmentsRequiringAttachment"
                            menu item name is AdjustmentsRequiringAttachment
                            description is "Adjustments_Requiring_Attachment_will_not_be_processed_by_Application_Close_until_attachment_has_been_made."
                            icon is icon.clipboard-check-emerald
                            Search Terms
                                    "AdjustmentsRequiringAttachment"
                        list is ReceivableApplicationAdjustment.ReviewAllAdjustmentsList
                            title is "AllAdjustments"
                            menu item name is AllAdjustments
                            description is "Review_all_adjustments."
                            icon is icon.book-emerald
                            Search Terms
                                    "ReviewAllAdjustments"
                                    "ReviewAdjustments"
        Reversals is a Pane
            span 6 columns
            Layout
                two column distributed
                    large menu
                        list is ReceivablePayment.IncludedHistoricalPaymentsWithPostedApplicationReversalList
                            title is "PaymentReversal"
                            menu item name is PaymentReversal
                            description is "Reverse_applications_for_a_payment."
                            icon is icon.payment-emerald
                            Search Terms
                                    "PaymentApplicationReversals"
                                    "PaymentApplicationReversal"
                        action is ReceivablesJobResult.ApplicationReversalCreation
                            title is "ApplicationReversalCreation"
                            menu item name is ApplicationReversalCreation
                            description is "Creates_reversal_applications_for_unreleased_payment_and_invoice_reversal_records."
                            icon is icon.cycle-emerald
                        list is ReceivableInvoiceDetail.IncludedHistoricalTransactionsWithPostedApplicationsForReversal
                            title is "InvoiceReversal"
                            menu item name is InvoiceReversal
                            description is "Reverse_applications_for_an_invoice."
                            icon is icon.document-finance-emerald
                            Search Terms
                                    "InvoiceApplicationReversals"
                                    "InvoiceApplicationReversal"
                        list is ReceivablesJobResult.ApplicationReversalResultList
                            title is "ApplicationReversalCreationResults"
                            menu item name is ApplicationReversalCreationResult
                            description is "View_reversal_results."
                            icon is icon.document-finance-emerald
                            Search Terms
                                    "ReversalResults"
                                    "ApplicationReversalResults"

