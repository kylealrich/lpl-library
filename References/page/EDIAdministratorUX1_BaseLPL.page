EDIAdministratorUX1 is a Page
    title is "EDIAdministration"
    implements MenuSearch

    General is a DashBoardPanel
        use 12 column grid
        EDI is a Pane
            span 12 columns
            icon is icon.outline-maintenance-azure
            Layout
                single column
                    small menu
                        list is PfiEDICarrier.CarrierList
                            title is "Carriers"
                            menu item name is Carriers
                        list is EDIDataSubstitutionTable.EDIDataSubstitutions
                            title is "DataSubstitutions"
                            menu item name is DataSubstitutions
                        page is TradingPartnerDashboard
                            title is "TradingPartners"
                            menu item name is TradingPartners
                        list is IsaControlNumber.IsaControlNumberList
                            title is "ISAControlNumbers"
                            menu item name is ISAControlNumbers
                        list is GsControlNumber.GsControlNumberList
                            title is "GSControlNumbers"
                            menu item name is GSControlNumbers
                        action is EDITradingPartner.CreateControlRecords
                            title is "CreateControlNumberRecords"
                            menu item name is CreateControlNumberRecords

