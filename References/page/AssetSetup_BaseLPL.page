AssetSetup is a Page
    title is "Maintenance"
    implements MenuSearch

    System is a Panel
        title is "System"
        business class is AssetCompany
            list is AssetMaintenanceCardList
                title is "System"

    Companies is a Panel
        business class is AssetCompany
            list is MaintenanceCardList

    Books is a Panel
        business class is Book
            list is BookCardList

    General is a DashBoardPanel
        title is "General"
        use 12 column grid
        DepreciationAndValuationMethods is a Pane
            span 6 columns
            title is "Depreciation_andValuationMethods"
            Layout
                two column distributed
                    small menu
                        list is Method.DepreciationMethodList
                            title is "DepreciationMethods"
                            menu item name is DepreciationMethods
                            icon is icon.clipboard-list-azure
                            Search Terms
                                    "DepreciationMethods"
                        list is Method.InsuranceMethodList
                            title is "InsuranceMethods"
                            menu item name is InsuranceMethods
                            icon is icon.clipboard-list-azure
                            Search Terms
                                    "InsuranceMethods"
                        list is Method.ReplacementMethodList
                            title is "ReplacementMethods"
                            menu item name is ReplacementMethods
                            icon is icon.clipboard-list-azure
                            Search Terms
                                    "ReplacementMethods"
                        list is BusinessClassCompute.primary
                            title is "BusinessClassComputes"
                            menu item name is BusinessClassComputes
                            icon is icon.clipboard-list-azure
                            Search Terms
                                    "BusinessClassComputes"
        GuidelineAndRegulations is a Pane
            span 6 columns
            title is "Guidelines_andRegulations"
            Layout
                two column distributed
                    small menu
                        list is AssetGuidelineClass.AssetGuidelineClasses
                            title is "GuidelineClasses"
                            menu item name is GuidelineClasses
                            icon is icon.clipboard-list-azure
                            Search Terms
                                    "GuidelineClasses"
                        list is AssetGuidelineClassRegulation.AssetGuidelineClassRegulations
                            title is "GuidelineClassRegulations"
                            menu item name is GuidelineClassRegulations
                            icon is icon.clipboard-list-azure
                            Search Terms
                                    "GuidelineClassRegulations"
                        list is BookCalendar.BookCalendarsWithinACompany
                            title is "BookCalendars"
                            menu item name is BookCalendars
                            icon is icon.clipboard-list-azure
                            Search Terms
                                    "BookCalendars"
                        list is BookRegulation.BookRegulations
                            title is "BookRegulations"
                            menu item name is BookRegulations
                            icon is icon.clipboard-list-azure
                            Search Terms
                                    "BookRegulations"

    Import is a DashBoardPanel
        use 12 column grid
        ImportListings is a Pane
            span 6 columns
            Layout
                two column distributed
                    small menu
                        list is AssetBookAdditionalImport.AssetBookAdditionalImportList
                            title is "AdditionalBookImport"
                            menu item name is AdditionalBookImport
                            icon is icon.clipboard-list-azure
                            Search Terms
                                    "AdditionalBookImport"
                                    "BookImport"
                        list is AssetImport.AssetImportList
                            title is "ImportGroups"
                            menu item name is ImportGroups
                            icon is icon.clipboard-list-azure
                            Search Terms
                                    "ImportGroups"
                        list is AssetItemAdjustmentImport.AssetItemAdjustmentImportList
                            title is "ItemAdjustments"
                            menu item name is ItemAdjustments
                            icon is icon.clipboard-list-azure
                            Search Terms
                                    "ItemAdjustments"
        ImportProcessing is a Pane
            span 6 columns
            Layout
                two column distributed
                    small menu
                        action is AdditionalBookConversion.AdditionalBookConversion
                            title is "AdditionalBooks"
                            menu item name is AdditionalBookImport
                            icon is icon.check-azure
                            Search Terms
                                    "AdAdditionalBooksImport"
                        action is AssetItemUpdateAdjustmentInterface.AssetItemUpdateAdjustmentInterface
                            title is "AdditionalItemImport"
                            menu item name is AdditionalItemImport
                            icon is icon.check-azure
                            Search Terms
                                    "AdditionalItemImport"

    Utilities is a DashBoardPanel
        use 12 column grid
        GeneralUtilities is a Pane
            span 6 columns
            title is "General"
            Layout
                two column distributed
                    small menu
                        action is BookCopy.BookCopy
                            title is "CopyBook"
                            menu item name is CopyBook
                            icon is icon.documents-azure
                            Search Terms
                                    "CopyBook"
                        action is BookNameChange.BookNameChange
                            title is "ChangeBookName"
                            menu item name is BookNameChange
                            icon is icon.check-azure
                            Search Terms
                                    "BookNameChange"
                        action is AssetBarCodeLabels.AssetBarCodeLabels
                            title is "PrintBarCodeLabels"
                            menu item name is PrintBarCodeLabels
                            icon is icon.document-azure
                            Search Terms
                                    "PrintBarCodeLabels"
                        action is AssetTransaction.UpdateYearEndDate
                            title is "UpdateTransactionYearEndDate"
                            menu item name is UpdateTransactionYearEndDate
                            description is "Populate_Year_End_Date_on_Asset_Transaction_and_Asset_Book_Transaction"
                            icon is icon.check-azure
                            Search Terms
                                    "UpdateTransactionYearEndDate"
                        action is AssetBook.UpdateYearEndBalance
                            title is "UpdateCurrentYearEndBalance"
                            menu item name is UpdateYearEndBalance
                            icon is icon.check-azure
                            Search Terms
                                    "UpdateYearEndBalance"
                        action is Asset.AssetBuildIndexSetAction
                            title is "RebuildAssetTextSearch"
                            menu item name is RebuildAssetTextSearch
                            icon is icon.check-azure
                            Search Terms
                                    "RebuildAssetTextSearch"
                        action is AssetItem.CreateNonExistingAssetBarCode
                            title is "GenerateItemBarcodes"
                            menu item name is GenerateItemBarcodes
                            icon is icon.check-azure
                            Search Terms
                                    "GenerateItemBarcodes"
                        action is AssetDisposal.FixMigratedAssetDisposals
                            title is "FixMigratedAssetDisposals"
                            menu item name is FixMigratedAssetDisposals
                            icon is icon.check-azure
                            Search Terms
                                    "FixMigratedAssetDisposals"
                        action is Project.RemoveHoldFromCapitalizableTransactions
                            title is "RemoveHoldFromCapitalTransactions"
                            menu item name is MassAssetAdjustments
                            icon is icon.check-azure
                            Search Terms
                                    "RemoveHoldFromCapitalTransactions"
                                    "RemoveHold"
        PurgeData is a Pane
            span 6 columns
            Layout
                two column
                    small menu
                        action is AssetCompany.DisposedAssetsPurge
                            title is "DisposedAssets"
                            menu item name is DisposedAssetsPurge
                            description is "Purge_disposed_asset_records_from_the_asset_accounting_database"
                            icon is icon.check-azure
                            Search Terms
                                    "DisposedAssetsPurge"
                        action is Book.BookPurge
                            title is "BookPurge"
                            menu item name is BookPurge
                            description is "Remove_asset_book_records_from_a_book_and_company_or_company_group"
                            icon is icon.check-azure
                            Search Terms
                                    "BookPurge"
                        action is AssetCompany.PurgeAssetHistory
                            title is "AssetHistory"
                            menu item name is PurgeAssetHistory
                            description is "Purging_asset_history_permanently_deletes_data"
                            icon is icon.check-azure
                            Search Terms
                                    "PurgeAssetHistory"
        Depreciation is a Pane
            span 6 columns
            title is "DepreciationUtilities"
            Layout
                two column
                    small menu
                        action is AssetDepreciationCalculationAndList.AssetDepreciationCalculationAndList
                            title is "CalculateDepreciation"
                            menu item name is CalculateDepreciation
                            description is "Calculate_current_period_depreciation"
                            icon is icon.calculator-azure
                            Search Terms
                                    "CalculateDepreciation"
                        action is DepreciationRecalculation.DepreciationRecalculation
                            title is "RecalculateDepreciation"
                            menu item name is RecalculateDepreciation
                            description is "Recalculate_depreciation_values"
                            icon is icon.calculator-azure
                            Search Terms
                                    "RecalculateDepreciation"
                        action is DepreciationHistoryBalances.DepreciationHistoryBalances
                            title is "DepreciationHistoryBalances"
                            menu item name is DepreciationHistoryBalances
                            description is "DepreciationHistoryBalances"
                            icon is icon.check-azure
                            Search Terms
                                    "DepreciationHistoryBalances"
                        action is DepreciationHistoryRebuild.DepreciationHistoryRebuild
                            title is "HistoryRebuild"
                            menu item name is DepreciationHistoryRebuild
                            description is "Rebuild_Depreciation_History_Balances"
                            icon is icon.check-azure
                            Search Terms
                                    "DepreciationHistoryRebuild"

