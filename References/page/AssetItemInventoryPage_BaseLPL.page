AssetItemInventoryPage is a Page
    title is "AssetItemInventory"
    implements MenuSearch
    is report for AssetManager,AssetAnalyst,ApplicationAdministrator,FinancialsViewer,Assets,FinancialsProcessor
        category is "Asset"

    Scanned is a Panel
        title is "Scanned"
        business class is AssetItemInventory
            list is InventoryComparison
                title is "Scanned"

    NotScanned is a Panel
        title is "NotScanned"
        business class is AssetItem
            list is NotScannedItems
                title is "NotScanned"

