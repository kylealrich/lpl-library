RecallNoticeTaskPage is a Page
    title is "Notices"

    PendingNotices is a Panel
        mouse over text is "PendingNotices"
        business class is RecallNotice
            list is PendingRecallNotices
            Context
                RecallGroup

    NoticesWithTransactions is a Panel
        mouse over text is "NoticesWithTransactions"
        business class is RecallNotice
            list is NoticesWithTransactions
            Context
                RecallGroup

    NoticesWithNoTransactions is a Panel
        mouse over text is "NoticesWithNoTransactions"
        business class is RecallNotice
            list is NoticesWithNoTransactions
            Context
                RecallGroup

    NewItemProposals is a Panel
        business class is RecallNotice
            list is ItemProposals
            Context
                RecallGroup

    ActiveNotices is a MultiPanePanel
        mouse over text is "ActiveNotices"
        pane 1
            business class is RecallNotice
                list is ActiveRecallNotices
                Context
                    RecallGroup
        pane 3
            business class is pane 1.RecallNoticeTaskStatus set
                list is RecallNoticeTaskStatus
        pane 4
            business class is pane 3.RecallNoticeTaskRel
                list is RecallNoticeTaskChartList

    HistoricalNotices is a MultiPanePanel
        mouse over text is "HistoricalNotices"
        pane 1
            business class is RecallNoticeStatus
                list is HistoricalRecallNoticeStatus
                Context
                    RecallGroup
        pane 2
            business class is pane 1.RecallNoticeRel
                list is RecallNoticeChartList

    StatusAnalysis is a MultiPanePanel
        mouse over text is "StatusAnalysis"
        pane 1
            business class is RecallGroup
                list is StatusList
                Context
                    RecallGroup
        pane 3
            business class is pane 1.RecallNoticeStatus set
                list is RecallNoticeStatus
        pane 4
            business class is pane 3.RecallNoticeRel
                list is RecallNoticeChartList

    RSS is a Panel
        title is "FDA_RSS"
        mouse over text is "FDA_RSS"
        business class is RecallRSS
            list is RecallRSSList
                title is "FDA_RSS"

