MyResourcePage is a Page
    title is "MyClosePage"
    show panel control on left

    Overdue is a Panel
        business class is FinanceResource.ShowOverDueRel
            list is FinanceResourceScheduleListForPage
            Context
                HROrganization
                FinanceResource

    RejectedOneTimeTasks is a Panel
        business class is FinanceResource.MyOneTimeTaskRejectedRel
            list is RejectedOneTimeTasksForPage
            Context
                HROrganization
                FinanceResource

    Approvals is a MultiPanePanel
        pane 1
            business class is FinanceResource.MyApproverClosePeriodTaskRel
                list is MaintainApprovalsListForPage
                Context
                    HROrganization
                    FinanceResource
        pane 2
            business class is FinanceResource.MyOneTimeTaskApproverRel
                list is MaintainOneTimeApprovalsListForPage
                Context
                    HROrganization
                    FinanceResource

    TeamApprovals is a MultiPanePanel
        pane 1
            business class is FinanceResource.MyApproverTeamTaskRel
                list is MaintainApprovalsListForPage
                Context
                    HROrganization
                    FinanceResource
        pane 2
            business class is FinanceResource.MyOneTimeApproverTeamTaskRel
                list is MaintainOneTimeApprovalsListForPage
                Context
                    HROrganization
                    FinanceResource

    Messages is a MultiPanePanel
        pane 1
            business class is FinanceResource.MyMessagesRel
                list is ViewResourceMessageList
                Context
                    HROrganization
                    FinanceResource
        pane 2
            business class is FinanceResource.MessagesSentRel
                list is ViewResourceMessageSentList
                Context
                    HROrganization
                    FinanceResource

    ToDo is a Panel
        business class is FinanceResource.CurrentToDoRel
            list is FinanceResourceScheduleListForPage
            Context
                HROrganization
                FinanceResource

    TeamToDo is a Panel
        business class is FinanceResource.UnassignedRel
            list is UnassignedTasksListForPage
            Context
                HROrganization
                FinanceResource

    FutureTasks is a MultiPanePanel
        pane 1
            business class is FinanceResource.FutureTasksRel
                list is FutureFinanceResourceScheduleListForPage
                Context
                    HROrganization
                    FinanceResource
        pane 2
            business class is FinanceResource.UnassignedFutureRel
                list is UnassignedFutureTasksListForPage
                Context
                    HROrganization
                    FinanceResource

    CompletedTasks is a Panel
        business class is FinanceResource.ClosedOrVoidedRel
            list is CompletedTasksList
            visible when (actor.context.FinanceEnterpriseGroup.CloseManagementGroup.AllowResourceToViewCompletedTasks)
            Context
                HROrganization
                FinanceResource

    Calendar is a Panel
        business class is FinanceResource.MyRemindersRel
            list is CalendarList
            Context
                HROrganization
                FinanceResource

    Preferences is a Panel
        business class is FinanceResource.MyFinanceResourceRel
            list is MyPreferencesForPage
            Context
                HROrganization
                FinanceResource

