ItemLifecycleManagementOtherProcessesPage is a Page
    title is "ProcessingAndAnalysis"
    implements MenuSearch

    ProcessesAndReports is a DashBoardPanel
        use 12 column grid
        Processes is a Pane
            span 6 columns
            title is "Processes"
            Layout
                single column
                    small menu
                        action is RecallGroup.RetrieveNRACRecalls
                            title is "RetrieveNRACNotices"
                            menu item name is RetrieveNRACNotices
                            icon is icon.clipboard-check-amber
                        action is RecallNotice.CreateLotOrSerialDetailsSet
                            title is "CreateLotOrSerialDetails"
                            menu item name is CreateLotOrSerialDetails
                            icon is icon.clipboard-check-amber
                        action is RecallGroup.RunProductTransactionQueries
                            title is "RunProductTransactionQueries"
                            menu item name is RunProductTransactionQueries
                            icon is icon.clipboard-check-amber
                        action is RecallGroup.MonitorTasks
                            title is "MonitorTasks"
                            menu item name is MonitorTasks
                            icon is icon.clipboard-check-amber
                        action is RecallNotice.CloseResolvedNotices
                            title is "CloseResolvedAndCompletedNotices"
                            menu item name is CloseResolvedAndCompletedNotices
                            icon is icon.clipboard-check-amber
        Analysis is a Pane
            span 6 columns
            title is "NoticeAnalysis"
            Layout
                single column
                    small menu
                        list is RecallNotice.RecallNoticeAnalysisSearch
                            title is "Notices"
                            menu item name is NoticeAnalysis
                            icon is icon.document-certified-amethyst
                        list is RecallOrganization.RecallOrganizationAnalysisDashboard
                            title is "NoticesByOrganizationOrCompany"
                            menu item name is OrganizationAnalysis
                            icon is icon.document-certified-amethyst
                        list is RecallUser.RecallCoordinatorAnalysisDashboard
                            title is "NoticesByCoordinator"
                            menu item name is CoordinatorAnalysis
                            icon is icon.document-certified-amethyst
                        list is RecallNotice.RecallTaskAnalysisDashboard
                            title is "Tasks"
                            menu item name is TaskAnalysis
                            icon is icon.document-certified-amethyst
                        list is RecallUser.RecallResponderAnalysisDashboard
                            title is "TasksByResponder"
                            menu item name is ResponderAnalysis
                            icon is icon.document-certified-amethyst
                        list is RecallReport.RecallReportList
                            title is "OtherNoticeReports"
                            menu item name is ItemLifecycleManagementReports
                            icon is icon.document-certified-amethyst
        FactSheets is a Pane
            span 3 columns
            title is "FactSheet"
            Layout
                single column
                    small menu
                        list is SampleDocumentTemplate.FactSheetsForItemLifecycleManagement
                            title is "FactSheets"
                            menu item name is FactSheets
                            icon is icon.bookmark-outlined

