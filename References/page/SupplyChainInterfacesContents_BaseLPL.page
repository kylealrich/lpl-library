SupplyChainInterfacesContents is a Page
    title is "SupplyChainInterfaces"
    implements MenuSearch

    SupplyChainInterfacesDashboard is a DashBoardPanel
        title is "Home"
        use 12 column grid
        Processes is a Pane
            span 6 columns
            Layout
                single column
                    action is PurchasingMassPOIssue.PurchasingMassPOIssue
                        title is "PurchasingMassPOIssue"
                        menu item name is PurchasingMassPOIssue
                        icon is icon.cycle-azure
                    action is PurchaseOrderEDIAcknowledgement.ProcessPurchaseOrderEDIAcknowledgement
                        title is "ProcessPurchaseOrderEDIAcknowledgement"
                        menu item name is ProcessPurchaseOrderEDIAcknowledgement
                        icon is icon.cycle-azure
                    action is ReceiptInterfaceInput.InterfaceReceipts
                        title is "ReceiptInterfaceCreation"
                        menu item name is InterfaceReceiptsCreation
                        icon is icon.cycle-azure
                    action is EDIInvoiceMatchInterfaceInput.InterfaceInvoices
                        title is "MatchInvoiceInterfaceCreation"
                        menu item name is MatchInvoiceInterfaceCreation
                        icon is icon.cycle-azure
        ListsAndResultsAndMaintenance is a Pane
            span 6 columns
            title is "Maintenance"
            Layout
                single column
                    list is PurchaseOrder.PurchaseOrderList
                        title is "PurchaseOrder"
                        menu item name is PurchaseOrder
                        icon is icon.document-emerald
                    list is PurchaseOrderAcknowledgementResult.PurchaseOrderAcknowledgementResultList
                        title is "PurchaseOrderAcknowledgementResults"
                        menu item name is PurchaseOrderAcknowledgementResult
                        icon is icon.document-emerald
                    page is PurchaseOrderReceiptInterfaceMaintenance
                        title is "ReceiptInterfaceMaintenance"
                        menu item name is PurchaseOrderReceiptInterfaceMaintenance
                        icon is icon.document-emerald
                    page is MatchInterfacePage
                        title is "InterfaceInvoiceMaintenance"
                        menu item name is InterfaceInvoiceMaintenance
                        icon is icon.document-emerald

    PurchaseOrder is a Panel
        business class is PurchaseOrderEDIOutput
            list is PurchaseOrderEDIOutputList

    Acknowledgement is a Panel
        business class is PurchaseOrderEDIAcknowledgement
            list is PurchaseOrderEDIAcknowledgementList

    Receipt is a Panel
        business class is ReceiptInterfaceInput
            list is ReceiptInterfaceInputList

    Invoice is a Panel
        business class is EDIInvoiceMatchInterfaceInput
            list is EDIInvoiceMatchInterfaceInputList

    Contract is a Panel
        business class is ContractInterfaceCatalog
            list is ContractInterfaceCatalogList

