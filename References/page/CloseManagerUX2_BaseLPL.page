CloseManagerUX2 is a Page
    title is "MyProcesses"
    implements MenuSearch

    MyProcesses is a MultiPanePanel
        title is "MyProcesses"
        vertical split is 25/75
        pane 1
            business class is CloseManagementGroup.ManagerMyOpenPeriodProcessesAllRel
                list is ManagerAllPeriodCardViewList
                Context
                    CloseManagementGroup
        pane 3
            business class is pane 1.ManagerMyOpenProcessesDetailRel
                list is ManagerOpenProcessDetailList

    MyProcessesTeam is a MultiPanePanel
        title is "MyTeamProcesses"
        vertical split is 25/75
        pane 1
            business class is CloseManagementGroup.ManagerMyTeamOpenPeriodProcessesAllRel
                list is ManagerTeamAllPeriodCardViewList
                Context
                    CloseManagementGroup
        pane 3
            business class is pane 1.ManagerMyOpenProcessesDetailRel
                list is ManagerOpenProcessDetailList

    OpenPeriodSelection is a Panel
        business class is ClosePeriod
            list is ManagerOpenPeriodList

