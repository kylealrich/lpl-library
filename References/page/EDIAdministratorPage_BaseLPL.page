EDIAdministratorPage is a Page
    title is "Overview"

    Dashboard is a DashBoardPanel
        title is "Dashboard"
        show 3 columns
        QuickLinks is a Pane
            Layout
                single column
                    small menu
                        action is TranslateOutbound.TranslateOutbound
                            title is "TranslateOutboundData"
                            menu item name is TranslateOutbound
                            icon is icon.social-space-amber
                        action is TranslateInbound.TranslateInbound
                            title is "TranslateInboundData"
                            menu item name is TranslateInbound
                            icon is icon.social-space-azure
                        action is EDITradingPartner.Create
                            title is "CreateTradingPartner"
                            menu item name is EDITradingPartnerCreate
                            icon is icon.CSFSourcing
                        action is EDIDataSubstitutionTable.Create
                            title is "CreateDataSubstitution"
                            menu item name is EDIDataSubstitutionTableCreate
                            icon is icon.CSFMatch
        Processing is a Pane
            double width
            business class is EDIProcessingMonitor
                list is CardViewList
        OutboundToSend is a Pane
            business class is EDIDataExchange
                list is OutboundToSendList
        InboundToTranslate is a Pane
            business class is EDIDataExchange
                list is InboundToTranslateList
        InboundWithErrors is a Pane
            business class is EDIDataExchange
                list is InboundErrorList
        EDIWorkErrors is a Pane
            triple width
            business class is EDIWork
                list is EDIWorkErrorsList

    DataExchange is a Panel
        business class is EDIDataExchange
            list is EDIDataExchangeList

    EDIWork is a Panel
        business class is EDIWork
            list is EDIWorkList

    Partners is a Panel
        title is "Partners"
        business class is EDITradingPartner
            list is AllPartnersDashboard
                title is "Partners"

    DataSubstitutions is a Panel
        business class is EDIDataSubstitutionTable
            list is EDIDataSubstitutions

    History is a Panel
        title is "History"
        business class is EDITransactionHistory
            list is EDITransactionHistory
                title is "History"

