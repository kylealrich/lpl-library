AdminConsoleStartPage is a Page
    title is "AdministrationConsole"
    implements MenuSearch

    AdministrationConsole is a DashBoardPanel
        use 12 column grid
        Welcome is a Pane
            span 12 columns
            title is "WelcomeToAdministrationConsole"
            Layout
                single column
                    large menu
                        text of "UseTheMainMenuToFind,ManageAndReviewTechnologyCapabilitiesLike_Replication,_Cubes,_Reporting,And_Database."
                            header4
                            bold
                        text of "ForSecurityRelatedItemsSwitchToThe_Security_AdministrationApplication."
                header4 of "BelowAreAFewQuickLinks"
                single column
                    small menu
                        list is LandmarkExceptionLogEntry.primary
                            title is "ExceptionLogs"
                            menu item name is ExceptionLogInquiry
                            description is "LookupAFullExceptionUsingAnExceptionID"
                            icon is icon.travel-plan
                        link is external "<base url(webapp is AdministratorConsole)>/web/landmark/reveal.html"
                            title is "Application&SystemLinks"
                            menu item name is ApplicationSystemLinks
                            description is "Application&SystemLinks"
                            icon is icon.link
                        page is ReportDistributionConfiguration
                            title is "ReportDistribution"
                            menu item name is ReportDistribution
                            description is "ManageReportDistributionSetup"
                            icon is icon.report
        RecentDBSessionDebugLogs is a Pane
            span 6 columns
            business class is DBSessionDebug
                list is RecentDBSessionDebugsList
                    overview navigation is ViewDBSessionDebug
                        label is "ViewAll"
        RecentCubeActivity is a Pane
            span 6 columns
            business class is AnalyticCube
                list is AnalyticCubeRecentList
                    overview navigation is AnalyticCubes
                        label is "ViewAll"

