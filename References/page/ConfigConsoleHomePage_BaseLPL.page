ConfigConsoleHomePage is a Page
    title is "Home"

    Overview is a DashBoardPanel
        use 12 column grid
        ConfigurationActivityLast14Days is a Pane
            span 6 columns
            business class is ConfigEntity
                list is RecentConfigurationsList
                    overview navigation is ConfigurationChangeLog
                        label is "ViewFullChangeLog"
        RemoteSnapshots is a Pane
            span 6 columns
            business class is ConfigBundleSnapshot
                list is ImportConsoleList
                    overview navigation is CompletedImportNav
                        label is "ViewAll"
        InvalidConfigurations is a Pane
            span 6 columns
            business class is ConfigEntity
                list is InvalidConfigurationsList
                    overview navigation is ManageInvalidConfigurations
                        label is "ManageInvalid"
        OverriddenConfigurations is a Pane
            span 6 columns
            business class is ConfigEntity
                list is OverriddenConfigurationsDashboardList
                    overview navigation is OverriddenConfigurationsNav
                        label is "ManageOverridden"
        DuplicateConfigurations is a Pane
            span 6 columns
            business class is ConfigEntity
                list is ConfigEntityDuplicateList
                    overview navigation is ManageDuplicateConfigurations
                        label is "ManageDuplicates"
        InvalidPersonalizations is a Pane
            span 6 columns
            business class is ConfigEntity
                list is InvalidPersonalizationsList
                    overview navigation is ManageInvalidPersonalizations
                        label is "ManageInvalid"
        PersonalizationActivityLast14Days is a Pane
            span 6 columns
            business class is ConfigEntity
                list is RecentPersonalizationList
                    overview navigation is PersonalizationChangeLog
                        label is "ViewFullChangeLog"
        InvalidReports is a Pane
            span 6 columns
            business class is UserFolder
                list is InvalidConfigConsoleReportsList
                    overview navigation is ManageInvalidReports
                        label is "ManageInvalid"
        MissingUserListReports is a Pane
            span 6 columns
            title is "ReportsWithMissingUserList"
            business class is UserFolder
                list is MissingUserListList
                    overview navigation is ManageMissingUserListReports
                        label is "ManageMissingUserList"
        MissingUserPageReports is a Pane
            span 6 columns
            title is "ReportsWithMissingUserPage"
            business class is UserFolder
                list is MissingUserPageList
                    overview navigation is ManageMissingUserPageReports
                        label is "ManageMissingUserPage"
        UnusedTags is a Pane
            span 6 columns
            business class is ConfigurationTag
                list is DashboardUnusedTagList
                    overview navigation is ManageUnusedTags
                        label is "ManageUnused"

