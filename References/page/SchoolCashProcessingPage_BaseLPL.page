SchoolCashProcessingPage is a Page
    title is "CashProcessing"
    show panel control on left

    Context Form
        Layout
            paragraph
                StudentActivitySchool
                    label is "School"
                StudentActivityBankAccount
                    label is "BankAccount"
    OpenCashTransactions is a MultiPanePanel
        pane 1
            business class is StudentActivityReceipt
                list is StudentActivityEnteredReceiptList
                Context
                    StudentActivityDistrict
                    StudentActivitySchool
        pane 2
            business class is StudentActivityBankTransaction
                list is StudentActivityDepositsList
                Context
                    StudentActivityDistrict
                    StudentActivitySchool
                    StudentActivityBankAccount
        pane 3
            business class is StudentActivityBankTransaction
                list is StudentActivityDisbursementsList
                Context
                    StudentActivityDistrict
                    StudentActivitySchool
                    StudentActivityBankAccount

    AllReceipts is a Panel
        business class is StudentActivityReceipt
            list is ActivityReceipts
            Context
                StudentActivityDistrict
                StudentActivitySchool

    OpenBatchReceipts is a MultiPanePanel
        pane 1
            business class is StudentActivityReceiptBatch
                list is OpenBatchReceipts
                Context
                    StudentActivityDistrict
                    StudentActivitySchool
        pane 2
            business class is pane 1.StudentActivityReceipt set
                list is BatchEnteredReceiptList
        pane 3
            business class is pane 1.StudentAssignedActivityByNameRel
                list is ActivitySummaryByNameList

    ReleasedBatchReceipts is a MultiPanePanel
        pane 1
            business class is StudentActivityReceiptBatch
                list is ReleasedBatchReceipts
                Context
                    StudentActivityDistrict
                    StudentActivitySchool
        pane 3
            business class is pane 1.StudentActivityReceipt set
                list is BatchEnteredReceiptList

    AllBankTransactions is a Panel
        business class is StudentActivityBankTransaction
            list is BankTransactions
            Context
                StudentActivityDistrict
                StudentActivitySchool
                StudentActivityBankAccount

    BatchCheckProcessing is a MultiPanePanel
        pane 1
            business class is StudentActivityBankAccount
                list is BatchCheckProcessingPageList
                Context
                    StudentActivityDistrict
                    StudentActivitySchool
                    StudentActivityBankAccount
        pane 2
            business class is pane 1.DisbursementsReleasedForPaymentRel
                list is ReleasedDisbursementsList
        pane 4
            business class is pane 1.DisbursementsForLaserCheckRel
                list is SelectedForBatchPrintingList

    BankReconciliation is a MultiPanePanel
        pane 1
            business class is StudentActivityBankStatement
                list is ReconciledBankBalancesList
                Context
                    StudentActivityDistrict
                    StudentActivitySchool
                    StudentActivityBankAccount
        pane 2
            business class is pane 1.UnreconciledBankTransactionsRel
                list is UnreconciledTransactionsList
        pane 4
            business class is pane 1.StudentActivityBankTransaction set
                list is ReconciledTransactionsList

    ActivityTransfers is a MultiPanePanel
        pane 1
            business class is StudentActivityTransfer
                list is ActivityTransfers
                Context
                    StudentActivityDistrict
                    StudentActivitySchool
        pane 3
            business class is pane 1.StudentActivityTransferDetailRel
                list is StudentActivityTransferDetailList

