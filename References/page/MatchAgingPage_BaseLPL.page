MatchAgingPage is a Page
    title is "MatchAging:NumberOfDaysFromCreateDateToMatchDate"
    implements MenuSearch

    VendorAgingWithPercents is a Panel
        title is "ByVendor"
        visible when (actor.context.FinanceEnterpriseGroup.MatchAgingComplete)
        business class is MatchAgingSummary
            list is VendorAgingWithPercent
                title is "ByVendor"
            Context
                MatchAging

    YearMonthAgingWithPercents is a Panel
        title is "ByMonth"
        visible when (actor.context.FinanceEnterpriseGroup.MatchAgingComplete)
        business class is MatchAgingSummary
            list is YearMonthAgingWithPercent
                title is "ByMonth"
            Context
                MatchAging

    YearMonthAgingChart is a Panel
        title is "ByMonthBarChart"
        visible when (actor.context.FinanceEnterpriseGroup.MatchAgingComplete)
        business class is MatchAgingSummary
            list is YearMonthAgingChart
                title is "ByMonthBarChart"
            Context
                MatchAging

    UnmatchedCard is a Panel
        title is "Unmatched"
        visible when (actor.context.FinanceEnterpriseGroup.MatchAgingComplete)
        business class is MatchAgingSummary
            list is VendorUnmatchedCardList
                title is "Unmatched"
            Context
                MatchAging

    MatchAging is a Panel
        visible when (not actor.context.FinanceEnterpriseGroup.MatchAgingComplete)
        business class is MatchAging
            list is MatchAgingList
            Context
                MatchAging

