GLCommitmentReport is a Page
    title is "CommitmentsAndEncumbrancesReport"
    is report for FinancialBusinessAnalyst,GlobalLedger,StaffAccountant,BudgetController,ChiefFinancialOfficer,Projects,ProjectAccountant,BudgetAnalyst,Controller,ApplicationAdministrator,FinancialsViewer,ProjectAdministrator,FinancialsProcessor
        category is "Transactions"
        visible when (actor.context.FinanceEnterpriseGroup.GLCommitDocumentExists)
    CommitmentsAndEncumbrancesReport is a MultiPanePanel
        title is "DocumentDetails"
        pane 1
            business class is GLCommitDocument
                list is GLCommitDocumentPageList
        pane 2
            business class is pane 1.GLCommitRel
                list is GLCommitList
        pane 4
            business class is pane 2.GLCommitHistoryRel
                list is GLCommitHistoryList

    CommitmentsAndEncumbrancesPivot is a Panel
        title is "Pivot"
        business class is GeneralLedgerTotal
            list is DimensionAnalysisEncumbranceCommits
                title is "Pivot"

    ColumnarCommitmentsAndEncumbrancesList is a Panel
        title is "SearchReplicated"
        business class is GLCommit
            list is ColumnarList
                title is "SearchReplicated"
            visible when (actor.context.FinanceEnterpriseGroup.ShowGlobalLedgerColumnarPanels)
    CommitmentsAndEncumbrancesList is a Panel
        title is "Search"
        business class is GLCommit
            list is Commitments
                title is "Search"

    CustomGroupSearch is a Panel
        title is "CustomGroupSearch"
        business class is GLCommit
            list is DepartmentalCommitmentAndEncumbranceList
                title is "CustomGroupSearch"

    CommitmentsAndEncumbrancesSearch is a Panel
        title is "Search_withProjects"
        business class is GLCommit
            list is ProjectCommitmentsSearch
                title is "Search_withProjects"

