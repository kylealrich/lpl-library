ContractLineAttributeUpdatePage is a Page
    title is "UpdateAttributesForContractLinesAndItems"

    ToProcessFromConsolidatedItemLevelMasterData is a Panel
        visible when (actor.context.FinanceEnterpriseGroup.UpdateContractLineAttributesFromConsolidatedItemMaster)
        business class is ContractLineAttributeUpdate
            list is ContractLineAttributeUpdateListConsolidatedItemMaster

    ProcessedFromConsolidatedItemLevelMasterData is a Panel
        visible when (actor.context.FinanceEnterpriseGroup.UpdateContractLineAttributesFromConsolidatedItemMaster)
        business class is ContractLineAttributeUpdate
            list is NotAvailableToProcessListConsolidatedItemMaster

    AttributeUpdatesToProcess is a Panel
        title is "ToProcessForContractLines"
        visible when (not actor.context.FinanceEnterpriseGroup.UpdateContractLineAttributesFromConsolidatedItemMaster)
        business class is ContractLineAttributeUpdate
            list is ContractLineAttributeUpdateList
                title is "ToProcessForContractLines"

    AttributeUpdatesProcessed is a Panel
        title is "ProcessedForContractLines"
        visible when (not actor.context.FinanceEnterpriseGroup.UpdateContractLineAttributesFromConsolidatedItemMaster)
        business class is ContractLineAttributeUpdate
            list is NotAvailableToProcessList
                title is "ProcessedForContractLines"

    SuggestedLineChanges is a Panel
        business class is ContractLineSuggestedItem
            list is SuggestedLineChanges

    ProcessedLineChanges is a Panel
        business class is ContractLineSuggestedItem
            list is ProcessedLineChanges

