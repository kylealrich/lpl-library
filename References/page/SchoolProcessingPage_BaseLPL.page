SchoolProcessingPage is a Page
    title is "SchoolProcessing"

    Context Form
        Layout
            single column
                StudentActivitySchool
                    label is "School"
    ActivityTransfers is a MultiPanePanel
        pane 1
            business class is StudentActivityTransfer
                list is ActivityTransfers
                Context
                    StudentActivityDistrict
                    StudentActivitySchool
        pane 3
            business class is pane 1.StudentActivityTransferDetailRel
                list is StudentActivityTransferDetailList

    Vendors is a Panel
        business class is StudentActivityVendor
            list is Vendors
            Context
                StudentActivityDistrict

    ClubSponsors is a MultiPanePanel
        pane 1
            business class is StudentActivityClubSponsor
                list is ClubSponsorSelectList
                Context
                    StudentActivityDistrict
                    StudentActivitySchool
        pane 3
            business class is pane 1.StudentActivityReceipt set
                list is ActivityReceipts

    Activities is a Panel
        business class is SchoolActivityAccount
            list is SchoolActivities
            Context
                StudentActivityDistrict
                StudentActivitySchool

    Students is a Panel
        title is "DistrictStudentList"
        business class is StudentRecord
            list is StudentRecordList
                title is "DistrictStudentList"
            Context
                StudentActivityDistrict

    Classes is a Panel
        business class is StudentActivitySchoolClass
            list is SchoolClasses
            Context
                StudentActivityDistrict
                StudentActivitySchool

