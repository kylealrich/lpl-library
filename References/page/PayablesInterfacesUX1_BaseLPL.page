PayablesInterfacesUX1 is a Page
    title is "Interfaces"
    implements MenuSearch

    Main is a DashBoardPanel
        use 12 column grid
        DocumentInvoice is a Pane
            span 3 columns
            Layout
                single column
                    small menu
                        page is PayablesInterfacePageUX1.InterfaceInvoiceMaintenance
                            title is "Invoices"
                            menu item name is Invoices
                            icon is icon.outline-list-amber
                        page is PayablesInterfacePageUX1.InterfaceResults
                            title is "Results"
                            menu item name is Results
                            icon is icon.outline-list-amber
        PayablesInvoice is a Pane
            span 3 columns
            Layout
                single column
                    small menu
                        page is PayablesInvoiceInterfaceMaintenance.Invoices
                            title is "Invoices"
                            menu item name is Invoices
                            icon is icon.outline-list-amethyst
                        page is PayablesInvoiceInterfaceMaintenance.Results
                            title is "Results"
                            menu item name is Results
                            icon is icon.outline-list-amethyst
                        action is InterfaceSelectedPayablesInvoiceRunGroups.ScheduleInterface
                            title is "InterfaceInvoices_forSelectedRunGroups"
                            menu item name is InterfaceInvoicesForSelectedRunGroups
                            icon is icon.outline-process-amethyst
        Match is a Pane
            span 3 columns
            Layout
                single column
                    small menu
                        page is MatchInterfaceViewerPage.Invoices
                            title is "Invoices"
                            menu item name is Invoices
                            icon is icon.outline-list-ruby
                        page is MatchInterfaceViewerPage.Results
                            title is "Results"
                            menu item name is Results
                            icon is icon.outline-list-ruby
                        action is InterfaceMatchInvoices.InterfaceMatchInvoices
                            title is "InterfaceMatchInvoices"
                            menu item name is InterfaceMatchInvoices
                            icon is icon.outline-process-ruby
                        action is InterfaceSelectedMatchInvoiceRunGroups.ScheduleInterface
                            title is "InterfaceMatchInvoices_forSelectedRunGroups"
                            menu item name is InterfaceMatchInvoicesForSelectedRunGroups
                            icon is icon.outline-process-ruby
        EDI is a Pane
            span 3 columns
            Layout
                single column
                    small menu
                        page is EDIInterfaceViewerPage.InterfaceInvoices
                            title is "Invoices"
                            menu item name is Invoices
                            icon is icon.outline-list-emerald
                        page is EDIInterfaceViewerPage.InterfaceResults
                            title is "Results"
                            menu item name is Results
                            icon is icon.outline-list-emerald
                        page is PayablesImageUploadResultPage
                            title is "ImageUploadResults"
                            menu item name is ImageUploadResults
                            icon is icon.outline-list-emerald
        Vendor is a Pane
            span 3 columns
            Layout
                single column
                    small menu
                        page is VendorInterfaces.Vendors
                            title is "Vendors"
                            menu item name is Vendors
                            icon is icon.outline-list-ruby
                        page is VendorInterfaces.Balances
                            title is "Balances"
                            menu item name is Balances
                            icon is icon.outline-list-ruby
                        page is VendorInterfaces.Addresses
                            title is "Addresses"
                            menu item name is Addresses
                            icon is icon.outline-list-ruby
                        page is VendorInterfaces.Results
                            title is "Results"
                            menu item name is Results
                            icon is icon.outline-list-ruby
        ExpenseManagement is a Pane
            span 3 columns
            Layout
                single column
                    small menu
                        page is ExpenseManagementInterfaceUX1.Outstanding
                            title is "Outstanding"
                            menu item name is Outstanding
                            icon is icon.outline-list-azure
                        page is ExpenseManagementInterfaceUX1.Processed
                            title is "Processed"
                            menu item name is Processed
                            icon is icon.outline-list-azure
                        page is ExpenseManagementInterfaceUX1.OutstandingDetails
                            title is "OutstandingDetails"
                            menu item name is OutstandingDetails
                            icon is icon.outline-list-azure
                        page is ExpenseManagementInterfaceUX1.ProcessedDetails
                            title is "ProcessedDetails"
                            menu item name is ProcessedDetails
                            icon is icon.outline-list-azure
                        page is ExpenseManagementInterfaceUX1.Results
                            title is "Results"
                            menu item name is Results
                            icon is icon.outline-list-azure

    Analysis is a Panel
        business class is InterfaceAnalysis
            list is InterfaceAnalysisCardList

    WebServices is a Panel
        business class is WebService
            list is Definitions

