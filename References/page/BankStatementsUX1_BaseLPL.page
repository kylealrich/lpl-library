BankStatementsUX1 is a Page
    title is "BankStatements"
    implements MenuSearch
    show panel control on left

    Processing is a DashBoardPanel
        use 12 column grid
        Processing is a Pane
            span 8 columns
            Layout
                two column distributed
                    small menu
                        page is BankStatementPage
                            title is "BankStatements"
                            menu item name is BankStatements
                            icon is icon.document-finance-emerald
                        list is BankStatementLineDetail.BankStatementLineDetailPrimary
                            title is "BankStatementLineDetails"
                            menu item name is BankStatementLineDetails
                            icon is icon.document-finance-emerald
                        page is CashPositionPage
                            title is "CashPosition"
                            menu item name is CashPosition
                            icon is icon.document-finance-emerald
                        page is CashFeeServicePageUX1.FeeServiceDashBoard
                            title is "FeeServiceStatements"
                            menu item name is FeeServiceStatements
                            icon is icon.document-finance-emerald
                        list is CashManagementAccount.CashAccountStatementReconciliationList
                            title is "ReconcileByAccount"
                            menu item name is ReconcileByAccount
                            icon is icon.document-finance-emerald
                        list is IncomingBankStatementHeader.MissingBankStatementsCalendarList
                            title is "IncomingBankStatements"
                            menu item name is IncomingBankStatements
                            icon is icon.document-finance-emerald
                        page is CashReconciliationPage
                            title is "StatementReconciliation"
                            menu item name is StatementReconciliation
                            icon is icon.document-finance-emerald
                        list is BankStatement.ReconciliationBalancesReportList
                            title is "ReconciliationBalancesReport"
                            menu item name is ReconciliationBalancesReport
                            icon is icon.document-finance-emerald
                        list is CashManagementAccount.CurrentCashLedgerBalanceList
                            title is "CurrentCashLedgerBalances"
                            menu item name is CurrentCashLedgerBalances
                            icon is icon.document-finance-emerald
                        form is CashManagementGroup.ReleasedDistributionsForm
                            title is "ReleasedDistributions"
                            menu item name is ReleasedDistributions
                            icon is icon.document-finance-emerald
                        list is CashLedgerTransaction.AccountLevelCreatedFromBankStatementLine
                            title is "UnreleasedCashLedgerTransactions"
                            menu item name is UnreleasedCashLedgerTransactions
                            icon is icon.document-finance-emerald
        Search is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        list is BankStatementLine.BankStatementLineSearch
                            title is "StatementLines"
                            menu item name is StatementLineSearch
                            icon is icon.search-azure
                        list is BankStatementLineDistribution.StatementLineDistributionSearch
                            title is "StatementLineDistributions"
                            menu item name is StatementLineDistributionSearch
                            icon is icon.search-azure
                        list is BankStatementReconciliation.AccountReconciliationTransactionSearch
                            title is "BankReconciliationTransactions"
                            menu item name is BankReconciliationTransactionSearch
                            icon is icon.search-azure
                        list is NonCashLedgerTransaction.NonCashLedgerTransSearch
                            title is "NonCashLedgerTransactions"
                            menu item name is NonCashLedgerTransactionSearch
                            icon is icon.search-azure

