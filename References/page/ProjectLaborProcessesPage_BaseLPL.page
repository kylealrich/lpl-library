ProjectLaborProcessesPage is a Page
    title is "GrantLaborDistribution"
    implements MenuSearch

    LaborDistribution is a DashBoardPanel
        title is "LaborDistribution"
        use 12 column grid
        SponsoredProjects is a Pane
            span 4 columns
            Layout
                single column distributed
                    column1
                        large menu
                            list is FinanceDimension2.FinanceDimension2PersonResponsibleFlatListUX1
                                title is "FederalAwards"
                                menu item name is FederalAwards
                                description is "View_and_manage_external_grants_flagged_as_included_in_SEFA_reporting."
                                icon is icon.clipboard-list-azure
                        large menu
                            list is Project.PostingProjectsListUX1
                                title is "FederalProjects"
                                menu item name is FederalProjects
                                description is "View_and_manage_posting_projects_flagged_as_requiring_certification."
                                icon is icon.clipboard-list-azure
        ManageLabor is a Pane
            span 8 columns
            Layout
                two column distributed
                    column1
                        large menu
                            page is ProjectAccountantLaborDistribution
                                title is "ManageLaborDistribution"
                                menu item name is ManageLaborDistribution
                                description is "View_and_manage_employees_labor_distribution_by_period_and_employee."
                                icon is icon.person-global-emerald
                        large menu
                            page is ProjectAccountantLaborPage
                                title is "LaborDistribution"
                                menu item name is ProjectLaborHistory
                                description is "Access_search_lists_for_labor_distribution_transactions,_project_assignments_and_historical_labor."
                                icon is icon.clipboard-list-emerald
                        large menu
                            list is ProjectEmployee.ProjectEmployeeList
                                title is "ProjectEmployees"
                                menu item name is ProjectEmployees
                                description is "View_and_manage_project_employees_flagged_for_labor_distribution_and_effort_certification."
                                icon is icon.clipboard-list-emerald
                    column2
                        large menu
                            action is ProjectAssignmentLabor.CopyAllLabor
                                title is "CopyLabor"
                                menu item name is CopyLabor
                                description is "Run_copy_labor_action_to_copy_labor_distribution_for_an_employee_group_for_a_labor_period."
                                icon is icon.settings-emerald
                        large menu
                            action is ProjectEmployeeLaborPeriod.ApproveAllLabor
                                title is "ApproveLabor"
                                menu item name is ApproveLabor
                                description is "Run_approve_labor_action_prior_to_distribute_labor_action_if_your_organization_requires_approval_processing_for_labor_distribution."
                                icon is icon.settings-emerald
                        large menu
                            action is ProjectEmployeeLaborPeriod.UnapproveAllLabor
                                title is "UnapproveLabor"
                                menu item name is UnapproveLabor
                                description is "Run_unapprove_labor_action_to_unapprove_labor_distribution_records_in_mass."
                                icon is icon.settings-emerald
        ManageEncumbrances is a Pane
            span 4 columns
            Layout
                single column distributed
                    column1
                        large menu
                            action is ProjectAssignmentLabor.CreateAllEncumbrances
                                title is "CreateEncumbrances"
                                menu item name is CreateEncumbrances
                                description is "Create_labor_distribution_encumbrances_in_mass_for_a_labor_period_or_range_of_labor_periods."
                                icon is icon.settings-amethyst
                        large menu
                            action is ProjectAssignmentLabor.DeleteAllEncumbrances
                                title is "DeleteEncumbrances"
                                menu item name is DeleteEncumbrances
                                description is "Delete_labor_distribution_encumbrances_in_mass_for_a_labor_period_or_range_of_labor_periods."
                                icon is icon.settings-amethyst
                        large menu
                            page is GLCommitmentReport
                                title is "CommitmentsAndEncumbrances"
                                menu item name is CommitmentsAndEncumbrances
                                description is "Access_commitment_and_encumbrances_report_page."
                                icon is icon.clipboard-list-amethyst
        DistributeLabor is a Pane
            span 4 columns
            Layout
                single column
                    column1
                        large menu
                            action is ProjectAssignmentLabor.ValidateLaborJournalEntries
                                title is "ValidateLaborJournalEntries"
                                menu item name is ValidateLaborJournalEntries
                                description is "Run_this_action_to_validate_journal_entries_before_running_distribute_labor_action._Access_validation_errors_via_errors_tab."
                                icon is icon.settings-amber
                        large menu
                            action is ProjectAssignmentLabor.DistributeAllLabor
                                title is "DistributeLabor"
                                menu item name is DistributeLabor
                                description is "Run_distribute_labor_action_to_distribute_and_journalize_labor_in_mass_for_a_labor_period."
                                icon is icon.settings-amber
                        large menu
                            list is ProjectAssignmentLabor.PendingCostTransferList
                                title is "PendingCostTransfers"
                                menu item name is PendingCostTransfers
                                description is "View_and_manage_pending_cost_transfers._Includes_any_adjusted_processed_project_assignments_that_have_not_been_redistributed."
                                icon is icon.clipboard-list-amber
        RedistributeAndReverseLabor is a Pane
            span 4 columns
            Layout
                single column
                    column1
                        large menu
                            action is ProjectAssignmentLabor.ValidateLaborRedistributionJournalEntries
                                title is "ValidateLaborRedistributionJournalEntries"
                                menu item name is ValidateLaborRedistributionJournalEntries
                                description is "Run_this_action_to_validate_journal_entries_before_running_redistribute_labor_action._Access_validation_errors_via_errors_tab."
                                icon is icon.settings-ruby
                        large menu
                            action is ProjectAssignmentLabor.RedistributeAllLabor
                                title is "RedistributeLabor"
                                menu item name is RedistributeLabor
                                description is "Run_redistribute_labor_action_to_adjust_or_transfer_costs_to_correct_original_distributions."
                                icon is icon.settings-ruby
                        large menu
                            action is ProjectAssignmentLabor.ReverseAllLabor
                                title is "ReverseLabor"
                                menu item name is ReverseLabor
                                description is "Run_reverse_labor_action_to_reverse_all_labor_distributions,_fringe_burdens,_adjustments_and_journal_entries."
                                icon is icon.settings-ruby
                        large menu
                            action is ProjectAssignmentLaborTransaction.RecalculateFringeBurdens
                                title is "RecalculateFringeBurdens"
                                menu item name is RecalculateFringeBurdens
                                description is "Run_recalculate_fringe_burdens_action_to_recalculate_fringe_without_having_to_reverse_and_redistribute_labor."
                                icon is icon.settings-ruby

    JournalizeGroupResults is a Panel
        title is "JournalizeGroupResults"
        business class is JournalizeGroupResult
            list is JournalizeGroupResults
                title is "JournalizeGroupResults"

    Errors is a DashBoardPanel
        use 12 column grid
        Errors is a Pane
            Layout
                two column distributed
                    column1
                        large menu
                            list is ProjectStructureValidationResult.ProjectStructureValidationLaborResultList
                                title is "ValidationResults"
                                menu item name is ValidationResults
                                description is "View_results_of_running_validate_journal_entries_for_labor_distribution._Correct_errors_prior_to_running_journalize_action."
                                icon is icon.clipboard-list-azure
                    column2
                        large menu
                            list is ProjectAssignmentLabor.ProjectAssignmentLaborErrorList
                                title is "LaborProcessingErrors"
                                menu item name is LaborProcessingErrors
                                description is "View_any_error_suspense_entries_resulting_from_running_labor_distribution._Link_to_journalize_group _provides_access_to_GL_journal."
                                icon is icon.clipboard-list-azure

    EffortCertification is a DashBoardPanel
        title is "EffortCertification"
        use 12 column grid
        ManageEffort is a Pane
            span 6 columns
            Layout
                single column distributed
                    column1
                        large menu
                            list is ProjectEmployee.ProjectEmployeeListUX1
                                title is "EmployeesWhoRequireEffortCertification"
                                menu item name is EmployeesWhoRequireEffortCertification
                                description is "View_and_manage_employees_requiring_effort_certification."
                                icon is icon.clipboard-list-azure
                        large menu
                            page is ProjectAccountantEffortCertification
                                title is "ManageEffortCertification"
                                menu item name is ManageEffortCertification
                                description is "View_and_manage_employees_effort_certification_by_period_and_employee."
                                icon is icon.person-global-azure
        EffortProcesses is a Pane
            span 6 columns
            Layout
                two column distributed
                    column1
                        large menu
                            action is ProjectEmployeeEffortPeriod.PopulateAllEffortPercent
                                title is "PopulateEffortPercent"
                                menu item name is PopulateEffortPercent
                                description is "Run_this_action_to_populate_effort_percentages_from_the_average_actual_percentages."
                                icon is icon.settings-emerald
                        large menu
                            action is ProjectAssignmentEffort.CertifyAllEffort
                                title is "CertifyEffort"
                                menu item name is CertifyEffort
                                description is "Run_certify_effort_action_to_update_all_effort_in_mass."
                                icon is icon.settings-emerald
                    column2
                        large menu
                            action is ProjectAssignmentEffort.UncertifyAllEffort
                                title is "UncertifyEffort"
                                menu item name is UncertifyEffort
                                description is "Run_uncertify_effort_action_to_uncertify_effort_in_mass."
                                icon is icon.settings-emerald
                        large menu
                            page is ProjectAccountantEffortCertificationUX1
                                title is "EffortVariance"
                                menu item name is EffortVariance
                                description is "View_effort_variances_and_all_effort_certification._Includes_effort_variance_where_actual_effort_is_not_equal_to_certified_effort."
                                icon is icon.clipboard-list-emerald

