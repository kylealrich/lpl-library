CurrencyPage is a Page
    title is "Currency"

    DailyRate is a Panel
        business class is CurrencyTable
            list is CurrencyRateTables

    PeriodRate is a Panel
        business class is GeneralLedgerTranslationCode
            list is GeneralLedgerTranslationCodeList

    CurrencyRelationship is a Panel
        business class is CurrencyRelationship
            list is CurrencyRelationship

    CurrencyCode is a Panel
        business class is Currency
            list is Currencies

    CurrencyCurrency is a DashBoardPanel
        title is "RateInterface"
        use 12 column grid
        DailyInterface is a Pane
            span 3 columns
            Layout
                single column
                    list is GLExchangeRateInterface.primary
                        title is "DailyRateInterface"
                        menu item name is DailyRateInterface
                        icon is icon.document-finance-emerald
                    list is GLExchangeRateInterfaceResult.primary
                        title is "DailyRateInterfaceResults"
                        menu item name is DailyRateInterfaceResults
                        icon is icon.document-finance-emerald
        PeriodInterface is a Pane
            span 3 columns
            Layout
                single column
                    list is GLTranslationRateInterface.primary
                        title is "PeriodRateInterface"
                        menu item name is PeriodRateInterface
                        icon is icon.document-finance-emerald
                    list is GLTranslationRateInterfaceResult.primary
                        title is "PeriodRateInterfaceResults"
                        menu item name is PeriodRateInterfaceResults
                        icon is icon.document-finance-emerald

