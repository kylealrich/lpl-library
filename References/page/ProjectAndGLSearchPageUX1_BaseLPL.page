ProjectAndGLSearchPageUX1 is a Page
    title is "TransactionsAndBalances"
    implements MenuSearch
    is report for Projects
        category is "TransactionsAndBalances"

    TransactionsAndBalances is a DashBoardPanel
        title is "Searches"
        use 12 column grid
        TransactionsAndBalances is a Pane
            Layout
                three column distributed
                    column1
                        large menu
                            page is GLTransactionDetailPage
                                title is "TransactionDetail"
                                menu item name is TransactionDetail
                                description is "View_General_Ledger_detail_transactions_by_project,_accounting_entity,_accounting_unit,_account_and_journalize_group._Filter_by_project_specific_status_fields_including_billed,_recognized,_burden,_and_capitalize."
                                icon is icon.clipboard-list-azure
                        large menu
                            page is TransactionPage
                                title is "TransactionSummary"
                                menu item name is TransactionSummary
                                description is "View_summarized_General_Ledger_transactions_by_project,_accounting_entity,_accounting_unit,_account,_and_posting_date._Filter_by_system,_event,_and_date_ranges."
                                icon is icon.clipboard-list-azure
                        large menu
                            page is TotalsPage
                                title is "Totals"
                                menu item name is Totals
                                description is "View_General_Ledger_Total_balances_by_period_ending_date,_project,_and_scenario._Filter_by_dimensions_including_ledger,_accounting_entity,_account,_chart_sections,_system,_and_dates."
                                icon is icon.calculator-azure
                        large menu
                            page is GeneralLedgerTransactionInterfacePage
                                title is "TransactionInterface"
                                menu item name is TransactionInterface
                                description is "Create_and_process_project_interface_transactions."
                                icon is icon.calculator-azure
                    column2
                        large menu
                            page is GLCommitmentReport
                                title is "CommitmentsAndEncumbrances"
                                menu item name is CommitmentsAndEncumbrancesReport
                                description is "View_all_current_commitments_and_encumbrances_records_for_projects._Page_also_includes_commitments_and_encumbrances_document_and_history."
                                icon is icon.clipboard-list-azure
                        visible when (actor.context.FinanceEnterpriseGroup.GLCommitDocumentExists)
                            large menu
                                list is CommitmentEncumbranceVarianceResult.primary
                                    title is "CommitmentsAndEncumbrancesVarianceReport"
                                    menu item name is CommitmentsAndEncumbrancesVarianceReport
                                    description is "Run_the_report_action_to_view_commitments_and_encumbrances_variances."
                                    icon is icon.settings-azure
                        visible when (actor.context.FinanceEnterpriseGroup.GLCommitDocumentExists)
                            large menu
                                list is CommitmentEncumbrancePOVarianceResult.primary
                                    title is "PO_EncumbranceVarianceReport"
                                    menu item name is CommitmentsAndEncumbrancesPOVarianceReport
                                    description is "Run_the_report_action_to_view_PO_and_encumbrances_variances."
                                    icon is icon.settings-azure
                        large menu
                            list is Project.ProjectReportingFlatUX1List
                                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Distributions"
                                menu item name is Distributions
                                description is "View_subsystem_distributions_by_posting_project._Distribution_lists_include_project_requisitions,_purchase_orders,_contract_line,_payables_invoices_and_cash_ledger."
                                icon is icon.clipboard-list-azure
                        large menu
                            list is ProjectIndirectBurdenTransaction.AllProjectIndirectBurdenTransactionList
                                title is "BurdenTransactions"
                                menu item name is BurdenTransactions
                                description is "View_and_maintain_indirect_burden_transactions._Use_the_status_field_to_view_created_or_journalized_transactions."
                                icon is icon.clipboard-list-azure
                    column3
                        large menu
                            page is ProjectFESAllocatedPageUX1
                                title is "PendingFrontEndSplitTransactions"
                                menu item name is PendingFrontEndSplitTransactions
                                description is "View_front_end_split_pending_transactions_to_reconcile_front_end_split_commitment_balances."
                                icon is icon.clipboard-list-azure
                        large menu
                            list is GeneralLedgerTransaction.GeneralLedgerTransactionFESList
                                title is "FrontEndSplitJournals"
                                menu item name is FrontEndSplitJournals
                                description is "View_front_end_split_journals_to_reconcile_front_end_split_expenditures."
                                icon is icon.clipboard-list-azure
        LegacyData is a Pane
            Layout
                single column
                    large menu
                        list is LegacyAchisthdr.LegacyAchisthdrList
                            title is "LegacyAchisthdr"
                            menu item name is LegacyAchisthdr
                            description is "View_legacy_Achisthdr_records."
                            icon is icon.clipboard-list-emerald
                    large menu
                        list is LegacyActrans.LegacyActransList
                            title is "LegacyActrans"
                            menu item name is LegacyActrans
                            description is "View_legacy_Actrans_records."
                            icon is icon.clipboard-list-emerald

