    title is "AsyncSetup"
    implements MenuSearch

    Queues is a MultiPanePanel
        pane 1
            business class is AsyncQueueDefinition
                list is QueueDefinitionSimpleList
        pane 2
            business class is pane 1.AsyncQueueMapping set
                list is QueueMappingByQueueList

    QueueGroups is a MultiPanePanel
        pane 1
            business class is AsyncQueueGroup
                list is QueueGroupList
        pane 2
            business class is pane 1.AsyncQueueDefinition set
                list is QueueDefinitionSimpleList

    BackgroundGroups is a Panel
        business class is AsyncBackgroundGroup
            list is AsyncBackgroundGroupList

    SynchronizedGroups is a Panel
        business class is AsyncSynchronizedGroup
            list is AsyncSynchronizedGroupList

    MaintenanceRequests is a Panel
        business class is AsyncActionRequest
            list is MaintenanceRequestList

    UpgradeRequests is a Panel
        business class is AsyncActionRequest
            list is UpgradeRequestList

    Auditing is a Panel
        business class is AsyncAuditSetup
            list is AsyncAuditSetupList

    Schedule is a MultiListPanel
        business class is AsyncScheduleTemplate
            list is ScheduleTemplatesList
        business class is AsyncScheduleTemplate
            list is ScheduleExclusionsList
