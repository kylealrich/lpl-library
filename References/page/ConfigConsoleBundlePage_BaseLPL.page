ConfigConsoleBundlePage is a Page
    title is "ManageChanges"

    Tags is a Panel
        mouse over text is "SearchAndWorkWithConfigurationsByTag"
        business class is ConfigurationTag
            list is ConsoleList

    Bundles is a Panel
        mouse over text is "WorkWithBundleConfiguration"
        business class is ConfigBundleDefinition
            list is ConsoleList

    LocalSnapshots is a Panel
        title is "LocalSnapshots"
        mouse over text is "WorkWithSnapshotsForExport"
        business class is ConfigBundleSnapshot
            list is ExportConsoleList
                title is "LocalSnapshots"

    ImportedSnapshots is a Panel
        title is "ImportedSnapshots"
        mouse over text is "WorkWithImportedSnapshots"
        business class is ConfigBundleSnapshot
            list is ImportConsoleList
                title is "ImportedSnapshots"

    AppliedSnapshots is a Panel
        title is "AppliedSnapshots"
        mouse over text is "HistoricalViewOfAppliedSnapshots"
        business class is ConfigBundleSnapshot
            list is AppliedConsoleList
                title is "AppliedSnapshots"

