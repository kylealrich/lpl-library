ProjectUtilitiesPage is a Page
    title is "ProjectUtilities"

    ProjectUtilities is a Panel
        title is "ProjectUtilities"
        Layout
            three column distributed
                column1
                    large menu
                        action is RebuildProjectShadow.ScheduleRebuildProjectShadow
                            title is "RebuildProjectShadow"
                            menu item name is RebuildProjectShadow
                            description is "Run_this_action_to_rebuild_the_project_shadow_file,_which_contain_relations_between_summary_and_posting_elements_in_hierarchical_structures."
                            icon is icon.cycle-settings-azure
                    large menu
                        action is ProjectContract.RecalculateFundingSourceFESExpenditures
                            title is "RecalculateFundingSourceFESExpenditures"
                            menu item name is RecalculateFundingSourceFESExpenditures
                            description is "Run_this_action_to_recalculate_FES_commitment_and_FES_expenditure_balances_that_are_stored_on_project_contract_funding_sources."
                            icon is icon.cycle-settings-azure
                    large menu
                        action is ProjectEmployee.ConvertLaborFTE
                            title is "ConvertEmployeeLaborFTE"
                            menu item name is ConvertEmployeeLaborFTE
                            description is "Run_this_action_to_update_project_employee_FTE_from_the_log_file._FTE_is_used_when_calculating_salary_caps_for_grant_labor_distribution."
                            icon is icon.cycle-settings-azure
                    large menu
                        action is ProjectAssignmentLabor.CreateProjectEmployeeLaborPeriod
                            title is "CreateProjectEmployeeLaborPeriod"
                            menu item name is CreateProjectEmployeeLaborPeriod
                            description is "Run_this_action_to_create_project_employee_labor_period."
                            icon is icon.cycle-settings-azure
                column2
                    large menu
                        action is Project.UpdateFinancialCubes
                            title is "UpdateAnalyticCube"
                            menu item name is UpdateAnalyticCube
                            description is "Run_this_action_to_refresh_existing_cube_data_or_to_reload_cube_data_for_both_finance_enterprise_group_and_general_ledger_total_cubes._Cube_data_is_used_for_project_reports_and_project_dimension_analysis."
                            icon is icon.cycle-settings-azure
                    large menu
                        action is Project.RecalculateBillingAndRevenueTotals
                            title is "RecalculateProjectBillingAndRevenueTotals"
                            menu item name is RecalculateProjectBillingAndRevenueTotals
                            description is "Run_this_action_to_recalculate_billing_and_revenue_totals_that_are_maintained_on_the_posting_projects."
                            icon is icon.cycle-settings-azure
                    large menu
                        action is ProjectContract.ConvertDeferAndHoldFlag
                            title is "ConvertDeferAndHoldFlag"
                            menu item name is ConvertDeferAndHoldFlag
                            description is "Run_this_action_to_update_the_defer_and_hold_remainder_option_on_the_project_contract."
                            icon is icon.cycle-settings-azure
                    large menu
                        action is ProjectAssignmentLabor.RecalculateLaborTotals
                            title is "RecalculateLaborTotals"
                            menu item name is RecalculateLaborTotals
                            description is "Run_this_action_to_recalculate_labor_totals."
                            icon is icon.cycle-settings-azure
                column3
                    large menu
                        action is ProjectContract.RebuildContractLTDBalances
                            title is "RebuildContractLTDBalances"
                            menu item name is RebuildContractLTDBalances
                            description is "Run_this_action_to_rebuild_LTD_invoiced_and_recognized_revenue_balance_amounts_for_project_contracts."
                            icon is icon.cycle-settings-azure
                    large menu
                        action is ProjectEmployee.UpdateEmployeeImages
                            title is "UpdateEmployeeImages"
                            menu item name is UpdateEmployeeImages
                            description is "Run_this_action_to_update_the_employee_image_from_the_GHR_employee_to_the_project_employee."
                            icon is icon.cycle-settings-azure
                    large menu
                        action is ProjectContractFee.CreateNewFeeRateOrPercent
                            title is "CreateNewFeeRateOrPercent"
                            menu item name is CreateNewFeeRateOrPercent
                            description is "Run_this_action_to_create_a_new_project_contract_fee_rate_or_percent."
                            icon is icon.cycle-settings-azure
                    large menu
                        action is ProjectIndirectBurdenTransaction.GenerateProjectIndirectBurdenJournalizeGroups
                            title is "GenerateProjectIndirectBurdenJournalizeGroupLinks"
                            menu item name is GenerateProjectIndirectBurdenJournalizeGroups
                            description is "Run_this_action_to_generate_journalize_group_links_for_previously_journalized_indirect_burden_transactions."
                            icon is icon.cycle-settings-azure

