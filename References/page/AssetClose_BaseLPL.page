AssetClose is a Page
    title is "CloseProcessing"
    implements MenuSearch

    Dashboard is a Panel
        business class is AssetCompany
            list is CloseDashboard

    AnnualDepreciation is a Panel
        business class is AssetBook
            list is AssetDepreciationOverride

    ActionsPanel is a Panel
        title is "RunProcesses"
        Layout
            four column distributed
                column1
                    large menu
                        action is ReleaseAssets.ReleaseAssets
                            title is "ReleaseAssets"
                            menu item name is ReleaseAssets
                            description is "Assets_must_be_released_before_a_period_can_be_closed"
                            icon is icon.check-azure
                            Search Terms
                                    "ReleaseAssets"
                        action is InsuranceAndReplacementCalculation.InsuranceAndReplacementCalculation
                            title is "InsuranceAndReplacementValues"
                            menu item name is InsuranceAndReplacementCalculation
                            description is "Calculate_insurance_or_replacement_values_for_selected_assets"
                            icon is icon.check-azure
                            Search Terms
                                    "InsuranceAndReplacementValues"
                column2
                    large menu
                        action is AssetDepreciationCalculationAndList.AssetDepreciationCalculationAndList
                            title is "CalculateDepreciation"
                            menu item name is CalculateDepreciation
                            description is "Calculate_and_review_current_period_depreciation"
                            icon is icon.check-azure
                            Search Terms
                                    "CalculateDepreciation"
                                    "DepreciationCalculation"
                        action is AssetPeriodClose.AssetPeriodClose
                            title is "ClosePeriod"
                            menu item name is AssetPeriodClose
                            icon is icon.clipboard-list-azure
                            Search Terms
                                    "AssetClose"
                                    "AssetPeriodClose"
                column3
                    large menu
                        action is DepreciationRecalculation.DepreciationRecalculation
                            title is "RecalculateDepreciation"
                            menu item name is DepreciationRecalculation
                            description is "Recalculate_and_review_updated_depreciation_values"
                            icon is icon.check-azure
                            Search Terms
                                    "RecalculateDepreciation"
                                    "DepreciationRecalculation"
                        action is AssetShortYearClose.AssetShortYearClose
                            title is "ShortYearClose"
                            menu item name is ShortYearClose
                            icon is icon.clipboard-list-azure
                            Search Terms
                                    "ShortYearClose"
                                    "AssetShortYearClose"
                column4
                    large menu
                        action is AssetInventoryUpdate.AssetInventoryUpdate
                            title is "AssetInventoryUpdate"
                            menu item name is AssetInventoryUpdate
                            icon is icon.check-azure
                            Search Terms
                                    "AssetInventory"
                                    "AssetInventoryUpdate"
                        action is AssetYearEndClose.AssetYearEndClose
                            title is "CloseYear"
                            menu item name is AssetYearEndClose
                            icon is icon.clipboard-list-azure
                            Search Terms
                                    "AssetYearEnd"
                                    "YearEndClose"
                                    "CloseYear"
                                    "AssetYearEndClose"

