AsyncPage is a Page
    title is "AsyncAdministrator"
    implements MenuSearch

    AtAGlance is a MultiPanePanel
        pane 1
            business class is AsyncActionTrigger
                list is QueuedAsyncActionTriggerList
        pane 3
            business class is AsyncActionTrigger
                list is ActiveAsyncActionTriggerList

    QueuedNow is a Panel
        business class is AsyncQueueDefinition
            list is QueuedQueueDefinitionWithTriggersList

    ActiveNow is a Panel
        business class is AsyncQueueDefinition
            list is ActiveQueueDefinitionWithTriggersList

    Failed is a Panel
        business class is AsyncActionTrigger
            list is FailedAsyncActionTriggerList

    AllQueues is a Panel
        business class is AsyncQueueDefinition
            list is QueueDefinitionWithTriggersList

    RequestsPendingScheduling is a Panel
        business class is AsyncActionRequest
            list is AsyncActionRequestScheduledList

    RecurringRequests is a Panel
        business class is AsyncActionRequest
            list is AsyncActionRequestRecurringList

    AllRequests is a Panel
        business class is AsyncActionRequest
            list is AsyncActionRequestList
            Actions
                action is Create
                action is Update
                action is Delete
                action is search

    Misfires is a Panel
        business class is AsyncActionMisfire
            list is AsyncActionMisfireList

    Blocked is a Panel
        business class is AsyncBlocklist
            list is BlockedActions

    SystemStatus is a MultiListPanel
        business class is AsyncStatus
            list is AsyncStatus
            show 15 lines
        business class is AsyncQueueDefinition
            list is ActiveQueuedChart


