MonthEndUX1 is a Page
    title is "PeriodEndClose"
    implements MenuSearch

    Process is a DashBoardPanel
        use 12 column grid
        PeriodEnd is a Pane
            span 12 columns
            Layout
                large menu
                    three column distributed
                        action is RecurringInvoiceUpdate.RecurringInvoiceUpdate
                            title is "Step1_-UpdateRecurringInvoices"
                            menu item name is UpdateRecurringInvoices
                            icon is icon.calendar-emerald
                        action is MatchDiscountPosting.MatchDiscountPosting
                            title is "Step2_-PostMatchDiscounts"
                            menu item name is PostMatchDiscounts
                            description is "Accrue discounts that were taken during the match process."
                            icon is icon.calendar-emerald
                        action is PayablesInvoiceDistribution.JournalizeDistributions
                            title is "Step3*_-Journalize"
                            menu item name is Journalize
                            icon is icon.calendar-emerald
                    three column distributed
                        action is UnrealizedGainAndLossCalculation.UnrealizedGainAndLossCalculation
                            title is "Step4_-CalculatedUnrealizedGains_andLosses"
                            menu item name is CalculatedUnrealizedGainsAndLosses
                            description is "Calculate and create unrealized gain and loss transactions for payment accrual and accounts."
                            icon is icon.calendar-emerald
                        action is InvoiceAccrualReconciliationReport.InvoiceAccrualReconciliationReport
                            title is "Step5*_-InvoiceAccrualReconciliation"
                            menu item name is InvoiceAccrualReconciliation
                            description is "View invoice transactions and analyze the balance of your accrual accounts."
                            icon is icon.calendar-emerald
                        action is PayablesInvoice.PeriodClosePreprocessorToUpdatePostDate
                            title is "Step6_-PeriodClosePreprocessorToUpdatePostDate"
                            menu item name is PeriodClosePreprocessorToUpdatePostDate
                            description is "Run before closing the period to move to the next period any Unreleased invoices in the period being closed."
                            icon is icon.calendar-emerald
                    three column distributed
                        action is PayablesPeriodClose.PayablesPeriodClose
                            title is "Step7*_-ClosePayables"
                            menu item name is ClosePayables
                            description is "Run at the end of each accounting period for companies that have system control that is selected in Global Ledger."
                            icon is icon.calendar-emerald
                        action is PayablesInvoice.PeriodEndInvoiceExpenseAccrual
                            title is "PeriodEndInvoiceExpenseAccrual"
                            menu item name is PeriodEndInvoiceExpenseAccrual
                            description is "Accrues expenses for invoices in Pending Approval status."
                            icon is icon.calendar-emerald
                        action is PayablesCompany.PayablesHistoryPurge
                            title is "PayablesHistoryPurge"
                            menu item name is PayablesHistoryPurge
                            icon is icon.calendar-emerald
                    three column distributed
                        blank line
                        blank line
                        text of "*_-_Required"
        Reports is a Pane
            span 12 columns
            Layout
                three column distributed
                    large menu
                        action is InvoiceProcessingStatistics.InvoiceProcessingStatistics
                            title is "InvoiceProcessingStatistics"
                            menu item name is InvoiceProcessingStatistics
                            description is "Track and report invoice, match, and canceled invoice statistics."
                            icon is icon.document-text-ruby
                        action is BankAccountPaymentHistory.BankAccountPaymentHistory
                            title is "BankAccountPaymentHistory"
                            menu item name is BankAccountPaymentHistory
                            description is "View manual and system-generated payments for a pay group."
                            icon is icon.document-text-ruby
                        action is VendorStatements.VendorStatements
                            title is "CreateVendorStatements"
                            menu item name is CreateVendorStatements
                            description is "Create statements to send to your vendors"
                            icon is icon.document-text-ruby
                        action is VendorBalanceYearEnd.VendorBalanceYearEnd
                            title is "VendorBalanceYearEnd"
                            menu item name is VendorBalanceYearEnd
                            description is "Run at the end of the fiscal year to establish new period end dates for a vendor group and transfer current year balances."
                            icon is icon.document-text-ruby
                        action is InvoiceDataArchival.InvoiceDataArchival
                            title is "InvoiceDataArchival"
                            menu item name is InvoiceDataArchival
                            icon is icon.document-text-ruby
                        action is UnmatchedInvoiceList.UnmatchedInvoiceList
                            title is "UnmatchedInvoices"
                            menu item name is UnmatchedInvoices
                            description is "Generate a list of unmatched invoices for a specific company."
                            icon is icon.document-text-ruby
                        action is IntrastatDataReport.IntrastatDataReport
                            title is "IntrastatDataReport"
                            menu item name is IntrastatDataReport
                            description is "Edit Intrastat data and create, re-create, or reprint an Intrastat data report for a selected period."
                            icon is icon.document-text-ruby
                        action is FinancialReportingUpdate.FinancialReportingUpdate
                            title is "FinancialReportingUpdate"
                            menu item name is FinancialReportingUpdate
                            description is "Provide adjustments or difference postings for Stock Ledger or Purchase Journal Interface."
                            icon is icon.document-text-ruby
        Journal is a Pane
            span 6 columns
            Layout
                single column distributed
                    large menu
                        action is JournalFileCreate.JournalFileCreate
                            title is "Step1_-CreateJournalFiles"
                            menu item name is CreateJournalFiles
                            description is "Before running the purchasing and sales journal report, you must create journal files to create a tax file."
                            icon is icon.target-azure
                        action is PurchasingAndSalesJournal.PurchasingAndSalesJournal
                            title is "Step2_-Purchasing_andSalesJournal"
                            menu item name is PurchasingAndSalesJournal
                            description is "Generate a report of transaction records and their distribution details."
                            icon is icon.target-azure
                        action is VatPurchaseSaleTransaction.PurgeVatPurchaseSaleTransaction
                            title is "Step3_-PurgeJournalDetails"
                            menu item name is PurgeJournalDetails
                            description is "Purge the journal data in order to report on different transactions."
                            icon is icon.target-azure
        ForeignTradeAndPayment is a Pane
            span 3 columns
            Layout
                two column distributed
                    large menu
                        action is ForeignTradeAndPaymentsReport.RunForeignTradeAndPaymentsReport
                            title is "RunReport"
                            menu item name is RunForeignTradeAndPaymentsReport
                            icon is icon.check-turquoise
                            Search Terms
                                    "RunForeignTradeAndPaymentsReport"
                                    "RunReport"
                        page is ForeignTradeAndPaymentReports
                            title is "Reports"
                            menu item name is ForeignTradeAndPaymentReports
                            icon is icon.clipboard-list-turquoise
                            Search Terms
                                    "ForeignTradeAndPaymentReports"
                                    "Reports"
        ClaimsAndLiabilities is a Pane
            span 3 columns
            Layout
                two column distributed
                    large menu
                        action is ClaimsAndLiabilitiesHeader.CreateClaimsAndLiabilitiesReport
                            title is "RunReport"
                            menu item name is RunClaimsAndLiabilitiesReport
                            icon is icon.check-amber
                            Search Terms
                                    "RunClaimsAndLiabilitiesReport"
                                    "RunReport"
                        page is ClaimsAndLiabilitiesPage
                            title is "Reports"
                            menu item name is ClaimsAndLiabilitiesReports
                            icon is icon.clipboard-list-amber
                            Search Terms
                                    "ClaimsAndLiabilitiesReports"
                                    "Reports"

    CloseManagement is a MultiPanePanel
        title is "Summary"
        mouse over text is "ViewTheStatusOfTheCloseByPeriod"
        pane 1
            business class is ClosePeriod
                list is TheCloseStatusList
        pane 2
            business class is pane 1.AllSummaryTasksRel
                list is TheCloseProcessTreeViewList
        pane 3
            business class is pane 2.ClosePeriodTaskFlatSummaryRel
                list is ClosePeriodTaskFlatList

    JournalizePreview is a Panel
        business class is PayablesInvoiceDistribution
            list is JournalizePreviewList

    JournalizeResults is a Panel
        business class is JournalizeGroupResult
            list is JournalizeGroupResults

