        business class is Contract
            list is ActiveExpiringAndExpiredContracts

    ContractsWithErrors is a Panel
        business class is Contract
            list is ContractsWithErrors

    MilestonesNotCompleted is a Panel
        business class is ContractMilestone
            list is ContractMilestonesForContractGroup

    DeliverablesNotClosed is a Panel
