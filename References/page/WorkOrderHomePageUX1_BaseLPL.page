WorkOrderHomePageUX1 is a Page
    title is "Overview"
    implements MenuSearch

    Dashboard is a DashBoardPanel
        use 12 column grid
        UnreleasedWorkOrder is a Pane
            span 4 columns
            title is "Unreleased"
            business class is WorkOrder
                list is UnreleasedWorkOrderCardList
                    overview navigation is WorkOrderNumberCardNav
        PickingFeedback is a Pane
            span 4 columns
            title is "Picking_Feedback_Not_Started"
            business class is WorkOrder
                list is PickingFeedbackCardList
                    overview navigation is WorkOrderNumberCardNav
        ProductionFeedback is a Pane
            span 4 columns
            title is "Production_Feedback_Not_Started"
            business class is WorkOrder
                list is ProductionFeedbackCardList
                    overview navigation is WorkOrderNumberCardNav
        ScheduleHeaders is a Pane
            span 4 columns
            business class is WorkOrderDataScheduleHeader
                list is ScheduledItems
                    overview navigation is ScheduledItemsCardNav
        ForecastDemand is a Pane
            span 4 columns
            business class is WorkOrderForecastDemand
                list is WorkOrderForecastDemandCardList
                    overview navigation is WorkOrderForecastDemandCardNav
        CancelledWorkOrder is a Pane
            span 4 columns
            title is "Cancelled"
            business class is WorkOrder
                list is CancelledWorkOrderCardList
                    overview navigation is WorkOrderNumberCardNav

