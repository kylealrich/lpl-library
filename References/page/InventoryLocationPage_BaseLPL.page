InventoryLocationPage is a Page
    show panel control on left

    AllInventoryLocations is a Panel
        business class is InventoryLocation
            list is InventoryLocationList

    StandardInventoryLocations is a Panel
        business class is InventoryLocation
            list is StandardLocationList

    ParLocations is a Panel
        business class is InventoryLocation
            list is ParLocations

    CartLocations is a Panel
        business class is InventoryLocation
            list is CartLocations

    BinAuditHistory is a Panel
        business class is Bin
            list is BinAuditList

    InventoryLocationAuditHistory is a Panel
        visible when (not actor.context.FinanceEnterpriseGroup.ColumnarInventoryLocationAuditInUse)
        business class is InventoryLocation
            list is InventoryLocationAuditList

    InventoryLocationAuditHistoryColumnar is a Panel
        title is "InventoryLocationAuditHistory"
        visible when (actor.context.FinanceEnterpriseGroup.ColumnarInventoryLocationAuditInUse)
        business class is InventoryLocation
            list is ColumnarInventoryLocationAuditList
                title is "InventoryLocationAuditHistory"

    InventoryLocationAccountAuditHistory is a Panel
        business class is GeneralLedgerCategory
            list is GLCategoryAuditList

