        use 12 column grid
        Create is a Pane
            span 3 columns
            title is "ImportantLinks"
            Layout
                single column
                    large menu
                        action is PayablesInvoice.Unreleased.CreateInvoice
                            title is "Create_anInvoice"
                            menu item name is CreateAnInvoice
                            description is "Create_a_new_invoice_using_the_full_invoice_entry_form."
                            icon is icon.document-finance-emerald
                        action is PayablesInvoice.Unreleased.CreateAndRelease
                            title is "Create_andRelease"
                            menu item name is CreateAndRelease
                            description is "Create_and_automatically_release_an_invoice."
                            icon is icon.document-finance-emerald
                        page is APClerkInvoicesPageUX1
                            title is "MyInvoices"
                            menu item name is MyInvoices
                            description is "View_invoices_assigned_to_you."
                            icon is icon.document-finance-emerald
        ByStatus is a Pane
            span 3 columns
            title is "InProcess_-_ByStatus"
            business class is PayablesInvoice
                list is APManagerInvoicesByStatus
                    display InvoiceStatusCubePieChartList
                    overview navigation is APManagerInvoicesByStatusCubeLink
        Exceptions is a Pane
            span 6 columns
            Layout
                two column distributed
                    large menu
                        page is InvoiceExceptionsUX1.OnHold
                            title is "OnHold"
                            menu item name is OnHold
                            description is "Review_payments_that_are_'On_Hold'_and_prevented_from_being_released."
                            icon is icon.warning-amber
                        page is InvoiceExceptionsUX1.OutOfBalance
                            title is "OutOfBalance"
                            menu item name is OutOfBalance
                            description is "ViewInvoices_where_the_payment,_distribution,_and/or_tax_amounts_are_out_of_balance."
                            icon is icon.warning-amber
                        page is InvoiceExceptionsUX1.ProcessingInBackground
                            title is "Processing_in_Background"
                            menu item name is ProcessingInBackground
                            description is "Invoices_processing_in_the_background_will_not_be_paid_or_matched."
                            icon is icon.warning-amber
                        page is InvoiceExceptionsUX1.Commitments
                            title is "Commitments"
                            menu item name is Commitments
                            description is "View_invoices_with_open_commitments."
                            icon is icon.warning-amber
