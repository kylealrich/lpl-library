SecurityAdministrationPage is a Page
    title is "SecurityAdministration"

    Overview is a DashBoardPanel
        use 12 column grid
        NavigationPoliciesInUseNeedingReview is a Pane
            span 6 columns
            business class is MenuSecurity
                list is ImpactedPolicyDashboardList
                    overview navigation is PoliciesNeedingReview
                        label is "ViewAll"
        DeliveredReportPoliciesInUseNeedingReview is a Pane
            span 6 columns
            business class is ReportSecurity
                list is ImpactedPolicyDashboardList
                    overview navigation is PoliciesNeedingReview
                        label is "ViewAll"
        ConfigurationActivityLast14Days is a Pane
            span 6 columns
            business class is SecurityClass
                list is RecentConfigurationsList
                    overview navigation is SecurityChangeLogList
                        label is "ViewFullChangeLog"
        InactiveConfigurations is a Pane
            span 6 columns
            business class is SecurityClass
                list is InactiveConfigurationsList
                    overview navigation is ManageInvalidConfigurations
                        label is "ManageInvalid"
        ActorRoleActivityLast14Days is a Pane
            span 6 columns
            business class is ActorRole
                list is RecentActorRoleChangesList
                    overview navigation is ActorRoleChangeLogList
                        label is "ViewFullChangeLog"
        RoleSecurityClassActivityLast14Days is a Pane
            span 6 columns
            business class is RoleSecurityClass
                list is RecentRoleSecurityClassChangesList
                    overview navigation is RoleSecurityChangeLogList
                        label is "ViewFullChangeLog"
        MissingCopiedFromSecurityClasses is a Pane
            span 6 columns
            business class is SecurityClass
                list is MissingCopiedFrom
                    overview navigation is MissingCopiedFromSecurityClasses
                        label is "ManageMissing"
        MissingExtendFromSecurityClasses is a Pane
            span 6 columns
            business class is SecurityClass
                list is MissingExtendFrom
                    overview navigation is MissingExtendFromSecurityClasses
                        label is "ManageMissing"

