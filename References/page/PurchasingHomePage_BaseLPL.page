PurchasingHomePage is a Page
    title is "PurchasingManager"

    PurchasingManager is a MultiPanePanel
        pane 1
            business class is PurchaseOrder
                list is UnreleasedPurchaseOrders
        pane 2
            business class is PurchaseOrderLine
                list is POLinesOverdueReceipts
        pane 3
            business class is PurchaseOrder
                list is UnapprovedPurchaseOrders
        pane 4
            business class is Contract
                list is ActiveExpiringAndExpiredContracts

    BuyerMessages is a MultiPanePanel
        pane 1
            business class is PurchaseOrderBuyerMessage
                list is UnreleasedBuyerMessagesReceiving
        pane 2
            business class is MatchInvoiceMessage
                list is OpenInvoiceMessages

    AutoMatchResults is a Panel
        business class is AutoMatchResult
            list is PurchasingManagerAutoMatchResults

