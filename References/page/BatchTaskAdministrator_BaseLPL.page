BatchTaskAdministrator is a Page
    title is "JobConsole"

    Jobs is a Panel
        business class is BatchTask
            list is AllJobsAndJobStreams

    JobStatus is a MultiPanePanel
        pane 1
            business class is AsyncActionInvocationState
                list is GlobalAsyncActionInvocationStateList
        pane 3
            business class is pane 1.GlobalTopLevelAsyncActionInvocationsInStateRel
                list is GlobalAsyncActionInvocationCardViewList

    MyJobs is a Panel
        business class is BatchTask
            list is BatchTasksCreatedByActor

    MyJobStatus is a MultiPanePanel
        pane 1
            business class is AsyncActionInvocationState
                list is ActorAsyncActionInvocationStateList
        pane 3
            business class is pane 1.ActorTopLevelAsyncActionInvocationsInStateRel
                list is ActorAsyncActionInvocationCardViewList

