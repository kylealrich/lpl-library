ContractPricingManagerHomePage is a Page
    title is "ContractInformation"

    Home is a DashBoardPanel
        show 4 columns
        QuickLinks is a Pane
            double width
            Layout
                single column
                    small menu
                        page is ContractPricingProcessingPage
                            title is "ManageContracts"
                            menu item name is ManageContracts
                            icon is icon.document-text-amber
                        page is ContractSearchPage
                            title is "SearchContracts"
                            menu item name is SearchContracts
                            icon is icon.CSFSupplyManagementViewer
                        page is SupplierPage
                            title is "ManageSuppliers"
                            menu item name is ManageSupplier
                            icon is icon.hcm-ruby
        ActiveExpiringAndExpired is a Pane
            double width
            business class is Contract
                list is ActiveExpiringWidget
                    title is "Active_-Expiring_andExpired"
                    overview navigation is ActiveExpiringAndExpiredContractsNav
        ContractsWithErrors is a Pane
            double width
            business class is Contract
                list is ContractsWithErrorsCardview
                    title is "Contracts_withErrors"
                    overview navigation is WithErrorsNav
        OpenServiceInvoiceMessages is a Pane
            double width
            business class is MatchInvoiceMessage
                list is OpenContractInvoiceMessagesWidget
                    title is "OpenServiceInvoiceMessages"
                    overview navigation is OpenContractInvoiceMessagesNav

    SearchContracts is a Panel
        business class is Contract
            list is CMManagerDashboard

    SearchContractLines is a Panel
        business class is ContractLine
            list is ItemSearch

