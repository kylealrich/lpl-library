CloseAdministrationUX1 is a Page
    title is "<actor.context.FinanceEnterpriseGroup.CloseAdministrationTitle>"
    implements MenuSearch

    SetupAndProcess is a DashBoardPanel
        title is "SetupAndProcess"
        use 12 column grid
        GeneralSetup is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        list is CloseManagementGroup.CloseManagementGroupList
                            title is "CloseManagementGroup"
                            menu item name is CloseManagementGroup
                            icon is icon.person-global-amethyst
                        list is SystemCalendar.SystemCalendars
                            title is "SystemCalendars"
                            menu item name is SystemCalendars
                            icon is icon.person-global-amethyst
                        list is ActionReason.ActionReasonList
                            title is "ActionReasons"
                            menu item name is ActionReasons
                            icon is icon.person-global-amethyst
        Resources is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        page is FinanceResourceAdmin
                            title is "FinanceResources"
                            menu item name is FinanceResources
                            icon is icon.person-finance-emerald
                        list is FinanceTeam.BuildTeamList
                            title is "FinanceTeams"
                            menu item name is FinanceTeams
                            icon is icon.person-finance-emerald
        CloseSetup is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        page is CloseTaskTypeSetupPage
                            title is "TaskTypes"
                            menu item name is TaskTypes
                            icon is icon.clipboard-check-ruby
                        list is ApprovalCode.ApprovalCodes
                            title is "ApprovalCodes"
                            menu item name is ApprovalCodes
                            icon is icon.clipboard-check-ruby
                        list is SampleDocumentTemplate.SampleSpreadsheetsForCloseTasks
                            title is "SampleCloseTaskInterface"
                            menu item name is SampleCloseTaskInterface
                            icon is icon.clipboard-check-ruby
        Process is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        page is ProcessPage.ManageProcess
                            title is "ManageProcess"
                            menu item name is ManageProcess
                            icon is icon.CSFOrderEntry
                        page is ProcessPage.ViewSchedule
                            title is "ViewSchedule"
                            menu item name is ViewSchedule
                            icon is icon.CSFOrderEntry
                        page is ProcessPage.OpenSchedules
                            title is "OpenSchedules"
                            menu item name is OpenSchedules
                            icon is icon.CSFOrderEntry
                        page is ProcessPage.PastSchedules
                            title is "PastSchedules"
                            menu item name is PastSchedules
                            icon is icon.CSFOrderEntry
                        page is ProcessPage.ManageOneTimeTasks
                            title is "ManageOneTimeTasks"
                            menu item name is ManageOneTimeTasks
                            icon is icon.CSFOrderEntry

    Utilities is a DashBoardPanel
        title is "Utilities"
        use 12 column grid
        GenerateNotifications is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        action is ClosePeriod.GeneralOverdueNotifications
                            title is "GeneralOverdueNotifications"
                            menu item name is GeneralOverdueNotifications
                            icon is icon.chat-emerald
                        action is ClosePeriod.TimeSensitiveOverdueNotifications
                            title is "TimeSensitiveOverdueNotifications"
                            menu item name is TimeSensitiveOverdueNotifications
                            icon is icon.chat-emerald
                        action is ClosePeriod.ExternalEmailNotifications
                            title is "ExternalEmails"
                            menu item name is ExternalEmails
                            icon is icon.chat-emerald
        CubeRefresh is a Pane
            span 4 columns
            title is "Cube"
            Layout
                single column
                    text of "Status"
                        header4
                    visible when (actor.context.CloseManagementGroup.CubeOnlineMode)
                        text of "Cube_is_online"
                            bold
                            color of green
                    visible when (actor.context.CloseManagementGroup.CubeOfflineMode)
                        text of "Cube_is_offline"
                            bold
                            color of red
                    text of "Close_Period_Task_Cube"
                        header4
                    paragraph
                        text of "LastRefreshDate"
                        text of "<actor.context.CloseManagementGroup.ClosePeriodTaskAnalyticCubeRel.LastRefreshDate>"
                            bold
                    paragraph
                        text of "LastReloadDate"
                        text of "<actor.context.CloseManagementGroup.ClosePeriodTaskAnalyticCubeRel.LastReloadDate>"
                            bold
                    action is CloseManagementGroup.UpdateClosePeriodTaskCube
                        title is "RefreshCube"
                        menu item name is RefreshCube
                        description is "Will_refresh_the_cube"
                        icon is icon.outline-process-ruby
                    action is ClosePeriod.LoadCloseMgmtCubeFile
                        title is "LoadCube"
                        menu item name is LoadCube
                        description is "Will_load_the_cube"
                        icon is icon.outline-process-ruby
        Others is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        action is ClosePeriod.CreatePeriodTaskApprovalResourceRecords
                            title is "CreatePeriodTaskApprovalResourceRecords"
                            menu item name is CreatePeriodTaskApprovalResourceRecords
                            icon is icon.createContract
                        visible when (actor.context.FinanceEnterpriseGroup.CloseManagementGroup.HasEmptyCalendar)
                            action is CloseManagementGroup.UpdateCalendar
                                title is "PopulateNewCalendarField"
                                menu item name is PopulateNewCalendarField
                                icon is icon.outline-process-ruby

