MyActionsPage is a Page
    title is "MyActions"
    implements MenuSearch

    RequestActions is a MultiListPanel
        mouse over text is "SeeYourOpenRequestActionsOrReviewAllRequestActions"
        title is "ActionRequests"
        Search Terms
                "Pending"
                "Actions"
                "RequestAction"
                "ActionRequest"
                "Requests"
        business class is ActionRequest
            list is MyOpenRequestActions
        business class is ActionRequest
            list is MyNonOpenRequestActions
            show 15 lines


    ScheduledActions is a Panel
        business class is AsyncActionRequest
            list is MyAsyncActionRequestList

    DBImportRequests is a Panel
        business class is DBImportRequest
            list is MyDBImportRequests

