ProjectReportingAll is a Page
    title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Reports"
    show panel control on left

    ProjectObligations is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Obligations"
        business class is ProjectReportingSettingsAll
            list is ProjectReportingSettingsProjectObligationsList
                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Obligations"

    ProjectVariance is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Variance"
        business class is ProjectReportingSettingsAll
            list is ProjectReportingSettingsProjectVarianceList
                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Variance"

    ProjectPeriodComparison is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>PeriodComparison"
        business class is ProjectReportingSettingsAll
            list is ProjectReportingSettingsProjectPeriodComparisonList
                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>PeriodComparison"

    ProjectContracts is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Contracts"
        business class is ProjectContract
            list is ProjectContracts
                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Contracts"

    ContractPercentComplete is a Panel
        business class is ProjectReportingSettingsAll
            list is ProjectReportingSettingsContractPercentCompleteFlatList

    ContractGrossMargin is a Panel
        business class is ProjectReportingSettingsAll
            list is ProjectReportingSettingsContractGrossMarginFlatList

    Requisitions is a MultiPanePanel
        pane 1
            business class is ProjectStructure.PostingProjectsInEnterpriseStructureRel
                list is ProjectReportingFlatList
                Context
                    FinanceEnterpriseGroup
                    ProjectStructure
        pane 2
            business class is pane 1.RequisitionLineDistributionsAllRel
                list is ProjectRequisitionLineDistributions

    PurchaseOrders is a MultiPanePanel
        pane 1
            business class is ProjectStructure.PostingProjectsInEnterpriseStructureRel
                list is ProjectReportingFlatList
                Context
                    FinanceEnterpriseGroup
                    ProjectStructure
        pane 2
            business class is pane 1.PurchaseOrderLineDistributionsAllRel
                list is PurchaseOrderLinesForAProject

    PayablesInvoices is a MultiPanePanel
        pane 1
            business class is ProjectStructure.PostingProjectsInEnterpriseStructureRel
                list is ProjectReportingFlatList
                Context
                    FinanceEnterpriseGroup
                    ProjectStructure
        pane 2
            business class is pane 1.PayablesInvoiceDistributionsAllRel
                list is ProjectPayablesInvoiceDistributionList

    ContractLineDistributions is a MultiPanePanel
        title is "ContractLine"
        pane 1
            business class is ProjectStructure.PostingProjectsInEnterpriseStructureRel
                list is ProjectReportingFlatList
                Context
                    FinanceEnterpriseGroup
                    ProjectStructure
        pane 2
            business class is pane 1.ContractLineDistributionAllRel
                list is ProjectContractLineDistributions

    CashLedgerDistributions is a MultiPanePanel
        title is "CashLedger"
        pane 1
            business class is ProjectStructure.PostingProjectsInEnterpriseStructureRel
                list is ProjectReportingFlatList
                Context
                    FinanceEnterpriseGroup
                    ProjectStructure
        pane 2
            business class is pane 1.CashLedgerGLDistributionsRel
                list is ProjectCashLedgerGLDistributionList

    CommitmentsAndEncumbrances is a Panel
        business class is GLCommit
            list is ProjectCommitments

    CommitmentsAndEncumbrancesSearch is a Panel
        business class is GLCommit
            list is ProjectCommitmentsSearch

    ScheduleOfFederalAwards is a Panel
        business class is ProjectReportingSettingsAll
            list is ProjectReportingSettingsScheduleOfFederalAwardFlatList

