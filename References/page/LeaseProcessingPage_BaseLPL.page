LeaseProcessingPage is a Page
    title is "Processing"
    implements MenuSearch

    Processing is a DashBoardPanel
        title is "Processing"
        use 12 column grid
        LeaseProcessing is a Pane
            span 4 columns
            title is "Leases"
            Layout
                single column
                    small menu
                        action is CreateAPInvoicesForLeasePayments.CreateAPInvoicesForLeasePayments
                            title is "CreateInvoicesForPayments"
                            menu item name is CreateInvoicesForLeasePayments
                            icon is icon.check-azure
                        action is InterfaceLeasePaymentInvoicesToPayables.InterfaceLeasePaymentInvoicesToPayables
                            title is "InterfaceInvoicesToPayables"
                            menu item name is InterfaceInvoicesToPayables
                            icon is icon.check-azure
                        action is LeaseInterestCalculation.LeaseInterestCalculation
                            title is "CalculateInterest"
                            menu item name is InterestCalculation
                            icon is icon.check-azure
                        action is LeasePosting.LeasePosting
                            title is "PostLeases"
                            menu item name is LeasePosting
                            icon is icon.check-azure
                        action is LeasePeriodClose.LeasePeriodClose
                            title is "ClosePeriod"
                            menu item name is LeasePeriodClose
                            icon is icon.check-azure
        MassProcesses is a Pane
            span 4 columns
            title is "MassLeaseProcesses"
            Layout
                single column
                    small menu
                        action is MassLeasePaymentApproval.MassLeasePaymentApproval
                            title is "ApprovePayments"
                            menu item name is PaymentApproval
                            icon is icon.check-emerald
                        action is MassLeaseRelease.MassLeaseRelease
                            title is "Release"
                            menu item name is LeaseRelease
                            icon is icon.check-emerald
        SubleaseProcessing is a Pane
            span 4 columns
            title is "Subleases"
            Layout
                single column
                    small menu
                        action is SubleaseBillingScheduleDetail.CreateInvoices
                            title is "CreateInvoices"
                            menu item name is CreateSubleaseInvoices
                            icon is icon.check-ruby
                        action is SubleaseBillingScheduleDetail.PeriodProcessing
                            title is "PostSubleases"
                            menu item name is PeriodProcessing
                            icon is icon.check-ruby

    ViewJournalizedEntries is a Panel
        business class is JournalizeGroupResult
            list is JournalizeGroupResults

