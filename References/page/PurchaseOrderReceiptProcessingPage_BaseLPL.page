PurchaseOrderReceiptProcessingPage is a Page
    title is "ManagePurchaseOrderReceipts"
    show panel control on left

    Unreleased is a Panel
        business class is PurchaseOrderReceipt
            list is UnreleasedPurchaseOrderReceipts

    PendingTwoStepReceiving is a Panel
        business class is PurchaseOrderReceipt
            list is PendingTwoStepReceivingPurchaseOrderReceipts

    Released is a Panel
        business class is PurchaseOrderReceipt
            list is ReleasedPurchaseOrderReceipts

    ReadyToArchive is a Panel
        business class is PurchaseOrderReceipt
            list is ReadyToArchivePurchaseOrderReceipts

    DropShip is a Panel
        business class is PurchaseOrderReceipt
            list is DropShipPurchaseOrderReceipts

    ArchivedClosed is a Panel
        title is "Archived_/Closed"
        business class is PurchaseOrderReceipt
            list is ArchivedClosedPurchaseOrderReceipts
                title is "Archived_/Closed"

    All is a Panel
        business class is PurchaseOrderReceipt
            list is AllPurchaseOrderReceipts

    Search is a Panel
        business class is PurchaseOrderReceiptLine
            list is ReceiptSearchList

    AuditHistory is a Panel
        visible when (not actor.context.FinanceEnterpriseGroup.ColumnarPurchaseOrderReceiptAuditInUse)
        business class is PurchaseOrderReceipt
            list is PurchaseOrderReceiptAuditList

    AuditHistoryColumnar is a Panel
        title is "AuditHistory"
        visible when (actor.context.FinanceEnterpriseGroup.ColumnarPurchaseOrderReceiptAuditInUse)
        business class is PurchaseOrderReceipt
            list is ColumnarPurchaseOrderReceiptAuditList
                title is "AuditHistory"

    LineAuditHistory is a Panel
        visible when (not actor.context.FinanceEnterpriseGroup.ColumnarPurchaseOrderReceiptLineAuditInUse)
        business class is PurchaseOrderReceiptLine
            list is PurchaseOrderReceiptLineAuditList

    LineAuditHistoryColumnar is a Panel
        title is "LineAuditHistory"
        visible when (actor.context.FinanceEnterpriseGroup.ColumnarPurchaseOrderReceiptLineAuditInUse)
        business class is PurchaseOrderReceiptLine
            list is ColumnarPurchaseOrderReceiptLineAuditList
                title is "LineAuditHistory"

