ItemMaintenancePage is a Page
    title is "MaintainItems"

    Items is a Panel
        business class is Item
            list is ItemPrimaryDashboard

    ItemLocations is a Panel
        business class is ItemLocation
            list is ItemsForAnInventoryLocation

    ParItems is a Panel
        business class is ItemLocation
            list is ItemsAtParLocation

    KitItems is a Panel
        business class is KitItem
            list is KitItemList

    KitComponents is a Panel
        business class is KitComponent
            list is KitComponentUsage

    ItemAuditHistory is a Panel
        visible when (not actor.context.FinanceEnterpriseGroup.ColumnarItemAuditInUse)
        business class is Item
            list is ItemAuditList

    ItemAuditHistoryColumnar is a Panel
        title is "ItemAuditHistory"
        visible when (actor.context.FinanceEnterpriseGroup.ColumnarItemAuditInUse)
        business class is Item
            list is ColumnarItemAuditList
                title is "ItemAuditHistory"

    ItemUOMAuditHistory is a Panel
        business class is ItemUOM
            list is ItemUOMAuditList

    ItemLocationAuditHistory is a Panel
        visible when (not actor.context.FinanceEnterpriseGroup.ColumnarItemLocationAuditInUse)
        business class is ItemLocation
            list is ItemLocationAuditList

    ItemLocationAuditHistoryColumnar is a Panel
        title is "ItemLocationAuditHistory"
        visible when (actor.context.FinanceEnterpriseGroup.ColumnarItemLocationAuditInUse)
        business class is ItemLocation
            list is ColumnarItemLocationAuditList
                title is "ItemLocationAuditHistory"

