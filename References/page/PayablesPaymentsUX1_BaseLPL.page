PayablesPaymentsUX1 is a Page
    title is "Cash_andPayments"
    implements MenuSearch

    General is a DashBoardPanel
        title is "Main"
        use 12 column grid
        CurrentCashRequirementsResults is a Pane
            span 6 columns
            business class is CashRequirementsResult
                list is CurrentCashRequirementsResultsCardList
                    overview navigation is CurrentCashRequirementsResults
        PaymentProcessing is a Pane
            span 6 columns
            title is "Processing"
            Layout
                two column distributed
                    large menu
                        action is PaymentClearingFileCreation.PaymentClearingFileCreation
                            title is "CreatePaymentClearingFiles"
                            menu item name is CreatePaymentClearingFiles
                            description is "Create an electronic file for non-CSV flat file formats."
                            icon is icon.cycle-azure
                        action is ElectronicPaymentCreation.ElectronicPaymentCreation
                            title is "CreateElectronicPayments"
                            menu item name is CreateElectronicPayments
                            description is "Create an electronic payment transfer file to pay scheduled invoices for vendors that are defined to pay by electronic payment."
                            icon is icon.cycle-azure
                        action is PaymentClose.PaymentClose
                            title is "PaymentCloseOld"
                            menu item name is PaymentClose
                            description is "Complete a cash payment cycle using old Payment Close program for a pay group."
                            icon is icon.cycle-azure
                        visible when (actor.context.FinanceEnterpriseGroup.NewPaymentCloseConfigExists)
                            action is CashRequirementsResult.ClosePaymentCycle
                                title is "PaymentCloseNew"
                                menu item name is PaymentCloseNew
                                description is "Complete a cash payment cycle using new Payment Close action for a pay group."
                                icon is icon.cycle-azure
                        action is CashPaymentRegister.CashPaymentRegister
                            title is "CreateCashPaymentRegister"
                            menu item name is CreateCashPaymentRegister
                            description is "View invoice payments for a pay group."
                            icon is icon.cycle-azure
                        action is PaymentVoidStopPayUpdate.PaymentVoidStopPayUpdate
                            title is "PaymentVoidStopPayUpdate"
                            menu item name is PaymentVoidStopPayUpdate
                            description is "Reinstate invoices that are associated with payments that are voided in Cash Management."
                            icon is icon.cycle-azure
                        action is PaidInvoiceProjectUpdate.PaidInvoiceProjectUpdate
                            title is "UpdatePaidInvoiceProjects"
                            menu item name is UpdatePaidInvoiceProjects
                            description is "Run after Payment Close to list the transactions that are affected by payment status updates."
                            icon is icon.cycle-azure
                        visible when (actor.context.FinanceEnterpriseGroup.NewPaymentCloseConfigExists)
                            list is PayablesJobResult.ManualPaymentsClosingResults
                                title is "ManualPaymentsClosingResults"
                                menu item name is ManualPaymentsClosingResults
                                description is "View payment close results for manually paid invoices for a pay group."
                                icon is icon.cycle-azure
        PastDuePayments is a Pane
            span 12 columns
            business class is PayablesInvoicePayment
                list is PastDuePayments
                    overview navigation is PastDuePayments
        DiscountPaymentsDueWithinNextWeek is a Pane
            span 6 columns
            business class is PayablesInvoicePayment
                list is DiscountPaymentsDueWithinNextWeek
                    overview navigation is DiscountPaymentsDueWithinNextWeek
        PaymentsDueWithinNextWeek is a Pane
            span 6 columns
            business class is PayablesInvoicePayment
                list is PaymentsDueWithinNextWeek
                    overview navigation is PaymentsDueWithinNextWeek
        CashLedgerPayments is a Pane
            span 6 columns
            business class is CashLedgerPayment
                list is ReleasedCashLedgerPaymentsForPayCycle
                    overview navigation is ReleasedCashLedgerPayments
        InvoicesOnHold is a Pane
            span 6 columns
            business class is PayablesOnHold
                list is PayablesPaymentProcessorOnHoldList
                    overview navigation is OnHoldInvoiceSearch

    PaymentSearch is a Panel
        title is "Search"
        business class is PayablesInvoicePayment
            list is PaymentSearchSelectionList
                title is "Search"

    PaymentOutputFiles is a Panel
        title is "OutputFiles"
        business class is PaymentOutputFileHeader
            list is PaymentOutputFileHeaderList
                title is "OutputFiles"

    VendorPaymentHistory is a Panel
        title is "VendorPayments"
        business class is PayablesInvoicePayment
            list is VendorPaymentHistoryList
                title is "VendorPayments"

    ManualPayments is a Panel
        business class is CashLedgerPayablesPayment
            list is ManualPaymentsList

    RetrospectiveInvoices is a Panel
        business class is PayablesInvoicePayment
            list is RetrospectiveInvoicePaymentsList

    DistributionsByPayment is a Panel
        business class is PayablesInvoicePayment
            list is DistributionsByPayment

    CreditMemoApplication is a Panel
        title is "CreditMemos"
        business class is PayablesCreditMemo
            list is CreditMemosAppliedList
                title is "CreditMemos"

    CashLedgerTransaction is a Panel
        title is "CashTransactions"
        business class is CashLedgerTransaction
            list is AllCashLedgerTransactionsList
                title is "CashTransactions"

    AgingReportByFund is a Panel
        title is "AgingReportByFund"
        business class is AgingByFund
            list is AgingByFundList
                title is "AgingReportByFund"

    More is a DashBoardPanel
        use 12 column grid
        Cash is a Pane
            span 4 columns
            title is "CashRequirements"
            Layout
                single column
                    large menu
                        page is CashRequirementsUX1.Current
                            title is "Current"
                            menu item name is Current
                            icon is icon.money-bag-emerald
                        page is CashRequirementsUX1.Historical
                            title is "Historical"
                            menu item name is Historical
                            icon is icon.money-bag-emerald
                        page is CashRequirementsUX1.Templates
                            title is "Templates"
                            menu item name is Templates
                            icon is icon.money-bag-emerald
        AuditHistory is a Pane
            span 4 columns
            Layout
                single column
                    large menu
                        page is PayablesAuditHistoryUX1.Invoice
                            title is "Invoice"
                            menu item name is Invoice
                            icon is icon.calendar-variant-azure
                        page is PayablesAuditHistoryUX1.Distribution
                            title is "Distribution"
                            menu item name is Distribution
                            icon is icon.calendar-variant-azure
                        page is PayablesAuditHistoryUX1.Payment
                            title is "Payment"
                            menu item name is Payment
                            icon is icon.calendar-variant-azure
        Forecasting is a Pane
            span 4 columns
            Layout
                single column
                    large menu
                        list is PayablesInvoicePayment.PayablesForecastingList
                            title is "PayablesForecasting"
                            menu item name is PayablesForecasting
                            icon is icon.window-time-charts-amethyst
                        page is DirectForecastPage
                            title is "DirectForecasts"
                            menu item name is DirectForecasts
                            description is "Loading direct forecast data."
                            icon is icon.window-time-charts-amethyst
        PaymentCreationDefaults is a Pane
            span 12 columns
            business class is PaymentCreationDefaults
                list is PaymentCreationDefaultsList
                    overview navigation is PaymentCreationDefaultsListNav

