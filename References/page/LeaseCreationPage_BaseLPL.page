LeaseCreationPage is a Page
    title is "CreateLeases"
    implements MenuSearch
    show panel control on left

    Create is a DashBoardPanel
        use 12 column grid
        Lease is a Pane
            span 6 columns
            Layout
                single column
                    small menu
                        action is Lease.Unreleased.Create
                            title is "CreateNewLease"
                            menu item name is CreateNewLease
                            icon is icon.check-azure
                        action is Lease.Unreleased.CreateLeaseFromTemplate
                            title is "CreateLeaseFromTemplate"
                            menu item name is CreateLeaseFromTemplate
                            icon is icon.check-azure
                        action is CreateAPInvoicesForLeasePayments.CreateAPInvoicesForLeasePayments
                            title is "CreateInvoices"
                            menu item name is CreateInvoices
                            icon is icon.check-azure
        Sublease is a Pane
            span 6 columns
            Layout
                single column
                    small menu
                        action is Sublease.Unreleased.Create
                            title is "CreateNewSublease"
                            menu item name is CreateNewSublease
                            icon is icon.check-ruby
                        action is SubleaseBillingScheduleDetail.CreateInvoices
                            title is "CreateInvoices"
                            menu item name is CreateInvoices
                            icon is icon.check-ruby

    LeasePaymentScheduleImport is a Panel
        title is "PaymentScheduleImport"
        business class is LeasePaymentImport
            list is LeasePaymentImportLists
                title is "PaymentScheduleImport"

