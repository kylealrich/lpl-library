ActorSecurityReports is a Page
    title is "Actors"

    ActorRole is a Panel
        business class is Actor
            list is ActorRoleList

    RoleSecurityClassesForActor is a MultiPanePanel
        pane 1
            business class is Actor
                list is ActiveActorList
        pane 2
            business class is pane 1.ActorRole set
                list is DefaultActorRoleList
        pane 3
            business class is pane 2.RoleSecurityClassRel
                list is CurrentDataAreaPoliciesList

    ActorAndRoleReport is a MultiPanePanel
        pane 1
            business class is ActorAndRoleHistoryAudit
                list is ActorAndRoleHistoryList

