CashTransactionSetupPage is a Page
    title is "CashTransactionSetup"
    show panel control on left

    CashAllocationCodes is a Panel
        business class is CashAllocationCode
            list is CashAllocationCodes

    Categories is a Panel
        business class is CashTransactionCategory
            list is CashTransactionCategories

    IdentifierTypes is a Panel
        business class is CashTransactionIdentifierType
            list is CashTransactionIdentifierTypes

    Identifiers is a Panel
        business class is CashTransactionIdentifier
            list is CashTransactionIdentifiers

    TransactionTypes is a Panel
        business class is BankTransactionTypeCode
            list is BankTransactionTypeCodes

    CustomGroups is a Panel
        business class is BusinessClassGroup
            list is BusinessClassGroups

    ProcessingRules is a Panel
        business class is CashTransactionProcessingRule
            list is CashTransactionProcessingRules

