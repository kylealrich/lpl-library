FinanceAdministrationPage is a Page
    title is "CashAdministration"

    FinancialInstitutionSummary is a Panel
        business class is FinancialInstitution
            list is FinancialInstitutionDashBoard

    AccountSummary is a Panel
        business class is CashManagementAccount
            list is CashAccountDashBoard

    BankStatementInterface is a Panel
        business class is BankStatementInterfaceHeader
            list is BankStatementInterfaceHeaderList

    Resources is a Panel
        mouse over text is "DefineResources"
        business class is FinanceResource
            list is MaintainFinanceResourceList
            Context
                HROrganization

    Teams is a Panel
        business class is FinanceEnterpriseGroup
            list is FinanceEnterpriseGroupTeamDashboard

    Calendars is a MultiPanePanel
        mouse over text is "Calendars"
        pane 1
            business class is EnterpriseGroup
                list is EnterpriseGroups
        pane 2
            business class is pane 3.SystemCalendarPeriodGroup set
                list is SystemCalendarPeriodSets
        pane 3
            business class is pane 1.SystemCalendar set
                list is SystemCalendars
        pane 4
            business class is pane 2.SystemCalendarPeriod set
                list is SystemCalendarPeriods

