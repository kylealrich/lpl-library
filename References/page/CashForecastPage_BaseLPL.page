CashForecastPage is a Page
    title is "CashForecasts"

    CashForecasts is a Panel
        business class is CashForecast
            list is CashForecastCardList

    Analysis is a DashBoardPanel
        title is "Analysis"
        mouse over text is "CashForecastAnalysis"
        show 4 columns
        RevisedForecast is a Pane
            double width
            business class is CashForecastPeriodAmount
                list is RevisedCashForecastPeriodAmountCube
                    title is "RevisedForecast"
                    overview navigation is RevisedCashForecastPeriodAmountCubeLink
                        label is "Detail"
        OriginalForecast is a Pane
            double width
            business class is CashForecastPeriodAmount
                list is CashForecastPeriodAmountCube
                    title is "OriginalForecast"
                    overview navigation is CashForecastPeriodAmountCubeLink
                        label is "Detail"
        WeeklyPublishedForecast is a Pane
            double width
            business class is CashForecastPeriodAmount
                list is WeeklyPublishedCashForecastPeriodAmountCube
                    title is "WeeklyPublishedForecast"
                    overview navigation is WeeklyPublishedCashForecastPeriodAmountCubeLink
                        label is "Detail"
        MonthlyPublishedForecast is a Pane
            double width
            business class is CashForecastPeriodAmount
                list is MonthlyPublishedCashForecastPeriodAmountCube
                    title is "MonthlyPublishedForecast"
                    overview navigation is MonthlyPublishedCashForecastPeriodAmountCubeLink
                        label is "Detail"

