RecallSetupPage is a Page
    title is "ItemLifecycleManagementSetup"

    Dashboard is a DashBoardPanel
        show 4 columns
        PastDueTasks is a Pane
            double width
            business class is RecallNotice
                list is PastDueTasksCardList
                    overview navigation is PastDueTasksNav
        UnresolvedNoticeQuantities is a Pane
            double width
            business class is RecallNotice
                list is UnresolvedQuantitiesCardList
                    overview navigation is UnresolvedQuantitiesNav
        QAndAToAnswer is a Pane
            quad width
            business class is RecallNoticeQandA
                list is MyOpenQandACardList
                    overview navigation is MyUnansweredNav
        NoticeUpdates is a Pane
            quad width
            business class is RecallNotice
                list is RecallNoticeUpdatesCardList
                    overview navigation is RecallNoticeUpdatesNav

    Groups is a Panel
        business class is RecallGroup
            list is RecallGroups

    Users is a Panel
        business class is RecallUser
            list is RecallUsers

    Domains is a Panel
        business class is RecallDomain
            list is RecallDomains

    Organizations is a Panel
        business class is RecallOrganization
            list is RecallOrganizationsTree

    TaskInstructions is a Panel
        business class is RecallTaskInstruction
            list is RecallTaskInstructions

    NoticeTypes is a Panel
        business class is RecallType
            list is RecallTypes

    ActionReasons is a Panel
        business class is ActionReason
            list is ActionReasonList

