UX1InventoryReorderPage is a Page
    title is "Reorder"
    implements MenuSearch

    Reorder is a DashBoardPanel
        title is "Dashboard"
        use 12 column grid
        Replenishment is a Pane
            span 12 columns
            title is "ReplenishmentJobs"
            Layout
                three column distributed
                    column1
                        large menu
                            action is ReplenishmentByRequisition.ReplenishmentByRequisition
                                title is "ByRequisition"
                                menu item name is ByRequisition
                                description is "Replenish_an_inventory_location_through_a_requisition_interface_with_the_Purchase_Order_application"
                                icon is icon.createContract
                            action is InventoryVendorReorderAdvice.InventoryVendorReorderAdvice
                                title is "VendorReorderAdvice"
                                menu item name is VendorReorderAdvice
                                description is "Replenish_quantities_for_items_with_an_available_quantity_that_is_less_than_the_reorder_point"
                                icon is icon.person-box-azure
                    column2
                        large menu
                            action is InventoryLocationReplenishmentReport.InventoryLocationReplenishmentReport
                                title is "Location"
                                menu item name is LocationReplenishment
                                description is "Generate_report_of_replenishment_quantity_for_items_with_an_available_quantity_less_than_the_reorder_point_for_items_that_are_stocked_from_another_location"
                                icon is icon.compare-azure
                            action is BinReplenishmentReport.BinReplenishmentReport
                                title is "Bin"
                                menu item name is BinReplenishment
                                description is "Generate_a_report_of_item_bins_that_need_replenishment_from_another_bin_within_the_same_location"
                                icon is icon.recall-azure
                    column3
                        large menu
                            action is InventoryEOQandReorderCalculation.InventoryEOQandReorderCalculation
                                title is "EOQ_andReorderCalculation"
                                menu item name is EOQandReorderCalculation
                                description is "Calculate the Economic Order Quantity (EOQ) and reorder point (ROP)"
                                icon is icon.calculator-azure
                            action is InventoryFinishedGoodReplenishment.InventoryFinishedGoodReplenishment
                                title is "FinishedGoods"
                                menu item name is FinishedGoods
                                description is "Generate_report_of_finished_goods_that_must_be_replenished"
                                icon is icon.window-box-azure
        ParLocation is a Pane
            span 6 columns
            title is "Par_And_Cart_Locations"
            Layout
                two column distributed
                    large menu
                        page is UX1ParLocationPage.ParLocations
                            title is "ParLocations"
                            menu item name is ParLocations
                            description is "Replenish_Stock_On_Hand_quantities_up_to_a_par_level"
                            icon is icon.home-amber
                        page is UX1ParLocationPage.CartLocations
                            title is "CartLocations"
                            menu item name is CartLocations
                            description is "Replenish_Exchange_Cart's_Stock_On_Hand_quantities_up_to_a_par_level"
                            icon is icon.shopping-cart-amber
                        list is InventoryControlResult.ParLocationUtilizationReportList
                            title is "Par_Location_Utilization"
                            menu item name is ParLocationUtilization
                            description is "Display_list_of_Par_Location_Utilization"
                            icon is icon.outline-square-amber
                        page is UX1ParLocationPage.ParItems
                            title is "ParItems"
                            menu item name is ParItems
                            description is "View_optimum_amount_of_Par_Items_stock_on_hand_after_each_order_delivery"
                            icon is icon.directory-amber
                        page is UX1ParLocationPage.ParItemsWithInvalidUOM
                            title is "ParItemsWithInvalidUOM"
                            menu item name is ParItemsWithInvalidUOM
                            description is "List_of_Items_With_Invalid_Unit_Of_Measure"
                            icon is icon.hcm-amber
                        visible when (actor.context.FinanceEnterpriseGroup.ShowMobileSupplyChain)
                            page is MobileSupplyChainUploadsPage.ParCounts
                                title is "ParCount"
                                menu item name is ParCount
                                description is "View_list_of_par_counts_done_via_MSCM_application_in_handheld"
                                icon is icon.clipboard-text-emerald
                            page is MobileSupplyChainUploadsPage.ParLevelUpdates
                                title is "ParLevelUpdates"
                                menu item name is ParLevelUpdates
                                description is "View_list_of_items_with_par_levels_updated_thru_MSCM"
                                icon is icon.clipboard-text-emerald
                            page is KanbanCountPage.KanbanCounts
                                title is "KanbanCount"
                                menu item name is KanbanCount
                                description is "View_list_of_submitted_Kanban_counts_from_MSCM_in_handheld"
                                icon is icon.clipboard-text-emerald
        OtherActions is a Pane
            span 6 columns
            Layout
                two column distributed
                    column1
                        small menu
                            action is InventoryReorderAdvice.InventoryReorderAdvice
                                title is "InventoryReorderAdvice"
                                menu item name is InventoryReorderAdvice
                                description is "Print_a_report_of_the_Inventory_Reorder_Advice_report"
                                icon is icon.outline-list-amethyst
                            action is InventoryIntransitShipping.InventoryIntransitShipping
                                title is "IntransitShipping"
                                menu item name is IntransitShipping
                                description is "Print_a_report_of_the_released_intransit_transaction_documents_that_have_not_beenr_received"
                                icon is icon.window-box-amethyst
                            action is InventoryOutOfStockExtract.InventoryOutOfStockExtract
                                title is "OutOfStockExtract"
                                menu item name is OutOfStockExtract
                                description is "Print_a_list_of_items_where_the_stock_on_hand_quantity_is_less_than_the_safety_stock"
                                icon is icon.recall-amethyst
                            list is Requisition.RequisitionList
                                title is "ViewRequisitions"
                                menu item name is ViewRequisitions
                                icon is icon.documents-amethyst
                    column2
                        small menu
                            list is BinReorderPolicy.primary
                                title is "BinReorderPolicies"
                                menu item name is BinReorderPolicies
                                description is "Manage_Bin_Reorder_Policies"
                                icon is icon.document-certified-amethyst
                            list is PurchaseOrderInterface.ReplenishmentPurchaseOrderWorksheet
                                title is "Replenishment_PO_Requests"
                                menu item name is ReplenishmentPORequests
                                description is "Add,_change,_or_delete_items_to_be_replenished_in_inventory"
                                icon is icon.box-amethyst
                            list is InventoryForecast.InventoryForecastList
                                title is "Forecasts"
                                menu item name is Forecast
                                description is "Analyze_past_ordering_activity_for_an_item_in_order_to_predict_the_future_ordering_demand_for_that_item"
                                icon is icon.window-time-charts-amethyst
                            page is ManagePurchaseRequestPage
                                title is "ManagePurchaseRequest"
                                menu item name is ManagePurchaseRequest
                                icon is icon.document-amethyst

