GlobalLedgerAdmin is a Page
    title is "<actor.context.FinanceEnterpriseGroup.GlobalLedgerAdministrationTitle>"
    implements MenuSearch

    Setup is a DashBoardPanel
        title is "Setup"
        use 12 column grid
        FEGMaintenance is a Pane
            span 6 columns
            title is "Maintenance"
            Layout
                two column distributed
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceEnterpriseGroupExists)
                        form is FinanceEnterpriseGroup.FinanceEnterpriseGroupDetails
                            title is "MyFinanceEnterpriseGroup"
                            menu item name is MyFinanceEnterpriseGroup
                            icon is icon.outline-maintenance-emerald
                    list is FinanceEnterpriseGroup.FinanceEnterpriseGroupList
                        title is "FinanceEnterpriseGroups"
                        menu item name is FinanceEnterpriseGroups
                        icon is icon.outline-maintenance-emerald
                    visible when (actor.context.FinanceEnterpriseGroup.AccountingEntityExists)
                        page is GLFEGEntity
                            title is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
                            menu item name is AccountingEntity
                            icon is icon.outline-maintenance-emerald
                    page is GLFEGAccounts
                        title is "Accounts"
                        menu item name is Accounts
                        icon is icon.outline-maintenance-emerald
                    page is GLFEGDimensions
                        title is "UserDimensions"
                        menu item name is UserDimensions
                        icon is icon.outline-maintenance-emerald
                    list is GeneralLedgerCalendar.GeneralLedgerCalendarList
                        title is "Calendars"
                        menu item name is Calendars
                        icon is icon.outline-maintenance-emerald
                    list is ReportingBasis.ReportingBasisList
                        title is "ReportingBasis"
                        menu item name is ReportingBasis
                        icon is icon.outline-maintenance-emerald
                    page is CurrencyPage
                        title is "Currency"
                        menu item name is Currency
                        icon is icon.outline-maintenance-emerald
        RelationsRules is a Pane
            span 3 columns
            title is "Rules"
            Layout
                single column
                    list is CodeBlockRelation.CodeBlockRelations
                        title is "StructureRelations"
                        menu item name is StructureRelations
                        icon is icon.outline-utility-azure
                    visible when (actor.context.FinanceEnterpriseGroup.ValidateFinanceStructure)
                        list is GeneralLedgerCode.GeneralLedgerCodeList
                            title is "StructureExceptions"
                            menu item name is StructureExceptions
                            icon is icon.outline-utility-azure
                    visible when (actor.context.FinanceEnterpriseGroup.StructureValidationDimensionEntry)
                        list is ProjectStructureValidation.ProjectStructureValidationList
                            title is "ProjectStructureValidation"
                            menu item name is ProjectStructureValidation
                            icon is icon.outline-utility-azure
                    visible when (actor.context.FinanceEnterpriseGroup.EnableResponsibilityMatrix)
                        list is ResponsibilityMatrixApproval.ResponsibilityMatrixMatrixList
                            title is "ResponsibilityMatrix"
                            menu item name is ResponsibilityMatrix
                            icon is icon.outline-utility-azure
                    visible when (actor.context.FinanceEnterpriseGroup.FundAccounting)
                        list is GLFinanceStructureRule.primary
                            title is "FundAccounting"
                            menu item name is FundAccounting
                            icon is icon.outline-utility-azure
        GLSetupCodes is a Pane
            span 3 columns
            title is "Codes"
            Layout
                single column
                    list is AddressCode.AddressCodeList
                        title is "Address"
                        menu item name is Address
                        icon is icon.outline-list-amber
                    list is PostingCategory.primary
                        title is "PostingCategory"
                        menu item name is PostingCategory
                        icon is icon.outline-list-amber
                    list is GeneralLedgerSystemCode.GLSystemCodeList
                        title is "System"
                        menu item name is System
                        icon is icon.outline-list-amber
                    list is GeneralLedgerEvent.GLEventList
                        title is "Event"
                        menu item name is Event
                        icon is icon.outline-list-amber
                    list is ApprovalCode.ApprovalCodes
                        title is "Approval"
                        menu item name is Approval
                        icon is icon.outline-list-amber
                    list is FinanceTeam.BuildTeamList
                        title is "Teams"
                        menu item name is Teams
                        icon is icon.outline-list-amber
        OtherSetups is a Pane
            span 6 columns
            title is "GeneralSetup"
            Layout
                two column distributed
                    page is GLSecurityDimension
                        title is "SecurityGroups"
                        menu item name is SecurityGroups
                        icon is icon.briefcase-ruby
                    list is EnterpriseGroup.EnterpriseGroups
                        title is "EnterpriseGroups"
                        menu item name is EnterpriseGroups
                        icon is icon.briefcase-ruby
                    list is GeneralLedgerCompany.primary
                        title is "Companies"
                        menu item name is Companies
                        icon is icon.briefcase-ruby
                    list is GeneralLedgerCompanyGroup.primary
                        title is "CompanyGroups"
                        menu item name is CompanyGroups
                        icon is icon.briefcase-ruby
                    list is BusinessGroup.BusinessGroups
                        title is "VendorBusinessGroups"
                        menu item name is VendorBusinessGroups
                        icon is icon.briefcase-ruby
                    list is CustomerBusinessGroup.primary
                        title is "CustomerBusinessGroups"
                        menu item name is CustomerBusinessGroups
                        icon is icon.briefcase-ruby
        StructureComparePane is a Pane
            span 6 columns
            title is "StructureCompare"
            Layout
                two column distributed
                    list is ReportingChart.XiReportingChartList
                        title is "Chart_of_Account"
                        menu item name is ChartOfAccount
                        icon is icon.compare-amethyst
                    visible when (actor.context.FinanceEnterpriseGroup.AccountingUnitSelected)
                        list is AccountingUnitStructure.XiAccountingUnitStructureList
                            title is "<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>"
                            menu item name is AccountingUnit
                            icon is icon.compare-amethyst
                    page is StructureComparePage
                        title is "FinancialDimensions"
                        menu item name is FinancialDimensions
                        icon is icon.compare-amethyst
                    visible when (actor.context.FinanceEnterpriseGroup.ProjectEntry)
                        list is ProjectStructure.XiProjectStructureAdmiList
                            title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
                            menu item name is Project
                            icon is icon.compare-amethyst

    Utilities is a DashBoardPanel
        title is "Utilities"
        use 12 column grid
        RebuildsUpdateData is a Pane
            span 3 columns
            title is "Rebuilds_to_update_data"
            Layout
                single column
                    large menu
                        action is RebuildZoneBalances.ScheduleRebuildZoneBalances
                            title is "RebuildZoneBalances"
                            menu item name is RebuildZoneBalances
                            description is "Run_this_to_rebuild_your_zone_balances_for_balances_report"
                            icon is icon.settings-azure
                        visible when (actor.context.FinanceEnterpriseGroup.AccountingUnitSelected)
                            action is RebalanceAccountingUnitZones.ScheduleRebalanceAccountingUnitZones
                                title is "<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>ZoneRebalance"
                                menu item name is AccountingUnitZoneRebalance
                                description is "Run_this_to_generate_system_journals_to_rebalance_zones_when_reorganizing_zone_<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>"
                                icon is icon.settings-azure
                        visible when (actor.context.FinanceEnterpriseGroup.Dimension1ZoneBalancing)
                            action is FinanceEnterpriseGroup.RebalanceFinanceDimension1Zones
                                title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension1Label>ZoneRebalance"
                                menu item name is FinanceDimension1ZoneRebalance
                                description is "Run_this_to_generate_system_journals_to_rebalance_zones_when_reorganizing_zone_<actor.context.FinanceEnterpriseGroup.FinanceDimension1Label>"
                                icon is icon.settings-azure
        DimensionShadow is a Pane
            span 6 columns
            title is "DimensionRebuilds"
            Layout
                two column distributed
                    action is FinanceEnterpriseGroup.RebuildAccountingEntityShadow
                        title is "EntityShadow"
                        menu item name is EntityShadow
                        icon is icon.settings-amber
                    action is RebuildReportingChartShadow.ScheduleRebuildReportingChartShadow
                        title is "ReportingChartShadow"
                        menu item name is ReportingChartShadow
                        icon is icon.settings-amber
                    visible when (actor.context.FinanceEnterpriseGroup.AccountingUnitSelected)
                        action is RebuildAccountingUnitShadow.ScheduleRebuildAccountingUnitShadow
                            title is "<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>Shadow"
                            menu item name is AccountingUnitShadow
                            icon is icon.settings-amber
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension1Entry)
                        action is RebuildFinanceDimension1Shadow.ScheduleRebuildFinanceDimension1Shadow
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension1Label>Shadow"
                            menu item name is FinanceDimension1Shadow
                            icon is icon.settings-amber
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension2Entry)
                        action is RebuildFinanceDimension2Shadow.ScheduleRebuildFinanceDimension2Shadow
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>Shadow"
                            menu item name is FinanceDimension2Shadow
                            icon is icon.settings-amber
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension3Entry)
                        action is RebuildFinanceDimension3Shadow.ScheduleRebuildFinanceDimension3Shadow
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension3Label>Shadow"
                            menu item name is FinanceDimension3hadow
                            icon is icon.settings-amber
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension4Entry)
                        action is RebuildFinanceDimension4Shadow.ScheduleRebuildFinanceDimension4Shadow
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension4Label>Shadow"
                            menu item name is FinanceDimension4Shadow
                            icon is icon.settings-amber
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension5Entry)
                        action is RebuildFinanceDimension5Shadow.ScheduleRebuildFinanceDimension5Shadow
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension5Label>Shadow"
                            menu item name is FinanceDimension5Shadow
                            icon is icon.settings-amber
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension6Entry)
                        action is RebuildFinanceDimension6Shadow.ScheduleRebuildFinanceDimension6Shadow
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension6Label>Shadow"
                            menu item name is FinanceDimension6Shadow
                            icon is icon.settings-amber
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension7Entry)
                        action is RebuildFinanceDimension7Shadow.ScheduleRebuildFinanceDimension7Shadow
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension7Label>Shadow"
                            menu item name is FinanceDimension7Shadow
                            icon is icon.settings-amber
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension8Entry)
                        action is RebuildFinanceDimension8Shadow.ScheduleRebuildFinanceDimension8Shadow
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension8Label>Shadow"
                            menu item name is FinanceDimension8Shadow
                            icon is icon.settings-amber
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension9Entry)
                        action is RebuildFinanceDimension9Shadow.ScheduleRebuildFinanceDimension9Shadow
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension9Label>Shadow"
                            menu item name is FinanceDimension9Shadow
                            icon is icon.settings-amber
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension10Entry)
                        action is RebuildFinanceDimension10Shadow.ScheduleRebuildFinanceDimension10Shadow
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension10Label>Shadow"
                            menu item name is FinanceDimension10Shadow
                            icon is icon.settings-amber
                    visible when (actor.context.FinanceEnterpriseGroup.ProjectEntry)
                        action is RebuildProjectShadow.ScheduleRebuildProjectShadow
                            title is "ProjectShadow"
                            menu item name is ProjectShadow
                            icon is icon.settings-amber
                    action is RebuildGeneralLedgerPeriodShadow.ScheduleRebuildGeneralLedgerPeriodShadow
                        title is "CalendarPeriodShadow"
                        menu item name is CalendarPeriodShadow
                        icon is icon.settings-amber
                    action is Scenario.BuildScenarioShadowFile
                        title is "ScenarioShadow"
                        menu item name is ScenarioShadow
                        icon is icon.settings-amber
        BudgetRebuilds is a Pane
            span 3 columns
            title is "Budget"
            Layout
                single column
                    action is FinanceEnterpriseGroup.SetBudgetCurrencyCounters
                        title is "RebuildCurrencyCounters"
                        menu item name is RebuildCurrencyCounters
                        icon is icon.settings-emerald
                    action is BudgetTemplateMember.RebuildAll
                        title is "RebuildTemplateMembers"
                        menu item name is RebuildTemplateMembers
                        icon is icon.settings-emerald
                    action is BudgetTemplatePending.FixAll
                        title is "RebuildTemplatePending"
                        menu item name is RebuildTemplatePending
                        icon is icon.settings-emerald
                    action is GeneralLedgerTotal.FixPeriodEndingDates
                        title is "FixPeriodEndingDates_forBudgetaryRows"
                        menu item name is FixPeriodEndingDatesForBudgetaryRows
                        icon is icon.settings-emerald
                    action is FinanceEnterpriseGroup.ClearUseDEPMBudgetingFlag
                        title is "ClearEPMFlag"
                        menu item name is ClearEPMFlag
                        icon is icon.settings-emerald
        OtherPane is a Pane
            span 3 columns
            title is "Others"
            Layout
                single column
                    large menu
                        page is GlobalLedgerUtilitiesPurge
                            title is "Purge"
                            menu item name is Purge
                            description is "Programs_to_purge_transactional_records"
                            icon is icon.settings-ruby
                        page is GlobalLedgerUtilitiesMigrateCommits
                            title is "Migrations"
                            menu item name is Migrations
                            description is "Programs_to_migrate_commitments"
                            icon is icon.settings-ruby
                        page is GlobalLedgerUtilitiesLegacyTransactions
                            title is "LegacyTransactions"
                            menu item name is LegacyTransactions
                            description is "Programs_to_build_legacy_transactions"
                            icon is icon.settings-ruby
                        action is ReallocateDistributionsForFundAccounting.ReallocateDistributionsForFundAccounting
                            title is "ReallocateDistributionsForFundAccounting"
                            menu item name is ReallocateDistributionsForFundAccounting
                            description is "Run_this_to_update_any_open_invoices_in_Payables_and_Receivables_to_be_allocated_per_the_fund_accounting_rules."
                            icon is icon.settings-ruby
        RebuildFixData is a Pane
            span 9 columns
            title is "Rebuild_to_fix_data"
            Layout
                three column distributed
                    large menu
                        visible when (actor.context.FinanceEnterpriseGroup.AccountingUnitSelected)
                            action is RebuildOrganizationPairs.ScheduleRebuildOrganizationPairs
                                title is "RebuildOrganizationPairs"
                                menu item name is RebuildOrganizationPairs
                                description is "Rebuild_system_structure_relations_between_<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>_<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>"
                                icon is icon.settings-amethyst
                        action is AccountingEntity.FillEntityDimensionType
                            title is "FillEntityDimensionType"
                            menu item name is FillEntityDimensionType
                            description is "Populates_dimension_type_if_empty_for_<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
                            icon is icon.settings-amethyst
                        action is FinanceEnterpriseGroup.RebuildCalendarDateAndPeriod
                            title is "RebuildCalendarDateAndPeriods"
                            menu item name is RebuildCalendarDateAndPeriods
                            description is "Run_this_to_rebuild_the_calendar_date_and_periods_for_average_daily_balance_reporting"
                            icon is icon.settings-amethyst
                        action is RebuildLedgerEntityPairs.ScheduleRebuildLedgerEntityPairs
                            title is "RebuildPairsForLedger<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
                            menu item name is RebuildPairsForLedgerAccountingEntity
                            description is "Rebuild_system_structure_relations_between_Ledger_and_<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
                            icon is icon.settings-amethyst
                        action is FixEntityPeriod.ScheduleFixEntityPeriod
                            title is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>PeriodRebuild"
                            menu item name is AccountingEntityPeriodRebuild
                            description is "Run_this_if_close_periods_are_missing"
                            icon is icon.settings-amethyst
                        action is RebuildTotals.ScheduleRebuildTotals
                            title is "RebuildTotals"
                            menu item name is RebuildTotals
                            description is "Run_this_if_Totals_are_out_of_sync_with_Transactions"
                            icon is icon.settings-amethyst
                        action is GeneralLedgerTotal.ClearCommitmentTotals
                            title is "RebuildCommitmentsAndEncumbrances"
                            menu item name is RebuildCommitmentsAndEncumbrances
                            description is "Run_this_if_Totals_for_Encumbrance_or_Commitments_are_out_of_sync_with_GLCommit"
                            icon is icon.settings-amethyst
                        action is RebuildUndistributedRetainedEarnings.ScheduleRebuildUndistributedRetainedEarnings
                            title is "RebuildUndistributedRetainedEarnings"
                            menu item name is RebuildUndistributedRetainedEarnings
                            description is "Run_this_if_undistributed_retained_earnings_are_out_of_sync_with_net_income_gain/loss"
                            icon is icon.settings-amethyst
                        action is ResetAverageDailyBalance.ResetAverageDailyBalance
                            title is "RebuildAverageDailyBalance"
                            menu item name is RebuildAverageDailyBalance
                            description is "Run_this_to_rebuild_the_average_daily_balance_reports"
                            icon is icon.settings-amethyst

    ReportDesigner is a DashBoardPanel
        title is "ReportEnablement"
        use 12 column grid
        visible when (actor.context.FinanceEnterpriseGroup.ReportingPowerUserExists)
        ReportEnablement is a Pane
            span 4 columns
            title is "ReportDesignerEnablement"
            Layout
                single column
                    action is DataCreation.ValidateReportDesignerSetup
                        title is "ValidateSetup"
                        menu item name is ValidateSetup
                        icon is icon.window-time-charts-amethyst
                    list is DataCreationResult.CreateGLReportDesignerModelResults
                        title is "ViewSetupResults"
                        menu item name is ViewSetupResults
                        icon is icon.window-time-charts-amethyst
                    action is DataCreation.ReportDesignerUpdate
                        title is "ReportDesignerUpdate"
                        menu item name is ReportDesignerUpdate
                        icon is icon.window-time-charts-amethyst
        ReportUtilities is a Pane
            span 4 columns
            title is "ReportDesignerUtilities"
            Layout
                single column
                    action is DataCreation.CreateBirstModels
                        title is "CreateModel"
                        menu item name is CreateModel
                        icon is icon.book-chart-emerald
                    list is DataCreationResult.CreateGLReportDesignerModelResults
                        title is "ViewModelResults"
                        menu item name is ViewModelResults
                        icon is icon.book-chart-emerald
                    action is DataCreation.OptimizeBirstModelForFEG
                        title is "OptimizeModel"
                        menu item name is OptimizeModel
                        icon is icon.book-chart-emerald
                    action is DataCreation.ResetAndSetCreateBlankFinanceDimensionRows
                        title is "ResetAndCreateBlankFinanceDimensionRows"
                        menu item name is ResetAndCreateBlankFinanceDimensionRows
                        icon is icon.book-chart-emerald
                    action is DataCreation.EnableGLReportDesigner
                        title is "EnableReportDesigner"
                        menu item name is EnableReportDesigner
                        icon is icon.book-chart-emerald
        Columnar is a Pane
            span 4 columns
            Layout
                single column
                    action is DataCreation.CreateReplicationSets
                        title is "CreateReplicationSets"
                        menu item name is CreateReplicationSets
                        icon is icon.window-value-charts-ruby
                    list is DataCreationResult.CreateReplicationSetsResults
                        title is "ViewReplicationSetsResults"
                        menu item name is ViewReplicationSetsResults
                        icon is icon.window-value-charts-ruby
                    action is DataCreation.EnableGlobalLedgerTransactionAnalysis
                        title is "EnableGlobalLedgerTransactionAnalysis"
                        menu item name is EnableGlobalLedgerTransactionAnalysis
                        icon is icon.window-value-charts-ruby
                    action is DataCreation.EnableProjectColumnarSearch
                        title is "EnableProjectColumnarSearch"
                        menu item name is EnableProjectColumnarSearch
                        icon is icon.window-value-charts-ruby
                    visible when (actor.context.FinanceEnterpriseGroup.EnableXPAFeatures)
                        action is DataCreation.CreateColumnarQueries
                            title is "CreateColumnarQueries"
                            menu item name is CreateColumnarQueries
                            icon is icon.window-value-charts-ruby
                    visible when (actor.context.FinanceEnterpriseGroup.EnableXPAFeatures)
                        list is DataCreationResult.CreateColumnarQueryResults
                            title is "ViewCreateColumnarQueryResults"
                            menu item name is ViewCreateColumnarQueryResults
                            icon is icon.window-value-charts-ruby

