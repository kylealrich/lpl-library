ContractEvaluationsPage is a Page
    title is "ContractEvaluations"
    implements MenuSearch

    MyOpenEvaluations is a Panel
        business class is SupplierPerformanceEvaluation
            list is MyOpenContractEvaluations

    MyEvaluations is a Panel
        business class is SupplierPerformanceEvaluation
            list is MyContractEvaluations

    Open is a Panel
        business class is SupplierPerformanceEvaluation
            list is OpenContractEvaluations

    All is a Panel
        business class is SupplierPerformanceEvaluation
            list is ContractEvaluations

    ContractsWithEvaluations is a Panel
        business class is Contract
            list is ContractsWithEvaluations

    Reporting is a Panel
        business class is SupplierPerformanceEvaluation
            list is ContractEvaluationReporting

    ReportingDetails is a Panel
        business class is SupplierPerformanceCriteriaResponse
            list is PerformanceCriteriaReporting

