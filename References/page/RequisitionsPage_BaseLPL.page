RequisitionsPage is a Page
    title is "ManageRequisitions"
    show panel control on left

    Unreleased is a Panel
        business class is Requisition
            list is UnreleasedRequisitions

    NeedsApproval is a Panel
        business class is Requisition
            list is NeedsApprovalRequisitions

    Processed is a Panel
        business class is Requisition
            list is ProcessedRequisitions

    InProcess is a Panel
        business class is Requisition
            list is InProcessRequisitions

    Rejected is a Panel
        business class is Requisition
            list is RejectedRequisitions

    All is a Panel
        business class is Requisition
            list is AllRequisitions

    Closed is a Panel
        business class is Requisition
            list is ClosedRequisitions

    SearchRequisitions is a Panel
        business class is Requisition
            list is RequisitionSearch

    SearchLines is a Panel
        business class is RequisitionLine
            list is RequisitionLineSearch

    SearchPOLines is a Panel
        business class is PurchaseOrderLineSource
            list is PurchaseOrdersFromRequisitions

    ContractCosts is a Panel
        business class is ContractLine
            list is ContractCostsForAnItem

    Commitments is a Panel
        business class is RequisitionLineDistribution
            list is RequisitionCommitments

    AuditHistory is a Panel
        title is "AuditHistory"
        visible when (not actor.context.FinanceEnterpriseGroup.ColumnarRequisitionAuditInUse)
        business class is Requisition
            list is RequisitionAuditList
                title is "AuditHistory"

    AuditHistoryColumnar is a Panel
        title is "AuditHistory"
        visible when (actor.context.FinanceEnterpriseGroup.ColumnarRequisitionAuditInUse)
        business class is Requisition
            list is ColumnarRequisitionAuditList
                title is "AuditHistory"

    LineAuditHistory is a Panel
        title is "LineAuditHistory"
        visible when (not actor.context.FinanceEnterpriseGroup.ColumnarRequisitionLineAuditInUse)
        business class is RequisitionLine
            list is RequisitionLineAuditList
                title is "LineAuditHistory"

    LineAuditHistoryColumnar is a Panel
        title is "LineAuditHistory"
        visible when (actor.context.FinanceEnterpriseGroup.ColumnarRequisitionLineAuditInUse)
        business class is RequisitionLine
            list is ColumnarRequisitionLineAuditList
                title is "LineAuditHistory"

