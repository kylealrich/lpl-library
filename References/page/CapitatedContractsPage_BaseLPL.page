CapitatedContractsPage is a Page
    title is "CapitatedContracts"

    Contracts is a Panel
        business class is Contract
            list is CapitatedContracts

    Procedures is a MultiPanePanel
        pane 1
            business class is ContractLine
                list is CapitatedProceduresOnPage
        pane 2
            business class is pane 1.ContractCapitatedComponentGroup set
                list is ContractCapitatedComponentGroupsOnPage
        pane 3
            business class is pane 2.ContractCapitatedComponentGroupItem set
                list is ContractCapitatedComponentGroupItemsOnPageMultiPane

    Components is a Panel
        business class is ContractCapitatedComponentGroupItem
            list is ContractCapitatedComponentGroupItemsOnPage

    CompanyAndLocationComponents is a Panel
        business class is ContractCapitatedCompanyLocationItem
            list is ContractCapitatedCompanyLocationItemsOnPage

