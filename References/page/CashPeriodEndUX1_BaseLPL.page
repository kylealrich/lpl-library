CashPeriodEndUX1 is a Page
    title is "PeriodEndClose"
    implements MenuSearch
    show panel control on left

    Main is a DashBoardPanel
        use 12 column grid
        Journalize is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        page is CashManagementJournalizePage.CashLedgerGLDistribution
                            title is "CashLedger"
                            menu item name is CashLedger
                            icon is icon.document-emerald
                        page is CashManagementJournalizePage.BankStatementLineDistribution
                            title is "BankStatementLine"
                            menu item name is BankStatementLine
                            icon is icon.document-emerald
                        page is CashManagementJournalizePage.ReconciliationVarianceDistributions
                            title is "ReconciliationVariance"
                            menu item name is NonCashLedgerReconciliationVariance
                            icon is icon.document-emerald
                        page is CashManagementJournalizePage.ViewJournalizedGLDistributions
                            title is "Results"
                            menu item name is Results
                            icon is icon.document-emerald
        Escheatment is a Pane
            span 8 columns
            Layout
                two column distributed
                    small menu
                        list is CashLedgerTransaction.StaleDateAndEscheatSearch
                            title is "StaleDateAndEscheatSearch"
                            menu item name is StaleDateAndEscheatSearch
                            icon is icon.document-amber
                        action is CashLedgerTransaction.StaleDateProcessing
                            title is "StaleDateProcessing"
                            menu item name is StaleDateProcessing
                            icon is icon.outline-process-amber
                        list is StaleDateOutputFileHeader.StaleDateOutputFileHeaderList
                            title is "StaleDateOutputFiles"
                            menu item name is StaleDateOutputFiles
                            icon is icon.document-amber
                        action is CashLedgerTransaction.EscheatmentProcessing
                            title is "EscheatmentProcessing"
                            menu item name is EscheatmentProcessing
                            icon is icon.outline-process-amber
                        list is EscheatmentOutputFileHeader.EscheatmentOutputFileHeaderList
                            title is "EscheatmentOutputFiles"
                            menu item name is EscheatmentOutputFiles
                            icon is icon.document-amber
                        action is GenerateIDMEscheatmentNotice.GenerateIDMEscheatmentNotice
                            title is "GenerateIDMEscheatmentNotice"
                            menu item name is GenerateIDMEscheatmentNotice
                            icon is icon.outline-process-amber
                        list is EscheatmentNoticeIDMHeader.EscheatmentNoticeIDMResultList
                            title is "IDMEscheatmentNoticeResults"
                            menu item name is IDMNoticeResults
                            icon is icon.document-amber
        PeriodClose is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        action is GeneralLedgerCompany.PerformCashLedgerCompanyPeriodClose
                            title is "Company"
                            menu item name is CashLedgerCompanyPeriodClose
                            icon is icon.outline-process-ruby
                        list is GeneralLedgerCompany.CashLedgerCompanyPeriodCloseCompaniesList
                            title is "CompanyStatus"
                            menu item name is CashLedgerCompanyPeriodCloseStatus
                            icon is icon.calendar-ruby
                        list is CashLedgerJobResult.CashLedgerCompanyPeriodCloseList
                            title is "CompanyResults"
                            menu item name is CashLedgerCompanyPeriodCloseResults
                            icon is icon.calendar-ruby
                        action is BankPeriodClose.BankPeriodClose
                            title is "Bank"
                            menu item name is BankPeriodClose
                            icon is icon.outline-process-ruby
                        visible when (actor.context.FinanceEnterpriseGroup.FundAccounting)
                            list is CashCodeFundPeriodBalance.CashCodeFundPeriodBalanceList
                                title is "CashCodeFundPeriodBalance"
                                menu item name is CashCodeFundPeriodBalance
                                icon is icon.calendar-ruby

