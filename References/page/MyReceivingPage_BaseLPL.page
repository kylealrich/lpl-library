MyReceivingPage is a Page
    title is "ManageMyPurchaseOrderReceipts"
    show panel control on left

    MyPOs is a MultiPanePanel
        title is "MyP\\Os"
        pane 1
            business class is PurchaseOrder
                list is MyPurchaseOrdersNotFullyReceived
        pane 2
            business class is pane 1.AllPurchaseOrderReceiptsRel
                list is MyPurchaseOrderReceiptsForPO

    MyPOLines is a MultiPanePanel
        pane 1
            business class is PurchaseOrderLine
                list is SearchMyPurchaseOrderLinesNotFullyReceived
        pane 2
            business class is pane 1.POReceiptLineTemplatesRel
                list is POReceiptLineTemplatesPOLine

    SearchPOs is a MultiPanePanel
        title is "SearchP\\Os"
        pane 1
            business class is PurchaseOrder
                list is RSSPurchaseOrderSearchList
        pane 2
            business class is pane 1.AllPurchaseOrderReceiptsRel
                list is RSSPurchaseOrderReceiptsForPO

    MyReceipts is a MultiPanePanel
        pane 1
            business class is PurchaseOrderReceipt
                list is OpenPurchaseOrderReceiptsForRequester
        pane 2
            business class is pane 1.POReceiptLineTemplate set
                list is POReceiptLineTemplatesForRequester

    MyReceiptLines is a Panel
        business class is PurchaseOrderReceiptLine
            list is POReceiptLinesForRequester

    SearchAllReceipts is a MultiPanePanel
        pane 1
            business class is PurchaseOrderReceipt
                list is RSSPurchaseOrderReceiptsSearchList
        pane 2
            business class is pane 1.POReceiptLineTemplate set
                list is RSSPOReceiptLineTemplates

