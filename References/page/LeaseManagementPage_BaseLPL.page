LeaseManagementPage is a Page
    title is "Overview"
    implements MenuSearch
    show panel control on left

    Dashboard is a DashBoardPanel
        use 12 column grid
        ImportantLinks is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        action is Lease.Unreleased.Create
                            title is "CreateNewLease"
                            menu item name is CreateNewLease
                            icon is icon.hcm-azure
                        page is ManageLease
                            title is "Leases"
                            menu item name is Leases
                            icon is icon.document-text-azure
                        list is LeasePayment.AllPaymentSchedules
                            title is "Payments"
                            menu item name is LeasePayments
                            icon is icon.cash-azure
                        list is LeasePaymentBalance.AllLeasePaymentBalanceList
                            title is "PaymentBalances"
                            menu item name is LeasePaymentBalances
                            icon is icon.chart-stacked-bar-azure
                        action is LeaseProjections.LeaseProjections
                            title is "Projections"
                            menu item name is LeaseProjections
                            icon is icon.chart-bar-azure
        Processing is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        action is CreateAPInvoicesForLeasePayments.CreateAPInvoicesForLeasePayments
                            title is "CreateInvoices"
                            menu item name is CreateInvoices
                            icon is icon.window-text-emerald
                        action is InterfaceLeasePaymentInvoicesToPayables.InterfaceLeasePaymentInvoicesToPayables
                            title is "InterfaceInvoicesToPayables"
                            menu item name is InterfaceInvoicesToPayables
                            icon is icon.payment-emerald
                        action is LeaseInterestCalculation.LeaseInterestCalculation
                            title is "CalculateInterest"
                            menu item name is LeaseInterestCalculation
                            icon is icon.calculator-emerald
                        action is LeasePosting.LeasePosting
                            title is "PostInvoices"
                            menu item name is LeasePosting
                            icon is icon.clipboard-check-emerald
                        action is LeasePeriodClose.LeasePeriodClose
                            title is "ClosePeriod"
                            menu item name is LeasePeriodClose
                            icon is icon.check-emerald
        Subleases is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        action is Sublease.Unreleased.Create
                            title is "CreateNewSublease"
                            menu item name is CreateNewSublease
                            icon is icon.hcm-ruby
                        list is Sublease.SubleaseListByCompany
                            title is "Subleases"
                            menu item name is Subleases
                            icon is icon.document-text-ruby
                        action is SubleaseBillingScheduleDetail.CreateInvoices
                            title is "CreateInvoices"
                            menu item name is Item22
                            icon is icon.window-text-ruby
                        action is SubleaseBillingScheduleDetail.PeriodProcessing
                            title is "PeriodProcessing"
                            menu item name is PeriodProcessing
                            icon is icon.check-ruby
        UnreleasedLeases is a Pane
            span 4 columns
            business class is Lease
                list is UnreleasedLeaseHomeCard
                    overview navigation is UnreleasedleasesNav
        NextLeaseDueDate is a Pane
            span 4 columns
            business class is LeasePaymentDetail
                list is NextLeaseDueDateList
                    overview navigation is NextDueDateNav
        LeasesEndingThisPeriod is a Pane
            span 4 columns
            business class is Lease
                list is LeaseEndingThisPeriodHome
                    title is "LeasesEndingInNext_30_Days"
                    overview navigation is LeasesEndingNav
        UnreleasedSubleases is a Pane
            span 4 columns
            business class is Sublease
                list is UnreleasedSubleaseHome
                    title is "UnreleasedSubleases"
        NextSubleasePayments is a Pane
            span 4 columns
            business class is Sublease
                list is NextSubleasePaymentHome
                    title is "NextSubleasePayment"
        SubleasesEndingThisPeriod is a Pane
            span 4 columns
            business class is Sublease
                list is SubleaseEndingThisPeriodHome
                    title is "SubleasesEndingInTheNext_30_Days"
        LeaseModifications is a Pane
            span 4 columns
            business class is LeaseModification
                list is ModificationHomePage
        LeaseAdjustments is a Pane
            span 4 columns
            business class is LeaseAdjustment
                list is AdjustmentsHomePage
        LeaseImpairments is a Pane
            span 4 columns
            business class is LeaseImpairment
                list is ImpairmentsHomePage
        UpcomingLeaseRenewalOptions is a Pane
            span 4 columns
            business class is Lease
                list is UpcomingLeaseRenewalsDashboard
                    overview navigation is UpcomingLeaseRenewals

    Invoices is a DashBoardPanel
        use 12 column grid
        LeaseInvoices is a Pane
            span 12 columns
            title is "Lease"
            business class is LeaseInvoice
                list is AllLeaseInvoiceList
        SubleaseInvoices is a Pane
            span 12 columns
            title is "Sublease"
            business class is SubleaseBillingInvoice
                list is SubleaseBillingInvoiceDashboardList

