ReconciliationSearchUX1 is a Page
    title is "Overview"
    implements MenuSearch

    Reconciliation is a DashBoardPanel
        title is "Reconciliation"
        use 12 column grid
        ManageReconciliations is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        page is ReconciliationDashBoardPage.OpenReconciliations
                            title is "OpenReconciliations"
                            menu item name is OpenReconciliations
                            icon is icon.clipboard-check-azure
                        page is ReconciliationDashBoardPage.OpenReconciliationsByStructure
                            title is "OpenReconciliationsByStructure"
                            menu item name is OpenReconciliationsByStructure
                            icon is icon.clipboard-check-azure
                        page is ReconciliationDashBoardPage.OpenReconciliationsByResource
                            title is "OpenReconciliationsByResource"
                            menu item name is OpenReconciliationsByResource
                            icon is icon.clipboard-check-azure
                        page is ReconciliationDashBoardPage.AllReconciliationsByPeriod
                            title is "AllReconciliationsByPeriod"
                            menu item name is AllReconciliationsByPeriod
                            icon is icon.clipboard-check-azure
                        page is ReconciliationDashBoardPage.ReconcilingItems
                            title is "ReconcilingItems"
                            menu item name is ReconcilingItems
                            icon is icon.clipboard-check-azure
                        page is ReconciliationDashBoardPage.OpenBankReconciliations
                            title is "OpenBankReconciliations"
                            menu item name is OpenBankReconciliations
                            icon is icon.clipboard-check-azure
        GeneralSetup is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        list is ReconciliationManagementGroup.ReconciliationManagementGroupList
                            title is "ReconciliationManagementGroup"
                            menu item name is ReconciliationManagementGroup
                            icon is icon.CSFSupplyManagement
                        list is BusinessClassGroup.BusinessClassGroups
                            title is "CustomGroups"
                            menu item name is CustomGroups
                            icon is icon.CSFSupplyManagement
                        list is ActionReason.ActionReasonList
                            title is "ActionReasons"
                            menu item name is ActionReasons
                            icon is icon.CSFSupplyManagement
                        page is FinanceResourceAdmin
                            title is "Resources"
                            menu item name is Resources
                            icon is icon.CSFSupplyManagement
                        list is FinanceTeam.BuildTeamList
                            title is "Teams"
                            menu item name is Teams
                            icon is icon.CSFSupplyManagement
        ReconciliationSetup is a Pane
            span 4 columns
            Layout
                two column distributed
                    small menu
                        list is ApprovalCode.ApprovalCodes
                            title is "ApprovalCodes"
                            menu item name is ApprovalCodes
                            icon is icon.CSFInventory
                        list is ReconciliationAccountGroup.ReconciliationAccountGroupList
                            title is "ReportGroups"
                            menu item name is ReportGroups
                            icon is icon.CSFInventory
                        list is ReconciliationPolicy.ReconciliationPolicies
                            title is "AutoReconciliationPolicies"
                            menu item name is AutoReconciliationPolicies
                            icon is icon.CSFInventory
                        list is DetailReconciliationMatchRule.DetailReconciliationMatchRules
                            title is "DetailReconciliationMatchRules"
                            menu item name is DetailReconciliationMatchRules
                            icon is icon.CSFInventory
                        list is DetailReconciliationMatchRuleGroup.DetailReconciliationMatchRuleGroupList
                            title is "DetailReconciliationMatchRuleGroups"
                            menu item name is DetailReconciliationMatchRuleGroups
                            icon is icon.CSFInventory
                        form is ReconciliationManagementGroup.ReconciliationAccountManagementForm
                            title is "ReconciliationAccounts"
                            menu item name is ReconciliationAccounts
                            icon is icon.CSFInventory
        MyReconciliations is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        page is ReconcilerPage.OpenReconciliations
                            title is "OpenReconciliations"
                            menu item name is OpenReconciliations
                            icon is icon.group-ruby
                        page is ReconcilerPage.TeamReconciliations
                            title is "TeamReconciliations"
                            menu item name is TeamReconciliations
                            icon is icon.group-ruby
                        page is ReconcilerPage.ClosedReconciliations
                            title is "ClosedReconciliations"
                            menu item name is ClosedReconciliations
                            icon is icon.group-ruby

    PeriodReconciliations is a Panel
        business class is ReconciliationAccountStructure
            list is ClosedReconciliationAccountStructureCardPeriodList

