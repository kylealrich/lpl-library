RequesterHomePage is a Page
    title is "<actor.agent(Employee).Employee.Requester.RSSLPLHomePageTitle>"
    implements MenuSearch
    show panel control on left

    RequisitionSelfService is a DashBoardPanel
        title is "RequisitionSelfService"
        use 12 column grid
        visible when (actor.agent(Employee).Employee.Requester.RQMenuAndRSSLPLEnabled)
        StartRequisitions is a Pane
            span 3 columns
            title is "StartRequisitions"
            Layout
                single column
                    page is RequesterShoppingPage
                        title is "StartShopping"
                        menu item name is StartShopping
                        icon is icon.CSFBuyer
                    action is Requisition.Unreleased.RSSLPLCreate
                        title is "CreateNewRequisition"
                        menu item name is CreateNewRequisition
                        icon is icon.target-amethyst
                    action is Requisition.Unreleased.RSSLPLCreateUsingCopyFromRequisition
                        title is "CopyRequisition"
                        menu item name is CopyRequisition
                        icon is icon.documents-emerald
                    visible when (not actor.agent(Employee).Employee.Requester.HideShoppingList)
                        page is RequesterShoppingPage.AddFromShoppingLists
                            title is "Add_fromShoppingLists"
                            menu item name is AddFromShoppingLists
                            icon is icon.clipboard-check-amber
                    visible when (not actor.agent(Employee).Employee.Requester.HideCatalogSearch)
                        page is RequesterShoppingPage.SearchTheCatalog
                            title is "SearchTheCatalog"
                            menu item name is SearchTheCatalog
                            icon is icon.CSFSupplyManagementViewer
                    visible when (not actor.agent(Employee).Employee.Requester.HideShopPunchoutVendors)
                        page is RequesterShoppingPage.ShopPunchoutVendors
                            title is "ShopPunchoutVendors"
                            menu item name is ShopPunchoutVendors
                            icon is icon.directory-azure
                    visible when (actor.agent(Employee).Employee.Requester.CanCreateCapitatedRequisition)
                        form is Requisition.CreateForCapitatedContract
                            title is "CreateCapitatedRequisition"
                            menu item name is CreateCapitatedRequisition
                            icon is icon.directory-amethyst
        MyUnreleasedRequisitions is a Pane
            span 3 columns
            business class is Requisition
                list is RSSLPLUnreleasedRequisitions
                    overview navigation is RSSLPLUnreleasedRequisitionsNav
        MyRequisitionsNotFullyReceived is a Pane
            span 6 columns
            business class is Requisition
                list is RSSLPLInProccessRequisitions
                    overview navigation is RSSLPLInProccessRequisitionsNav
        MyRecentlyRequestedItems is a Pane
            span 6 columns
            business class is RequisitionLine
                list is RSSLPLRecentlyRequestedItems
                    overview navigation is RSSLPLRecentlyRequestedItemsNav
        MyRecentlyRequestedSpecial_andServiceItems is a Pane
            span 6 columns
            business class is RequisitionLine
                list is RSSLPLRecentlyRequestedSpecialItems
                    overview navigation is RSSLPLRecentlyRequestedSpecialtemsNav

    RequesterHomePage is a DashBoardPanel
        title is "Requisitions"
        use 12 column grid
        visible when (not actor.context.FinanceEnterpriseGroup.FSM251004RQMenuEnabled)
        MyRQ is a Pane
            span 6 columns
            title is "MyRequisitionLinesPastRequestedDeliveryDate"
            business class is RequisitionLine
                list is MyRequisitionLinesPastRequestedDeliveryDate
        ApprovalNeeded is a Pane
            span 6 columns
            title is "RequisitionsNeedingApproval"
            business class is Requisition
                list is MyNeedsApprovalRequisitions
        Unreleased is a Pane
            span 6 columns
            title is "UnreleasedRequisitions"
            business class is Requisition
                list is MyUnreleasedRequisitions
        SupplierReview is a Pane
            span 6 columns
            title is "SupplierRequisitionsToReview"
            business class is Requisition
                list is SupplierRequisitionsToReview

    Requisitions is a DashBoardPanel
        title is "Requisitions"
        use 12 column grid
        visible when (actor.context.FinanceEnterpriseGroup.FSM251004RQMenuEnabled)
        Create is a Pane
            span 3 columns
            icon is icon.outline-list-azure
            Layout
                single column
                    action is Requisition.Unreleased.Create
                        title is "CreateRequisition"
                        menu item name is AdditionalBookImport
                    visible when (actor.agent(Employee).Employee.Requester.CanCreateCapitatedRequisition)
                        visible when (not actor.agent(Employee).Employee.Buyer.CanCreateCapitatedRequisition)
                            form is Requisition.CreateForCapitatedContract
                                title is "CreateCapitatedRequisition"
                                menu item name is CreateCapitatedRequisition
                    visible when (actor.agent(Employee).Employee.Buyer.CanCreateCapitatedRequisition)
                        visible when (not actor.agent(Employee).Employee.Requester.CanCreateCapitatedRequisition)
                            form is Requisition.CreateForCapitatedContract
                                title is "CreateCapitatedRequisition"
                                menu item name is CreateCapitatedRequisition
                    visible when (actor.agent(Employee).Employee.Buyer.CanCreateCapitatedRequisition)
                        visible when (actor.agent(Employee).Employee.Requester.CanCreateCapitatedRequisition)
                            form is Requisition.CreateForCapitatedContract
                                title is "CreateCapitatedRequisition"
                                menu item name is CreateCapitatedRequisition
        QuickLinks is a Pane
            span 3 columns
            icon is icon.window-box-azure
            Layout
                single column
                    list is RequisitionLine.MyRequisitionLinesPastRequestedDeliveryDate
                        title is "LateRequisitionLines"
                        menu item name is MyRequisitionLinesPastRequestedDeliveryDate
                    list is Requisition.MyNeedsApprovalRequisitions
                        title is "RequisitionsNeedingApproval"
                        menu item name is RequisitionsNeedingApproval
                    list is Requisition.MyUnreleasedRequisitions
                        title is "UnreleasedRequisitions"
                        menu item name is UnreleasedRequisitions
                    page is RequesterManageRequisitions
                        title is "AllRequisitionsForMyRequestingLocations"
                        menu item name is AllRequisitionsForMyRequestingLocations
                    page is MyRequisitionsPage
                        title is "MyRequisitions"
                        menu item name is MyRequisitions
                    page is RequisitionsPage
                        title is "ManageRequisitions"
                        menu item name is ManageRequisitions
                    page is SupplierRequisitionsPage
                        title is "SupplierRequisitions"
                        menu item name is SupplierRequisitions
                    action is Requisition.UpdateRequisitionCubes
                        title is "RefreshRequisitionCubes"
                        menu item name is RefreshRequisitionCubes
                        Search Terms
                                "UpdateRequisitionCubes"
                                "RefreshRequisitionCubes"
        Contracts is a Pane
            span 3 columns
            title is "Contracts"
            icon is icon.documents-azure
            Layout
                single column
                    list is ContractLine.ContractCostsForAnItem
                        title is "SearchContractCosts"
                        menu item name is ContractCostsForAnItem
                    action is Contract.Draft.RequesterCreateProposedContract
                        title is "ProposeAContract"
                        menu item name is ProposeAContract
                    list is Contract.RequesterProposedContracts
                        title is "ViewMyProposedContracts"
                        menu item name is ViewMyProposedContracts
                    list is Contract.ContractsEligibleForChangeOrders
                        title is "CreateAChangeOrder"
                        menu item name is CreateAChangeOrder
                    list is ContractVersion.RequesterChangeOrders
                        title is "ViewMyChangeOrders"
                        menu item name is RequesterChangeOrders
        General is a Pane
            span 3 columns
            title is "General"
            icon is icon.settings-azure
            Layout
                single column
                    list is ProcurementTemplate.RequesterProcurementTemplateList
                        title is "ProcurementTemplates"
                        menu item name is ProcurementTemplate
                    visible when (not actor.context.FinanceEnterpriseGroup.ColumnarRequisitionAuditInUse)
                        list is Requisition.MyRequisitionAuditList
                            title is "AuditHistory"
                            menu item name is RequisitionAuditHistory
                    visible when (actor.context.FinanceEnterpriseGroup.ColumnarRequisitionAuditInUse)
                        list is Requisition.ColumnarMyRequisitionAuditList
                            title is "AuditHistory"
                            menu item name is MyRequisitionAuditHistory
                    visible when (not actor.context.FinanceEnterpriseGroup.ColumnarRequisitionLineAuditInUse)
                        list is RequisitionLine.MyRequisitionLineAuditList
                            title is "LineAuditHistory"
                            menu item name is RequisitionLineAuditHistory
                    visible when (actor.context.FinanceEnterpriseGroup.ColumnarRequisitionLineAuditInUse)
                        list is RequisitionLine.ColumnarMyRequisitionLineAuditList
                            title is "LineAuditHistory"
                            menu item name is MyRequisitionLineAuditHistory
                    list is SampleDocumentTemplate.SampleTemplatesForRequisition
                        title is "SampleDocumentTemplates"
                        menu item name is SampleDocumentTemplatesForRequisitions
        NewRequests is a Pane
            span 3 columns
            title is "NewRequests"
            icon is icon.inbox-azure
            Layout
                single column
                    action is Item.RequestNewItem
                        title is "Item"
                        menu item name is RequestNewItem
                    action is Vendor.RequestNewVendor
                        title is "Vendor"
                        menu item name is RequestNewVendor
                    action is PayablesInvoice.RequestNewInvoicePayment
                        title is "InvoicePayment"
                        menu item name is InvoicePayment
        ItemLifecycle is a Pane
            span 3 columns
            icon is icon.outline-maintenance-azure
            Layout
                single column
                    action is RecallNotice.Pending.RecallPortalCreate
                        title is "ReportANotice"
                        menu item name is ReportANotice
                    list is RecallNotice.MyReportedRecallPortalNotices
                        title is "ViewMyNotices"
                        menu item name is ViewMyNotices
                    action is RecallNotice.Pending.RequesterCreateItemProposal
                        title is "ProposeNewItem"
                        menu item name is ProposeNewItem
                    list is RecallNotice.RequesterItemProposals
                        title is "ViewMyItemProposals"
                        menu item name is ViewMyItemProposals
                    list is RecallNotice.ItemProposals
                        title is "AllItemProposals"
                        menu item name is AllItemProposals
                    list is RecallNoticeQandA.MyUnansweredQandA
                        title is "MyProposalQAndAToAnswer"
                        menu item name is MyQAndAToAnswer
                    list is RecallNoticeQandA.ItemProposalQAndAList
                        title is "AllProposalQAndA"
                        menu item name is AllProposalQAndA
        OtherProcesses is a Pane
            span 3 columns
            icon is icon.cycle-azure
            Layout
                single column
                    page is ProcurementCardTransactionApprovalPage
                        title is "ApproveProcurementCardTransactions"
                        menu item name is ApproveProcurementCardTransactions
                    page is CanceledPurchaseOrderLinesPage
                        title is "CanceledPurchaseOrderLines"
                        menu item name is CanceledPurchaseOrderLines
                    page is IDMPrinterPage
                        title is "IDMPrinters"
                        menu item name is IDMPrinters
                    page is MyIDMJobsPage
                        title is "MyIDMJobs"
                        menu item name is MyIDMJobs
                    page is ReceivingUX1
                        title is "Receiving"
                        menu item name is Receiving
                    visible when (actor.agent(Employee).Employee.Requester.SupplierRequisitionReviewer)
                        page is SupplierMessagePage
                            title is "SupplierMessagesAndDialog"
                            menu item name is MessagesAndDialog

    ListsAndReports is a DashBoardPanel
        title is "Lists_andReports"
        use 12 column grid
        visible when (actor.context.FinanceEnterpriseGroup.FSM251004RQMenuEnabled)
        Requisitions is a Pane
            span 3 columns
            title is "Requisitions"
            icon is icon.document-azure
            Layout
                single column
                    list is Requisition.AllRequisitions
                        title is "AllRequisitions"
                        menu item name is AllRequisitions
                    action is DepartmentUsageReport.DepartmentUsageReport
                        title is "DepartmentUsageReport"
                        menu item name is DepartmentUsageReport
                    list is RequisitionLine.KilledItemsList
                        title is "KilledItems"
                        menu item name is KilledItems
                    action is RQFillRateAnalysis.RQFillRateAnalysis
                        title is "RequisitionFillRateAnalysis"
                        menu item name is RequisitionFillRateAnalysis
                    page is MyRequisitionStatuses
                        title is "RequisitionStatuses"
                        menu item name is RequisitionStatuses
                    action is RequisitionVolumeSummaryReport.RequisitionVolumeSummaryReport
                        title is "RequisitionVolumeSummaryReport"
                        menu item name is RequisitionVolumeSummaryReport
                    action is RequisitionedItemsReport.RequisitionedItemsReport
                        title is "RequisitionedItemsReport"
                        menu item name is RequisitionedItemsReport

