DirectForecastPage is a Page
    title is "DirectForecasts"
    is report for CashManager,PayablesManager,ApplicationAdministrator,CashAnalyst,FinancialsViewer,FinancialsProcessor,PayablesProcessor,Cash
        module is cb
    DirectForecasts is a MultiPanePanel
        pane 1
            business class is DirectForecast
                list is DirectForecastList
        pane 3
            business class is pane 1.DirectForecastPeriod set
                list is DirectForecastPeriodChart
        pane 4
            business class is pane 1.DirectForecastPeriod set
                list is DirectForecastPeriodList

