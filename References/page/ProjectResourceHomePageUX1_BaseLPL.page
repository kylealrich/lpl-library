ProjectResourceHomePageUX1 is a Page
    title is "EffortEntryAndCertification"
    implements MenuSearch

    ProjectResourceAndCertifier is a DashBoardPanel
        title is "LaborDistribution"
        use 12 column grid
        LaborDistribution is a Pane
            span 6 columns
            Layout
                single column
                    column1
                        large menu
                            page is ProjectResourceLaborDistribution
                                title is "ProjectLabor"
                                menu item name is MyLaborDistribution
                                description is "View_open_labor_assignments_for_grant_sponsored_and_non-sponsored_projects._Enter_and_submit_actual_effort_percentages_for_the_period_for_labor_distribution."
                                icon is icon.person-global-azure
                        large menu
                            page is ProjectRedistributionLabor
                                title is "RedistributeLabor"
                                menu item name is MyRedistributeLabor
                                description is "View_processed_labor_distributions._Enter_and_submit_any_adjustments_to_actual_effort_percentages_for_labor_cost_transfers_or_redistribution_of_original_expenses."
                                icon is icon.person-global-azure
                        large menu
                            page is MyProjectResourceLaborDistribution
                                title is "EmployeesLaborDistributionAndRedistribution"
                                menu item name is MyLaborDistributionApprovals
                                description is "Enter_and_approve_labor_distributions_and_redistibutions_for_employees_that_I_am_the_approver."
                                icon is icon.person-global-azure
        EffortCertification is a Pane
            span 6 columns
            Layout
                single column
                    column1
                        large menu
                            page is ProjectResourceEffortCertification
                                title is "EffortCertification"
                                menu item name is MyEffortCertification
                                description is "View_processed_labor_distribution_planned_and_actual_effort_percentages_by_period._Enter_effort_percentages_for_grant_effort_reporting."
                                icon is icon.person-global-emerald
                        large menu
                            page is ProjectCertifierPage
                                title is "EmployeesEffortCertification"
                                menu item name is EffortCertification
                                description is "Enter_and_certify_effort_for_employees_that_I_am_the_approver."
                                icon is icon.person-global-emerald

