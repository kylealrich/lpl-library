UX1AssetReports is a Page
    title is "Reports"
    implements MenuSearch

    RunReports is a DashBoardPanel
        title is "Run"
        use 12 column grid
        General is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        page is AssetProjectReportPage
                            title is "Projects"
                            menu item name is AssetProjectReport
                            description is "Reconcile_journal_entries_with_project_codes"
                            icon is icon.document-azure
                            Search Terms
                                    "AssetProjectReport"
                        action is AssetProjectReport.AssetProjectReport
                            title is "Projects-\\Old"
                            menu item name is AssetProjectReportOld
                            description is "Reconcile_journal_entries_with_project_codes"
                            icon is icon.document-azure
                            Search Terms
                                    "AssetProjectReport-\\Old"
                        action is AssetReport.AssetReport
                            title is "Assets"
                            menu item name is AssetReport
                            description is "Reconcile_asset_financial_information_by_book"
                            icon is icon.document-azure
                            Search Terms
                                    "AssetReport"
                        action is LeasedAssetReport.LeasedAssetReport
                            title is "LeasedAssets"
                            menu item name is LeasedAssetReport
                            icon is icon.document-azure
                            Search Terms
                                    "LeasedAssetReport"
                        action is AssetTransactionReport.AssetTransactionReport
                            title is "Transactions"
                            menu item name is AssetTransactionReport
                            icon is icon.document-azure
                            Search Terms
                                    "AssetTransactionReport"
                        list is AssetBook.AssetRegisterReport
                            title is "Register"
                            menu item name is AssetRegister
                            description is "List_disposed_assets_that_were_reinstated"
                            icon is icon.document-azure
                            Search Terms
                                    "AssetRegister"
                        action is AssetRegister.AssetRegister
                            title is "Register-Old"
                            menu item name is AssetRegisterOld
                            description is "List_disposed_assets_that_were_reinstated"
                            icon is icon.document-azure
                            Search Terms
                                    "AssetRegister"
        Depreciation is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        list is AssetTransaction.TransactionColumnarList
                            title is "PostingBookDepreciation"
                            menu item name is PostingBookDepreciation
                            icon is icon.document-emerald
                            Search Terms
                                    "AssetTransactionColumnarReport"
                                    "TransactionColumnarList"
                                    "PostingBookDepreciation"
                        list is AssetBookTransaction.BookTransactionColumnarList
                            title is "NonPostingBookDepreciation"
                            menu item name is NonPostingBookDepreciation
                            icon is icon.document-emerald
                            Search Terms
                                    "AssetBookTransactionColumnarReport"
                                    "BookTransactionColumnarList"
                                    "NonPostingBookDepreciation"
                        action is ClassDepreciationReport.ClassDepreciationReport
                            title is "ClassDepreciation"
                            menu item name is ClassDepreciationReport
                            description is "View_depreciation_for_asset_classes_by_company,transaction_dimension,and_asset_account"
                            icon is icon.document-emerald
                            Search Terms
                                    "ClassDepreciationReport"
                        action is CurrentYearDepreciationReport.CurrentYearDepreciationReport
                            title is "CurrentYearDepreciation"
                            menu item name is CurrentYearDepreciation
                            description is "View_book_depreciation_information_for_the_current_year"
                            icon is icon.document-emerald
                            Search Terms
                                    "CurrentYearDepreciation"
                        action is DepreciableAssetRegister.DepreciableAssetRegister
                            title is "DepreciableAssetRegister"
                            menu item name is DepreciableAssetRegister
                            description is "View_year-to-date_depreciation"
                            icon is icon.document-emerald
                            Search Terms
                                    "DepreciableAssetRegister"
                        action is DepreciationHistoryReport.DepreciationHistoryReport
                            title is "DepreciationHistory"
                            menu item name is DepreciationHistoryReport
                            description is "View_historical_year-to-date_and_life-to-date_depreciation_information"
                            icon is icon.document-emerald
                            Search Terms
                                    "DepreciationHistoryReport"
                        action is AssetProjectionReport.AssetProjectionReport
                            title is "Projections"
                            menu item name is AssetProjectionReport
                            description is "Calculate_future_depreciation_for_assets_by_book_for_budgeting,forecasting,or_other_planning_purposes"
                            icon is icon.document-emerald
                            Search Terms
                                    "AssetProjectionReport"
                                    "AssetProjection"
                        action is AssetDepreciationCalculationAndList.AssetDepreciationCalculationAndList
                            title is "DepreciationCalculation"
                            menu item name is DepreciationCalculation
                            icon is icon.document-emerald
                            Search Terms
                                    "DepreciationCalculation"
                        action is DepreciationRecalculationReport.DepreciationRecalculationReport
                            title is "DepreciationRecalculation"
                            menu item name is DepreciationRecalculation
                            icon is icon.document-emerald
                            Search Terms
                                    "DepreciationRecalculation"
        Tax is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        action is AssetTaxCreditReport.AssetTaxCreditReport
                            title is "TaxCredit"
                            menu item name is AssetTaxCreditReport
                            icon is icon.document-amethyst
                            Search Terms
                                    "AssetTaxCreditReport"
                                    "TaxCredits"
                        action is PropertyReport.PropertyReport
                            title is "Property"
                            menu item name is PropertyReport
                            icon is icon.document-amethyst
                            Search Terms
                                    "PropertyReport"
                        action is AssetRecaptureReport.AssetRecaptureReport
                            title is "Recapture"
                            menu item name is AssetRecaptureReport
                            description is "View_depreciation_and_tax_credit_recapture_by_book"
                            icon is icon.document-amethyst
                            Search Terms
                                    "AssetRecaptureReport"
                        action is FASBReconciliationReportForAssets.FASBReconciliationReportForAssets
                            title is "FASBReconciliation"
                            menu item name is FASBReconciliation
                            icon is icon.document-amethyst
                            Search Terms
                                    "FASBReconciliation"
                        action is BookComparisonReport.BookComparisonReport
                            title is "BookComparison"
                            menu item name is BookComparison
                            icon is icon.document-amethyst
                            Search Terms
                                    "BookComparison"

    MyReports is a Panel
        title is "View"
        business class is UserReport
            list is MyUserReportsList
                title is "View"

