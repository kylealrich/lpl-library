KitchenManagerInventoryInformationPage is a Page
    title is "InventoryInformation"
    show panel control on left

    StockOnHand is a Panel
        business class is ItemLocation
            list is KitchenItemLocationStockOnHand

    LotsAndBins is a Panel
        business class is StockOnHandDetail
            list is KitchenStockOnHandForLotAndSerial

    SerialNumbers is a Panel
        business class is ItemSerialNumber
            list is KitchenSerialNumbersOnHand

    TransactionHistory is a Panel
        business class is InventoryTransactionLine
            list is KitchenReleasedTransactions

    CostHistory is a Panel
        business class is InventoryCostHistory
            list is KitchenCostHistory

    DemandAndSupply is a Panel
        business class is WarehouseDemandLine
            list is KitchenWarehouseDemandAndSupply

