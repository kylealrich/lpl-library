UX1InventoryListAndAnalysisPage is a Page
    title is "ListAndAnalysis"
    implements MenuSearch

    Main is a DashBoardPanel
        use 12 column grid
        InventoryAnalysis is a Pane
            span 6 columns
            title is "Analysis"
            Layout
                two column distributed
                    column1
                        small menu
                            action is InventoryAccuracyProgress.InventoryAccuracyProgress
                                title is "AccuracyProgress"
                                menu item name is AccuracyProgress
                                icon is icon.window-value-charts-azure
                            action is InventoryForecastVariance.InventoryForecastVariance
                                title is "ForecastVariance"
                                menu item name is ForecastVariance
                                icon is icon.window-time-charts-azure
                            action is InventoryMovementAnalysisCost.InventoryMovementAnalysisCost
                                title is "MovementAnalysisCost"
                                menu item name is MovementAnalysisCost
                                icon is icon.CSFFinancialBusinessAnalyst
                            action is InventoryMovementAnalysisGMROI.InventoryMovementAnalysisGMROI
                                title is "MovementAnalysisGMROI"
                                menu item name is MovementAnalysisGMROI
                                icon is icon.CSFController
                    column2
                        small menu
                            action is InventoryMovementAnalysisSales.InventoryMovementAnalysisSales
                                title is "MovementAnalysisSales"
                                menu item name is MovementAnalysisSales
                                icon is icon.chart-stacked-bar-azure
                            action is InventoryOverallocatedItems.InventoryOverallocatedItems
                                title is "OverallocatedItems"
                                menu item name is OverallocatedItems
                                icon is icon.chart-bar-azure
                            list is ItemLocation.LeadtimeReviewList
                                title is "LeadtimeReview"
                                menu item name is LeadtimeReview
                                icon is icon.clipboard-check-azure
                            list is GMDNCategorySpend.CategorySpend
                                title is "GMDNCategorySpend"
                                menu item name is GMDNCategorySpend
                                icon is icon.chart-bar-azure
        ParUtilization is a Pane
            span 3 columns
            Layout
                two column distributed
                    large menu
                        action is ParLocationUtilization.ParLocationUtilization
                            title is "ParLocationUtilization"
                            menu item name is ParLocationUtilization
                            description is "Generate_report_of_recommended_par_utilization"
                            icon is icon.chart-stacked-bar-amber
                        action is DetailParLocationUtilization.DetailParLocationUtilization
                            title is "DetailParLocationUtilization"
                            menu item name is DetailParLocationUtilization
                            description is "Generate_report_of_detail_usage_of_an_item"
                            icon is icon.window-value-charts-amber
        Turnover is a Pane
            span 3 columns
            Layout
                single column
                    large menu
                        action is InventoryTurnoverbyIssues.InventoryTurnoverbyIssues
                            title is "TurnoverByIssues"
                            menu item name is TurnoverByIssues
                            description is "Genearte_report_of_inventory_turnover_based_on_item_issues"
                            icon is icon.briefcases-amethyst
                        action is InventoryTurnover.InventoryTurnover
                            title is "InventoryTurnover"
                            menu item name is InventoryTurnover
                            description is "Generate_report_of_inventory_turnover_based_on_item_sales"
                            icon is icon.briefcases-amethyst
        List is a Pane
            span 6 columns
            title is "Lists"
            Layout
                two column distributed
                    column1
                        large menu
                            list is MSDSAcknowledgement.MSDSAcknowledgementList
                                title is "MSDSAcknowledgement"
                                menu item name is MSDSAcknowledgement
                                description is "View_list_of_customers_who_have_received_the_required_MSDS_for_items"
                                icon is icon.clipboard-text-emerald
                            list is InventoryTransactionLine.InventoryIssuesList
                                title is "IssuesList"
                                menu item name is IssuesList
                                description is "View_list_of_Inventory_Issues"
                                icon is icon.clipboard-text-emerald
                            action is ParInventoryWorksheet.ParInventoryWorksheet
                                title is "ParInventoryWorksheet"
                                menu item name is ParInventoryWorksheet
                                description is "Print_either_a_worksheet_or_alphabetic_locator_list"
                                icon is icon.window-text-emerald
                    column2
                        large menu
                            list is WarehouseDemandLine.WarehouseItemsOnBackorder
                                title is "WarehouseItemsOnBackorder"
                                menu item name is WarehouseItemsOnBackorder
                                description is "View_list_of_items_that_have_been_backordered_for_shipments_from_the_specified_inventory_location"
                                icon is icon.clipboard-text-emerald
                            list is RequisitionLine.KilledItemsList
                                title is "KilledItems"
                                menu item name is KilledItems
                                description is "View_list_of_unprocessed_items_from_Requisition_Line_with_'kill'_backorder_setup"
                                icon is icon.clipboard-text-emerald
                            action is InventoryIssueDocument.InventoryIssueDocument
                                title is "IssueDocument"
                                menu item name is IssueDocument
                                description is "Review_transactions_before_they_are_updated_to_Inventory"
                                icon is icon.window-text-emerald
        Valuation is a Pane
            span 6 columns
            Layout
                two column distributed
                    column1
                        large menu
                            action is InventoryValuation.InventoryValuation
                                title is "InventoryValuation"
                                menu item name is InventoryValuation
                                description is "Generate_a_report_of_inventory_valuation"
                                icon is icon.chart-stacked-bar-ruby
                            action is PeriodEndValuation.PeriodEndValuation
                                title is "PeriodEnd"
                                menu item name is PeriodEndValuation
                                description is "Valuate_inventory_as_of_a_specific_date"
                                icon is icon.window-value-charts-ruby
                            action is ParLocationValuation.ParLocationValuation
                                title is "ParLocation"
                                menu item name is ParLocationValuation
                                description is "Generate_a_report_that_calculates_the_value_of_all_items_in_a_cart_or_par_location"
                                icon is icon.home-ruby
                    column2
                        large menu
                            action is MarketInventoryValuation.MarketInventoryValuation
                                title is "MarketInventoryValuation"
                                menu item name is MarketInventoryValuation
                                description is "Generate_item_material_cost,_item_overhead_cost,_and_item_average_unit_add_on_cost_at_a_given_inventory_location"
                                icon is icon.window-time-charts-ruby
                            action is IFRSFIFOPeriodEndValuation.IFRSFIFOPeriodEndValuation
                                title is "IFRS_FIFOPeriodEnd"
                                menu item name is IFRSFIFOPeriodEndValuation
                                description is "Generate_FIFO_inventory_valuation_regardless_of_the_company_inventory_valuation_method_settings"
                                icon is icon.chart-bar-ruby
                            list is ItemLocation.StandardCostValuationPreview
                                title is "StandardCostValuation"
                                menu item name is StandardCostValuationPreview
                                description is "List_that_valuates_current_inventory_stock_on_hand_quantity_based_on_standard_and_future_costs_defined_for_items"
                                icon is icon.book-chart-ruby
        MobileSupplyChain is a Pane
            span 6 columns
            visible when (actor.context.FinanceEnterpriseGroup.ShowMobileSupplyChain)
            Layout
                two column distributed
                    large menu
                        page is MobileSupplyChainUploadsPage.PickForPar
                            title is "PickForPar"
                            menu item name is PickForPar
                            description is "View_list_of_pick_tickets_generated_for_par_locations_tagged_as_PFP"
                            icon is icon.clipboard-text-emerald
                        page is CycleCountUploadPage.CycleCounts
                            title is "CycleCount"
                            menu item name is CycleCount
                            description is "View_list_of_cycle_counts_done_via_MSCM_application_in_handheld"
                            icon is icon.clipboard-text-emerald
                        page is CycleCountUploadPage.ParCycleCount
                            title is "ParCycleCount"
                            menu item name is ParCycleCount
                            description is "View_list_of_cycle_count_for_par_location_transactions_submitted_via_MSCM_handheld"
                            icon is icon.clipboard-text-emerald
                        page is MobileSupplyChainInventoryReplenishmentPage
                            title is "InventoryReplenishment"
                            menu item name is InventoryReplenishment
                            description is "View_list_of_replenishment_transactions_for_inventory_locations_done_via_MSCM_handheld"
                            icon is icon.clipboard-text-emerald
                        page is MobileSupplyChainInventoryTransactionPage
                            title is "InventoryTransactions"
                            menu item name is InventoryTransactions
                            description is "View_list_of_inventory_issues,_returns,_adjustments_or_transfers_transacted_in_MSCM_handheld"
                            icon is icon.clipboard-text-emerald
                        page is MobileSupplyChainShipment
                            title is "Picking"
                            menu item name is Picking
                            description is "View_list_of_shipment_transactions_done_thru_MSCM."
                            icon is icon.clipboard-text-emerald
                        page is DeliveryTrackingPage
                            title is "DeliveryTracking"
                            menu item name is DeliveryTracking
                            description is "View_list_of_MSCM_PO_Receiving,_Shipment_and_Delivery_events_for_monitoring_and_status_checking."
                            icon is icon.clipboard-text-emerald

    Reports is a Panel
        Layout
            three column distributed
                column1
                    large menu
                        action is InventoryAvailabilityReport.InventoryAvailabilityReport
                            title is "Availability"
                            menu item name is InventoryAvailabilityReport
                            description is "Print_a_report_of_items_available_quantities_by_location_based_on_the_available_calculation_defined_for_the_company"
                            icon is icon.outline-report-amethyst
                        action is InventoryVarianceExplanations.InventoryVarianceExplanations
                            title is "VarianceExplanations"
                            menu item name is InventoryVarianceExplanations
                            description is "Run_Variance_Explanations_to_print_a_list_of_explanations_by_reason_code_for_cycle_count_variances"
                            icon is icon.outline-report-amethyst
                        action is InventoryStockStatusReport.InventoryStockStatusReport
                            title is "StockStatus"
                            menu item name is InventoryStockStatusReport
                            description is "Run_Stock_Status_Report_to_print_a_report_of_item_availability_based_on_the_actual_supply_and_demand"
                            icon is icon.outline-report-amethyst
                column2
                    large menu
                        action is InventoryGrossMarginReturnOnInvestment.InventoryGrossMarginReturnOnInvestment
                            title is "GrossMarginReturnOnInvestment"
                            menu item name is GrossMarginReturnOnInvestment
                            description is "Print_a_report_of_gross_margin_return_on_investment"
                            icon is icon.outline-report-amethyst
                        action is RequestingLocationUsage.RequestingLocationUsage
                            title is "RequestingLocationUsage"
                            menu item name is RequestingLocationUsage
                            description is "View_the_requesting_location_or_departmental_demand_on_items_over_12_periods"
                            icon is icon.outline-report-amethyst
                column3
                    large menu
                        action is InventoryIssueSummaryReport.InventoryIssueSummaryReport
                            title is "IssueSummary"
                            menu item name is InventoryIssueSummaryReport
                            description is "Run_to_list_all_issues_and_requisition_returns_by_dimension_or_account"
                            icon is icon.outline-report-amethyst
                        action is WarehouseItemsOnBackorderReport.WarehouseItemsOnBackorderReport
                            title is "Items_onBackorder"
                            menu item name is WarehouseItemsOnBackorderReport
                            description is "Print_a_report_of_items_that_have_been_backordered_for_shipments_from_the_specified_inventory_location"
                            icon is icon.outline-report-amethyst

    HistoricalData is a DashBoardPanel
        use 12 column grid
        HistoricalReports is a Pane
            span 3 columns
            Layout
                single column
                    large menu
                        list is InventoryCostHistory.SearchInventoryCostHistory
                            title is "InventoryCost"
                            menu item name is SearchInventoryCostHistory
                            description is "View_Cost_history_for_inventory_quantities_received_at_a_specific_location"
                            icon is icon.calculator-amber
                        action is InventoryDistributionHistoryReport.InventoryDistributionHistoryReport
                            title is "InventoryDistribution"
                            menu item name is InventoryDistributionHistoryReport
                            description is "Run_to_print_a_report_of_every_inventory_transaction_for_the_month"
                            icon is icon.clipboard-check-amber
                        list is InventoryUsageHistory.InventoryUsageHistoryList
                            title is "InventoryUsage"
                            menu item name is InventoryUsageHistoryList
                            description is "View_the_usage_history_like_receipts,_adjustments,_transfers,_issues_plus_sales_for_a_specific_item_for_a_specified_year"
                            icon is icon.compare-amber
        AuditHistoryReports is a Pane
            span 9 columns
            Layout
                three column distributed
                    large menu
                        list is InventoryTransaction.InventoryTransactionAuditList
                            title is "Transaction"
                            menu item name is InventoryTransactionAuditList
                            description is "View_audit_transaction_quantities_and_stock-on-hand_balances_for_a_specific_period_and_year"
                            icon is icon.requestActionsIcon
                        list is ItemUOM.ItemUOMAuditList
                            title is "ItemUOM"
                            menu item name is ItemUOMAuditList
                            description is "View_and_generate_audit_report_for_Item_UOM"
                            icon is icon.requestActionsIcon
                        list is InventoryLocation.InventoryLocationAuditList
                            title is "InventoryLocation"
                            menu item name is InventoryLocationAuditList
                            description is "Generate_audit_report_from_inventory_location"
                            icon is icon.requestActionsIcon
                        list is InventoryTransactionLine.InventoryTransactionLineAuditList
                            title is "TransactionLine"
                            menu item name is InventoryTransactionLineAuditList
                            description is "View_detail_item_transaction_history_records"
                            icon is icon.requestActionsIcon
                        list is ItemLocation.ItemLocationAuditList
                            title is "ItemLocation"
                            menu item name is ItemLocationAuditList
                            description is "View_an_audit_of_changes_for_selected_item_location"
                            icon is icon.requestActionsIcon
                        list is GeneralLedgerCategory.GLCategoryAuditList
                            title is "LocationAccountGroup"
                            menu item name is GLCategoryAuditList
                            description is "Print_a_report_of_all_field_maintenance_activity_that_occurred_in_the_GL_Category"
                            icon is icon.requestActionsIcon
                        list is Item.ItemAuditList
                            title is "Item"
                            menu item name is ItemAuditList
                            description is "View_audit_of_changes_for_selected_item_master_records"
                            icon is icon.requestActionsIcon
                        list is Bin.BinAuditList
                            title is "Bin"
                            menu item name is BinAuditList
                            description is "View_and_print_a_report_for_both_unapproved_and_preferred_bin_items"
                            icon is icon.requestActionsIcon

