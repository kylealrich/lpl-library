MatchListsPageUX1 is a Page
    title is "Lists"
    implements MenuSearch

    UnmatchedReceipts is a Panel
        title is "UnmatchedReceiptLines"
        business class is PurchaseOrderReceiptLine
            list is UnmatchedReceiptLineSearchList
                title is "UnmatchedReceiptLines"

    InvoicedNotReceived is a Panel
        business class is PayablesInvoiceDetail
            list is InvoicedNotReceivedListing

    InvoiceMessageListing is a Panel
        title is "InvoiceMessages"
        business class is MatchInvoiceMessage
            list is InvoiceMessageListing
                title is "InvoiceMessages"

    InvoiceCostUpdate is a Panel
        business class is PurchaseOrderReceiptInvoiceLink
            list is MatchInvoiceCostUpdateList

    InvoiceAddOnChargeListing is a Panel
        title is "InvoiceAddOnCharges"
        business class is PayablesInvoiceAddOnCharge
            list is InvoiceAddOnChargeListing
                title is "InvoiceAddOnCharges"

    PrepaidInvoicedNotReceived is a MultiListPanel
        title is "PrepaidInvoicedNotReceived"
        business class is MatchPurchaseOrderInvoice
            list is PrepaidInvoiceNotReceivedPreviewList
                title is "PrepaidInvoicedNotReceived"
        business class is PrepaidInvoiceNotReceivedResult
            list is PrepaidInvoiceNotReceivedResultList
                title is "PrepaidInvoicedNotReceivedResults"


    MatchReferenceAdjustment is a Panel
        title is "MatchReferenceAdjustment"
        business class is PayablesInvoice
            list is MatchReferenceAdjustmentList
                title is "MatchReferenceAdjustment"

    Aging is a Panel
        business class is PayablesInvoice
            list is APManagerRetrospectiveOpenInvoiceList

    Dashboard is a DashBoardPanel
        title is "InvoiceSummary"
        use 12 column grid
        ByClerk is a Pane
            span 4 columns
            business class is PayablesInvoice
                list is APManagerInvoicesByClerk
                    title is "InvoiceCountByProcessor"
                    display InvoicesByClerkCubePieChartList
                    overview navigation is APManagerInvoicesByClerkCubeLink
                        label is "Detail"
        ByClerkAmount is a Pane
            span 4 columns
            business class is PayablesInvoice
                list is APManagerInvoiceAmountsByClerk
                    title is "InvoiceAmountByProcessor"
                    display InvoiceAmountsByClerkCubePieChartList
                    overview navigation is APManagerInvoicesByClerkAmountCubeLink
                        label is "Detail"
        ByStatus is a Pane
            span 4 columns
            business class is PayablesInvoice
                list is APManagerInvoicesByStatus
                    title is "InvoiceCountByStatus"
                    display InvoiceStatusCubePieChartList
                    overview navigation is APManagerInvoicesByStatusCubeLink
                        label is "Detail"
        ByStatusAmount is a Pane
            span 4 columns
            business class is PayablesInvoice
                list is APManagerInvoiceAmountsByStatus
                    title is "InvoiceAmountByStatus"
                    display InvoiceStatusAmountCubePieChartList
                    overview navigation is APManagerInvoicesByStatusAmountCubeLink
                        label is "Detail"
        ByApprover is a Pane
            span 4 columns
            business class is PayablesInvoice
                list is APManagerInvoicesByApprover
                    title is "InvoiceCountByCurrentApprover"
                    display InvoicesByApproverCubePieChartList
                    overview navigation is APManagerInvoicesByApproverCubeLink
                        label is "Detail"
        InvoiceAging is a Pane
            span 4 columns
            business class is PayablesInvoice
                list is APManagerInvoiceAgingCube
                    title is "InvoiceAging"
                    display InvoiceAmountDue
                    overview navigation is APManagerInvoiceAgingCubeLink
                        label is "Detail"
        OverdueInvoices is a Pane
            span 6 columns
            business class is PayablesInvoice
                list is APManagerDashboardOverduePayablesInvoiceList
                    title is "OverdueInvoices"
                    overview navigation is APManagerOverduePayablesInvoices
                        label is "Detail"
        OpenVendorInvoices is a Pane
            span 6 columns
            business class is PayablesInvoice
                list is APManagerDashboardOpenVendorInvoices
                    title is "OpenInvoicesByDueDate"
                    overview navigation is APManagerOpenVendorInvoices
                        label is "Detail"
        CashForecasting is a Pane
            span 4 columns
            business class is PayablesInvoice
                list is APManagerCashForecastingCube
                    title is "CashForecasting"
                    display InvoiceAmountDue
                    overview navigation is APManagerCashForecastingCubeLink
                        label is "Detail"

    OpenCommitments is a Panel
        business class is PayablesInvoice
            list is APManagerInvoicesWithCommitmentsList

