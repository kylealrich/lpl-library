MatchInvoicesPage is a Page
    title is "MatchInvoices"
    implements MenuSearch

    AllInvoices is a DashBoardPanel
        title is "Main"
        use 12 column grid
        Create is a Pane
            span 3 columns
            title is "ImportantLinks"
            Layout
                single column
                    large menu
                        action is PayablesInvoice.Unreleased.CreateInvoice
                            title is "Create_anInvoice"
                            menu item name is CreateAnInvoice
                            description is "Create_a_new_invoice_using_the_full_invoice_entry_form."
                            icon is icon.document-finance-emerald
                        action is PayablesInvoice.Unreleased.CreateAndRelease
                            title is "Create_andRelease"
                            menu item name is CreateAndRelease
                            description is "Create_and_automatically_release_an_invoice."
                            icon is icon.document-finance-emerald
                        page is MatchMyInvoicesPage
                            title is "MyInvoices"
                            menu item name is MyInvoices
                            description is "View_invoices_assigned_to_you."
                            icon is icon.document-finance-emerald
        GeneralSearch is a Pane
            span 3 columns
            Layout
                single column
                    large menu
                        page is MatchInvoiceAdvancedSearch.Advanced
                            title is "Advanced"
                            menu item name is Advanced
                            description is "Find_invoices_using_multiple_search_criteria."
                            icon is icon.search-azure
                        page is MatchInvoiceAdvancedSearch.FilteredSearch
                            title is "Filter"
                            menu item name is Filter
                            description is "Find_invoices_using_list_column_filters."
                            icon is icon.search-azure
        OptimizedSearch is a Pane
            span 6 columns
            Layout
                two column distributed
                    large menu
                        page is MatchInvoiceAdvancedSearch.ByCompanyVendor
                            title is "Vendor"
                            menu item name is Vendor
                            description is "Quickly_find_allInvoices_for_a_specificCompany_andVendor."
                            icon is icon.search-azure
                        page is MatchInvoiceAdvancedSearch.ByInvoiceNumber
                            title is "InvoiceNumber"
                            menu item name is InvoiceNumber
                            description is "Quickly_find_anInvoice_by_InvoiceNumber."
                            icon is icon.search-azure
                        page is MatchInvoiceAdvancedSearch.ByAmount
                            title is "Amount"
                            menu item name is Amount
                            description is "Quickly_findInvoices_based_on_Amount."
                            icon is icon.search-azure
                        page is MatchInvoiceAdvancedSearch.ByInvoiceDate
                            title is "InvoiceDate"
                            menu item name is InvoiceDate
                            description is "Quickly_find_allInvoices_for_a_specificDate."
                            icon is icon.search-azure
                        page is MatchInvoiceAdvancedSearch.ByStatus
                            title is "Status"
                            menu item name is Status
                            description is "Quickly_find_allInvoices_for_a_specificStatus."
                            icon is icon.search-azure
                        page is MatchInvoiceAdvancedSearch.MatchInvoices
                            title is "MatchInvoices"
                            menu item name is MatchInvoices
                            description is "Quickly_find_MatchInvoices_by_PurchaseOrderNumber."
                            icon is icon.search-azure
        MatchProcesses is a Pane
            span 6 columns
            Layout
                two column distributed
                    large menu
                        action is MatchAnalysisReport.MatchAnalysisReport
                            title is "MatchAnalysisReport"
                            menu item name is MatchAnalysisReport
                            icon is icon.outline-process-amethyst
                        action is CostVarianceAnalysis.CostVarianceAnalysis
                            title is "CostVarianceAnalysis"
                            menu item name is CostVarianceAnalysis
                            icon is icon.outline-process-amethyst
                        action is OpenReceiptArchive.OpenReceiptArchive
                            title is "OpenReceiptArchive"
                            menu item name is OpenReceiptArchive
                            icon is icon.outline-process-amethyst
                        action is ERSInvoiceCreation.ERSInvoiceCreation
                            title is "ERSInvoiceCreation"
                            menu item name is ERSInvoiceCreation
                            icon is icon.outline-process-amethyst
                        action is ReceiptInterfaceInput.InterfaceReceipts
                            title is "ReceiptInterface"
                            menu item name is ReceiptInterface
                            icon is icon.outline-process-amethyst
                        action is AutoMatch.AutoMatch
                            title is "AutoMatch"
                            menu item name is AutoMatch
                            icon is icon.outline-process-amethyst
                        action is AutoMatchResult.PurgeResults
                            title is "PurgeAutoMatchResults"
                            menu item name is PurgeAutoMatchResults
                            icon is icon.outline-process-amethyst
                        action is Chargeback.Chargeback
                            title is "Chargeback"
                            menu item name is Chargeback
                            icon is icon.outline-process-amethyst
        Exceptions is a Pane
            span 6 columns
            Layout
                two column distributed
                    large menu
                        page is PayablesInvoiceNotInProcessUX1.Amended
                            title is "Amended"
                            menu item name is Amended
                            description is "View_amended_invoices."
                            icon is icon.warning-amber
                        page is InvoiceExceptionsUX1.Commitments
                            title is "Commitments"
                            menu item name is Commitments
                            description is "View_invoices_with_open_commitments."
                            icon is icon.warning-amber
                        page is InvoiceExceptionsUX1.OutOfBalance
                            title is "OutOfBalance"
                            menu item name is OutOfBalance
                            description is "ViewInvoices_where_the_payment,_distribution,_and/or_tax_amounts_are_out_of_balance."
                            icon is icon.warning-amber
                        page is PayablesInvoiceNotInProcessUX1.Rejected
                            title is "Rejected"
                            menu item name is Rejected
                            description is "Review_invoices_that_have_been_rejected._These_invoices__can_be_reinstated_and_resubmitted."
                            icon is icon.warning-amber
                        page is InvoiceExceptionsUX1.OnHold
                            title is "OnHold"
                            menu item name is OnHold
                            description is "Review_payments_that_are_'On_Hold'_and_prevented_from_being_released."
                            icon is icon.warning-amber
                        page is InvoiceExceptionsUX1.ProcessingInBackground
                            title is "Processing_in_Background"
                            menu item name is ProcessingInBackground
                            description is "Invoices_processing_in_the_background_will_not_be_paid_or_matched."
                            icon is icon.warning-amber
        Processes is a Pane
            span 6 columns
            Layout
                two column distributed
                    large menu
                        page is PayablesJournalizePage.JournalizePreview
                            title is "JournalizeDistributions"
                            menu item name is JournalizeDistributions
                            icon is icon.outline-process-amber
                        list is PayablesInvoiceRetainageDistribution.InvoiceRetainageDistributionList
                            title is "RetainageDistributions"
                            menu item name is RetainageDistributions
                            icon is icon.outline-process-amber
                        action is MassRetainagePaymentAdjust.MassRetainagePaymentAdjust
                            title is "MassAdjustRetainagePayments"
                            menu item name is MassRetainagePaymentAdjust
                            icon is icon.outline-process-amber
                            Search Terms
                                    "MassRetainagePaymentAdjust"
                        list is PayablesInvoice.MassInvoiceReleasePreview
                            title is "MassReleaseInvoicesPreview"
                            menu item name is MassInvoiceReleasePreview
                            icon is icon.outline-process-amber
                            Search Terms
                                    "MassInvoiceReleasePreview"
                        action is PayablesInvoice.MassInvoiceRelease
                            title is "MassReleaseInvoices"
                            menu item name is MassInvoiceRelease
                            icon is icon.outline-process-amber
                            Search Terms
                                    "MassInvoiceRelease"
                        action is PayablesInvoice.InvoiceHoldMassUpdate
                            title is "MassUpdateInvoicesOnHold"
                            menu item name is MassUpdateInvoicesOnHold
                            icon is icon.outline-process-amber
                        action is ProofOfDeliveryNotification.ProofOfDeliveryNotification
                            title is "ProofOfDeliveryNotification"
                            menu item name is ProofOfDeliveryNotification
                            icon is icon.outline-process-amber
                        action is PayablesInvoice.RequestNewInvoicePayment
                            title is "RequestInvoicePayment"
                            menu item name is RequestInvoicePayment
                            icon is icon.outline-process-amber
        AuditHistory is a Pane
            span 3 columns
            Layout
                single column
                    large menu
                        page is PayablesAuditHistoryUX1.Invoice
                            title is "Invoice"
                            menu item name is Invoice
                            icon is icon.calendar-variant-emerald
                        page is PayablesAuditHistoryUX1.Distribution
                            title is "Distribution"
                            menu item name is Distribution
                            icon is icon.calendar-variant-emerald
                        page is PayablesAuditHistoryUX1.Payment
                            title is "Payment"
                            menu item name is Payment
                            icon is icon.calendar-variant-emerald
        QuickCreate is a Pane
            span 3 columns
            title is "QuickInvoiceCreation"
            Layout
                single column
                    large menu
                        page is SimpleInvoiceCreationUX1.SpeedEntry
                            title is "Invoice"
                            menu item name is Invoice
                            description is "Quickly_create_multiple_invoices."
                            icon is icon.document-finance-emerald
                        page is SimpleInvoiceCreationUX1.ListEntry
                            title is "Invoice_andDistributions"
                            menu item name is InvoiceAndDistribution
                            description is "Quickly_create_multiple_invoices__and_add_distributions."
                            icon is icon.document-finance-emerald

    SearchInvoices is a Panel
        business class is PayablesInvoice
            list is MatchInvoiceSearchList

    Unsubmitted is a Panel
        title is "Unsubmitted"
        business class is PayablesInvoice
            list is APManagerUnsubmittedPayablesInvoiceList
                title is "Unsubmitted"

    Released is a Panel
        title is "Released"
        business class is PayablesInvoice
            list is APManagerReleasedPayablesInvoiceList
                title is "Released"

    Paid is a Panel
        business class is PayablesInvoice
            list is APManagerPaidPayablesInvoiceList

    Cancelled is a Panel
        business class is PayablesInvoice
            list is APManagerCancelledPayablesInvoiceList

    Aging is a DashBoardPanel
        VendorAgingWithPercents is a Pane
            quad width
            title is "ByVendor"
            business class is MatchAgingSummary
                list is VendorPeriod4List
                    overview navigation is MatchAgingPageNav
                        label is "NavigateToCompleteAgingPage"

    MatchPrepayments is a Panel
        business class is MatchReconQueue
            list is MatchPrepayments

    AutoMatching is a Panel
        title is "AutoMatching"
        business class is AutoMatchResult
            list is AutoMatchResults
                title is "AutoMatching"

    ApprovedUnreleased is a Panel
        business class is PayablesInvoice
            list is APManagerApprovedPayablesInvoiceList

