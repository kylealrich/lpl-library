UX1InventoryClinicalProcedurePage is a Page
    title is "ClinicalProcedureProcessing"
    implements MenuSearch

    ProcedureSupplyPlanning is a DashBoardPanel
        use 12 column grid
        System is a Pane
            span 12 columns
            Layout
                two column distributed
                    column1
                        large menu
                            page is ScheduledClinicalProcedurePage
                                title is "ScheduledProcedures"
                                menu item name is ScheduledProcedures
                                description is "View_&_Create_Scheduled_Procedures"
                                icon is icon.window-value-charts-azure
                            page is WorksheetDetailPage
                                title is "DemandWorksheet"
                                menu item name is DemandWorksheet
                                description is "View_Demanded_Items_from_Scheduled_Procedures"
                                icon is icon.window-value-charts-azure
                            list is ItemLocationDemandForecast.DemandForecastSummary
                                title is "DemandProjection"
                                menu item name is DemandProjection
                                description is "Generate_and_View_list_of_items_with_Unreserved_Procedure_Demand_Quantities"
                                icon is icon.window-value-charts-azure
                    column2
                        large menu
                            action is ItemLocationDemandForecast.ReserveForScheduledProcedures
                                title is "MassReservation"
                                menu item name is MassReservation
                                description is "Reserve_Items_needed_for_upcoming_Scheduled_Procedures"
                                icon is icon.chart-bar-azure
                            action is ScheduledProcedure.MassRequisitionFromScheduledProcedure
                                title is "MassRequisition"
                                menu item name is MassRequisition
                                description is "Create_Requisitions_for_Upcoming_Scheduled_Procedures"
                                icon is icon.chart-bar-azure
                            action is ScheduledProcedure.CancelScheduledProcedures
                                title is "CancelScheduledProcedures"
                                menu item name is CancelScheduledProcedures
                                description is "Cancel_Scheduled_Procedures_past_their_scheduled_date"
                                icon is icon.chart-bar-azure

