CloseManagerUX1 is a Page
    title is "CurrentPeriods"
    implements MenuSearch

    ByPeriodAndProcess is a MultiPanePanel
        title is "ByPeriodAndProcess"
        pane 1
            business class is ClosePeriod
                list is TheCloseStatusList
        pane 2
            business class is pane 1.AllSummaryTasksRel
                list is TheCloseProcessTreeViewList
                    title is "ProcesProgress"
        pane 3
            business class is pane 2.ClosePeriodTaskFlatSummaryRel
                list is ClosePeriodTaskFlatList
                    title is "TaskStatusByDueDate"

    ByPeriodAndTypePrimary is a MultiPanePanel
        title is "ByPeriodAndTypePrimary"
        pane 1
            business class is ClosePeriod
                list is AdminOpenPeriodList
        pane 2
            business class is pane 1.ClosePeriodTask set
                list is TaskStatusByPrimaryType

    ByPeriodAndTypeSecondary is a MultiPanePanel
        title is "ByPeriodAndTypeSecondary"
        pane 1
            business class is ClosePeriod
                list is AdminOpenPeriodList
        pane 2
            business class is pane 1.ClosePeriodTask set
                list is TaskStatusBySecondaryType

    ByPeriodAndTypeTertiary is a MultiPanePanel
        title is "ByPeriodAndTypeTertiary"
        pane 1
            business class is ClosePeriod
                list is AdminOpenPeriodList
        pane 2
            business class is pane 1.ClosePeriodTask set
                list is TaskStatusByTertiaryType

