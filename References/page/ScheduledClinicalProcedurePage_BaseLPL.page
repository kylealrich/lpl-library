ScheduledClinicalProcedurePage is a Page
    title is "ScheduledProcedures"
    show panel control on left

    ScheduledProcedures is a Panel
        business class is ScheduledProcedure
            list is OpenFullyReservedInProcessProceduresList

    RescheduledProcedures is a Panel
        business class is ScheduledProcedure
            list is RescheduledProceduresList

    CanceledScheduledProcedures is a Panel
        business class is ScheduledProcedure
            list is CanceledScheduledProcedureList

    ClosedProcedures is a Panel
        business class is ScheduledProcedure
            list is ClosedProceduresList

    ErrorsInRequisitionCreation is a Panel
        title is "ErrorsInRequisition"
        business class is ScheduledProcedure
            list is ScheduledProceduresWithRequisitionCreationErrorsList
                title is "ErrorsInRequisition"

    MassRequisitionResults is a Panel
        business class is InventoryControlResult
            list is MassRequisitionResultList

