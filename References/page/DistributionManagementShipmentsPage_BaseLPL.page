DistributionManagementShipmentsPage is a Page
    title is "Shipments"
    show panel control on left

    ShipmentsForPicking is a Panel
        business class is WarehouseShipment
            list is ShipmentsForPickingListForDistributionClerkList

    ShipmentsForPacking is a Panel
        business class is WarehouseShipment
            list is ShipmentsForPackingListForDistributionClerkList

    ShipmentsForRelease is a Panel
        business class is WarehouseShipment
            list is ShipmentsForReleaseListForDistributionClerkList

    ShipmentsTracking is a Panel
        business class is WarehouseShipmentTracking
            list is AdvancedWarehouseShipmentTrackingList

    AuditHistory is a Panel
        visible when (not actor.context.FinanceEnterpriseGroup.ColumnarWarehouseShipmentAuditInUse)
        business class is WarehouseShipment
            list is WarehouseShipmentAuditList

    AuditHistoryColumnar is a Panel
        title is "AuditHistory"
        visible when (actor.context.FinanceEnterpriseGroup.ColumnarWarehouseShipmentAuditInUse)
        business class is WarehouseShipment
            list is ColumnarWarehouseShipmentAuditList
                title is "AuditHistory"

    LineAuditHistory is a Panel
        visible when (not actor.context.FinanceEnterpriseGroup.ColumnarWarehouseShipmentLineAuditInUse)
        business class is WarehouseShipmentLine
            list is WarehouseShipmentLineAuditList

    LineAuditHistoryColumnar is a Panel
        title is "LineAuditHistory"
        visible when (actor.context.FinanceEnterpriseGroup.ColumnarWarehouseShipmentLineAuditInUse)
        business class is WarehouseShipmentLine
            list is ColumnarWarehouseShipmentLineAuditList
                title is "LineAuditHistory"

