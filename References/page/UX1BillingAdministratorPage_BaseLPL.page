UX1BillingAdministratorPage is a Page
    title is "<actor.context.FinanceEnterpriseGroup.BillingAdministrationTitle>"
    implements MenuSearch

    Setup is a DashBoardPanel
        use 12 column grid
        Search Terms
                "Maintenance"
                "Admin"
        Company is a Pane
            span 3 columns
            Layout
                single column
                    small menu
                        list is BillingCompany.primary
                            title is "Companies"
                            menu item name is Companies
                            icon is icon.settings-azure
                        list is BillingProcessLevel.primary
                            title is "ProcessLevels"
                            menu item name is ProcessLevel
                            icon is icon.settings-azure
                        list is OrderCancelCreditReason.primary
                            title is "OrderCancelCreditReasons"
                            menu item name is OrderCancelCreditReason
                            icon is icon.settings-azure
                        list is BillingStandardCommentHeader.primary
                            title is "StandardCommentHeaders"
                            menu item name is StandardCommentHeader
                            icon is icon.settings-azure
                        list is BillingStandardComment.primary
                            title is "StandardComments"
                            menu item name is StandardComment
                            icon is icon.settings-azure
        Codes is a Pane
            span 3 columns
            Layout
                single column
                    small menu
                        list is BillingCustomerCode.primary
                            title is "Customer"
                            menu item name is Customer
                            icon is icon.settings-emerald
                        list is CustomerShipTo.primary
                            title is "CustomerShipTo"
                            menu item name is CustomerShipTo
                            icon is icon.settings-emerald
                        list is OrderDiscountCode.primary
                            title is "OrderDiscount"
                            menu item name is OrderDiscount
                            icon is icon.settings-emerald
                        list is PriceDiscountCode.primary
                            title is "PriceDiscount"
                            menu item name is PriceDiscount
                            icon is icon.settings-emerald
                        list is BillingFreightCode.primary
                            title is "Freight"
                            menu item name is Freight
                            icon is icon.settings-emerald
        Item is a Pane
            span 3 columns
            Layout
                single column
                    small menu
                        list is BillingItemGeneralLedgerAccount.primary
                            title is "GlobalLedgerAccount"
                            menu item name is ItemGlobalLedgerAccount
                            icon is icon.settings-amethyst
                        list is BillingItemAddOnCost.primary
                            title is "AddOnCost"
                            menu item name is ItemAddOnCost
                            icon is icon.settings-amethyst
                        list is OrderEntryAddOnCharge.primary
                            title is "ItemAndOrderBillingAddOnCharge"
                            menu item name is ItemAndOrderBillingAddOnCharge
                            icon is icon.settings-amethyst
                        list is CustomerItemCrossReference.primary
                            title is "CustomerItemCrossReference"
                            menu item name is CustomerItemCrossReference
                            icon is icon.settings-amethyst
                        list is RecurringBillingItemOperator.primary
                            title is "RecurringBillingItemOperator"
                            menu item name is RecurringBillingItemOperator
                            icon is icon.settings-amethyst
        Invoice is a Pane
            span 3 columns
            Layout
                single column
                    small menu
                        list is BillingInvoiceType.primary
                            title is "Type"
                            menu item name is InvoiceType
                            icon is icon.settings-amethyst
                        list is BillingInvoicePrefix.primary
                            title is "Prefix"
                            menu item name is InvoicePrefix
                            icon is icon.settings-amethyst
                        list is BillingInvoiceDocument.primary
                            title is "Documents"
                            menu item name is InvoiceDocuments
                            icon is icon.settings-amethyst
        Pricing is a Pane
            span 6 columns
            Layout
                two column distributed
                    column1
                        small menu
                            list is PriceBase.primary
                                title is "Base"
                                menu item name is Base
                                icon is icon.settings-ruby
                            list is PriceListContractPromotion.primary
                                title is "ListContractPromotion"
                                menu item name is ListContractPromotion
                                icon is icon.settings-ruby
                            list is PriceListSalesClass.primary
                                title is "ListSalesClasses"
                                menu item name is ListSalesClasses
                                icon is icon.settings-ruby
                            list is PriceBook.primary
                                title is "Book"
                                menu item name is Book
                                icon is icon.settings-ruby
                    column2
                        small menu
                            list is PriceBookSalesClassComment.primary
                                title is "BookSalesClassComment"
                                menu item name is BookSalesClassComment
                                icon is icon.settings-ruby
                            list is PriceBook.PriceBookDetailList
                                title is "BookDetails"
                                menu item name is BookDetails
                                icon is icon.settings-ruby
                            list is PricingWorksheet.primary
                                title is "Worksheet"
                                menu item name is Worksheet
                                icon is icon.settings-ruby
                            list is PricingPreview.primary
                                title is "Preview"
                                menu item name is Preview
                                icon is icon.settings-ruby
        DocumentTemplate is a Pane
            span 3 columns
            Layout
                single column
                    small menu
                        page is BillingIDMTemplatesPage
                            title is "IDM"
                            menu item name is IDM
                            icon is icon.settings-amber
                            Search Terms
                                    "IDM"
                                    "Templates"
                                    "InvoiceIDMTemplate"
                        list is SampleDocumentTemplate.SampleDocumentTemplatesForBilling
                            title is "SampleDocument"
                            menu item name is SampleDocument
                            icon is icon.settings-amber

    Utilities is a DashBoardPanel
        use 12 column grid
        Purge is a Pane
            span 6 columns
            Layout
                two column distributed
                    column1
                        small menu
                            action is PricingWorksheet.BaseListWorksheetPurge
                                title is "BaseListWorksheet"
                                menu item name is BaseListWorksheet
                                icon is icon.settings-ruby
                            action is BillingInvoice.InvoicePurge
                                title is "Invoice"
                                menu item name is Invoice
                                icon is icon.settings-ruby
                    column2
                        small menu
                            action is CustomerRecurringInvoicePurge.CustomerRecurringInvoicePurge
                                title is "CustomerRecurringInvoice"
                                menu item name is CustomerRecurringInvoice
                                icon is icon.settings-ruby
                            action is RecurringInvoicePurge.RecurringInvoicePurge
                                title is "RecurringInvoice"
                                menu item name is RecurringInvoice
                                icon is icon.settings-ruby
        DataRepair is a Pane
            span 3 columns
            Layout
                single column
                    small menu
                        action is BillingGLDistribution.FixGLTransactions
                            title is "FixTransactions"
                            menu item name is FixTransactions
                            icon is icon.settings-emerald

