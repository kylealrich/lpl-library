AdminConsoleSetup is a Page
    title is "System"
    implements MenuSearch

    Setup is a DashBoardPanel
        use 12 column grid
        System is a Pane
            span 8 columns
            Layout
                two column
                    large menu
                        link is external "<base url(webapp is AdministratorConsole)>/web/landmark/reveal.html"
                            title is "Application&SystemLinks"
                            menu item name is ApplicationSystemLinks
                            description is "Application&SystemLinks"
                            icon is icon.link
                        list is ConfigurationParameter.ConfigurationParameterAdminConsoleList
                            title is "FTP_Setup"
                            menu item name is FTPSetup
                            description is "FTP_Setup"
                            icon is icon.move
                        list is IsoLocaleSet.primary
                            title is "Locales"
                            menu item name is LocaleSetSetup
                            description is "SpecifyDesiredLocalesToLimitTranslatableDataDuringReplication"
                            icon is icon.language
                        action is SpellDictMaint.RunCommand
                            title is "SpellingDictionaryCreate_andUpdate"
                            menu item name is SpellDictMaintRunCommand
                            description is "SpellingDictionaryCreate_andUpdate"
                            icon is icon.spell-check
                        action is SpellDictMaintDelete.RunCommand
                            title is "SpellingDictionaryDelete"
                            menu item name is SpellDictMaintDeleteRunCommand
                            description is "SpellingDictionaryDelete"
                            icon is icon.spell-check
        Triage is a Pane
            span 4 columns
            title is "Triage"
            Layout
                single column
                    large menu
                        list is LandmarkExceptionLogEntry.primary
                            title is "ExceptionLogs"
                            menu item name is ExceptionLogInquiry
                            description is "LookupAFullExceptionUsingAnExceptionID"
                            icon is icon.travel-plan
                        link is external "<base url(webapp is AdministratorConsole)>/web/landmark/WebPing.html"
                            title is "WebPerformanceAnalyzer"
                            menu item name is WebPerformanceAnalyzer
                            description is "WebPerformanceAnalyzer"
                            icon is icon.travel-plan

