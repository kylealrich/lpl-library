MatchAdministratorHomePage is a Page
    title is "MatchAdministrator"
    implements MenuSearch
    show panel control on left

    MatchSetup is a Panel
        Layout
            two column distributed
                column1
                    large menu
                        list is MatchCompany.primary
                            title is "MatchCompanies"
                            menu item name is MatchCompanies
                            description is "Set_up_match_companies"
                            icon is icon.CSFPayables
                        list is MatchRule.primary
                            title is "MatchRules"
                            menu item name is MatchRules
                            description is "Set_up_match_rules"
                            icon is icon.CSFSupplyManagement
                        list is MatchTable.primary
                            title is "MatchTable"
                            menu item name is MatchTables
                            description is "Set_up_the_match_table"
                            icon is icon.CSFFinancialBusinessAnalyst
                column2
                    large menu
                        page is FinanceResourceAdmin
                            title is "FinanceResources"
                            menu item name is FinanceResources
                            description is "Set_up_finance_resources"
                            icon is icon.CSFChiefFinancialOfficer
                        list is ProcurementReasonCode.ProcurementReasonCodesInline
                            title is "ProcurementReasonCodes"
                            menu item name is ReasonCodes
                            description is "Set_up_reason_codes"
                            icon is icon.CSFSupplyManagement
                        list is AddOnCharge.primary
                            title is "AddOnCharges"
                            menu item name is AddOnCharges
                            description is "Set_up_add_on_charges"
                            icon is icon.CSFFinancialBusinessAnalyst

