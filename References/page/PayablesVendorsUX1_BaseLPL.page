PayablesVendorsUX1 is a Page
    title is "Vendors"
    implements MenuSearch

    Dashboard is a Panel
        business class is Vendor
            list is APManagerDashboard

    Processing is a Panel
        title is "List"
        business class is Vendor
            list is VendorList
                title is "List"

    Search is a Panel
        title is "Search"
        business class is Vendor
            list is VendorSearchList
                title is "Search"

    Balances is a Panel
        business class is VendorBalance
            list is VendorBalanceList

    More is a DashBoardPanel
        use 12 column grid
        Suppliers is a Pane
            span 3 columns
            Layout
                two column
                    large menu
                        page is SupplierPage
                            title is "Suppliers"
                            menu item name is Suppliers
                            icon is icon.hcm-emerald
                        list is Subcontractor.SubcontractorList
                            title is "Subcontractors"
                            menu item name is Subcontractors
                            icon is icon.hcm-emerald
                        page is SupplierMessagePage
                            title is "Messages_andDialog"
                            menu item name is MessagesAndDialog
                            icon is icon.hcm-emerald
                        list is SupplierVendorMismatch.SupplierVendorMismatchList
                            title is "Supplier_andVendorComparison"
                            menu item name is SupplierAndVendorComparison
                            icon is icon.hcm-emerald
        PayablesToReceivables is a Pane
            span 9 columns
            double height
            title is "Lists_and_Reports"
            Layout
                two column distributed
                    large menu
                        list is Vendor.PayablesAndReceivableInvoices
                            title is "OpenInvoices"
                            menu item name is OpenInvoices
                            description is "View_open_Payables_and_Receivables_invoices_by_vendor."
                            icon is icon.clipboard-list-amber
                        action is VendorStatements.VendorStatements
                            title is "Statements"
                            menu item name is Statements
                            description is "Create_statements_to_send_to_vendors."
                            icon is icon.clipboard-list-amber
                        list is IncomingVendorStatement.IncomingVendorStatementList
                            title is "ReconcileStatements"
                            menu item name is VendorStatementReconciliation
                            description is "Review_and_reconcile_imported_vendor_statements"
                            icon is icon.clipboard-list-amber
                        list is Vendor.PayablesToReceivablesList
                            title is "Payables_toReceivablesInterface"
                            menu item name is PayablesToReceivablesInterface
                            description is "Send_data_from_Payables_to_Receivables."
                            icon is icon.clipboard-list-amber
                        list is Vendor.VendorVATValidation
                            title is "VATValidation"
                            menu item name is VATValidation
                            description is "Review_and_validate_vendor_VAT_registration_information."
                            icon is icon.clipboard-list-amber
                        list is VendorComment.FullVendorCommentsList
                            title is "Comments"
                            menu item name is Comments
                            description is "View_vendor_comments."
                            icon is icon.clipboard-list-amber
                        list is PayablesInvoice.VendorSpendSeachList
                            title is "Spend"
                            menu item name is Spend
                            description is "View_total_amount_spent_by_vendor."
                            icon is icon.clipboard-list-amber
                        list is PayablesInvoicePayment.VendorPaymentHistoryList
                            title is "VendorPayments"
                            menu item name is VendorPayments
                            icon is icon.clipboard-list-amber
                        list is VendorTemplate.VendorTemplateList
                            title is "VendorTemplates"
                            menu item name is VendorTemplates
                            icon is icon.clipboard-list-amber
                        list is Vendor.OfficeOfInspectorGeneralExclusionList
                            title is "VendorExclusionList"
                            menu item name is VendorExclusionList
                            icon is icon.clipboard-list-amber
        AuditHistory is a Pane
            span 3 columns
            Layout
                two column
                    large menu
                        page is VendorAuditHistoryUX1.Vendor
                            title is "Vendor"
                            menu item name is Vendor
                            description is "View_vendor_audit_history."
                            icon is icon.calendar-azure
                        page is VendorAuditHistoryUX1.Location
                            title is "Location"
                            menu item name is Location
                            description is "View_vendor_location_audit_history."
                            icon is icon.calendar-azure
                        page is VendorAuditHistoryUX1.Address
                            title is "Address"
                            menu item name is Address
                            description is "View_vendor_address_audit_history."
                            icon is icon.calendar-azure
        Utilities is a Pane
            span 9 columns
            double height
            Layout
                three column distributed
                    large menu
                        action is VendorBalanceRebuild.VendorBalanceRebuild
                            title is "RebuildBalances"
                            menu item name is RebuildBalances
                            description is "Rebuild_vendor_balances."
                            icon is icon.cycle-settings-amethyst
                        action is VendorMerge.VendorMerge
                            title is "MergeVendor"
                            menu item name is MergeVendor
                            description is "Transfer_a_vendor's_open_invoices_to_another_vendor."
                            icon is icon.cycle-settings-amethyst
                        action is ProcessGroupUpdate.ProcessGroupUpdate
                            title is "UpdateProcessGroup"
                            menu item name is UpdateProcessGroup
                            icon is icon.cycle-settings-amethyst
                        action is MassVendorUpdate.MassVendorUpdate
                            title is "MassUpdateVendors"
                            menu item name is MassUpdateVendors
                            description is "Update_a_group_of_vendors."
                            icon is icon.cycle-settings-amethyst
                            Search Terms
                                    "MassVendorUpdate"
                        action is VendorDeletion.VendorDeletion
                            title is "DeleteVendors"
                            menu item name is DeleteVendors
                            description is "Delete_multiple_vendors."
                            icon is icon.cycle-settings-amethyst
                        action is PayablesInvoiceTotal.SetupAllRecords
                            title is "UpdatePayablesChartTotals"
                            menu item name is UpdatePayablesChartTotals
                            icon is icon.cycle-settings-amethyst
                        action is VendorGroupCopy.VendorGroupCopy
                            title is "CopyVendorGroup"
                            menu item name is CopyVendorGroup
                            description is "Copy_selected_hold_codes,_vendors,_vendor_classes,_vendor_locations,_and_user_fields_from_one_vendor_group_to_another."
                            icon is icon.cycle-settings-amethyst
                        action is Vendor.BuildVendorTextSetAction
                            title is "VendorKeywordGeneration"
                            menu item name is PopulateVendorKeywords
                            description is "Populate_the_VendorKeyword_field_used_by_vendor_search._Field_includesVendorID,Name,SearchName,LegalName,TaxID,etc."
                            icon is icon.cycle-settings-amethyst
                        list is SampleDocumentTemplate.SampleDocumentTemplatesForPayables
                            title is "SampleDocumentTemplates"
                            menu item name is SampleDocumentTemplates
                            icon is icon.cycle-settings-amethyst
                        list is MatchAging.MatchAgingList
                            title is "MatchAging"
                            menu item name is MatchAging
                            icon is icon.cycle-settings-amethyst
                        action is PayablesCompany.Netting
                            title is "PayablesAndReceivablesNetting"
                            menu item name is PayablesAndReceivablesNetting
                            icon is icon.cycle-settings-amethyst

