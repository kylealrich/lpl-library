WorkOrderPagesUX1 is a Page
    title is "WorkOrders"
    implements MenuSearch

    WorkOrders is a Panel
        business class is WorkOrder
            list is WorkOrderNumber

    ScheduleHeaders is a Panel
        business class is WorkOrderDataScheduleHeader
            list is ScheduledItems

    ForecastDemand is a Panel
        business class is WorkOrderForecastDemand
            list is WorkOrderForecastDemandList

    Processing is a DashBoardPanel
        use 12 column grid
        Warehouse is a Pane
            span 6 columns
            Layout
                two column distributed
                    large menu
                        action is WarehouseBatchWorkOrderScheduling.WarehouseBatchWorkOrderScheduling
                            title is "BatchWorkOrderScheduling"
                            menu item name is BatchWorkOrderScheduling
                            icon is icon.clipboard-check-amber
                        action is WarehouseWorkOrderExplosion.WarehouseWorkOrderExplosion
                            title is "WorkOrderExplosion"
                            menu item name is WorkOrderExplosion
                            icon is icon.window-text-amber
                        action is WarehouseWorkOrderPickListPrint.WarehouseWorkOrderPickListPrint
                            title is "WorkOrderPickListPrint"
                            menu item name is WorkOrderPickListPrint
                            icon is icon.documents-amber
                        action is WarehousePickingFeedbackUpdate.WarehousePickingFeedbackUpdate
                            title is "PickingFeedbackUpdate"
                            menu item name is PickingFeedbackUpdate
                            icon is icon.document-amber
                        action is WarehouseProductionFeedback.WarehouseProductionFeedback
                            title is "ProductionFeedback"
                            menu item name is ProductionFeedback
                            icon is icon.clipboard-list-amber
                        action is WarehouseProductionPrint.WarehouseProductionPrint
                            title is "ProductionPrint"
                            menu item name is ProductionPrint
                            icon is icon.documents-amber

    Utilities is a DashBoardPanel
        use 12 column grid
        Interface is a Pane
            span 3 columns
            Layout
                single column
                    large menu
                        action is WarehouseForecastDemandInterface.WarehouseForecastDemandInterface
                            title is "ForecastDemandInterface"
                            menu item name is ForecastDemandInterface
                            icon is icon.outline-interface-emerald
        Purge is a Pane
            span 3 columns
            Layout
                single column
                    large menu
                        action is WarehouseWorkOrderPurge.WarehouseWorkOrderPurge
                            title is "WarehouseWorkOrderPurge"
                            menu item name is WarehouseWorkOrderPurge
                            icon is icon.outline-form-ruby
                        action is EAMWorkOrder.EAMWorkOrderPurge
                            title is "EAMWorkOrderPurge"
                            menu item name is EAMWorkOrderPurge
                            icon is icon.outline-checklist-ruby

