PurchasingAdministratorUX1 is a Page
    title is "PurchasingAdministration"
    implements MenuSearch

    Purchasing is a DashBoardPanel
        use 12 column grid
        Purchasing is a Pane
            span 4 columns
            icon is icon.outline-maintenance-emerald
            Layout
                two column
                    small menu
                        list is ShipTerm.primary
                            title is "ShipTerms"
                            menu item name is ShipTerms
                        list is FreightTerm.primary
                            title is "FreightTerms"
                            menu item name is FreightTerms
                        list is BuyerGroup.primary
                            title is "BuyerGroups"
                            menu item name is BuyerGroups
                        list is Buyer.primary
                            title is "Buyers"
                            menu item name is Buyers
                        list is POCode.primary
                            title is "POCodes"
                            menu item name is POCodes
                        list is AddOnCharge.primary
                            title is "AddOnCharges"
                            menu item name is AddOnCharges
                        list is PurchasingVendor.primary
                            title is "PurchasingVendors"
                            menu item name is PurchasingVendors
                        list is ProcurementTemplate.primary
                            title is "ProcurementTemplates"
                            menu item name is ProcurementTemplates
                        list is PurchasingCompany.primary
                            title is "Companies"
                            menu item name is PurchasingCompanies
        ProcurementCard is a Pane
            span 4 columns
            icon is icon.outline-maintenance-amethyst
            Layout
                single column
                    small menu
                        page is ProcurementCardSetupPage
                            title is "ProcurementCards"
                            menu item name is ProcurementCards
                        page is MerchantSetupPage
                            title is "Merchants"
                            menu item name is Merchants
                        list is ProcurementCardReasonCode.ProcurementCardReasonCodes
                            title is "ReasonCodes"
                            menu item name is ReasonCodes
        Punchout is a Pane
            span 4 columns
            icon is icon.outline-maintenance-azure
            Layout
                two column
                    small menu
                        list is BuyingOrganization.BuyingOrganizations
                            title is "BuyingOrganizations"
                            menu item name is BuyingOrganizations
                        list is PunchoutVendor.PunchoutVendors
                            title is "Vendors"
                            menu item name is PunchoutVendors
                        list is PunchoutVendorCategory.PunchoutVendorCategories
                            title is "VendorCategories"
                            menu item name is PunchoutVendorCategories
                        list is PunchoutVendorFieldMapping.PunchoutVendorsFieldMapping
                            title is "VendorFieldMappings"
                            menu item name is PunchoutVendorsFieldMapping
                        list is PunchoutVendorData.PunchoutVendorsData
                            title is "VendorData"
                            menu item name is PunchoutVendorsData
                        list is RequesterPunchoutVendor.RequestersPunchoutVendors
                            title is "RequesterVendors"
                            menu item name is RequestersPunchoutVendors
                        list is RequesterPunchoutVendorData.RequesterPunchoutVendorsData
                            title is "RequesterVendorData"
                            menu item name is RequesterPunchoutVendorsData
                        list is RequesterPunchoutVendor.RSSRequestersPunchoutVendors
                            title is "MyVendors"
                            menu item name is MyPunchoutVendors
                        list is PurchaseOrderDispatchVendor.PurchaseOrderDispatchVendors
                            title is "PurchaseOrderDispatchVendors"
                            menu item name is PurchaseOrderDispatchVendors

