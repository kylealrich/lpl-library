ProjectAccountantHomePage is a Page
    title is "ProjectAccountant"

    ProjectAccountant is a Panel
        title is "ProjectAccountant"
        Layout
            three column
                column1
                    header2 of "ProjectLedger"
                    large menu
                        page is ProjectAccountingSetupPage
                            title is "Projects"
                            menu item name is Projects
                            description is "Setup_and_maintain_project_structures,_hierarchies,_contracts_and_posting_projects."
                            icon is icon.CSFProject
                        page is ProjectAccountantReportingPage
                            title is "ReportsByActor"
                            menu item name is Reports
                            description is "View_project_reports_including_obligations,_variance,_comparison_and_percent_complete."
                            icon is icon.CSFFinancialBusinessAnalyst
                        page is ProjectAccountantReportingPageAll
                            title is "GlobalReports"
                            menu item name is GlobalReports
                            description is "View_project_reports_including_obligations,_variance,_comparison_and_percent_complete."
                            icon is icon.CSFFinancialBusinessAnalyst
                        list is GeneralLedgerTotal.ProjectBudgetAnalysis
                            title is "ProjectBudgetAnalysis"
                            menu item name is ProjectBudgetAnalysis
                            description is "Analyze_project_LTD_balance_amounts."
                            icon is icon.CSFController
                        list is GeneralLedgerTotal.ProjectAmountAnalysis
                            title is "ProjectAmountAnalysis"
                            menu item name is ProjectAmountAnalysis
                            description is "Analyze_project_PTD,_YTD_and_LTD_balance_amounts."
                            icon is icon.CSFController
                        list is GeneralLedgerTotal.ProjectTrendAnalysis
                            title is "ProjectTrendAnalysis"
                            menu item name is ProjectTrendAnalysis
                            description is "Analyze_project_period_amounts_for_a_range_of_periods."
                            icon is icon.CSFController
                        page is GLReportWriter
                            title is "ReportDesigner"
                            menu item name is ReportDesigner
                            description is "View_and_create_global_ledger_reports."
                            icon is icon.CSFFinancialBusinessAnalyst
                        list is AccountAnalysisSettings.AccountAnalysisSettingsForActorList
                            title is "AccountAnalysis"
                            menu item name is AccountAnalysis
                            description is "Analyze_project_account_and_budget_PTD,_YTD_and_LTD_balance_amounts."
                            icon is icon.CSFController
                column2
                    header2 of "ProjectInvoicingAndRevenue"
                    large menu
                        page is ProjectAccountantFundingSourcePage
                            title is "ProjectFundingSources"
