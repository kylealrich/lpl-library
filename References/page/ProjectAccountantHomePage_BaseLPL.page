                            description is "Setup_and_maintain_project_funding."
                            icon is icon.CSFProject
                        page is ProjectAccountantContractsPage
                            title is "ProjectContracts"
                            menu item name is ProjectContracts
                            description is "Analyze_and_manage_project_contracts."
                            icon is icon.CSFProject
                        page is ProjectReceivablesPage
