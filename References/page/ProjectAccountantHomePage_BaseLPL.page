                            icon is icon.CSFSourcing
                        page is ProjectAccountantEffortCertification
                            title is "EffortCertification"
                            menu item name is EffortCertification
                            description is "View_and_manage_effort_certification_and_reporting."
                            icon is icon.CSFSourcing
                    large menu
                        page is FederalFinancialReports
                            title is "FederalFinancialReports"
                            menu item name is FederalFinancialReports
