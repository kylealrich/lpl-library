ProjectManagerHomePageUX1 is a Page
    title is "MyProjects"
    implements MenuSearch

    ProjectManagerHomePageUX1 is a DashBoardPanel
        title is "MyProjects"
        use 12 column grid
        ManageProjects is a Pane
            span 6 columns
            Layout
                single column
                    large menu
                        page is ProjectManagerPageUX1
                            title is "SummaryProjects"
                            menu item name is SummaryProjects
                            description is "View_projects_where_I_am_the_person_responsible_or_team_member_responsible_for_managing_the_summary_project_and_its_children._Views_include_posting_projects,_assignments,_and_project_personnel,_actual_and_billed_labor."
                            icon is icon.presentation-azure
                        large menu
                            page is PostingProjectsPage
                                title is "PostingProjects"
                                menu item name is PostingProjects
                                description is "View_posting_projects_status_list_and_posting_projects_dashboard."
                                icon is icon.presentation-azure
                        large menu
                            page is ProjectManagerAssetPage
                                title is "CapitalProjects"
                                menu item name is CapitalProjects
                                description is "View_posting_projects_that_are_flagged_for_capitalization._Select_a_posting_project_to_view_the_asset_interface_options,_the_asset_dashboard,_or_the_project_assets."
                                icon is icon.presentation-azure
        ProjectContracts is a Pane
            span 6 columns
            Layout
                single column
                    large menu
                        page is ProjectManagerFundingSourcePage
                            title is "FundingSources"
                            menu item name is FundingSources
                            description is "View_funding_sources_where_I_am_the_person_responsible_or_team_member_responsible_for_managing_the_summary_funding_source_and_its_children._Views_include_funding_source_dashboard_and_balances."
                            icon is icon.presentation-emerald
                    large menu
                        page is ProjectManagerContractsPage
                            title is "MyContracts"
                            menu item name is MyContracts
                            description is "View_contracts_and_contract_details_where_I_am_the_person_responsible_for_managing_the_contract._Views_include_contracts_dashboard_and_lists."
                            icon is icon.presentation-emerald
                    large menu
                        page is ProjectManagerTeamContractsPage
                            title is "TeamContracts"
                            menu item name is TeamContracts
                            description is "View_contracts_and_contract_details_where_I_am_a_team_member_responsible_for_managing_the_contract."
                            icon is icon.presentation-emerald
        TransactionsAndBalances is a Pane
            span 8 columns
            Layout
                two column distributed
                    column1
                        large menu
                            list is ProjectPersonnel.ProjectPersonnelTransactionList
                                title is "Transactions"
                                menu item name is Transactions
                                description is "View_project_transactions_for_posting_project_where_I_am_assigned_as_project_personnel."
                                icon is icon.person-global-amethyst
                        large menu
                            page is ProjectManagerCostPage
                                title is "Balances"
                                menu item name is Balances
                                description is "View_General_Ledger_balances_by_projects_and_account_or_project_account_category."
                                icon is icon.presentation-amethyst
                    column2
                        large menu
                            page is ProjectManagerCommitmentsPageUX1
                                title is "Commitments"
                                menu item name is Commitments
                                description is "View_commitments_and_encumbrances_by_posting_project._Views_include_open_commitments_for_requisitions,_purchase orders,_payable_invoices,_cash_ledger,_and_grant_salary_encumbrances."
                                icon is icon.clipboard-list-amethyst
                        large menu
                            page is ProjectManagerDistributionPageUX1
                                title is "ProjectDistributions"
                                menu item name is ProjectDistributions
                                description is "View_subsystem_distributions_by_posting_project._Distribution_lists_include_project_requisitions,_purchase_orders,_contract_line,_payables_invoices_and_cash_ledger."
                                icon is icon.clipboard-list-amethyst
        Analysis is a Pane
            span 4 columns
            Layout
                single column
                    large menu
                        page is ProjectManagerReportingSettingsUX1
                            title is "ProjectReports"
                            menu item name is ProjectReports
                            description is "View_project_cube_reports._Reports_include_project_obligation,_variance,_period_comparison,_percent_complete,_gross_margin_and_SEFA_worksheet."
                            icon is icon.clipboard-list-amber
                    large menu
                        page is ProjectManagerVendorContractPageUX1
                            title is "VendorContracts"
                            menu item name is VendorContracts
                            description is "View_a_list_of_the_vendors_that_are_assigned_to_the_project_contract._Vendors_can_be contractors,_suppliers,_or_any_other_relation_to_the_project_contract."
                            icon is icon.clipboard-list-amber

