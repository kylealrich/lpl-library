ConfigConsoleCreatePage is a Page
    title is "Create"

    Configuration is a Panel
        Layout
            header1 of "TailorUserInterface"
            three column
                column1
                    header4 of "ConfigureAnExistingComponent"
                    large menu
                        action is UserList.CreateConfiguration
                            title is "List"
                            menu item name is ConfigureList
                            icon is icon.listEmeraldOutline
                        action is UserForm.CreateConfiguration
                            title is "Form"
                            menu item name is ConfigureForm
                            icon is icon.formAzureOutline
                        action is UserCardView.CreateConfiguration
                            title is "CardView"
                            menu item name is ConfigureCardView
                            icon is icon.cardListAmethyst
                        action is UserPage.CreateConfiguration
                            title is "Page"
                            menu item name is ConfigurePage
                            icon is icon.interfaceAmberOutline
                        action is UserMenu.CreateConfiguration
                            title is "Menu"
                            menu item name is ConfigureMenu
                            icon is icon.menuSlate
                column2
                    header4 of "AddANewComponent"
                    large menu
                        action is UserList.CreateNewList
                            title is "List"
                            menu item name is AddList
                            icon is icon.listEmeraldOutline
                        action is UserForm.CreateNewForm
                            title is "Form"
                            menu item name is AddForm
                            icon is icon.formAzureOutline
                        action is UserCardView.CreateNewCard
                            title is "CardView"
                            menu item name is AddCardView
                            icon is icon.cardListAmethyst
                        action is UserPage.CreateNewPage
                            title is "Page"
                            menu item name is AddPage
                            icon is icon.interfaceAmberOutline
                        action is UserMenu.CreateNewMenu
                            title is "Menu"
                            menu item name is AddMenu
                            icon is icon.menuSlate
                        action is UserNavigation.CreateNewNavigation
                            title is "Navigation"
                            menu item name is AddNavigation
                            icon is icon.reportAmethystOutline
                column3
                    header4 of "ApplicationLevelComponents"
                    large menu
                        action is UserWebApp.CreateConfiguration
                            title is "WebApplication"
                            menu item name is ConfigureWebApp
                            description is "Configure only"
                            icon is icon.webAppRuby
            header1 of "TailorBusinessLogic"
            three column
                column1
                    header4 of "AddANewField"
                    large menu
                        action is UserField.CreateField
                            title is "Field"
                            menu item name is CreateField
                            description is "A field where data can be entered, stored and changed"
                            icon is icon.menuTurquoise
                        action is UserField.CreateConditionField
                            title is "Condition"
                            menu item name is CreateConditionField
                            description is "A special boolean field that returns true or false"
                            icon is icon.menuTurquoise
                        action is UserField.CreateDerivedField
                            title is "Derived"
                            menu item name is CreateDerivedField
                            description is "A field that returns a derived value based on the logic of your LPL definition"
                            icon is icon.menuTurquoise
                        action is UserField.CreateComputeField
                            title is "Compute"
                            menu item name is CreateComputedField
                            description is "A field that returns a computed value based on your LPL definition"
                            icon is icon.menuTurquoise
                        action is UserField.CreateSnapshotField
                            title is "Snapshot"
                            menu item name is CreateSnapshotField
                            description is "A field that mirrors and is kept in sync with another field"
                            icon is icon.menuTurquoise
                        action is UserField.CreateContextField
                            title is "Context"
                            menu item name is CreateContextField
                            description is "A non-ontological context field"
                            icon is icon.menuTurquoise
                column2
                    header4 of "Configure_andAddActions"
                    large menu
                        action is UserAction.CreateConfiguration
                            title is "CreateActionConfiguration"
                            menu item name is CreateConfiguration
                            description is "Configure a delivered Action. Add Entrance/Exit rules and require reason/comments/effective date"
                            icon is icon.checklistRubyOutline
                        action is UserAction.CopyNewAction
                            title is "CopyAction"
                            menu item name is CopyNewAction
                            description is "Copy a delivered Action. Get a new name and new security"
                            icon is icon.checklistRubyOutline
                        action is UserAction.CreateActionExtension
                            title is "CreateActionExtension"
                            menu item name is CreateActionExtension
                            description is "Configure a delivered Action. Get a new name, a new flow and require reason/comments/effective date"
                            icon is icon.checklistRubyOutline
                        action is UserAction.CreateNewRequestAction
                            title is "CreateNewRequestAction"
                            menu item name is CreateNewRequestAction
                            description is "Create a new Request Action with parameters and workflow"
                            icon is icon.checklistRubyOutline
                        action is UserAction.CreateNewAction
                            title is "CreateNewAction"
                            menu item name is CreateNewAction
                            description is "Create a new Action. Various action types are available such as Create, Import and Set Actions"
                            icon is icon.checklistRubyOutline
                column3
                    header4 of "Other"
                    large menu
                        action is UserRelation.CreateRelation
                            title is "CreateNewRelation"
                            menu item name is CreateRelation
                            icon is icon.reportAmethystOutline
                    large menu
                        action is UserCube.ConfigureCube
                            title is "ConfigureCube"
                            menu item name is ConfigureCube
                            icon is icon.reportAmethystOutline
                    large menu
                        action is UserBusinessClass.CreateBusinessClass
                            title is "CreateNewBusinessClass"
                            menu item name is CreateBusinessClass
                            icon is icon.reportAmethystOutline
                    large menu
                        action is UserProd.CreateConfiguration
                            title is "ConfigureProductLine"
                            menu item name is ConfigureProductLine
                            icon is icon.reportAmethystOutline

    Personalization is a Panel
        Layout
            header1 of "PersonalizeYourUserInterface"
            three column
                column1
                    large menu
                        action is UserList.CreatePersonalization
                            title is "List"
                            menu item name is PersonalizationList
                            icon is icon.listEmeraldOutline
                        action is UserCardView.CreatePersonalization
                            title is "CardView"
                            menu item name is AddList1
                            icon is icon.cardListAmethyst
                column2
                    large menu
                        action is UserForm.CreatePersonalization
                            title is "Form"
                            menu item name is PersonalizationForm
                            icon is icon.formAzureOutline
                        action is UserPage.CreatePersonalization
                            title is "Page"
                            menu item name is PersonalizationPage
                            icon is icon.interfaceAmberOutline
                column3
                    large menu
                        action is UserMenu.CreatePersonalization
                            title is "Menu"
                            menu item name is PersonalizationMenu
                            icon is icon.menuSlate
            paragraph
                text of "Pro_Tip:_PersonalizationsAreAGreatWayToBeginMost_User_InterfaceConfigurations." with icon.new-solid

    Report is a Panel
        Layout
            header1 of "ExpandYourReporting"
            three column
                column1
                    large menu
                        action is UserFolder.CreateListReport
                            title is "CreateReport"
                            menu item name is ConfigureListReport
                            description is "Create a Report from any List. Then Personalize it for all report users."
                            icon is icon.reportTurquoiseOutline
                    large menu
                        action is UserFolder.CreatePageReport
                            title is "CreatePageReport"
                            menu item name is ConfigurePageReport
                            description is "Create a Report from any Page. Then Personalize it for all report users."
                            icon is icon.reportSlateOutline
                column2
                    large menu
                        action is UserList.CreateNewList
                            title is "AddList"
                            menu item name is AddList2
                            description is "Add a brand new list and tie it into existing menus, pages or forms."
                            icon is icon.listEmeraldOutline
                        action is UserPage.CreateNewPage
                            title is "AddPage"
                            menu item name is AddPage1
                            description is "Add a brand new page and tie it into existing menus or web apps."
                            icon is icon.interfaceAmberOutline
                column3
                    large menu
                        action is UserSJRptMap.CreateConfiguration
                            title is "StaticReport"
                            menu item name is ConfigureStaticReport
                            icon is icon.frameworkGenericForm

