ReceivablesHomePage is a Page
    title is "Overview"
    implements MenuSearch

    QuickActions is a DashBoardPanel
        title is "Processes_and_Reports"
        use 12 column grid
        QuickActions is a Pane
            span 6 columns
            title is "Processes"
            Layout
                two column distributed
                    column1
                        small menu
                            action is AutoCashApplication.AutoCashApplication
                                title is "AutomaticCashApplication"
                                menu item name is AutomaticCashApplication
                                description is "Automatically_apply_payments_to_invoices_using_the_customer's_automatic_application_method."
                                icon is icon.check-azure
                            action is AutomaticMemoApplication.AutomaticMemoApplication
                                title is "AutomaticMemoApplication"
                                menu item name is AutomaticMemoApplication
                                description is "Automatically_apply_credit_memos_to_cross-referenced_invoices,_or_apply_all_open_credits_to_open_invoices."
                                icon is icon.compare-azure
                            action is AutomaticPrepaymentApply.AutomaticPrepaymentApply
                                title is "AutomaticPrepaymentApply"
                                menu item name is AutomaticPrepaymentApply
                                description is "Automatically_apply_customer_prepayments."
                                icon is icon.document-finance-azure
                            action is ARStatementPrint.ARStatementPrint
                                title is "StatementPrint"
                                menu item name is StatementPrint
                                description is "Create_account_statements_for_customers."
                                icon is icon.person-finance-azure
                    column2
                        small menu
                            action is ApplicationCloseResult.PerformApplicationClose
                                title is "ApplicationClose"
                                menu item name is ApplicationClose
                                description is "Completes_applications_to_be_journalized."
                                icon is icon.check-azure
                            action is ReceivableActivitySummary.ReceivableActivitySummary
                                title is "ReceivableActivitySummary"
                                menu item name is ReceivableActivitySummary
                                description is "Updates_customer_aging_balances."
                                icon is icon.cycle-azure
                            action is TransactionStatusUpdate.TransactionStatusUpdate
                                title is "TransactionStatusUpdate"
                                menu item name is TransactionStatusUpdate
                                description is "Updates_the_status_of_non-historical_applied_transactions._Required_for_best_report_performance."
                                icon is icon.check-azure
                            action is PeriodCloseReport.PeriodCloseReport
                                title is "PeriodCloseReport"
                                menu item name is PeriodCloseReport
                                description is "Preview_pending_transactions_and_close_receivables."
                                icon is icon.document-certified-azure
        QuickReports is a Pane
            span 6 columns
            title is "Reports"
            Layout
                two column distributed
                    column1
                        small menu
                            action is CompanyAgingReport.CompanyAgingReport
                                title is "CompanyAging"
                                menu item name is CompanyAgingReport
                                description is "Age_invoices_and_payments_by_company,_as_of_a_specified_date."
                                icon is icon.document-finance-emerald
                            action is CustomerAgingReport.CustomerAgingReport
                                title is "CustomerAging"
                                menu item name is CustomerAgingReport
                                description is "Age_invoices_and_payments_by_customer,_as_of_a_specified_date."
                                icon is icon.document-finance-emerald
                            action is NationalAccountAgingReport.NationalAccountAgingReport
                                title is "NationalAccountAging"
                                menu item name is NationalAccount
                                description is "Age_invoices_and_payments_by_national_account,_as_of_a_specified_date."
                                icon is icon.group-emerald
                    column2
                        small menu
                            action is ReceivablesTieBackReport.ReceivablesTieBackReport
                                title is "TieBack"
                                menu item name is TieBackReport
                                description is "Open_receivables_by_global_ledger_account,_as_of_a_specified_date."
                                icon is icon.document-finance-emerald
                            action is ARCustomerProjectAgingReport.ARCustomerProjectAgingReport
                                title is "ProjectAging"
                                menu item name is Project
                                description is "Age_invoices_and_payments_by_project,_as_of_a_specified_date."
                                icon is icon.presentation-emerald
                            action is CreditExceptionReport.CreditExceptionReport
                                title is "CreditException"
                                menu item name is CreditExceptionReport
                                description is "Customers_exceeding_their_credit_limit,_or_with_transactions_a_number_of_days_past_due."
                                icon is icon.clipboard-check-emerald
        AutomaticCashApplicationResults is a Pane
            span 6 columns
            business class is CashApplicationResult
                list is ShortCashApplicationResultsWithTag
                    overview navigation is CashApplicationResultsListNav
        ApplicationCloseResults is a Pane
            span 6 columns
            business class is ApplicationCloseResult
                list is ShortApplicationCloseResultsWithTag
                    overview navigation is ResultsListNav
        PrintFiles is a Pane
            span 6 columns
            title is "MyPrintFiles"
            business class is UserReport
                list is MyUserReportsCatalogList
                    overview navigation is ViewMyUserReportsList
        MyIDMJobs is a Pane
            span 6 columns
            title is "MyIDMJobs"
            business class is IDMJob
                list is IDMJobList

    Journalize is a Panel
        title is "Journalize"
        business class is ReceivableGLDistribution
            list is JournalizePreviewListWithResultsLink
                title is "Journalize"

    CreditAndCollections is a DashBoardPanel
        title is "Credit_and_Collections"
        use 12 column grid
        CreditAnalystCustomerReview is a Pane
            span 6 columns
            business class is CreditAnalyst
                list is ShortCreditAnalystCustomerReview
                    overview navigation is CreditAnalystCustomerReviewNav
        CompanyCustomers is a Pane
            span 6 columns
            business class is CompanyCustomer
                list is CompanyCustomersWithBalance
                    overview navigation is CompanyCustomerListNav
        Deductions is a Pane
            span 6 columns
            title is "Deductions"
            Layout
                two column distributed
                    column1
                        large menu
                            list is ReceivableApplicationAdjustment.AdjustmentsExceededReviewList
                                title is "AdjustmentsExceededReview"
                                menu item name is AdjustmentsExceededReview
                                description is "Review_adjustments_exceeding_their_limit."
                                icon is icon.check-azure
                            list is ReceivableApplicationAdjustment.AdjustmentMaintenance
                                title is "AdjustmentMaintenance"
                                menu item name is AdjustmentMaintenance
                                description is "Review_and_update_existing_adjustments."
                                icon is icon.clipboard-list-azure
                    column2
                        large menu
                            list is ReceivableApplicationAdjustment.AdjustmentsNeedingReviewByProcessLevel
                                title is "ProcessLevelAdjustmentReview"
                                menu item name is ProcessLevelAdjustmentReview
                                description is "Adjustments_needing_review_by_process_level."
                                icon is icon.clipboard-check-azure
                            action is AutomaticTransactionRemoval.AutomaticTransactionRemoval
                                title is "AutomaticTransactionRemoval"
                                menu item name is AutomaticTransactionRemoval
                                description is "Automatically_write_off_transactions_based_on_specified_criteria."
                                icon is icon.recall-azure
        CustomerExceed is a Pane
            span 6 columns
            title is "CustomersExceededCreditLimit"
            business class is CompanyCustomer
                list is ShortCustomerExceededLimitList
                    overview navigation is CustomersExceededLimitNav

    Dashboard is a DashBoardPanel
        title is "AtAGlance"
        use 12 column grid
        UnappliedPayments is a Pane
            span 4 columns
            business class is ReceivablePayment
                list is UnappliedCashCardList
                    overview navigation is UnappliedCashListNav
        UnappliedCredit is a Pane
            span 4 columns
            business class is ReceivableInvoiceDetail
                list is UnappliedCreditCardList
                    overview navigation is OpenCreditMemoListNav
        UnassignedPayments is a Pane
            span 4 columns
            business class is ReceivablePayment
                list is UnassignedCashCardList
                    overview navigation is UnassignedCashListNav
        OverdueInvoices is a Pane
            span 4 columns
            business class is ReceivableInvoiceDetail
                list is OverDueInvoicesCardList
                    overview navigation is OverdueInvoicesListNav
        UnresolvedDisputes is a Pane
            span 4 columns
            business class is ReceivableInvoiceDispute
                list is ReceivableOpenInvoiceDisputeCardList
                    overview navigation is ReceivableOpenInvoiceDisputeListNav
        UnpostedApplications is a Pane
            span 4 columns
            business class is ReceivableApplication
                list is UnpostedApplicationsCardList
                    overview navigation is UnpostedApplicationsNav
        UnreleasedPayments is a Pane
            span 4 columns
            business class is ReceivablePayment
                list is UnreleasedPaymentsCardList
                    overview navigation is UnreleasedPaymentsCardListNav
        UnreleasedRTM is a Pane
            span 4 columns
            business class is RTMPayment
                list is UnreleasedRTMPaymentCardList
                    overview navigation is UnreleasedRTMPaymentListNav
        AdjustmentExceededReview is a Pane
            span 4 columns
            title is "AdjustmentsExceededLimitReview"
            business class is ReceivableApplicationAdjustment
                list is AdjustmentExceedCardList
                    overview navigation is AdjustmentsExceededReviewListNav

