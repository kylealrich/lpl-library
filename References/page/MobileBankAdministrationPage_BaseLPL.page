MobileBankAdministrationPage is a Page
    title is "BankAdministration"
    show panel control on left

    FinancialInstitutionSummary is a Panel
        business class is FinancialInstitution
            list is MobileFinancialInstitutionDashBoard

    AccountSummary is a Panel
        business class is CashManagementAccount
            list is MobileCashAccountDashBoard

    FeeServiceDashBoard is a DashBoardPanel
        title is "FeeServiceAnalysis"
        mouse over text is "FeeServiceDashboard"
        show 4 columns
        FeeServiceSummary is a Pane
            double width
            business class is BankServiceDetail
                list is BankServiceCube
                    title is "FeeServiceSummary"
                    overview navigation is BankServiceCubeLink
                        label is "Detail"
        ServiceTrend is a Pane
            double width
            business class is BankServiceDetail
                list is MonthlyServiceCube
                    title is "MonthlyFeeServiceTrend"
                    display ServiceTrend
                    Row Dimensions
                        CashManagementAccount.FinancialInstitution
                        BankServiceCode.BankServiceCategory
                        BankServiceCode
                    overview navigation is MonthlyServiceCubeLink
                        label is "Detail"
        ServicesByAccount is a Pane
            double width
            business class is BankServiceDetail
                list is BankServiceCube
                    title is "FeeServiceChargedByAccount"
                    display BankServicesChargedByAccountChart
                    Row Dimensions
                        CashManagementAccount
                        BankServiceCode.BankServiceCategory
                        BankServiceCode
                    overview navigation is BankServiceCubeLink
                        label is "Detail"
        ServicesChargedByCategory is a Pane
            double width
            business class is BankServiceDetail
                list is BankServiceCube
                    title is "FeeServiceChargedByCategory"
                    display ServicesChargedByCategory
                    Row Dimensions
                        BankServiceCode.BankServiceCategory
                        BankServiceCode
                    overview navigation is BankServiceCubeLink
                        label is "Detail"
        ServicesByInstitution is a Pane
            double width
            business class is BankServiceDetail
                list is BankServiceCube
                    title is "FeeServiceByInstitution"
                    display ServicesByInstitutionChart
                    Row Dimensions
                        CashManagementAccount.FinancialInstitution
                        BankServiceCode.BankServiceCategory
                        BankServiceCode
                    overview navigation is BankServiceCubeLink
                        label is "Detail"
        ServicesByLocation is a Pane
            double width
            business class is BankServiceDetail
                list is BankServiceCube
                    title is "FeeServiceByLocation"
                    display ServicesByLocationChart
                    Row Dimensions
                        CashManagementAccount.CashManagementLocation
                        CashManagementAccount.FinancialInstitution
                        BankServiceCode.BankServiceCategory
                        BankServiceCode
                    overview navigation is BankServiceCubeLink
                        label is "Detail"
        ServicesByLegalEntity is a Pane
            double width
            business class is BankServiceDetail
                list is BankServiceCube
                    title is "FeeServiceByLegalEntity"
                    display ServicesByLegalEntityChart
                    Row Dimensions
                        CashManagementAccount.LegalEntity
                        CashManagementAccount.FinancialInstitution
                        BankServiceCode.BankServiceCategory
                        BankServiceCode
                    overview navigation is BankServiceCubeLink
                        label is "Detail"

