            business class is ContractParticipant
                list is ContractParticipantSearch
        pane 2
            business class is ContractDistributorPricingMember
                list is DistributorMemberSearch
        pane 3
            business class is ContractTierMember
                list is ManufacturerMemberSearch

    ContactsAndSubcontractors is a MultiPanePanel
