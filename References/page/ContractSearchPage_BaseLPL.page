    title is "SearchForContracts"

    Contracts is a Panel
        business class is Contract
            list is ContractSearch

    LineItems is a Panel
        business class is ContractLine
            list is ItemSearch

    ParticipantsAndMembers is a MultiPanePanel
        title is "Participants_andMembers"
