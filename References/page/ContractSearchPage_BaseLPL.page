            list is RebateSearch

    MilestonesAndDeliverables is a MultiPanePanel
        title is "Milestones_andDeliverables"
        pane 1
            business class is ContractMilestone
                list is MilestoneSearch
        pane 2
            business class is ContractDeliverable
