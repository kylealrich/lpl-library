            business class is ContractTermAndCondition
                list is TermAndConditionSearch

    CommentsAndAttachments is a MultiPanePanel
        title is "Comments_andAttachments"
        pane 1
            business class is ContractComment
                list is CommentSearch
        pane 2
            business class is ContractAttachment
                list is AttachmentSearch

    Rebates is a Panel
