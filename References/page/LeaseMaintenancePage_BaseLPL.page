LeaseMaintenancePage is a Page
    title is "<actor.context.FinanceEnterpriseGroup.LeasesAdministrationTitle>"
    implements MenuSearch

    Setup is a DashBoardPanel
        title is "Setup"
        use 12 column grid
        NonLeaseComponents is a Pane
            span 4 columns
            business class is ExecutoryCostCode
                list is ExecutoryCostCodeMaitenance
                    overview navigation is ExecutoryCostCodeNav
        Templates is a Pane
            span 4 columns
            business class is LeaseTemplate
                list is LeaseTemplateMaitenance
                    overview navigation is LeaseTemplateNav
        LeaseTypes is a Pane
            span 4 columns
            business class is LeaseType
                list is LeaseTypeMaitenance
                    overview navigation is LeaseTypeNav
        AllocationCodes is a Pane
            span 4 columns
            business class is LeaseAllocationCode
                list is LeaseAllocationMaitenance
                    overview navigation is LeaseAllocationNav
        DateCodes is a Pane
            span 4 columns
            business class is LeaseDateCode
                list is LeaseDateCodeMaitenance
                    overview navigation is LeaseDateCodeNav
        LeaseRateTable is a Pane
            span 4 columns
            business class is LeaseRateTable
                list is LeaseRateTableMaintenance
                    overview navigation is LeaseRateTableNav
        LeaseAdjustmentReasonCode is a Pane
            span 4 columns
            business class is LeaseAdjustmentReasonCode
                list is LeaseReasonCodeMaitenance
                    overview navigation is LeaseAdjustmentReasonCodeNav

    Dashboard is a DashBoardPanel
        title is "Utilities"
        use 12 column grid
        Leases is a Pane
            span 6 columns
            Layout
                single column
                    small menu
                        action is Lease.LeasePurge
                            title is "PurgeLeases"
                            menu item name is LeasePurge
                            icon is icon.check-azure
                        action is Lease.LeaseHistoryPurgeForAllCompanies
                            title is "PurgeLeaseHistoryForAllCompanies"
                            menu item name is LeaseHistoryPurgeForAllCompanies
                            icon is icon.check-azure
                        list is LeasePurgeResults.LeasePurgeResultsList
                            title is "LeasePurgeResults"
                            menu item name is LeasePurgeResults
                            icon is icon.check-azure
                        action is Lease.LeaseBuildIndexSetAction
                            title is "CreateKeywordsForLeaseSearch"
                            menu item name is CreateKeywordsForLeaseSearch
                            icon is icon.check-azure
                        visible when (actor.context.FinanceEnterpriseGroup.LeaseImpairmentsExists)
                            action is LeaseImpairment.LeaseImpairmentsTransfer
                                title is "LeaseImpairmentsTransfer"
                                menu item name is LeaseImpairmentsTransfer
                                icon is icon.check-azure
                        action is LeasePaymentPeriodBalance.SetNewDisclosureReportFields
                            title is "UpdateDisclosureReport"
                            menu item name is UpdateDisclosureReport
                            icon is icon.check-azure
        IDM is a Pane
            span 6 columns
            Layout
                single column
                    small menu
                        page is MyIDMJobsPage
                            title is "MyIDMJobs"
                            menu item name is MyIDMJobs
                            icon is icon.check-emerald
                        page is IDMPrinterPage
                            title is "IDMPrinter"
                            menu item name is IDMPrinter
                            icon is icon.check-emerald
        CubeRefresh is a Pane
            span 12 columns
            Layout
                two column distributed
                    column1
                        header4 of "LeasePaymentPeriodBalanceCube"
                        small menu
                            header4 of "Status"
                            visible when (actor.context.LeasePaymentPeriodBalance.CubeOnlineMode)
                                text of "Cube_is_online"
                                    bold
                                    color of green
                            visible when (actor.context.LeasePaymentPeriodBalance.CubeOfflineMode)
                                text of "Cube_is_offline"
                                    bold
                                    color of red
                            paragraph
                                text of "LastRefreshDate"
                                text of "<actor.context.LeasePaymentPeriodBalance.LeasePaymentPeriodBalanceAnalyticCubeRel.LastRefreshDate>"
                                    bold
                            action is LeasePaymentPeriodBalance.UpdateLeasePaymentPeriodBalanceCube
                                title is "RefreshCube"
                                menu item name is RefreshLeasePaymentPeriodBalanceCube
                                description is "Will_refresh_the_cube"
                                icon is icon.outline-process-ruby
                    column2
                        visible when (not actor.context.FinanceEnterpriseGroup.ShortTermLiabilityAccounting)
                            header4 of "MassConversionIntoShortAndLongLiabilityAccounts"
                        small menu
                            visible when (not actor.context.FinanceEnterpriseGroup.ShortTermLiabilityAccounting)
                                header4 of "ForSingleCompany"
                            visible when (not actor.context.FinanceEnterpriseGroup.ShortTermLiabilityAccounting)
                                action is Lease.MassConversionofLiabilityAccountsForOneCompany
                                    title is "RunConversionProcess"
                                    menu item name is RunConversionIntoShortAndLongTerms
                                    icon is icon.check-emerald
                                blank line
                            visible when (not actor.context.FinanceEnterpriseGroup.ShortTermLiabilityAccounting)
                                header4 of "ForMultipleCompanies"
                            visible when (not actor.context.FinanceEnterpriseGroup.ShortTermLiabilityAccounting)
                                action is Lease.SetLiabilityAccounts
                                    title is "StartTheConversionProcess"
                                    menu item name is SetLiabilityAccounts
                                    icon is icon.check-azure
                            visible when (not actor.context.FinanceEnterpriseGroup.ShortTermLiabilityAccounting)
                                list is LeaseCompany.LeaseCompanyList
                                    title is "SetTheLiabilityAccounts"
                                    menu item name is LeaseCompanies
                                    icon is icon.check-azure
                            visible when (not actor.context.FinanceEnterpriseGroup.ShortTermLiabilityAccounting)
                                action is Lease.MassConversionOfLiabilityAccounts
                                    title is "RunConversionProcess"
                                    menu item name is ConversionIntoShortAndLongTerms
                                    icon is icon.check-azure

