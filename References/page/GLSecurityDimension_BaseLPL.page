GLSecurityDimension is a Page
    title is "SecurityGroups"

    SecurityGroupPanel is a DashBoardPanel
        title is "Main"
        use 12 column grid
        GLDimensionSecurity is a Pane
            span 9 columns
            title is "DimensionSecurity"
            Layout
                two column distributed
                    list is FinanceDimensionSecurityGroup.ChartAccountSecurityGroupList
                        title is "<actor.context.FinanceEnterpriseGroup.AccountLabel>"
                        menu item name is Account
                        icon is icon.outline-utility-azure
                    list is FinanceDimensionSecurityGroup.LedgerBasisSecurityGroupList
                        title is "Ledger"
                        menu item name is Ledger
                        icon is icon.outline-utility-azure
                    list is FinanceDimensionSecurityGroup.AccountingEntitySecurityGroupList
                        title is "<actor.context.FinanceEnterpriseGroup.AccountingEntityLabel>"
                        menu item name is AccountingEntity
                        icon is icon.outline-utility-azure
                    visible when (actor.context.FinanceEnterpriseGroup.AccountingUnitSelected)
                        list is AccountingUnitSecurityGroup.primary
                            title is "<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>"
                            menu item name is AccountingUnit
                            icon is icon.outline-utility-azure
                    visible when (actor.context.FinanceEnterpriseGroup.ProjectEntry)
                        list is FinanceDimensionSecurityGroup.ProjectSecurityGroupList
                            title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>"
                            menu item name is Project
                            icon is icon.outline-utility-azure
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension1Entry)
                        list is FinanceDimensionSecurityGroup.FinanceDimension1SecurityGroupList
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension1Label>"
                            menu item name is FinanceDimension1
                            icon is icon.outline-utility-azure
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension2Entry)
                        list is FinanceDimensionSecurityGroup.FinanceDimension2SecurityGroupList
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>"
                            menu item name is FinanceDimension2
                            icon is icon.outline-utility-azure
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension3Entry)
                        list is FinanceDimensionSecurityGroup.FinanceDimension3SecurityGroupList
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension3Label>"
                            menu item name is FinanceDimension3
                            icon is icon.outline-utility-azure
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension4Entry)
                        list is FinanceDimensionSecurityGroup.FinanceDimension4SecurityGroupList
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension4Label>"
                            menu item name is FinanceDimension4
                            icon is icon.outline-utility-azure
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension5Entry)
                        list is FinanceDimensionSecurityGroup.FinanceDimension5SecurityGroupList
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension5Label>"
                            menu item name is FinanceDimension5
                            icon is icon.outline-utility-azure
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension6Entry)
                        list is FinanceDimensionSecurityGroup.FinanceDimension6SecurityGroupList
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension6Label>"
                            menu item name is FinanceDimension6
                            icon is icon.outline-utility-azure
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension7Entry)
                        list is FinanceDimensionSecurityGroup.FinanceDimension7SecurityGroupList
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension7Label>"
                            menu item name is FinanceDimension7
                            icon is icon.outline-utility-azure
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension8Entry)
                        list is FinanceDimensionSecurityGroup.FinanceDimension8SecurityGroupList
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension8Label>"
                            menu item name is FinanceDimension8
                            icon is icon.outline-utility-azure
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension9Entry)
                        list is FinanceDimensionSecurityGroup.FinanceDimension9SecurityGroupList
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension9Label>"
                            menu item name is FinanceDimension9
                            icon is icon.outline-utility-azure
                    visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension10Entry)
                        list is FinanceDimensionSecurityGroup.FinanceDimension10SecurityGroupList
                            title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension10Label>"
                            menu item name is FinanceDimension10
                            icon is icon.outline-utility-azure
        SecurityActor is a Pane
            span 3 columns
            title is "SecurityGroupActorContext"
            Layout
                single column
                    action is CopyActorContext.ExecuteCopy
                        title is "CopyActorContext"
                        menu item name is CopyActorContext
                        icon is icon.outline-utility-amber
                    page is GeneralLedgerSecurityActorContext
                        title is "SetUpActorForGLSecurityGroup"
                        menu item name is SetUpActorForGLSecurityGroup
                        icon is icon.outline-utility-amber
        OtherSecurity is a Pane
            span 9 columns
            title is "AdditionalSecurity"
            Layout
                two column distributed
                    list is FinanceDimensionSecurityGroup.CompanySecurityGroupList
                        title is "Company"
                        menu item name is Company
                        icon is icon.outline-utility-emerald
                    list is FinanceDimensionSecurityGroup.CashCodeSecurityGroupList
                        title is "CashCode"
                        menu item name is CashCode
                        icon is icon.outline-utility-emerald
                    list is FinanceDimensionSecurityGroup.VendorClassSecurityGroupList
                        title is "VendorClass"
                        menu item name is VendorClass
                        icon is icon.outline-utility-emerald
                    list is BusinessGroup.BusinessGroupListForSecurity
                        title is "VendorBusiness"
                        menu item name is VendorBusiness
                        icon is icon.outline-utility-emerald
                    list is CustomerBusinessGroup.BusinessGroupListForSecurity
                        title is "CustomerBusiness"
                        menu item name is CustomerBusiness
                        icon is icon.outline-utility-emerald
                    list is ApprovalCodeSecurityGroup.primary
                        title is "ApprovalCode"
                        menu item name is ApprovalCode
                        icon is icon.outline-utility-emerald

    ChartAccount is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.AccountLabel>"
        business class is FinanceDimensionSecurityGroup
            list is ChartAccountSecurityGroupList
                title is "<actor.context.FinanceEnterpriseGroup.AccountLabel>"
            Context
