MatchInterfacePage is a Page
    title is "MatchInvoiceInterface"
    implements MenuSearch

    Invoices is a Panel
        title is "Invoices"
        business class is MatchInvoiceImport
            list is InterfaceInvoicesList
                title is "Invoices"

    Process is a Panel
        Layout
            two column
                large menu
                    action is InterfaceMatchInvoices.InterfaceMatchInvoices
                        title is "InterfaceMatchInvoices"
                        menu item name is InterfaceMatchInvoices
                        icon is icon.eventCreationWizard
                    action is InterfaceSelectedMatchInvoiceRunGroups.ScheduleInterface
                        title is "InterfaceMatchSelectedRunGroups"
                        menu item name is InterfaceMatchSelectedRunGroups
                        icon is icon.requestActionsIcon

    Results is a Panel
        title is "Results"
        business class is PayablesInvoiceInterfaceResult
            list is MatchInvoiceInterfaceResults
                title is "Results"

