GlobalLedgerBudget is a Page
    title is "Budgeting"
    implements MenuSearch

    Scenarios is a Panel
        title is "Overview"
        business class is Scenario
            list is BudgetScenarioDashBoard
                title is "Overview"

    BudgetScenarios is a Panel
        title is "Scenarios"
        business class is Scenario
            list is Budgets
                title is "Scenarios"
            Context
                FinanceEnterpriseGroup

    BudgetTemplates is a Panel
        title is "Templates"
        business class is BudgetTemplate
            list is BudgetTemplateList
                title is "Templates"
            Context
                FinanceEnterpriseGroup

    ChangeRequests is a Panel
        business class is BudgetChangeOrder
            list is BudgetChangeOrders
            Context
                FinanceEnterpriseGroup

    UnreleasedObligations is a Panel
        business class is BudgetTemplateGroupTotal
            list is BudgetTemplateGroupTotalList
            Context
                FinanceEnterpriseGroup

    BudgetEdits is a Panel
        title is "EditBatch"
        business class is BudgetEditBatch
            list is BudgetEditBatchList
                title is "EditBatch"

    BudgetEditErrors is a Panel
        title is "EditErrors"
        business class is BudgetEditError
            list is EditErrorList
                title is "EditErrors"

    GLBudget is a DashBoardPanel
        title is "More"
        use 12 column grid
        GLBudgetPane is a Pane
            span 3 columns
            title is "SelectiveTemplate"
            Layout
                single column
                    page is BudgetLazyPrimePage
                        title is "Transactions_forSelectiveActivation"
                        menu item name is TransactionsForSelectiveActivation
                        icon is icon.document-finance-emerald
        Threshold is a Pane
            span 3 columns
            title is "Threshold"
            Layout
                single column
                    list is BudgetGroup.AllCheckedGroups
                        title is "GroupsOverThreshold"
                        menu item name is GroupsOverThreshold
                        icon is icon.chart-stacked-bar-azure
                    action is BudgetGroupCheck.CheckGroups
                        title is "CheckGroups"
                        menu item name is CheckGroups
                        icon is icon.check-azure
        BudgetActions is a Pane
            span 4 columns
            title is "ConfigurationAndUtilities"
            Layout
                single column
                    list is BudgetEditConfiguration.primary
                        title is "BudgetEditConfiguration"
                        menu item name is BudgetEditConfiguration
                        icon is icon.settings-azure
                    action is GeneralLedgerTotal.FixPeriodEndingDates
                        title is "FixPeriodEndingDates_forBudgetaryRows"
                        menu item name is FixPeriodEndingDatesForBudgetaryRows
                        icon is icon.settings-refresh-amber
        Reports is a Pane
            span 3 columns
            title is "Reports"
            Layout
                single column
                    list is BudgetGroupObligationVarianceRun.primary
                        title is "BudgetGroupObligationVariance"
                        menu item name is BudgetGroupObligationVariance
                        icon is icon.chart-stacked-bar-azure

