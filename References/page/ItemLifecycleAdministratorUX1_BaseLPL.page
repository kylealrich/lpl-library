ItemLifecycleAdministratorUX1 is a Page
    title is "ItemLifecycleAdministration"
    implements MenuSearch

    OtherModules is a DashBoardPanel
        use 12 column grid
        ItemLifecycle is a Pane
            span 12 columns
            icon is icon.outline-maintenance-amber
            Layout
                single column
                    small menu
                        page is RecallUsersPage
                            title is "Users"
                            menu item name is Users
                        list is RecallGroup.RecallGroups
                            title is "Groups"
                            menu item name is ItemLifecycleManagementGroups
                        list is RecallDomain.RecallDomains
                            title is "Domains"
                            menu item name is Domains
                        page is RecallOrganizationPage
                            title is "Organizations"
                            menu item name is Organizations
                        list is RecallTaskInstruction.RecallTaskInstructions
                            title is "TaskInstructions"
                            menu item name is TaskInstructions
                        list is RecallType.RecallTypes
                            title is "NoticeTypes"
                            menu item name is NoticeTypes
                        list is ActionReason.ActionReasonList
                            title is "ActionReasons"
                            menu item name is ActionReasons

