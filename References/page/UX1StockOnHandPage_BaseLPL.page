UX1StockOnHandPage is a Page
    title is "Inventory_Information"
    implements MenuSearch

    StockOnHand is a Panel
        business class is ItemLocation
            list is ItemLocationStockOnHand

    DaysOnHand is a Panel
        visible when (actor.context.FinanceEnterpriseGroup.DaysOnHandInUse)
        business class is ItemLocation
            list is DaysOnHandList

    LotsAndBins is a Panel
        business class is StockOnHandDetail
            list is StockOnHandForLotAndSerial

    SerialNumbers is a Panel
        business class is ItemSerialNumber
            list is SerialNumbersOnHand

    TransactionHistory is a Panel
        business class is InventoryTransactionLine
            list is ReleasedTransactions

    CostHistory is a Panel
        business class is InventoryCostHistory
            list is CostHistory

    DemandAndSupply is a Panel
        business class is WarehouseDemandLine
            list is WarehouseDemandAndSupply

    PotentialObsolete is a Panel
        business class is ItemLocation
            list is PotentialObsoleteList

    PotentialOverstock is a Panel
        business class is ItemLocation
            list is PotentialOverstockList

