UX1FranchiseInterfacesPage is a Page
    title is "Interfaces"
    implements MenuSearch

    Interfaces is a DashBoardPanel
        use 12 column grid
        BudgetImports is a Pane
            span 6 columns
            business class is FranchiseBudgetImport
                list is FranchiseBudgetImportNewList
                    overview navigation is BudgetImportsNav
        SalesImports is a Pane
            span 6 columns
            business class is FranchiseSalesImport
                list is FranchiseSalesImportNewList
                    overview navigation is SalesImportNav
        Interfaces is a Pane
            span 6 columns
            Layout
                two column distributed
                    large menu
                        action is FranchiseSalesInterface.FranchiseSalesInterface
                            title is "SalesInterface"
                            menu item name is SalesInterface
                            description is "Interface imported sales"
                            icon is icon.outline-process-amethyst
                        action is SalesBudgetInterface.SalesBudgetInterface
                            title is "BudgetInterface"
                            menu item name is BudgetInterface
                            description is "Interface imported customer based sales budgets"
                            icon is icon.outline-process-ruby
                        action is InvoiceInterface.InvoiceInterface
                            title is "InvoiceInterface"
                            menu item name is InvoiceInterface
                            description is "Interface actual sales to billing"
                            icon is icon.outline-process-azure

