CashInterfacesUX1 is a Page
    title is "Interfaces"
    implements MenuSearch
    show panel control on left

    Main is a DashBoardPanel
        use 12 column grid
        Transactions is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        list is CashTransactionImportResult.primary
                            title is "CashTransactionResults"
                            menu item name is CashTransactionInterfaceResults
                            icon is icon.outline-list-amber
                        action is InterfaceSelectedCashLedgerRunGroups.ScheduleInterface
                            title is "InterfaceSelectedRunGroups"
                            menu item name is InterfaceSelectedRunGroups
                            icon is icon.outline-process-amber
                        action is CashLedgerTransactionImport.InterfaceCashLedgerTransactions
                            title is "InterfaceTransactions"
                            menu item name is InterfaceTransactions
                            icon is icon.outline-process-amber
                        list is CashLedgerTransactionImport.primary
                            title is "TransactionMaintenance"
                            menu item name is CashLedgerTransactionInterfaceMaintenance
                            icon is icon.outline-list-amber
                        action is CashConsolidationsCurrentDayBuild.CashConsolidationsCurrentDayBuild
                            title is "CashConsolidationsCurrentDayBuild"
                            menu item name is CashConsolidationsCurrentDayBuild
                            icon is icon.outline-process-amber
        Payments is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        action is CashLedgerPaymentImport.InterfaceCashPayments
                            title is "InterfacePayments"
                            menu item name is InterfacePayments
                            icon is icon.outline-process-emerald
                        list is CashLedgerPaymentImport.primary
                            title is "PaymentMaintenance"
                            menu item name is PaymentInterfaceMaintenance
                            icon is icon.outline-list-emerald
        Receipts is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        action is CashLedgerReceiptImport.InterfaceCashReceipts
                            title is "InterfaceReceipts"
                            menu item name is InterfaceReceipts
                            icon is icon.outline-process-amethyst
                        list is CashLedgerReceiptImport.primary
                            title is "ReceiptMaintenance"
                            menu item name is ReceiptsInterfaceMaintenance
                            icon is icon.outline-list-amethyst
        ReturnedPayments is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        action is CashLedgerReturnedPaymentImport.InterfaceReturnedCashPayments
                            title is "InterfaceReturnedPayments"
                            menu item name is InterfaceReturnedPayments
                            icon is icon.outline-process-ruby
                        list is CashLedgerReturnedPaymentImport.primary
                            title is "ReturnedPaymentMaintenance"
                            menu item name is ReturnedPaymentInterfaceMaintenance
                            icon is icon.outline-list-ruby
        BankStatementInterface is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        list is BankStatementInterfaceHeader.BankStatementInterfaceHeaderList
                            title is "BankStatement"
                            menu item name is BankStatementInterface
                            icon is icon.outline-list-azure
                        list is CashManagementAccount.NonCashLedgerTransactionAccountList
                            title is "NonCashLedger"
                            menu item name is NonCashLedgerInterface
                            icon is icon.outline-list-azure
        CashForecastInterface is a Pane
            span 4 columns
            Layout
                single column
                    small menu
                        list is OtherForecastTransaction.OtherForecastTransactionList
                            title is "InterfaceCashForecast"
                            menu item name is CashForecastInterface
                            icon is icon.outline-list-azure

