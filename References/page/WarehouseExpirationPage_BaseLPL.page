WarehouseExpirationPage is a Page
    title is "Lot_Expiration_List"
    show panel control on left

    ExpiredLot is a MultiPanePanel
        pane 1
            business class is ItemLot
                list is DistributionManagementExpiredItemList
        pane 2
            business class is pane 1.StockOnHandDetailForItemLotRel
                list is DMBinStockOnHandDetailList

    ExpiringLots is a MultiPanePanel
        pane 1
            business class is ItemLot
                list is DistributionManagementExpiringItemList
        pane 2
            business class is pane 1.StockOnHandDetailForItemLotRel
                list is DMBinStockOnHandDetailList

    AllLotsWithExpirationDate is a MultiPanePanel
        pane 1
            business class is ItemLot
                list is DistributionManagementItemWithExpirationDateList
        pane 2
            business class is pane 1.StockOnHandDetailForItemLotRel
                list is DMBinStockOnHandDetailList

    LotManagement is a Panel
        business class is ItemLot
            list is AdvancedWarehouseItemLotList

