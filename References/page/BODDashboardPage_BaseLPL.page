BODDashboardPage is a Page
    title is "MaintainBOD\\s"

    Dashboard is a DashBoardPanel
        use 12 column grid
        ActiveOutbounds is a Pane
            span 3 columns
            business class is FSMBODConfiguration
                list is ActiveOutboundsList
                    overview navigation is ActiveOutboundsNav
        ActiveInbounds is a Pane
            span 3 columns
            business class is FSMBODConfiguration
                list is ActiveInboundsList
                    overview navigation is ActiveInboundsNav
        ActiveAlerts is a Pane
            span 6 columns
            business class is FSMBODConfigurationDetail
                list is ActiveAlertsList
        OutboundTrackers is a Pane
            span 6 columns
            business class is FSMInboundBODTracker
                list is TrackerOutboundsList
                    overview navigation is TrackerOutboundsNav
        InboundTrackers is a Pane
            span 6 columns
            business class is FSMInboundBODTracker
                list is TrackerInboundsList
                    overview navigation is TrackerInboundsNav

