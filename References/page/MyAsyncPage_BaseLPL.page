MyAsyncPage is a Page
    title is "MyProcesses"

    ProcessingViewer is a MultiPanePanel
        pane 2
            business class is AsyncActionTrigger
                list is MyQueuedAsyncActionTriggerList
        pane 3
            business class is AsyncActionTrigger
                list is MyActiveAsyncActionTriggerList
        pane 4
            business class is AsyncActionTrigger
                list is MyFailedAsyncActionTriggerList

    RequestManagement is a MultiPanePanel
        pane 1
            business class is AsyncActionRequest
                list is MyAsyncActionRequestList
                Actions
                    action is ProcessRequests
                    action is Create
                    action is Update
                    action is Delete
        pane 2
            business class is pane 1.AsyncActionTrigger set
                list is MyAsyncActionTriggerList

    QueueView is a MultiPanePanel
        pane 1
            business class is AsyncQueueDefinition
                list is RestrictedQueueDefinitionList
        pane 2
            business class is pane 1.AsyncActionTrigger set
                list is AsyncActionTriggerByQueueList

    Failed is a Panel
        business class is AsyncActionTrigger
            list is MyFailedAsyncActionTriggerDetailedList

    Misfires is a Panel
        business class is AsyncActionMisfire
            list is MyAsyncActionMisfireList

