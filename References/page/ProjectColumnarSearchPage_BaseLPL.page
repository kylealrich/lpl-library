ProjectColumnarSearchPage is a Page
    title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Search"
    is report for Projects,ProjectAdministrator,ProjectAccountant,ProjectManager
        category is "TransactionsAndBalances"

    Transactions is a Panel
        business class is GeneralLedgerTransaction
            list is ProjectColumnarList

    TransactionDetails is a Panel
        business class is GLTransactionDetail
            list is ProjectColumnarList

    Totals is a Panel
        business class is GeneralLedgerTotal
            list is ProjectColumnarList

    Commitments is a Panel
        business class is GLCommit
            list is ProjectColumnarList

    FundSearch is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension1Label>TransactionDetails"
        visible when (actor.context.FinanceEnterpriseGroup.FinanceDimension1Entry)
        business class is GLTransactionDetail
            list is FinDim1ColumnarList
                title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension1Label>TransactionDetails"

