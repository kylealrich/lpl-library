ProjectAnalysisReportsUX1 is a Page
    title is "Analysis"
    implements MenuSearch

    Analysis is a DashBoardPanel
        title is "Analysis"
        use 12 column grid
        ProjectAnalysis is a Pane
            span 4 columns
            Layout
                single column
                    column1
                        large menu
                            page is ProjectReportingSettingsUX1
                                title is "ProjectReports"
                                menu item name is ProjectReports
                                description is "View_project_cube_reports._Reports_include_project_obligation,_variance,_period_comparison,_percent_complete,_gross_margin_and_SEFA_worksheet."
                                icon is icon.clipboard-list-azure
                        large menu
                            page is ProjectAnalysisUX1
                                title is "ProjectAnalysis"
                                menu item name is ProjectAnalysis
                                description is "View_project_dimension_analysis_reports._Reports_include_project_budget,_amount,_and_trend_analysis_reports."
                                icon is icon.clipboard-list-azure
        GlobalLedgerAnalysis is a Pane
            span 4 columns
            Layout
                single column
                    column1
                        large menu
                            page is AccountAnalysisSettingsPage
                                title is "AccountAnalysis"
                                menu item name is AccountAnalysis
                                description is "Dimension_analysis_reports_by_GL_account_for_a_specific_project._Analysis_incudes_by_account,_budget,_and_trial_balance."
                                icon is icon.clipboard-list-emerald
                        large menu
                            page is DimensionOnePivotAnalysisPage
                                title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension1Label>Analysis"
                                menu item name is DimensionOnePivotAnalysis
                                description is "Dimension_analysis_reports_by_Finance_Dimension1_for_a_specific_project._Reports_include_compare,_period_over_period_and_variance_analysis_reports."
                                icon is icon.clipboard-list-emerald
                        large menu
                            page is AccountingUnitPivotAnalysisPage
                                title is "<actor.context.FinanceEnterpriseGroup.AccountingUnitLabel>Analysis"
                                menu item name is AccountingUnitPivotAnalysis
                                description is "Dimension_analysis_reports_by_accounting_unit_for_a_specific_project._Reports_include_compare,_period_over_period_and_variance_analysis_reports."
                                icon is icon.clipboard-list-emerald
        ReportDesigner is a Pane
            span 4 columns
            Layout
                single column
                    column1
                        large menu
                            page is GLReportWriter
                                title is "ReportDesigner"
                                menu item name is ReportDesigner
                                description is "View_and_create_global_ledger_and_project_balance_reports."
                                icon is icon.clipboard-list-amethyst
                        large menu
                            page is GLReportDesignerLog
                                title is "ReportDesignerLog"
                                menu item name is ReportDesignerLog
                                description is "View_report_designer_report_logs."
                                icon is icon.clipboard-list-amethyst

