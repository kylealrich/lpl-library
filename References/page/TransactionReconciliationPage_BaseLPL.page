TransactionReconciliationPage is a Page
    title is "ManualReconciliation"

    TransactionReconciliation is a MultiPanePanel
        pane 1
            business class is TransactionReconDetail
                list is UnmatchedTransactionsList
                Context
                    ReconciliationManagementGroup
                    ReconciliationAccountStructure
                    ReconciliationAccount
        pane 2
            business class is TransactionReconDetail
                list is SelectedTransactionsList
                Context
                    ReconciliationManagementGroup
                    ReconciliationAccountStructure
                    ReconciliationAccount
        pane 4
            business class is TransactionReconDetail
                list is ReconciliationSummaryRecordList
                Context
                    ReconciliationManagementGroup
                    ReconciliationAccountStructure
                    ReconciliationAccount

