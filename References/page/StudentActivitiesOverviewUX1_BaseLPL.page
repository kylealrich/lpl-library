StudentActivitiesOverviewUX1 is a Page
    title is "Overview"
    implements MenuSearch

    CurrentBalances is a Panel
        business class is StudentActivityDistrict
            list is CurrentBalancesDashboard

    PeriodBalances is a Panel
        business class is StudentActivityClosedPeriod
            list is PeriodBalancesDashboard

    PeriodBalancesBySchool is a Panel
        business class is SchoolClosedPeriod
            list is PeriodBalancesBySchoolDashboard

    BalancesBySchoolType is a MultiPanePanel
        pane 1
            business class is StudentActivitySchoolType
                list is SchoolTypeBalanceList
                Context
                    StudentActivityDistrict

    School is a Panel
        business class is StudentActivitySchool
            list is SchoolProcessingDashboard

    Receipts is a MultiPanePanel
        pane 1
            business class is StudentActivitySchool
                list is SchoolReceiptProcessingList
        pane 2
            business class is pane 1.StudentActivityReceipt set
                list is ActivityReceipts

