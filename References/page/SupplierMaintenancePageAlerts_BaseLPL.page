SupplierMaintenancePageAlerts is a Page
    title is "SupplierAlerts"
    implements MenuSearch

    ValidationNeeded is a Panel
        business class is Supplier
            list is SuppliersWhoNeedValidation

    MissingCommodityCodes is a Panel
        business class is SupplierSourceId
            list is MissingCommodityCodes

    UnansweredRequiredQuestions is a Panel
        business class is Supplier
            list is RequiredQuestionsNotAnswered

    DialogNeedingResponses is a Panel
        business class is SupplierMessageDialog
            list is MessageDialogRequestingResponses

    MyDialogResponseRequests is a Panel
        business class is SupplierMessageDialog
            list is MyMessageDialogRequestingResponses

    ExpiringCertifications is a Panel
        business class is Supplier
            list is ExpiringCertificates

    ExpiringContactCertifications is a Panel
        business class is SupplierSourceId
            list is ExpiringContractCertifications

    ExpiringDiversityCodes is a Panel
        business class is Supplier
            list is ExpiringDiversityCodes

    DiversityCodesNeedingDates is a Panel
        business class is Supplier
            list is DiversityCodesNeedingDates

    UnreleasedSupplierMessages is a Panel
        business class is SupplierMessage
            list is UnreleasedMessages

    NotRegisteredInExternalEntity is a Panel
        visible when (actor.context.FinanceEnterpriseGroup.IFSEnableSupplierExtEntityEnabled)
        business class is SupplierSourceId
            list is NotRegisteredInExternalEntity

