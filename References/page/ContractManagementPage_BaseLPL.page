ContractManagementPage is a Page
    title is "ContractManagement"

    InProcess is a Panel
        business class is Contract
            list is InProcessContracts

    Active is a Panel
        business class is Contract
            list is ActiveContracts

    Inactive is a Panel
        business class is Contract
            list is InactiveContracts

    Closed is a Panel
        business class is Contract
            list is ClosedContracts

    ContractHierarchy is a Panel
        business class is Contract
            list is ContractHierarchy

    EventCreation is a MultiPanePanel
        pane 1
            business class is ContractEventWorksheet
                list is CreateEventFromContract
                Context
                    Actor
                Actions
                    action is CreateEvent
                    action is AppendToEvent
                    action is Delete
        pane 2
            business class is ContractLine
                list is WorksheetContractLine
                Context
                    ContractGroup
                    Contract
                Actions
                    action is Active.CreateEventWorksheet
                    action is Active.CreateEventWorksheetByContract
    InProcessStatusAnalysis is a MultiPanePanel
        pane 1
            business class is ContractStatusCount
                list is ContractStatusList
        pane 2
            business class is pane 1.ContractStatusRel
                list is ChartContracts

    ProcessedStatusAnalysis is a MultiPanePanel
        pane 1
            business class is ContractStatusCount
                list is ProcessedContractStatusList
        pane 2
            business class is pane 1.ContractStatusRel
                list is ChartContracts

    Alerts is a MultiPanePanel
        pane 1
            business class is Contract
                list is ActiveExpiringAndExpiredContracts
        pane 2
            business class is Contract
                list is ContractExpirationWidget
        pane 3
            business class is Contract
                list is ContractsInTermsNegotiation
        pane 4
            business class is ContractSubcontractor
                list is SubcontractorAlert

