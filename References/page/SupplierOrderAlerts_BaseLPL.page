SupplierOrderAlerts is a Page
    title is "Alerts"

    OrdersNotReceived is a Panel
        visible when (not session.key.SupplierGroup.SPDisablePortalPOs)
        business class is PurchaseOrder
            list is SupplierPortalOpenShipmentOrders

    InvoiceExceptions is a Panel
        visible when (not session.key.SupplierGroup.SPDisablePortalInvoices)
        business class is PayablesInvoice
            list is SupplierPortalExceptionInvoiceInquiry

    InvoicesOverdue is a Panel
        visible when (not session.key.SupplierGroup.SPDisablePortalInvoices)
        business class is PayablesInvoice
            list is SupplierPortalOverdueInvoiceInquiry

    ReturnsRequestingReturnMaterialAuthorizations is a Panel
        visible when (not session.key.SupplierGroup.SPDisablePortalReturns)
        business class is VendorReturn
            list is SupplierRMAReturns

    BelowReorderPoint is a Panel
        visible when (not session.key.SupplierGroup.SPDisablePortalItems)
        business class is ItemReplenishmentSource
            list is SupplierPortalItemsBelowReorder

