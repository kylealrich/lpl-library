SupplierOrderManagementPage is a Page
    title is "OrderManagement"
    implements MenuSearch

    Alerts is a DashBoardPanel
        use 12 column grid
        visible when (session.key.SupplierGroup.AlertsPageOK)
        Alerts is a Pane
            span 6 columns
            title is "Alerts"
            Layout
                two column
                    small menu
                        visible when (not session.key.SupplierGroup.SPDisablePortalPOs)
                            page is SupplierOrderAlerts.OrdersNotReceived
                                title is "PurchaseOrdersNotReceived"
                                menu item name is PurchaseOrdersNotReceived
                                icon is icon.warning-ruby
                        visible when (not session.key.SupplierGroup.SPDisablePortalInvoices)
                            page is SupplierOrderAlerts.InvoiceExceptions
                                title is "InvoiceExceptions"
                                menu item name is InvoiceExceptions
                                icon is icon.warning-ruby
                        visible when (not session.key.SupplierGroup.SPDisablePortalInvoices)
                            page is SupplierOrderAlerts.InvoicesOverdue
                                title is "InvoicesOverdue"
                                menu item name is InvoicesOverdue
                                icon is icon.warning-ruby
                        visible when (not session.key.SupplierGroup.SPDisablePortalReturns)
                            page is SupplierOrderAlerts.ReturnsRequestingReturnMaterialAuthorizations
                                title is "ReturnRMARequests"
                                menu item name is ReturnRequests
                                icon is icon.warning-ruby
                        visible when (not session.key.SupplierGroup.SPDisablePortalItems)
                            page is SupplierOrderAlerts.BelowReorderPoint
                                title is "ItemsBelowReorderPoint"
                                menu item name is ItemsBelowReorderPoint
                                icon is icon.warning-ruby

    Invoices is a DashBoardPanel
        use 12 column grid
        visible when (session.key.SupplierGroup.CanViewInvoicesOrPayments)
        CreateInvoices is a Pane
            span 3 columns
            title is "CreateInvoices"
            visible when (not session.key.SupplierGroup.SPDisablePortalInvoiceCreation)
            Layout
                single column
                    small menu
                        action is PayablesInvoice.Unreleased.CreateBySupplier
                            title is "CreateInvoice"
                            menu item name is CreateSupplierInvoices
                            icon is icon.document-blank-azure
                        page is SupplierManageInvoiceCreation.UnsubmittedInvoices
                            title is "MyUnsubmittedInvoices"
                            menu item name is UnsubmittedSupplierInvoices
                            icon is icon.document-blank-azure
                        page is SupplierManageInvoiceCreation.SubmittedInvoices
                            title is "MySubmittedInvoices"
                            menu item name is SubmittedSupplierInvoices
                            icon is icon.document-blank-azure
        ViewInvoices is a Pane
            span 6 columns
            title is "ViewInvoices"
            visible when (not session.key.SupplierGroup.SPDisablePortalInvoices)
            Layout
                two column
                    small menu
                        page is SupplierManageInvoices.Dashboard
                            title is "Dashboard"
                            menu item name is SupplierInvoiceDashboard
                            icon is icon.document-finance-amethyst
                        page is SupplierManageInvoices.All
                            title is "All"
                            menu item name is SupplierInvoicesAll
                            icon is icon.document-finance-amethyst
                        page is SupplierManageInvoices.Open
                            title is "Open"
                            menu item name is SupplierInvoicesOpen
                            icon is icon.document-finance-amethyst
                        page is SupplierManageInvoices.Paid
                            title is "Paid"
                            menu item name is SupplierInvoicesPaid
                            icon is icon.document-finance-amethyst
                        page is SupplierManageInvoices.Exceptions
                            title is "Exceptions"
                            menu item name is SupplierInvoiceExceptions
                            icon is icon.document-finance-amethyst
                        page is SupplierManageInvoices.Overdue
                            title is "Overdue"
                            menu item name is SupplierInvoicesOverdue
                            icon is icon.document-finance-amethyst
        Payments is a Pane
            span 3 columns
            title is "Payments"
            visible when (not session.key.SupplierGroup.SPDisablePortalPayments)
            Layout
                single column
                    small menu
                        page is SupplierManagePayments.Dashboard
                            title is "Dashboard"
                            menu item name is SupplierPaymentDashboard
                            icon is icon.check-emerald
                        page is SupplierManagePayments.AllPayments
                            title is "All"
                            menu item name is SupplierPaymentsAll
                            icon is icon.check-emerald

    OrdersAndReceipts is a DashBoardPanel
        use 12 column grid
        visible when (session.key.SupplierGroup.CanViewOrdersOrReceiptsOrAcknowledgements)
        Orders is a Pane
            span 3 columns
            title is "Orders"
            visible when (not session.key.SupplierGroup.SPDisablePortalPOs)
            Layout
                single column
                    small menu
                        page is SupplierManageOrders.Dashboard
                            title is "Dashboard"
                            menu item name is SupplierOrderDashboard
                            icon is icon.check-emerald
                        page is SupplierManageOrders.OpenOrders
                            title is "Open"
                            menu item name is SupplierOpenOrders
                            icon is icon.check-emerald
                        page is SupplierManageOrders.OrdersNotReceived
                            title is "NotReceived"
                            menu item name is SupplierOrdersNotReceived
                            icon is icon.check-emerald
                        page is SupplierManageOrders.OpenOrderLines
                            title is "OpenLines"
                            menu item name is SupplierOpenOrderLines
                            icon is icon.check-emerald
                        page is SupplierManageOrders.ClosedOrders
                            title is "Closed"
                            menu item name is SupplierClosedOrders
                            icon is icon.check-emerald
        Receipts is a Pane
            span 3 columns
            title is "Receipts"
            visible when (not session.key.SupplierGroup.SPDisablePortalReceipts)
            Layout
                single column
                    small menu
                        page is SupplierManageReceipts.Dashboard
                            title is "Dashboard"
                            menu item name is SupplierReceiptsDashboard
                            icon is icon.receipt-azure
                        page is SupplierManageReceipts.AllReceipts
                            title is "AllReceipts"
                            menu item name is SupplierAllReceipts
                            icon is icon.receipt-azure
        AcknowledgeOrders is a Pane
            span 3 columns
            title is "AcknowledgeOrders"
            visible when (not session.key.SupplierGroup.SPDisablePortalAcknowledgments)
            Layout
                single column
                    small menu
                        list is PurchaseOrder.SupplierPortalOrdersNotAcknowledged
                            title is "AcknowledgeOrders"
                            menu item name is AcknowledgeOrders
                            icon is icon.cycle-azure

    ReturnsAndItems is a DashBoardPanel
        use 12 column grid
        visible when (session.key.SupplierGroup.CanViewReturnsOrItems)
        Returns is a Pane
            span 3 columns
            title is "Returns"
            visible when (not session.key.SupplierGroup.SPDisablePortalReturns)
            Layout
                single column
                    small menu
                        page is SupplierManageReturns.Dashboard
                            title is "Dashboard"
                            menu item name is SupplierReturnDashboard
                            icon is icon.recall-emerald
                        page is SupplierManageReturns.OpenReturns
                            title is "Open"
                            menu item name is SupplierOpenReturns
                            icon is icon.recall-emerald
                        page is SupplierManageReturns.ClosedReturns
                            title is "Closed"
                            menu item name is SupplierClosedReturns
                            icon is icon.recall-emerald
                        page is SupplierManageReturns.ReturnsRequestingReturnMaterialAuthorizations
                            title is "RMARequests"
                            menu item name is SupplierRMARequests
                            icon is icon.recall-emerald
        Items is a Pane
            span 3 columns
            title is "ReplenishmentItems"
            visible when (not session.key.SupplierGroup.SPDisablePortalItems)
            Layout
                single column
                    small menu
                        page is SupplierManageItems.All
                            title is "All"
                            menu item name is SupplierItems
                            icon is icon.box-azure
                        page is SupplierManageItems.BelowReorderPoint
                            title is "BelowReorderPoint"
                            menu item name is SupplierItemsBelowReorderPoint
                            icon is icon.box-azure

