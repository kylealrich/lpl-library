ManageItemLocationPage is a Page
    title is "ManageItemLocations"

    ItemLocations is a Panel
        business class is ItemLocation
            list is ItemsForAnInventoryLocation

    ItemLocationAuditHistory is a Panel
        visible when (not actor.context.FinanceEnterpriseGroup.ColumnarItemLocationAuditInUse)
        business class is ItemLocation
            list is ItemLocationAuditList

    ItemLocationAuditHistoryColumnar is a Panel
        title is "ItemLocationAuditHistory"
        visible when (actor.context.FinanceEnterpriseGroup.ColumnarItemLocationAuditInUse)
        business class is ItemLocation
            list is ColumnarItemLocationAuditList
                title is "ItemLocationAuditHistory"

    DaysOnHand is a Panel
        visible when (actor.context.FinanceEnterpriseGroup.DaysOnHandInUse)
        business class is ItemLocation
            list is DaysOnHandList

    WarehouseItemLocations is a Panel
        visible when (actor.context.FinanceEnterpriseGroup.FSMAdvancedWarehouseEnabled)
        business class is ItemLocation
            list is WarehouseItemLocationList

