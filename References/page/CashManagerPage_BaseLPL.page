CashManagerPage is a Page
    title is "CashManager"

    Main is a DashBoardPanel
        CashBalance is a Pane
            business class is BankStatement
                list is CashBalanceByDayChart
        OpenStatements is a Pane
            business class is BankStatement
                list is StatementsByStatusChart
        IncomingBankStatements is a Pane
            business class is IncomingBankStatementHeader
                list is MissingBankStatementsChart
        ProjectedCashBalances is a Pane
            business class is BankStatement
                list is FinalizedBankStatementPositionList
        ActualCashBalances is a Pane
            business class is CashManagementAccount
                list is CashManagementAccountLatestCashBalances
        OpenBankStatementsList is a Pane
            business class is BankStatement
                list is OpenBankStatementList
        IntradayBankStatements is a Pane
            business class is BankStatement
                list is IntradayBankStatementList
        CommitmentsForNonCashLedgerTransactions is a Pane
            business class is NonCashLedgerTransaction
                list is NonCashLedgerTransactionsWithCommitmentsList
        CommitmentsForBankStatementLines is a Pane
            business class is BankStatementLine
                list is BankStatementLinesWithCommitmentsList

    CashPositionByAccount is a Panel
        business class is CashManagementAccount
            list is DailyCashManagementAccountList

    BankStatementReconciliation is a Panel
        business class is BankStatement
            list is BankStatementReconciliationDashBoard

    Resources is a Panel
        mouse over text is "DefineResources"
        business class is FinanceResource
            list is MaintainFinanceResourceList
            Context
                HROrganization

    Teams is a MultiPanePanel
        mouse over text is "DefineTeams"
        pane 1
            business class is FinanceEnterpriseGroup
                list is FinanceEnterpriseGroupList
        pane 2
            business class is pane 1.FinanceTeam set
                list is MaintainFinanceTeamList
        pane 4
            business class is pane 2.FinanceTeamMember
                list is FinanceTeamMemberList

