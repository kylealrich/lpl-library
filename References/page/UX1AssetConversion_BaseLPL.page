UX1AssetConversion is a Page
    title is "Interfaces"
    implements MenuSearch

    Templates is a Panel
        title is "Templates"
        business class is AssetTemplate
            list is AssetTemplatesAndInterfaceRecords
                title is "Templates"

    Capitalization is a DashBoardPanel
        use 12 column grid
        ActionsPanel is a Pane
            span 4 columns
            title is "Actions&Lists"
            Layout
                two column
                    column1
                        small menu
                            action is AssetInterfaceOptions.DraftProjectAssetInterfaceSet
                                title is "DraftProjectInterface"
                                menu item name is DraftProjectInterface
                                icon is icon.document-azure
                                Search Terms
                                        "DraftProjectInterface"
                                        "DraftProjectAssetInterface"
                            action is AssetInterfaceOptions.ProjectCapitalizationAndAssetInterface
                                title is "ProjectAssetInterface"
                                menu item name is ProjectAssetInterface
                                icon is icon.document-azure
                                Search Terms
                                        "ProjectCapitalization"
                                        "ProjectAssetInterface"
                            action is MassAssetAdditions.MassAssetAdditions
                                title is "AssetInterfaceUpdate"
                                menu item name is AssetInterfaceUpdate
                                icon is icon.document-azure
                                Search Terms
                                        "AssetInterfaceUpdate"
                                        "MassAssetAddition"
                            action is AssetInterfaceOptions.AssetInterfaceAppend
                                title is "AssetInterfaceAppend"
                                menu item name is AssetInterfaceAppend
                                icon is icon.document-azure
                                Search Terms
                                        "AssetInterfaceAppend"
                                        "AppendAssetInterface"
                            action is AssetRepairRelease.AssetRepairRelease
                                title is "RepairInterface"
                                menu item name is AssetRepairRelease
                                icon is icon.document-azure
                                Search Terms
                                        "AssetRepairRelease"
                                        "RepairRelease"
                                        "RepairInterface"
                            action is Project.HoldCapitalizableTransactions
                                title is "CapitalizeHold"
                                menu item name is HoldCapitalizableTransactions
                                icon is icon.document-azure
                                Search Terms
                                        "InterfaceHold"
                                        "AssetInterfaceHold"
                                        "CapitalizeHold"
                            action is Project.RemoveHoldFromCapitalizableTransactions
                                title is "RemoveCapitalizeHold"
                                menu item name is RemoveCapitalizeHold
                                icon is icon.document-azure
                                Search Terms
                                        "RemoveCapitalizeHold"
                                        "RemoveHold"
                    column2
                        small menu
                            page is CapitalizationErrorsPage
                                title is "CapitalizationErrorsPage"
                                menu item name is CapitalizationErrorsPage
                                icon is icon.document-amber
                                Search Terms
                                        "CapitalizationErrorsPage"
        ProjectInterfaceControl is a Pane
            span 8 columns
            title is "ProjectInterfaceControl"
            business class is AssetInterfaceOptions
                list is UX1ProjectInterfaceOptionsList
                    overview navigation is AssetInterfaceControlList

    AssetInterface is a Panel
        title is "AssetInterface"
        business class is AssetInterface
            list is ContextAssetInterfaceList
                title is "AssetInterface"

    Import is a DashBoardPanel
        use 12 column grid
        ImportListings is a Pane
            span 6 columns
            Layout
                two column distributed
                    small menu
                        list is AssetImport.AssetImportList
                            title is "Assets"
                            menu item name is AssetImportList
                            icon is icon.clipboard-list-amethyst
                            Search Terms
                                    "AssetImportList"
                                    "AssetImport"
                        list is AssetItemAdjustmentImport.AssetItemAdjustmentImportList
                            title is "AdditionalItems"
                            menu item name is AdditionalItemImportList
                            icon is icon.clipboard-list-amethyst
                            Search Terms
                                    "AdditionalItemImportList"
                                    "AssetImportList"
                                    "AdditionalItemImport"
                        list is AssetBookAdditionalImport.AssetBookAdditionalImportList
                            title is "AdditionalBooks"
                            menu item name is AdditionalBookImportList
                            icon is icon.clipboard-list-amethyst
                            Search Terms
                                    "AdditionalBookImportList"
                                    "AssetBookImportList"
                                    "AssetBookAdditionalImportList"
                        list is AssetTransactionImport.AssetTransactionImportList
                            title is "Transactions"
                            menu item name is TransactionImportList
                            icon is icon.clipboard-list-amethyst
                            Search Terms
                                    "TransactionImportList"
                                    "AssetTransactionImportList"
                        list is AssetRepairImport.InterfacingAssetRepairs
                            title is "Repairs"
                            menu item name is RepairInterfaceList
                            icon is icon.clipboard-list-amethyst
                            Search Terms
                                    "AssetRepairInterfaceList"
                                    "InterfacingAssetRepairs"
                        list is AssetManagementInterfaceResult.AssetManagementInterfaceResults
                            title is "ImportResults"
                            menu item name is InterfaceResults
                            icon is icon.clipboard-list-amethyst
                            Search Terms
                                    "InterfaceResults"
                                    "AssetManagementInterfaceResults"
                        list is AssetCommentImport.AssetCommentImportList
                            title is "Comments"
                            menu item name is CommentImportList
                            icon is icon.clipboard-list-amethyst
                            Search Terms
                                    "CommentImportList"
                                    "AssetCommentImportList"
        ImportProcessing is a Pane
            span 6 columns
            Layout
                two column distributed
                    small menu
                        action is AssetManagementInterfaceResult.AssetInterface
                            title is "AssetImport"
                            menu item name is AssetImport
                            icon is icon.check-amber
                            Search Terms
                                    "AssetImport"
                                    "AssetConversion"
                        action is MassAssetAdditions.MassAssetAdditions
                            title is "AssetInterfaceUpdate"
                            menu item name is AssetInterfaceUpdate
                            icon is icon.check-amber
                            Search Terms
                                    "AssetInterfaceUpdate"
                                    "MassAssetAddition"
                        action is AdditionalBookConversion.AdditionalBookConversion
                            title is "AdditionalBookInterface"
                            menu item name is AdditionalBookImport
                            icon is icon.check-amber
                            Search Terms
                                    "AdditionalBookImport"
                                    "BookConversion"
                        action is AssetItemUpdateAdjustmentInterface.AssetItemUpdateAdjustmentInterface
                            title is "AdditionalItemInterface"
                            menu item name is ItemAdjustmentInterface
                            icon is icon.check-amber
                            Search Terms
                                    "ItemAdjustmentInterface"
                                    "ItemConversion"
                        action is AssetRepairRelease.AssetRepairRelease
                            title is "AssetRepairInterface"
                            menu item name is RepairInterface
                            icon is icon.check-amber
                            Search Terms
                                    "AssetRepairInterface"
                                    "RepairUpdate"
                        action is MassAssetDistributionSplit.MassAssetDistributionSplit
                            title is "AssetInterfaceSplit"
                            menu item name is AssetInterfaceSplit
                            icon is icon.check-amber
                            Search Terms
                                    "AssetInterfaceSplit"
                                    "InterfaceSplit"

