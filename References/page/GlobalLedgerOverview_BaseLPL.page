GlobalLedgerOverview is a Page
    title is "Overview"
    implements MenuSearch

    Overview is a DashBoardPanel
        title is "GlobalLedgerOverview"
        use 12 column grid
        Budget is a Pane
            span 4 columns
            title is "Budgeting"
            Layout
                single column
                    page is GlobalLedgerBudget.Scenarios
                        title is "Overview"
                        menu item name is Overview
                        icon is icon.money-bag-emerald
                    page is GlobalLedgerBudget.ChangeRequests
                        title is "ChangeRequests"
                        menu item name is ChangeRequests
                        icon is icon.money-bag-emerald
                    page is GlobalLedgerBudget.BudgetEditErrors
                        title is "EditErrors"
                        menu item name is EditErrors
                        icon is icon.money-bag-emerald
        Journals is a Pane
            span 4 columns
            Layout
                single column
                    page is GlobalLedgerJournal.Unreleased
                        title is "Unreleased"
                        menu item name is Unreleased
                        icon is icon.document-amber
                    page is GlobalLedgerJournal.Search
                        title is "Search"
                        menu item name is Search
                        icon is icon.document-amber
                    page is GlobalLedgerJournal.PendingApproval
                        title is "PendingApproval"
                        menu item name is PendingApproval
                        icon is icon.document-amber
                    action is PostJournals.SchedulePosting
                        title is "PostJournals"
                        menu item name is PostJournals
                        icon is icon.settings-amber
        RecurringJournals is a Pane
            span 4 columns
            Layout
                single column
                    page is RecurringJournal.Process
                        title is "Process"
                        menu item name is Process
                        icon is icon.document-pie-chart-azure
                    page is RecurringJournal.JournalizeSchedule
                        title is "Schedule"
                        menu item name is Schedule
                        icon is icon.document-pie-chart-azure
                    page is RecurringJournal.PendingApproval
                        title is "PendingApproval"
                        menu item name is PendingApproval
                        icon is icon.document-pie-chart-azure
        AccountAnalysis is a Pane
            span 4 columns
            Layout
                single column
                    page is GlobalLedgerSearches.CompareAnalysis
                        title is "Compare"
                        menu item name is Compare
                        icon is icon.document-bar-chart-amethyst
                    page is GlobalLedgerSearches.PeriodOverPeriodAnalysis
                        title is "Period_overPeriod"
                        menu item name is PeriodOverPeriod
                        icon is icon.document-bar-chart-amethyst
                    page is GlobalLedgerSearches.VarianceAnalysis
                        title is "Variance"
                        menu item name is Variance
                        icon is icon.document-bar-chart-amethyst
        TransactionInterface is a Pane
            span 4 columns
            Layout
                single column
                    page is GeneralLedgerTransactionInterfacePage.TransactionsToInterface
                        title is "TransactionInterface"
                        menu item name is TransactionInterface
                        icon is icon.document-finance-ruby
                    page is GeneralLedgerTransactionInterfacePage.RunResult
                        title is "Results"
                        menu item name is Results
                        icon is icon.document-finance-ruby
                    page is GeneralLedgerTransactionInterfacePage.JournalizeGroupResult
                        title is "JournalizeGroupResults"
                        menu item name is JournalizeGroupResults
                        icon is icon.document-finance-ruby
                    action is InterfaceSelectedRunGroups.ScheduleInterface
                        title is "ScheduleInterface"
                        menu item name is ScheduleInterface
                        icon is icon.outline-process-ruby
                    action is InterfaceAllRunGroups.ScheduleInterface
                        title is "ScheduleAllInterface"
                        menu item name is ScheduleAllInterface
                        icon is icon.outline-process-ruby
        Transactions is a Pane
            span 4 columns
            Layout
                single column
                    page is TransactionAnalysis
                        title is "TransactionAnalysis"
                        menu item name is TransactionAnalysis
                        icon is icon.outline-list-amber
                    page is TotalsPage
                        title is "Totals"
                        menu item name is Totals
                        icon is icon.outline-list-amber
                    page is TransactionPage
                        title is "FinanceTransactions"
                        menu item name is FinanceTransactions
                        icon is icon.outline-list-amber
                    page is GLTransactionDetailPage
                        title is "FinanceTransactionDetail"
                        menu item name is FinanceTransactionDetail
                        icon is icon.outline-list-amber
        ReportDesigner is a Pane
            span 4 columns
            visible when (actor.context.FinanceEnterpriseGroup.ShowGLReportDesigner)
            Layout
                single column
                    page is GLReportWriter.DesignerDashboard
                        title is "Dashboard"
                        menu item name is Dashboard
                        icon is icon.outline-report-azure
        CAMPane is a Pane
            span 4 columns
            title is "CustodialManagement"
            visible when (actor.context.FinanceEnterpriseGroup.CustodialAccountManagement)
            Layout
                single column
                    list is CustodialAccountManagement.CustodialAccountManagementList
                        title is "CustodialAccountManagement"
                        menu item name is CustodialAccountManagement
                        icon is icon.document-certified-amethyst
                    list is CustodialDetailTransfer.CustodialDetailTransferList
                        title is "Transfer"
                        menu item name is Transfer
                        icon is icon.document-certified-amethyst
                    list is FinanceDimension10.CustodialAccountCompareList
                        title is "CustodialAccountCompare"
                        menu item name is CustodialAccountCompare
                        icon is icon.document-certified-amethyst

