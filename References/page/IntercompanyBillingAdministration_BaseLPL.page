IntercompanyBillingAdministration is a Page
    title is "<actor.context.FinanceEnterpriseGroup.IntercompanyBillingAdministrationTitle>"
    implements MenuSearch

    Administration is a DashBoardPanel
        use 12 column grid
        Groups is a Pane
            span 3 columns
            Layout
                single column
                    column1
                        large menu
                            page is IntercompanyBillingGroupsAndCodes.IntercompanyBillingGroups
                                title is "IntercompanyBillingGroups"
                                menu item name is IntercompanyBillingGroups
                                icon is icon.group-azure
        Codes is a Pane
            span 9 columns
            Layout
                three column distributed
                    column1
                        large menu
                            page is IntercompanyBillingGroupsAndCodes.RechargeItems
                                title is "RechargeItems"
                                menu item name is RechargeItems
                                icon is icon.book-emerald
                            page is IntercompanyBillingGroupsAndCodes.UnitsOfMeasure
                                title is "UnitsOfMeasure"
                                menu item name is UnitsOfMeasure
                                icon is icon.outline-square-emerald
                    column2
                        large menu
                            page is IntercompanyBillingGroupsAndCodes.DistributionCodes
                                title is "DistributionCodes"
                                menu item name is DistributionCodes
                                icon is icon.outline-diagonals-emerald
                            page is IntercompanyBillingGroupsAndCodes.ApprovalCodes
                                title is "ApprovalCodes"
                                menu item name is ApprovalCodes
                                icon is icon.check-emerald
        Utilities is a Pane
            span 3 columns
            Layout
                single column
                    column1
                        large menu
                            action is IntercompanyBillingTransaction.ProcessIntoBillingDocuments
                                title is "ProcessInterfacedTransactions"
                                menu item name is ProcessIntercompanyBillingTransactions
                                icon is icon.outline-process-amber
                            list is IntercompanyBillingSettlementHeader.PendingReversalSettlementList
                                title is "PendingReversalSettlements"
                                menu item name is PendingReversal
                                icon is icon.clipboard-list-amber
                            action is TransferPricing.BatchRun
                                title is "ProcessRecharges"
                                menu item name is ProcessRecharges
                                icon is icon.cycle-settings-amber
                            action is IntercompanyBillingGroup.UpdateDocumentHeaderApprovalEmailFields
                                title is "UpdateDocumentHeaderApprovalEmailFields"
                                menu item name is UpdateDocumentHeaderApprovalEmailFields
                                icon is icon.cycle-settings-amber
        Documents is a Pane
            span 9 columns
            Layout
                three column distributed
                    column1
                        large menu
                            page is IntercompanyBillingDocumentSetup.IDMDocumentTemplates
                                title is "IDMDocumentTemplates"
                                menu item name is IDMDocumentTemplates
                                icon is icon.document-amethyst
                            page is IntercompanyBillingDocumentSetup.IDMEmailTemplates
                                title is "IDMEmailTemplates"
                                menu item name is IDMEmailTemplates
                                icon is icon.web-amethyst
                            page is MyIDMJobsPage
                                title is "MyIDMJobs"
                                menu item name is MyIDMJobs
                                icon is icon.filled-link-amethyst
                    column2
                        large menu
                            page is IntercompanyBillingDocumentSetup.InvoiceTemplates
                                title is "InvoiceTemplates"
                                menu item name is InvoiceTemplates
                                icon is icon.document-finance-amethyst
                            page is IntercompanyBillingDocumentSetup.SampleInvoiceTemplates
                                title is "SampleInvoiceTemplates"
                                menu item name is SampleInvoiceTemplates
                                icon is icon.document-certified-amethyst
                    column3
                        large menu
                            page is IntercompanyBillingDocumentSetup.ActivePrinters
                                title is "ActivePrinters"
                                menu item name is ActivePrinters
                                icon is icon.check-amethyst
                            page is IntercompanyBillingDocumentSetup.InactivePrinters
                                title is "InactivePrinters"
                                menu item name is InactivePrinters
                                icon is icon.warning-amethyst

