UX1FranchiseAdministratorPage is a Page
    title is "<actor.context.FinanceEnterpriseGroup.FranchiseBillingAdministrationTitle>"
    implements MenuSearch

    Dashboard is a DashBoardPanel
        use 12 column grid
        Search Terms
                "Maintenance"
                "Admin"
                "Setup"
        Setup is a Pane
            span 4 columns
            Layout
                single column
                    large menu
                        list is FranchiseCompany.AllFranchiseCompanyNewList
                            title is "Companies"
                            menu item name is Companies
                            description is "Setup or Manage companies"
                            icon is icon.settings-azure
                        list is FranchiseCalendar.FranchiseCalendarNewList
                            title is "Calendars"
                            menu item name is Calendars
                            description is "Setup or Manage calendars"
                            icon is icon.settings-azure
        StandardCharges is a Pane
            span 4 columns
            Layout
                single column
                    large menu
                        page is UX1FranchiseStandardChargesPage.Sales
                            title is "Sales"
                            menu item name is Sales
                            description is "Setup or Manage sales charges"
                            icon is icon.presentation-emerald
                        page is UX1FranchiseStandardChargesPage.Note
                            title is "Note"
                            menu item name is Note
                            description is "Setup or Manage note charges"
                            icon is icon.presentation-emerald
                        page is UX1FranchiseStandardChargesPage.Special
                            title is "Special"
                            menu item name is Special
                            description is "Setup or Manage special charges"
                            icon is icon.presentation-emerald
                        page is UX1FranchiseStandardChargesPage.StandardGroups
                            title is "StandardGroups"
                            menu item name is StandardGroups
                            description is "Setup or Manage standard charge groups"
                            icon is icon.presentation-emerald
        Budget is a Pane
            span 4 columns
            Layout
                single column
                    large menu
                        list is FranchiseBudget.FranchiseBudgetNewList
                            title is "Budgets"
                            menu item name is Budgets
                            description is "Setup or Manage budgets"
                            icon is icon.money-bag-azure
                        list is FranchiseCompanyBudget.FranchiseCompanyBudgetsNew
                            title is "CompanyBudgets"
                            menu item name is CompanyBudgets
                            description is "Setup or Manage budgets for a company"
                            icon is icon.money-bag-azure
        Contract is a Pane
            span 4 columns
            Layout
                single column
                    large menu
                        list is FranchiseContract.FranchiseContractNewList
                            title is "Contracts"
                            menu item name is Contracts
                            description is "Setup or Manage customer contracts"
                            icon is icon.document-certified-ruby
                        list is FranchiseContractLicensee.FranchiseContractLicensee
                            title is "ContractLicensees"
                            menu item name is ContractLicensees
                            description is "Setup or Manage contract Licensees"
                            icon is icon.document-certified-ruby
        Balance is a Pane
            span 4 columns
            Layout
                single column
                    large menu
                        action is FranchiseeNoteBalance.FranchiseeNoteBalance
                            title is "NoteBalance"
                            menu item name is NoteBalance
                            description is "Lists note balances for all released actuals"
                            icon is icon.receipt-amber
                        action is OutstandingBalancesByRep.OutstandingBalancesByRep
                            title is "OutstandingBalancesBySalesRepresentative"
                            menu item name is OutstandingBalancesBySalesRepresentative
                            description is "Lists balances by sales representative and customer"
                            icon is icon.receipt-amber
        Purge is a Pane
            span 4 columns
            Layout
                single column
                    large menu
                        action is SalesPurge.SalesPurge
                            title is "Sales"
                            menu item name is Sales
                            description is "Purge all closed sales "
                            icon is icon.warning-amber

