CashPositionPage is a Page
    title is "CashPosition"

    ActualCashBalancesByAccount is a Panel
        business class is CashManagementAccount
            list is CashManagementAccountLatestCashBalances

    ActualCashBalancesByLocation is a MultiPanePanel
        vertical split is 25/75
        pane 1
            business class is CashManagementLocation
                list is CashManagementReportLocations
                Context
                    CashManagementGroup
        pane 3
            business class is pane 1.CashManagementAccountByLocationRel
                list is LatestCashBalancesByLocation

    ProjectedCashBalances is a Panel
        business class is BankStatement
            list is FinalizedBankStatementPositionList

    BankStatementTotalBalance is a DashBoardPanel
        title is "BalanceAnalysis"
        mouse over text is "BalanceAnalysis"
        show 4 columns
        EndingBalance is a Pane
            double width
            business class is BankStatementTotal
                list is BankStatementTotalBalanceCube
                    title is "EndingBalance_(BaseCurrency)"
                    Row Dimensions
                        BankStatementTotal.CashManagementAccount
                    overview navigation is BankStatementTotalBalanceCubeLink
                        label is "Detail"
                Context
                    CashManagementGroup
        EndingBalanceTrend is a Pane
            double width
            business class is BankStatementTotal
                list is BankStatementTotalBalanceTrendCube
                    title is "EndingBalanceTrend"
                    display EndingBalanceTrendLineChart
                    Row Dimensions
                        BankStatementTotal.CashManagementAccount
                    overview navigation is BankStatementTotalBalanceTrendCubeLink
                        label is "Detail"
        Accounts is a Pane
            double width
            business class is BankStatementTotal
                list is BankStatementTotalBalanceCube
                    title is "EndingBalanceByAccount"
                    display EndingBalanceByAccountBarChart
                    Row Dimensions
                        BankStatementTotal.CashManagementAccount
                    overview navigation is BankStatementTotalBalanceCubeLink
                        label is "Detail"
        Institutions is a Pane
            double width
            business class is BankStatementTotal
                list is BankStatementTotalBalanceCube
                    title is "EndingBalanceByInstitution"
                    display EndingBalanceByInstitutionBarChart
                    Row Dimensions
                        FinancialInstitution
                        BankStatementTotal.CashManagementAccount
                    overview navigation is BankStatementTotalBalanceCubeLink
                        label is "Detail"
        Locations is a Pane
            double width
            business class is BankStatementTotal
                list is BankStatementTotalBalanceCube
                    title is "EndingBalanceByLocation"
                    display EndingBalanceByLocationBarChart
                    Row Dimensions
                        CashManagementLocation
                        BankStatementTotal.CashManagementAccount
                    overview navigation is BankStatementTotalBalanceCubeLink
                        label is "Detail"
        LegalEntity is a Pane
            double width
            business class is BankStatementTotal
                list is BankStatementTotalBalanceCube
                    title is "EndingBalanceByLegalEntity"
                    display EndingBalanceByLegalEntityBarChart
                    Row Dimensions
                        BankStatementTotal.LegalEntity
                        BankStatementTotal.CashManagementAccount
                    overview navigation is BankStatementTotalBalanceCubeLink
                        label is "Detail"
        Currency is a Pane
            double width
            business class is BankStatementTotal
                list is BankStatementTotalBalanceCurrencyCube
                    title is "EndingBalanceByCurrency"
                    display EndingBalanceByCurrencyBarChart
                    Row Dimensions
                        AccountCurrency
                        BankStatementTotal.CashManagementAccount
                    overview navigation is BankStatementTotalBalanceCurrencyCubeLink
                        label is "Detail"

