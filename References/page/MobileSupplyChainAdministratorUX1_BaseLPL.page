MobileSupplyChainAdministratorUX1 is a Page
    title is "MobileSupplyChainAdministration"
    implements MenuSearch

    OtherModules is a DashBoardPanel
        use 12 column grid
        MobileSupplyChain is a Pane
            span 5 columns
            title is "General"
            icon is icon.outline-maintenance-amethyst
            Layout
                two column
                    small menu
                        list is MobileSupplyChainConfiguration.primary
                            title is "EnterpriseSetup"
                            menu item name is EnterpriseSetup
                        list is MobileSupplyChainCompany.MobileSupplyChainCompanyList
                            title is "Companies"
                            menu item name is Companies
                        list is MobileSupplyChainLocation.primary
                            title is "Locations"
                            menu item name is Locations
                        list is MobileSupplyChainResource.MobileSupplyChainResourceList
                            title is "Resources"
                            menu item name is Resources
                        list is MobileSupplyChainVendor.MSCMVendorList
                            title is "Vendors"
                            menu item name is Vendors
                        form is MobileSupplyChainConfiguration.DesktopAndMobileApplicationForm
                            title is "Desktop_andMobileApplications"
                            menu item name is DesktopAndMobileApplications
                        list is MobileSupplyChainPrinter.primary
                            title is "Printers"
                            menu item name is Printers
                        list is MobileSupplyChainSecurity.MobileSupplyChainSecurityList
                            title is "HandheldAccess"
                            menu item name is HandheldAccess
        ReceivingAndDelivery is a Pane
            span 3 columns
            title is "Receiving_andDelivery"
            icon is icon.outline-maintenance-amethyst
            Layout
                single column
                    small menu
                        page is DeliveryTrackingPage
                            title is "DeliveryTracking"
                            menu item name is DeliveryTracking
        MobileParAndCycleCounting is a Pane
            span 4 columns
            title is "MobilePar_andCycleCounting"
            icon is icon.outline-maintenance-amethyst
            Layout
                single column
                    small menu
                        page is KanbanItemPage.ConfigureKanbanItemBin
                            title is "KanbanItemBins"
                            menu item name is ConfigureKanbanItemBins
                        page is KanbanItemPage.ConfigureKanbanLocations
                            title is "KanbanLocations"
                            menu item name is ConfigureKanbanLocations
        LabelsAndPrinting is a Pane
            span 3 columns
            title is "Labels_andPrinting"
            icon is icon.outline-maintenance-amethyst
            Layout
                single column
                    small menu
                        page is PrintLabel
                            title is "PrintLabels"
                            menu item name is PrintLabels
                        list is LabelTemplate.LabelTemplateList
                            title is "LabelTemplates"
                            menu item name is LabelTemplates
                        list is MobileSupplyChainPrinterJob.MobileSupplyChainPrinterJobList
                            title is "PrintJobs"
                            menu item name is PrintJobs

