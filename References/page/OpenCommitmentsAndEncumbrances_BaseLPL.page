OpenCommitmentsAndEncumbrances is a Page
    show panel control on left

    Commitments is a MultiPanePanel
        title is "HeaderAndLineCommitments"
        pane 1
            business class is ContractLineDistribution
                list is LineCommitments
        pane 2
            business class is Contract
                list is ContractCommitments

    MoreCommitments is a MultiPanePanel
        title is "SpreadCommitments"
        pane 1
            business class is ContractSpread
                list is Commitments
        pane 2
            business class is ContractLineDistributionSpread
                list is Commitments

    RequisitionCommitments is a Panel
        business class is RequisitionLineDistribution
            list is RequisitionCommitments

    PurchaseOrderEncumbrances is a Panel
        business class is PurchaseOrderLineDistribution
            list is PurchaseOrderCommitments

    PayablesEncumbrances is a Panel
        business class is PayablesInvoice
            list is APManagerInvoicesWithCommitmentsList

