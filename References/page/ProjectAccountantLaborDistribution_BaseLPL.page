    LaborDistributionByEmployee is a Panel
        title is "ByEmployee"
        business class is ProjectEmployee
            list is XIAllEmployeesWithLaborList
                title is "ByEmployee"

    LaborDistributionByPeriod is a Panel
        title is "ByPeriod"
        business class is ProjectSchedulePeriod
            list is XILaborPeriodList
                title is "ByPeriod"

    AllLabor is a Panel
        business class is ProjectAssignmentLabor
            list is AllProjectAssignmentLaborList

    PendingCostTransfers is a Panel
        business class is ProjectAssignmentLabor
            list is PendingCostTransferList

    Search is a Panel
        business class is GLTransactionDetail
            list is GLTransactionHistoryDetailLaborDistributionSearch

    ValidationErrors is a Panel
        title is "ValidationResults"
        business class is ProjectStructureValidationResult
            list is ProjectStructureValidationLaborResultList
                title is "ValidationResults"

    Errors is a Panel
        business class is ProjectAssignmentLabor
