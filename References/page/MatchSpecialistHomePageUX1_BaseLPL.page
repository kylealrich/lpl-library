MatchSpecialistHomePageUX1 is a Page
    title is "Match"
    implements MenuSearch

    Interfaces is a Panel
        Layout
            single column
                large menu
                    page is MatchInterfacePage
                        title is "MatchInterface"
                        menu item name is MatchInterface
                        description is "MatchInvoice,ServiceContract,Basic,FreightAndAOCMatchInvoices"
                        icon is icon.cycle-amethyst
                    page is PayablesInvoiceInterfaceMaintenance
                        title is "PayablesInterface"
                        menu item name is PayablesInterface
                        description is "PrimarilyUsedForBasicExpenseInvoices"
                        icon is icon.cycle-amethyst
                    page is PayablesInterfacePage
                        title is "DocumentInterface"
                        menu item name is DocumentInterface
                        description is "AllInvoiceTypesViaDocumentImaging"
                        icon is icon.cycle-amethyst
                    page is EDIInterfacePageUX1
                        title is "EDIInterface"
                        menu item name is EDIInterface
                        description is "AllInvoiceTypesViaEDI"
                        icon is icon.cycle-amethyst

    AutoMatching is a Panel
        title is "AutoMatching"
        business class is AutoMatchResult
            list is AutoMatchResults
                title is "AutoMatching"

    SearchInvoices is a Panel
        business class is PayablesInvoice
            list is XiInvoiceSearchList

    Reconcile is a DashBoardPanel
        title is "Reconciliation"
        show 4 columns
        MatchReconciliation is a Pane
            quad width
            Layout
                two column distributed
                    large menu
                        list is MatchReconQueue.SmartReconWorkList
                            title is "MyWorkList"
                            menu item name is MyWorkList
                            description is "InvoicesOutsideOfToleranceAssignedToMeForProcessing"
                            icon is icon.compare-azure
                        list is MatchReconQueue.UnqueuedList
                            title is "Unassigned"
                            menu item name is Unassigned
                            description is "InvoicesOutsideOfToleranceNotYetAssignedToAnyoneForProcessing"
                            icon is icon.compare-azure
                        list is MatchReconQueueFinanceResource.MatchSpecialistAssignedList
                            title is "AllAssigned"
                            menu item name is AllAssigned
                            description is "AllAssignedInvoicesOutsideOfTolerance"
                            icon is icon.compare-azure
                        list is MatchReconQueue.SmartReconWorkListServiceContracts
                            title is "ServiceContracts"
                            menu item name is ServiceContracts
                            description is "ServiceContractInvoicesOutsideOfTolerance"
                            icon is icon.compare-azure
        Additional is a Pane
            quad width
            Layout
                two column distributed
                    large menu
                        list is MatchReconQueue.MatchPrepayments
                            title is "MatchPrepayments"
                            menu item name is MatchPrepayments
                            description is "PrepaidPurchaseOrderInvoicesThatNeedMatchingOnceReceivingHasOccurred"
                            icon is icon.CSFPayables
                        page is PrepaidInvoiceNotReceivedPage
                            title is "PrepaidInvoiceNotReceived"
                            menu item name is PrepaidInvoiceNotReceived
                            description is "CreateChargebacksForPrepaidInvoicesThatHaveNotBeenReceived"
                            icon is icon.cycle-emerald
                        list is PayablesInvoice.MatchAOCOnly
                            title is "AddOnCostMatch"
                            menu item name is AddOnCostMatch
                            description is "ReconciliationForInvoicesWithAddOnCostsOnly"
                            icon is icon.CSFPayables
                        list is PayablesInvoice.MatchReferenceAdjustmentList
                            title is "MatchReferenceAdjustment"
                            menu item name is MatchReferenceAdjustment
                            description is "ListOfUnmatchedInvoicesThatUseAMatchReferenceRatherThanAPurchaseOrderForMatching"
                            icon is icon.CSFPayables

    Aging is a DashBoardPanel
        VendorAgingWithPercents is a Pane
            quad width
            title is "ByVendor"
            business class is MatchAgingSummary
                list is VendorPeriod4List
                    overview navigation is MatchAgingPageNav
                        label is "NavigateToCompleteAgingPage"

