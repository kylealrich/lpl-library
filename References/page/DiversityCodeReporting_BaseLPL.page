DiversityCodeReporting is a Page

    DiversityCodes is a Panel
        business class is PayablesDiversityCode
            list is DiversityDashboardList

    Contracts is a MultiPanePanel
        pane 1
            business class is ContractDiversity
                list is ContractsByDiversity
        pane 2
            business class is Contract
                list is ContractsWithNoDiversity
        pane 3
            business class is Contract
                list is ContractsWithDiversity
        pane 4
            business class is Contract
                list is ContractsWithDiversityTotalsOnly

    PurchaseOrders is a MultiPanePanel
        pane 1
            business class is PurchaseOrderDiversity
                list is PurchaseOrdersByDiversity
        pane 2
            business class is PurchaseOrder
                list is PurchaseOrdersWithNoDiversity
        pane 3
            business class is PurchaseOrder
                list is PurchaseOrdersWithDiversity
        pane 4
            business class is PurchaseOrder
                list is PurchaseOrdersWithDiversityTotalsOnly

    Subcontractors is a MultiPanePanel
        pane 1
            business class is ContractSubcontractorDiversity
                list is ContractSubcontractorsByDiversity
        pane 2
            business class is ContractSubcontractor
                list is ContractsWithNoDiversity
        pane 3
            business class is ContractSubcontractor
                list is ContractSubcontractorsWithDiversity
        pane 4
            business class is ContractSubcontractor
                list is ContractSubcontractorsWithDiversityTotalsOnly

    InvoiceDetailsForACommodityCode is a Panel
        business class is PayablesInvoiceDetail
            list is InvoiceDetailsForACommodityCodeByDiversityCode

