ProjectAccountantBurdensPage is a Page
    title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Burdens"

    IndirectBurdenCodes is a Panel
        business class is ProjectIndirectBurdenCode
            list is ProjectIndirectBurdenCodes
            Context
                FinanceEnterpriseGroup

    BurdenTransactions is a Panel
        business class is ProjectIndirectBurdenTransaction
            list is AllProjectIndirectBurdenTransactionList
            Context
                FinanceEnterpriseGroup

    JournalizeGroups is a Panel
        business class is ProjectIndirectBurdenJournalizeGroup
            list is ProjectIndirectBurdenJournalizeGroupList
            Context
                FinanceEnterpriseGroup

    EnterpriseProjects is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>List"
        business class is FinanceEnterpriseGroup.first EnterpriseProjectStructureRel.ProjectNode.ProjectChildrenRel
            list is EnterpriseProjectList
                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>List"
            Context
                FinanceEnterpriseGroup

    BurdenProcessing is a Panel
        Layout
            single column
                header2 of "IndirectBurdenProcesses"
                large menu
                    action is Project.GenerateIndirectBurdenTransactions
                        title is "GenerateIndirectBurdenTransactions"
                        menu item name is GenerateIndirectBurdenTransactions
                        description is "Generate_indirect_burden_transactions."
                        icon is icon.CSFApplicationAdministrator
                    action is ProjectIndirectBurdenTransaction.ValidateIndirectBurdenJournalEntries
                        title is "ValidateIndirectBurdenJournalEntries"
                        menu item name is ValidateIndirectBurdenJournalEntries
                        description is "Validate_indirect_burden_journal_entries."
                        icon is icon.CSFApplicationAdministrator
                    action is ProjectIndirectBurdenTransaction.JournalizeIndirectBurdenTransactions
                        title is "JournalizeIndirectBurdenTransactions"
                        menu item name is JournalizeIndirectBurdenTransactions
                        description is "Journalize_indirect_burden_transactions."
                        icon is icon.CSFApplicationAdministrator
                    action is ProjectIndirectBurdenTransaction.ReverseJournalizedIndirectBurdens
                        title is "ReverseJournalizedBurdenTransactions"
                        menu item name is ReverseJournalizedBurdenTransactions
                        description is "Reverse_journalized_indirect_burden_transactions."
                        icon is icon.CSFApplicationAdministrator
                    action is ProjectIndirectBurdenTransaction.DeleteRunGroup
                        title is "DeleteRunGroupTransactions"
                        menu item name is DeleteRunGroupTransactions
                        description is "Delete_run_group_transactions."
                        icon is icon.CSFApplicationAdministrator
                    action is ProjectIndirectBurdenTransaction.RemoveBurdenAPHold
                        title is "RemoveIndirectBurdenAPHold"
                        menu item name is RemoveIndirectBurdenAPHold
                        description is "Remove_AP_hold_from_indirect_burden_transactions."
                        icon is icon.CSFApplicationAdministrator

    StructureValidationResults is a Panel
        title is "ValidationResults"
        business class is ProjectStructureValidationResult
            list is ProjectStructureValidationIndirectBurdenResultList
                title is "ValidationResults"

