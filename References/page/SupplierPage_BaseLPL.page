SupplierPage is a Page
    title is "Suppliers"
    show panel control on left

    Suppliers is a Panel
        business class is Supplier
            list is AllSupplierDashboard
            Context
                SupplierGroup

    SupplierList is a Panel
        business class is Supplier
            list is Suppliers
            Context
                SupplierGroup

    SupplierHierarchy is a Panel
        business class is Supplier
            list is SuppliersInTreeView
            Context
                SupplierGroup

    SupplierSearch is a Panel
        business class is Supplier
            list is SupplierSearchList

    SupplierContactSearch is a Panel
        business class is SupplierSourceId
            list is SupplierContactSearch

    MissingSupplierRequirements is a MultiPanePanel
        pane 2
            business class is Supplier
                list is SuppliersWhoNeedValidation
        pane 3
            business class is Supplier
                list is RequiredQuestionsNotAnswered
        pane 4
            business class is SupplierSourceId
                list is MissingCommodityCodes

    CertificationAlerts is a MultiPanePanel
        pane 1
            business class is SupplierCertification
                list is ExpiringSupplierCertificationsDirect
        pane 2
            business class is SupplierCertification
                list is ExpiringSupplierContactCertificationsDirect

    DiversityAlerts is a MultiPanePanel
        pane 1
            business class is Supplier
                list is ExpiringDiversityCodes
        pane 2
            business class is Supplier
                list is DiversityCodesNeedingDates

    NotRegisteredInExternalEntity is a Panel
        visible when (actor.context.FinanceEnterpriseGroup.IFSEnableSupplierExtEntityEnabled)
        business class is SupplierSourceId
            list is NotRegisteredInExternalEntity

