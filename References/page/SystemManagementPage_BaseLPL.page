SystemManagementPage is a Page
    title is "SystemManagement"

    SystemProperties is a DashBoardPanel
        use 12 column grid
        DomainManagement is a Pane
            span 6 columns
            Layout
                single column
                    small menu
                        list is SSODomain.primary
                            title is "SSODomain"
                            menu item name is SSODomainPrimary
                            icon is icon.group
                    small menu
                        list is SSODomainProperty.primary
                            title is "SSODomainProperty"
                            menu item name is SSODomainPropertyPrimary
                            icon is icon.group
                    small menu
                        page is DomainAuthorizationPage
                            title is "DomainAuthorization"
                            menu item name is DomainAuthorizationPage
                            icon is icon.group
        ServiceManagement is a Pane
            span 6 columns
            Layout
                single column
                    small menu
                        page is ServiceConsole
                            title is "ServiceConsole"
                            menu item name is ServiceConsole
                            icon is icon.technology
                    small menu
                        list is Service.primary
                            title is "Service"
                            menu item name is ServicePrimary
                            icon is icon.technology
                    small menu
                        list is Security.primary
                            title is "AuthorizationActivation"
                            menu item name is SecurityPrimary
                            icon is icon.technology
        SecurityPolicies is a Pane
            span 6 columns
            Layout
                single column
                    small menu
                        list is SecurityProfile.SecurityProfileList
                            title is "SecurityProfile"
                            menu item name is SecurityProfileSecurityProfileList
                            icon is icon.shield-safety
                    small menu
                        page is SecurityPolicyPage
                            title is "ConsoleView"
                            menu item name is SecurityPolicyPage
                            icon is icon.shield-safety
        SystemManagement is a Pane
            span 6 columns
            Layout
                single column
                    small menu
                        list is LoginScheme.primary
                            title is "LoginScheme"
                            menu item name is LoginSchemePrimary
                            icon is icon.workflow
                        page is EndpointManagement
                            title is "Endpoints"
                            menu item name is Endpoints
                            icon is icon.workflow
                        action is SecSessionDebugExport.ExportLogAndReport
                            title is "SecuritySessionDebugExport"
                            menu item name is SecSessionDebugExportExportLogAndReport
                            icon is icon.workflow
                        action is ValidateInquireCndOptimization.ValidateOptimization
                            title is "InquireConditionOptimizationValidation"
                            menu item name is InquireConditionOptimizationValidation
                            icon is icon.workflow

    SecurityLogger is a Panel
        Layout
            single column
                small menu
                    list is SecurityLogger.SecurityLoggerList
                        title is "SecurityLoggerSettings"
                        menu item name is SecurityLoggerSecurityLoggerList
                        icon is icon.server
                    list is SecuritySessionDebug.SecuritySessionDebugList
                        title is "SecuritySessionDebug"
                        menu item name is SecuritySessionDebugSecuritySessionDebugList
                        icon is icon.server

    SessionManagement is a Panel
        business class is UserSession
            list is SessionList

    SecurityCacheAnalyser is a MultiPanePanel
        pane 1
            business class is SecurityCacheAnalyser
                list is SecurityCacheAnalyserList
        pane 2
            business class is pane 1.SecurityCacheKey set
                list is SecurityCacheKeyList
        pane 3
            business class is pane 2.SecurityCachedObject set
                list is DefaultSecurityCachedObjectList
        pane 4
            business class is pane 3.SecurityCachedObjectContent set
                list is DefaultSecurityCachedObjectContentList

