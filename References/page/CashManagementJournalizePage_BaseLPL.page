CashManagementJournalizePage is a Page
    title is "JournalizeCashDistributions"
    show panel control on left

    CashLedgerGLDistribution is a Panel
        title is "CashLedger"
        business class is CashLedgerGLDistribution
            list is CashLedgerGLDistributionsToJournalize
                title is "CashLedger"

    BankStatementLineDistribution is a Panel
        title is "BankStatementLine"
        business class is BankStatementLineDistribution
            list is FullStatementLineDistributionsToJournalize
                title is "BankStatementLine"

    ReconciliationVarianceDistributions is a Panel
        title is "NonCashLedgerReconciliationVariance"
        business class is NonCashLedgerDistribution
            list is FullNonCashLedgerDistributionsToJournalize
                title is "NonCashLedgerReconciliationVariance"

    ViewJournalizedGLDistributions is a Panel
        title is "Results"
        business class is JournalizeGroupResult
            list is JournalizeGroupResults
                title is "Results"

