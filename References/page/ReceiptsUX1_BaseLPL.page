ReceiptsUX1 is a Page
    title is "Receipts"
    implements MenuSearch

    Main is a DashBoardPanel
        use 12 column grid
        ImportantLinks is a Pane
            span 3 columns
            Layout
                single column
                    large menu
                        page is ReceiptProcessingSpace
                            title is "ManageReceipts"
                            menu item name is ManageReceipts
                            description is "View_and_process_student_receipts."
                            icon is icon.outline-form-azure
                        list is StudentActivityReceipt.StudentActivityReceiptList
                            title is "ActivityReceipts"
                            menu item name is ActivityReceipts
                            description is "Create_a_receipt."
                            icon is icon.document-certified-azure
                        list is StudentActivityReceiptBatch.StudentActivityReceiptBatchList
                            title is "BatchActivityReceipts"
                            menu item name is BatchActivityReceipts
                            description is "Create_receipts_in_batch."
                            icon is icon.document-certified-azure

