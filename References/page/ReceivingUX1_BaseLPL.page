ReceivingUX1 is a Page
    title is "Receiving"
    implements MenuSearch

    AllReceipts is a DashBoardPanel
        title is "Main"
        use 12 column grid
        Creation is a Pane
            span 3 columns
            Layout
                single column
                    action is PurchaseOrderReceipt.Unreleased.Create
                        title is "Create"
                        menu item name is PurchaseOrderReceiptCreate
                        icon is icon.document-emerald
                    action is PurchaseOrderReceipt.Unreleased.AddReceiveAll
                        title is "CreateReceiveAll"
                        menu item name is PurchaseOrderReceiptCreateReceiveAll
                        icon is icon.document-emerald
        KeyProcesses is a Pane
            span 3 columns
            Layout
                single column
                    page is ReceivingKeyProcessesUX1.ReceiptAdjustments
                        title is "ReceiptAdjustments"
                        menu item name is PurchaseOrderReceiptAdjustments
                        icon is icon.cycle-azure
                    page is ReceivingKeyProcessesUX1.ReceiptInspections
                        title is "ReceiptInspections"
                        menu item name is PurchaseOrderReceiptInspections
                        icon is icon.cycle-azure
                    page is ReceivingKeyProcessesUX1.VendorReturns
                        title is "VendorReturns"
                        menu item name is VendorReturns
                        icon is icon.cycle-azure
                    page is ReceivingKeyProcessesUX1.ReceiptLinesPendingInspection
                        title is "ReceiptLinesPendingInspection"
                        menu item name is ReceiptLinesPendingInspection
                        icon is icon.cycle-azure
        OtherProcesses is a Pane
            span 6 columns
            Layout
                two column distributed
                    action is PurchaseOrderLine.MassCreatePurchaseOrderReceipts
                        title is "MassCreate"
                        menu item name is PurchaseOrderReceiptMassCreate
                        icon is icon.cycle-azure
                    page is PurchaseOrderReceiptInterfaceMaintenance
                        title is "ReceiptInterfaces"
                        menu item name is PurchaseOrderReceiptInterfaces
                        icon is icon.cycle-azure
                    action is ReceivingDocument.ReceivingDocument
                        title is "ReceivingDocument"
                        menu item name is ReceivingDocument
                        icon is icon.cycle-azure
                    action is ReceivingDeliveryAndPutaway.ReceivingDeliveryAndPutaway
                        title is "ReceivingDeliveryAndPutaway"
                        menu item name is ReceivingDeliveryAndPutaway
                        icon is icon.cycle-azure
                    action is PurchaseOrderReceipt.BatchReleaseAndPrint
                        title is "ReleaseAndPrintDeliveryAndPutaway"
                        menu item name is ReleaseAndPrintDeliveryAndPutaway
                        icon is icon.cycle-azure
                    action is OpenReceiptArchive.OpenReceiptArchive
                        title is "OpenReceiptArchive"
                        menu item name is OpenReceiptArchive
                        icon is icon.cycle-azure
                    page is ReceiverBatchReleaseAndPrintPage
                        title is "ReceiverResultDocuments"
                        menu item name is ReceiverResultDocuments
                        icon is icon.cycle-azure
        ListsAndReports is a Pane
            span 6 columns
            Layout
                two column distributed
                    page is InvoicedNotReceivedPage
                        title is "InvoicedNotReceived"
                        menu item name is InvoicedNotReceived
                        icon is icon.clipboard-list-ruby
                    action is MatchedNotReceivedReport.MatchedNotReceivedReport
                        title is "MatchedNotReceived"
                        menu item name is MatchedNotReceived
                        icon is icon.document-emerald
                    action is PurchasingDeliverQualityAnalysis.PurchasingDeliverQualityAnalysis
                        title is "PurchasingDeliverQualityAnalysis"
                        menu item name is PurchasingDeliverQualityAnalysis
                        icon is icon.document-emerald
                    action is PurchasingDeliverQuantityAnalysis.PurchasingDeliverQuantityAnalysis
                        title is "PurchasingDeliverQuantityAnalysis"
                        menu item name is PurchasingDeliverQuantityAnalysis
                        icon is icon.document-emerald
                    action is PurchasingDeliveryDateAnalysis.PurchasingDeliveryDateAnalysis
                        title is "PurchasingDeliveryDateAnalysis"
                        menu item name is PurchasingDeliveryDateAnalysis
                        icon is icon.document-emerald
                    action is PurchasingReturnStatusReport.PurchasingReturnStatusReport
                        title is "PurchasingReturnStatus"
                        menu item name is PurchasingReturnStatus
                        icon is icon.document-emerald
                    action is PurchasingReturnsAnalysis.PurchasingReturnsAnalysis
                        title is "PurchasingReturnsAnalysis"
                        menu item name is PurchasingReturnsAnalysis
                        icon is icon.document-emerald
                    action is PrintVendorRMA.PrintVendorRMA
                        title is "PrintVendorRMA"
                        menu item name is PrintVendorRMA
                        icon is icon.document-emerald
                    action is ReceivingAdjustmentLog.ReceivingAdjustmentLog
                        title is "ReceivingAdjustmentLog"
                        menu item name is ReceivingAdjustmentLog
                        icon is icon.document-emerald
                    action is ReceivingLog.ReceivingLog
                        title is "ReceivingLog"
                        menu item name is ReceivingLog
                        icon is icon.document-emerald
        AuditHistory is a Pane
            span 3 columns
            Layout
                single column
                    page is PurchasingAuditHistoryUX1.Receipt
                        title is "Receipt"
                        menu item name is ReceiptAuditHistory
                        icon is icon.calendar-variant-amethyst
                    page is PurchasingAuditHistoryUX1.ReceiptLine
                        title is "ReceiptLine"
                        menu item name is ReceiptLineAuditHistory
                        icon is icon.calendar-variant-amethyst
        MobileSupplyChain is a Pane
            span 3 columns
            visible when (actor.context.FinanceEnterpriseGroup.ShowMobileSupplyChain)
            Layout
                single column
                    page is MobileSupplyChainPurchaseOrderReceiptPage
                        title is "PurchaseOrderReceipts"
                        menu item name is PurchaseOrderReceipts
                        icon is icon.clipboard-text-emerald
                    page is MiscellaneousReceiptPage
                        title is "MiscellaneousReceipts"
                        menu item name is MiscellaneousReceipts
                        icon is icon.clipboard-text-emerald
                    page is DockLoggingPage
                        title is "DockLogs"
                        menu item name is DockLogs
                        icon is icon.clipboard-text-emerald
                    page is DeliveryTrackingPage
                        title is "DeliveryTracking"
                        menu item name is DeliveryTracking
                        icon is icon.clipboard-text-emerald

    Search is a Panel
        business class is PurchaseOrderReceiptLine
            list is ReceiptSearchList

    Unreleased is a Panel
        title is "Unreleased"
        business class is PurchaseOrderReceipt
            list is UnreleasedPurchaseOrderReceipts
                title is "Unreleased"

    PendingTwoStepReceiving is a Panel
        title is "PendingTwoStep"
        business class is PurchaseOrderReceipt
            list is PendingTwoStepReceivingPurchaseOrderReceipts
                title is "PendingTwoStep"

    Released is a Panel
        title is "Released"
        business class is PurchaseOrderReceipt
            list is ReleasedPurchaseOrderReceipts
                title is "Released"

    ReadyToArchive is a Panel
        title is "ReadyToArchive"
        business class is PurchaseOrderReceipt
            list is ReadyToArchivePurchaseOrderReceipts
                title is "ReadyToArchive"

    DropShip is a Panel
        title is "DropShip"
        business class is PurchaseOrderReceipt
            list is DropShipPurchaseOrderReceipts
                title is "DropShip"

    ArchivedClosed is a Panel
        title is "Archived_/Closed"
        business class is PurchaseOrderReceipt
            list is ArchivedClosedPurchaseOrderReceipts
                title is "Archived_/Closed"

    All is a Panel
        title is "All"
        business class is PurchaseOrderReceipt
            list is AllPurchaseOrderReceipts
                title is "All"

