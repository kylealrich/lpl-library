SupplierMaintenancePage is a Page
    title is "ManageSuppliers"
    implements MenuSearch

    Dashboard is a Panel
        business class is Supplier
            list is AllSupplierDashboard

    List is a Panel
        business class is Supplier
            list is Suppliers

    Search is a Panel
        business class is Supplier
            list is SupplierSearchList

    Contacts is a Panel
        business class is SupplierSourceId
            list is SupplierContactSearch

    Other is a DashBoardPanel
        use 12 column grid
        Alerts is a Pane
            span 6 columns
            Layout
                two column distributed
                    small menu
                        page is SupplierMaintenancePageAlerts.ValidationNeeded
                            title is "ValidationNeeded"
                            menu item name is ValidationNeeded
                            icon is icon.warning-ruby
                        page is SupplierMaintenancePageAlerts.MissingCommodityCodes
                            title is "MissingCommodityCodes"
                            menu item name is MissingCommodityCodes
                            icon is icon.warning-ruby
                        page is SupplierMaintenancePageAlerts.UnansweredRequiredQuestions
                            title is "UnansweredRequiredQuestions"
                            menu item name is UnansweredRequiredQuestions
                            icon is icon.warning-ruby
                        page is SupplierMaintenancePageAlerts.DialogNeedingResponses
                            title is "SupplierDialogNeedingResponses"
                            menu item name is SupplierDialogNeedingResponses
                            icon is icon.warning-ruby
                        page is SupplierMaintenancePageAlerts.MyDialogResponseRequests
                            title is "MyDialogNeedingResponses"
                            menu item name is MyDialogNeedingResponses
                            icon is icon.warning-ruby
                        page is SupplierMaintenancePageAlerts.ExpiringCertifications
                            title is "ExpiringSupplierCertifications"
                            menu item name is ExpiringSupplierCertifications
                            icon is icon.warning-ruby
                        page is SupplierMaintenancePageAlerts.ExpiringContactCertifications
                            title is "ExpiringContactCertifications"
                            menu item name is ExpiringContractCertifications
                            icon is icon.warning-ruby
                        page is SupplierMaintenancePageAlerts.ExpiringDiversityCodes
                            title is "ExpiringDiversityCodes"
                            menu item name is ExpiringDiversityCodes
                            icon is icon.warning-ruby
                        page is SupplierMaintenancePageAlerts.DiversityCodesNeedingDates
                            title is "DiversityCodesNeedingDates"
                            menu item name is DiversityCodesNeedingDates
                            icon is icon.warning-ruby
                        page is SupplierMaintenancePageAlerts.UnreleasedSupplierMessages
                            title is "UnreleasedSupplierMessages"
                            menu item name is UnreleasedSupplierMessages
                            icon is icon.warning-ruby
                        visible when (actor.context.FinanceEnterpriseGroup.IFSEnableSupplierExtEntityEnabled)
                            page is SupplierMaintenancePageAlerts.NotRegisteredInExternalEntity
                                title is "NotRegisteredInExternalEntity"
                                menu item name is NotRegisteredInExternalEntity
                                icon is icon.warning-ruby
        Messages is a Pane
            span 6 columns
            Layout
                two column distributed
                    small menu
                        action is Supplier.CreateAdHocSupplierMessages
                            title is "CreateAdhoc"
                            menu item name is CreateAdhoc
                            icon is icon.check-emerald
                        page is SupplierMessagePage.SupplierMessageDialogRequestingResponses
                            title is "NeedingResponses"
                            menu item name is NeedingResponses
                            icon is icon.inbox-emerald
                        page is SupplierMessagePage.MyMessageDialogRequestingResponses
                            title is "WaitingForResponses"
                            menu item name is WaitingForResponses
                            icon is icon.inbox-emerald
                        page is SupplierMessagePage.OpenMessageDialog
                            title is "OpenDialog"
                            menu item name is OpenDialog
                            icon is icon.inbox-emerald
                        page is SupplierMessagePage.HistoricalMessageDialog
                            title is "HistoricalDialog"
                            menu item name is HistoricalDialog
                            icon is icon.inbox-emerald
                        page is SupplierMessagePage.DialogSearch
                            title is "DialogSearch"
                            menu item name is DialogSearch
                            icon is icon.inbox-emerald
                        page is SupplierMessagePage.UnreleasedAdHocMessages
                            title is "UnreleasedAdhoc"
                            menu item name is UnreleasedAdhoc
                            icon is icon.inbox-emerald
        Miscellaneous is a Pane
            span 3 columns
            Layout
                single column
                    small menu
                        list is Subcontractor.SubcontractorList
                            title is "Subcontractors"
                            menu item name is Subcontractors
                            icon is icon.person-azure
                        list is Supplier.SupplierMetricsList
                            title is "Metrics"
                            menu item name is Metrics
                            icon is icon.chart-bar-azure
                        action is Supplier.CompareSupplierAndVendor
                            title is "CompareSuppliers_andVendors"
                            menu item name is CompareSuppliersAndVendors
                            icon is icon.compare-azure
                        list is SupplierVendorMismatch.SupplierVendorMismatchList
                            title is "ViewSupplier_andVendorCompareResults"
                            menu item name is ViewSupplierAndVendorCompareResults
                            icon is icon.compare-azure
                        list is Supplier.SuppliersInTreeView
                            title is "ParentsAndChildren"
                            menu item name is ParentsAndChildren
                            icon is icon.shopping-cart-azure

