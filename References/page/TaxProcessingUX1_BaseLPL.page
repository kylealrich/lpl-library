TaxProcessingUX1 is a Page
    title is "Tax_andIncome"
    implements MenuSearch

    Main is a DashBoardPanel
        use 12 column grid
        ReportableIncome is a Pane
            span 3 columns
            Layout
                single column
                    column1
                        large menu
                            page is ReportableIncomeUX1.Payables
                                title is "Payables"
                                menu item name is Payables
                                icon is icon.document-finance-amber
                            page is ReportableIncomeUX1.NonPayables
                                title is "NonPayables"
                                menu item name is NonPayables
                                icon is icon.document-finance-amber
                            page is ReportableIncomeUX1.Combined
                                title is "Combined"
                                menu item name is Combined
                                icon is icon.document-finance-amber
        US1099 is a Pane
            span 3 columns
            Layout
                single column
                    column1
                        large menu
                            page is US1099OutputUX1.TapeOutput
                                title is "US1099Output"
                                menu item name is US1099Output
                                icon is icon.pricetag-amethyst
                            page is US1099OutputUX1.IDMOutput
                                title is "US1099IDMOutput"
                                menu item name is US1099IDMOutput
                                icon is icon.pricetag-amethyst
        Processing is a Pane
            span 6 columns
            title is "Reports_and_Lists"
            Layout
                two column distributed
                    column1
                        large menu
                            action is US1099TapeCreation.US1099TapeCreation
                                title is "CreateUS1099OutputFiles"
                                menu item name is CreateUS1099OutputFiles
                                description is "Create_files_for_IRS_1099_yearly_income_reporting."
                                icon is icon.document-blank-azure
                            action is TaxAnalysisReport.TaxAnalysisReport
                                title is "Analysis"
                                menu item name is Analysis
                                description is "View_historical_tax_information_and_update_the_status_of_current_tax_transactions."
                                icon is icon.chart-stacked-bar-azure
                            action is TaxIdentificationNumber.TaxIdentificationNumber
                                title is "IdentificationNotifications"
                                menu item name is IdentificationNotifications
                                description is "Create_tax_identification_notifications."
                                icon is icon.look-up-azure
                    column2
                        large menu
                            action is TaxTransactionConversion.TaxTransactionConversion
                                title is "CreateTransactionConversions"
                                menu item name is CreateTransactionConversions
                                description is "Create_tax_transaction_conversions."
                                icon is icon.document-blank-azure
                            action is TaxReconciliationReport.TaxReconciliationReport
                                title is "Reconciliation"
                                menu item name is Reconciliation
                                description is "View_invoices_applied_to_a_tax_period."
                                icon is icon.clipboard-list-azure
                            list is CSVOutputHeader.CSVOutputHeaderList
                                title is "IdentificationOutputFiles"
                                menu item name is IdentificationOutputFiles
                                description is "View_tax_identification_output_files."
                                icon is icon.look-up-azure
        Listings is a Pane
            span 4 columns
            Layout
                single column
                    column1
                        large menu
                            list is TaxTransaction.TaxTransactionList
                                title is "TaxTransactions"
                                menu item name is TaxTransactions
                                icon is icon.chart-stacked-bar-ruby
                            list is PayablesJobResult.VendorReportHeaderList
                                title is "VendorPayments"
                                menu item name is VendorPayments
                                icon is icon.look-up-ruby
                            list is TaxConfiguration.TaxConfigurationAnalysisList
                                title is "TaxConfigurations"
                                menu item name is TaxConfiguration
                                icon is icon.look-up-ruby
                            list is TaxDistributionCode.TaxDistributionCodeList
                                title is "TaxDistributionCodes"
                                menu item name is TaxDistributionCode
                                icon is icon.look-up-ruby
                            list is TaxDistributionLevel.TaxDistributionLevelList
                                title is "TaxDistributionLevels"
                                menu item name is TaxDistributionLevel
                                icon is icon.look-up-ruby
                            list is TaxAttribute.TaxAttributeList
                                title is "TaxAttributes"
                                menu item name is TaxAttributes
                                icon is icon.look-up-ruby
        GlobalWithholding is a Pane
            span 4 columns
            Layout
                two column distributed
                    large menu
                        page is GlobalWithholdingReportingPage.PaidWithholding
                            title is "Paid"
                            menu item name is Paid
                            description is "View_invoice_payment_withholding_information."
                            icon is icon.money-bag-emerald
                        page is GlobalWithholdingReportingPage.AccruedWithholding
                            title is "Accrued"
                            menu item name is Accrued
                            description is "View_unpaid_accrued_withholding_amounts."
                            icon is icon.money-bag-emerald
        Diversity is a Pane
            span 4 columns
            Layout
                single column distributed
                    large menu
                        page is DiversityCodeReporting
                            title is "Reporting"
                            menu item name is Reporting
                            description is "View_diversity_information_for_contracts,_purchase_orders,_and_subcontractors."
                            icon is icon.group-amethyst

