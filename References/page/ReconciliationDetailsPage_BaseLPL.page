ReconciliationDetailsPage is a Page
    title is "ReconciliationDetails"

    ReconciliationDetails is a MultiPanePanel
        pane 1
            business class is ReconciliationAccount
                list is ReconciliationAccountList
        pane 2
            business class is pane 1.ReconciliationDetail set
                list is ReconcilingItemsList
        pane 3
            business class is pane 1.ReconciliationDetail set
                list is ReconciliationDetailsList
        pane 4
            business class is pane 1.ReconciliationDetail set
                list is AmortizationList

