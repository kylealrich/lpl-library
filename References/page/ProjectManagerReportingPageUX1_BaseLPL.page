ProjectManagerReportingPageUX1 is a Page
    title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Reports"
    implements MenuSearch
    show panel control on left

    ProjectObligations is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Obligations"
        business class is ProjectReportingSettings
            list is ProjectManagerReportingSettingsProjectObligationsListUX1
                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Obligations"

    ProjectVariance is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Variance"
        business class is ProjectReportingSettings
            list is ProjectManagerReportingSettingsProjectVarianceListUX1
                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Variance"

    ProjectPeriodComparison is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>PeriodComparison"
        business class is ProjectReportingSettings
            list is ProjectManagerReportingSettingsProjectPeriodComparisonListUX1
                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>PeriodComparison"

    ContractPercentComplete is a Panel
        business class is ProjectReportingSettings
            list is ProjectManagerReportingSettingsContractPercentCompleteFlatListUX1

    ContractGrossMargin is a Panel
        business class is ProjectReportingSettings
            list is ProjectManagerReportingSettingsContractGrossMarginFlatListUX1

    ScheduleOfFederalAwards is a Panel
        business class is ProjectReportingSettings
            list is ProjectManagerReportingSettingsScheduleOfFederalAwardFlatListUX1

    UnassignedProjectReportingSettings is a Panel
        business class is ProjectReportingSettings
            list is ProjectReportingSettingsNoActorList

