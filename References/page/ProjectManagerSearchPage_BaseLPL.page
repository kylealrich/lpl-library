ProjectManagerSearchPage is a Page
    title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>Search"
    is report for ProjectManager
        category is "TransactionsAndBalances"

    TransactionSearch is a Panel
        title is "Transaction"
        business class is GeneralLedgerTransaction
            list is ProjectAccountGeneralLedgerTransactionList
                title is "Transaction"

    TransactionDetailSearch is a Panel
        title is "TransactionDetail"
        business class is GLTransactionDetail
            list is ProjectAccountantGLTransactionDetailFilterList
                title is "TransactionDetail"

    TransactionDetailFilterSearch is a Panel
        title is "TransactionDetailSearch"
        business class is GLTransactionDetail
            list is ProjectAccountantGLTransactionDetailList
                title is "TransactionDetailSearch"

    ProjectContractTransactionDetailSearch is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>ContractTransactionDetail"
        business class is GLTransactionDetail
            list is ProjectContractGLTransactionDetailList
                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>ContractTransactionDetail"

    ProjectPersonnelTransactionDetailSearch is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>PersonnelTransactionDetail"
        business class is ProjectPersonnel
            list is ProjectPersonnelTransactionList
                title is "<actor.context.FinanceEnterpriseGroup.ProjectLabel>PersonnelTransactionDetail"

    TotalsSearch is a Panel
        title is "Total"
        business class is GeneralLedgerTotal
            list is GeneralLedgerProjectTotalSearchList
                title is "Total"

    LaborDistributionSearch is a Panel
        title is "LaborDistribution"
        business class is ProjectAssignmentLabor
            list is AllProjectAssignmentLaborList
                title is "LaborDistribution"

    EffortCertificationSearch is a Panel
        title is "EffortCertification"
        business class is ProjectAssignmentEffort
            list is AllProjectAssignmentEffortList
                title is "EffortCertification"

