UX1AssetClose is a Page
    title is "MonthlyProcessing"
    implements MenuSearch

    ActionsPanel is a Panel
        title is "Actions"
        Layout
            four column distributed
                column1
                    small menu
                        action is AssetBatchUpdateResult.ReleaseAssets
                            title is "ReleaseAssets"
                            menu item name is ReleaseAssets
                            icon is icon.check-azure
                            Search Terms
                                    "ReleaseAssets"
                        action is AssetBatchUpdateResult.DepreciationCalculation
                            title is "CalculateDepreciation"
                            menu item name is CalculateDepreciation
                            icon is icon.check-azure
                            Search Terms
                                    "CalculateDepreciation"
                                    "DepreciationCalculation"
                        paragraph
                            text of "_"
                        action is ReleaseAssets.ReleaseAssets
                            title is "ReleaseAssets_-Old"
                            menu item name is ReleaseAssetsOld
                            icon is icon.check-azure
                            Search Terms
                                    "ReleaseAssets"
                        action is AssetDepreciationCalculationAndList.AssetDepreciationCalculationAndList
                            title is "CalculateDepreciation_-Old"
                            menu item name is CalculateDepreciationOld
                            icon is icon.check-azure
                            Search Terms
                                    "CalculateDepreciation"
                                    "DepreciationCalculation"
                column2
                    small menu
                        action is AssetBatchUpdateResult.AssetPeriodClose
                            title is "ClosePeriod"
                            menu item name is AssetPeriodClose
                            icon is icon.check-azure
                            Search Terms
                                    "AssetClose"
                                    "AssetPeriodClose"
                        action is AssetYearEndClose.AssetYearEndClose
                            title is "CloseYear"
                            menu item name is AssetYearEndClose
                            icon is icon.check-azure
                            Search Terms
                                    "AssetYearEnd"
                                    "YearEndClose"
                                    "CloseYear"
                                    "AssetYearEndClose"
                        paragraph
                            text of "_"
                        action is AssetPeriodClose.AssetPeriodClose
                            title is "ClosePeriod-Old"
                            menu item name is AssetPeriodCloseOld
                            icon is icon.check-azure
                            Search Terms
                                    "AssetCloseOld"
                                    "AssetPeriodCloseOld"
                column3
                    small menu
                        action is DepreciationRecalculation.DepreciationRecalculation
                            title is "RecalculateDepreciation"
                            menu item name is RecalculateDepreciation
                            icon is icon.check-azure
                            Search Terms
                                    "RecalculateDepreciation"
                        action is AssetShortYearClose.AssetShortYearClose
                            title is "ShortYearClose"
                            menu item name is ShortYearClose
                            icon is icon.check-azure
                            Search Terms
                                    "ShortYearClose"
                                    "AssetShortYearClose"
                column4
                    small menu
                        action is AssetBook.UpdateYearEndBalance
                            title is "UpdateYearEndBalance"
                            menu item name is UpdateYearEndBalance
                            icon is icon.check-azure
                            Search Terms
                                    "UpdateYearEndBalance"

    CloseDashboard is a Panel
        business class is AssetCompany
            list is CloseDashboard

    DepreciationOverride is a Panel
        business class is AssetBook
            list is AssetDepreciationOverride

