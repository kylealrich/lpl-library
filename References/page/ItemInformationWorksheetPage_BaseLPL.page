ItemInformationWorksheetPage is a Page
    title is "MaintainItemWorksheet"
    show panel control on left

    AllWorksheetItems is a Panel
        business class is ItemInformationWorksheet
            list is CreateItemWorksheetList

    CreateItemInformation is a Panel
        business class is ItemInformationWorksheet
            list is EligibleForItemCreationList

    WorksheetItemsWithErrors is a Panel
        business class is ItemInformationWorksheet
            list is ErrorList

    Results is a Panel
        business class is ItemInformationWorksheetResult
            list is ItemInformationWorksheetResultList

    ContractLineSpecialItems is a Panel
        business class is ContractLine
            list is ContractLineSpecialItems

    ContractLineInactiveItems is a Panel
        business class is ContractLine
            list is ContractLineSpecialItemsToReactivate

