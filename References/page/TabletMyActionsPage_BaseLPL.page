TabletMyActionsPage is a Page
    title is "MyActions"

    MyRequestActions is a Panel
        title is "All"
        business class is ActionRequest
            list is MyRequestActions
                title is "All"

    MyOpenRequestActions is a Panel
        title is "Open"
        business class is ActionRequest
            list is MyOpenRequestActions
                title is "Open"

    MyScheduledActions is a Panel
        title is "Scheduled"
        business class is AsyncActionRequest
            list is MyAsyncActionRequestList
                title is "Scheduled"

