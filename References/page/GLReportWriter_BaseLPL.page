GLReportWriter is a Page
    title is "GlobalLedgerReportDesigner"
    is report for FinancialBusinessAnalyst,GlobalLedger,StaffAccountant,BudgetController,ChiefFinancialOfficer,Projects,ProjectAccountant,BudgetAnalyst,Controller,ApplicationAdministrator,FinancialsViewer,ProjectAdministrator,FinancialsProcessor
        category is "Dimension_\\Analysis"
        visible when (actor.context.FinanceEnterpriseGroup.ShowGLReportDesigner)
    DesignerDashboard is a Panel
        title is "Overview"
        business class is GLReport
            list is GLDesignerDashboard
                title is "Overview"

    Reports is a Panel
        title is "Reports"
        visible when (actor.context.FinanceEnterpriseGroup.ReportingPowerUserExists)
        business class is GLReport
            list is GLReportList
                title is "Reports"

    RowGroups is a Panel
        title is "RowLayout"
        visible when (actor.context.FinanceEnterpriseGroup.ReportingPowerUserExists)
        business class is GLRowView
            list is GLRowDimensionCardViewList
                title is "RowLayout"

    ColumnGroups is a Panel
        title is "ColumnLayout"
        visible when (actor.context.FinanceEnterpriseGroup.ReportingPowerUserExists)
        business class is GLColumnView
            list is GLColumnDimensionCardViewList
                title is "ColumnLayout"

    ReportGrouping is a Panel
        title is "ReportGrouping"
        business class is GLReportGrouping
            list is GLReportGroupingList
                title is "ReportGrouping"

