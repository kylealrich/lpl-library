UserManagementPage is a Page
    title is "UserManagement"

    ActorDashboard is a MultiPanePanel
        title is "ActorConsole"
        pane 1
            business class is Actor
                list is Last14DaysActorList
        pane 2
            business class is Actor
                list is AcknowledgedBodRequestActorList
        pane 3
            business class is Actor
                list is ChangesToActorsInLast14DaysList
        pane 4
            business class is ActorRole
                list is RecentActorRoleChangesList

    Actors is a Panel
        business class is Actor
            list is ActorList

    ActorRole is a Panel
        business class is ActorRole
            list is DefaultActorRoleList

    ActorContext is a Panel
        business class is ActorContext
            list is ActorContextAdminList

    ActorContextTemplate is a Panel
        business class is ActorContextTemplate
            list is ActorContextTemplateList

    Agent1 is a Panel
        title is "Agent"
        business class is Agent
            list is AgentWithActorList
                title is "Agent"

    Identity is a Panel
        business class is Identity
            list is IdentityList

    ExternalUser is a DashBoardPanel
        use 12 column grid
        ExternalUserAPI is a Pane
            span 6 columns
            icon is icon.user
            Layout
                single column
                    small menu
                        list is ExternalUser.ExternalUserRequestList
                            title is "ExternalUserAPI"
                            menu item name is ExternalUserExternalUserRequestList
                    small menu
                        action is IdentityActor.GetExternalUsersCount
                            title is "GetExternalUsersCount"
                            menu item name is ExternalUserGetExternalUsersCount
        ExternalUserMigration is a Pane
            span 6 columns
            Layout
                single column
                    small menu
                        action is ExternalUsersMigration.MigrateExternalUsers
                            title is "MigrateExternalUsers"
                            menu item name is ExternalUsersMigrationMigrateExternalUsers
                    small menu
                        page is ExternalUserMigrationPage
                            title is "ExternalUserMigration"
                            menu item name is ExternalUserMigrationPage
                    small menu
                        page is ExternalUserMigrationMetricPage
                            title is "ExternalUserMigrationMetric"
                            menu item name is ExternalUserMigrationMetricPage

    CreateUserWizard is a Panel
        Layout
            single column
                large menu
                    action is Actor.CreateUserWizard
                        title is "CreateUserWizard"
                        menu item name is ActorCreateUserWizard
                        description is "CreateUserWizard"
                        icon is icon.user

