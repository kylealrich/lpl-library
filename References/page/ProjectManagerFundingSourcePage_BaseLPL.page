ProjectManagerFundingSourcePage is a Page
    title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>List"
    is report for Projects,ProjectManager
        category is "InvoicingAndRevenue"

    DashBoard is a Panel
        title is "Dashboard"
        business class is FinanceEnterpriseGroup.first EnterpriseDim2StructureRel.PersonResponsiblePostingInDimensionStructureRel
            list is ProjectManagerFundingSourceDashBoardView
                title is "Dashboard"
            Context
                FinanceEnterpriseGroup

    MyFundingSources is a Panel
        title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>"
        business class is FinanceEnterpriseGroup.first EnterpriseDim2StructureRel.MySummaryDimension2Rel
            list is XiMySummaryFinanceDimension2List
                title is "<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>"
            Context
                FinanceEnterpriseGroup

    MyTeamFundingSources is a Panel
        title is "Team<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>"
        business class is FinanceEnterpriseGroup.first EnterpriseDim2StructureRel.MySummaryDimension2TeamRel
            list is XiMySummaryFinanceDimension2List
                title is "Team<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>"
            Context
                FinanceEnterpriseGroup

    MyPostingFundingSources is a Panel
        title is "Posting<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>"
        business class is FinanceEnterpriseGroup.first EnterpriseDim2StructureRel.PersonResponsiblePostingInDimensionStructureRel
            list is FinanceDimension2PersonResponsibleFlatList
                title is "Posting<actor.context.FinanceEnterpriseGroup.FinanceDimension2Label>"
            Context
                FinanceEnterpriseGroup

    MyDeliverables is a MultiPanePanel
        title is "Deliverables"
        pane 1
            business class is ProjectFundingSourceDeliverable
                list is MyDeliverableList
        pane 2
            business class is ProjectFundingSourceDeliverable
                list is TeamDeliverableList

    MyExpiringDeliverables is a MultiPanePanel
        title is "ExpiringDeliverables"
        pane 1
            business class is ProjectFundingSourceDeliverable
                list is MyEmailDeliverableList
        pane 2
            business class is ProjectFundingSourceDeliverable
                list is MyTeamEmailDeliverableList

