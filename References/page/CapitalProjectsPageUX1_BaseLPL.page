CapitalProjectsPageUX1 is a Page
    title is "CapitalProjects"
    implements MenuSearch

    CapitalProjects is a Panel
        title is "CapitalProjects"
        business class is Project
            list is AssetPostingProjectsList
                title is "CapitalProjects"

    ProjectAssetInterfaceControl is a Panel
        title is "ProjectAssetInterfaceControl"
        business class is AssetInterfaceOptions
            list is ProjectInterfaceOptions
                title is "ProjectAssetInterfaceControl"

    CapitalizationProcesses is a DashBoardPanel
        title is "CapitalizationProcesses"
        use 12 column grid
        Processes is a Pane
            span 12 columns
            Layout
                two column distributed
                    column1
                        large menu
                            visible when (actor.context.FinanceEnterpriseGroup.CapitalizationUpgradeNotEnabled)
                                action is Project.HoldCapitalizableTransactions
                                    title is "HoldCapitalTransactions"
                                    menu item name is HoldCapitalTransactions
                                    description is "Run_this_action_to_select_global_ledger_transactions_to_be_placed_on_capitalization_hold."
                                    icon is icon.settings-azure
                            visible when (actor.context.FinanceEnterpriseGroup.CapitalizationUpgradeCompleted)
                                action is Project.HoldCapitalizableTransactions
                                    title is "HoldAssetCapitalTransactions"
                                    menu item name is HoldAssetCapitalTransactions
                                    description is "Run_this_action_to_select_asset_capital_transactions_to_be_placed_on_capitalization_hold."
                                    icon is icon.settings-azure
                        large menu
                            visible when (actor.context.FinanceEnterpriseGroup.CapitalizationUpgradeNotEnabled)
                                action is Project.RemoveHoldFromCapitalizableTransactions
                                    title is "RemoveHoldFromCapitalizableTransactions"
                                    menu item name is RemoveHoldFromCapitalizableTransactions
                                    description is "Run_this_action_to_remove_hold_from_capitalizable_global_ledger_transactions."
                                    icon is icon.settings-azure
                            visible when (actor.context.FinanceEnterpriseGroup.CapitalizationUpgradeCompleted)
                                action is Project.RemoveHoldFromCapitalizableTransactions
                                    title is "RemoveHoldFromAssetCapitalizableTransactions"
                                    menu item name is RemoveHoldFromAssetCapitalizableTransactions
                                    description is "Run_this_action_to_remove_hold_from_capitalizable_asset_capital_transactions."
                                    icon is icon.settings-azure
                    column2
                        large menu
                            action is AssetInterfaceOptions.DraftProjectAssetInterfaceSet
                                title is "GenerateAssetInterfaceRecords"
                                menu item name is GenerateAssetInterfaceRecords
                                description is "Run_this_action_to_create_draft_project_asset_interface_records."
                                icon is icon.settings-azure
        Results is a Pane
            span 12 columns
            double height
            Layout
                two column distributed
                    column1
                        large menu
                            visible when (actor.context.FinanceEnterpriseGroup.CapitalizationUpgradeNotEnabled)
                                list is GLTransactionDetail.ProjectUnprocessedCapitalTransactionsList
                                    title is "UnprocessedCapitalTransactions"
                                    menu item name is UnprocessedCapitalTransactions
                                    description is "View_transaction_detail_records_where_capitalize_flag_is_unprocessed."
                                    icon is icon.clipboard-list-emerald
                            visible when (actor.context.FinanceEnterpriseGroup.CapitalizationUpgradeCompleted)
                                list is AssetCapitalTransaction.ProjectUnprocessedCapitalTransactionsList
                                    title is "UnprocessedAssetCapitalTransactions"
                                    menu item name is UnprocessedAssetCapitalTransactions
                                    description is "View_asset_capital_transaction_records_where_capitalize_flag_is_unprocessed."
                                    icon is icon.clipboard-list-emerald
                        large menu
                            visible when (actor.context.FinanceEnterpriseGroup.CapitalizationUpgradeNotEnabled)
                                list is GLTransactionDetail.ProjectHoldCapitalTransactionsList
                                    title is "CapitalTransactionsOnHold"
                                    menu item name is CapitalTransactionsOnHold
                                    description is "View_transaction_detail_records_where_capitalize_flag_is_hold."
                                    icon is icon.clipboard-list-emerald
                            visible when (actor.context.FinanceEnterpriseGroup.CapitalizationUpgradeCompleted)
                                list is AssetCapitalTransaction.ProjectHoldCapitalTransactionsList
                                    title is "AssetCapitalTransactionsOnHold"
                                    menu item name is AssetCapitalTransactionsOnHold
                                    description is "View_asset_capital_transaction_detail_records_where_capitalize_flag_is_hold."
                                    icon is icon.clipboard-list-emerald
                    column2
                        large menu
                            list is AssetInterface.ProjectAssetInterface
                                title is "ProjectAssetInterface"
                                menu item name is ProjectAssetInterface
                                description is "View_and_maintain_project_asset_interface_records."
                                icon is icon.clipboard-list-emerald
                        large menu
                            list is JournalizeGroupResult.JournalizeGroupResults
                                title is "JournalizeGroupResults"
                                menu item name is JournalizeGroupResults
                                description is "View_journalized_distributions."
                                icon is icon.clipboard-list-emerald

