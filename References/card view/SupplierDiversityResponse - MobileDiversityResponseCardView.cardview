MobileDiversityResponseCardView is a CardView
    show labels
    main column
        PayablesDiversityCode.Description
            no label
        EffectiveDate
            label is "Effective"
        ExpirationDate
            label is "Expires"
        button row
            button of icon.Attachment
                visible when (HasAttachment)
                view Attachment.File
            button of icon.delete
                action is Delete
