VehicleCardView is a CardView
    show labels
    main column
        text of "<Vehicle>_-_<Vehicle.Description>"
            header4
            bold
        Company
        VehicleType
            label is "Type"
        VehicleAttribute
            label is "Attribute"
            visible when (VehicleAttributeEntered)
        VehicleClassification
            label is "Classification"
        Availability
